{"dta.poem.11479": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Grabschrifft eines liederlichen  clerici.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Hjer hat der pater Sch\u00f6ps sein grab,", "tokens": ["Hjer", "hat", "der", "pa\u00b7ter", "Sch\u00f6ps", "sein", "grab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "ADJA", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der, als der kalte tod ihn in den arm genommen,", "tokens": ["Der", ",", "als", "der", "kal\u00b7te", "tod", "ihn", "in", "den", "arm", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "KOUS", "ART", "ADJA", "NN", "PPER", "APPR", "ART", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Zwar seine seele GOtt mit thr\u00e4nen \u00fcbergab;", "tokens": ["Zwar", "sei\u00b7ne", "see\u00b7le", "Gott", "mit", "thr\u00e4\u00b7nen", "\u00fc\u00b7berg\u00b7ab", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Allein man wei\u00df doch nicht, ob er sie angenommen.", "tokens": ["Al\u00b7lein", "man", "wei\u00df", "doch", "nicht", ",", "ob", "er", "sie", "an\u00b7ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "ADV", "PTKNEG", "$,", "KOUS", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "