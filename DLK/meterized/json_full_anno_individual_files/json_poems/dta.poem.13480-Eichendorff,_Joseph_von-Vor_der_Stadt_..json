{"dta.poem.13480": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Vor der Stadt .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.85", "id:0.14"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Zwei Musikanten ziehn daher", "tokens": ["Zwei", "Mu\u00b7si\u00b7kan\u00b7ten", "ziehn", "da\u00b7her"], "token_info": ["word", "word", "word", "word"], "pos": ["CARD", "NN", "VVFIN", "PAV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vom Wald aus weiter Ferne,", "tokens": ["Vom", "Wald", "aus", "wei\u00b7ter", "Fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ADV", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der eine ist verliebt gar sehr,", "tokens": ["Der", "ei\u00b7ne", "ist", "ver\u00b7liebt", "gar", "sehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VAFIN", "VVPP", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der andre w\u00e4r' es gerne.", "tokens": ["Der", "and\u00b7re", "w\u00e4r'", "es", "ger\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VAFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die stehn allhier im kalten Wind", "tokens": ["Die", "stehn", "all\u00b7hier", "im", "kal\u00b7ten", "Wind"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und singen sch\u00f6n und geigen:", "tokens": ["Und", "sin\u00b7gen", "sch\u00f6n", "und", "gei\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "KON", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ob nicht ein s\u00fc\u00dfvertr\u00e4umtes Kind", "tokens": ["Ob", "nicht", "ein", "s\u00fc\u00df\u00b7ver\u00b7tr\u00e4um\u00b7tes", "Kind"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PTKNEG", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Am Fenster sich wollt' zeigen?", "tokens": ["Am", "Fens\u00b7ter", "sich", "wollt'", "zei\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PRF", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}