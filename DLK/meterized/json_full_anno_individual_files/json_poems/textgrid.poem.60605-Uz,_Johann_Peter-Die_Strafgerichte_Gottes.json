{"textgrid.poem.60605": {"metadata": {"author": {"name": "Uz, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "Die Strafgerichte Gottes", "genre": "verse", "period": "N.A.", "pub_year": 1758, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gott ist die Liebe selbst, und seine Menschenhuld", "tokens": ["Gott", "ist", "die", "Lie\u00b7be", "selbst", ",", "und", "sei\u00b7ne", "Men\u00b7schen\u00b7huld"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VAFIN", "ART", "NN", "ADV", "$,", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ist reich an schonender Geduld.", "tokens": ["Ist", "reich", "an", "scho\u00b7nen\u00b7der", "Ge\u00b7duld", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch wann die Erde sich emp\u00f6ret,", "tokens": ["Doch", "wann", "die", "Er\u00b7de", "sich", "em\u00b7p\u00f6\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und allen Lastern dienstbar fr\u00f6hnt,", "tokens": ["Und", "al\u00b7len", "Las\u00b7tern", "dienst\u00b7bar", "fr\u00f6hnt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Entbrennt sein Eifer, und verzehret", "tokens": ["Ent\u00b7brennt", "sein", "Ei\u00b7fer", ",", "und", "ver\u00b7zeh\u00b7ret"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "KON", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Den S\u00fcnder, der ihn h\u00f6hnt.", "tokens": ["Den", "S\u00fcn\u00b7der", ",", "der", "ihn", "h\u00f6hnt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die furchtbarn Plagen stehn auf seinen Wink bereit,", "tokens": ["Die", "furcht\u00b7barn", "Pla\u00b7gen", "stehn", "auf", "sei\u00b7nen", "Wink", "be\u00b7reit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zum Dienste der Gerechtigkeit:", "tokens": ["Zum", "Diens\u00b7te", "der", "Ge\u00b7rech\u00b7tig\u00b7keit", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Krieg im blutigen Gewande", "tokens": ["Der", "Krieg", "im", "blu\u00b7ti\u00b7gen", "Ge\u00b7wan\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Geht w\u00fcrgend aus auf sein Geboth:", "tokens": ["Geht", "w\u00fcr\u00b7gend", "aus", "auf", "sein", "Ge\u00b7both", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Pest fliegt \u00fcber ganze Lande,", "tokens": ["Die", "Pest", "fliegt", "\u00fc\u00b7ber", "gan\u00b7ze", "Lan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Begleitet von dem Tod.", "tokens": ["Be\u00b7glei\u00b7tet", "von", "dem", "Tod", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Es zittert die Natur, wann sich der H\u00f6chste regt:", "tokens": ["Es", "zit\u00b7tert", "die", "Na\u00b7tur", ",", "wann", "sich", "der", "H\u00f6chs\u00b7te", "regt", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PWAV", "PRF", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Erde bebt und wird bewegt,", "tokens": ["Die", "Er\u00b7de", "bebt", "und", "wird", "be\u00b7wegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn auf den Fittigen der Winde", "tokens": ["Wenn", "auf", "den", "Fit\u00b7ti\u00b7gen", "der", "Win\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gott unter schwarzen Wolken geht,", "tokens": ["Gott", "un\u00b7ter", "schwar\u00b7zen", "Wol\u00b7ken", "geht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und eines ganzen Volkes S\u00fcnde", "tokens": ["Und", "ei\u00b7nes", "gan\u00b7zen", "Vol\u00b7kes", "S\u00fcn\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Vor seinem Antlitz steht.", "tokens": ["Vor", "sei\u00b7nem", "Ant\u00b7litz", "steht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ein Ungewitter braust, mit ungest\u00fcmem Lauf,", "tokens": ["Ein", "Un\u00b7ge\u00b7wit\u00b7ter", "braust", ",", "mit", "un\u00b7ge\u00b7st\u00fc\u00b7mem", "Lauf", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Auch \u00fcber uns vom Herrn herauf!", "tokens": ["Auch", "\u00fc\u00b7ber", "uns", "vom", "Herrn", "her\u00b7auf", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gott Zebaoth will uns vernichten!", "tokens": ["Gott", "Ze\u00b7bao\u00b7th", "will", "uns", "ver\u00b7nich\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Doch la\u00dft uns ihm entgegen gehn,", "tokens": ["Doch", "la\u00dft", "uns", "ihm", "ent\u00b7ge\u00b7gen", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "PPER", "PTKVZ", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und seinen drohenden Gerichten", "tokens": ["Und", "sei\u00b7nen", "dro\u00b7hen\u00b7den", "Ge\u00b7rich\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Durch Bu\u00dfe widerstehn!", "tokens": ["Durch", "Bu\u00b7\u00dfe", "wi\u00b7der\u00b7stehn", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "O schone, schone noch! Vertilg uns nicht, als Feind,", "tokens": ["O", "scho\u00b7ne", ",", "scho\u00b7ne", "noch", "!", "Ver\u00b7tilg", "uns", "nicht", ",", "als", "Feind", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "VVFIN", "ADV", "$.", "NN", "PPER", "PTKNEG", "$,", "KOUS", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gott, unser Sch\u00f6pfer, unser Freund!", "tokens": ["Gott", ",", "un\u00b7ser", "Sch\u00f6p\u00b7fer", ",", "un\u00b7ser", "Freund", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du d\u00fcrstest nicht nach unserm Blute:", "tokens": ["Du", "d\u00fcrs\u00b7test", "nicht", "nach", "un\u00b7serm", "Blu\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nimmt aber Bosheit \u00fcberhand,", "tokens": ["Nimmt", "a\u00b7ber", "Bos\u00b7heit", "\u00fc\u00b7ber\u00b7hand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So besserst du mit sch\u00e4rfrer Ruthe", "tokens": ["So", "bes\u00b7serst", "du", "mit", "sch\u00e4r\u00b7frer", "Ru\u00b7the"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ein ungehorsam Land.", "tokens": ["Ein", "un\u00b7ge\u00b7hor\u00b7sam", "Land", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "So ruchlos ist die Welt, als herrschte Gott nicht hier!", "tokens": ["So", "ruch\u00b7los", "ist", "die", "Welt", ",", "als", "herrschte", "Gott", "nicht", "hier", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "ART", "NN", "$,", "KOUS", "ADJA", "NN", "PTKNEG", "ADV", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ihr S\u00fcnder, soll er seyn, wie ihr,", "tokens": ["Ihr", "S\u00fcn\u00b7der", ",", "soll", "er", "seyn", ",", "wie", "ihr", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VMFIN", "PPER", "VAINF", "$,", "PWAV", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und schweigen, da die Unschuld schreyet,", "tokens": ["Und", "schwei\u00b7gen", ",", "da", "die", "Un\u00b7schuld", "schre\u00b7yet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Und ihr den Armen unterdr\u00fcckt,", "tokens": ["Und", "ihr", "den", "Ar\u00b7men", "un\u00b7ter\u00b7dr\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Unzucht euch zu Sklaven weihet,", "tokens": ["Der", "Un\u00b7zucht", "euch", "zu", "Skla\u00b7ven", "wei\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und euch mit Schande schm\u00fcckt?", "tokens": ["Und", "euch", "mit", "Schan\u00b7de", "schm\u00fcckt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Der Allerheiligste, den ganze Rotten schm\u00e4hn,", "tokens": ["Der", "Al\u00b7ler\u00b7hei\u00b7ligs\u00b7te", ",", "den", "gan\u00b7ze", "Rot\u00b7ten", "schm\u00e4hn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Soll eure Gr\u00e4uel schweigend sehn,", "tokens": ["Soll", "eu\u00b7re", "Gr\u00e4u\u00b7el", "schwei\u00b7gend", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wann ihr das Recht um Geld verhandelt,", "tokens": ["Wann", "ihr", "das", "Recht", "um", "Geld", "ver\u00b7han\u00b7delt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Euch mit der Wittwen Erndte speist;", "tokens": ["Euch", "mit", "der", "Witt\u00b7wen", "Ernd\u00b7te", "speist", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wann jeder Frevel nackend wandelt,", "tokens": ["Wann", "je\u00b7der", "Fre\u00b7vel", "na\u00b7ckend", "wan\u00b7delt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und nicht mehr Frevel hei\u00dft?", "tokens": ["Und", "nicht", "mehr", "Fre\u00b7vel", "hei\u00dft", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Erwartet ihr von Gott, in ganz verderbter Zeit,", "tokens": ["Er\u00b7war\u00b7tet", "ihr", "von", "Gott", ",", "in", "ganz", "ver\u00b7derb\u00b7ter", "Zeit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "$,", "APPR", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nur G\u00fcte, nicht Gerechtigkeit?", "tokens": ["Nur", "G\u00fc\u00b7te", ",", "nicht", "Ge\u00b7rech\u00b7tig\u00b7keit", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "PTKNEG", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Erde soll sein Lob verk\u00fcnden;", "tokens": ["Die", "Er\u00b7de", "soll", "sein", "Lob", "ver\u00b7k\u00fcn\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Er offenbart sich durch die Welt:", "tokens": ["Er", "of\u00b7fen\u00b7bart", "sich", "durch", "die", "Welt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ihr aber habt, mit schwarzen S\u00fcnden,", "tokens": ["Ihr", "a\u00b7ber", "habt", ",", "mit", "schwar\u00b7zen", "S\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der Sch\u00f6pfung Reiz entstellt!", "tokens": ["Der", "Sch\u00f6p\u00b7fung", "Reiz", "ent\u00b7stellt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Entwaffnet seinen Grimm! Der Bogen liegt gespannt,", "tokens": ["Ent\u00b7waff\u00b7net", "sei\u00b7nen", "Grimm", "!", "Der", "Bo\u00b7gen", "liegt", "ge\u00b7spannt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NE", "$.", "ART", "NN", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In seiner aufgehobnen Hand.", "tokens": ["In", "sei\u00b7ner", "auf\u00b7ge\u00b7hob\u00b7nen", "Hand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bald holt er, mit entflammten Pfeilen,", "tokens": ["Bald", "holt", "er", ",", "mit", "ent\u00b7flamm\u00b7ten", "Pfei\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Euch auf dem Wege Sodoms ein:", "tokens": ["Euch", "auf", "dem", "We\u00b7ge", "So\u00b7doms", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "NE", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dann werdet ihr um H\u00fclfe heulen,", "tokens": ["Dann", "wer\u00b7det", "ihr", "um", "H\u00fcl\u00b7fe", "heu\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV"