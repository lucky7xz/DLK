{"textgrid.poem.60457": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Die Nonne und die Rose", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dunkle Wolken niederdrohten,", "tokens": ["Dunk\u00b7le", "Wol\u00b7ken", "nie\u00b7der\u00b7droh\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und es zuckten Wetterscheine,", "tokens": ["Und", "es", "zuck\u00b7ten", "Wet\u00b7ter\u00b7schei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Brausend jagten schon die Boten", "tokens": ["Brau\u00b7send", "jag\u00b7ten", "schon", "die", "Bo\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVPP", "VVFIN", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Des Gewitters durch die Haine.", "tokens": ["Des", "Ge\u00b7wit\u00b7ters", "durch", "die", "Hai\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Eine Rose dort am Aste,", "tokens": ["Ei\u00b7ne", "Ro\u00b7se", "dort", "am", "As\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sch\u00f6ne Nonne, sahst du beben,", "tokens": ["Sch\u00f6\u00b7ne", "Non\u00b7ne", ",", "sahst", "du", "be\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "PP