{"textgrid.poem.61466": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "19.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auch der Schmerz ist Gottes Bote; ernster Mahnung heil'ge Worte", "tokens": ["Auch", "der", "Schmerz", "ist", "Got\u00b7tes", "Bo\u00b7te", ";", "erns\u00b7ter", "Mah\u00b7nung", "heil'\u00b7ge", "Wor\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VAFIN", "NN", "NN", "$.", "ADJA", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Bringt er uns und \u00f6ffnet leise tiefgeheimer Weisheit Pforte.", "tokens": ["Bringt", "er", "uns", "und", "\u00f6ff\u00b7net", "lei\u00b7se", "tief\u00b7ge\u00b7hei\u00b7mer", "Weis\u00b7heit", "Pfor\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "KON", "VVFIN", "ADJD", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Aber unser irrend Auge, vielgetr\u00fcbt vom Staub der M\u00e4ngel,", "tokens": ["A\u00b7ber", "un\u00b7ser", "ir\u00b7rend", "Au\u00b7ge", ",", "viel\u00b7ge\u00b7tr\u00fcbt", "vom", "Staub", "der", "M\u00e4n\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJD", "NN", "$,", "ADV", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Nicht erkennt es in der dunkeln Schattentracht sogleich den Engel.", "tokens": ["Nicht", "er\u00b7kennt", "es", "in", "der", "dun\u00b7keln", "Schat\u00b7ten\u00b7tracht", "sog\u00b7leich", "den", "En\u00b7gel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.5": {"text": "Da\u00df sein bittrer Kelch uns fromme, ach, es d\u00fcnkt uns eitles W\u00e4hnen,", "tokens": ["Da\u00df", "sein", "bit\u00b7trer", "Kelch", "uns", "from\u00b7me", ",", "ach", ",", "es", "d\u00fcnkt", "uns", "eit\u00b7les", "W\u00e4h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "PPER", "ADJA", "$,", "ITJ", "$,", "PPER", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.6": {"text": "Und das eigne Heil mi\u00dfachtend, gr\u00fc\u00dfen wir's mit hei\u00dfen Tr\u00e4nen.", "tokens": ["Und", "das", "eig\u00b7ne", "Heil", "mi\u00df\u00b7ach\u00b7tend", ",", "gr\u00fc\u00b7\u00dfen", "wir's", "mit", "hei\u00b7\u00dfen", "Tr\u00e4\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVPP", "$,", "VVFIN", "PIS", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.7": {"text": "Erst wenn scheidend der Verh\u00fcllte wiederum sich von uns wendet,", "tokens": ["Erst", "wenn", "schei\u00b7dend", "der", "Ver\u00b7h\u00fcll\u00b7te", "wie\u00b7de\u00b7rum", "sich", "von", "uns", "wen\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ADJD", "ART", "NN", "ADV", "PRF", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.8": {"text": "Sehn wir pl\u00f6tzlich \u00fcberm Haupt ihm eine Glorie, die uns blendet.", "tokens": ["Sehn", "wir", "pl\u00f6tz\u00b7lich", "\u00fc\u00b7berm", "Haupt", "ihm", "ei\u00b7ne", "Glo\u00b7rie", ",", "die", "uns", "blen\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPRART", "NN", "PPER", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.9": {"text": "Durch die dunkeln Schleier brechen Silberfl\u00fcgel, klar geteilte,", "tokens": ["Durch", "die", "dun\u00b7keln", "Schlei\u00b7er", "bre\u00b7chen", "Sil\u00b7ber\u00b7fl\u00fc\u00b7gel", ",", "klar", "ge\u00b7teil\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADJA", "NN", "$,", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.10": {"text": "Und die Seele ahnt es schauernd, welch ein Gast bei ihr verweilte.", "tokens": ["Und", "die", "See\u00b7le", "ahnt", "es", "schau\u00b7ernd", ",", "welch", "ein", "Gast", "bei", "ihr", "ver\u00b7weil\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "ADJD", "$,", "PWAT", "ART", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}, "stanza.2": {"line.1": {"text": "Auch der Schmerz ist Gottes Bote; ernster Mahnung heil'ge Worte", "tokens": ["Auch", "der", "Schmerz", "ist", "Got\u00b7tes", "Bo\u00b7te", ";", "erns\u00b7ter", "Mah\u00b7nung", "heil'\u00b7ge", "Wor\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VAFIN", "NN", "NN", "$.", "ADJA", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text"