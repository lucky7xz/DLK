{"dta.poem.3821": {"metadata": {"author": {"name": "Spindler, Christian Gotthold", "birth": "N.A.", "death": "N.A."}, "title": "Gl\u00fcckw\u00fcnschungs-Schreiben,  \n Zum Weyhnacht-Feyertagen, als der Heil. Christ etwas mit-  \n bringen solte.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-20581-9", "language": ["de:0.99"], "booktitle": "Spindler, Christian Gotthold: Unschuldige Jugend-Fr\u00fcchte. Leipzig, 1745."}, "poem": {"stanza.1": {"line.1": {"text": "Da das liebe Weyhnacht-Fest", "tokens": ["Da", "das", "lie\u00b7be", "Wey\u00b7hnacht\u00b7Fest"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sich nun wieder feyren l\u00e4st,", "tokens": ["Sich", "nun", "wie\u00b7der", "fey\u00b7ren", "l\u00e4st", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "ADV", "VVINF", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Lobet doch, mit treuen Lippen,", "tokens": ["Lo\u00b7bet", "doch", ",", "mit", "treu\u00b7en", "Lip\u00b7pen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Unsern JEsum in der Krippen,", "tokens": ["Un\u00b7sern", "Je\u00b7sum", "in", "der", "Krip\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "APPR", "ART", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.5": {"text": "Der zwar arm im Strohe liegt,", "tokens": ["Der", "zwar", "arm", "im", "Stro\u00b7he", "liegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Doch die Armuth selbst besiegt.", "tokens": ["Doch", "die", "Ar\u00b7muth", "selbst", "be\u00b7siegt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ist er gleich sehr schlecht erschienen,", "tokens": ["Ist", "er", "gleich", "sehr", "schlecht", "er\u00b7schie\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Der, dem alle Himmel dienen,", "tokens": ["Der", ",", "dem", "al\u00b7le", "Him\u00b7mel", "die\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PRELS", "PIAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Dient ein Stall ihm zum Pallast,", "tokens": ["Dient", "ein", "Stall", "ihm", "zum", "Pal\u00b7last", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Ist er auf der Welt ein Gast,", "tokens": ["Ist", "er", "auf", "der", "Welt", "ein", "Gast", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Will ihn niemand hier bewirthen,", "tokens": ["Will", "ihn", "nie\u00b7mand", "hier", "be\u00b7wirt\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PIS", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Dient ihm niemand, als die Hirten,", "tokens": ["Dient", "ihm", "nie\u00b7mand", ",", "als", "die", "Hir\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIS", "$,", "KOUS", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Mu\u00df er hier verachtet seyn?", "tokens": ["Mu\u00df", "er", "hier", "ver\u00b7ach\u00b7tet", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "VVPP", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "Nein, o sch\u00f6nster JEsu! nein.", "tokens": ["Nein", ",", "o", "sch\u00f6ns\u00b7ter", "Je\u00b7su", "!", "nein", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PTKANT", "$,", "FM", "ADJA", "NE", "$.", "PTKANT", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "Du wirst meine Bitt gew\u00e4hren,", "tokens": ["Du", "wirst", "mei\u00b7ne", "Bitt", "ge\u00b7w\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Eile, bey mir einzukehren,", "tokens": ["Ei\u00b7le", ",", "bey", "mir", "ein\u00b7zu\u00b7keh\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "PPER", "VVIZU", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Komm, o HErr! aus Davids Stamm,", "tokens": ["Komm", ",", "o", "Herr", "!", "aus", "Da\u00b7vids", "Stamm", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "FM", "NN", "$.", "APPR", "NE", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.18": {"text": "Komm, geliebter Br\u00e4utigam!", "tokens": ["Komm", ",", "ge\u00b7lieb\u00b7ter", "Br\u00e4u\u00b7ti\u00b7gam", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJA", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.19": {"text": "Komm, o Herrscher aller Thronen!", "tokens": ["Komm", ",", "o", "Herr\u00b7scher", "al\u00b7ler", "Thro\u00b7nen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "FM", "NN", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Eile nur bey mir zu wohnen.", "tokens": ["Ei\u00b7le", "nur", "bey", "mir", "zu", "woh\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "PPER", "PTKZU", "VVINF", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.21": {"text": "Herr! die Klarheit deiner Nacht", "tokens": ["Herr", "!", "die", "Klar\u00b7heit", "dei\u00b7ner", "Nacht"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.22": {"text": "Hat mich ausser sich gebracht.", "tokens": ["Hat", "mich", "aus\u00b7ser", "sich", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "PRF", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.23": {"text": "Seelen-Printz, aus Davids Zweigen,", "tokens": ["See\u00b7len\u00b7Printz", ",", "aus", "Da\u00b7vids", "Zwei\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "NE", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "La\u00df mein Flehen dich erweichen,", "tokens": ["La\u00df", "mein", "Fle\u00b7hen", "dich", "er\u00b7wei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "Denn ich wei\u00df, da\u00df du, o F\u00fcrst!", "tokens": ["Denn", "ich", "wei\u00df", ",", "da\u00df", "du", ",", "o", "F\u00fcrst", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "KOUS", "PPER", "$,", "FM", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.26": {"text": "Auch mich nicht verachten wirst.", "tokens": ["Auch", "mich", "nicht", "ver\u00b7ach\u00b7ten", "wirst", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "PTKNEG", "VVFIN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Freund! so singet nun mein Hertze,", "tokens": ["Freund", "!", "so", "sin\u00b7get", "nun", "mein", "Hert\u00b7ze", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ADV", "VVFIN", "ADV", "PPOSAT", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit dem angenehmsten Schertze,", "tokens": ["Mit", "dem", "an\u00b7ge\u00b7nehms\u00b7ten", "Schert\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da der liebe Heilge Christ", "tokens": ["Da", "der", "lie\u00b7be", "Heil\u00b7ge", "Christ"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wieder vor der Th\u00fcren ist.", "tokens": ["Wie\u00b7der", "vor", "der", "Th\u00fc\u00b7ren", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Nun ich kenn auch dein Gem\u00fcthe,", "tokens": ["Nun", "ich", "kenn", "auch", "dein", "Ge\u00b7m\u00fc\u00b7the", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Das die \u00fcberreiche G\u00fcte", "tokens": ["Das", "die", "\u00fc\u00b7berr\u00b7ei\u00b7che", "G\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Deines grossen Sch\u00f6pffers prei\u00dft,", "tokens": ["Dei\u00b7nes", "gros\u00b7sen", "Sch\u00f6pf\u00b7fers", "prei\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Da er sich so treu erweist,", "tokens": ["Da", "er", "sich", "so", "treu", "er\u00b7weist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Da du bey den Feyertagen", "tokens": ["Da", "du", "bey", "den", "Fe\u00b7yer\u00b7ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Nichts von Angst und Noth kanst sagen.", "tokens": ["Nichts", "von", "Angst", "und", "Noth", "kanst", "sa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "NN", "KON", "NN", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Nun, des H\u00f6chsten Vater-Hand,", "tokens": ["Nun", ",", "des", "H\u00f6chs\u00b7ten", "Va\u00b7ter\u00b7Hand", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Der es gn\u00e4digst abgewandt,", "tokens": ["Der", "es", "gn\u00e4\u00b7digst", "ab\u00b7ge\u00b7wandt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Der dir deinen Wunsch gew\u00e4hret,", "tokens": ["Der", "dir", "dei\u00b7nen", "Wunsch", "ge\u00b7w\u00e4h\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Gebe was dein Hertz begehret.", "tokens": ["Ge\u00b7be", "was", "dein", "Hertz", "be\u00b7ge\u00b7hret", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.15": {"text": "Bring di\u00df Fest in froher Ruh,", "tokens": ["Bring", "di\u00df", "Fest", "in", "fro\u00b7her", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDS", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.16": {"text": "Lachen, Schertz und Anmuth zu.", "tokens": ["La\u00b7chen", ",", "Schertz", "und", "An\u00b7muth", "zu", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.17": {"text": "Doch, gedenckt der Himmel deiner,", "tokens": ["Doch", ",", "ge\u00b7denckt", "der", "Him\u00b7mel", "dei\u00b7ner", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VVFIN", "ART", "NN", "PPOSAT", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Ey, so dencke du auch meiner,", "tokens": ["Ey", ",", "so", "den\u00b7cke", "du", "auch", "mei\u00b7ner", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VVFIN", "PPER", "ADV", "PPOSAT", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Da\u00df die angenehmste Nacht", "tokens": ["Da\u00df", "die", "an\u00b7ge\u00b7nehms\u00b7te", "Nacht"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.20": {"text": "Auf mich, Armen, mit bedacht,", "tokens": ["Auf", "mich", ",", "Ar\u00b7men", ",", "mit", "be\u00b7dacht", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "NN", "$,", "APPR", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.21": {"text": "Wenn du mir l\u00e4st fein bescheeren.", "tokens": ["Wenn", "du", "mir", "l\u00e4st", "fein", "be\u00b7schee\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "ADJD", "VVINF", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.22": {"text": "Nun die Zeit wird alles lehren.", "tokens": ["Nun", "die", "Zeit", "wird", "al\u00b7les", "leh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "PIS", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}