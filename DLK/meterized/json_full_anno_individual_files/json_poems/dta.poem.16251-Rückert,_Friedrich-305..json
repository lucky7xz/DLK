{"dta.poem.16251": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "305.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Buch, gelesenes, bringt dir die Welt ins Haus,", "tokens": ["Ein", "Buch", ",", "ge\u00b7le\u00b7se\u00b7nes", ",", "bringt", "dir", "die", "Welt", "ins", "Haus", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJA", "$,", "VVFIN", "PPER", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und ein geschriebnes bringt dich in die Welt hinaus.", "tokens": ["Und", "ein", "ge\u00b7schrieb\u00b7nes", "bringt", "dich", "in", "die", "Welt", "hin\u00b7aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "VVFIN", "PPER", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Gefall' es wohl dem Gl\u00fcck, und m\u00f6g' es dir gelingen,", "tokens": ["Ge\u00b7fall'", "es", "wohl", "dem", "Gl\u00fcck", ",", "und", "m\u00f6g'", "es", "dir", "ge\u00b7lin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "ART", "NN", "$,", "KON", "VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dir immer sch\u00f6n die Welt, dich sch\u00f6n der Welt zu bringen.", "tokens": ["Dir", "im\u00b7mer", "sch\u00f6n", "die", "Welt", ",", "dich", "sch\u00f6n", "der", "Welt", "zu", "brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "wor