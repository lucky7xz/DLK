{"dta.poem.14858": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Die stille Gemeine.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "\u201eich seh' ihn noch: wie durch den Sturm", "tokens": ["\u201e", "ich", "seh'", "ihn", "noch", ":", "wie", "durch", "den", "Sturm"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VVFIN", "PPER", "ADV", "$.", "PWAV", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zwischen den feur'gen Zungen", "tokens": ["Zwi\u00b7schen", "den", "feur'\u00b7gen", "Zun\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mein stolzer Vater da vom Thurm", "tokens": ["Mein", "stol\u00b7zer", "Va\u00b7ter", "da", "vom", "Thurm"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sein Banner hat geschwungen.\u201c", "tokens": ["Sein", "Ban\u00b7ner", "hat", "ge\u00b7schwun\u00b7gen", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}