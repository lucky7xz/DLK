{"dta.poem.557": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Die Wollust an das Frauen-Zimmer.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Jhr sch\u00f6nes Jungfern - Volck/ ihr holden Anmuths-", "tokens": ["Ihr", "sch\u00f6\u00b7nes", "Jung\u00b7fern", "Volck", "/", "ihr", "hol\u00b7den", "An\u00b7muths"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$(", "NN", "$(", "PPOSAT", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Schwaanen/", "tokens": ["Schwaa\u00b7nen", "/"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Jhr seyd recht wunder-sch\u00f6n mit Pracht und Schmuck", "tokens": ["Ihr", "seyd", "recht", "wun\u00b7der\u00b7sch\u00f6n", "mit", "Pracht", "und", "Schmuck"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "geziert/", "tokens": ["ge\u00b7ziert", "/"], "token_info": ["word", "punct"], "pos": ["VVPP", "$("], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "Schaut auf das Liljen-Feld/ das auf den Brust-Altanen", "tokens": ["Schaut", "auf", "das", "Lil\u00b7jen\u00b7Feld", "/", "das", "auf", "den", "Brust\u00b7Al\u00b7ta\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "$(", "PDS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Mit Rosen untermischt die Anmuth aufgef\u00fchrt", "tokens": ["Mit", "Ro\u00b7sen", "un\u00b7ter\u00b7mischt", "die", "An\u00b7muth", "auf\u00b7ge\u00b7f\u00fchrt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "H\u00f6rt was mein kluger Mund vor s\u00fcsse Liebes-Lehren/", "tokens": ["H\u00f6rt", "was", "mein", "klu\u00b7ger", "Mund", "vor", "s\u00fcs\u00b7se", "Lie\u00b7bes\u00b7Leh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PPOSAT", "ADJA", "NN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Der Freund-und Lieblichkeit euch jetzo geben will.", "tokens": ["Der", "Freun\u00b7d\u00b7und", "Lieb\u00b7lich\u00b7keit", "euch", "jet\u00b7zo", "ge\u00b7ben", "will", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PPER", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Jhr seyd das beste Gut/ der M\u00e4nner ihr Begehren/", "tokens": ["Ihr", "seyd", "das", "bes\u00b7te", "Gut", "/", "der", "M\u00e4n\u00b7ner", "ihr", "Be\u00b7geh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$(", "ART", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Wohin ihr W\u00fcnschen geht/ und der Gedancken-Ziel.", "tokens": ["Wo\u00b7hin", "ihr", "W\u00fcn\u00b7schen", "geht", "/", "und", "der", "Ge\u00b7dan\u00b7cken\u00b7Ziel", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "VVFIN", "$(", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Sie m\u00fcssen euch zu Fu\u00df in h\u00f6chster Demuth fallen/", "tokens": ["Sie", "m\u00fcs\u00b7sen", "euch", "zu", "Fu\u00df", "in", "h\u00f6chs\u00b7ter", "De\u00b7muth", "fal\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "APPR", "NN", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Jhr siegt das harsche Hertz der wilden Krieger an", "tokens": ["Ihr", "siegt", "das", "har\u00b7sche", "Hertz", "der", "wil\u00b7den", "Krie\u00b7ger", "an"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "APPR"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Jhr spielt mit starcker Gluth/ und werffet Feuer-Ballen", "tokens": ["Ihr", "spielt", "mit", "star\u00b7cker", "Gluth", "/", "und", "werf\u00b7fet", "Feu\u00b7er\u00b7Bal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$(", "KON", "VVFIN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Damit man also fort ein Hertz erobern kan.", "tokens": ["Da\u00b7mit", "man", "al\u00b7so", "fort", "ein", "Hertz", "er\u00b7o\u00b7bern", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "PTKVZ", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Entbl\u00f6sset eure Brust die ", "tokens": ["Ent\u00b7bl\u00f6s\u00b7set", "eu\u00b7re", "Brust", "die"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.16": {"text": "Den Geistern m\u00fcssen sie der Liebe-Koder seyn/", "tokens": ["Den", "Geis\u00b7tern", "m\u00fcs\u00b7sen", "sie", "der", "Lie\u00b7be\u00b7Ko\u00b7der", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "ART", "NN", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Sie sehn da Schnee und Blut zusammen Hochzeit machen/", "tokens": ["Sie", "sehn", "da", "Schnee", "und", "Blut", "zu\u00b7sam\u00b7men", "Hoch\u00b7zeit", "ma\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "NN", "KON", "NN", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Die Anmuth ladet hier die G\u00e4ste selber ein.", "tokens": ["Die", "An\u00b7muth", "la\u00b7det", "hier", "die", "G\u00e4s\u00b7te", "sel\u00b7ber", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.20": {"text": "Ob sie ein ", "tokens": ["Ob", "sie", "ein"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPER", "ART"], "meter": "+-+", "measure": "trochaic.di"}, "line.21": {"text": "Die Perlen/ die man sieht an euren Br\u00fcsten hangen/", "tokens": ["Die", "Per\u00b7len", "/", "die", "man", "sieht", "an", "eu\u00b7ren", "Br\u00fcs\u00b7ten", "han\u00b7gen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PRELS", "PIS", "VVFIN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Die k\u00fchlen wenn man sich den matten Geist verbrennt.", "tokens": ["Die", "k\u00fch\u00b7len", "wenn", "man", "sich", "den", "mat\u00b7ten", "Geist", "ver\u00b7brennt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "KOUS", "PIS", "PRF", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "So putzt sie denn heraus/ entbl\u00f6\u00dft euch in die Wette/", "tokens": ["So", "putzt", "sie", "denn", "he\u00b7raus", "/", "ent\u00b7bl\u00f6\u00dft", "euch", "in", "die", "Wet\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PTKVZ", "$(", "VVFIN", "PPER", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Und seyd mit eurem Schatz nicht \u00fcberm\u00e4\u00dfig rar/", "tokens": ["Und", "seyd", "mit", "eu\u00b7rem", "Schatz", "nicht", "\u00fc\u00b7berm\u00b7\u00e4\u00b7\u00dfig", "rar", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "PPOSAT", "NN", "PTKNEG", "ADJD", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Er machet euch beliebt/ und bringet euch zu Bette/", "tokens": ["Er", "ma\u00b7chet", "euch", "be\u00b7liebt", "/", "und", "brin\u00b7get", "euch", "zu", "Bet\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "$(", "KON", "VVFIN", "PPER", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Und so verkauffet ihr dann theur genug die Waar.", "tokens": ["Und", "so", "ver\u00b7kauf\u00b7fet", "ihr", "dann", "theur", "ge\u00b7nug", "die", "Waar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADV", "ADJD", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Schneidt vorn die Kleider weg/ die Spitzen weggenommen/", "tokens": ["Schneidt", "vorn", "die", "Klei\u00b7der", "weg", "/", "die", "Spit\u00b7zen", "weg\u00b7ge\u00b7nom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "NN", "PTKVZ", "$(", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Den Anfang habt ihr zwar/ doch nicht genug gemacht/", "tokens": ["Den", "An\u00b7fang", "habt", "ihr", "zwar", "/", "doch", "nicht", "ge\u00b7nug", "ge\u00b7macht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "$(", "ADV", "PTKNEG", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Die Br\u00fcste m\u00fcssen blo\u00df bis an die Wartzen kommen/", "tokens": ["Die", "Br\u00fcs\u00b7te", "m\u00fcs\u00b7sen", "blo\u00df", "bis", "an", "die", "Wart\u00b7zen", "kom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "ADV", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Denn wird der sch\u00f6ne Schmuck ans Tages-Licht gebracht.", "tokens": ["Denn", "wird", "der", "sch\u00f6\u00b7ne", "Schmuck", "ans", "Ta\u00b7ges\u00b7Licht", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "ADJA", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Auf/ schn\u00fcret in die H\u00f6h die weissen Liebes-Ballen/", "tokens": ["Auf", "/", "schn\u00fc\u00b7ret", "in", "die", "H\u00f6h", "die", "weis\u00b7sen", "Lie\u00b7bes\u00b7Bal\u00b7len", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$(", "VVFIN", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Versuchts auf alle Art/ macht Hals und Br\u00fcste blo\u00df/", "tokens": ["Ver\u00b7suchts", "auf", "al\u00b7le", "Art", "/", "macht", "Hals", "und", "Br\u00fcs\u00b7te", "blo\u00df", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PIAT", "NN", "$(", "VVFIN", "NN", "KON", "NN", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "So mu\u00df jedwedes Hertz wie hart es ist doch fallen/", "tokens": ["So", "mu\u00df", "jed\u00b7we\u00b7des", "Hertz", "wie", "hart", "es", "ist", "doch", "fal\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIAT", "NN", "KOKOM", "ADJD", "PPER", "VAFIN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Die Macht des ", "tokens": ["Die", "Macht", "des"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.35": {"text": "So kan man keinen Blick in euren Busen f\u00fchren/", "tokens": ["So", "kan", "man", "kei\u00b7nen", "Blick", "in", "eu\u00b7ren", "Bu\u00b7sen", "f\u00fch\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "PIAT", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Da\u00df nicht ein freyer Sinn in Sclaven-Ketten \u00e4chtzt", "tokens": ["Da\u00df", "nicht", "ein", "frey\u00b7er", "Sinn", "in", "Scla\u00b7ven\u00b7Ket\u00b7ten", "\u00e4chtzt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PTKNEG", "ART", "ADJA", "NN", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Nicht einer kan sein Heyl an solcher Macht probiren", "tokens": ["Nicht", "ei\u00b7ner", "kan", "sein", "Heyl", "an", "sol\u00b7cher", "Macht", "pro\u00b7bi\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "PIS", "VMFIN", "PPOSAT", "NN", "APPR", "PIAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Der nicht/ wenn sie sich zeigt/ nach ihrer Gnade l\u00e4chtzt.", "tokens": ["Der", "nicht", "/", "wenn", "sie", "sich", "zeigt", "/", "nach", "ih\u00b7rer", "Gna\u00b7de", "l\u00e4chtzt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "$(", "KOUS", "PPER", "PRF", "VVFIN", "$(", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Wie! wenn ein sanffter Wind auf diesen Atla\u00df wehet/", "tokens": ["Wie", "!", "wenn", "ein", "sanff\u00b7ter", "Wind", "auf", "die\u00b7sen", "At\u00b7la\u00df", "we\u00b7het", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "KOUS", "ART", "ADJA", "NN", "APPR", "PDAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "So h\u00e4lt die Lieblichkeit da ihren Tummel Platz/", "tokens": ["So", "h\u00e4lt", "die", "Lieb\u00b7lich\u00b7keit", "da", "ih\u00b7ren", "Tum\u00b7mel", "Platz", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "PPOSAT", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Die Anmuth mit der Lust auf Rosen Bl\u00e4ttern gehet", "tokens": ["Die", "An\u00b7muth", "mit", "der", "Lust", "auf", "Ro\u00b7sen", "Bl\u00e4t\u00b7tern", "ge\u00b7het"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN", "APPR", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Die Artigkeit vermehrt den ungemeinen Schatz.", "tokens": ["Die", "Ar\u00b7tig\u00b7keit", "ver\u00b7mehrt", "den", "un\u00b7ge\u00b7mei\u00b7nen", "Schatz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Und wenn ihr nun dadurch die Ober-Herrschafft funden/", "tokens": ["Und", "wenn", "ihr", "nun", "da\u00b7durch", "die", "O\u00b7ber\u00b7Herr\u00b7schafft", "fun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "PAV", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Die euch/ beliebtes Volck doch schon in H\u00e4nden ist/", "tokens": ["Die", "euch", "/", "be\u00b7lieb\u00b7tes", "Volck", "doch", "schon", "in", "H\u00e4n\u00b7den", "ist", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "$(", "ADJA"