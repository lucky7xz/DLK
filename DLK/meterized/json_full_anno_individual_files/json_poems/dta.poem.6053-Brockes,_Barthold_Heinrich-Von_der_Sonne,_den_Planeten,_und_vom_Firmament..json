{"dta.poem.6053": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Sonne, den Planeten,  \n und vom Firmament.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.85", "af:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn wir die Sonn' in beyden Sonnen-Wenden,", "tokens": ["Wenn", "wir", "die", "Sonn'", "in", "bey\u00b7den", "Son\u00b7nen\u00b7Wen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie es uns vorkommt, sehn;", "tokens": ["Wie", "es", "uns", "vor\u00b7kommt", ",", "sehn", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "VVFIN", "$,", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So w\u00fcrckt dies alles doch allein der Erde Drehn.", "tokens": ["So", "w\u00fcrckt", "dies", "al\u00b7les", "doch", "al\u00b7lein", "der", "Er\u00b7de", "Drehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PDS", "PIS", "ADV", "ADV", "ART", "NN", "CARD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Denn, wenn sich die gemessne Schrancken enden,", "tokens": ["Denn", ",", "wenn", "sich", "die", "ge\u00b7mess\u00b7ne", "Schran\u00b7cken", "en\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PRF", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Es mag beym Steinbock so, als auch beym Krebs geschehn;", "tokens": ["Es", "mag", "beym", "Stein\u00b7bock", "so", ",", "als", "auch", "beym", "Krebs", "ge\u00b7schehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPRART", "NN", "ADV", "$,", "KOUS", "ADV", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So scheint im Gegen-Lauff die Sonn\u2019 in jenen H\u00f6hen,", "tokens": ["So", "scheint", "im", "Ge\u00b7gen\u00b7Lauff", "die", "Sonn'", "in", "je\u00b7nen", "H\u00f6\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "ART", "NN", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "So viele Grade fortzugehen.", "tokens": ["So", "vie\u00b7le", "Gra\u00b7de", "fort\u00b7zu\u00b7ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Jhr Blick allein verursacht allezeit", "tokens": ["Ihr", "Blick", "al\u00b7lein", "ver\u00b7ur\u00b7sacht", "al\u00b7le\u00b7zeit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADV", "VVFIN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Der Aenderung getheilten Unterscheid.", "tokens": ["Der", "A\u00b7en\u00b7de\u00b7rung", "ge\u00b7theil\u00b7ten", "Un\u00b7ter\u00b7scheid", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "So wie die Erd\u2019 am Sonnen-Strahl", "tokens": ["So", "wie", "die", "Erd'", "am", "Son\u00b7nen\u00b7Strahl"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "In einem halben Jahr die Norder-Seite beut;", "tokens": ["In", "ei\u00b7nem", "hal\u00b7ben", "Jahr", "die", "Nor\u00b7der\u00b7Sei\u00b7te", "beut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Jm andern halben ihr die S\u00fcder-Seite zeiget,", "tokens": ["Jm", "an\u00b7dern", "hal\u00b7ben", "ihr", "die", "S\u00fc\u00b7der\u00b7Sei\u00b7te", "zei\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "ADJA", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "So zeiget sie gantz richtig allemahl", "tokens": ["So", "zei\u00b7get", "sie", "gantz", "rich\u00b7tig", "al\u00b7le\u00b7mahl"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADJD", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Vest\u00e4ndig\u2019 Aendrung ohne Zahl:", "tokens": ["Ve\u00b7st\u00e4n\u00b7dig'", "A\u00b7en\u00b7drung", "oh\u00b7ne", "Zahl", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPR", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.15": {"text": "Der Sommer folget stets dem grausen Frost,", "tokens": ["Der", "Som\u00b7mer", "fol\u00b7get", "stets", "dem", "grau\u00b7sen", "Frost", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Der Frost folgt abermahl des Sommers schw\u00fclen Glantz:", "tokens": ["Der", "Frost", "folgt", "a\u00b7ber\u00b7mahl", "des", "Som\u00b7mers", "schw\u00fc\u00b7len", "Glantz", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Wenn N\u00e4cht und Tage gleich; so reicht der Herbst uns Most,", "tokens": ["Wenn", "N\u00e4cht", "und", "Ta\u00b7ge", "gleich", ";", "so", "reicht", "der", "Herbst", "uns", "Most", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "ADV", "$.", "ADV", "VVFIN", "ART", "NN", "PPER", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Der Fr\u00fchling manchen Bluhmen-Crantz.", "tokens": ["Der", "Fr\u00fch\u00b7ling", "man\u00b7chen", "Bluh\u00b7men\u00b7Crantz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Durch eben das, was uns der T\u00e4g\u2019 Ungleichheit macht.", "tokens": ["Durch", "e\u00b7ben", "das", ",", "was", "uns", "der", "T\u00e4g'", "Un\u00b7gleich\u00b7heit", "macht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "PDS", "$,", "PRELS", "PPER", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Sind auch die Aendrungen des Jahrs hervorgebracht.", "tokens": ["Sind", "auch", "die", "A\u00b7en\u00b7drun\u00b7gen", "des", "Jahrs", "her\u00b7vor\u00b7ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}}}}