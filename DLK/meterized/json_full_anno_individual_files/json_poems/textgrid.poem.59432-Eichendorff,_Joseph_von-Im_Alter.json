{"textgrid.poem.59432": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Im Alter", "genre": "verse", "period": "N.A.", "pub_year": 1839, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie wird nun alles so stille wieder!", "tokens": ["Wie", "wird", "nun", "al\u00b7les", "so", "stil\u00b7le", "wie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ADV", "PIS", "ADV", "VVFIN", "ADV", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "So war mir's oft in der Kinderzeit,", "tokens": ["So", "war", "mir's", "oft", "in", "der", "Kin\u00b7der\u00b7zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NE", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Die B\u00e4che gehen rauschend nieder", "tokens": ["Die", "B\u00e4\u00b7che", "ge\u00b7hen", "rau\u00b7schend", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJD", "PTKVZ"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Durch die d\u00e4mmernde Einsamkeit,", "tokens": ["Durch", "die", "d\u00e4m\u00b7mern\u00b7de", "Ein\u00b7sam\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.5": {"text": "Kaum noch h\u00f6rt man einen Hirten singen,", "tokens": ["Kaum", "noch", "h\u00f6rt", "man", "ei\u00b7nen", "Hir\u00b7ten", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PIS", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Aus allen D\u00