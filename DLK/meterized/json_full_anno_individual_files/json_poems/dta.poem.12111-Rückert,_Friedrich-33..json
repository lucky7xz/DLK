{"dta.poem.12111": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "33.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Soviel in eurer Art ist einfach, uranf\u00e4nglich;", "tokens": ["So\u00b7viel", "in", "eu\u00b7rer", "Art", "ist", "ein\u00b7fach", ",", "u\u00b7ran\u00b7f\u00e4ng\u00b7lich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PIS", "APPR", "PPOSAT", "NN", "VAFIN", "ADJD", "$,", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Warum? nur weil ihr seid der Bildung unempf\u00e4nglich.", "tokens": ["Wa\u00b7rum", "?", "nur", "weil", "ihr", "seid", "der", "Bil\u00b7dung", "un\u00b7emp\u00b7f\u00e4ng\u00b7lich", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "ADV", "KOUS", "PPER", "VAFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wenn unempf\u00e4nglich nicht, der Bildung doch nicht werth;", "tokens": ["Wenn", "un\u00b7emp\u00b7f\u00e4ng\u00b7lich", "nicht", ",", "der", "Bil\u00b7dung", "doch", "nicht", "werth", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "PTKNEG", "$,", "ART", "NN", "ADV", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ihr seid so wahr wenn roh, so falsch wenn aufgekl\u00e4rt.", "tokens": ["Ihr", "seid", "so", "wahr", "wenn", "roh", ",", "so", "falsch", "wenn", "auf\u00b7ge\u00b7kl\u00e4rt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "KOUS", "ADJD", "$,", "ADV", "ADJD", "KOUS", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Euch scheinet ganz und gar versagt die rechte Mitte,", "tokens": ["Euch", "schei\u00b7net", "ganz", "und", "gar", "ver\u00b7sagt", "die", "rech\u00b7te", "Mit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "KON", "ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Roheit abzuthun ohn' abzuthun die Sitte.", "tokens": ["Die", "Ro\u00b7heit", "ab\u00b7zu\u00b7thun", "ohn'", "ab\u00b7zu\u00b7thun", "die", "Sit\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVIZU", "ADJD", "VVIZU", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}