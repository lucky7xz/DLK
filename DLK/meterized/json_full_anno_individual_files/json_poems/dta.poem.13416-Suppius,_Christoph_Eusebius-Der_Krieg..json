{"dta.poem.13416": {"metadata": {"author": {"name": "Suppius, Christoph Eusebius", "birth": "N.A.", "death": "N.A."}, "title": "Der Krieg.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1749", "urn": "urn:nbn:de:kobv:b4-20594-7", "language": ["de:0.99"], "booktitle": "Suppius, Christoph Eusebius: Oden und Lieder. Gotha, 1749."}, "poem": {"stanza.1": {"line.1": {"text": "Der Krieg f\u00fchrt lauter schlimme Streiche,", "tokens": ["Der", "Krieg", "f\u00fchrt", "lau\u00b7ter", "schlim\u00b7me", "Strei\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die man auch in der Ferne f\u00fchlt,", "tokens": ["Die", "man", "auch", "in", "der", "Fer\u00b7ne", "f\u00fchlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-----+-+", "measure": "unknown.measure.di"}, "line.3": {"text": "Manch Mutterkind macht er zur Leiche,", "tokens": ["Manch", "Mut\u00b7ter\u00b7kind", "macht", "er", "zur", "Lei\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bevor es seinen Muth gek\u00fchlt.", "tokens": ["Be\u00b7vor", "es", "sei\u00b7nen", "Muth", "ge\u00b7k\u00fchlt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Viel Sch\u00f6nen hat er auch betrogen,", "tokens": ["Viel", "Sch\u00f6\u00b7nen", "hat", "er", "auch", "be\u00b7tro\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und ihren holden Br\u00e4utigam,", "tokens": ["Und", "ih\u00b7ren", "hol\u00b7den", "Br\u00e4u\u00b7ti\u00b7gam", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der zwar in Gliedern mitgezogen,", "tokens": ["Der", "zwar", "in", "Glie\u00b7dern", "mit\u00b7ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Doch auf der Liste wiederkam.", "tokens": ["Doch", "auf", "der", "Lis\u00b7te", "wie\u00b7der\u00b7kam", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Den Hochmuth setzet er herunter", "tokens": ["Den", "Hoch\u00b7muth", "set\u00b7zet", "er", "her\u00b7un\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Durch ein gebietrisch Machtgebot,", "tokens": ["Durch", "ein", "ge\u00b7bie\u00b7trisch", "Macht\u00b7ge\u00b7bot", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er ruft der Faulheit, sie wird munter,", "tokens": ["Er", "ruft", "der", "Faul\u00b7heit", ",", "sie", "wird", "mun\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn seine finstre Stirne droht;", "tokens": ["Wenn", "sei\u00b7ne", "finst\u00b7re", "Stir\u00b7ne", "droht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Den Geizigen verhei\u00dft er Beute", "tokens": ["Den", "Gei\u00b7zi\u00b7gen", "ver\u00b7hei\u00dft", "er", "Beu\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Durch Pl\u00fcndern und durch Kaperey,", "tokens": ["Durch", "Pl\u00fcn\u00b7dern", "und", "durch", "Ka\u00b7pe\u00b7rey", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Doch so, da\u00df Jrus Cr\u00f6sus heute,", "tokens": ["Doch", "so", ",", "da\u00df", "Jrus", "Cr\u00f6\u00b7sus", "heu\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "KOUS", "NE", "NE", "ADV", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Und morgen wieder Jrus sey.", "tokens": ["Und", "mor\u00b7gen", "wie\u00b7der", "Jrus", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "NE", "VAFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Jhr, die ihr auf die Meereswellen", "tokens": ["Ihr", ",", "die", "ihr", "auf", "die", "Mee\u00b7res\u00b7wel\u00b7len"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PRELS", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Wohlfahrt eures Hauses baut,", "tokens": ["Die", "Wohl\u00b7fahrt", "eu\u00b7res", "Hau\u00b7ses", "baut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie m\u00fcssen euch die Ohren gellen,", "tokens": ["Wie", "m\u00fcs\u00b7sen", "euch", "die", "Oh\u00b7ren", "gel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wird erst die Hiobs-Botschaft laut,", "tokens": ["Wird", "erst", "die", "Hi\u00b7obs\u00b7Bot\u00b7schaft", "laut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da\u00df euer Schiff mit allen Waaren,", "tokens": ["Da\u00df", "eu\u00b7er", "Schiff", "mit", "al\u00b7len", "Waa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Durch eine kriegerische Macht,", "tokens": ["Durch", "ei\u00b7ne", "krie\u00b7ge\u00b7ri\u00b7sche", "Macht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Bey seinem nun nach Hause Fahren", "tokens": ["Bey", "sei\u00b7nem", "nun", "nach", "Hau\u00b7se", "Fah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADV", "APPR", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Erobert sey und aufgebracht.", "tokens": ["Er\u00b7o\u00b7bert", "sey", "und", "auf\u00b7ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "KON", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Jhr dauret mich, betr\u00fcbte Herzen!", "tokens": ["Ihr", "dau\u00b7ret", "mich", ",", "be\u00b7tr\u00fcb\u00b7te", "Her\u00b7zen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Krieg ist freylich schuld daran,", "tokens": ["Der", "Krieg", "ist", "frey\u00b7lich", "schuld", "da\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich leide mit bey euren Schmerzen,", "tokens": ["Ich", "lei\u00b7de", "mit", "bey", "eu\u00b7ren", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie ich es euch nicht bergen kann;", "tokens": ["Wie", "ich", "es", "euch", "nicht", "ber\u00b7gen", "kann", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "PPER", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Beliebt es euch vielleicht zu fragen:", "tokens": ["Be\u00b7liebt", "es", "euch", "viel\u00b7leicht", "zu", "fra\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Was mir dazu die Ursach gab?", "tokens": ["Was", "mir", "da\u00b7zu", "die", "Ur\u00b7sach", "gab", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PAV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der Coffee ist ja aufgeschlagen,", "tokens": ["Der", "Cof\u00b7fee", "ist", "ja", "auf\u00b7ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und meine Renten nehmen ab!", "tokens": ["Und", "mei\u00b7ne", "Ren\u00b7ten", "neh\u00b7men", "ab", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}