{"dta.poem.21232": {"metadata": {"author": {"name": "Czepko, Daniel von", "birth": "N.A.", "death": "N.A."}, "title": "XiI.  \n  Die R\u00f6mische Cr\u00f6nung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-20871-8", "language": ["de:0.99"], "booktitle": "Czepko, Daniel von: Triumph Bogen. Breslau, 1641."}, "poem": {"stanza.1": {"line.1": {"text": "Als Deutschland/ dessen Macht den R\u00f6mern schrecklich war/", "tokens": ["Als", "Deutschland", "/", "des\u00b7sen", "Macht", "den", "R\u00f6\u00b7mern", "schreck\u00b7lich", "war", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "$(", "PRELAT", "NN", "ART", "NN", "ADJD", "VAFIN", "$("], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Dir wolt/ ", "tokens": ["Dir", "wolt", "/"], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VMFIN", "$("], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Legt\u2019 vmb den ", "tokens": ["Legt'", "vmb", "den"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "APPR", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "zumeiden/\nDie freyen Stimmen ab/ der Deutschen F\u00fcrsten Schaar.", "tokens": ["zu\u00b7mei\u00b7den", "/", "Die", "frey\u00b7en", "Stim\u00b7men", "ab", "/", "der", "Deut\u00b7schen", "F\u00fcrs\u00b7ten", "Schaar", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVINF", "$(", "ART", "ADJA", "NN", "PTKVZ", "$(", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+--+-+-+-+-+-+", "measure": "iambic.septa.relaxed"}}, "stanza.2": {"line.1": {"text": "Was wolt Jhr? (so fieng Wien in Regenspurg drauff an)", "tokens": ["Was", "wolt", "Ihr", "?", "(", "so", "fi\u00b7eng", "Wi\u00b7en", "in", "Re\u00b7gen\u00b7spurg", "drauff", "an", ")"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "$.", "$(", "ADV", "VVFIN", "NE", "APPR", "NN", "PAV", "PTKVZ", "$("], "meter": "-+--+-++-+-+-+", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "O Sein/ O Codan du/ vnd Jhr Antenors spitzen/", "tokens": ["O", "Sein", "/", "O", "Co\u00b7dan", "du", "/", "vnd", "Ihr", "An\u00b7te\u00b7nors", "spit\u00b7zen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "$(", "NE", "NE", "NE", "$(", "KON", "PPOSAT", "NN", "VVINF", "$("], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.3": {"text": "Vnd du ", "tokens": ["Vnd", "du"], "token_info": ["word", "word"], "pos": ["KON", "PPER"], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "Gedencket da\u00df mein Strom blo\u00df ", "tokens": ["Ge\u00b7den\u00b7cket", "da\u00df", "mein", "Strom", "blo\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "KOUS", "PPOSAT", "NN", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Di\u00dfsprach es; vnd al\u00dfbald must\u2019 Vnsers ", "tokens": ["Di\u00df\u00b7sprach", "es", ";", "vnd", "al\u00df\u00b7bald", "must'", "Vn\u00b7sers"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "PPER", "$.", "KON", "ADV", "VMFIN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da Ewer ", "tokens": ["Da", "E\u00b7wer"], "token_info": ["word", "word"], "pos": ["KOUS", "PPOSAT"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Ich glaube/ da\u00df sich GOtt selbst zu der Wahl geschlagen.", "tokens": ["Ich", "glau\u00b7be", "/", "da\u00df", "sich", "Gott", "selbst", "zu", "der", "Wahl", "ge\u00b7schla\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "KOUS", "PRF", "NN", "ADV", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dann machet/ was Jhr wolt/ so bleibt es ", "tokens": ["Dann", "ma\u00b7chet", "/", "was", "Ihr", "wolt", "/", "so", "bleibt", "es"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$(", "PWS", "PPER", "VMFIN", "$(", "ADV", "VVFIN", "PPER"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Die h\u00f6chste Majest\u00e4t stimbt ja mit ", "tokens": ["Die", "h\u00f6chs\u00b7te", "Ma\u00b7jes\u00b7t\u00e4t", "stimbt", "ja", "mit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "APPR"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die herrschet in der Welt; Sie in de\u00df Himmels h\u00f6hen:", "tokens": ["Die", "herr\u00b7schet", "in", "der", "Welt", ";", "Sie", "in", "de\u00df", "Him\u00b7mels", "h\u00f6\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "ART", "NN", "$.", "PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Jedoch wo ", "tokens": ["Je\u00b7doch", "wo"], "token_info": ["word", "word"], "pos": ["ADV", "PWAV"], "meter": "+--", "measure": "dactylic.init"}, "line.4": {"text": "Wird ", "tokens": ["Wird"], "token_info": ["word"], "pos": ["VAFIN"], "meter": "+", "measure": "single.up"}}}}}