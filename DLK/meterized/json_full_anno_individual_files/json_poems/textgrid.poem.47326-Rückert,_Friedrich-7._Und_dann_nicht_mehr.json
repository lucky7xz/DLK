{"textgrid.poem.47326": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "7. Und dann nicht mehr", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich sah sie nur ein einzig Mal, und dann nicht mehr.", "tokens": ["Ich", "sah", "sie", "nur", "ein", "ein\u00b7zig", "Mal", ",", "und", "dann", "nicht", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ART", "ADJD", "NN", "$,", "KON", "ADV", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da sah ich einen Himmelsstrahl, und dann nicht mehr.", "tokens": ["Da", "sah", "ich", "ei\u00b7nen", "Him\u00b7mels\u00b7strahl", ",", "und", "dann", "nicht", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,", "KON", "ADV", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich sah umspielt vom Morgenhauch durchs Thal sie gehn;", "tokens": ["Ich", "sah", "um\u00b7spielt", "vom", "Mor\u00b7gen\u00b7hauch", "durchs", "Thal", "sie", "gehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VVFIN", "APPRART", "NN", "APPRART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da war der Fr\u00fchling in dem Thal, und dann nicht mehr.", "tokens": ["Da", "war", "der", "Fr\u00fch\u00b7ling", "in", "dem", "Thal", ",", "und", "dann", "nicht", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "APPR", "ART", "NN", "$,", "KON", "ADV", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Im Saal des Festes sah ich sie entschleiern sich;", "tokens": ["Im", "Saal", "des", "Fes\u00b7tes", "sah", "ich", "sie", "ent\u00b7schlei\u00b7ern", "sich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "PPER", "PPER", "VVFIN", "PRF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da war das Paradies im Saal, und dann nicht mehr.", "tokens": ["Da", "war", "das", "Pa\u00b7ra\u00b7dies", "im", "Saal", ",", "und", "dann", "nicht", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "APPRART", "NN", "$,", "KON", "ADV", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Sie war die Schenkin, Lust im Kreis kredenzte sie;", "tokens": ["Sie", "war", "die", "Schen\u00b7kin", ",", "Lust", "im", "Kreis", "kre\u00b7denz\u00b7te", "sie", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "NN", "APPRART", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Sie bot mir l\u00e4chelnd eine Schal', und dann nicht mehr.", "tokens": ["Sie", "bot", "mir", "l\u00e4\u00b7chelnd", "ei\u00b7ne", "Schal'", ",", "und", "dann", "nicht", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "ART", "NN", "$,", "KON", "ADV", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Sie war die Ros', ich sah sie bl\u00fchn im Morgentau;", "tokens": ["Sie", "war", "die", "Ros'", ",", "ich", "sah", "sie", "bl\u00fchn", "im", "Mor\u00b7gen\u00b7tau", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PPER", "VVFIN", "PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Am Abend war die Rose fahl, und dann nicht mehr.", "tokens": ["Am", "A\u00b7bend", "war", "die", "Ro\u00b7se", "fahl", ",", "und", "dann", "nicht", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "ART", "NN", "VVFIN", "$,", "KON", "ADV", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Nur einmal weinte G\u00e4rtner Lenz um eine Ros':", "tokens": ["Nur", "ein\u00b7mal", "wein\u00b7te", "G\u00e4rt\u00b7ner", "Lenz", "um", "ei\u00b7ne", "Ros'", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJA", "NN", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Als Tod ihm diese Rose stahl, und dann nicht mehr.", "tokens": ["Als", "Tod", "ihm", "die\u00b7se", "Ro\u00b7se", "stahl", ",", "und", "dann", "nicht", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "PDAT", "NN", "VVFIN", "$,", "KON", "ADV", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ein einz'ges Mal, als sie erblich, war herb die Lust", "tokens": ["Ein", "einz'\u00b7ges", "Mal", ",", "als", "sie", "er\u00b7blich", ",", "war", "herb", "die", "Lust"], "token_info":