{"textgrid.poem.24586": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Uber das h\u00e4ufige Absterben der Kinder", "genre": "verse", "period": "N.A.", "pub_year": 1701, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Himmel rief den Tod/ und sprach: will niemand sterben?", "tokens": ["Der", "Him\u00b7mel", "rief", "den", "Tod", "/", "und", "sprach", ":", "will", "nie\u00b7mand", "ster\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$(", "KON", "VVFIN", "$.", "VMFIN", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df keine Seelen mehr zu mir/ wie sonsten gehn?", "tokens": ["Da\u00df", "kei\u00b7ne", "See\u00b7len", "mehr", "zu", "mir", "/", "wie", "sons\u00b7ten", "gehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "ADV", "APPR", "PPER", "$(", "KOKOM", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Zu Millionen la\u00df ich durch die Pest verderben/", "tokens": ["Zu", "Mil\u00b7lion\u00b7en", "la\u00df", "ich", "durch", "die", "Pest", "ver\u00b7der\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "Erwiederte der Todt; kan auch wohl mehr geschehn?", "tokens": ["Er\u00b7wie\u00b7der\u00b7te", "der", "Todt", ";", "kan", "auch", "wohl", "mehr", "ge\u00b7schehn", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$.", "VMFIN", "ADV", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-++--+", "measure": "iambic.hexa.chol"}, "line.5": {"text": "Allein der Himmel sprach: ich will auch welche haben.", "tokens": ["Al\u00b7lein", "der", "Him\u00b7mel", "sprach", ":", "ich", "will", "auch", "wel\u00b7che", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "$.", "PPER", "VMFIN", "ADV", "PIS", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Es koste/ was es will; so war des Todes Sinn:", "tokens": ["Es", "kos\u00b7te", "/", "was", "es", "will", ";", "so", "war", "des", "To\u00b7des", "Sinn", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "PWS", "PPER", "VMFIN", "$.", "ADV", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Kriegst du die Alten nicht/ so will ich die begraben/", "tokens": ["Kriegst", "du", "die", "Al\u00b7ten", "nicht", "/", "so", "will", "ich", "die", "be\u00b7gra\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKNEG", "$(", "ADV", "VMFIN", "PPER", "ART", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die noch unschuldig sind. Drauf starb die Jugend hin.", "tokens": ["Die", "noch", "un\u00b7schul\u00b7dig", "sind", ".", "Drauf", "starb", "die", "Ju\u00b7gend", "hin", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "VAFIN", "$.", "PAV", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": 