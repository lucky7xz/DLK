{"textgrid.poem.34410": {"metadata": {"author": {"name": "Hartleben, Otto Erich", "birth": "N.A.", "death": "N.A."}, "title": "1", "genre": "verse", "period": "N.A.", "pub_year": 1885, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In Nordberlin, im Hinterhaus, vier Treppen,", "tokens": ["In", "Nord\u00b7ber\u00b7lin", ",", "im", "Hin\u00b7ter\u00b7haus", ",", "vier", "Trep\u00b7pen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "APPRART", "NN", "$,", "CARD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "wohnt ein Student. Er war nicht reich; doch arm,", "tokens": ["wohnt", "ein", "Stu\u00b7dent", ".", "Er", "war", "nicht", "reich", ";", "doch", "arm", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$.", "PPER", "VAFIN", "PTKNEG", "ADJD", "$.", "ADV", "ADJD", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "blutarm war seine Wirthin, eine Wittwe.", "tokens": ["blu\u00b7tarm", "war", "sei\u00b7ne", "Wirt\u00b7hin", ",", "ei\u00b7ne", "Witt\u00b7we", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPOSAT", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Die sass in ihrem d\u00fcsteren Hinterst\u00fcbchen,", "tokens": ["Die", "sass", "in", "ih\u00b7rem", "d\u00fcs\u00b7te\u00b7ren", "Hin\u00b7ter\u00b7st\u00fcb\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "und vor ihr stand bek\u00fcmmert ihre Tochter,", "tokens": ["und", "vor", "ihr", "stand", "be\u00b7k\u00fcm\u00b7mert", "ih\u00b7re", "Toch\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VVFIN", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "das bleiche, h\u00fcbsche, vierzehnj\u00e4hrge Gretchen.", "tokens": ["das", "blei\u00b7che", ",", "h\u00fcb\u00b7sche", ",", "vier\u00b7zehn\u00b7j\u00e4hr\u00b7ge", "Gret\u00b7chen", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Sie stand vor ihr, als w\u00e4r sie schuldbewusst,", "tokens": ["Sie", "stand", "vor", "ihr", ",", "als", "w\u00e4r", "sie", "schuld\u00b7be\u00b7wusst", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "$,", "KOKOM", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "und liess das K\u00f6pfchen h\u00e4ngen; ihre Mutter", "tokens": ["und", "liess", "das", "K\u00f6pf\u00b7chen", "h\u00e4n\u00b7gen", ";", "ih\u00b7re", "Mut\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "VVINF", "$.", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "schalt auf sie ein mit ihrer harten Stimme:", "tokens": ["schalt", "auf", "sie", "ein", "mit", "ih\u00b7rer", "har\u00b7ten", "Stim\u00b7me", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "ART", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ein neues Kleid? Zur Confirmation!", "tokens": ["Ein", "neu\u00b7es", "Kleid", "?", "Zur", "Con\u00b7fir\u00b7ma\u00b7ti\u00b7on", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "F\u00fcrn lieben Gott! Was? Frag doch mal den Pastor,", "tokens": ["F\u00fcrn", "lie\u00b7ben", "Gott", "!", "Was", "?", "Frag", "doch", "mal", "den", "Pas\u00b7tor", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$.", "PWS", "$.", "NN", "ADV", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "wieso denn die, die nicht mal so viel Geld", "tokens": ["wie\u00b7so", "denn", "die", ",", "die", "nicht", "mal", "so", "viel", "Geld"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "$,", "PRELS", "PTKNEG", "ADV", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "bekamen, um in einem ganzen Kleide", "tokens": ["be\u00b7ka\u00b7men", ",", "um", "in", "ei\u00b7nem", "gan\u00b7zen", "Klei\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "KOUI", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "des Sonntags in die Kirche gehn zu k\u00f6nnen,", "tokens": ["des", "Sonn\u00b7tags", "in", "die", "Kir\u00b7che", "gehn", "zu", "k\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVINF", "PTKZU", "VMINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "wieso denn die an Gott noch glauben m\u00fcssten!", "tokens": ["wie\u00b7so", "denn", "die", "an", "Gott", "noch", "glau\u00b7ben", "m\u00fcss\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "APPR", "NN", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Geh, frag ihn, aber bitt mich nicht um Geld", "tokens": ["Geh", ",", "frag", "ihn", ",", "a\u00b7ber", "bitt", "mich", "nicht", "um", "Geld"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "VVIMP", "PPER", "$,", "ADV", "VVFIN", "PPER", "PTKNEG", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "und Kleider .. freu dich, wenn du nicht verhungerst ...", "tokens": ["und", "Klei\u00b7der", "..", "freu", "dich", ",", "wenn", "du", "nicht", "ver\u00b7hun\u00b7gerst", "..."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$.", "VVFIN", "PPER", "$,", "KOUS", "PPER", "PTKNEG", "ADV", "$("], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Und weinend wendet Gretchen sich zur Th\u00fcr.", "tokens": ["Und", "wei\u00b7nend", "wen\u00b7det", "Gret\u00b7chen", "sich", "zur", "Th\u00fcr."], "token_info": ["word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["KON", "ADJD", "VVFIN", "NE", "PRF", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da kommt ihr ein Gedanke. Mutter! ruft sie,", "tokens": ["Da", "kommt", "ihr", "ein", "Ge\u00b7dan\u00b7ke", ".", "Mut\u00b7ter", "!", "ruft", "sie", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$.", "NN", "$.", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "ich will den Herren Doctor bitten \u2013 Mutter!", "tokens": ["ich", "will", "den", "Her\u00b7ren", "Doc\u00b7tor", "bit\u00b7ten", "\u2013", "Mut\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "NN", "VVINF", "$(", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Was lachst du? \u2013 Das ist recht! Nur zu! Nur zu!", "tokens": ["Was", "lachst", "du", "?", "\u2013", "Das", "ist", "recht", "!", "Nur", "zu", "!", "Nur", "zu", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$.", "$(", "PDS", "VAFIN", "ADJD", "$.", "ADV", "PTKVZ", "$.", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-++-", "measure": "unknown.measure.penta"}, "line.7": {"text": "Es muss ja doch mal kommen. Geh nur hin! \u2013", "tokens": ["Es", "muss", "ja", "doch", "mal", "kom\u00b7men", ".", "Geh", "nur", "hin", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "ADV", "VVINF", "$.", "NE", "ADV", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Ich glaube, Mutter, dass ers thut. \u2013 Gewiss!", "tokens": ["Ich", "glau\u00b7be", ",", "Mut\u00b7ter", ",", "dass", "ers", "thut", ".", "\u2013", "Ge\u00b7wiss", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "NN", "$,", "KOUS", "PIS", "VVFIN", "$.", "$(", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Er w\u00e4re ja ein Narr, wenn er sich zierte!", "tokens": ["Er", "w\u00e4\u00b7re", "ja", "ein", "Narr", ",", "wenn", "er", "sich", "zier\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN", "$,", "KOUS", "PPER", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Und wieder lacht sie bitter h\u00f6hnisch auf.", "tokens": ["Und", "wie\u00b7der", "lacht", "sie", "bit\u00b7ter", "h\u00f6h\u00b7nisch", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADJD", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ein Bangen vor der Mutter fasst das Kind.", "tokens": ["Ein", "Ban\u00b7gen", "vor", "der", "Mut\u00b7ter", "fasst", "das", "Kind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Es geht hinaus und leise, sch\u00fcchtern klopft es", "tokens": ["Es", "geht", "hin\u00b7aus", "und", "lei\u00b7se", ",", "sch\u00fcch\u00b7tern", "klopft", "es"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "KON", "ADJD", "$,", "VVFIN", "VVFIN", "PPER"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "an des Studenten Th\u00fcr. Herein! Und zagend,", "tokens": ["an", "des", "Stu\u00b7den\u00b7ten", "Th\u00fcr.", "Her\u00b7ein", "!", "Und", "za\u00b7gend", ","], "token_info": ["word", "word", "word", "abbreviation", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "PTKVZ", "$.", "KON", "ADJD", "$,"], "meter": "--+---+-+-", "measure": "anapaest.init"}, "line.4": {"text": "err\u00f6thend \u00fcberschreitet sie die Schwelle.", "tokens": ["er\u00b7r\u00f6\u00b7thend", "\u00fc\u00b7bersc\u00b7hrei\u00b7tet", "sie", "die", "Schwel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Sie hat noch nicht gebettelt. \u2013", "tokens": ["Sie", "hat", "noch", "nicht", "ge\u00b7bet\u00b7telt", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PTKNEG", "VVPP", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Gretchen! Du? \u2013", "tokens": ["Gret\u00b7chen", "!", "Du", "?", "\u2013"], "token_info": ["word", "punct", "word", "punct", "punct"], "pos": ["NE", "$.", "PPER", "$.", "$("], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "So komm doch n\u00e4her, Kind .. was giebt es denn?", "tokens": ["So", "komm", "doch", "n\u00e4\u00b7her", ",", "Kind", "..", "was", "giebt", "es", "denn", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADJD", "$,", "NN", "$.", "PWS", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Was hast du denn? O sieh, du hast geweint!", "tokens": ["Was", "hast", "du", "denn", "?", "O", "sieh", ",", "du", "hast", "ge\u00b7weint", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "ADV", "$.", "NE", "PTKVZ", "$,", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Gieb mir die Hand: wer hat dir was gethan? \u2013", "tokens": ["Gieb", "mir", "die", "Hand", ":", "wer", "hat", "dir", "was", "ge\u00b7than", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "$.", "PWS", "VAFIN", "PPER", "PIS", "VVPP", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und freundlich fasst er ihre Hand und schaut", "tokens": ["Und", "freund\u00b7lich", "fasst", "er", "ih\u00b7re", "Hand", "und", "schaut"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "PPOSAT", "NN", "KON", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "in ihre grossen braunen Augen. Flehend,", "tokens": ["in", "ih\u00b7re", "gros\u00b7sen", "brau\u00b7nen", "Au\u00b7gen", ".", "Fle\u00b7hend", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "ADJA", "NN", "$.", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "doch ohne Scheu sind sie auf ihn gerichtet.", "tokens": ["doch", "oh\u00b7ne", "Scheu", "sind", "sie", "auf", "ihn", "ge\u00b7rich\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VAFIN", "PPER", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Und langsam sagt sie: N\u00e4chsten Sonntag schon,", "tokens": ["Und", "lang\u00b7sam", "sagt", "sie", ":", "N\u00e4chs\u00b7ten", "Sonn\u00b7tag", "schon", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "$.", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "am Ostersonntag .. werd ich eingesegnet ..", "tokens": ["am", "Os\u00b7ter\u00b7sonn\u00b7tag", "..", "werd", "ich", "ein\u00b7ge\u00b7seg\u00b7net", ".."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$.", "VAFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "und alle kommen sie in schwarzen Kleidern ..", "tokens": ["und", "al\u00b7le", "kom\u00b7men", "sie", "in", "schwar\u00b7zen", "Klei\u00b7dern", ".."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "in neuen schwarzen Kleidern .. aber ich ..", "tokens": ["in", "neu\u00b7en", "schwar\u00b7zen", "Klei\u00b7dern", "..", "a\u00b7ber", "ich", ".."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "$.", "KON", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "ich bat die Mutter ... Ach, wir sind so arm!", "tokens": ["ich", "bat", "die", "Mut\u00b7ter", "...", "Ach", ",", "wir", "sind", "so", "arm", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$(", "ITJ", "$,", "PPER", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Von j\u00e4hem Mitleid mit sich selbst bew\u00e4ltigt,", "tokens": ["Von", "j\u00e4\u00b7hem", "Mit\u00b7leid", "mit", "sich", "selbst", "be\u00b7w\u00e4l\u00b7tigt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "PRF", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "bricht sie aufs neu in heisse Thr\u00e4nen aus,", "tokens": ["bricht", "sie", "aufs", "neu", "in", "heis\u00b7se", "Thr\u00e4\u00b7nen", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "ADJD", "APPR", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "und, wie nach Tr\u00f6stung suchend, fasst sie fester", "tokens": ["und", ",", "wie", "nach", "Tr\u00f6s\u00b7tung", "su\u00b7chend", ",", "fasst", "sie", "fes\u00b7ter"], "token_info": ["word