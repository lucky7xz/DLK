{"textgrid.poem.61426": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Herbstnacht", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich schreit' hinan die Waldesbahn", "tokens": ["Ich", "schreit'", "hi\u00b7nan", "die", "Wal\u00b7des\u00b7bahn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In Finsternis und Schweigen,", "tokens": ["In", "Fins\u00b7ter\u00b7nis", "und", "Schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da kommt ein Sausen dumpf heran,", "tokens": ["Da", "kommt", "ein", "Sau\u00b7sen", "dumpf", "he\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da r\u00fchrt sich's in den Zweigen.", "tokens": ["Da", "r\u00fchrt", "sich's", "in", "den", "Zwei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der Geist der Nacht ist aufgewacht,", "tokens": ["Der", "Geist", "der", "Nacht", "ist", "auf\u00b7ge\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Er singt in dunklen Zungen;", "tokens": ["Er", "singt", "in", "dunk\u00b7len", "Zun\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Hei, wie so wild das braust und schwillt", "tokens": ["Hei", ",", "wie", "so", "wild", "das", "braust", "und", "schwillt"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "PWAV", "ADV", "ADJD", "PDS", "VVFIN", "KON", "VVFIN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Von Berg zu Berg geschwungen!", "tokens": ["Von", "Berg", "zu", "Berg", "ge\u00b7schwun\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Dahin, daher, wie Wogen im Meer,", "tokens": ["Da\u00b7hin", ",", "da\u00b7her", ",", "wie", "Wo\u00b7gen", "im", "Meer", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "PAV", "$,", "PWAV", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Wiegen die Wipfel und schwanken,", "tokens": ["Wie\u00b7gen", "die", "Wip\u00b7fel", "und", "schwan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "KON", "VVINF", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Schon rieselt das Laub herab in den Staub,", "tokens": ["Schon", "rie\u00b7selt", "das", "Laub", "her\u00b7ab", "in", "den", "Staub", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Schon brechen \u00c4st' und Ranken;", "tokens": ["Schon", "bre\u00b7chen", "\u00c4st'", "und", "Ran\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der Eiche First erseufzt und birst,", "tokens": ["Der", "Ei\u00b7che", "First", "er\u00b7seufzt", "und", "birst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVPP", "KON", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Fichte kracht vom Hange,", "tokens": ["Die", "Fich\u00b7te", "kracht", "vom", "Han\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Der Waldbach zischt, verkehrt in Gischt,", "tokens": ["Der", "Wald\u00b7bach", "zischt", ",", "ver\u00b7kehrt", "in", "Gischt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wie eine b\u00e4umende Schlange.", "tokens": ["Wie", "ei\u00b7ne", "b\u00e4u\u00b7men\u00b7de", "Schlan\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Im Busch verirrt die Eule schwirrt,", "tokens": ["Im", "Busch", "ver\u00b7irrt", "die", "Eu\u00b7le", "schwirrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Augen rot ihr funkeln,", "tokens": ["Die", "Au\u00b7gen", "rot", "ihr", "fun\u00b7keln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Damhirsch setzt, vom Sturm gehetzt,", "tokens": ["Der", "Dam\u00b7hirsch", "setzt", ",", "vom", "Sturm", "ge\u00b7hetzt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Quer \u00fcber den Steig im Dunkeln.", "tokens": ["Quer", "\u00fc\u00b7ber", "den", "Steig", "im", "Dun\u00b7keln", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Das kreischt und ruft aus Fels und Kluft!", "tokens": ["Das", "kreischt", "und", "ruft", "aus", "Fels", "und", "Kluft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "KON", "VVFIN", "APPR", "NE", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das ist ein Flattern und Rasen!", "tokens": ["Das", "ist", "ein", "Flat\u00b7tern", "und", "Ra\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "KON", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Dazwischen schallt aus hoher Luft", "tokens": ["Da\u00b7zwi\u00b7schen", "schallt", "aus", "ho\u00b7her", "Luft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Des wilden J\u00e4gers Blasen.", "tokens": ["Des", "wil\u00b7den", "J\u00e4\u00b7gers", "Bla\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "La\u00df schallen sein Horn, la\u00df sieden den Born!", "tokens": ["La\u00df", "schal\u00b7len", "sein", "Horn", ",", "la\u00df", "sie\u00b7den", "den", "Born", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "VVFIN", "PPOSAT", "NN", "$,", "VVIMP", "NN", "ART", "NE", "$."], "meter": "-+--+-++-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "La\u00df Busch und Wipfel brausen!", "tokens": ["La\u00df", "Busch", "und", "Wip\u00b7fel", "brau\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "La\u00df krachen die Tann' in des Windes Zorn!", "tokens": ["La\u00df", "kra\u00b7chen", "die", "Tann'", "in", "des", "Win\u00b7des", "Zorn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Mir soll darob nicht grausen.", "tokens": ["Mir", "soll", "da\u00b7rob", "nicht", "grau\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PAV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ich wei\u00df einen Bann, der zwingen kann", "tokens": ["Ich", "wei\u00df", "ei\u00b7nen", "Bann", ",", "der", "zwin\u00b7gen", "kann"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PRELS", "VVINF", "VMFIN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Den Nachtgeist, wie er w\u00fcte:", "tokens": ["Den", "Nacht\u00b7geist", ",", "wie", "er", "w\u00fc\u00b7te", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Von dir ein Lied, Geliebte, zieht", "tokens": ["Von", "dir", "ein", "Lied", ",", "Ge\u00b7lieb\u00b7te", ",", "zieht"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["APPR", "PPER", "ART", "NN", "$,", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mir wonnig durchs Gem\u00fcte.", "tokens": ["Mir", "won\u00b7nig", "durchs", "Ge\u00b7m\u00fc\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Beim Lampenschein jetzt harrst du mein", "tokens": ["Beim", "Lam\u00b7pen\u00b7schein", "jetzt", "harrst", "du", "mein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADV", "VAFIN", "PPER", "PPOSAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im warmen Erkersaale,", "tokens": ["Im", "war\u00b7men", "Er\u00b7ker\u00b7saa\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Aus rankendem Gr\u00fcn rings Blumen gl\u00fchn,", "tokens": ["Aus", "ran\u00b7ken\u00b7dem", "Gr\u00fcn", "rings", "Blu\u00b7men", "gl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "NN", "VVINF", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Von D\u00fcften qualmt die Schale;", "tokens": ["Von", "D\u00fcf\u00b7ten", "qualmt", "die", "Scha\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Du horchst empor mit leisem Ohr:", "tokens": ["Du", "horchst", "em\u00b7por", "mit", "lei\u00b7sem", "Ohr", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "\u00bbso war's der Nachtsturm wieder?\u00ab", "tokens": ["\u00bb", "so", "wa\u00b7r's", "der", "Nachts\u00b7turm", "wie\u00b7der", "?", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "VAFIN", "ART", "NN", "ADV", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Entfesselt rollt der Locken Gold", "tokens": ["Ent\u00b7fes\u00b7selt", "rollt", "der", "Lo\u00b7cken", "Gold"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Dir \u00fcber die Stirn hernieder.", "tokens": ["Dir", "\u00fc\u00b7ber", "die", "Stirn", "her\u00b7nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Gott gr\u00fc\u00df' dich, Kind! Ich schreite geschwind,", "tokens": ["Gott", "gr\u00fc\u00df'", "dich", ",", "Kind", "!", "Ich", "schrei\u00b7te", "ge\u00b7schwind", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,", "NN", "$.", "PPER", "VVFIN", "ADJD", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Wie der Pilger zum tr\u00f6stenden Bilde.", "tokens": ["Wie", "der", "Pil\u00b7ger", "zum", "tr\u00f6s\u00b7ten\u00b7den", "Bil\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Deine Hand so wei\u00df, wie wird sie mit Flei\u00df", "