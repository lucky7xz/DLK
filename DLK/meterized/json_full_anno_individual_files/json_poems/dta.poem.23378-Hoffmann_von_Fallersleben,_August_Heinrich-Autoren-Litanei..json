{"dta.poem.23378": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Autoren-Litanei.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mathematische Figuren,", "tokens": ["Ma\u00b7the\u00b7ma\u00b7ti\u00b7sche", "Fi\u00b7gu\u00b7ren", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wahre sympathetische Curen,", "tokens": ["Wah\u00b7re", "sym\u00b7pa\u00b7the\u00b7ti\u00b7sche", "Cu\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Logarithmen, Rechenknechte,", "tokens": ["Lo\u00b7ga\u00b7rith\u00b7men", ",", "Re\u00b7chen\u00b7knech\u00b7te", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ueber Infanterie-Gefechte,", "tokens": ["Ue\u00b7ber", "In\u00b7fan\u00b7te\u00b7rie\u00b7Ge\u00b7fech\u00b7te", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Anatomische Tabellen,", "tokens": ["A\u00b7nat\u00b7o\u00b7mi\u00b7sche", "Ta\u00b7bel\u00b7len", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.6": {"text": "Die entdeckten Oderquellen,", "tokens": ["Die", "ent\u00b7deck\u00b7ten", "O\u00b7der\u00b7quel\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Pater Cochems Fegefeuer,", "tokens": ["Pa\u00b7ter", "Coc\u00b7hems", "Fe\u00b7ge\u00b7feu\u00b7er", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NE", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "N\u00fctzlichkeit der Hundesteuer.", "tokens": ["N\u00fctz\u00b7lich\u00b7keit", "der", "Hun\u00b7des\u00b7teu\u00b7er", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "O du himmlischer Vater, erbarme dich der Autoren", "tokens": ["O", "du", "himm\u00b7li\u00b7scher", "Va\u00b7ter", ",", "er\u00b7bar\u00b7me", "dich", "der", "Au\u00b7to\u00b7ren"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NE", "ADJA", "NN", "$,", "VVFIN", "PPER", "ART", "NN"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.10": {"text": "Vor den R\u00f6theln, Stiften und Federn der Censoren,", "tokens": ["Vor", "den", "R\u00f6\u00b7theln", ",", "Stif\u00b7ten", "und", "Fe\u00b7dern", "der", "Cen\u00b7so\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "NN", "KON", "NN", "ART", "NN", "$,"], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.11": {"text": "Da\u00df nicht unsre gro\u00dfe M\u00fch' und Arbeit gehe verloren!", "tokens": ["Da\u00df", "nicht", "uns\u00b7re", "gro\u00b7\u00dfe", "M\u00fch'", "und", "Ar\u00b7beit", "ge\u00b7he", "ver\u00b7lo\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "PPOSAT", "ADJA", "NN", "KON", "NN", "VVFIN", "VVPP", "$."], "meter": "+-+-+-+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.12": {"text": "Kyrie eleison!", "tokens": ["Ky\u00b7rie", "e\u00b7lei\u00b7son", "!"], "token_info": ["word", "word", "punct"], "pos": ["NE", "NE", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Die entlarvte Wasserhose,", "tokens": ["Die", "ent\u00b7larv\u00b7te", "Was\u00b7ser\u00b7ho\u00b7se", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sammlung aller Lebermose,", "tokens": ["Samm\u00b7lung", "al\u00b7ler", "Le\u00b7ber\u00b7mo\u00b7se", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ueber Palm- und andre Seifen,", "tokens": ["Ue\u00b7ber", "Pal\u00b7m", "und", "and\u00b7re", "Sei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "TRUNC", "KON", "ADJA", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Ein Versuch von Meerschaumpfeifen,", "tokens": ["Ein", "Ver\u00b7such", "von", "Meer\u00b7schaum\u00b7pfei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Neue Kunst mit Gl\u00fcck zu freien,", "tokens": ["Neu\u00b7e", "Kunst", "mit", "Gl\u00fcck", "zu", "frei\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ueber Branntweinbrennereien,", "tokens": ["Ue\u00b7ber", "Brannt\u00b7wein\u00b7bren\u00b7ne\u00b7rei\u00b7en", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Bibliographie der Fibeln,", "tokens": ["Bib\u00b7lio\u00b7gra\u00b7phie", "der", "Fi\u00b7beln", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.8": {"text": "Lehrgedicht: die Tulpenzwiebeln.", "tokens": ["Lehr\u00b7ge\u00b7dicht", ":", "die", "Tul\u00b7pen\u00b7zwie\u00b7beln", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$.", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "O du himmlischer Vater, erbarme dich der Autoren", "tokens": ["O", "du", "himm\u00b7li\u00b7scher", "Va\u00b7ter", ",", "er\u00b7bar\u00b7me", "dich", "der", "Au\u00b7to\u00b7ren"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NE", "ADJA", "NN", "$,", "VVFIN", "PPER", "ART", "NN"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.10": {"text": "Vor den R\u00f6theln, Stiften und Federn der Censoren,", "tokens": ["Vor", "den", "R\u00f6\u00b7theln", ",", "Stif\u00b7ten", "und", "Fe\u00b7dern", "der", "Cen\u00b7so\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "NN", "KON", "NN", "ART", "NN", "$,"], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.11": {"text": "Da\u00df nicht unsre gro\u00dfe M\u00fch' und Arbeit gehe verloren!", "tokens": ["Da\u00df", "nicht", "uns\u00b7re", "gro\u00b7\u00dfe", "M\u00fch'", "und", "Ar\u00b7beit", "ge\u00b7he", "ver\u00b7lo\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "PPOSAT", "ADJA", "NN", "KON", "NN", "VVFIN", "VVPP", "$."], "meter": "+-+-+-+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.12": {"text": "Kyrie eleison!", "tokens": ["Ky\u00b7rie", "e\u00b7lei\u00b7son", "!"], "token_info": ["word", "word", "punct"], "pos": ["NE", "NE", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "R\u00e4thsel- und Charadenb\u00fcchlein,", "tokens": ["R\u00e4th\u00b7sel", "und", "Cha\u00b7ra\u00b7den\u00b7b\u00f