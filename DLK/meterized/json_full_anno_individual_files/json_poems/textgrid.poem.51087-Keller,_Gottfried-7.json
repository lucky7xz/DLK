{"textgrid.poem.51087": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "7", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dort gegen Westen, traulich unterm Dach,", "tokens": ["Dort", "ge\u00b7gen", "Wes\u00b7ten", ",", "trau\u00b7lich", "un\u00b7term", "Dach", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$,", "ADJD", "APPRART", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Liegt hoch und abgeschieden das Gemach,", "tokens": ["Liegt", "hoch", "und", "ab\u00b7ge\u00b7schie\u00b7den", "das", "Ge\u00b7mach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "KON", "VVPP", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das sich des Hauses T\u00f6chter jederzeit", "tokens": ["Das", "sich", "des", "Hau\u00b7ses", "T\u00f6ch\u00b7ter", "je\u00b7der\u00b7zeit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "PRF", "ART", "NN", "NN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Zu ihrem Allerheiligsten geweiht.", "tokens": ["Zu", "ih\u00b7rem", "Al\u00b7ler\u00b7hei\u00b7ligs\u00b7ten", "ge\u00b7weiht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+---+", "measure": "zehnsilber"}}, "stanza.2": {"line.1": {"text": "Es ist ein eng und niedrig K\u00e4mmerlein", "tokens": ["Es", "ist", "ein", "eng", "und", "nied\u00b7rig", "K\u00e4m\u00b7mer\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJD", "KON", "ADJD", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"t