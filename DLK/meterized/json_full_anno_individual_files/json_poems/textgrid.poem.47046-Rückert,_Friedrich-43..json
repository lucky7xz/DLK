{"textgrid.poem.47046": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "43.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Begl\u00fcckt, wer, wenn des Winters St\u00fcrme schnauben,", "tokens": ["Be\u00b7gl\u00fcckt", ",", "wer", ",", "wenn", "des", "Win\u00b7ters", "St\u00fcr\u00b7me", "schnau\u00b7ben", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PWS", "$,", "KOUS", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und Schauer durch die \u00f6den R\u00e4ume zucken,", "tokens": ["Und", "Schau\u00b7er", "durch", "die", "\u00f6\u00b7den", "R\u00e4u\u00b7me", "zu\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "APPR", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Froh fl\u00fcchten darf und heimlich unterducken", "tokens": ["Froh", "fl\u00fcch\u00b7ten", "darf", "und", "heim\u00b7lich", "un\u00b7ter\u00b7du\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVINF", "VMFIN", "KON", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wohl unter eines Strohdachs warme Schauben.", "tokens": ["Wohl", "un\u00b7ter", "ei\u00b7nes", "Stroh\u00b7dachs", "war\u00b7me", "Schau\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wenn n\u00e4her dann in ihrem Nest die Tauben,", "tokens": ["Wenn", "n\u00e4\u00b7her", "dann", "in", "ih\u00b7rem", "Nest", "die", "Tau\u00b7be