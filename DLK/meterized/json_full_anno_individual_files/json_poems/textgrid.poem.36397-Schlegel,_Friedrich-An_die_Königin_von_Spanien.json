{"textgrid.poem.36397": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "An die K\u00f6nigin von Spanien", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein dunkler Nebel deckte weit die Orte,", "tokens": ["Ein", "dunk\u00b7ler", "Ne\u00b7bel", "deck\u00b7te", "weit", "die", "Or\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da traf ein zarter Klagelaut mein Ohr;", "tokens": ["Da", "traf", "ein", "zar\u00b7ter", "Kla\u00b7ge\u00b7laut", "mein", "Ohr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch nein! es waren lichte Heldenworte", "tokens": ["Doch", "nein", "!", "es", "wa\u00b7ren", "lich\u00b7te", "Hel\u00b7den\u00b7wor\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PTKANT", "$.", "PPER", "VAFIN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Liebeswahrheit, und aus dieser Pforte", "tokens": ["Der", "Lie\u00b7bes\u00b7wahr\u00b7heit", ",", "und", "aus", "die\u00b7ser", "Pfor\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "KON", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Geht milde Klarheit siegreich bald hervor.", "tokens": ["Geht", "mil\u00b7de", "Klar\u00b7heit", "sieg\u00b7reich", "bald", "her\u00b7vor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Der Zwiespalt h\u00e4lt das sch\u00f6ne Land zerrissen,", "tokens": ["Der", "Zwies\u00b7palt", "h\u00e4lt", "das", "sch\u00f6\u00b7ne", "Land", "zer\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der edle Garten wird zur W\u00fcstenei.", "tokens": ["Der", "ed\u00b7le", "Gar\u00b7ten", "wird", "zur", "W\u00fcs\u00b7te\u00b7nei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der L\u00fcgengeist vergiftet die Gewissen,", "tokens": ["Der", "L\u00fc\u00b7gen\u00b7geist", "ver\u00b7gif\u00b7tet", "die", "Ge\u00b7wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Sitte flieht vor solchen Schlangenbissen;", "tokens": ["Die", "Sit\u00b7te", "flieht", "vor", "sol\u00b7chen", "Schlan\u00b7gen\u00b7bis\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Die Macht des B\u00f6sen wird auf Erden frei.", "tokens": ["Die", "Macht", "des", "B\u00f6\u00b7sen", "wird", "auf", "Er\u00b7den", "frei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Das harte Recht sinnt nur auf strenge Rache,", "tokens": ["Das", "har\u00b7te", "Recht", "sinnt", "nur", "auf", "stren\u00b7ge", "Ra\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der Ha\u00df durchflammt die blut'ge Finsternis.", "tokens": ["Der", "Ha\u00df", "durch\u00b7flammt", "die", "blut'\u00b7ge", "Fins\u00b7ter\u00b7nis", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Wut entadelt selbst die heil'ge Sache,", "tokens": ["Die", "Wut", "en\u00b7ta\u00b7delt", "selbst", "die", "heil'\u00b7ge", "Sa\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df jeder Hauch dann neu den Brand anfache;", "tokens": ["Da\u00df", "je\u00b7der", "Hauch", "dann", "neu", "den", "Brand", "an\u00b7fa\u00b7che", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "ADV", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Unheilbar scheint der gro\u00dfe Zeitenri\u00df.", "tokens": ["Un\u00b7heil\u00b7bar", "scheint", "der", "gro\u00b7\u00dfe", "Zei\u00b7ten\u00b7ri\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Gebietend steigt allein vom Himmel nieder", "tokens": ["Ge\u00b7bie\u00b7tend", "steigt", "al\u00b7lein", "vom", "Him\u00b7mel", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "VVFIN", "ADV", "APPRART", "NN", "PTKVZ"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im milden Wort des Friedens heil'ge Kraft.", "tokens": ["Im", "mil\u00b7den", "Wort", "des", "Frie\u00b7dens", "heil'\u00b7ge", "Kraft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mit der Verschonung kehrt die Gnade wieder,", "tokens": ["Mit", "der", "Ver\u00b7scho\u00b7nung", "kehrt", "die", "Gna\u00b7de", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Balsam tr\u00e4ufelt in die wunden Glieder,", "tokens": ["Die", "Bal\u00b7sam", "tr\u00e4u\u00b7felt", "in", "die", "wun\u00b7den", "Glie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und neue Ordnung den Geheilten schafft.", "tokens": ["Und", "neu\u00b7e", "Ord\u00b7nung", "den", "Ge\u00b7heil\u00b7ten", "schafft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Du hast das sch\u00f6ne Liebeswort vernommen,", "tokens": ["Du", "hast", "das", "sch\u00f6\u00b7ne", "Lie\u00b7bes\u00b7wort", "ver\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In zarter Seele, deutscher F\u00fcrstin Brust.", "tokens": ["In", "zar\u00b7ter", "See\u00b7le", ",", "deut\u00b7scher", "F\u00fcrs\u00b7tin", "Brust", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Vom Himmel ist ein Strahl herabgekommen,", "tokens": ["Vom", "Him\u00b7mel", "ist", "ein", "Strahl", "her\u00b7ab\u00b7ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zum Volke ist der K\u00f6nig frei gekommen;", "tokens": ["Zum", "Vol\u00b7ke", "ist", "der", "K\u00f6\u00b7nig", "frei", "ge\u00b7kom\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Es schlie\u00dft sich neu der Bund in Sieg und Lust.", "tokens": ["Es", "schlie\u00dft", "sich", "neu", "der", "Bund", "in", "Sieg", "und", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADJD", "ART", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Bezaubert vom hesperischen Gesange,", "tokens": ["Be\u00b7zau\u00b7bert", "vom", "hes\u00b7pe\u00b7ri\u00b7schen", "Ge\u00b7san\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "F\u00fchlt sich das sch\u00f6ne Spanien wieder jung.", "tokens": ["F\u00fchlt", "sich", "das", "sch\u00f6\u00b7ne", "Spa\u00b7ni\u00b7en", "wie\u00b7der", "jung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "ADJA", "NN", "ADV", "ADJD", "$."], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Er traf auch mich mit holdem Seelenklange,", "tokens": ["Er", "traf", "auch", "mich", "mit", "hol\u00b7dem", "See\u00b7len\u00b7klan\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Entri\u00df den Lippen in melod'schem Zwange", "tokens": ["Ent\u00b7ri\u00df", "den", "Lip\u00b7pen", "in", "me\u00b7lo\u00b7d'\u00b7schem", "Zwan\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Das schwache Echo dieser Huldigung.", "tokens": ["Das", "schwa\u00b7che", "E\u00b7cho", "die\u00b7ser", "Hul\u00b7di\u00b7gung", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PDAT", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "So eile denn gefl\u00fcgelt durch die Lande,", "tokens": ["So", "ei\u00b7le", "denn", "ge\u00b7fl\u00fc\u00b7gelt", "durch", "die", "Lan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "O Lied! hin zu der k\u00f6niglichen Frau.", "tokens": ["O", "Lied", "!", "hin", "zu", "der", "k\u00f6\u00b7nig\u00b7li\u00b7chen", "Frau", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+---+", "measure": "zehnsilber"}, "line.3": {"text": "Es schwankt die Welt sinkend am Flammenrande.", "tokens": ["Es", "schwankt", "die", "Welt", "sin\u00b7kend", "am", "Flam\u00b7men\u00b7ran\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD", "APPRART", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Es l\u00f6sen sich der heil'gen Ordnung Bande;", "tokens": ["Es", "l\u00f6\u00b7sen", "sich", "der", "heil'\u00b7gen", "Ord\u00b7nung", "Ban\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Im Stillen w\u00e4chst der Zukunft Gottes Bau.", "tokens": ["Im", "Stil\u00b7len", "w\u00e4chst", "der", "Zu\u00b7kunft", "Got\u00b7tes", "Bau", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Ein dunkler Nebel deckte weit die Orte,", "tokens": ["Ein", "dunk\u00b7ler", "Ne\u00b7bel", "deck\u00b7te", "weit", "die", "Or\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da traf ein zarter Klagelaut mein Ohr;", "tokens": ["Da", "traf", "ein", "zar\u00b7ter", "Kla\u00b7ge\u00b7laut", "mein", "Ohr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch nein! es waren lichte Heldenworte", "tokens": ["Doch", "nein", "!"