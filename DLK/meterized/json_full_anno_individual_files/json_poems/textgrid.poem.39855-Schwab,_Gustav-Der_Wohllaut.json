{"textgrid.poem.39855": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Der Wohllaut", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wo str\u00f6met ihr, in dunkle Nacht begraben,", "tokens": ["Wo", "str\u00f6\u00b7met", "ihr", ",", "in", "dunk\u00b7le", "Nacht", "be\u00b7gra\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "$,", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des heil'gen Wohllauts unversiegte Quellen?", "tokens": ["Des", "heil'\u00b7gen", "Wohl\u00b7lauts", "un\u00b7ver\u00b7sieg\u00b7te", "Quel\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Im dichten Hain forsch' ich nach euren F\u00e4llen;", "tokens": ["Im", "dich\u00b7ten", "Hain", "for\u00b7sch'", "ich", "nach", "eu\u00b7ren", "F\u00e4l\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Ihr rauschet klangreich durch mein altes Schwaben!", "tokens": ["Ihr", "rau\u00b7schet", "klan\u00b7greich", "durch", "mein", "al\u00b7tes", "Schwa\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wie gl\u00fccklich ist, wem eure s\u00fc\u00dfen Gaben", "tokens": ["Wie", "gl\u00fcck\u00b7lich", "ist", ",", "wem", "eu\u00b7re", "s\u00fc\u00b7\u00dfen", "Ga\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VAFIN", "$,", "PWS", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Aus reicher Ader selbst entgegenschwellen,", "tokens": ["Aus", "rei\u00b7cher", "A\u00b7der", "selbst", "ent\u00b7ge\u00b7gen\u00b7schwel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wem seine Lippen in den blauen Wellen", "tokens": ["Wem", "sei\u00b7ne", "Lip\u00b7pen", "in", "den", "blau\u00b7en", "Wel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PPOSAT", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der lautern Flut sich keusch gebadet haben.", "tokens": ["Der", "lau\u00b7tern", "Flut", "sich", "keusch", "ge\u00b7ba\u00b7det", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PRF", "ADJD", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ihm t\u00f6nt sein Sang zur\u00fcck aus tausend Kehlen.", "tokens": ["Ihm", "t\u00f6nt", "sein", "Sang", "zu\u00b7r\u00fcck", "aus", "tau\u00b7send", "Keh\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "PTKVZ", "APPR", "CARD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und, wie in eines klaren Wassers Grunde,", "tokens": ["Und", ",", "wie", "in", "ei\u00b7nes", "kla\u00b7ren", "Was\u00b7sers", "Grun\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWAV", "APPR", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Bespiegeln sich in seinem Lied die Seelen.", "tokens": ["Be\u00b7spie\u00b7geln", "sich", "in", "sei\u00b7nem", "Lied", "die", "See\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ja, gl\u00fccklich ist das fremde Lied zu preisen,", "tokens": ["Ja", ",", "gl\u00fcck\u00b7lich", "ist", "das", "frem\u00b7de", "Lied", "zu", "prei\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADJD", "VAFIN", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das je, versch\u00f6nt, entwandelt seinem Munde,", "tokens": ["Das", "je", ",", "ver\u00b7sch\u00f6nt", ",", "ent\u00b7wan\u00b7delt", "sei\u00b7nem", "Mun\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "$,", "VVFIN", "$,", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In goldnen Klang verkehrt er dumpfe Weisen.", "tokens": ["In", "gold\u00b7nen", "Klang", "ver\u00b7kehrt", "er", "dump\u00b7fe", "Wei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Wo str\u00f6met ihr, in dunkle Nacht begraben,", "tokens": ["Wo", "str\u00f6\u00b7met", "ihr", ",", "in", "dunk\u00b7le", "Nacht", "be\u00b7gra\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["