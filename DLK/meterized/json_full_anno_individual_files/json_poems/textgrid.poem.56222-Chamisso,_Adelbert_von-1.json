{"textgrid.poem.56222": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "1", "genre": "verse", "period": "N.A.", "pub_year": 1832, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was klappert im Hause so laut? horch, horch!", "tokens": ["Was", "klap\u00b7pert", "im", "Hau\u00b7se", "so", "laut", "?", "horch", ",", "horch", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PWS", "VVFIN", "APPRART", "NN", "ADV", "ADJD", "$.", "PTKVZ", "$,", "VVIMP", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Ich glaub, ich glaube, das ist der Storch.", "tokens": ["Ich", "glaub", ",", "ich", "glau\u00b7be", ",", "das", "ist", "der", "Storch", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "$,", "PDS", "VAFIN", "ART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Das war der Storch. Seid, Kinder, nur still,", "tokens": ["Das", "war", "der", "Storch", ".", "Seid", ",", "Kin\u00b7der", ",", "nur", "still", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$.", "VAIMP", "$,", "NN", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Und h\u00f6rt, was gern ich erz\u00e4hlen euch will.", "tokens": ["Und", "h\u00f6rt", ",", "was", "gern", "ich", "er\u00b7z\u00e4h\u00b7len", "euch", "will", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PRELS", "ADV", "PPER", "VVFIN", "PPER", "VMFIN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Er hat euch gebracht ein Br\u00fcderlein", "tokens": ["Er", "hat", "euch", "ge\u00b7bracht", "ein", "Br\u00fc\u00b7derl\u00b7ein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "VVPP", "ART", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und hat gebissen Mutter ins Bein.", "tokens": ["Und", "hat", "ge\u00b7bis\u00b7sen", "Mut\u00b7ter", "ins", "Bein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "VVPP", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.4": {"line.1": {"text": "Sie liegt nun krank, doch freudig dabei,", "tokens": ["Sie", "liegt", "nun", "krank", ",", "doch", "freu\u00b7dig", "da\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "$,", "ADV", "ADJD", "PAV", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Sie meint, der Schmerz zu ertragen sei.", "tokens": ["Sie", "meint", ",", "der", "Schmerz", "zu", "er\u00b7tra\u00b7gen", "sei", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ART", "NN", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Das Br\u00fcderlein hat euer gedacht,", "tokens": ["Das", "Br\u00fc\u00b7derl\u00b7ein", "hat", "eu\u00b7er", "ge\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "VVPP", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Und Zuckerwerk die Menge gebracht,", "tokens": ["Und", "Zu\u00b7cker\u00b7werk", "die", "Men\u00b7ge", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "pu