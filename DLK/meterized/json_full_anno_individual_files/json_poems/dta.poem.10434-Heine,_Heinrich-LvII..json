{"dta.poem.10434": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "LvII.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Alln\u00e4chtlich im Traume seh' ich dich,", "tokens": ["All\u00b7n\u00e4cht\u00b7lich", "im", "Trau\u00b7me", "seh'", "ich", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVFIN", "PPER", "PRF", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und sehe dich freundlich gr\u00fc\u00dfen,", "tokens": ["Und", "se\u00b7he", "dich", "freund\u00b7lich", "gr\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADJD", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und lautaufweinend st\u00fcrz' ich mich", "tokens": ["Und", "laut\u00b7auf\u00b7wei\u00b7nend", "st\u00fcrz'", "ich", "mich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu deinen s\u00fc\u00dfen F\u00fc\u00dfen.", "tokens": ["Zu", "dei\u00b7nen", "s\u00fc\u00b7\u00dfen", "F\u00fc\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Du siehst mich an wehm\u00fcthiglich,", "tokens": ["Du", "siehst", "mich", "an", "weh\u00b7m\u00fct\u00b7hig\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und sch\u00fcttelst das blonde K\u00f6pfchen;", "tokens": ["Und", "sch\u00fct\u00b7telst", "das", "blon\u00b7de", "K\u00f6pf\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Aus deinen Augen schleichen sich", "tokens": ["Aus", "dei\u00b7nen", "Au\u00b7gen", "schlei\u00b7chen", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Perlenthr\u00e4nentr\u00f6pfchen.", "tokens": ["Die", "Per\u00b7lent\u00b7hr\u00e4\u00b7nen\u00b7tr\u00f6pf\u00b7chen", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Du sagst mir heimlich ein leises Wort,", "tokens": ["Du", "sagst", "mir", "heim\u00b7lich", "ein", "lei\u00b7ses", "Wort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und giebst mir den Strau\u00df von Zypressen.", "tokens": ["Und", "giebst", "mir", "den", "Strau\u00df", "von", "Zyp\u00b7res\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NE", "APPR", "NE", "$."], "meter": "-+--+-+--", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ich wache auf, und der Strau\u00df ist fort,", "tokens": ["Ich", "wa\u00b7che", "auf", ",", "und", "der", "Strau\u00df", "ist", "fort", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "KON", "ART", "NE", "VAFIN", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und's Wort hab' ich vergessen.", "tokens": ["Un\u00b7d's", "Wort", "hab'", "ich", "ver\u00b7ges\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}