{"textgrid.poem.34865": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du schicktest mit dem Flammenschwert", "tokens": ["Du", "schick\u00b7test", "mit", "dem", "Flam\u00b7men\u00b7schwert"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den himmlischen Gendarmen,", "tokens": ["Den", "himm\u00b7li\u00b7schen", "Gen\u00b7dar\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und jagtest mich aus dem Paradies,", "tokens": ["Und", "jag\u00b7test", "mich", "aus", "dem", "Pa\u00b7ra\u00b7dies", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ganz ohne Recht und Erbarmen!", "tokens": ["Ganz", "oh\u00b7ne", "Recht", "und", "Er\u00b7bar\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Ich ziehe fort mit meiner Frau", "tokens": ["Ich", "zie\u00b7he", "fort", "mit", "mei\u00b7ner", "Frau"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nach andren Erdenl\u00e4ndern;", "tokens": ["Nach", "an\u00b7dren", "Er\u00b7den\u00b7l\u00e4n\u00b7dern", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Doch da\u00df ich genossen des Wissens Frucht,", "tokens": ["Doch", "da\u00df", "ich", "ge\u00b7nos\u00b7sen", "des", "Wis\u00b7sens", "Frucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVPP", "ART", "NN", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Das kannst du nicht mehr \u00e4ndern.", "tokens": ["Das", "kannst", "du", "nicht", "mehr", "\u00e4n\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "PTKNEG", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Du kannst nicht \u00e4ndern, da\u00df ich wei\u00df,", "tokens": ["Du", "kannst", "nicht", "\u00e4n\u00b7dern", ",", "da\u00df", "ich", "wei\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "VVINF", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie sehr du klein und nichtig,", "tokens": ["Wie", "sehr", "du", "klein", "und", "nich\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und machst du dich auch noch so sehr", "tokens": ["Und", "machst", "du", "dich", "auch", "noch", "so", "sehr"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PRF", "ADV", "ADV", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Durch Tod und Donnern wichtig.", "tokens": ["Durch", "Tod", "und", "Don\u00b7nern", "wich\u00b7tig", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "