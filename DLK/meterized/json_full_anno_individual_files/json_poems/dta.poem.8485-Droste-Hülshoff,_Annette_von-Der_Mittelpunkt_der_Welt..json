{"dta.poem.8485": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Der Mittelpunkt der Welt.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1860", "urn": "urn:nbn:de:kobv:b4-200905191007", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "J&#252;ngst hast die Phrase scherzend du gestellt:", "tokens": ["J", "&#252;", "ngst", "hast", "die", "Phra\u00b7se", "scher\u00b7zend", "du", "ge\u00b7stellt", ":"], "token_info": ["word", "XML_entity", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "ADV", "VAFIN", "ART", "NN", "ADJD", "PPER", "VVPP", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "\u201ewer Reichthum, Liebe will und Gl\u00fcck erlangen,", "tokens": ["\u201e", "wer", "Reicht\u00b7hum", ",", "Lie\u00b7be", "will", "und", "Gl\u00fcck", "er\u00b7lan\u00b7gen", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "NN", "$,", "NN", "VMFIN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der mache sich zum Mittelpunkt der Welt,", "tokens": ["Der", "ma\u00b7che", "sich", "zum", "Mit\u00b7tel\u00b7punkt", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Zum Kreise, drin sich alle Strahlen fangen.\u201c", "tokens": ["Zum", "Krei\u00b7se", ",", "drin", "sich", "al\u00b7le", "Strah\u00b7len", "fan\u00b7gen", ".", "\u201c"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "NN", "$,", "ADV", "PRF", "PIAT", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Dein Wort, mein Freund, war wie des Tempels Th\u00fcr,", "tokens": ["Dein", "Wort", ",", "mein", "Freund", ",", "war", "wie", "des", "Tem\u00b7pels", "Th\u00fcr", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "VAFIN", "KOKOM", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Die Inschrift drau\u00dfen und das Volksgedr\u00e4nge,", "tokens": ["Die", "In\u00b7schrift", "drau\u00b7\u00dfen", "und", "das", "Volks\u00b7ge\u00b7dr\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Doch durch die Spalten blinkt der Lampen Zier,", "tokens": ["Doch", "durch", "die", "Spal\u00b7ten", "blinkt", "der", "Lam\u00b7pen", "Zier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Zieh\u2019n Opferduft und heilige Ges\u00e4nge.", "tokens": ["Zieh'n", "Op\u00b7fer\u00b7duft", "und", "hei\u00b7li\u00b7ge", "Ge\u00b7s\u00e4n\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wo k\u00f6nnte jemals wohl des Gl\u00fcckes Born", "tokens": ["Wo", "k\u00f6nn\u00b7te", "je\u00b7mals", "wohl", "des", "Gl\u00fc\u00b7ckes", "Born"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "ADV", "ADV", "ART", "NN", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Aus anderm als aus eignem Herzen flie\u00dfen?", "tokens": ["Aus", "an\u00b7derm", "als", "aus", "eig\u00b7nem", "Her\u00b7zen", "flie\u00b7\u00dfen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "KOKOM", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Aus welcher Schaale wohl des Himmels Zorn", "tokens": ["Aus", "wel\u00b7cher", "Schaa\u00b7le", "wohl", "des", "Him\u00b7mels", "Zorn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PWAT", "NN", "ADV", "ART", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Als aus der selbstgebotnen sich ergie\u00dfen?", "tokens": ["Als", "aus", "der", "selbst\u00b7ge\u00b7bot\u00b7nen", "sich", "er\u00b7gie\u00b7\u00dfen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "ADJA", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "O gl\u00fccklich sein, geliebt und gl\u00fccklich sein!", "tokens": ["O", "gl\u00fcck\u00b7lich", "sein", ",", "ge\u00b7liebt", "und", "gl\u00fcck\u00b7lich", "sein", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "VAINF", "$,", "VVPP", "KON", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "M\u00f6ge mein Engel mir die Pfade deuten!", "tokens": ["M\u00f6\u00b7ge", "mein", "En\u00b7gel", "mir", "die", "Pfa\u00b7de", "deu\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.7": {"text": "Da schrillt des Tempels Vorhang, zart und rein", "tokens": ["Da", "schrillt", "des", "Tem\u00b7pels", "Vor\u00b7hang", ",", "zart", "und", "rein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "$,", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "H\u00f6r\u2019 ich\u2019s, wie Echo durch die Falten gleiten.", "tokens": ["H\u00f6r'", "ich's", ",", "wie", "E\u00b7cho", "durch", "die", "Fal\u00b7ten", "glei\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$,", "PWAV", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Standest an einem Krankenbett du je,", "tokens": ["Stan\u00b7dest", "an", "ei\u00b7nem", "Kran\u00b7ken\u00b7bett", "du", "je", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PPER", "ADV", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Nach wochenlangen selbstvergessnen Sorgen?", "tokens": ["Nach", "wo\u00b7chen\u00b7lan\u00b7gen", "selbst\u00b7ver\u00b7gess\u00b7nen", "Sor\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Hobst deine schweren Wimper in die H\u00f6h\u2019,", "tokens": ["Hobst", "dei\u00b7ne", "schwe\u00b7ren", "Wim\u00b7per", "in", "die", "H\u00f6h'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ger\u00fchrt zum hei\u00dfen Dankgebet am Morgen,", "tokens": ["Ge\u00b7r\u00fchrt", "zum", "hei\u00b7\u00dfen", "Dank\u00b7ge\u00b7bet", "am", "Mor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPRART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und sah\u2019st auf des Genesenden Gesicht", "tokens": ["Und", "sah'st", "auf", "des", "Ge\u00b7ne\u00b7sen\u00b7den", "Ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Ein neuerwachtes Seelenleben schweben,", "tokens": ["Ein", "neu\u00b7er\u00b7wach\u00b7tes", "See\u00b7len\u00b7le\u00b7ben", "schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und einen Liebesblick auf dich, wie nicht", "tokens": ["Und", "ei\u00b7nen", "Lie\u00b7bes\u00b7blick", "auf", "dich", ",", "wie", "nicht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "PPER", "$,", "PWAV", "PTKNEG"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Ihn Freund und nicht Geliebte k\u00f6nnen geben:", "tokens": ["Ihn", "Freund", "und", "nicht", "Ge\u00b7lieb\u00b7te", "k\u00f6n\u00b7nen", "ge\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "KON", "PTKNEG", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Hieltest du je den Griffel in der Hand", "tokens": ["Hiel\u00b7test", "du", "je", "den", "Grif\u00b7fel", "in", "der", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und rechnetest mit frohem Geiz zusammen", "tokens": ["Und", "rech\u00b7ne\u00b7test", "mit", "fro\u00b7hem", "Geiz", "zu\u00b7sam\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Groschen, die du selber dir entwandt;", "tokens": ["Die", "Gro\u00b7schen", ",", "die", "du", "sel\u00b7ber", "dir", "ent\u00b7wandt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADV", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Schien jeder Heller dir wie Gold zu flammen,", "tokens": ["Schien", "je\u00b7der", "Hel\u00b7ler", "dir", "wie", "Gold", "zu", "flam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "PPER", "KOKOM", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Des Preises f\u00fcr den fremden Sorgenpf\u00fchl,", "tokens": ["Des", "Prei\u00b7ses", "f\u00fcr", "den", "frem\u00b7den", "Sor\u00b7gen\u00b7pf\u00fchl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Um den du deine Freuden schlau betrogen,", "tokens": ["Um", "den", "du", "dei\u00b7ne", "Freu\u00b7den", "schlau", "be\u00b7tro\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "PPER", "PPOSAT", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und hast in deines Reichthums Vollgef\u00fchl", "tokens": ["Und", "hast", "in", "dei\u00b7nes", "Reicht\u00b7hums", "Voll\u00b7ge\u00b7f\u00fchl"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Tief, tief den Odem in die Brust gesogen:", "tokens": ["Tief", ",", "tief", "den", "O\u00b7dem", "in", "die", "Brust", "ge\u00b7so\u00b7gen", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "ART", "NN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Und der Moment, wo eine Rechte schwimmt", "tokens": ["Und", "der", "Mo\u00b7ment", ",", "wo", "ei\u00b7ne", "Rech\u00b7te", "schwimmt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "$,", "PWAV", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ob theurem Haupte mit bewegtem Segen,", "tokens": ["Ob", "theu\u00b7rem", "Haup\u00b7te", "mit", "be\u00b7weg\u00b7tem", "Se\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wo sie das Herz vom eignen Herzen nimmt,", "tokens": ["Wo", "sie", "das", "Herz", "vom", "eig\u00b7nen", "Her\u00b7zen", "nimmt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Um weinend an das fremde es zu legen,", "tokens": ["Um", "wei\u00b7nend", "an", "das", "frem\u00b7de", "es", "zu", "le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADJD", "APPR", "ART", "ADJA", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Hast du ihn je erlebt? und standest dann,", "tokens": ["Hast", "du", "ihn", "je", "er\u00b7lebt", "?", "und", "stan\u00b7dest", "dann", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "ADV", "VVPP", "$.", "KON", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Die Arme still und freundlich umgeschlagen,", "tokens": ["Die", "Ar\u00b7me", "still", "und", "freund\u00b7lich", "um\u00b7ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "KON", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Selig berechnend, welche Fr\u00fcchte kann,", "tokens": ["Se\u00b7lig", "be\u00b7rech\u00b7nend", ",", "wel\u00b7che", "Fr\u00fcch\u00b7te", "kann", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,", "PWAT", "NN", "VMFIN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.8": {"text": "Wie liebliche das neue B\u00fcndni\u00df tragen:", "tokens": ["Wie", "lieb\u00b7li\u00b7che", "das", "neu\u00b7e", "B\u00fcn\u00b7dni\u00df", "tra\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Dann bist du gl\u00fccklich, bist geliebt und reich,", "tokens": ["Dann", "bist", "du", "gl\u00fcck\u00b7lich", ",", "bist", "ge\u00b7liebt", "und", "reich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "$,", "VAFIN", "VVPP", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ein Dach, an dem sich alle Blitze spalten;", "tokens": ["Ein", "Dach", ",", "an", "dem", "sich", "al\u00b7le", "Blit\u00b7ze", "spal\u00b7ten", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "PRELS", "PRF", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dann mag dein Lorbeer welken, m\u00f6gen bleich", "tokens": ["Dann", "mag", "dein", "Lor\u00b7beer", "wel\u00b7ken", ",", "m\u00f6\u00b7gen", "bleich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VMFIN", "PPOSAT", "NN", "VVINF", "$,", "VMFIN", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Krankheit und Alter dir die Stirne falten:", "tokens": ["Krank\u00b7heit", "und", "Al\u00b7ter", "dir", "die", "Stir\u00b7ne", "fal\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.5": {"text": "Dann bist der Mittelpunkt du deiner Welt,", "tokens": ["Dann", "bist", "der", "Mit\u00b7tel\u00b7punkt", "du", "dei\u00b7ner", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Der Kreis, aus dem dir freud\u2019ge Strahlen quillen,", "tokens": ["Der", "Kreis", ",", "aus", "dem", "dir", "freu\u00b7d'\u00b7ge", "Strah\u00b7len", "quil\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "PRELS", "PPER", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Und was so frisch der B\u00e4che Ufer schwellt,", "tokens": ["Und", "was", "so", "frisch", "der", "B\u00e4\u00b7che", "U\u00b7fer", "schwellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "ADJD", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Wie sollte seinen Born es nicht erf\u00fcllen?", "tokens": ["Wie", "soll\u00b7te", "sei\u00b7nen", "Born", "es", "nicht", "er\u00b7f\u00fcl\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPOSAT", "NN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}