{"textgrid.poem.26184": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wir gingen die Landstra\u00df', die nicht enden wollte,", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir gingen die Landstra\u00df', die nicht enden wollte,", "tokens": ["Wir", "gin\u00b7gen", "die", "Land\u00b7stra\u00df'", ",", "die", "nicht", "en\u00b7den", "woll\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PRELS", "PTKNEG", "VVINF", "VMFIN", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Hoch \u00fcberm Wald die goldene Mondkugel rollte.", "tokens": ["Hoch", "\u00fc\u00b7berm", "Wald", "die", "gol\u00b7de\u00b7ne", "Mond\u00b7ku\u00b7gel", "roll\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ein Kr\u00e4henhauf' flog johlend \u00fcber die Stra\u00dfe", "tokens": ["Ein", "Kr\u00e4\u00b7hen\u00b7hauf'", "flog", "joh\u00b7lend", "\u00fc\u00b7ber", "die", "Stra\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Und suchte Nachtruh' in der Wipfelmasse.", "tokens": ["Und", "such\u00b7te", "Nachtruh'", "in", "der", "Wip\u00b7fel\u00b7mas\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Die Kr\u00e4h'n, die ich vom Winter 