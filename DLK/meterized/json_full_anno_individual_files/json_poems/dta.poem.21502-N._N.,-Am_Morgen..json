{"dta.poem.21502": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Am Morgen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Tr&#252;b der Morgen und kalt.               ", "tokens": ["Tr", "&#252;", "b", "der", "Mor\u00b7gen", "und", "kalt", "."], "token_info": ["word", "XML_entity", "word", "word", "word", "word", "word", "punct"], "pos": ["XY", "XY", "XY", "ART", "NN", "KON", "ADJD", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.2": {"text": "Ueber die Wiesen schweifen", "tokens": ["Ue\u00b7ber", "die", "Wie\u00b7sen", "schwei\u00b7fen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVINF"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Feuchte Nebelstreifen;", "tokens": ["Feuch\u00b7te", "Ne\u00b7bel\u00b7strei\u00b7fen", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Auf den Bergen ringsum", "tokens": ["Auf", "den", "Ber\u00b7gen", "ring\u00b7sum"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADV"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Liegen Wolken geballt,", "tokens": ["Lie\u00b7gen", "Wol\u00b7ken", "ge\u00b7ballt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Grau und stumm.", "tokens": ["Grau", "und", "stumm", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "M\u00fchsam", "tokens": ["M\u00fch\u00b7sam"], "token_info": ["word"], "pos": ["NE"], "meter": "+-", "measure": "trochaic.single"}, "line.8": {"text": "Gegen die dunklen Schatten,", "tokens": ["Ge\u00b7gen", "die", "dunk\u00b7len", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Halb wagend,", "tokens": ["Halb", "wa\u00b7gend", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.10": {"text": "Halb zagend,", "tokens": ["Halb", "za\u00b7gend", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "ADJD", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.11": {"text": "Sendet Sonne den matten,", "tokens": ["Sen\u00b7det", "Son\u00b7ne", "den", "mat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "ART", "ADJA", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.12": {"text": "Bebenden Strahl.", "tokens": ["Be\u00b7ben\u00b7den", "Strahl", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.13": {"text": "Nieder in\u2019s Thal", "tokens": ["Nie\u00b7der", "in's", "Thal"], "token_info": ["word", "word", "word"], "pos": ["ADV", "APPRART", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.14": {"text": "R\u00f6thlich bricht", "tokens": ["R\u00f6th\u00b7lich", "bricht"], "token_info": ["word", "word"], "pos": ["ADJD", "VVFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.15": {"text": "Hier und dort unsicheres Licht \u2026", "tokens": ["Hier", "und", "dort", "un\u00b7si\u00b7che\u00b7res", "Licht", "\u2026"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KON", "ADV", "PPOSAT", "NN", "$("], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.16": {"text": "K&#228;mpfen mu\u00df die herrlichste Gluth,               ", "tokens": ["K", "&#228;", "mp\u00b7fen", "mu\u00df", "die", "herr\u00b7lichs\u00b7te", "Gluth", ","], "token_info": ["word", "XML_entity", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "VVINF", "VMFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.17": {"text": "Die hehrste Feindin irdischer Fehle:", "tokens": ["Die", "hehrs\u00b7te", "Fein\u00b7din", "ir\u00b7di\u00b7scher", "Feh\u00b7le", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Muth, Muth,", "tokens": ["Muth", ",", "Muth", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "Arme ringende Menschenseele!", "tokens": ["Ar\u00b7me", "rin\u00b7gen\u00b7de", "Men\u00b7schen\u00b7see\u00b7le", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}}}}