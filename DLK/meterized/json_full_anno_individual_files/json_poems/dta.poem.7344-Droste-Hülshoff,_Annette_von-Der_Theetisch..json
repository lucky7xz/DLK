{"dta.poem.7344": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Der Theetisch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-20090519994", "language": ["de:0.99"], "booktitle": "Droste-H\u00fclshoff, Annette von: Gedichte. Stuttgart u. a., 1844."}, "poem": {"stanza.1": {"line.1": {"text": "L\u00e4ugnen willst du Zaubertr\u00e4nke,", "tokens": ["L\u00e4ug\u00b7nen", "willst", "du", "Zau\u00b7ber\u00b7tr\u00e4n\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lachst mir h\u00f6hnisch in die Z\u00e4hne,", "tokens": ["Lachst", "mir", "h\u00f6h\u00b7nisch", "in", "die", "Z\u00e4h\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn Isoldens ich gedenke,", "tokens": ["Wenn", "I\u00b7sol\u00b7dens", "ich", "ge\u00b7den\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "PPER", "VVFIN", "$,"], "meter": "-+-++-+-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Wenn Gudrunens ich erw\u00e4hne?", "tokens": ["Wenn", "Gud\u00b7ru\u00b7nens", "ich", "er\u00b7w\u00e4h\u00b7ne", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "PPER", "VVFIN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Und was deine kluge Amme", "tokens": ["Und", "was", "dei\u00b7ne", "klu\u00b7ge", "Am\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In der D\u00e4mmrung dir vertraute,", "tokens": ["In", "der", "D\u00e4mm\u00b7rung", "dir", "ver\u00b7trau\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Von Schneewittchen und der Flamme,", "tokens": ["Von", "Schnee\u00b7witt\u00b7chen", "und", "der", "Flam\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Die den Hexenschwaden braute;", "tokens": ["Die", "den", "He\u00b7xen\u00b7schwa\u00b7den", "brau\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Alles will dir nicht gen\u00fcgen,", "tokens": ["Al\u00b7les", "will", "dir", "nicht", "ge\u00b7n\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ueberweiser M\u00fcckensieber?", "tokens": ["Ue\u00b7ber\u00b7wei\u00b7ser", "M\u00fc\u00b7cken\u00b7sie\u00b7ber", "?"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nun, so la\u00df die Feder liegen,", "tokens": ["Nun", ",", "so", "la\u00df", "die", "Fe\u00b7der", "lie\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "VVIMP", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schieb dich in den Cirkel, Lieber,", "tokens": ["Schieb", "dich", "in", "den", "Cir\u00b7kel", ",", "Lie\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "ART", "NN", "$,", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Wo des zopfigen Chinesen", "tokens": ["Wo", "des", "zop\u00b7fi\u00b7gen", "Chi\u00b7ne\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Trank im Silberkessel zischet,", "tokens": ["Trank", "im", "Sil\u00b7ber\u00b7kes\u00b7sel", "zi\u00b7schet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+---", "measure": "unknown.measure.tri"}, "line.3": {"text": "Sein Aroma auserlesen", "tokens": ["Sein", "A\u00b7ro\u00b7ma", "au\u00b7ser\u00b7le\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit des Patschul's D\u00fcften mischet;", "tokens": ["Mit", "des", "Pat\u00b7schul's", "D\u00fcf\u00b7ten", "mi\u00b7schet", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Wo ein sch\u00f6ner Geist, den Bogen", "tokens": ["Wo", "ein", "sch\u00f6\u00b7ner", "Geist", ",", "den", "Bo\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN", "$,", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Feingef\u00e4ltelt in der Tasche,", "tokens": ["Fein\u00b7ge\u00b7f\u00e4l\u00b7telt", "in", "der", "Ta\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Lauscht wie in den Redewogen", "tokens": ["Lauscht", "wie", "in", "den", "Re\u00b7de\u00b7wo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KOKOM", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Er das Steuer sich erhasche;", "tokens": ["Er", "das", "Steu\u00b7er", "sich", "er\u00b7ha\u00b7sche", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Wo in zarten H\u00e4nden h\u00f6rbar", "tokens": ["Wo", "in", "zar\u00b7ten", "H\u00e4n\u00b7den", "h\u00f6r\u00b7bar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADJA", "NN", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Blanke Nadelst\u00e4be knittern,", "tokens": ["Blan\u00b7ke", "Na\u00b7del\u00b7st\u00e4\u00b7be", "knit\u00b7tern", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die Herren stramm und ehrbar", "tokens": ["Und", "die", "Her\u00b7ren", "stramm", "und", "ehr\u00b7bar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "KON", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Breiten ihrer Weisheit Flittern.", "tokens": ["Brei\u00b7ten", "ih\u00b7rer", "Weis\u00b7heit", "Flit\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Alles scheint dir noch gew\u00f6hnlich,", "tokens": ["Al\u00b7les", "scheint", "dir", "noch", "ge\u00b7w\u00f6hn\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Von der Sohle bis zum Scheitel,", "tokens": ["Von", "der", "Soh\u00b7le", "bis", "zum", "Schei\u00b7tel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und du rufst, dem Weisen \u00e4hnlich:", "tokens": ["Und", "du", "rufst", ",", "dem", "Wei\u00b7sen", "\u00e4hn\u00b7lich", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "\u201ealles unter'm Mond ist eitel!\u201c", "tokens": ["\u201e", "al\u00b7les", "un\u00b7ter'm", "Mond", "ist", "ei\u00b7tel", "!", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PIS", "APPRART", "NN", "VAFIN", "ADJD", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Dir gen\u00fcber und zur Seite", "tokens": ["Dir", "ge\u00b7n\u00fc\u00b7ber", "und", "zur", "Sei\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "KON", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hier Christinos, dort Carlisten,", "tokens": ["Hier", "Chris\u00b7ti\u00b7nos", ",", "dort", "Car\u00b7lis\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "NE", "$,", "ADV", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Lauter ordin\u00e4re Leute,", "tokens": ["Lau\u00b7ter", "or\u00b7di\u00b7n\u00e4\u00b7re", "Leu\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Deutsche Michel, gute Christen!", "tokens": ["Deut\u00b7sche", "Mi\u00b7chel", ",", "gu\u00b7te", "Chris\u00b7ten", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Aber sieh die wei\u00dfen schmalen", "tokens": ["A\u00b7ber", "sieh", "die", "wei\u00b7\u00dfen", "schma\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "ART", "ADJA", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Finger sich zum Griff bereiten,", "tokens": ["Fin\u00b7ger", "sich", "zum", "Griff", "be\u00b7rei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die dampfumh\u00fcllten Schalen", "tokens": ["Und", "die", "damp\u00b7fum\u00b7h\u00fcll\u00b7ten", "Scha\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zierlich an die Lippen gleiten:", "tokens": ["Zier\u00b7lich", "an", "die", "Lip\u00b7pen", "glei\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Noch Minuten \u2014 und die Stube", "tokens": ["Noch", "Mi\u00b7nu\u00b7ten", "und", "die", "Stu\u00b7be"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "NN", "$(", "KON", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Ist zum Kiosk umgestaltet,", "tokens": ["Ist", "zum", "Ki\u00b7osk", "um\u00b7ge\u00b7stal\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo der thr\u00e4nenreiche Bube,", "tokens": ["Wo", "der", "thr\u00e4\u00b7nen\u00b7rei\u00b7che", "Bu\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der Chinese zaubernd waltet;", "tokens": ["Der", "Chi\u00b7ne\u00b7se", "zau\u00b7bernd", "wal\u00b7tet", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.11": {"line.1": {"text": "Von der rosenfarbnen Rolle", "tokens": ["Von", "der", "ro\u00b7sen\u00b7farb\u00b7nen", "Rol\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Liest er seine Zauberreime,", "tokens": ["Liest", "er", "sei\u00b7ne", "Zau\u00b7ber\u00b7rei\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Verse, zart wie Seidenwolle,", "tokens": ["Ver\u00b7se", ",", "zart", "wie", "Sei\u00b7den\u00b7wol\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "KOKOM", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "S\u00fc\u00df wie Jungfernhonigseime;", "tokens": ["S\u00fc\u00df", "wie", "Jung\u00b7fern\u00b7ho\u00b7ni\u00b7gsei\u00b7me", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "\u201eting, tang, tong\u201c \u2014 das steigt und sinket,", "tokens": ["\u201e", "ting", ",", "tang", ",", "tong", "\u201c", "das", "steigt", "und", "sin\u00b7ket", ","], "token_info": ["punct", "word", "punct", "word", "punct", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$,", "VVFIN", "$,", "NE", "$(", "$(", "PDS", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Welch Ges\u00e4usel, welches Zischen!", "tokens": ["Welch", "Ge\u00b7s\u00e4u\u00b7sel", ",", "wel\u00b7ches", "Zi\u00b7schen", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PWAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie ein irres H\u00fcndlein hinket", "tokens": ["Wie", "ein", "ir\u00b7res", "H\u00fcnd\u00b7lein", "hin\u00b7ket"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Noch ein deutsches Wort dazwischen.", "tokens": ["Noch", "ein", "deut\u00b7sches", "Wort", "da\u00b7zwi\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Und die s\u00fc\u00dfen Damen l\u00e4cheln,", "tokens": ["Und", "die", "s\u00fc\u00b7\u00dfen", "Da\u00b7men", "l\u00e4\u00b7cheln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Leise schaukelnde Pagoden;", "tokens": ["Lei\u00b7se", "schau\u00b7keln\u00b7de", "Pa\u00b7go\u00b7den", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie sie nicken, wie sie f\u00e4cheln,", "tokens": ["Wie", "sie", "ni\u00b7cken", ",", "wie", "sie", "f\u00e4\u00b7cheln", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVINF", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie der Kn\u00e4uel h\u00fcpft am Boden!", "tokens": ["Wie", "der", "Kn\u00e4u\u00b7el", "h\u00fcpft", "am", "Bo\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Aber, weh, nun wird's gef\u00e4hrlich,", "tokens": ["A\u00b7ber", ",", "weh", ",", "nun", "wird's", "ge\u00b7f\u00e4hr\u00b7lich", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PTKVZ", "$,", "ADV", "VAFIN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "\u201etschi, tsi, tsung.\u201c \u2014 Die T\u00f6ne schneiden,", "tokens": ["\u201e", "tschi", ",", "tsi", ",", "tsung", ".", "\u201c", "Die", "T\u00f6\u00b7ne", "schnei\u00b7den", ","], "token_info": ["punct", "word", "punct", "word", "punct", "word", "punct", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "NE", "$,", "NE", "$,", "NN", "$.", "$(", "$(", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Schnell hinweg die Messer! schwerlich", "tokens": ["Schnell", "hin\u00b7weg", "die", "Mes\u00b7ser", "!", "schwer\u00b7lich"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ADJD", "ADV", "ART", "NN", "$.", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Uebersteht er solche Leiden;", "tokens": ["Ue\u00b7ber\u00b7steht", "er", "sol\u00b7che", "Lei\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "Denn er schaukelt und er dehnet", "tokens": ["Denn", "er", "schau\u00b7kelt", "und", "er", "deh\u00b7net"], "token_info": ["word", "word", "word", "word", 