{"dta.poem.16337": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "73.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sieh an den Wasserfall, wo du ihm nahe stehst,", "tokens": ["Sieh", "an", "den", "Was\u00b7ser\u00b7fall", ",", "wo", "du", "ihm", "na\u00b7he", "stehst", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "$,", "PWAV", "PPER", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und sieh ihn wieder an, wenn du ihm ferne gehst!", "tokens": ["Und", "sieh", "ihn", "wie\u00b7der", "an", ",", "wenn", "du", "ihm", "fer\u00b7ne", "gehst", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "ADV", "PTKVZ", "$,", "KOUS", "PPER", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Er ist dir bald im Aug' und ist dir bald im Ohr,", "tokens": ["Er", "ist", "dir", "bald", "im", "Aug'", "und", "ist", "dir", "bald", "im", "Ohr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "APPRART", "NN", "KON", "VAFIN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ist in und au\u00dfer dir, t\u00f6nt nach und schwebt dir vor.", "tokens": ["Ist", "in", "und", "au\u00b7\u00dfer", "dir", ",", "t\u00f6nt", "nach", "und", "schwebt", "dir", "vor", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "KON", "APPR", "PPER", "$,", "VVFIN", "APPR", "KON", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Er f\u00fcllt dir jeden Sinn, und spricht zu allen Sinnen;", "tokens": ["Er", "f\u00fcllt", "dir", "je\u00b7den", "Sinn", ",", "und", "spricht", "zu", "al\u00b7len", "Sin\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIAT", "NN", "$,", "KON", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Versuch es und entrinn, ihm ist nicht zu entrinnen.", "tokens": ["Ver\u00b7such", "es", "und", "ent\u00b7rinn", ",", "ihm", "ist", "nicht", "zu", "ent\u00b7rin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "KON", "NN", "$,", "PPER", "VAFIN", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Er rauscht und rauscht und rauscht, die Gegend h\u00f6rt ihn rauschen,", "tokens": ["Er", "rauscht", "und", "rauscht", "und", "rauscht", ",", "die", "Ge\u00b7gend", "h\u00f6rt", "ihn", "rau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "$,", "ART", "NN", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und lauscht und lauscht und lauscht, und wird nicht satt zu lauschen.", "tokens": ["Und", "lauscht", "und", "lauscht", "und", "lauscht", ",", "und", "wird", "nicht", "satt", "zu", "lau\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "$,", "KON", "VAFIN", "PTKNEG", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Er w\u00fchlt und w\u00fchlt und w\u00fchlt, der Boden f\u00fchlt ihn w\u00fchlen,", "tokens": ["Er", "w\u00fchlt", "und", "w\u00fchlt", "und", "w\u00fchlt", ",", "der", "Bo\u00b7den", "f\u00fchlt", "ihn", "w\u00fch\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "$,", "ART", "NN", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und f\u00fchlt und f\u00fchlt und f\u00fchlt, und reicht nicht aus zu f\u00fchlen.", "tokens": ["Und", "f\u00fchlt", "und", "f\u00fchlt", "und", "f\u00fchlt", ",", "und", "reicht", "nicht", "aus", "zu", "f\u00fch\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "$,", "KON", "VVFIN", "PTKNEG", "PTKVZ", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Er sch\u00e4umt und sch\u00e4umt und sch\u00e4umt, die Blume l\u00e4\u00dft ihn sch\u00e4umen,", "tokens": ["Er", "sch\u00e4umt", "und", "sch\u00e4umt", "und", "sch\u00e4umt", ",", "die", "Blu\u00b7me", "l\u00e4\u00dft", "ihn", "sch\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "$,", "ART", "NN", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und tr\u00e4umt und tr\u00e4umt und tr\u00e4umt, und h\u00f6rt nicht auf zu tr\u00e4umen.", "tokens": ["Und", "tr\u00e4umt", "und", "tr\u00e4umt", "und", "tr\u00e4umt", ",", "und", "h\u00f6rt", "nicht", "auf", "zu", "tr\u00e4u\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "$,", "KON", "VVFIN", "PTKNEG", "APPR", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Er stralt und stralt und stralt, der Maler sieht ihn stralen,", "tokens": ["Er", "stralt", "und", "stralt", "und", "stralt", ",", "der", "Ma\u00b7ler", "sieht", "ihn", "stra\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "$,", "ART", "NN", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und malt und malt und malt, und wird nicht m\u00fcd zu malen.", "tokens": ["Und", "malt", "und", "malt", "und", "malt", ",", "und", "wird", "nicht", "m\u00fcd", "zu", "ma\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "$,", "KON", "VAFIN", "PTKNEG", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Er haucht und haucht und haucht, feucht f\u00fchlt die Luft sein Hauchen,", "tokens": ["Er", "haucht", "und", "haucht", "und", "haucht", ",", "feucht", "f\u00fchlt", "die", "Luft", "sein", "Hau\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "$,", "ADJD", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und taucht und taucht und taucht, sich satt darein zu tauchen.", "tokens": ["Und", "taucht", "und", "taucht", "und", "taucht", ",", "sich", "satt", "da\u00b7rein", "zu", "tau\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "$,", "PRF", "ADJD", "PAV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Er quillt und quillt und quillt, und wird nicht matt zu quellen;", "tokens": ["Er", "quillt", "und", "quillt", "und", "quillt", ",", "und", "wird", "nicht", "matt", "zu", "quel\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "$,", "KON", "VAFIN", "PTKNEG", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er schwillt und schwillt und schwillt, und wird nicht satt zu schwellen.", "tokens": ["Er", "schwillt", "und", "schwillt", "und", "schwillt", ",", "und", "wird", "nicht", "satt", "zu", "schwel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "$,", "KON", "VAFIN", "PTKNEG", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Und wie er quoll und quoll, und wie er schwoll und schwoll,", "tokens": ["Und", "wie", "er", "quoll", "und", "quoll", ",", "und", "wie", "er", "schwoll", "und", "schwoll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADJD", "KON", "VVFIN", "$,", "KON", "PWAV", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sein Quellen wird nie leer, sein Schwellen wird nie voll.", "tokens": ["Sein", "Quel\u00b7len", "wird", "nie", "leer", ",", "sein", "Schwel\u00b7len", "wird", "nie", "voll", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ADJD", "$,", "PPOSAT", "NN", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+--++-+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.11": {"line.1": {"text": "Kein Gleiches hat die Flur, ein Gleiches ihm hat nur", "tokens": ["Kein", "Glei\u00b7ches", "hat", "die", "Flur", ",", "ein", "Glei\u00b7ches", "ihm", "hat", "nur"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VAFIN", "ART", "NN", "$,", "ART", "NN", "PPER", "VAFIN", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die ewig sich aus sich geb\u00e4rende Natur.", "tokens": ["Die", "e\u00b7wig", "sich", "aus", "sich", "ge\u00b7b\u00e4\u00b7ren\u00b7de", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PRF", "APPR", "PRF", "ADJA", "NN", "$."], "meter": "-+-+-+-+---+", "measure": "unknown.measure.penta"}}}}}