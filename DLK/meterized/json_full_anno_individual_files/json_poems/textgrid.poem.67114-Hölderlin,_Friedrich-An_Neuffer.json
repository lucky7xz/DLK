{"textgrid.poem.67114": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "An Neuffer", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Br\u00fcderlich Herz! ich komme zu dir, wie der tauende Morgen,", "tokens": ["Br\u00fc\u00b7der\u00b7lich", "Herz", "!", "ich", "kom\u00b7me", "zu", "dir", ",", "wie", "der", "tau\u00b7en\u00b7de", "Mor\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "$.", "PPER", "VVFIN", "APPR", "PPER", "$,", "PWAV", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+---+-+--+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Schlie\u00dfe du, wie der Kelch z\u00e4rtlicher Blumen, dich auf;", "tokens": ["Schlie\u00b7\u00dfe", "du", ",", "wie", "der", "Kelch", "z\u00e4rt\u00b7li\u00b7cher", "Blu\u00b7men", ",", "dich", "auf", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "ART", "NN", "ADJA", "NN", "$,", "PPER", "PTKVZ", "$."], "meter": "+--+-++--+--+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Einen Himmel empf\u00e4ngst du, der Freude goldene Wolke", "tokens": ["Ei\u00b7nen", "Him\u00b7mel", "emp\u00b7f\u00e4ngst", "du", ",", "der", "Freu\u00b7de", "gol\u00b7de\u00b7ne", "Wol\u00b7ke"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "ART", "NN", "ADJA", "NN"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Rieselt in eilenden freundlichen T\u00f6nen herab.", "tokens": ["Rie\u00b7selt", "in", "ei\u00b7len\u00b7den", "freund\u00b7li\u00b7chen", "T\u00f6\u00b7nen", "her\u00b7ab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "ADJA", "NN", "ADV", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.5": {"text": "Freund! ich kenne mich nicht, ich kenne nimmer den Menschen,", "tokens": ["Freund", "!", "ich", "ken\u00b7ne", "mich", "nicht", ",", "ich", "ken\u00b7ne", "nim\u00b7mer", "den", "Men\u00b7schen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "VVFIN", "PPER", "PTKNEG", "$,", "PPER", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.6": {"text": "Und es sch\u00e4met der Geist aller Gedanken sich nun.", "tokens": ["Und", "es", "sch\u00e4\u00b7met", "der", "Geist", "al\u00b7ler", "Ge\u00b7dan\u00b7ken", "sich", "nun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "PIAT", "NN", "PRF", "ADV", "$."], "meter": "--+--+---+--+", "measure": "anapaest.di.plus"}, "line.7": {"text": "Fassen wollt er auch sie, wie er fa\u00dft die Dinge der Erde,", "tokens": ["Fas\u00b7sen", "wollt", "er", "auch", "sie", ",", "wie", "er", "fa\u00dft", "die", "Din\u00b7ge", "der", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "ADV", "PPER", "$,", "PWAV", "PPER", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.8": {"text": "Fassen...", "tokens": ["Fas\u00b7sen", "..."], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.9": {"text": "Aber ein Schwindel ergriff ihn s\u00fc\u00df, und die ewige Feste", "tokens": ["A\u00b7ber", "ein", "Schwin\u00b7del", "er\u00b7griff", "ihn", "s\u00fc\u00df", ",", "und", "die", "e\u00b7wi\u00b7ge", "Fes\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "ADJD", "$,", "KON", "ART", "ADJA", "NN"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.10": {"text": "Seiner Gedanken st\u00fcrzt'...", "tokens": ["Sei\u00b7ner", "Ge\u00b7dan\u00b7ken", "st\u00fcrzt'", "..."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "Br\u00fcderlich Herz! ich komme zu dir, wie der tauende Morgen,", "tokens": ["Br\u00fc\u00b7der\u00b7lich", "Herz", "!", "ich", "kom\u00b7me", "zu", "dir", ",", "wie", "der", "tau\u00b7en\u00b7de", "Mor\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "$.", "PPER", "VVFIN", "APPR", "PPER", "$,", "PWAV", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+---+-+--+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Schlie\u00dfe du, wie der Kelch z\u00e4rtlicher Blumen, dich auf;", "tokens": ["Schlie\u00b7\u00dfe", "du", ",", "wie", "der", "Kelch", "z\u00e4rt\u00b7li\u00b7cher", "Blu\u00b7men", ",", "dich", "auf", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "ART", "NN", "ADJA", "NN", "$,", "PPER", "PTKVZ", "$."], "meter": "+--+-++--+--+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Einen Himmel empf\u00e4ngst du, der Freude goldene Wolke", "tokens": ["Ei\u00b7nen", "Him\u00b7mel", "emp\u00b7f\u00e4ngst", "du", ",", "der", "Freu\u00b7de", "gol\u00b7de\u00b7ne", "Wol\u00b7ke"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "ART", "NN", "ADJA", "NN"], "meter": "+-+--+--+-+--+-", "measure": "hexame