{"textgrid.poem.67047": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Demut", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "H\u00f6rt, gr\u00f6\u00dfre, edlere der Schwabens\u00f6hne!", "tokens": ["H\u00f6rt", ",", "gr\u00f6\u00df\u00b7re", ",", "ed\u00b7le\u00b7re", "der", "Schwa\u00b7ben\u00b7s\u00f6h\u00b7ne", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$,", "ADJA", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die ihr vor keinem Dominiksgesicht", "tokens": ["Die", "ihr", "vor", "kei\u00b7nem", "Do\u00b7mi\u00b7niks\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Euch kr\u00fcmmet, welchen keine Dirnentr\u00e4ne", "tokens": ["Euch", "kr\u00fcm\u00b7met", ",", "wel\u00b7chen", "kei\u00b7ne", "Dir\u00b7nen\u00b7tr\u00e4\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PWAT", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das winzige, geschw\u00e4chte Herzchen bricht.", "tokens": ["Das", "win\u00b7zi\u00b7ge", ",", "ge\u00b7schw\u00e4ch\u00b7te", "Herz\u00b7chen", "bricht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "H\u00f6rt, gr\u00f6\u00dfre, edlere der Schwabens\u00f6hne!", "tokens": ["H\u00f6rt", ",", "gr\u00f6\u00df\u00b7re", ",", "ed\u00b7le\u00b7re", "der", "Schwa\u00b7ben\u00b7s\u00f6h\u00b7ne", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$,", "ADJA", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In welchen noch das Kleinod Freiheit pocht,", "tokens": ["In", "wel\u00b7chen", "noch", "das", "Klei\u00b7nod", "Frei\u00b7heit", "pocht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "ADV", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die ihr euch keines reichen Ahnherrn Miene,", "tokens": ["Die", "ihr", "euch", "kei\u00b7nes", "rei\u00b7chen", "Ahn\u00b7herrn", "Mie\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "PIAT", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und keiner F\u00fcrstenlaune unterjocht.", "tokens": ["Und", "kei\u00b7ner", "F\u00fcrs\u00b7ten\u00b7lau\u00b7ne", "un\u00b7ter\u00b7jocht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Geschlecht von oben! Vaterlandeskronen!", "tokens": ["Ge\u00b7schlecht", "von", "o\u00b7ben", "!", "Va\u00b7ter\u00b7lan\u00b7des\u00b7kro\u00b7nen", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "APPR", "ADV", "$.", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nur euch bewahre Gott vor \u00dcbermut!", "tokens": ["Nur", "euch", "be\u00b7wah\u00b7re", "Gott", "vor", "\u00dc\u00b7ber\u00b7mut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "O! Br\u00fcder! der Gedanke soll uns lohnen,", "tokens": ["O", "!", "Br\u00fc\u00b7der", "!", "der", "Ge\u00b7dan\u00b7ke", "soll", "uns", "loh\u00b7nen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "NN", "$.", "ART", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In Hermann brauste kein Despotenblut.", "tokens": ["In", "Her\u00b7mann", "braus\u00b7te", "kein", "Des\u00b7po\u00b7ten\u00b7blut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Beweinensw\u00fcrdig ist des Stolzen Ende,", "tokens": ["Be\u00b7wei\u00b7nens\u00b7w\u00fcr\u00b7dig", "ist", "des", "Stol\u00b7zen", "En\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wann er die Grube seiner Gr\u00f6\u00dfe gr\u00e4bt,", "tokens": ["Wann", "er", "die", "Gru\u00b7be", "sei\u00b7ner", "Gr\u00f6\u00b7\u00dfe", "gr\u00e4bt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch f\u00fcrchterlich sind seine Henkersh\u00e4nde,", "tokens": ["Doch", "f\u00fcrch\u00b7ter\u00b7lich", "sind", "sei\u00b7ne", "Hen\u00b7ker\u00b7sh\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wann er sich gl\u00fccklich \u00fcber andre hebt.", "tokens": ["Wann", "er", "sich", "gl\u00fcck\u00b7lich", "\u00fc\u00b7ber", "and\u00b7re", "hebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "ADJD", "APPR", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Viel sind und sch\u00f6n des stillen Mannes Freuden,", "tokens": ["Viel", "sind", "und", "sch\u00f6n", "des", "stil\u00b7len", "Man\u00b7nes", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "KON", "ADJD", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und st\u00fcrmten auch auf ihn der Leiden viel,", "tokens": ["Und", "st\u00fcrm\u00b7ten", "auch", "auf", "ihn", "der", "Lei\u00b7den", "viel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "PPER", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Er blickt gen Himmel unter seinen Leiden,", "tokens": ["Er", "blickt", "gen", "Him\u00b7mel", "un\u00b7ter", "sei\u00b7nen", "Lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Beneidet nie des Lachers Possenspiel.", "tokens": ["Be\u00b7nei\u00b7det", "nie", "des", "La\u00b7chers", "Pos\u00b7sen\u00b7spiel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Sein feurigster, sein erster Wunsch auf Erden", "tokens": ["Sein", "feu\u00b7rigs\u00b7ter", ",", "sein", "ers\u00b7ter", "Wunsch", "auf", "Er\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "ADJA", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ist, allen, allen Menschen n\u00fctzlich sein,", "tokens": ["Ist", ",", "al\u00b7len", ",", "al\u00b7len", "Men\u00b7schen", "n\u00fctz\u00b7lich", "sein", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "PIAT", "$,", "PIAT", "NN", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und wann sie froh durch seine Taten werden,", "tokens": ["Und", "wann", "sie", "froh", "durch", "sei\u00b7ne", "Ta\u00b7ten", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADJD", "APPR", "PPOSAT", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dann will der edle ihres Danks sich freun.", "tokens": ["Dann", "will", "der", "ed\u00b7le", "ih\u00b7res", "Danks", "sich", "freun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "ADJA", "PPOSAT", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "O! Demut, Demut! la\u00df uns all dich lieben,", "tokens": ["O", "!", "De\u00b7mut", ",", "De\u00b7mut", "!", "la\u00df", "uns", "all", "dich", "lie\u00b7ben", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "NN", "$,", "NN", "$.", "VVIMP", "PPER", "PIAT", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du bists, die uns zu einem Bund vereint,", "tokens": ["Du", "bists", ",", "die", "uns", "zu", "ei\u00b7nem", "Bund", "ver\u00b7eint", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "PRELS", "PPER", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "In welchem gute Herzen nie sich tr\u00fcben,", "tokens": ["In", "wel\u00b7chem", "gu\u00b7te", "Her\u00b7zen", "nie", "sich", "tr\u00fc\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "ADJA", "NN", "ADV", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In welchem nie bedr\u00e4ngte Unschuld weint.", "tokens": ["In", "wel\u00b7chem", "nie", "be\u00b7dr\u00e4ng\u00b7te", "Un\u00b7schuld", "weint", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Drum gr\u00f6\u00dfre, edlere der Schwabens\u00f6hne!", "tokens": ["Drum", "gr\u00f6\u00df\u00b7re", ",", "ed\u00b7le\u00b7re", "der", "Schwa\u00b7ben\u00b7s\u00f6h\u00b7ne", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "$,", "ADJA", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "La\u00dft Demut, Demut euer erstes sein,", "tokens": ["La\u00dft", "De\u00b7mut", ",", "De\u00b7mut", "eu\u00b7er", "ers\u00b7tes", "sein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "$,", "NN", "PPOSAT", "ADJA", "VAINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wie sehr das Herz nach Au\u00dfenglanz sich sehne,", "tokens": ["Wie", "sehr", "das", "Herz", "nach", "Au\u00b7\u00dfen\u00b7glanz", "sich", "seh\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "APPR", "NE", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "La\u00dft Demut, Demut euer erstes sein.", "tokens": ["La\u00dft", "De\u00b7mut", ",", "De\u00b7mut", "eu\u00b7er", "ers\u00b7tes", "sein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "$,", "NN", "PPOSAT", "ADJA", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Vor allen, welchen Gott ein Herz gegeben,", "tokens": ["Vor", "al\u00b7len", ",", "wel\u00b7chen", "Gott", "ein", "Herz", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "$,", "PWAT", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das gro\u00df und k\u00f6niglich, und feurig ist,", "tokens": ["Das", "gro\u00df", "und", "k\u00f6\u00b7nig\u00b7lich", ",", "und", "feu\u00b7rig", "ist", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "KON", "ADJD", "$,", "KON", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die in Gefahren nur vor Freude beben,", "tokens": ["Die", "in", "Ge\u00b7fah\u00b7ren", "nur", "vor", "Freu\u00b7de", "be\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "ADV", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "F\u00fcr Tugend selbst auf einem Blutger\u00fcst,", "tokens": ["F\u00fcr", "Tu\u00b7gend", "selbst", "auf", "ei\u00b7nem", "Blut\u00b7ge\u00b7r\u00fcst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Vor allen, allen, solche Schwabens\u00f6hne,", "tokens": ["Vor", "al\u00b7len", ",", "al\u00b7len", ",", "sol\u00b7che", "Schwa\u00b7ben\u0