{"dta.poem.19394": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "127.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Von einem Wandersmann wird nur das Land beschaut,", "tokens": ["Von", "ei\u00b7nem", "Wan\u00b7ders\u00b7mann", "wird", "nur", "das", "Land", "be\u00b7schaut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vom Landbewohner wird's dagegen angebaut.", "tokens": ["Vom", "Land\u00b7be\u00b7woh\u00b7ner", "wird's", "da\u00b7ge\u00b7gen", "an\u00b7ge\u00b7baut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PAV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wo du auf Erden wallst als Pilger voll Beschauung,", "tokens": ["Wo", "du", "auf", "Er\u00b7den", "wallst", "als", "Pil\u00b7ger", "voll", "Be\u00b7schau\u00b7ung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "NN", "VVFIN", "KOUS", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dient's zur Erbauung dir, doch ihr nicht zur Bebauung.", "tokens": ["Dient's", "zur", "Er\u00b7bau\u00b7ung", "dir", ",", "doch", "ihr", "nicht", "zur", "Be\u00b7bau\u00b7ung", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "PPER", "$,", "KON", "PPER", "PTKNEG", "APPRART", "NN", "$."], "meter": "+--+-+--+--+-", "measure": "iambic.penta.invert"}}}}}