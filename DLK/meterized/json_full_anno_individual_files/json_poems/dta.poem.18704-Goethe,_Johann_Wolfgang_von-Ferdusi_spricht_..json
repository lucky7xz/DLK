{"dta.poem.18704": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "Ferdusi  \n spricht .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "O Welt! wie schamlos und boshaft bist du!", "tokens": ["O", "Welt", "!", "wie", "scham\u00b7los", "und", "bos\u00b7haft", "bist", "du", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "PWAV", "ADJD", "KON", "ADJD", "VAFIN", "PPER", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Du n\u00e4hrst und erziehest und t\u00f6dtest zugleich.", "tokens": ["Du", "n\u00e4hrst", "und", "er\u00b7zie\u00b7hest", "und", "t\u00f6d\u00b7test", "zu\u00b7gleich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "ADV", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.2": {"line.1": {"text": "Nur wer von Allah beg\u00fcnstiget ist,", "tokens": ["Nur", "wer", "von", "Al\u00b7lah", "be\u00b7g\u00fcns\u00b7ti\u00b7get", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "APPR", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der n\u00e4hrt sich, erzieht sich, lebendig und reich.", "tokens": ["Der", "n\u00e4hrt", "sich", ",", "er\u00b7zieht", "sich", ",", "le\u00b7ben\u00b7dig", "und", "reich", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "$,", "VVFIN", "PRF", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.3": {"line.1": {"text": "Was heisst denn Reichthum? \u2014 Eine w\u00e4rmende", "tokens": ["Was", "heisst", "denn", "Reicht\u00b7hum", "?", "Ei\u00b7ne", "w\u00e4r\u00b7men\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word"], "pos": ["PWS", "VVFIN", "KON", "NN", "$.", "$(", "ART", "ADJA"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.2": {"text": "Sonne,", "tokens": ["Son\u00b7ne", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Geniesst sie der Bettler, wie wir sie geniessen!", "tokens": ["Ge\u00b7niesst", "sie", "der", "Bett\u00b7ler", ",", "wie", "wir", "sie", "ge\u00b7nies\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "PWAV", "PPER", "PPER", "VVPP", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Es m\u00f6ge doch keinen der Reichen verdriessen", "tokens": ["Es", "m\u00f6\u00b7ge", "doch", "kei\u00b7nen", "der", "Rei\u00b7chen", "ver\u00b7dries\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "PIAT", "ART", "NN", "VVINF"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.5": {"text": "Des Bettlers, im Eigensinn, selige Wonne.", "tokens": ["Des", "Bett\u00b7lers", ",", "im", "Ei\u00b7gen\u00b7sinn", ",", "se\u00b7li\u00b7ge", "Won\u00b7ne", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPRART", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+--+-++--+-", "measure": "iambic.penta.relaxed"}}}}}