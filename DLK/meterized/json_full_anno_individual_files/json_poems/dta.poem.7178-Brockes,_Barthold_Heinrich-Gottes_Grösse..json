{"dta.poem.7178": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Gottes Gr\u00f6sse.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sollt\u2019 ein gantzes Rund der Erden,", "tokens": ["Sollt'", "ein", "gant\u00b7zes", "Rund", "der", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "HeRR, durch Deine Wunder-Hand,", "tokens": ["HeRR", ",", "durch", "Dei\u00b7ne", "Wun\u00b7der\u00b7Hand", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aus jedwedem St\u00e4ubchen werden,", "tokens": ["Aus", "jed\u00b7we\u00b7dem", "St\u00e4ub\u00b7chen", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VAINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und aus jedem K\u00f6rnchen Sand:", "tokens": ["Und", "aus", "je\u00b7dem", "K\u00f6rn\u00b7chen", "Sand", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "W\u00e4r\u2019 ein Creis von solcher Gr\u00f6sse,", "tokens": ["W\u00e4r'", "ein", "Creis", "von", "sol\u00b7cher", "Gr\u00f6s\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Der sie all\u2019 in sich beschl\u00f6sse;", "tokens": ["Der", "sie", "all'", "in", "sich", "be\u00b7schl\u00f6s\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PIS", "APPR", "PRF", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "W\u00fcrd\u2019 er gegen GOTT doch klein,", "tokens": ["W\u00fcrd'", "er", "ge\u00b7gen", "GoTT", "doch", "klein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "NE", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Und nicht einst ein St\u00e4ubchen seyn.", "tokens": ["Und", "nicht", "einst", "ein", "St\u00e4ub\u00b7chen", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ADV", "ART", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}