{"textgrid.poem.44477": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Die Schwestern", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als Gott die Menschen schuf nach seinem Bilde,", "tokens": ["Als", "Gott", "die", "Men\u00b7schen", "schuf", "nach", "sei\u00b7nem", "Bil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sandt er, der karg und unvollendend nie,", "tokens": ["Sandt", "er", ",", "der", "karg", "und", "un\u00b7voll\u00b7en\u00b7dend", "nie", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PRELS", "ADJD", "KON", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Zwei Engel in das werdende Gefilde,", "tokens": ["Zwei", "En\u00b7gel", "in", "das", "wer\u00b7den\u00b7de", "Ge\u00b7fil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+---+-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Die Prosa er genannt und Poesie.", "tokens": ["Die", "Pro\u00b7sa", "er", "ge\u00b7nannt", "und", "Poe\u00b7sie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVPP", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die eine, stark von Wuchs, mit sichern H\u00e4nden,", "tokens": ["Die", "ei\u00b7ne", ",", "stark", "von", "Wuchs", ",", "mit", "si\u00b7chern", "H\u00e4n\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ART", "$,", "ADJD", "APPR", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Betritt den Boden festen Tritts und scharf,", "tokens": ["Be\u00b7tritt", "den", "Bo\u00b7den", "fes\u00b7ten", "Tritts", "und", "scharf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJA", "NN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Des S\u00e4manns Tuch um ihre m\u00e4chtgen Lenden,", "tokens": ["Des", "S\u00e4\u00b7manns", "Tuch", "um", "ih\u00b7re", "m\u00e4cht\u00b7gen", "Len\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Streut sie den Samen jeglichem Bedarf.", "tokens": ["Streut", "sie", "den", "Sa\u00b7men", "jeg\u00b7li\u00b7chem", "Be\u00b7darf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PIAT", "NN", "$."], "meter": "++-+-+-+-+", "measure": "iambic.penta.spondeus"}}, "stanza.3": {"line.1": {"text": "Die andre, zarten Baus und schm\u00e4chtger Glieder,", "tokens": ["Die", "and\u00b7re", ",", "zar\u00b7ten", "Baus", "und", "schm\u00e4cht\u00b7ger", "Glie\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "$,", "ADJA", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den kleinen Fu\u00df von jedem Stein verletzt,", "tokens": ["Den", "klei\u00b7nen", "Fu\u00df", "von", "je\u00b7dem", "Stein", "ver\u00b7letzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Trug, wie den leichten Vogel sein Gefieder,", "tokens": ["Trug", ",", "wie", "den", "leich\u00b7ten", "Vo\u00b7gel", "sein", "Ge\u00b7fie\u00b7der", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ART", "ADJA", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein Fl\u00fcgelpaar, den Schultern angesetzt.", "tokens": ["Ein", "Fl\u00fc\u00b7gel\u00b7paar", ",", "den", "Schul\u00b7tern", "an\u00b7ge\u00b7setzt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "So wandeln sie; die \u00c4ltre, stark und t\u00fcchtig,", "tokens": ["So", "wan\u00b7deln", "sie", ";", "die", "\u00c4l\u00b7tre", ",", "stark", "und", "t\u00fcch\u00b7tig", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "ART", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Erkennt, was dieser Erde n\u00fctzt und frommt,", "tokens": ["Er\u00b7kennt", ",", "was", "die\u00b7ser", "Er\u00b7de", "n\u00fctzt", "und", "frommt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWS", "PDAT", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Indes die J\u00fcngre, eine Botin fl\u00fcchtig,", "tokens": ["In\u00b7des", "die", "J\u00fcng\u00b7re", ",", "ei\u00b7ne", "Bo\u00b7tin", "fl\u00fcch\u00b7tig", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Kunde bringt, die hoch von oben kommt,", "tokens": ["Die", "Kun\u00b7de", "bringt", ",", "die", "hoch", "von", "o\u00b7ben", "kommt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PRELS", "ADJD", "APPR", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Doch ist sie leicht verge\u00dflich, schwanker Sinne,", "tokens": ["Doch", "ist", "sie", "leicht", "ver\u00b7ge\u00df\u00b7lich", ",", "schwan\u00b7ker", "Sin\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADJD", "ADJD", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sie wei\u00df nur halb die Botschaft jener Welt;", "tokens": ["Sie", "wei\u00df", "nur", "halb", "die", "Bot\u00b7schaft", "je\u00b7ner", "Welt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "ART", "NN", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Des wird die strenge Schwester z\u00fcrnend inne,", "tokens": ["Des", "wird", "die", "stren\u00b7ge", "Schwes\u00b7ter", "z\u00fcr\u00b7nend", "in\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VAFIN", "ART", "ADJA", "NN", "VVPP", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der nur, was sicher und was ganz, gef\u00e4llt.", "tokens": ["Der", "nur", ",", "was", "si\u00b7cher", "und", "was", "ganz", ",", "ge\u00b7f\u00e4llt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADV", "$,", "PRELS", "ADJD", "KON", "PWS", "ADV", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Und einst zu Nacht, da scheinbar beide ruhten,", "tokens": ["Und", "einst", "zu", "Nacht", ",", "da", "schein\u00b7bar", "bei\u00b7de", "ruh\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "NN", "$,", "KOUS", "ADJD", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Tritt sie, von Groll bewegt, wohl auch von Neid,", "tokens": ["Tritt", "sie", ",", "von", "Groll", "be\u00b7wegt", ",", "wohl", "auch", "von", "Neid", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "APPR", "NN", "VVFIN", "$,", "ADV", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Still auf den Zehen zu der Leichtgemuten", "tokens": ["Still", "auf", "den", "Ze\u00b7hen", "zu", "der", "Leicht\u00b7ge\u00b7mu\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und raubt ihr raschen Griffs das Fl\u00fcgelkleid", "tokens": ["Und", "raubt", "ihr", "ra\u00b7schen", "Griffs", "das", "Fl\u00fc\u00b7gel\u00b7kleid"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Und pa\u00dft sichs an und schwingt sich in die L\u00fcfte,", "tokens": ["Und", "pa\u00dft", "sichs", "an", "und", "schwingt", "sich", "in", "die", "L\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "PTKVZ", "KON", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Allein der schweren Glieder m\u00e4chtger Bau", "tokens": ["Al\u00b7lein", "der", "schwe\u00b7ren", "Glie\u00b7der", "m\u00e4cht\u00b7ger", "Bau"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Tr\u00e4gt sie nicht h\u00f6her als zum Felsgekl\u00fcfte,", "tokens": ["Tr\u00e4gt", "sie", "nicht", "h\u00f6\u00b7her", "als", "zum", "Fels\u00b7ge\u00b7kl\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ADJD", "KOKOM", "APPRART", "NN", "$,"], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.4": {"text": "Das formlos schaut ins unbegrenzte Blau.", "tokens": ["Das", "form\u00b7los", "schaut", "ins", "un\u00b7be\u00b7grenz\u00b7te", "Blau", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Dem Lichte n\u00e4her, doch nicht den Gestalten,", "tokens": ["Dem", "Lich\u00b7te", "n\u00e4\u00b7her", ",", "doch", "nicht", "den", "Ge\u00b7stal\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,", "ADV", "PTKNEG", "ART", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "In denen sich das Ewge selbst erkennt,", "tokens": ["In", "de\u00b7nen", "sich", "das", "Ew\u00b7ge", "selbst", "er\u00b7kennt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PRF", "ART", "ADJA", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Fehlt unten Raum, den schweren Fu\u00df zu halten,", "tokens": ["Fehlt", "un\u00b7ten", "Raum", ",", "den", "schwe\u00b7ren", "Fu\u00df", "zu", "hal\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "NN", "$,", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nach oben Schwungkraft, die die L\u00fcfte trennt.", "tokens": ["Nach", "o\u00b7ben", "Schwung\u00b7kraft", ",", "die", "die", "L\u00fcf\u00b7te", "trennt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "---+-+-+-+", "measure": "zehnsilber"}}, "stanza.9": {"line.1": {"text": "Und doch zum Werk den trotzgen Mut verbindend,", "tokens": ["Und", "doch", "zum", "Werk", "den", "trotz\u00b7gen", "Mut", "ver\u00b7bin\u00b7dend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "NN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "H\u00f6rt achtlos sie der Schwester Jammerruf,", "tokens": ["H\u00f6rt", "acht\u00b7los", "sie", "der", "Schwes\u00b7ter", "Jam\u00b7mer\u00b7ruf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die, hei\u00dfer Tr\u00e4nen sich am Boden windend,", "tokens": ["Die", ",", "hei\u00b7\u00dfer", "Tr\u00e4\u00b7nen", "sich", "am", "Bo\u00b7den", "win\u00b7dend", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "ADJA", "NN", "PRF", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Saat erdr\u00fcckt, die 