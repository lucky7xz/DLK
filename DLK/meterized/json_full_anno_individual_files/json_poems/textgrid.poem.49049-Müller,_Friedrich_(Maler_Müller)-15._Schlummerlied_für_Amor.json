"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NN", "VAFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Du lieblich \u00fcbergossen,", "tokens": ["Du", "lieb\u00b7lich", "\u00fc\u00b7ber\u00b7gos\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Von Mondenglanz umflossen,", "tokens": ["Von", "Mon\u00b7den\u00b7glanz", "um\u00b7flos\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Vom Sternenlicht gek\u00fcsst.", "tokens": ["Vom", "Ster\u00b7nen\u00b7licht", "ge\u00b7k\u00fcsst", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Still feiert rings die Nacht;", "tokens": ["Still", "fei\u00b7ert", "rings", "die", "Nacht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Zephyre halten Wacht", "tokens": ["Ze\u00b7phy\u00b7re", "hal\u00b7ten", "Wacht"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADJA", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Um die erw\u00e4hlte Rose,", "tokens": ["Um", "die", "er\u00b7w\u00e4hl\u00b7te", "Ro\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Geschm\u00fcckt mit zartem Moose", "tokens": ["Ge\u00b7schm\u00fcckt", "mit", "zar\u00b7tem", "Moo\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und wiegen sanft dich ein;", "tokens": ["Und", "wie\u00b7gen", "sanft", "dich", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Auch flechten Amoretten", "tokens": ["Auch", "flech\u00b7ten", "A\u00b7mo\u00b7ret\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Dir sch\u00f6ne Silberketten", "tokens": ["Dir", "sch\u00f6\u00b7ne", "Sil\u00b7ber\u00b7ket\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["PPER", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Aus Mond- und Sternenschein;", "tokens": ["Aus", "Mon\u00b7d", "und", "Ster\u00b7nen\u00b7schein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "TRUNC", "KON", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Jetzt lassen sie sich nieder,", "tokens": ["Jetzt", "las\u00b7sen", "sie", "sich", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Und senken ihr Gefieder", "tokens": ["Und", "sen\u00b7ken", "ihr", "Ge\u00b7fie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Und schlummern g\u00e4hnend ein.", "tokens": ["Und", "schlum\u00b7mern", "g\u00e4h\u00b7nend", "ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Nur du wachst noch allein!", "tokens": ["Nur", "du", "wachst", "noch", "al\u00b7lein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "O schliess die Augen zu!", "tokens": ["O", "schliess", "die", "Au\u00b7gen", "zu", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Erdkreis liegt in Ruh,", "tokens": ["Der", "Erd\u00b7kreis", "liegt", "in", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Entschlummert schweigt der Hain,", "tokens": ["Ent\u00b7schlum\u00b7mert", "schweigt", "der", "Hain", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Kein Laut entbebt den Triften", "tokens": ["Kein", "Laut", "ent\u00b7bebt", "den", "Trif\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "APPR", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Kein Wiederhall den Kl\u00fcften;", "tokens": ["Kein", "Wie\u00b7der\u00b7hall", "den", "Kl\u00fcf\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Die Nachtigall allein,", "tokens": ["Die", "Nach\u00b7ti\u00b7gall", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Die Heroldin der Liebe,", "tokens": ["Die", "He\u00b7rol\u00b7din", "der", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Sie sinkt verschm\u00e4hte Triebe.", "tokens": ["Sie", "sinkt", "ver\u00b7schm\u00e4h\u00b7te", "Trie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "O gieb der Armen Ruh!", "tokens": ["O", "gieb", "der", "Ar\u00b7men", "Ruh", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "O schliess dein Auge zu!", "tokens": ["O", "schliess", "dein", "Au\u00b7ge", "zu", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "So stillst du ihre Pein.", "tokens": ["So", "stillst", "du", "ih\u00b7re", "Pein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Schlaf, trautes Kind,", "tokens": ["Schlaf", ",", "trau\u00b7tes", "Kind", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.14": {"text": "Schlaf ein!", "tokens": ["Schlaf", "ein", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKVZ", "$."], "meter": "-+", "measure": "iambic.single"}}}}}