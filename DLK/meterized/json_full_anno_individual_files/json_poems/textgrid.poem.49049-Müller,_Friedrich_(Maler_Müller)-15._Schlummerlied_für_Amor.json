{"textgrid.poem.49049": {"metadata": {"author": {"name": "M\u00fcller, Friedrich (Maler M\u00fcller)", "birth": "N.A.", "death": "N.A."}, "title": "15. Schlummerlied f\u00fcr Amor", "genre": "verse", "period": "N.A.", "pub_year": 1787, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schliess, Amor, trautes Kind", "tokens": ["Schliess", ",", "A\u00b7mor", ",", "trau\u00b7tes", "Kind"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["NE", "$,", "NE", "$,", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Schliess, holder Gott, geschwind", "tokens": ["Schliess", ",", "hol\u00b7der", "Gott", ",", "ge\u00b7schwind"], "token_info": ["word", "punct", "word", "word", "punct", "word"], "pos": ["NE", "$,", "ADJA", "NN", "$,", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Augen Zauberschein;", "tokens": ["Der", "Au\u00b7gen", "Zau\u00b7ber\u00b7schein", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Der Schlummer wartet dein,", "tokens": ["Der", "Schlum\u00b7mer", "war\u00b7tet", "dein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mit ihm ein Fr\u00fchlingstr\u00e4umchen.", "tokens": ["Mit", "ihm", "ein", "Fr\u00fch\u00b7lings\u00b7tr\u00e4um\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Dort am Orangenb\u00e4umchen", "tokens": ["Dort", "am", "O\u00b7ran\u00b7gen\u00b7b\u00e4um\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["ADV", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Schl\u00e4ft's noch, vom Flattern matt,", "tokens": ["Schl\u00e4ft's", "noch", ",", "vom", "Flat\u00b7tern", "matt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "$,", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Auf einem Bl\u00fcthenblatt.", "tokens": ["Auf", "ei\u00b7nem", "Bl\u00fc\u00b7then\u00b7blatt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Bald s\u00e4uselt es dir zu", "tokens": ["Bald", "s\u00e4u\u00b7selt", "es", "dir", "zu"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "APPR"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Und senkt sein Goldgefieder", "tokens": ["Und", "senkt", "sein", "Gold\u00b7ge\u00b7fie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Dir auf die Augenlieder,", "tokens": ["Dir", "auf", "die", "Au\u00b7gen\u00b7lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Und s\u00fcsse, s\u00fcsse Ruh.", "tokens": ["Und", "s\u00fcs\u00b7se", ",", "s\u00fcs\u00b7se", "Ruh", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "O schliess den Zauberschein", "tokens": ["O", "schliess", "den", "Zau\u00b7ber\u00b7schein"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Der Augen, und schlaf ein!", "tokens": ["Der", "Au\u00b7gen", ",", "und", "schlaf", "ein", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KON", "VVFIN", "PTKVZ", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Die sch\u00f6nste Rose tr\u00e4gt,", "tokens": ["Die", "sch\u00f6ns\u00b7te", "Ro\u00b7se", "tr\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Von Floren selbst gepflegt,", "tokens": ["Von", "Flo\u00b7ren", "selbst", "ge\u00b7pflegt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Im Purpurschooss dein Bettchen;", "tokens": ["Im", "Pur\u00b7pur\u00b7scho\u00b7oss", "dein", "Bett\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Dich deckt ein Nelkenbl\u00e4ttchen;", "tokens": ["Dich", "deckt", "ein", "Nel\u00b7ken\u00b7bl\u00e4tt\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Von Balsamd\u00fcften bist", "tokens": ["Von", "Bal\u00b7sam\u00b7d\u00fcf\u00b7ten", "bist"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NN", "VAFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Du lieblich \u00fcbergossen,", "tokens": ["Du", "lieb\u00b7lich", "\u00fc\u00b7ber\u00b7gos\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Von Mondenglanz umflossen,", "tokens": ["Von", "Mon\u00b7den\u00b7glanz", "um\u00b7flos\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Vom Sternenlicht gek\u00fcsst.", "tokens": ["Vom", "Ster\u00b7nen\u00b7licht", "ge\u00b7k\u00fcsst", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Still feiert rings die Nacht;", "tokens": ["Still", "fei\u00b7ert", "rings", "die", "Nacht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Zephyre halten Wacht", "tokens": ["Ze\u00b7phy\u00b7re", "hal\u00b7ten", "Wacht"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADJA", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Um die erw\u00e4hlte Rose,", "tokens": ["Um", "die", "er\u00b7w\u00e4hl\u00b7te", "Ro\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Geschm\u00fcckt mit zartem Moose", "tokens": ["Ge\u00b7schm\u00fcckt", "mit", "zar\u00b7tem", "Moo\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und wiegen sanft dich ein;", "tokens": ["Und", "wie\u00b7gen", "sanft", "dich", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Auch flechten Amoretten", "tokens": ["Auch", "flech\u00b7ten", "A\u00b7mo\u00b7ret\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Dir sch\u00f6ne Silberketten", "tokens": ["Dir", "sch\u00f6\u00b7ne", "Sil\u00b7ber\u00b7ket\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["PPER", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Aus Mond- und Sternenschein;", "tokens": ["Aus", "Mon\u00b7d", "und", "Ster\u00b7nen\u00b7schein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "TRUNC", "KON", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Jetzt lassen sie sich nieder,", "tokens": ["Jetzt", "las\u00b7sen", "sie", "sich", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Und senken ihr Gefieder", "tokens": ["Und", "sen\u00b7ken", "ihr", "Ge\u00b7fie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Und schlummern g\u00e4hnend ein.", "tokens": ["Und", "schlum\u00b7mern", "g\u00e4h\u00b7nend", "ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Nur du wachst noch allein!", "tokens": ["Nur", "du", "wachst", "noch", "al\u00b7lein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "O schliess die Augen zu!", "tokens": ["O", "schliess", "die", "Au\u00b7gen", "zu", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Erdkreis liegt in Ruh,", "tokens": ["Der", "Erd\u00b7kreis", "liegt", "in", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Entschlummert schweigt der Hain,", "tokens": ["Ent\u00b7schlum\u00b7mert", "schweigt", "der", "Hain", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Kein Laut entbebt den Triften", "tokens": ["Kein", "Laut", "ent\u00b7bebt", "den", "Trif\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "APPR", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Kein Wiederhall den Kl\u00fcften;", "tokens": ["Kein", "Wie\u00b7der\u00b7hall", "den", "Kl\u00fcf\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Die Nachtigall allein,", "tokens": ["Die", "Nach\u00b7ti\u00b7gall", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Die Heroldin der Liebe,", "tokens": ["Die", "He\u00b7rol\u00b7din", "der", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Sie sinkt verschm\u00e4hte Triebe.", "tokens": ["Sie", "sinkt", "ver\u00b7schm\u00e4h\u00b7te", "Trie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "O gieb der Armen Ruh!", "tokens": ["O", "gieb", "der", "Ar\u00b7men", "Ruh", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "O schliess dein Auge zu!", "tokens": ["O", "schliess", "dein", "Au\u00b7ge", "zu", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "So stillst du ihre Pein.", "tokens": ["So", "stillst", "du", "ih\u00b7re", "Pein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Schlaf, trautes Kind,", "tokens": ["Schlaf", ",", "trau\u00b7tes", "Kind", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.14": {"text": "Schlaf ein!", "tokens": ["Schlaf", "ein", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKVZ", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.5": {"line.1": {"text": "Schliess, Amor, trautes Kind", "tokens": ["Schliess", ",", "A\u00b7mor", ",", "trau\u00b7tes", "Kind"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["NE", "$,", "NE", "$,", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Schliess, holder Gott, geschwind", "tokens": ["Schliess", ",", "hol\u00b7der", "Gott", ",", "ge\u00b7schwind"], "token_info": ["word", "punct", "word", "word", "punct", "word"], "pos": ["NE", "$,", "ADJA", "NN", "$,", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Augen Zauberschein;", "tokens": ["Der", "Au\u00b7gen", "Zau\u00b7ber\u00b7schein", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Der Schlummer wartet dein,", "tokens": ["Der", "Schlum\u00b7mer", "war\u00b7tet", "dein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, 