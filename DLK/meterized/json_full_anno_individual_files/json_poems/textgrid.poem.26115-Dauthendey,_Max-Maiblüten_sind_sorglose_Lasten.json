{"textgrid.poem.26115": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Maibl\u00fcten sind sorglose Lasten", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie des Weines Geist duften die Bl\u00fcten drau\u00dfen,", "tokens": ["Wie", "des", "Wei\u00b7nes", "Geist", "duf\u00b7ten", "die", "Bl\u00fc\u00b7ten", "drau\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "--+-++--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und vorbei ist das endlose Fasten.", "tokens": ["Und", "vor\u00b7bei", "ist", "das", "end\u00b7lo\u00b7se", "Fas\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ein Singen ist tagelang und nachts ohne Pausen,", "tokens": ["Ein", "Sin\u00b7gen", "ist", "ta\u00b7ge\u00b7lang", "und", "nachts", "oh\u00b7ne", "Pau\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "ADV", "APPR", "NN", "$,"], "meter": "-+--+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Maibl\u00fcten sind sorglose Lasten.", "tokens": ["Mai\u00b7bl\u00fc\u00b7ten", "sind", "sorg\u00b7lo\u00b7se", "Las\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADJA", "NN", "$."], "meter": "++--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Alle Liebe kommt allen jetzt zugeflogen", "tokens": ["Al\u00b7le", "Lie\u00b7be", "kommt", "al\u00b7len", "jetzt", "zu\u00b7ge\u00b7flo\u00b7gen"], "token_info": ["word", "word