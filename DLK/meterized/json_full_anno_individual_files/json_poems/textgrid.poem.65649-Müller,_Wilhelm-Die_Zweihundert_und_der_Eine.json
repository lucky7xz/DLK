, "Br\u00fc\u00b7der", ",", "nur", "den", "Ei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PTKNEG", "$,", "VVFIN", "$,", "NN", "$,", "ADV", "ART", "NN", "$,"], "meter": "+--+-+-+-+-+-+-", "measure": "iambic.septa.invert"}, "line.2": {"text": "Der vierhundert Arme kann so zu ", "tokens": ["Der", "vier\u00b7hun\u00b7dert", "Ar\u00b7me", "kann", "so", "zu"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "CARD", "NN", "VMFIN", "ADV", "APPR"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Da\u00df er des Gerichtes Feuer in des W\u00fcrgers Flotte sende.", "tokens": ["Da\u00df", "er", "des", "Ge\u00b7rich\u00b7tes", "Feu\u00b7er", "in", "des", "W\u00fcr\u00b7gers", "Flot\u00b7te", "sen\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "APPR", "ART", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}, "stanza.3": {"line.1": {"text": "Preiset die Zweihundert nicht, preiset, Br\u00fcder, nur den Einen,", "tokens": ["Prei\u00b7set", "die", "Zwei\u00b7hun\u00b7dert", "nicht", ",", "prei\u00b7set", ",", "Br\u00fc\u00b7der", ",", "nur", "den", "Ei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PTKNEG", "$,", "VVFIN", "$,", "NN", "$,", "ADV", "ART", "NN", "$,"], "meter": "+--+-+-+-+-+-+-", "measure": "iambic.septa.invert"}, "line.2": {"text": "Der sich glorreich offenbart in Zweihunderten der Seinen,", "tokens": ["Der", "sich", "glor\u00b7reich", "of\u00b7fen\u00b7bart", "in", "Zwei\u00b7hun\u00b7der\u00b7ten", "der", "Sei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ADJD", "VVFIN", "APPR", "NN", "ART", "PPOSS", "$,"], "meter": "+-+-+-+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Als sie durch der Heiden Segel schifften mit der Kreuzesfahne,", "tokens": ["Als", "sie", "durch", "der", "Hei\u00b7den", "Se\u00b7gel", "schiff\u00b7ten", "mit", "der", "Kreu\u00b7zes\u00b7fah\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Und die hohen Masten bebten vor dem kleinen Wunderkahne.", "tokens": ["Und", "die", "ho\u00b7hen", "Mas\u00b7ten", "beb\u00b7ten", "vor", "dem", "klei\u00b7nen", "Wun\u00b7der\u00b7kah\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}, "stanza.4": {"line.1": {"text": "Preiset die Zweihundert nicht, preiset, Br\u00fcder, nur den Einen,", "tokens": ["Prei\u00b7set", "die", "Zwei\u00b7hun\u00b7dert", "nicht", ",", "prei\u00b7set", ",", "Br\u00fc\u00b7der", ",", "nur", "den", "Ei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PTKNEG", "$,", "VVFIN", "$,", "NN", "$,", "ADV", "ART", "NN", "$,"], "meter": "+--+-+-+-+-+-+-", "measure": "iambic.septa.invert"}, "line.2": {"text": "Der ein gaukelnd Wolkenbild lie\u00df dem Heidenheer erscheinen,", "tokens": ["Der", "ein", "gau\u00b7kelnd", "Wol\u00b7ken\u00b7bild", "lie\u00df", "dem", "Hei\u00b7den\u00b7heer", "er\u00b7schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJD", "NN", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Also da\u00df es, wie geblendet, uns in festlich wildem Drange", "tokens": ["Al\u00b7so", "da\u00df", "es", ",", "wie", "ge\u00b7blen\u00b7det", ",", "uns", "in", "fest\u00b7lich", "wil\u00b7dem", "Dran\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "PPER", "$,", "PWAV", "VVPP", "$,", "PPER", "APPR", "ADJD", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Gr\u00fc\u00dfe bot von nah und ferne mit bet\u00e4ubendem Gesange.", "tokens": ["Gr\u00fc\u00b7\u00dfe", "bot", "von", "nah", "und", "fer\u00b7ne", "mit", "be\u00b7t\u00e4u\u00b7ben\u00b7dem", "Ge\u00b7san\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ADJD", "KON", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}, "stanza.5": {"line.1": {"text": "Preiset die Zweihundert nicht, preiset, Br\u00fcder, nur den Einen,", "tokens": ["Prei\u00b7set", "die", "Zwei\u00b7hun\u00b7dert", "nicht", ",", "prei\u00b7set", ",", "Br\u00fc\u00b7der", ",", "nur", "den", "Ei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PTKNEG", "$,", "VVFIN", "$,", "NN", "$,", "ADV", "ART", "NN", "$,"], "meter": "+--+-+-+-+-+-+-", "measure": "iambic.septa.invert"}, "line.2": {"text": "Dem Zweihundert hier im Staub ihres Dankes Thr\u00e4nen weinen,", "tokens": ["Dem", "Zwei\u00b7hun\u00b7dert", "hier", "im", "Staub", "ih\u00b7res", "Dan\u00b7kes", "Thr\u00e4\u00b7nen", "wei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPRART", "NN", "PPOSAT", "NN", "NN", "VVINF", "$,"], "meter": "-+--+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Da\u00df er ihre Blitzgeschosse hat gelenkt zum rechten Ziele", "tokens": ["Da\u00df", "er", "ih\u00b7re", "Blitz\u00b7ge\u00b7schos\u00b7se", "hat", "ge\u00b7lenkt", "zum", "rech\u00b7ten", "Zie\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VAFIN", "VVPP", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Und des W\u00fcrgers Haupt getroffen auf dem blutgetr\u00e4nkten Pf\u00fchle.", "tokens": ["Und", "des", "W\u00fcr\u00b7gers", "Haupt", "ge\u00b7trof\u00b7fen", "auf", "dem", "blut\u00b7ge\u00b7tr\u00e4nk\u00b7ten", "Pf\u00fch\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "VVPP", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}, "stanza.6": {"line.1": {"text": "Preiset die Zweihundert nicht, preiset, Br\u00fcder, nur den Einen,", "tokens": ["Prei\u00b7set", "die", "Zwei\u00b7hun\u00b7dert", "nicht", ",", "prei\u00b7set", ",", "Br\u00fc\u00b7der", ",", "nur", "den", "Ei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PTKNEG", "$,", "VVFIN", "$,", "NN", "$,", "ADV", "ART", "NN", "$,"], "meter": "+--+-+-+-+-+-+-", "measure": "iambic.septa.invert"}, "line.2": {"text": "Der sein schreckliches Gericht lie\u00df dem Heidenvolk erscheinen,", "tokens": ["Der", "sein", "schreck\u00b7li\u00b7ches", "Ge\u00b7richt", "lie\u00df", "dem", "Hei\u00b7den\u00b7volk", "er\u00b7schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Also da\u00df sie seine Wunder predigten in den Moscheen;", "tokens": ["Al\u00b7so", "da\u00df", "sie", "sei\u00b7ne", "Wun\u00b7der", "pre\u00b7dig\u00b7ten", "in", "den", "Mo\u00b7scheen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+--+--+", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Denn sie sahn die Todesengel leiblich in den Wolken stehen.", "tokens": ["Denn", "sie", "sahn", "die", "To\u00b7de\u00b7sen\u00b7gel", "leib\u00b7lich", "in", "den", "Wol\u00b7ken", "ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "ADJD", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}, "stanza.7": {"line.1": {"text": "Preiset die Zweihundert nicht, preiset, Br\u00fcder, nur den Einen,", "tokens": ["Prei\u00b7set", "die", "Zwei\u00b7hun\u00b7dert", "nicht", ",", "prei\u00b7set", ",", "Br\u00fc\u00b7der", ",", "nur", "den", "Ei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PTKNEG", "$,", "VVFIN", "$,", "NN", "$,", "ADV", "ART", "NN", "$,"], "meter": "+--+-+-+-+-+-+-", "measure": "iambic.septa.invert"}, "line.2": {"text": "Der Zweihundert kann so fest in der Liebe Gluth vereinen.", "tokens": ["Der", "Zwei\u00b7hun\u00b7dert", "kann", "so", "fest", "in", "der", "Lie\u00b7be", "Gluth", "ver\u00b7ei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "ADJD", "APPR", "ART", "ADJA", "NN", "VVINF", "$.