{"dta.poem.14686": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "J\u00e4ger und J\u00e4gerin.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Weil ich so sehr verliebet bin,", "tokens": ["Weil", "ich", "so", "sehr", "ver\u00b7lie\u00b7bet", "bin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "VVFIN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wollt' ich das Hirschlein, sch\u00f6n und wild,", "tokens": ["Wollt'", "ich", "das", "Hirsc\u00b7hlein", ",", "sch\u00f6n", "und", "wild", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Aufsuchen tief im Walde drinn", "tokens": ["Auf\u00b7su\u00b7chen", "tief", "im", "Wal\u00b7de", "drinn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADJD", "APPRART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und streicheln, bis es stille hielt.", "tokens": ["Und", "strei\u00b7cheln", ",", "bis", "es", "stil\u00b7le", "hielt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}