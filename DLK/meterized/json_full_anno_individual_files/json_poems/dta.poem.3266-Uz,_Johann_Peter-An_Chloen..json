{"dta.poem.3266": {"metadata": {"author": {"name": "Uz, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "An Chloen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1755", "urn": "urn:nbn:de:kobv:b4-20614-6", "language": ["de:0.99"], "booktitle": "Uz, Johann Peter: Lyrische und andere Gedichte. 2. Aufl. Ansbach, 1755."}, "poem": {"stanza.1": {"line.1": {"text": "Die Munterkeit ist meinen Wangen,", "tokens": ["Die", "Mun\u00b7ter\u00b7keit", "ist", "mei\u00b7nen", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den Augen Glut und Sprach entgangen;", "tokens": ["Den", "Au\u00b7gen", "Glut", "und", "Sprach", "ent\u00b7gan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der Mund will kaum ein L\u00e4cheln wagen;", "tokens": ["Der", "Mund", "will", "kaum", "ein", "L\u00e4\u00b7cheln", "wa\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Kaum will der welke Leib sich tragen,", "tokens": ["Kaum", "will", "der", "wel\u00b7ke", "Leib", "sich", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "ADJA", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der Bluhmen am Mittage gleicht,", "tokens": ["Der", "Bluh\u00b7men", "am", "Mit\u00b7ta\u00b7ge", "gleicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.6": {"text": "Wann Flora lechzt und Zephyr weicht.", "tokens": ["Wann", "Flo\u00b7ra", "lechzt", "und", "Ze\u00b7phyr", "weicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Doch merk ich, wann sich Chloe zeiget,", "tokens": ["Doch", "merk", "ich", ",", "wann", "sich", "Chloe", "zei\u00b7get", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "PWAV", "PRF", "NE", "VVFIN", "$,"], "meter": "-+-++-+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Da\u00df mein entflammter Blick nicht schweiget,", "tokens": ["Da\u00df", "mein", "ent\u00b7flamm\u00b7ter", "Blick", "nicht", "schwei\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und Suada nach den Lippen flieget;", "tokens": ["Und", "Sua\u00b7da", "nach", "den", "Lip\u00b7pen", "flie\u00b7get", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein gl\u00fchend Roth im Antlitz sieget,", "tokens": ["Ein", "gl\u00fc\u00b7hend", "Roth", "im", "Ant\u00b7litz", "sie\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und alles sich an mir verj\u00fcngt,", "tokens": ["Und", "al\u00b7les", "sich", "an", "mir", "ver\u00b7j\u00fcngt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "PRF", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.6": {"text": "Wie Bluhmen, die der Thau durchdringt.", "tokens": ["Wie", "Bluh\u00b7men", ",", "die", "der", "Thau", "durch\u00b7dringt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ich seh auf sie mit bangem Sehnen,", "tokens": ["Ich", "seh", "auf", "sie", "mit", "ban\u00b7gem", "Seh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und kann den Blick nicht weggew\u00f6hnen:", "tokens": ["Und", "kann", "den", "Blick", "nicht", "weg\u00b7ge\u00b7w\u00f6h\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die Anmuth, die im Auge wachet", "tokens": ["Die", "An\u00b7muth", ",", "die", "im", "Au\u00b7ge", "wa\u00b7chet"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und um die jungen Wangen lachet,", "tokens": ["Und", "um", "die", "jun\u00b7gen", "Wan\u00b7gen", "la\u00b7chet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Zieht meinen weggewichnen Blick", "tokens": ["Zieht", "mei\u00b7nen", "weg\u00b7ge\u00b7wich\u00b7nen", "Blick"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit g\u00fcldnen Banden stets zur\u00fcck.", "tokens": ["Mit", "g\u00fcld\u00b7nen", "Ban\u00b7den", "stets", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}