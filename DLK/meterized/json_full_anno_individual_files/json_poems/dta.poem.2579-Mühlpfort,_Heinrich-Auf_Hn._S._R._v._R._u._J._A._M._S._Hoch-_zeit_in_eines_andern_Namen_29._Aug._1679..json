rd", "word", "word", "punct"], "pos": ["KON", "NE", "ADV", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Pales trotzet Samm\u2019t und Seiden/", "tokens": ["Die", "Pa\u00b7les", "trot\u00b7zet", "Sam\u00b7m't", "und", "Sei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NE", "KON", "NN", "$("], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wenn sie ihr Vieh l\u00e4st weyden gehn;", "tokens": ["Wenn", "sie", "ihr", "Vieh", "l\u00e4st", "wey\u00b7den", "gehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Ceres lehrt mit ihren Garben/", "tokens": ["Die", "Ce\u00b7res", "lehrt", "mit", "ih\u00b7ren", "Gar\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df nichts an Reichthum noch gebricht/", "tokens": ["Da\u00df", "nichts", "an", "Reicht\u00b7hum", "noch", "ge\u00b7bricht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "NN", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wenn andre L\u00e4nder m\u00fcssen darben", "tokens": ["Wenn", "and\u00b7re", "L\u00e4n\u00b7der", "m\u00fcs\u00b7sen", "dar\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJA", "NN", "VMFIN", "PAV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Hat sie ein Korn-Haus aufgericht.", "tokens": ["Hat", "sie", "ein", "Korn\u00b7Haus", "auf\u00b7ge\u00b7richt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Alleine diese Treffligkeiten", "tokens": ["Al\u00b7lei\u00b7ne", "die\u00b7se", "Tref\u00b7flig\u00b7kei\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["ADV", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie herrlich man sie sch\u00e4tzen mag/", "tokens": ["Wie", "herr\u00b7lich", "man", "sie", "sch\u00e4t\u00b7zen", "mag", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PIS", "PPER", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Thun nichts zu den\u2019n Vollkommenheiten", "tokens": ["Thun", "nichts", "zu", "den'n", "Voll\u00b7kom\u00b7men\u00b7hei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und halten nicht die Gegen-Wag\u2019/", "tokens": ["Und", "hal\u00b7ten", "nicht", "die", "Ge\u00b7gen\u00b7Wag'", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "In welcher sich sein Hertz ietzt wieget/", "tokens": ["In", "wel\u00b7cher", "sich", "sein", "Hertz", "ietzt", "wie\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PRF", "PPOSAT", "NN", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Herr Br\u00e4utigam/ das nunmehr brennt/", "tokens": ["Herr", "Br\u00e4u\u00b7ti\u00b7gam", "/", "das", "nun\u00b7mehr", "brennt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$(", "PDS", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": 