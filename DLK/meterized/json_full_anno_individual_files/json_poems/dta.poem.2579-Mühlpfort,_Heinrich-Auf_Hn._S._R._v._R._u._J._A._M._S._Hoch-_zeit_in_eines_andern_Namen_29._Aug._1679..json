", "word"], "pos": ["ADV", "VAFIN", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Von solchem Fadem/ Zeug und Krafft/", "tokens": ["Von", "sol\u00b7chem", "Fa\u00b7dem", "/", "Zeug", "und", "Krafft", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$(", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da\u00df sie ihm das Gebl\u00fct aussaugen", "tokens": ["Da\u00df", "sie", "ihm", "das", "Ge\u00b7bl\u00fct", "aus\u00b7sau\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und nehmen Hertz und Seel\u2019 in hafft.", "tokens": ["Und", "neh\u00b7men", "Hertz", "und", "Seel'", "in", "hafft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "KON", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Es sey da\u00df uns viel Silber-Kuchen", "tokens": ["Es", "sey", "da\u00df", "uns", "viel", "Sil\u00b7ber\u00b7Ku\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "KOUS", "PPER", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zusenden kan die neue Welt;", "tokens": ["Zu\u00b7sen\u00b7den", "kan", "die", "neu\u00b7e", "Welt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Hier kan er mehr Vergn\u00fcgung suchen/", "tokens": ["Hier", "kan", "er", "mehr", "Ver\u00b7gn\u00fc\u00b7gung", "su\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PIAT", "NN", "V