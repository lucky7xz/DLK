{"dta.poem.2579": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Auf Hn. S. R. v. R. u. J. A. M. S. Hoch-  \n zeit in eines andern Namen/ 29. Aug.  \n 1679.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Soll denn die Vor-Burg deutscher Erden/", "tokens": ["Soll", "denn", "die", "Vor\u00b7Burg", "deut\u00b7scher", "Er\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Das Schatz-Haus aller Fruchtbarkeit/", "tokens": ["Das", "Schatz\u00b7Haus", "al\u00b7ler", "Frucht\u00b7bar\u00b7keit", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Barbarn Raub und Beute werden/", "tokens": ["Der", "Bar\u00b7barn", "Raub", "und", "Beu\u00b7te", "wer\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "NN", "VAINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Durch Enderung der b\u00f6sen Zeit?", "tokens": ["Durch", "En\u00b7de\u00b7rung", "der", "b\u00f6\u00b7sen", "Zeit", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Gewehrt es nicht mehr seine Gaben", "tokens": ["Ge\u00b7wehrt", "es", "nicht", "mehr", "sei\u00b7ne", "Ga\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKNEG", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und zieh\u2019t uns seine G\u00fcter ein/", "tokens": ["Und", "zieh't", "uns", "sei\u00b7ne", "G\u00fc\u00b7ter", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN", "ART", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "L\u00e4st es die Berge nicht durchgraben", "tokens": ["L\u00e4st", "es", "die", "Ber\u00b7ge", "nicht", "durch\u00b7gra\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKNEG", "VVINF"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Und wil ohn Gold und Silber seyn?", "tokens": ["Und", "wil", "ohn", "Gold", "und", "Sil\u00b7ber", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "APPR", "NN", "KON", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Nein: Unger-Land steht dennoch feste/", "tokens": ["Nein", ":", "Un\u00b7ger\u00b7Land", "steht", "den\u00b7noch", "fes\u00b7te", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "NN", "VVFIN", "ADV", "ADJA", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie sehr ietzt Blitz und Donner kracht/", "tokens": ["Wie", "sehr", "ietzt", "Blitz", "und", "Don\u00b7ner", "kracht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADV", "NN", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie hoch es kr\u00e4ncken wilde G\u00e4ste", "tokens": ["Wie", "hoch", "es", "kr\u00e4n\u00b7cken", "wil\u00b7de", "G\u00e4s\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "PPER", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit zugesto\u00