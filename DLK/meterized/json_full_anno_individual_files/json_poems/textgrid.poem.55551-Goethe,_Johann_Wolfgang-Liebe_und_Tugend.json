{"textgrid.poem.55551": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Liebe und Tugend", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn einem M\u00e4dchen, das uns liebt,", "tokens": ["Wenn", "ei\u00b7nem", "M\u00e4d\u00b7chen", ",", "das", "uns", "liebt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Mutter strenge Lehren gibt", "tokens": ["Die", "Mut\u00b7ter", "stren\u00b7ge", "Leh\u00b7ren", "gibt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von Tugend, Keuschheit und von Pflicht,", "tokens": ["Von", "Tu\u00b7gend", ",", "Keuschheit", "und", "von", "Pflicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Und unser M\u00e4dchen folgt ihr nicht", "tokens": ["Und", "un\u00b7ser", "M\u00e4d\u00b7chen", "folgt", "ihr", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PPER", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und fliegt mit neuverst\u00e4rktem Triebe", "tokens": ["Und", "fliegt", "mit", "neu\u00b7ver\u00b7st\u00e4rk\u00b7tem", "Trie\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Zu unsern hei\u00dfen K\u00fcssen hin,", "tokens": ["Zu", "un\u00b7sern", "hei\u00b7\u00dfen", "K\u00fcs\u00b7sen", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da hat daran der Eigensinn", "tokens": ["Da", "hat", "da\u00b7ran", "der", "Ei\u00b7gen\u00b7sinn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PAV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "So vielen Anteil als die Liebe.", "tokens": ["So", "vie\u00b7len", "An\u00b7teil", "als", "die", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Doch wenn die Mutter es erreicht,", "tokens": ["Doch", "wenn", "die", "Mut\u00b7ter", "es", "er\u00b7reicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df sie das gute Herz erweicht,", "tokens": ["Da\u00df", "sie", "das", "gu\u00b7te", "Herz", "er\u00b7weicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Voll Stolz auf ihre Lehren sieht,", "tokens": ["Voll", "Stolz", "auf", "ih\u00b7re", "Leh\u00b7ren", "sieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df uns das M\u00e4dchen spr\u00f6de flieht,", "tokens": ["Da\u00df", "uns", "das", "M\u00e4d\u00b7chen", "spr\u00f6\u00b7de", "flieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So kennt sie nicht das Herz der Jugend;", "tokens": ["So", "kennt", "sie", "nicht", "das", "Herz", "der", "Ju\u00b7gend", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Denn wenn das je ein M\u00e4dchen tut,", "tokens": ["Denn", "wenn", "das", "je", "ein", "M\u00e4d\u00b7chen", "tut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PDS", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So hat daran der Wankelmut", "tokens": ["So", "hat", "da\u00b7ran", "der", "Wan\u00b7kel\u00b7mut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PAV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Gewi\u00df mehr Anteil als die Tugend.", "tokens": ["Ge\u00b7wi\u00df", "mehr", "An\u00b7teil", "als", "die", "Tu\u00b7gend", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wenn einem M\u00e4dchen, das uns liebt,", "tokens": ["Wenn", "ei\u00b7nem", "M\u00e4d\u00b7chen", ",", "das", "uns", "liebt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Mutter strenge Lehren gibt", "tokens": ["Die", "Mut\u00b7ter", "stren\u00b7ge", "Leh\u00b7ren", "gibt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART",