{"dta.poem.9266": {"metadata": {"author": {"name": "Weise, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Das andere Gespr\u00e4ch.", "genre": "Lyrik; Drama; Prosa", "period": "N.A.", "pub_year": "1701", "urn": "urn:nbn:de:kobv:b4-25043-0", "language": ["de:0.99"], "booktitle": "Weise, Christian: \u00dcberfl\u00fc\u00dfige Gedancken Der gr\u00fcnenden jugend. Leipzig, 1701."}, "poem": {"stanza.1": {"line.1": {"text": "Mein geist verzeihe mir/ da\u00df ich dem schweren leibe", "tokens": ["Mein", "geist", "ver\u00b7zei\u00b7he", "mir", "/", "da\u00df", "ich", "dem", "schwe\u00b7ren", "lei\u00b7be"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "$(", "KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gehorsam leisten mu\u00df/ der mich gefangen h\u00e4lt", "tokens": ["Ge\u00b7hor\u00b7sam", "leis\u00b7ten", "mu\u00df", "/", "der", "mich", "ge\u00b7fan\u00b7gen", "h\u00e4lt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "VVINF", "VMFIN", "$(", "PRELS", "PRF", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "An einem solchen ort/ da ich gezwungen bleibe/", "tokens": ["An", "ei\u00b7nem", "sol\u00b7chen", "ort", "/", "da", "ich", "ge\u00b7zwun\u00b7gen", "blei\u00b7be", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PIAT", "NN", "$(", "KOUS", "PPER", "VVPP", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und da mir fast kein mensch und keine lust gef\u00e4llt.", "tokens": ["Und", "da", "mir", "fast", "kein", "mensch", "und", "kei\u00b7ne", "lust", "ge\u00b7f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "PIAT", "NN", "KON", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich wolte lieber seyn wo meine b\u00fccher liegen/", "tokens": ["Ich", "wol\u00b7te", "lie\u00b7ber", "seyn", "wo", "mei\u00b7ne", "b\u00fc\u00b7cher", "lie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "VAINF", "PWAV", "PPOSAT", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Doch leide die gewalt die ich vertragen mu\u00df/", "tokens": ["Doch", "lei\u00b7de", "die", "ge\u00b7walt", "die", "ich", "ver\u00b7tra\u00b7gen", "mu\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ART", "PPER", "VVFIN", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Was heute nicht geschicht das kan sich morgen f\u00fcgen/", "tokens": ["Was", "heu\u00b7te", "nicht", "ge\u00b7schicht", "das", "kan", "sich", "mor\u00b7gen", "f\u00fc\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PTKNEG", "VVPP", "PDS", "VMFIN", "PRF", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Du f\u00fchlest diese nacht den letzten \u00fcberdru\u00df.", "tokens": ["Du", "f\u00fch\u00b7lest", "die\u00b7se", "nacht", "den", "letz\u00b7ten", "\u00fc\u00b7berd\u00b7ru\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Was soll ein eitles wort ein unvern\u00fcnfftig lachen/", "tokens": ["Was", "soll", "ein", "eit\u00b7les", "wort", "ein", "un\u00b7ver\u00b7n\u00fcnff\u00b7tig", "la\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "ADJA", "NN", "ART", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ein winck/ ein leichter blick/ ein halbes kinderspiel/", "tokens": ["Ein", "win\u00b7ck", "/", "ein", "leich\u00b7ter", "blick", "/", "ein", "hal\u00b7bes", "kin\u00b7der\u00b7spiel", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "ADJA", "NN", "$(", "ART", "ADJA", "NN", "$("], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "Ein ungereimter tantz mir doch vor freude machen/", "tokens": ["Ein", "un\u00b7ge\u00b7reim\u00b7ter", "tantz", "mir", "doch", "vor", "freu\u00b7de", "ma\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "ADV", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Wenn ich verdrie\u00dflich bin und mich entreissen wil.", "tokens": ["Wenn", "ich", "ver\u00b7drie\u00df\u00b7lich", "bin", "und", "mich", "en\u00b7treis\u00b7sen", "wil", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VAFIN", "KON", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ja freylich hilfft es nicht; wiewohl von guten freunden", "tokens": ["Ja", "frey\u00b7lich", "hilfft", "es", "nicht", ";", "wie\u00b7wohl", "von", "gu\u00b7ten", "freun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "ADV", "VVFIN", "PPER", "PTKNEG", "$.", "KOUS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Nim\u0303t man dergleichen zwang aus guter meinung an:", "tokens": ["Nim\u0303t", "man", "derg\u00b7lei\u00b7chen", "zwang", "aus", "gu\u00b7ter", "mei\u00b7nung", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PIS", "VVFIN", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Doch wolt ich lieber seyn bey meinen \u00e4rgsten feinden/", "tokens": ["Doch", "wolt", "ich", "lie\u00b7ber", "seyn", "bey", "mei\u00b7nen", "\u00e4rgs\u00b7ten", "fein\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "VAINF", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Da man doch \u00f6ffentlich den zwang verschlagen kan.", "tokens": ["Da", "man", "doch", "\u00f6f\u00b7fent\u00b7lich", "den", "zwang", "ver\u00b7schla\u00b7gen", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "ADJD", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.17": {"text": "Wolan der abendstern ist itzo mein Prophete/", "tokens": ["Wo\u00b7lan", "der", "a\u00b7bends\u00b7tern", "ist", "it\u00b7zo", "mein", "Pro\u00b7phe\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "VAFIN", "ADV", "PPOSAT", "NN", "$("], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.18": {"text": "Der rufft mir gleichsam zu: Geh fort und schlaffe wohl/", "tokens": ["Der", "rufft", "mir", "gleich\u00b7sam", "zu", ":", "Geh", "fort", "und", "schlaf\u00b7fe", "wohl", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADJD", "PTKVZ", "$.", "NE", "PTKVZ", "KON", "VVFIN", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Und dencke da\u00df das liecht der s\u00fcssen morgenr\u00f6hte/", "tokens": ["Und", "den\u00b7cke", "da\u00df", "das", "liecht", "der", "s\u00fcs\u00b7sen", "mor\u00b7gen\u00b7r\u00f6h\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KOUS", "ART", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Dich von der schn\u00f6den lust gewi\u00df erl\u00f6sen soll.", "tokens": ["Dich", "von", "der", "schn\u00f6\u00b7den", "lust", "ge\u00b7wi\u00df", "er\u00b7l\u00f6\u00b7sen", "soll", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "ADJA", "NN", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}