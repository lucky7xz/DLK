{"textgrid.poem.48675": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "38. An Anna, die spr\u00f6de", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als Echo ward zu einem Schalle,", "tokens": ["Als", "E\u00b7cho", "ward", "zu", "ei\u00b7nem", "Schal\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VAFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "zu einer unbeleibten Luft,", "tokens": ["zu", "ei\u00b7ner", "un\u00b7be\u00b7leib\u00b7ten", "Luft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "die durch das Tal mit halbem Halle,", "tokens": ["die", "durch", "das", "Tal", "mit", "hal\u00b7bem", "Hal\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "die, so sie rufen, wieder ruft,", "tokens": ["die", ",", "so", "sie", "ru\u00b7fen", ",", "wie\u00b7der", "ruft", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "$,", "ADV", "PPER", "VVINF", "$,", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "da ward der hole Wald voll Klage,", "tokens": ["da", "ward", "der", "ho\u00b7le", "Wald", "voll", "Kla\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "das feige Wild stund als bet\u00f6rt,", "tokens": ["das", "fei\u00b7ge", "Wild", "stund", "als", "be\u00b7t\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "KOKOM", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "die Nymfen ruften Nacht und Tage,", "tokens": ["die", "Nym\u00b7fen", "ruf\u00b7ten", "Nacht", "und", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "wo bist du, Lust, die man nur h\u00f6rt?", "tokens": ["wo", "bist", "du", ",", "Lust", ",", "die", "man", "nur", "h\u00f6rt", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "$,", "NN", "$,", "PRELS", "PIS", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Narzissus, dir ist recht geschehen,", "tokens": ["Nar\u00b7zis\u00b7sus", ",", "dir", "ist", "recht", "ge\u00b7sche\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VAFIN", "ADJD", "VVPP", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "vor sahst du sie und woltst sie nicht.", "tokens": ["vor", "sahst", "du", "sie", "und", "woltst", "sie", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "PPER", "PPER", "KON", "VMFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Itzt wilt du, die du nicht kanst sehen,", "tokens": ["Itzt", "wilt", "du", ",", "die", "du", "nicht", "kanst", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "$,", "PRELS", "PPER", "PTKNEG", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "und h\u00f6rst nur, was sie dir nachspricht.", "tokens": ["und", "h\u00f6rst", "nur", ",", "was", "sie", "dir", "nach\u00b7spricht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "$,", "PRELS", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Brunnen, der dich dich lie\u00df schauen,", "tokens": ["Der", "Brun\u00b7nen", ",", "der", "dich", "dich", "lie\u00df", "schau\u00b7en", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "PPER", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "der strafte deinen stolzen Muth,", "tokens": ["der", "straf\u00b7te", "dei\u00b7nen", "stol\u00b7zen", "Muth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "da\u00df nun nicht eine von den Frauen", "tokens": ["da\u00df", "nun", "nicht", "ei\u00b7ne", "von", "den", "Frau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "PTKNEG", "ART", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "dir bis auf diesen Tag ist gut.", "tokens": ["dir", "bis", "auf", "die\u00b7sen", "Tag", "ist", "gut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "PDAT", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Nicht, nicht so ist mein Sinn gesinnet,", "tokens": ["Nicht", ",", "nicht", "so", "ist", "mein", "Sinn", "ge\u00b7sin\u00b7net", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "$,", "PTKNEG", "ADV", "VAFIN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "bei mir ist alles umgewant.", "tokens": ["bei", "mir", "ist", "al\u00b7les", "um\u00b7ge\u00b7want", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "PIS", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich liebe, die mir B\u00f6ses g\u00f6nnet,", "tokens": ["Ich", "lie\u00b7be", ",", "die", "mir", "B\u00f6\u00b7ses", "g\u00f6n\u00b7net", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PRELS", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "ich folge der, die nicht h\u00e4lt Stand.", "tokens": ["ich", "fol\u00b7ge", "der", ",", "die", "nicht", "h\u00e4lt", "Stand", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "$,", "PRELS", "PTKNEG", "VVFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich lauf', ich ruf', ich bitt', ich weine;", "tokens": ["Ich", "lauf'", ",", "ich", "ruf'", ",", "ich", "bitt'", ",", "ich", "wei\u00b7ne", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "sie weicht und schweigt und stellt sich taub.", "tokens": ["sie", "weicht", "und", "schweigt", "und", "stellt", "sich", "taub", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "PRF", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Sie l\u00e4ugnets und ists doch alleine,", "tokens": ["Sie", "l\u00e4ug\u00b7nets", "und", "ists", "doch", "al\u00b7lei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VAFIN", "ADV", "ADV", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.8": {"text": "die mir mein Herze nimmt in Raub.", "tokens": ["die", "mir", "mein", "Her\u00b7ze", "nimmt", "in", "Raub", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPOSAT", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ach, Freundin, scheu der G\u00f6tter Rache,", "tokens": ["Ach", ",", "Freun\u00b7din", ",", "scheu", "der", "G\u00f6t\u00b7ter", "Ra\u00b7che", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "NN", "$,", "ADJD", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "da\u00df da dir nicht zu sehr gef\u00e4llst,", "tokens": ["da\u00df", "da", "dir", "nicht", "zu", "sehr", "ge\u00b7f\u00e4llst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "PTKNEG", "PTKA", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "da\u00df Amor nicht einst deiner lache,", "tokens": ["da\u00df", "A\u00b7mor", "nicht", "einst", "dei\u00b7ner", "la\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "PTKNEG", "ADV", "PPOSAT", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "den du itzt h\u00f6hnst und sp\u00f6ttlich h\u00e4ltst.", "tokens": ["den", "du", "itzt", "h\u00f6hnst", "und", "sp\u00f6tt\u00b7lich", "h\u00e4ltst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADJD", "KON", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da\u00df, weil du nichts von mir wilst wissen,", "tokens": ["Da\u00df", ",", "weil", "du", "nichts", "von", "mir", "wilst", "wis\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "KOUS", "PPER", "PIS", "APPR", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "ich nicht mit Echo lasse mich,", "tokens": ["ich", "nicht", "mit", "E\u00b7cho", "las\u00b7se", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "APPR", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "und du denn m\u00fcssest mit Narzissen", "tokens": ["und", "du", "denn", "m\u00fcs\u00b7sest", "mit", "Nar\u00b7zis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "ADV", "VMFIN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "selbst lieben und doch hassen dich.", "tokens": ["selbst", "lie\u00b7ben", "und", "doch", "has\u00b7sen", "dich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "KON", "ADV", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Als Echo ward zu einem Schalle,", "tokens": ["Als", "E\u00b7cho", "ward", "zu", "ei\u00b7nem", "Schal\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VAFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "zu einer unbeleibten Luft,", "tokens": ["zu", "ei\u00b7ner", "un\u00b7be\u00b7leib\u00b7ten", "Luft", ","], "to