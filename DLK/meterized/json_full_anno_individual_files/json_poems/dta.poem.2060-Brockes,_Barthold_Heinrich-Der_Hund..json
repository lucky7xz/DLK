{"dta.poem.2060": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Hund.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Noch ersieht man an den Hunden, so, da\u00df es kaum zu", "tokens": ["Noch", "er\u00b7sieht", "man", "an", "den", "Hun\u00b7den", ",", "so", ",", "da\u00df", "es", "kaum", "zu"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "APPR", "ART", "NN", "$,", "ADV", "$,", "KOUS", "PPER", "ADV", "APPR"], "meter": "+-+-+-+-++--+", "measure": "iambic.septa.chol"}, "line.2": {"text": "begreifen,", "tokens": ["be\u00b7grei\u00b7fen", ","], "token_info": ["word", "punct"], "pos": ["VVINF", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Wie mehr, als bey allen Thieren, ihre Meng\u2019 und Art", "tokens": ["Wie", "mehr", ",", "als", "bey", "al\u00b7len", "Thie\u00b7ren", ",", "ih\u00b7re", "Meng'", "und", "Art"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "$,", "KOUS", "APPR", "PIAT", "NN", "$,", "PPOSAT", "NN", "KON", "NN"], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "sich h\u00e4ufen;", "tokens": ["sich", "h\u00e4u\u00b7fen", ";"], "token_info": ["word", "word", "punct"], "pos": ["PRF", "VVINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Seht und z\u00e4hlet mit Bewundrung, wie so viel- und man-", "tokens": ["Seht", "und", "z\u00e4h\u00b7let", "mit", "Be\u00b7wund\u00b7rung", ",", "wie", "so", "viel", "und", "man"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "KON", "VVFIN", "APPR", "NN", "$,", "PWAV", "ADV", "TRUNC", "KON", "TRUNC"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.6": {"text": "cherley,", "tokens": ["cher\u00b7ley", ","], "token_info": ["word", "punct"], "pos": ["NE", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.7": {"text": "Und in einer jeden Art wieder solche Menge sey.", "tokens": ["Und", "in", "ei\u00b7ner", "je\u00b7den", "Art", "wie\u00b7der", "sol\u00b7che", "Men\u00b7ge", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "PIAT", "NN", "ADV", "PIAT", "NN", "VAFIN", "$."], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.8": {"text": "Wenn man denn f\u00fcr andre Thiere unserm Gott zu danken,", "tokens": ["Wenn", "man", "denn", "f\u00fcr", "and\u00b7re", "Thie\u00b7re", "un\u00b7serm", "Gott", "zu", "dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "APPR", "ADJA", "NN", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.9": {"text": "pflichtig;", "tokens": ["pflich\u00b7tig", ";"], "token_info": ["word", "punct"], "pos": ["ADJD", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.10": {"text": "So sind es die Hunde gleichfalls, da die Dienste gro\u00df", "tokens": ["So", "sind", "es", "die", "Hun\u00b7de", "gleich\u00b7falls", ",", "da", "die", "Diens\u00b7te", "gro\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "ADV", "$,", "KOUS", "ART", "NN", "ADJD"], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "und wichtig,", "tokens": ["und", "wich\u00b7tig", ","], "token_info": ["word", "word", "punct"], "pos": ["KON", "ADJD", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.12": {"text": "Welche wir von ihnen haben: Ja sie scheinen uns im Le-", "tokens": ["Wel\u00b7che", "wir", "von", "ih\u00b7nen", "ha\u00b7ben", ":", "Ja", "sie", "schei\u00b7nen", "uns", "im", "Le"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "APPR", "PPER", "VAFIN", "$.", "PTKANT", "PPER", "VVFIN", "PPER", "APPRART", "TRUNC"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.13": {"text": "ben,", "tokens": ["ben", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-", "measure": "single.down"}, "line.14": {"text": "Au\u00dfer ihrem vielen Nutzen, zur Gesellschaft fast gege-", "tokens": ["Au\u00b7\u00dfer", "ih\u00b7rem", "vie\u00b7len", "Nut\u00b7zen", ",", "zur", "Ge\u00b7sell\u00b7schaft", "fast", "ge\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "PIAT", "NN", "$,", "APPRART", "NN", "ADV", "TRUNC"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.15": {"text": "ben.", "tokens": ["ben", "."], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-", "measure": "single.down"}}}}}