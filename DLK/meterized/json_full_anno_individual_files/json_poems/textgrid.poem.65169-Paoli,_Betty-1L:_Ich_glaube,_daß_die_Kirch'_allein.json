{"textgrid.poem.65169": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich glaube, da\u00df die Kirch' allein", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich glaube, da\u00df die Kirch' allein", "tokens": ["Ich", "glau\u00b7be", ",", "da\u00df", "die", "Kirch'", "al\u00b7lein"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Seelen hat zu walten,", "tokens": ["Der", "See\u00b7len", "hat", "zu", "wal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df nur in ihrem lichten Schrein", "tokens": ["Da\u00df", "nur", "in", "ih\u00b7rem", "lich\u00b7ten", "Schrein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Wahrheit Gut enthalten.", "tokens": ["Der", "Wahr\u00b7heit", "Gut", "ent\u00b7hal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ich glaube, da\u00df die Priester, die", "tokens": ["Ich", "glau\u00b7be", ",", "da\u00df", "die", "Pries\u00b7ter", ",", "die"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ART", "NN", "$,", "PRELS"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sie an den Feind verraten,", "tokens": ["Sie", "an", "den", "Feind", "ver\u00b7ra\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Von Gott verworfen dort und hie", "tokens": ["Von", "Gott", "ver\u00b7wor\u00b7fen", "dort", "und", "hie"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "ADV", "KON", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Als schn\u00f6de Apostaten.", "tokens": ["Als", "schn\u00f6\u00b7de", "A\u00b7pos\u00b7ta\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Ich glaube, da\u00df ein Sakrament,", "tokens": ["Ich", "glau\u00b7be", ",", "da\u00df", "ein", "Sak\u00b7ra\u00b7ment", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Von solcher Hand bescheret,", "tokens": ["Von", "sol\u00b7cher", "Hand", "be\u00b7sche\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Wie Glut des H\u00f6llenpfuhles brennt,", "tokens": ["Wie", "Glut", "des", "H\u00f6l\u00b7len\u00b7pfuh\u00b7les", "brennt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "In Fluch den Segen kehret.", "tokens": ["In", "Fluch", "den", "Se\u00b7gen", "keh\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Ich glaub', da\u00df es ein heil'ger Krieg", "tokens": ["Ich", "glaub'", ",", "da\u00df", "es", "ein", "heil'\u00b7ger", "Krieg"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "In dem wir jetzo streiten,", "tokens": ["In", "dem", "wir", "jet\u00b7zo", "strei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "Und da\u00df uns auf dem Weg zum Sieg", "tokens": ["Und", "da\u00df", "uns", "auf", "dem", "Weg", "zum", "Sieg"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "APPR", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Voran die Engel schreiten.", "tokens": ["Vo\u00b7ran", "die", "En\u00b7gel", "schrei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich hoffe, da\u00df der Tag nicht fern,", "tokens": ["Ich", "hof\u00b7fe", ",", "da\u00df", "der", "Tag", "nicht", "fern", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ART", "NN", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der alles Unheil wendet!", "tokens": ["Der", "al\u00b7les", "Un\u00b7heil", "wen\u00b7det", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Tag, an dem die Hand des Herrn", "tokens": ["Der", "Tag", ",", "an", "dem", "die", "Hand", "des", "Herrn"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "APPR", "PRELS", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das gro\u00dfe Werk vollendet.", "tokens": ["Das", "gro\u00b7\u00dfe", "Werk", "voll\u00b7en\u00b7det", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ich hoffe, da\u00df das K\u00f6nigskind,", "tokens": ["Ich", "hof\u00b7fe", ",", "da\u00df", "das", "K\u00f6\u00b7nigs\u00b7kind", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Jetzt zu Paris gefangen,", "tokens": ["Jetzt", "zu", "Pa\u00b7ris", "ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NE", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Die Krone bald zur\u00fcckgewinnt,", "tokens": ["Die", "Kro\u00b7ne", "bald", "zu\u00b7r\u00fcck\u00b7ge\u00b7winnt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die Frevler ihm entrangen.", "tokens": ["Die", "Frev\u00b7ler", "ihm", "ent\u00b7ran\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Ich hoffe, da\u00df der gute Hirt", "tokens": ["Ich", "hof\u00b7fe", ",", "da\u00df", "der", "gu\u00b7te", "Hirt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Der gl\u00e4ubigen Gemeinde,", "tokens": ["Der", "gl\u00e4u\u00b7bi\u00b7gen", "Ge\u00b7mein\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Uns nicht zu Schanden machen wird", "tokens": ["Uns", "nicht", "zu", "Schan\u00b7den", "ma\u00b7chen", "wird"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "PTKNEG", "APPR", "NN", "VVINF", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Vor seinem grimmen Feinde.", "tokens": ["Vor", "sei\u00b7nem", "grim\u00b7men", "Fein\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Und wenn mein irdisch Teil verdirbt,", "tokens": ["Und", "wenn", "mein", "ir\u00b7disch", "Teil", "ver\u00b7dirbt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Lebendig bleibt mein Hoffen!", "tokens": ["Le\u00b7ben\u00b7dig", "bleibt", "mein", "Hof\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.15": {"text": "Denn, wer in diesem Kampfe stirbt,", "tokens": ["Denn", ",", "wer", "in", "die\u00b7sem", "Kamp\u00b7fe", "stirbt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWS", "APPR", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Dem steht der Himmel offen!", "tokens": ["Dem", "steht", "der", "Him\u00b7mel", "of\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ich liebe wie mein eigen Blut", "tokens": ["Ich", "lie\u00b7be", "wie", "mein", "ei\u00b7gen", "Blut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KOKOM", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des heil'gen Ludwig Erben,", "tokens": ["Des", "heil'\u00b7gen", "Lud\u00b7wig", "Er\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Prinzen, die mit treuem Mut", "tokens": ["Die", "Prin\u00b7zen", ",", "die", "mit", "treu\u00b7em", "Mut"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im Ausland f\u00fcr ihn werben!", "tokens": ["Im", "Aus\u00b7land", "f\u00fcr", "ihn", "wer\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ich lieb' die Herr'n, die, heldenhaft", "tokens": ["Ich", "lieb'", "die", "Herr'n", ",", "die", ",", "hel\u00b7den\u00b7haft"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PRELS", "$,", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mitfechtend in den Reihen,", "tokens": ["Mit\u00b7fech\u00b7tend", "in", "den", "Rei\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Nicht minder wie des Armes Kraft", "tokens": ["Nicht", "min\u00b7der", "wie", "des", "Ar\u00b7mes", "Kraft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "KOKOM", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der Einsicht Rat uns leihen!", "tokens": ["Der", "Ein\u00b7sicht", "Rat", "uns", "lei\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Ich liebe recht vom Herzensgrund,", "tokens": ["Ich", "lie\u00b7be", "recht", "vom", "Her\u00b7zens\u00b7grund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Ob ich sie nie gesehen,", "tokens": ["Ob", "ich", "sie", "nie", "ge\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "All jene, die zu unsrem Bund", "tokens": ["All", "je\u00b7ne", ",", "die", "zu", "uns\u00b7rem", "Bund"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIAT", "PDS", "$,", "PRELS", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Mit ihren W\u00fcnschen stehen!", "tokens": ["Mit", "ih\u00b7ren", "W\u00fcn\u00b7schen", "ste\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Dies glaube, hoffe, liebe ich", "tokens": ["Dies", "glau\u00b7be", ",", "hof\u00b7fe", ",", "lie\u00b7be", "ich"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PDS", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "In unsres Heilands Namen!", "tokens": ["In", "uns\u00b7res", "Hei\u00b7lands", "Na\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "Herr Jesu Christ! erbarme dich", "tokens": ["Herr", "Je\u00b7su", "Christ", "!", "er\u00b7bar\u00b7me", "dich"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "NE", "NE", "$.", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Des jungen K\u00f6nigs! Amen.", "tokens": ["Des", "jun\u00b7gen", "K\u00f6\u00b7nigs", "!", "A\u00b7men", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ich glaube, da\u00df die Kirch' allein", "tokens": ["Ich", "glau\u00b7be", ",", "da\u00df", "die", "Kirch'", "al\u00b7lein"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Seelen hat zu walten,", "tokens": ["Der", "See\u00b7len", "hat", "zu", "wal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df nur in ihrem lichten Schrein", "tokens": ["Da\u00df", "nur", "in", "ih\u00b7rem", "lich\u00b7ten", "Schrein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Wahrheit Gut enthalten.", "tokens": ["Der", "Wahr\u00b7heit", "Gut", "ent\u00b7hal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ich glaube, da\u00df die Priester, die", "tokens": ["Ich", 