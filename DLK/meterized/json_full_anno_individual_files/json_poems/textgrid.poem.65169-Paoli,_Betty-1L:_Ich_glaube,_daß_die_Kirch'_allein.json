easure": "iambic.tetra"}, "line.12": {"text": "In Fluch den Segen kehret.", "tokens": ["In", "Fluch", "den", "Se\u00b7gen", "keh\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Ich glaub', da\u00df es ein heil'ger Krieg", "tokens": ["Ich", "glaub'", ",", "da\u00df", "es", "ein", "heil'\u00b7ger", "Krieg"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "In dem wir jetzo streiten,", "tokens": ["In", "dem", "wir", "jet\u00b7zo", "strei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "Und da\u00df uns auf dem Weg zum Sieg", "tokens": ["Und", "da\u00df", "uns", "auf", "dem", "Weg", "zum", "Sieg"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "APPR", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Voran die Engel schreiten.", "tokens": ["Vo\u00b7ran", "die", "En\u00b7gel", "schrei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ich hoffe, da\u00df der Tag nicht fern,", "tokens": ["Ich", "hof\u00b7fe", ",", "da\u00df", "der", "Tag", "nicht", "fern", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ART", "NN", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der alles Unheil wendet!", "tokens": ["Der", "al\u0