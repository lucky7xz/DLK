{"dta.poem.462": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Auf die Blumen in der  Doris  Busen.  \n Aus dem Frantz\u00f6sischen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Wie werdt ihr Blumen doch verehret/", "tokens": ["Wie", "werdt", "ihr", "Blu\u00b7men", "doch", "ver\u00b7eh\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPOSAT", "NN", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df ihr in ", "tokens": ["Da\u00df", "ihr", "in"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPER", "APPR"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Da\u00df eure schlechte Pracht", "tokens": ["Da\u00df", "eu\u00b7re", "schlech\u00b7te", "Pracht"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Zur Zierde wird begehret;", "tokens": ["Zur", "Zier\u00b7de", "wird", "be\u00b7ge\u00b7hret", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Da\u00df euch ein Blumen-Topff umfa\u00dft/", "tokens": ["Da\u00df", "euch", "ein", "Blu\u00b7men\u00b7Topff", "um\u00b7fa\u00dft", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der auch den ", "tokens": ["Der", "auch", "den"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADV", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Ein jeder neidet euch/", "tokens": ["Ein", "je\u00b7der", "nei\u00b7det", "euch", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "PPER", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Da\u00df ihr den Sitz", "tokens": ["Da\u00df", "ihr", "den", "Sitz"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Recht in der Anmuth Reich/", "tokens": ["Recht", "in", "der", "An\u00b7muth", "Reich", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.10": {"text": "Am ersten k\u00f6nnt bekommen.", "tokens": ["Am", "ers\u00b7ten", "k\u00f6nnt", "be\u00b7kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Doch wird der Schmuck euch bald benommen:", "tokens": ["Doch", "wird", "der", "Schmuck", "euch", "bald", "be\u00b7nom\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Weil ihr vor Hitze sterben m\u00fcst.", "tokens": ["Weil", "ihr", "vor", "Hit\u00b7ze", "ster\u00b7ben", "m\u00fcst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Allein! d\u00fcrfft ich da auch ersterben/", "tokens": ["Al\u00b7lein", "!", "d\u00fcrfft", "ich", "da", "auch", "ers\u00b7ter\u00b7ben", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "VMFIN", "PPER", "ADV", "ADV", "VVPP", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.14": {"text": "Ich w\u00fcrde \u00e4usserst mich bem\u00fchn", "tokens": ["Ich", "w\u00fcr\u00b7de", "\u00e4us\u00b7serst", "mich", "be\u00b7m\u00fchn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "PPER", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Da\u00df ich bey ihrem Schoo\u00df Rubin", "tokens": ["Da\u00df", "ich", "bey", "ih\u00b7rem", "Schoo\u00df", "Ru\u00b7bin"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Ein Grab-Mahl k\u00f6nnt erwerben.", "tokens": ["Ein", "Grab\u00b7Mahl", "k\u00f6nnt", "er\u00b7wer\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}