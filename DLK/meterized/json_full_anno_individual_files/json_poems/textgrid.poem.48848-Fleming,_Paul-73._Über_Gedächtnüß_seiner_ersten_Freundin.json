{"textgrid.poem.48848": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "73. \u00dcber Ged\u00e4chtn\u00fc\u00df seiner ersten Freundin", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Noch dennoch bleib' ich ihr, mu\u00df ich sie gleich verlassen,", "tokens": ["Noch", "den\u00b7noch", "bleib'", "ich", "ihr", ",", "mu\u00df", "ich", "sie", "gleich", "ver\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "PPER", "$,", "VMFIN", "PPER", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "und meine sie, mu\u00df ich gleich ihr entzogen sein,", "tokens": ["und", "mei\u00b7ne", "sie", ",", "mu\u00df", "ich", "gleich", "ihr", "ent\u00b7zo\u00b7gen", "sein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "VMFIN", "PPER", "ADV", "PPER", "VVPP", "VAINF", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "bezwungen durch das Tun, das unsern Trost und Pein", "tokens": ["be\u00b7zwun\u00b7gen", "durch", "das", "Tun", ",", "das", "un\u00b7sern", "Trost", "und", "Pein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVPP", "APPR", "ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "verwechselt, wie es will. Ich will mein Tr\u00fcbn\u00fc\u00df massen,", "tokens": ["ver\u00b7wech\u00b7selt", ",", "wie", "es", "will", ".", "Ich", "will", "mein", "Tr\u00fcb\u00b7n\u00fc\u00df", "mas\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PWAV", "PPER", "VMFIN", "$.", "PPER", "VMFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "tun wie ein Weiser tut, ein gro\u00dfes Herze fassen,", "tokens": ["tun", "wie", "ein", "Wei\u00b7ser", "tut", ",", "ein", "gro\u00b7\u00dfes", "Her\u00b7ze", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVINF", "KOKOM", "ART", "NN", "VVFIN", "$,", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "sein meine, wie ich soll. Sie aller Tugend Schein,", "tokens": ["sein", "mei\u00b7ne", ",", "wie", "ich", "soll", ".", "Sie", "al\u00b7ler", "Tu\u00b7gend", "Schein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAINF", "VVFIN", "$,", "PWAV", "PPER", "VMFIN", "$.", "PPER", "PIAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "mein Alles und auch Nichts, ist nicht und ist doch mein'.", "tokens": ["mein", "Al\u00b7les", "und", "auch", "Nichts", ",", "ist", "nicht", "und", "ist", "doch", "mein'", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "PIS", "KON", "ADV", "PIS", "$,", "VAFIN", "PTKNEG", "KON", "VAFIN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Hass' ich das sch\u00f6ne Kind, so mu\u00df ich selbst mich hassen.", "tokens": ["Hass'", "ich", "das", "sch\u00f6\u00b7ne", "Kind", ",", "so", "mu\u00df", "ich", "selbst", "mich", "has\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "ADJA", "NN", "$,", "ADV", "VMFIN", "PPER", "ADV", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Verh\u00e4ngn\u00fc\u00df, schone nicht, rei\u00df sie nur immer hin,", "tokens": ["Ver\u00b7h\u00e4ng\u00b7n\u00fc\u00df", ",", "scho\u00b7ne", "nicht", ",", "rei\u00df", "sie", "nur", "im\u00b7mer", "hin", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PTKNEG", "$,", "VVFIN", "PPER", "ADV", "ADV", "PTKVZ", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "du raubst mir ihren Leib, nicht aber ihren Sinn,", "tokens": ["du", "raubst", "mir", "ih\u00b7ren", "Leib", ",", "nicht", "a\u00b7ber", "ih\u00b7ren", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPOSAT", "NN", "$,", "PTKNEG", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "der nun und nimmermehr von mir spricht sich zu lenken.", "tokens": ["der", "nun", "und", "nim\u00b7mer\u00b7mehr", "von", "mir", "spricht", "sich", "zu", "len\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "KON", "ADV", "APPR", "PPER", "VVFIN", "PRF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Mir bleibt dein bester Teil, o meiner Seelen Licht,", "tokens": ["Mir", "bleibt", "dein", "bes\u00b7ter", "Teil", ",", "o", "mei\u00b7ner", "See\u00b7len", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$,", "FM", "PPOSAT", "NN", "NN", "