{"textgrid.poem.25157": {"metadata": {"author": {"name": "Keats, John", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich schreie: hab Erbarmen! \u2013 Mitleid! \u2013 Liebe!", "genre": "verse", "period": "N.A.", "pub_year": 1819, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich schreie: hab Erbarmen! \u2013 Mitleid! \u2013 Liebe!", "tokens": ["Ich", "schrei\u00b7e", ":", "hab", "Er\u00b7bar\u00b7men", "!", "\u2013", "Mit\u00b7leid", "!", "\u2013", "Lie\u00b7be", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct", "word", "punct", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "VAFIN", "NN", "$.", "$(", "NN", "$.", "$(", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Liebe, die sich erbarmt und die nicht qu\u00e4lt,", "tokens": ["Lie\u00b7be", ",", "die", "sich", "er\u00b7barmt", "und", "die", "nicht", "qu\u00e4lt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PRF", "VVPP", "KON", "ART", "PTKNEG", "VVFIN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Best\u00e4ndige, arglose, offene Liebe,", "tokens": ["Be\u00b7st\u00e4n\u00b7di\u00b7ge", ",", "arg\u00b7lo\u00b7se", ",", "of\u00b7fe\u00b7ne", "Lie\u00b7be", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Die, makellos, sich keine Maske w\u00e4hlt.", "tokens": ["Die", ",", "ma\u00b7kel\u00b7los", ",", "sich", "kei\u00b7ne", "Mas\u00b7ke", "w\u00e4hlt", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "ADJD", "$,", "PRF", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "O gib dich ganz! Sei mein \u2013 sei meinem Flehen!", "tokens": ["O", "gib", "dich", "ganz", "!", "Sei", "mein", "\u2013", "sei", "mei\u00b7nem", "Fle\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVIMP", "PPER", "ADV", "$.", "VAFIN", "PPOSAT", "$(", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gestalt und Antlitz \u2013 s\u00fc\u00dfer kleiner Mund \u2013", "tokens": ["Ge\u00b7stalt", "und", "Ant\u00b7litz", "\u2013", "s\u00fc\u00b7\u00dfer", "klei\u00b7ner", "Mund", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$(", "ADJD", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Himmlische Augen, H\u00e4nde, die verstehen,", "tokens": ["Himm\u00b7li\u00b7sche", "Au\u00b7gen", ",", "H\u00e4n\u00b7de", ",", "die", "ver\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "NN", "$,", "PRELS", "VVINF", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Der warmen Br\u00fcste freudevolles Rund, \u2013", "tokens": ["Der", "war\u00b7men", "Br\u00fcs\u00b7te", "freu\u00b7de\u00b7vol\u00b7les", "Rund", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$,", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Gib deine Seele \u2013 gib dein ganzes All,", "tokens": ["Gib", "dei\u00b7ne", "See\u00b7le", "\u2013", "gib", "dein", "gan\u00b7zes", "All", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "$(", "VVIMP", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "mea