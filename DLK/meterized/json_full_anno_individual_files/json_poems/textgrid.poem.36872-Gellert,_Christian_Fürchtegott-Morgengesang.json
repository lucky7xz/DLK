{"textgrid.poem.36872": {"metadata": {"author": {"name": "Gellert, Christian F\u00fcrchtegott", "birth": "N.A.", "death": "N.A."}, "title": "Morgengesang", "genre": "verse", "period": "N.A.", "pub_year": 1742, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein erst Gef\u00fchl sei Preis und Dank;", "tokens": ["Mein", "erst", "Ge\u00b7f\u00fchl", "sei", "Preis", "und", "Dank", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADV", "NN", "VAFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Erheb ihn, meine Seele!", "tokens": ["Er\u00b7heb", "ihn", ",", "mei\u00b7ne", "See\u00b7le", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Herr h\u00f6rt deinen Lobgesang;", "tokens": ["Der", "Herr", "h\u00f6rt", "dei\u00b7nen", "Lob\u00b7ge\u00b7sang", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Lobsing ihm, meine Seele!", "tokens": ["Lob\u00b7sing", "ihm", ",", "mei\u00b7ne", "See\u00b7le", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Mich selbst zu sch\u00fctzen, ohne Macht,", "tokens": ["Mich", "selbst", "zu", "sch\u00fct\u00b7zen", ",", "oh\u00b7ne", "Macht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADV", "PTKZU", "VVINF", "$,", "KOUI", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Lag ich und schlief im Frieden.", "tokens": ["Lag", "ich", "und", "schlief", "im", "Frie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KON", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wer schafft die Sicherheit der Nacht,", "tokens": ["Wer", "schafft", "die", "Si\u00b7cher\u00b7heit", "der", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und Ruhe f\u00fcr die M\u00fcden?", "tokens": ["Und", "Ru\u00b7he", "f\u00fcr", "die", "M\u00fc\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wer wacht, wenn ich von mir nichts wei\u00df,", "tokens": ["Wer", "wacht", ",", "wenn", "ich", "von", "mir", "nichts", "wei\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "KOUS", "PPER", "APPR", "PPER", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Leben zu bewahren?", "tokens": ["Mein", "Le\u00b7ben", "zu", "be\u00b7wah\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wer st\u00e4rkt mein Blut in seinem Flei\u00df,", "tokens": ["Wer", "st\u00e4rkt", "mein", "Blut", "in", "sei\u00b7nem", "Flei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und sch\u00fctzt mich vor Gefahren?", "tokens": ["Und", "sch\u00fctzt", "mich", "vor", "Ge\u00b7fah\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wer lehrt das Auge seine Pflicht,", "tokens": ["Wer", "lehrt", "das", "Au\u00b7ge", "sei\u00b7ne", "Pflicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sich sicher zu bedecken?", "tokens": ["Sich", "si\u00b7cher", "zu", "be\u00b7de\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wer ruft dem Tag und seinem Licht,", "tokens": ["Wer", "ruft", "dem", "Tag", "und", "sei\u00b7nem", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Seele zu erwecken?", "tokens": ["Die", "See\u00b7le", "zu", "er\u00b7we\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Du bist es, Herr und Gott der Welt,", "tokens": ["Du", "bist", "es", ",", "Herr", "und", "Gott", "der", "Welt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "$,", "NN", "KON", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und dein ist unser Leben.", "tokens": ["Und", "dein", "ist", "un\u00b7ser", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Du bist es, der es uns erh\u00e4lt,", "tokens": ["Du", "bist", "es", ",", "der", "es", "uns", "er\u00b7h\u00e4lt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "$,", "PRELS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und mir's itzt neu gegeben.", "tokens": ["Und", "mir's", "itzt", "neu", "ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Gelobet seist du, Gott der Macht,", "tokens": ["Ge\u00b7lo\u00b7bet", "seist", "du", ",", "Gott", "der", "Macht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "$,", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gelobt sei deine Treue!", "tokens": ["Ge\u00b7lobt", "sei", "dei\u00b7ne", "Treu\u00b7e", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df ich nach einer sanften Nacht", "tokens": ["Da\u00df", "ich", "nach", "ei\u00b7ner", "sanf\u00b7ten", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mich dieses Tags erfreue.", "tokens": ["Mich", "die\u00b7ses", "Tags", "er\u00b7freu\u00b7e", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "La\u00df deinen Segen auf mir ruhn,", "tokens": ["La\u00df", "dei\u00b7nen", "Se\u00b7gen", "auf", "mir", "ruhn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mich deine Wege wallen;", "tokens": ["Mich", "dei\u00b7ne", "We\u00b7ge", "wal\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und lehre du mich selber tun", "tokens": ["Und", "leh\u00b7re", "du", "mich", "sel\u00b7ber", "tun"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PRF", "ADV", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nach deinem Wohlgefallen.", "tokens": ["Nach", "dei\u00b7nem", "Wohl\u00b7ge\u00b7fal\u00b7len", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Nimm meines Lebens gn\u00e4dig wahr;", "tokens": ["Nimm", "mei\u00b7nes", "Le\u00b7bens", "gn\u00e4\u00b7dig", "wahr", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf dich hofft meine Seele.", "tokens": ["Auf", "dich", "hofft", "mei\u00b7ne", "See\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Sei mir ein Retter in Gefahr,", "tokens": ["Sei", "mir", "ein", "Ret\u00b7ter", "in", "Ge\u00b7fahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Vater, wenn ich fehle.", "tokens": ["Ein", "Va\u00b7ter", ",", "wenn", "ich", "feh\u00b7le", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Gib mir ein Herz voll Zuversicht,", "tokens": ["Gib", "mir", "ein", "Herz", "voll", "Zu\u00b7ver\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Erf\u00fcllt mit Lieb und Ruhe,", "tokens": ["Er\u00b7f\u00fcllt", "mit", "Lieb", "und", "Ru\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein weises Herz, das seine Pflicht", "tokens": ["Ein", "wei\u00b7ses", "Herz", ",", "das", "sei\u00b7ne", "Pflicht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Erkenn und willig tue.", "tokens": ["Er\u00b7kenn", "und", "wil\u00b7lig", "tue", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Da\u00df ich, als ein getreuer Knecht,", "tokens": ["Da\u00df", "ich", ",", "als", "ein", "ge\u00b7treu\u00b7er", "Knecht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "KOUS", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nach deinem Reiche strebe,", "tokens": ["Nach", "dei\u00b7nem", "Rei\u00b7che", "stre\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NE", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Gottselig, z\u00fcchtig und gerecht", "tokens": ["Gott\u00b7se\u00b7lig", ",", "z\u00fcch\u00b7tig", "und", "ge\u00b7recht"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADJD", "$,", "ADJD", "KON", "ADJD"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Durch deine Gnade lebe.", "tokens": ["Durch", "dei\u00b7ne", "Gna\u00b7de", "le\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Da\u00df ich, dem N\u00e4chsten beizustehn,", "tokens": ["Da\u00df", "ich", ",", "dem", "N\u00e4chs\u00b7ten", "bei\u00b7zu\u00b7stehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nie Flei\u00df und Arbeit scheue,", "tokens": ["Nie", "Flei\u00df", "und", "Ar\u00b7beit", "scheu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mich gern an andrer Wohlergehn", "tokens": ["Mich", "gern", "an", "an\u00b7drer", "Woh\u00b7ler\u00b7gehn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und ihrer Tugend freue.", "tokens": ["Und", "ih\u00b7rer", "Tu\u00b7gend", "freu\u00b7e", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Da\u00df ich das Gl\u00fcck der Lebenszeit", "tokens": ["Da\u00df", "ich", "das", "Gl\u00fcck", "der", "Le\u00b7bens\u00b7zeit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In deiner Furcht genie\u00dfe,", "tokens": ["In", "dei\u00b7ner", "Furcht", "ge\u00b7nie\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und meinen Lauf mit Freudigkeit,", "tokens": ["Und", "mei\u00b7nen", "Lauf", "mit", "Freu\u00b7dig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn du gebeutst, beschlie\u00dfe.", "tokens": ["Wenn", "du", "ge\u00b7beutst", ",", "be\u00b7schlie\u00b7\u00dfe", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Mein erst Gef\u00fchl sei Preis und Dank;", "tokens": ["Mein", "erst", "Ge\u00b7f\u00fchl", "sei", "Preis", "und", "Dank", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADV", "NN", "VAFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Erheb ihn, meine Seele!", "tokens": ["Er\u00b7heb", "ihn", ",", "mei\u00b7ne", "See\u00b7le", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Herr h\u00f6rt deinen Lobgesang;", "tokens": ["Der", "Herr", "h\u00f6rt", "dei\u00b7nen", "Lob\u00b7ge\u00b7sang", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Lobsing ihm, meine Seele!", "tokens": ["Lob\u00b7sing", "ihm", ",", "mei\u00b7ne", "See\u00b7le", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Mich selbst zu sch\u00fctzen, ohne Macht,", "tokens": ["Mich", "selbst", "zu", "sch\u00fct\u00b7zen", ",", "oh\u00b7ne", "Macht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADV", "PTKZU", "VVINF", "$,", "KOUI", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Lag ich und schlief im Frieden.", "tokens": ["Lag", "ich", "und", "schlief", "im", "Frie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KON", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wer schafft die Sicherheit der Nacht,", "tokens": ["Wer", "schafft", "die", "Si\u00b7cher\u00b7heit", "der", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und Ruhe f\u00fcr die M\u00fcden?", "tokens": ["Und", "Ru\u00b7he", "f\u00fcr", "die", "M\u00fc\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Wer wacht, wenn ich von mir nichts wei\u00df,", "tokens": ["Wer", "wacht", ",", "wenn", "ich", "von", "mir", "nichts", "wei\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "KOUS", "PPER", "APPR", "PPER", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measu