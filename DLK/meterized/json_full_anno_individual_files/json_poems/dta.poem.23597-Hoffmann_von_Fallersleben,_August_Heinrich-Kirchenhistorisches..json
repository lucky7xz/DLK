{"dta.poem.23597": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Kirchenhistorisches.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1840", "urn": "urn:nbn:de:kobv:b4-200905192626", "language": ["de:0.85", "da:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dank, Luther, Dank! du lehrtest jeden", "tokens": ["Dank", ",", "Lu\u00b7ther", ",", "Dank", "!", "du", "lehr\u00b7test", "je\u00b7den"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NE", "$,", "NN", "$.", "PPER", "VVFIN", "PIAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Gott in deutscher Sprache reden,", "tokens": ["Mit", "Gott", "in", "deut\u00b7scher", "Spra\u00b7che", "re\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Hast uns zu Gottes Preis und Ruhm", "tokens": ["Hast", "uns", "zu", "Got\u00b7tes", "Preis", "und", "Ruhm"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "APPR", "NN", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gebracht ein deutsches Christenthum.", "tokens": ["Ge\u00b7bracht", "ein", "deut\u00b7sches", "Chris\u00b7ten\u00b7thum", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Doch hat uns unter deinem Schilde", "tokens": ["Doch", "hat", "uns", "un\u00b7ter", "dei\u00b7nem", "Schil\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Gebracht die Philologengilde", "tokens": ["Ge\u00b7bracht", "die", "Phi\u00b7lo\u00b7lo\u00b7gen\u00b7gil\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Zu ihrem eignen Preis und Ruhm", "tokens": ["Zu", "ih\u00b7rem", "eig\u00b7nen", "Preis", "und", "Ruhm"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein protestantisch Heidenthum.", "tokens": ["Ein", "pro\u00b7tes\u00b7tan\u00b7tisch", "Hei\u00b7den\u00b7thum", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}