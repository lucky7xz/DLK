{"textgrid.poem.40222": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "Dichterfreundschaft", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Euch suchte ich. O kindisch weich Bewegen,", "tokens": ["Euch", "such\u00b7te", "ich", ".", "O", "kin\u00b7disch", "weich", "Be\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$.", "NE", "ADJD", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "o st\u00fcrmisch Wogen alter Liebeslust!", "tokens": ["o", "st\u00fcr\u00b7misch", "Wo\u00b7gen", "al\u00b7ter", "Lie\u00b7bes\u00b7lust", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM", "ADJD", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "der schwellende Strom dem rollenden Meer entgegen,", "tokens": ["der", "schwel\u00b7len\u00b7de", "Strom", "dem", "rol\u00b7len\u00b7den", "Meer", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+--+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "so st\u00fcrz' ich mich an eure Brust.", "tokens": ["so", "st\u00fcrz'", "ich", "mich", "an", "eu\u00b7re", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Noch Einmal rauschen auf aus ihren Gr\u00fcnden", "tokens": ["Noch", "Ein\u00b7mal", "rau\u00b7schen", "auf", "aus", "ih\u00b7ren", "Gr\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "APPR", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "der ersten Jugendsehnsucht scheue Laute,", "tokens": ["der", "ers\u00b7ten", "Ju\u00b7gend\u00b7sehn\u00b7sucht", "scheu\u00b7e", "Lau\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "zur Brandung sch\u00e4umend: in die Flut zu m\u00fcnden,", "tokens": ["zur", "Bran\u00b7dung", "sch\u00e4u\u00b7mend", ":", "in", "die", "Flut", "zu", "m\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$.", "APPR", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "die brausend lockende, die urvertraute ...", "tokens": ["die", "brau\u00b7send", "lo\u00b7cken\u00b7de", ",", "die", "ur\u00b7ver\u00b7trau\u00b7te", "..."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "$,", "ART", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "O langer Weg! oh, einst! wie jede Welle", "tokens": ["O", "lan\u00b7ger", "Weg", "!", "oh", ",", "einst", "!", "wie", "je\u00b7de", "Wel\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NE", "ADJA", "NN", "$.", "FM", "$,", "ADV", "$.", "PWAV", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "nach jedem W\u00f6lkchen sah am Himmelsrande,", "tokens": ["nach", "je\u00b7dem", "W\u00f6lk\u00b7chen", "sah", "am", "Him\u00b7mels\u00b7ran\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "nach jedem H\u00e4lmchen griff am Ufersande,", "tokens": ["nach", "je\u00b7dem", "H\u00e4lm\u00b7chen", "griff", "am", "U\u00b7fer\u00b7san\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "da\u00df Eines nur mitstr\u00f6mend weiterquelle!", "tokens": ["da\u00df", "Ei\u00b7nes", "nur", "mit\u00b7str\u00f6\u00b7mend", "wei\u00b7ter\u00b7quel\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "oh keines! fort! nur Bilder, wirr, zerflossen! \u2013", "tokens": ["oh", "kei\u00b7nes", "!", "fort", "!", "nur", "Bil\u00b7der", ",", "wirr", ",", "zer\u00b7flos\u00b7sen", "!", "\u2013"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct", "punct"], "pos": ["APPR", "PIS", "$.", "PTKVZ", "$.", "ADV", "NN", "$,", "ADJD", "$,", "VVPP", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Welch Sausen da auf Einmal! ausgegossen", "tokens": ["Welch", "Sau\u00b7sen", "da", "auf", "Ein\u00b7mal", "!", "aus\u00b7ge\u00b7gos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["PIAT", "NN", "ADV", "APPR", "ADV", "$.", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "durch alle L\u00fcfte schien ein Gottessegen,", "tokens": ["durch", "al\u00b7le", "L\u00fcf\u00b7te", "schien", "ein", "Got\u00b7tes\u00b7se\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "der ", "tokens": ["der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.4": {"text": "oh Tau der Reinheit \u2013 Liebe \u2013 Himmelsregen!", "tokens": ["oh", "Tau", "der", "Rein\u00b7heit", "\u2013", "Lie\u00b7be", "\u2013", "Him\u00b7mels\u00b7re\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADJD", "NN", "ART", "NN", "$(", "NN", "$(", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Es trank der Strom der Fr\u00fchlingswolke Thr\u00e4nen,", "tokens": ["Es", "trank", "der", "Strom", "der", "Fr\u00fch\u00b7lings\u00b7wol\u00b7ke", "Thr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "die tausend Augen, die herniederquollen,", "tokens": ["die", "tau\u00b7send", "Au\u00b7gen", ",", "die", "her\u00b7nie\u00b7der\u00b7quol\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "CARD", "NN", "$,", "PRELS", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "die tr\u00e4ufenden K\u00fcsse: seine Adern schwollen,", "tokens": ["die", "tr\u00e4u\u00b7fen\u00b7den", "K\u00fcs\u00b7se", ":", "sei\u00b7ne", "A\u00b7dern", "schwol\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "die Pulse barsten, ein unendlich W\u00e4hnen", "tokens": ["die", "Pul\u00b7se", "bars\u00b7ten", ",", "ein", "un\u00b7end\u00b7lich", "W\u00e4h\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "ADJD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "aus seinem Bette brach er, zu umfangen", "tokens": ["aus", "sei\u00b7nem", "Bet\u00b7te", "brach", "er", ",", "zu", "um\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "$,", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "des Erdballs Wall, des Firmamentes Zinne, \u2013 \u2013", "tokens": ["des", "Erd\u00b7balls", "Wall", ",", "des", "Fir\u00b7ma\u00b7men\u00b7tes", "Zin\u00b7ne", ",", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "NN", "NE", "$,", "ART", "NN", "NE", "$,", "$(", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "zur\u00fcck, zur\u00fcck! Ein m\u00fcdes, sanftes Bangen", "tokens": ["zu\u00b7r\u00fcck", ",", "zu\u00b7r\u00fcck", "!", "Ein", "m\u00fc\u00b7des", ",", "sanf\u00b7tes", "Ban\u00b7gen"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PTKVZ", "$,", "PTKVZ", "$.", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "umstrickte schmeichelnd seine taumelnden Wogen:", "tokens": ["um\u00b7strick\u00b7te", "schmei\u00b7chelnd", "sei\u00b7ne", "tau\u00b7meln\u00b7den", "Wo\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "die tausend K\u00fcsse, die er aufgesogen,", "tokens": ["die", "tau\u00b7send", "K\u00fcs\u00b7se", ",", "die", "er", "auf\u00b7ge\u00b7so\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "CARD", "NN", "$,", "PRELS", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "wie Lenzhauch nun sein lockiges Haupt umflogen,", "tokens": ["wie", "Lenz\u00b7hauch", "nun", "sein", "lo\u00b7cki\u00b7ges", "Haupt", "um\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADV", "PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "umschlangen die geballten F\u00e4uste, bogen", "tokens": ["um\u00b7schlan\u00b7gen", "die", "ge\u00b7ball\u00b7ten", "F\u00e4us\u00b7te", ",", "bo\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "sie spielend auf und kos'ten sie und zogen", "tokens": ["sie", "spie\u00b7lend", "auf", "und", "kos'\u00b7ten", "sie", "und", "zo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADJD", "PTKVZ", "KON", "VVFIN", "PPER", "KON", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "in seinen Schoo\u00df sie wieder, und ein S\u00e4umen", "tokens": ["in", "sei\u00b7nen", "Schoo\u00df", "sie", "wie\u00b7der", ",", "und", "ein", "S\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "ADV", "$,", "KON", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "und ", "tokens": ["und"], "token_info": ["word"], "pos": ["KON"], "meter": "-", "measure": "single.down"}, "line.15": {"text": "so rann er matt des Weges, wie in Tr\u00e4umen ...", "tokens": ["so", "rann", "er", "matt", "des", "We\u00b7ges", ",", "wie", "in", "Tr\u00e4u\u00b7men", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "ART", "NN", "$,", "PWAV", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "So fand ich Euch \u2013 und lauschte. O Erwachen!", "tokens": ["So", "fand", "ich", "Euch", "\u2013", "und", "lauschte", ".", "O", "Er\u00b7wa\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "$(", "KON", "VVFIN", "$.", "NE", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "o br\u00fcderlich Erkennen, jauchzend Lachen!", "tokens": ["o", "br\u00fc\u00b7der\u00b7lich", "Er\u00b7ken\u00b7nen", ",", "jauch\u00b7zend", "La\u00b7chen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["FM", "ADJD", "NN", "$,", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "o Meeresstimmen, s\u00fc\u00dfe Schwesterlaute!", "tokens": ["o", "Mee\u00b7res\u00b7stim\u00b7men", ",", "s\u00fc\u00b7\u00dfe", "Schwes\u00b7ter\u00b7lau\u00b7te", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["FM", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "erf\u00fcllter Ahnung Kl\u00e4nge, urvertraute! \u2013", "tokens": ["er\u00b7f\u00fcll\u00b7ter", "Ah\u00b7nung", "Kl\u00e4n\u00b7ge", ",", "ur\u00b7ver\u00b7trau\u00b7te", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["ADJA", "NN", "NE", "$,", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Zur tiefen See des vollen Stromes Sehnen", "tokens": ["Zur", "tie\u00b7fen", "See", "des", "vol\u00b7len", "Stro\u00b7mes", "Seh\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "erfa\u00dft mich, in die Arme euch zu fallen,", "tokens": ["er\u00b7fa\u00dft", "mich", ",", "in", "die", "Ar\u00b7me", "euch", "zu", "fal\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "APPR", "ART", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "zu f\u00fchlen eurer Herzensfluten Wallen,", "tokens": ["zu", "f\u00fch\u00b7len", "eu\u00b7rer", "Her\u00b7zens\u00b7flu\u00b7ten", "Wal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "an eurer sturmgeworfnen Brust zu lehnen.", "tokens": ["an", "eu\u00b7rer", "sturm\u00b7ge\u00b7worf\u00b7nen", "Brust", "zu", "leh\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "In Eure Seele sch\u00fctten will ich Meine", "tokens": ["In", "Eu\u00b7re", "See\u00b7le", "sch\u00fct\u00b7ten", "will", "ich", "Mei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "VMFIN", "PPER", "PPOSAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "mit allen Himmelswassern, die sie tr\u00e4nkten,", "tokens": ["mit", "al\u00b7len", "Him\u00b7mels\u00b7was\u00b7sern", ",", "die", "sie", "tr\u00e4nk\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "mit allen Quellenpulsen, die mich dr\u00e4ngten", "tokens": ["mit", "al\u00b7len", "Quel\u00b7len\u00b7pul\u00b7sen", ",", "die", "mich", "dr\u00e4ng\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "$,", "PRELS", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "euch zu umhalsen: da\u00df in Eine Eine", "tokens": ["euch", "zu", "um\u00b7hal\u00b7sen", ":", "da\u00df", "in", "Ei\u00b7ne", "Ei\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "PTKZU", "VVINF", "$.", "KOUS", "APPR", "ART", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "gl\u00fccksingende Woge unser Leben brande,", "tokens": ["gl\u00fcc\u00b7ksin\u00b7gen\u00b7de", "Wo\u00b7ge", "un\u00b7ser", "Le\u00b7ben", "bran\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "die glanzumzittert von dem Strahlennetze", "tokens": ["die", "glan\u00b7zum\u00b7zit\u00b7tert", "von", "dem", "Strah\u00b7len\u00b7net\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "der gro\u00dfen Sonne Liebe rings die Lande,", "tokens": ["der", "gro\u00b7\u00dfen", "Son\u00b7ne", "Lie\u00b7be", "rings", "die", "Lan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "die d\u00fcrren, f\u00fcr den ", "tokens": ["die", "d\u00fcr\u00b7ren", ",", "f\u00fcr", "den"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "$,", "APPR", "ART"], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"