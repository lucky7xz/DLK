{"dta.poem.9572": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auff ihr absterben.  \n C. E.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.71", "nl:0.28"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Arminde mustu schon des todes opffer werden?", "tokens": ["Ar\u00b7min\u00b7de", "mus\u00b7tu", "schon", "des", "to\u00b7des", "opf\u00b7fer", "wer\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "ADV", "ART", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Rei\u00dft dessen kalter arm dich in die grufft hinein?", "tokens": ["Rei\u00dft", "des\u00b7sen", "kal\u00b7ter", "arm", "dich", "in", "die", "grufft", "hin\u00b7ein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDS", "ADJD", "ADJD", "PPER", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ach deine sittsamkeit und heller tugend-schein", "tokens": ["Ach", "dei\u00b7ne", "sitt\u00b7sam\u00b7keit", "und", "hel\u00b7ler", "tu\u00b7gen\u00b7dschein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "PPOSAT", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Entweicht nur allzu fr\u00fch der kummer-vollen erden", "tokens": ["Ent\u00b7weicht", "nur", "all\u00b7zu", "fr\u00fch", "der", "kum\u00b7mer\u00b7vol\u00b7len", "er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "PTKA", "ADJD", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wo bleibt die stille pracht der h\u00f6fflichen geberden?", "tokens": ["Wo", "bleibt", "die", "stil\u00b7le", "pracht", "der", "h\u00f6ff\u00b7li\u00b7chen", "ge\u00b7ber\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Arminde/ ohne dich wird mir die welt zu klein/", "tokens": ["Ar\u00b7min\u00b7de", "/", "oh\u00b7ne", "dich", "wird", "mir", "die", "welt", "zu", "klein", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "APPR", "PPER", "VAFIN", "PPER", "ART", "NN", "PTKA", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Dein sterben l\u00e4sset mich nicht mehr am leben seyn/", "tokens": ["Dein", "ster\u00b7ben", "l\u00e4s\u00b7set", "mich", "nicht", "mehr", "am", "le\u00b7ben", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "VVFIN", "PPER", "PTKNEG", "ADV", "PTKA", "ADJD", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und h\u00e4uffet meine noth mit tausend angst-beschwerden.", "tokens": ["Und", "h\u00e4uf\u00b7fet", "mei\u00b7ne", "noth", "mit", "tau\u00b7send", "angst\u00b7be\u00b7schwer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "APPR", "CARD", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Mit dir ist meine lieb\u2019 aus dieser welt gewichen/", "tokens": ["Mit", "dir", "ist", "mei\u00b7ne", "lieb'", "aus", "die\u00b7ser", "welt", "ge\u00b7wi\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "PPOSAT", "NN", "APPR", "PDAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Mit dir ist meine lust und wohlergehn verblichen/", "tokens": ["Mit", "dir", "ist", "mei\u00b7ne", "lust", "und", "woh\u00b7ler\u00b7gehn", "ver\u00b7bli\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "PPOSAT", "NN", "KON", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Dein todt macht mich verwirrt/ best\u00fcrtzt und traurens-voll/", "tokens": ["Dein", "todt", "macht", "mich", "ver\u00b7wirrt", "/", "be\u00b7st\u00fcrtzt", "und", "trau\u00b7rens\u00b7voll", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "VVFIN", "PPER", "ADJD", "$(", "VVPP", "KON", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Bey dir ist meine gunst/ hertz/ seel und geist verblieben/", "tokens": ["Bey", "dir", "ist", "mei\u00b7ne", "gunst", "/", "hertz", "/", "seel", "und", "geist", "ver\u00b7blie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "PPOSAT", "NN", "$(", "NN", "$(", "XY", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Was kan ich ferner nun wohl ausser dir mehr lieben/", "tokens": ["Was", "kan", "ich", "fer\u00b7ner", "nun", "wohl", "aus\u00b7ser", "dir", "mehr", "lie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "ADV", "ADV", "APPR", "PPER", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Als meinen jammer/ der mich ehstens t\u00f6dten soll?", "tokens": ["Als", "mei\u00b7nen", "jam\u00b7mer", "/", "der", "mich", "ehs\u00b7tens", "t\u00f6d\u00b7ten", "soll", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "$(", "PRELS", "PPER", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}