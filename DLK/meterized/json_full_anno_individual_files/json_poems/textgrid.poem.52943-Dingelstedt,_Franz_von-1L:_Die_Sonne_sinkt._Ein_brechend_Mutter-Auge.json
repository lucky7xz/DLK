{"textgrid.poem.52943": {"metadata": {"author": {"name": "Dingelstedt, Franz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Sonne sinkt. Ein brechend Mutter-Auge", "genre": "verse", "period": "N.A.", "pub_year": 1847, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Sonne sinkt. Ein brechend Mutter-Auge", "tokens": ["Die", "Son\u00b7ne", "sinkt", ".", "Ein", "bre\u00b7chend", "Mut\u00b7ter\u00b7Au\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "ART", "ADJD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "H\u00e4ngt sie noch einmal auf der stillen Erde", "tokens": ["H\u00e4ngt", "sie", "noch", "ein\u00b7mal", "auf", "der", "stil\u00b7len", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und zittert in des Sees durchgl\u00fchten Wogen.", "tokens": ["Und", "zit\u00b7tert", "in", "des", "Sees", "durch\u00b7gl\u00fch\u00b7ten", "Wo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ja, dr\u00e4ng Dich an sie, Welten-Kind, und sauge", "tokens": ["Ja", ",", "dr\u00e4ng", "Dich", "an", "sie", ",", "Wel\u00b7ten\u00b7Kind", ",", "und", "sau\u00b7ge"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PTKANT", "$,", "VVFIN", "PPER", "APPR", "PPER", "$,", "NN", "$,", "KON", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Den Segen auf, eh er verdunkelt werde,", "tokens": ["Den", "Se\u00b7gen", "auf", ",", "eh", "er", "ver\u00b7dun\u00b7kelt", "wer\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "KOUS", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und eh an dem erstarrten Himmelsbogen", "tokens": ["Und", "eh", "an", "dem", "er\u00b7starr\u00b7ten", "Him\u00b7mels\u00b7bo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Die Nacht kommt aufgezogen.", "tokens": ["Die", "Nacht", "kommt", "auf\u00b7ge\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Auch meine Sonn, ich f\u00fchl es, neigt zum Ende;", "tokens": ["Auch", "mei\u00b7ne", "Sonn", ",", "ich", "f\u00fchl", "es", ",", "neigt", "zum", "En\u00b7de", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "PPER", "$,", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "So m\u00f6ge Dich ihr letzter Strahl verkl\u00e4ren!", "tokens": ["So", "m\u00f6\u00b7ge", "Dich", "ihr", "letz\u00b7ter", "Strahl", "ver\u00b7kl\u00e4\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Ob ich die Kraft, die schwindende, verschwende,", "tokens": ["Ob", "ich", "die", "Kraft", ",", "die", "schwin\u00b7den\u00b7de", ",", "ver\u00b7schwen\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "$,", "ART", "ADJA", "$,", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Was tuts? Sie kann ja doch nicht ewig w\u00e4hren.", "tokens": ["Was", "tuts", "?", "Sie", "kann", "ja", "doch", "nicht", "e\u00b7wig", "w\u00e4h\u00b7ren", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "$.", "PPER", "VMFIN", "ADV", "ADV", "PTKNEG", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Ein Bild noch \u2013 Deins! \u2013 will ich in Glorie fassen", "tokens": ["Ein", "Bild", "noch", "\u2013", "Deins", "!", "\u2013", "will", "ich", "in", "Glo\u00b7rie", "fas\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "punct", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "$(", "NE", "$.", "$(", "VMFIN", "PPER", "APPR", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Und l\u00e4chelnd als Verm\u00e4chtnis hinterlassen.", "tokens": ["Und", "l\u00e4\u00b7chelnd", "als", "Ver\u00b7m\u00e4cht\u00b7nis", "hin\u00b7ter\u00b7las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KOKOM", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die Sonne sinkt. Ein brechend Mutter-Auge", "tokens": ["Die", "Son\u00b7ne", "sinkt", ".", "Ein", "bre\u00b7chend", "Mut\u00b7ter\u00b7Au\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "ART", "ADJD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "H\u00e4ngt sie noch einmal auf der stillen Erde", "tokens": ["H\u00e4ngt", "sie", "noch", "ein\u00b7mal", "auf", "der", "stil\u00b7len", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "wo