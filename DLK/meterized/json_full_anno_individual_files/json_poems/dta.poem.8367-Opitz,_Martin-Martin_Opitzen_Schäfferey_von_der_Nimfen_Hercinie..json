{"dta.poem.8367": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "Martin Opitzen Sch\u00e4fferey  \n von der Nimfen Hercinie.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1630", "urn": "urn:nbn:de:kobv:b4-200905197863", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Sch\u00e4fferey Von der Nimfen Hercinie. Breslau, 1630."}, "poem": {"stanza.1": {"line.1": {"text": "Weil mein Verhengni\u00df wil/ vndt lest mir nicht das gl\u00fccke", "tokens": ["Weil", "mein", "Ver\u00b7heng\u00b7ni\u00df", "wil", "/", "vndt", "lest", "mir", "nicht", "das", "gl\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "VMFIN", "$(", "KON", "VVIMP", "PPER", "PTKNEG", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bey dir/ mein Augen trost/ zue leben nur allein/", "tokens": ["Bey", "dir", "/", "mein", "Au\u00b7gen", "trost", "/", "zue", "le\u00b7ben", "nur", "al\u00b7lein", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$(", "PPOSAT", "NN", "VVFIN", "$(", "PTKZU", "VVINF", "ADV", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So giebet zwar mein sinn sich mitt gedult darein/", "tokens": ["So", "gie\u00b7bet", "zwar", "mein", "sinn", "sich", "mitt", "ge\u00b7dult", "da\u00b7rein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPOSAT", "NN", "PRF", "APPR", "NN", "PAV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Doch sehnt vndt w\u00fcndtschet er auch st\u00fcndtlich sich zuer\u00fccke.", "tokens": ["Doch", "sehnt", "vndt", "w\u00fcndt\u00b7schet", "er", "auch", "st\u00fcndt\u00b7lich", "sich", "zu\u00b7e\u00b7r\u00fc\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "PPER", "ADV", "ADJD", "PRF", "VVFIN", "$."], "meter": "-+-+---+--+-+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Es ist ja lauter nichts wo diese sch\u00f6ne blicke/", "tokens": ["Es", "ist", "ja", "lau\u00b7ter", "nichts", "wo", "die\u00b7se", "sch\u00f6\u00b7ne", "bli\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PIAT", "PIS", "PWAV", "PDAT", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Di\u00df liecht das mich verblendt/ des g\u00fcldnen haares schein", "tokens": ["Di\u00df", "liecht", "das", "mich", "ver\u00b7blendt", "/", "des", "g\u00fcld\u00b7nen", "haa\u00b7res", "schein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ART", "PPER", "VVPP", "$(", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Das mein gem\u00fcte bindt/ di\u00df lachen nicht kan sein/", "tokens": ["Das", "mein", "ge\u00b7m\u00fc\u00b7te", "bindt", "/", "di\u00df", "la\u00b7chen", "nicht", "kan", "sein", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "ADJA", "VVFIN", "$(", "PDS", "VVFIN", "PTKNEG", "VMFIN", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Der mundt/ vndt alles das wormit ich mich erquicke.", "tokens": ["Der", "mundt", "/", "vndt", "al\u00b7les", "das", "wor\u00b7mit", "ich", "mich", "er\u00b7qui\u00b7cke", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "$(", "KON", "PIS", "ART", "PWAV", "PPER", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Die Sonne macht mir kalt/ der tag verfinstert mich;", "tokens": ["Die", "Son\u00b7ne", "macht", "mir", "kalt", "/", "der", "tag", "ver\u00b7fins\u00b7tert", "mich", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "$(", "ART", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ich geh'/ vndt wei\u00df nicht wie; ich geh'vndt suche dich", "tokens": ["Ich", "geh'", "/", "vndt", "wei\u00df", "nicht", "wie", ";", "ich", "geh'v\u00b7ndt", "su\u00b7che", "dich"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$(", "KON", "VVFIN", "PTKNEG", "KOKOM", "$.", "PPER", "VVPP", "VVFIN", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wohin du nie gedenckst. was macht mein trewes lieben?", "tokens": ["Wo\u00b7hin", "du", "nie", "ge\u00b7denckst", ".", "was", "macht", "mein", "tre\u00b7wes", "lie\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VVPP", "$.", "PWS", "VVFIN", "PPOSAT", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Ich seh' vndt finde nichts; der mangel deiner ziehr", "tokens": ["Ich", "seh'", "vndt", "fin\u00b7de", "nichts", ";", "der", "man\u00b7gel", "dei\u00b7ner", "ziehr"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PIS", "$.", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Hatt alles weggeraubt: zwey dinge sindt noch hier:", "tokens": ["Hatt", "al\u00b7les", "weg\u00b7ge\u00b7raubt", ":", "zwey", "din\u00b7ge", "sindt", "noch", "hier", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "VVPP", "$.", "CARD", "NN", "VVFIN", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Das elendt nur/ vndt ich der ich darein vertrieben.", "tokens": ["Das", "e\u00b7lendt", "nur", "/", "vndt", "ich", "der", "ich", "da\u00b7rein", "ver\u00b7trie\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "ADV", "$(", "KON", "PPER", "ART", "PPER", "PAV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}