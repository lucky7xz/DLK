{"dta.poem.676": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "2. Satz.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Dv dreymal H\u00f6chster Heldt/ ", "tokens": ["Dv", "drey\u00b7mal", "H\u00f6chs\u00b7ter", "Heldt", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Der du manch\u2019 hartes schlos/", "tokens": ["Der", "du", "man\u00b7ch'", "har\u00b7tes", "schlos", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PIAT", "ADJA", "ADJD", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wol ehermal zubrochen;", "tokens": ["Wol", "e\u00b7her\u00b7mal", "zu\u00b7bro\u00b7chen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVINF", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Dem der zu fusse f\u00e4lt", "tokens": ["Dem", "der", "zu", "fus\u00b7se", "f\u00e4lt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "ART", "APPR", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Den deine gunst vom pochen ", "tokens": ["Den", "dei\u00b7ne", "gunst", "vom", "po\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPOSAT", "NN", "APPRART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Des Feindes frey vnd lo\u00df ", "tokens": ["Des", "Fein\u00b7des", "frey", "vnd", "lo\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "KON", "PTKVZ"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Vnd ledig hat gemacht; ach wende dein gem\u00fcte", "tokens": ["Vnd", "le\u00b7dig", "hat", "ge\u00b7macht", ";", "ach", "wen\u00b7de", "dein", "ge\u00b7m\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VAFIN", "VVPP", "$.", "ADV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Auff dieses Jammer haus/", "tokens": ["Auff", "die\u00b7ses", "Jam\u00b7mer", "haus", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "PTKVZ", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Reis meine band\u2019 entzwey/ O Abgrund h\u00f6chster g\u00fcte.", "tokens": ["Reis", "mei\u00b7ne", "band'", "ent\u00b7zwey", "/", "O", "Ab\u00b7grund", "h\u00f6chs\u00b7ter", "g\u00fc\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "PTKVZ", "$(", "NE", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Erschein\u2019 vnd la\u00df mich aus!", "tokens": ["Er\u00b7schein'", "vnd", "la\u00df", "mich", "aus", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "VVIMP", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Komm ewig-trewer GOTT/", "tokens": ["Komm", "e\u00b7wig\u00b7tre\u00b7wer", "GoTT", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Vnd wende noth vnd spott.", "tokens": ["Vnd", "wen\u00b7de", "noth", "vnd", "spott", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}