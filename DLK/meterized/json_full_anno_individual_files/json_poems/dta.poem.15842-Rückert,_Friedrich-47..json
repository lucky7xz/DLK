{"dta.poem.15842": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "47.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Am besten geht es oft, wenn du es l\u00e4ssest gehn,", "tokens": ["Am", "bes\u00b7ten", "geht", "es", "oft", ",", "wenn", "du", "es", "l\u00e4s\u00b7sest", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "VVFIN", "PPER", "ADV", "$,", "KOUS", "PPER", "PPER", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie gehn es will und kann; allein du mu\u00dft verstehn:", "tokens": ["Wie", "gehn", "es", "will", "und", "kann", ";", "al\u00b7lein", "du", "mu\u00dft", "ver\u00b7stehn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "VMFIN", "KON", "VMFIN", "$.", "ADV", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Gehn lassen sollst du nur, was du nicht k\u00f6nntest lenken;", "tokens": ["Gehn", "las\u00b7sen", "sollst", "du", "nur", ",", "was", "du", "nicht", "k\u00f6nn\u00b7test", "len\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "VMFIN", "PPER", "ADV", "$,", "PWS", "PPER", "PTKNEG", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was aber ohne dich nicht geht, mu\u00dft du bedenken.", "tokens": ["Was", "a\u00b7ber", "oh\u00b7ne", "dich", "nicht", "geht", ",", "mu\u00dft", "du", "be\u00b7den\u00b7ken", "."], "token_info": ["word", "word", "word", "w