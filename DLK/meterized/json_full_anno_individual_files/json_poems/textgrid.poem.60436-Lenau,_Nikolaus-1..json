{"textgrid.poem.60436": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von der Thei\u00df, der klaren, fischereichen,", "tokens": ["Von", "der", "Thei\u00df", ",", "der", "kla\u00b7ren", ",", "fi\u00b7sche\u00b7rei\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ART", "ADJA", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ist der Geiger Mischka hingezogen,", "tokens": ["Ist", "der", "Gei\u00b7ger", "Mischka", "hin\u00b7ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "NE", "VVPP", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Wo der Marosch barsche Wogen", "tokens": ["Wo", "der", "Ma\u00b7rosch", "bar\u00b7sche", "Wo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Brausend durch besch\u00e4umte Klippen streichen.", "tokens": ["Brau\u00b7send", "durch", "be\u00b7sch\u00e4um\u00b7te", "Klip\u00b7pen", "strei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Der Zigeuner wandert, arm und heiter,", "tokens": ["Der", "Zi\u00b7geu\u00b7ner", "wan\u00b7dert", ",", "arm", "und", "hei\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "In die Ferne, Fremde, fort und weiter;", "tokens": ["In", "die", "Fer\u00b7ne", ",", "Frem\u00b7de", ",", "fort", "und", "wei\u00b7ter", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "NN", "$,", "PTKVZ", "KON", "ADV", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Wenn er auch am Wohlgeschmack der Erde", "tokens": ["Wenn", "er", "auch", "am", "Wohl\u00b7ge\u00b7schmack", "der", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPRART", "NN", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Karg und selten nur sich weidet,", "tokens": ["Karg", "und", "sel\u00b7ten", "nur", "sich", "wei\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJD", "ADV", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ist ihm jeder Ort doch bald entleidet,", "tokens": ["Ist", "ihm", "je\u00b7der", "Ort", "doch", "bald", "ent\u00b7lei\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PIAT", "NN", "ADV", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Und was heimisch, wird ihm zur Beschwerde;", "tokens": ["Und", "was", "hei\u00b7misch", ",", "wird", "ihm", "zur", "Be\u00b7schwer\u00b7de", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADJD", "$,", "VAFIN", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Wenig brauchend kommt und geht", "tokens": ["We\u00b7nig", "brau\u00b7chend", "kommt", "und", "geht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN", "KON", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Dieser fiedelnde Aszet.", "tokens": ["Die\u00b7ser", "fie\u00b7deln\u00b7de", "As\u00b7zet", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.3": {"line.1": {"text": "Mischkas H\u00fcttlein mit dem Halmendach", "tokens": ["Mischkas", "H\u00fctt\u00b7lein", "mit", "dem", "Hal\u00b7men\u00b7dach"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ragt empor vom Grund nur wenig Spannen,", "tokens": ["Ragt", "em\u00b7por", "vom", "Grund", "nur", "we\u00b7nig", "Span\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "APPRART", "NN", "ADV", "PIAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Und vor\u00fcber wild und jach", "tokens": ["Und", "vor\u00b7\u00fc\u00b7ber", "wild", "und", "jach"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADJD", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "St\u00fcrzt die Marosch durch die Felsen, Tannen.", "tokens": ["St\u00fcrzt", "die", "Ma\u00b7rosch", "durch", "die", "Fel\u00b7sen", ",", "Tan\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Horch, wie rauschen Mischkas helle Saiten", "tokens": ["Horch", ",", "wie", "rau\u00b7schen", "Mischkas", "hel\u00b7le", "Sai\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "PWAV", "ADJA", "NN", "ADJA", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Unter diesen Halmen, die vorzeiten", "tokens": ["Un\u00b7ter", "die\u00b7sen", "Hal\u00b7men", ",", "die", "vor\u00b7zei\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PDAT", "NN", "$,", "ART", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Bei dem Klang der Lerchenlieder", "tokens": ["Bei", "dem", "Klang", "der", "Ler\u00b7chen\u00b7lie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf dem Feld sich wiegten hin und wider.", "tokens": ["Auf", "dem", "Feld", "sich", "wieg\u00b7ten", "hin", "und", "wi\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PRF", "VVFIN", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Nicht allein an Schall und s\u00fc\u00dfen Weisen", "tokens": ["Nicht", "al\u00b7lein", "an", "Schall", "und", "s\u00fc\u00b7\u00dfen", "Wei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "APPR", "NN", "KON", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ist dies niedre H\u00fcttlein reich zu preisen;", "tokens": ["Ist", "dies", "nied\u00b7re", "H\u00fctt\u00b7lein", "reich", "zu", "prei\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "ADJA", "NN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Strahlen hegt es auch in F\u00fclle,", "tokens": ["Strah\u00b7len", "hegt", "es", "auch", "in", "F\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie sie aus den sch\u00f6nsten Welten", "tokens": ["Wie", "sie", "aus", "den", "sch\u00f6ns\u00b7ten", "Wel\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Uns her\u00fcber, fl\u00fcchtig, selten,", "tokens": ["Uns", "her\u00b7\u00fc\u00b7ber", ",", "fl\u00fcch\u00b7tig", ",", "sel\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "ADV", "$,", "ADJD", "$,", "ADJD", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.6": {"text": "Leuchten durch die Menschenh\u00fclle.", "tokens": ["Leuch\u00b7ten", "durch", "die", "Men\u00b7schen\u00b7h\u00fcl\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Mischkas treues Liebchen ruht im Grabe;", "tokens": ["Mischkas", "treu\u00b7es", "Lieb\u00b7chen", "ruht", "im", "Gra\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Doch sie lie\u00df zur Abschiedsgabe", "tokens": ["Doch", "sie", "lie\u00df", "zur", "Ab\u00b7schieds\u00b7ga\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Seines Gl\u00fccks ihm einen teuren Rest,", "tokens": ["Sei\u00b7nes", "Gl\u00fccks", "ihm", "ei\u00b7nen", "teu\u00b7ren", "Rest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Da\u00df sein Herz sich minder h\u00e4rme;", "tokens": ["Da\u00df", "sein", "Herz", "sich", "min\u00b7der", "h\u00e4r\u00b7me", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PRF", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie die holde Sommerw\u00e4rme", "tokens": ["Wie", "die", "hol\u00b7de", "Som\u00b7mer\u00b7w\u00e4r\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sterbend ihre Frucht uns l\u00e4\u00dft.", "tokens": ["Ster\u00b7bend", "ih\u00b7re", "Frucht", "uns", "l\u00e4\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Mischka geigt, und seine hellen T\u00f6ne", "tokens": ["Mischka", "geigt", ",", "und", "sei\u00b7ne", "hel\u00b7len", "T\u00f6\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VVPP", "$,", "KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Tr\u00e4gt hinaus der Abendwind;", "tokens": ["Tr\u00e4gt", "hin\u00b7aus", "der", "Ab\u00b7end\u00b7wind", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Vor der H\u00fctte steht die wundersch\u00f6ne", "tokens": ["Vor", "der", "H\u00fct\u00b7te", "steht", "die", "wun\u00b7der\u00b7sch\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Mira, das Zigeunerkind.", "tokens": ["Mi\u00b7ra", ",", "das", "Zi\u00b7geu\u00b7ner\u00b7kind", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.8": {"line.1": {"text": "Die vom Abendrot Gek\u00fc\u00dfte", "tokens": ["Die", "vom", "A\u00b7ben\u00b7drot", "Ge\u00b7k\u00fc\u00df\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPRART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist vom leichten West umflogen,", "tokens": ["Ist", "vom", "leich\u00b7ten", "West", "um\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und es flattert um der Br\u00fcste", "tokens": ["Und", "es", "flat\u00b7tert", "um", "der", "Br\u00fcs\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Melodiegeschwellte Wogen", "tokens": ["Me\u00b7lo\u00b7die\u00b7ge\u00b7schwell\u00b7te", "Wo\u00b7gen"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ihres Haars gelockte Nacht;", "tokens": ["Ih\u00b7res", "Haars", "ge\u00b7lock\u00b7te", "Nacht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "O, wenn diese sch\u00f6ne Brust erwacht!", "tokens": ["O", ",", "wenn", "die\u00b7se", "sch\u00f6\u00b7ne", "Brust", "er\u00b7wacht", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "PDAT", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Dieses Busens keusche Wellen,", "tokens": ["Die\u00b7ses", "Bu\u00b7sens", "keu\u00b7sche", "Wel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Die noch Liebe nie empfanden,", "tokens": ["Die", "noch", "Lie\u00b7be", "nie", "emp\u00b7fan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Selig, wem sie einst entgegenschwellen", "tokens": ["Se\u00b7lig", ",", "wem", "sie", "einst", "ent\u00b7ge\u00b7gen\u00b7schwel\u00b7len"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "PWS", "PPER", "ADV", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Und ans Herz im Sturm der Liebe branden!", "tokens": ["Und", "ans", "Herz", "im", "Sturm", "der", "Lie\u00b7be", "bran\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "APPRART", "NN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.11": {"text": "Selig, wer aus diesen schwarzen Augen", "tokens": ["Se\u00b7lig", ",", "wer", "aus", "die\u00b7sen", "schwar\u00b7zen", "Au\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "PWS", "APPR", "PDAT", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.12": {"text": "Darf den ersten Blitz der Leidenschaft", "tokens": ["Darf", "den", "ers\u00b7ten", "Blitz", "der", "Lei\u00b7den\u00b7schaft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "ART", "ADJA", "NN", "ART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.13": {"text": "Und aus diesem Mund ein Fl\u00fcstern saugen,", "tokens": ["Und", "aus", "die\u00b7sem", "Mund", "ein", "Fl\u00fcs\u00b7tern", "sau\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PDAT", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.14": {"text": "S\u00fc\u00df und wonneirr und zauberhaft,", "tokens": ["S\u00fc\u00df", "und", "won\u00b7neirr", "und", "zau\u00b7ber\u00b7haft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.15": {"text": "Da\u00df der Cherub beim Gesang der Worte", "tokens": ["Da\u00df", "der", "Che\u00b7rub", "beim", "Ge\u00b7sang", "der", "Wor\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NE", "APPRART", "NN", "ART", "NN"], "meter": "+-++--+-+-", "measure": "trochaic.penta.relaxed"}, "line.16": {"text": "Sinkt in Schlummer an des Edens Pforte!", "tokens": ["Sinkt", "in", "Schlum\u00b7mer", "an", "des", "E\u00b7dens", "Pfor\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.17": {"text": "Bald doch, bald die Worte unter K\u00fcssen", "tokens": ["Bald", "doch", ",", "bald", "die", "Wor\u00b7te", "un\u00b7ter", "K\u00fcs\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "$,", "ADV", "ART", "NN", "APPR", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.18": {"text": "In ein s\u00fc\u00dfres Leben sterben m\u00fcssen! \u2013", "tokens": ["In", "ein", "s\u00fc\u00df\u00b7res", "Le\u00b7ben", "ster\u00b7ben", "m\u00fcs\u00b7sen", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "VMINF", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.19": {"text": "Also gl\u00fchen die Gedanken", "tokens": ["Al\u00b7so", "gl\u00fc\u00b7hen", "die", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Durch die Brust dem Liebeskranken;", "tokens": ["Durch", "die", "Brust", "dem", "Lie\u00b7bes\u00b7kran\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Einsam dort am Waldessaume,", "tokens": ["Ein\u00b7sam", "dort", "am", "Wal\u00b7des\u00b7sau\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Harrt und lauscht er unterm Baume,", "tokens": ["Harrt", "und", "lauscht", "er", "un\u00b7term", "Bau\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "Ob kein Rascheln aus dem Tannengrunde", "tokens": ["Ob", "kein", "Ra\u00b7scheln", "aus", "dem", "Tan\u00b7nen\u00b7grun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIAT", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.24": {"text": "Ihm ein Wild verr\u00e4t, zur Abendstunde", "tokens": ["Ihm", "ein", "Wild", "ver\u00b7r\u00e4t", ",", "zur", "A\u00b7bends\u00b7tun\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "ART", "NN", "VVFIN", "$,", "APPRART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.25": {"text": "Sachte auf den freien Anger schreitend,", "tokens": ["Sach\u00b7te", "auf", "den", "frei\u00b7en", "An\u00b7ger", "schrei\u00b7tend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "ADJD", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.26": {"text": "Freundlich aus dem Wald den Tag begleitend.", "tokens": ["Freund\u00b7lich", "aus", "dem", "Wald", "den", "Tag", "be\u00b7glei\u00b7tend", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.9": {"line.1": {"text": "Und er stellt dem Liebesgl\u00fcck ein Zeichen:", "tokens": ["Und", "er", "stellt", "dem", "Lie\u00b7bes\u00b7gl\u00fcck", "ein", "Zei\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wenn ich heut ein edles Wild noch schie\u00dfe,", "tokens": ["Wenn", "ich", "heut", "ein", "ed\u00b7les", "Wild", "noch", "schie\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "ADJA", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Werd ich meinen hei\u00dfen Wunsch erreichen,", "tokens": ["Werd", "ich", "mei\u00b7nen", "hei\u00b7\u00dfen", "Wunsch", "er\u00b7rei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Da\u00df ich sie in meine Arme schlie\u00dfe.", "tokens": ["Da\u00df", "ich", "sie", "in", "mei\u00b7ne", "Ar\u00b7me", "schlie\u00b7\u00dfe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.10": {"line.1": {"text": "Sieh dort eine braune Wohlgestalt,", "tokens": ["Sieh", "dort", "ei\u00b7ne", "brau\u00b7ne", "Wohl\u00b7ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Ruhig kommt ein Hirsch dort aus dem Wald,", "tokens": ["Ru\u00b7hig", "kommt", "ein", "Hirsch", "dort", "aus", "dem", "Wald", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Da\u00df der J\u00e4ger kann die Enden z\u00e4hlen:", "tokens": ["Da\u00df", "der", "J\u00e4\u00b7ger", "kann", "die", "En\u00b7den", "z\u00e4h\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "\u00bbsechzehn! \u2013 sollens ", "tokens": ["\u00bb", "sech\u00b7zehn", "!", "\u2013", "sol\u00b7lens"], "token_info": ["punct", "word", "punct", "punct", "word"], "pos": ["$(", "VVINF", "$.", "$(", "XY"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Gott der Liebe, la\u00df mich jetzt nicht fehlen!", "tokens": ["Gott", "der", "Lie\u00b7be", ",", "la\u00df", "mich", "jetzt", "nicht", "feh\u00b7len", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "VVIMP", "PPER", "ADV", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Ha! er st\u00fcrzt, halloh! nun ist sie mein!\u00ab", "tokens": ["Ha", "!", "er", "st\u00fcrzt", ",", "hal\u00b7loh", "!", "nun", "ist", "sie", "mein", "!", "\u00ab"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ITJ", "$.", "PPER", "VVFIN", "$,", "VVFIN", "$.", "ADV", "VAFIN", "PPER", "PPOSAT", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.11": {"line.1": {"text": "Von der Thei\u00df, der klaren, fischereichen,", "tokens": ["Von", "der", "Thei\u00df", ",", "der", "kla\u00b7ren", ",", "fi\u00b7sche\u00b7rei\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ART", "ADJA", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ist der Geiger Mischka hingezogen,", "tokens": ["Ist", "der", "Gei\u00b7ger", "Mischka", "hin\u00b7ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "NE", "VVPP", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Wo der Marosch barsche Wogen", "tokens": ["Wo", "der", "Ma\u00b7rosch", "bar\u00b7sche", "Wo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Brausend durch besch\u00e4umte Klippen streichen.", "tokens": ["Brau\u00b7send", "durch", "be\u00b7sch\u00e4um\u00b7te", "Klip\u00b7pen", "strei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.12": {"line.1": {"text": "Der Zigeuner wandert, arm und heiter,", "tokens": ["Der", "Zi\u00b7geu\u00b7ner", "wan\u00b7dert", ",", "arm", "und", "hei\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "In die Ferne, Fremde, fort und weiter;", "tokens": ["In", "die", "Fer\u00b7ne", ",", "Frem\u00b7de", ",", "fort", "und", "wei\u00b7ter", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "NN", "$,", "PTKVZ", "KON", "ADV", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Wenn er auch am Wohlgeschmack der Erde", "tokens": ["Wenn", "er", "auch", "am", "Wohl\u00b7ge\u00b7schmack", "der", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPRART", "NN", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Karg und selten nur sich weidet,", "tokens": ["Karg", "und", "sel\u00b7ten", "nur", "sich", "wei\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJD", "ADV", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ist ihm jeder Ort doch bald entleidet,", "tokens": ["Ist", "ihm", "je\u00b7der", "Ort", "doch", "bald", "ent\u00b7lei\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PIAT", "NN", "ADV", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Und was heimisch, wird ihm zur Beschwerde;", "tokens": ["Und", "was", "hei\u00b7misch", ",", "wird", "ihm", "zur", "Be\u00b7schwer\u00b7de", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADJD", "$,", "VAFIN", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Wenig brauchend kommt und geht", "tokens": ["W