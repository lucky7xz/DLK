", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "PWAV", "ADJA", "NN", "ADJA", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Unter diesen Halmen, die vorzeiten", "tokens": ["Un\u00b7ter", "die\u00b7sen", "Hal\u00b7men", ",", "die", "vor\u00b7zei\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PDAT", "NN", "$,", "ART", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Bei dem Klang der Lerchenlieder", "tokens": ["Bei", "dem", "Klang", "der", "Ler\u00b7chen\u00b7lie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf dem Feld sich wiegten hin und wider.", "tokens": ["Auf", "dem", "Feld", "sich", "wieg\u00b7ten", "hin", "und", "wi\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PRF", "VVFIN", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.15": {"line.1": {"text": "Nicht allein an Schall und s\u00fc\u00dfen Weisen", "tokens": ["Nicht", "al\u00b7lein", "an", "Schall", "und", "s\u00fc\u00b7\u00dfen", "Wei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "APPR", "NN", "KON", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ist dies niedre H\u00fcttlein reich zu preisen;", "tokens": ["Ist", "dies", "nied\u00b7re", "H\u00fctt\u00b7lein", "reich", "zu", "prei\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "ADJA", "NN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Strahlen hegt es auch in F\u00fclle,", "tokens": ["Strah\u00b7len", "hegt", "es