{"dta.poem.20086": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Um Dich .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1851", "urn": "urn:nbn:de:kobv:b4-200905191321", "language": ["de:0.99"], "booktitle": "Fontane, Theodor: Gedichte. Berlin, 1851."}, "poem": {"stanza.1": {"line.1": {"text": "Nicht Gl\u00fcckes-bar sind Deine Lenze,", "tokens": ["Nicht", "Gl\u00fc\u00b7ckes\u00b7bar", "sind", "Dei\u00b7ne", "Len\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du forderst nur des Gl\u00fccks zu viel;", "tokens": ["Du", "for\u00b7derst", "nur", "des", "Gl\u00fccks", "zu", "viel", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "APPR", "PIS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gieb Deinem Wunsche Maa\u00df und Grenze,", "tokens": ["Gieb", "Dei\u00b7nem", "Wun\u00b7sche", "Maa\u00df", "und", "Gren\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und Dir entgegen kommt das Ziel.", "tokens": ["Und", "Dir", "ent\u00b7ge\u00b7gen", "kommt", "das", "Ziel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PTKVZ", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie dumpfes Unkraut la\u00df vermodern,", "tokens": ["Wie", "dum\u00b7pfes", "Un\u00b7kraut", "la\u00df", "ver\u00b7mo\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "VVIMP", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Was in Dir noch des Glaubens ist:", "tokens": ["Was", "in", "Dir", "noch", "des", "Glau\u00b7bens", "ist", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "PPER", "ADV", "ART", "NN", "VAFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Du h\u00e4ttest doppelt einzufodern", "tokens": ["Du", "h\u00e4t\u00b7test", "dop\u00b7pelt", "ein\u00b7zu\u00b7fo\u00b7dern"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADJD", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Des Lebens Gl\u00fcck, weil ", "tokens": ["Des", "Le\u00b7bens", "Gl\u00fcck", ",", "weil"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "NN", "$,", "KOUS"], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Das Gl\u00fcck, kein Reiter wird\u2019s erjagen,", "tokens": ["Das", "Gl\u00fcck", ",", "kein", "Rei\u00b7ter", "wird's", "er\u00b7ja\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PIAT", "NN", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Es ist nicht dort, es ist nicht hier;", "tokens": ["Es", "ist", "nicht", "dort", ",", "es", "ist", "nicht", "hier", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADV", "$,", "PPER", "VAFIN", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Lern\u2019 \u00fcberwinden, lern\u2019 entsagen,", "tokens": ["Lern'", "\u00fc\u00b7berw\u00b7in\u00b7den", ",", "lern'", "ent\u00b7sa\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und ungeahnt erbl\u00fcht es Dir.", "tokens": ["Und", "un\u00b7ge\u00b7ahnt", "er\u00b7bl\u00fcht", "es", "Dir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}