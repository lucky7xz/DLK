{"dta.poem.18185": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Die lehrenden Bienen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198805", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein sumsend Heer befli\u00dfner Bienen,", "tokens": ["Ein", "sum\u00b7send", "Heer", "be\u00b7fli\u00df\u00b7ner", "Bie\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sah mein ger\u00fchrtes Aug im Gr\u00fcnen,", "tokens": ["Sah", "mein", "ge\u00b7r\u00fchr\u00b7tes", "Aug", "im", "Gr\u00fc\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Das hin und her auf Blumen flog,", "tokens": ["Das", "hin", "und", "her", "auf", "Blu\u00b7men", "flog", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PTKVZ", "KON", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und daraus seinen Honig sog;", "tokens": ["Und", "da\u00b7raus", "sei\u00b7nen", "Ho\u00b7nig", "sog", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Jhr muntrer Flei\u00df nahm aus der Bl\u00fcthe,", "tokens": ["Ihr", "mun\u00b7trer", "Flei\u00df", "nahm", "aus", "der", "Bl\u00fc\u00b7the", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Den s\u00fcssen Saft und trug ihn fort", "tokens": ["Den", "s\u00fcs\u00b7sen", "Saft", "und", "trug", "ihn", "fort"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "KON", "VVFIN", "PPER", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Zum Korb, zu dem Verwahrungs-Ort:", "tokens": ["Zum", "Korb", ",", "zu", "dem", "Ver\u00b7wah\u00b7rungs\u00b7Ort", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der Anblik ging mir zu Gem\u00fcthe.", "tokens": ["Der", "An\u00b7blik", "ging", "mir", "zu", "Ge\u00b7m\u00fc\u00b7the", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Ich dachte, O mein ", "tokens": ["Ich", "dach\u00b7te", ",", "O", "mein"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "NE", "PPOSAT"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.10": {"text": "Zum Menschen, hat so starke Triebe", "tokens": ["Zum", "Men\u00b7schen", ",", "hat", "so", "star\u00b7ke", "Trie\u00b7be"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "VAFIN", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Dem Bienen Volke eingepr\u00e4gt:", "tokens": ["Dem", "Bie\u00b7nen", "Vol\u00b7ke", "ein\u00b7ge\u00b7pr\u00e4gt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Denn wenn man ihren Fleis erwegt,", "tokens": ["Denn", "wenn", "man", "ih\u00b7ren", "Fleis", "er\u00b7wegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Den sie beim Honigbau beweisen,", "tokens": ["Den", "sie", "beim", "Ho\u00b7nig\u00b7bau", "be\u00b7wei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Wie sie so bald die L\u00fcfte warm,", "tokens": ["Wie", "sie", "so", "bald", "die", "L\u00fcf\u00b7te", "warm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADV", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Mit hellen Hauffen, grosser Schwarm", "tokens": ["Mit", "hel\u00b7len", "Hauf\u00b7fen", ",", "gros\u00b7ser", "Schwarm"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Durch Felder und durch Wiesen reisen:", "tokens": ["Durch", "Fel\u00b7der", "und", "durch", "Wie\u00b7sen", "rei\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "So mu\u00df man warlich eingestehen,", "tokens": ["So", "mu\u00df", "man", "war\u00b7lich", "ein\u00b7ge\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Da\u00df ", "tokens": ["Da\u00df"], "token_info": ["word"], "pos": ["KOUS"], "meter": "+", "measure": "single.up"}, "line.19": {"text": "Der solche Kreatur gemacht,", "tokens": ["Der", "sol\u00b7che", "Kre\u00b7a\u00b7tur", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Uns zum Vergn\u00fcgen ausgedacht,", "tokens": ["Uns", "zum", "Ver\u00b7gn\u00fc\u00b7gen", "aus\u00b7ge\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Damit sie uns die S\u00fc\u00dfigkeiten,", "tokens": ["Da\u00b7mit", "sie", "uns", "die", "S\u00fc\u00b7\u00dfig\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Aus Blumen, Bl\u00fcth und Fr\u00fcchten ziehn,", "tokens": ["Aus", "Blu\u00b7men", ",", "Bl\u00fcth", "und", "Fr\u00fcch\u00b7ten", "ziehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Und durch ein uns geheim Bem\u00fchn,", "tokens": ["Und", "durch", "ein", "uns", "ge\u00b7heim", "Be\u00b7m\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.24": {"text": "Den klaren Honig zubereiten.", "tokens": ["Den", "kla\u00b7ren", "Ho\u00b7nig", "zu\u00b7be\u00b7rei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Mir deuchte, da ich meine Augen", "tokens": ["Mir", "deuch\u00b7te", ",", "da", "ich", "mei\u00b7ne", "Au\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Recht sch\u00e4rfte, ihr besch\u00e4ftigt Saugen,", "tokens": ["Recht", "sch\u00e4rf\u00b7te", ",", "ihr", "be\u00b7sch\u00e4f\u00b7tigt", "Sau\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "PPER", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Noch einmahl wieder zu besehn,", "tokens": ["Noch", "ein\u00b7mahl", "wie\u00b7der", "zu", "be\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Da\u00df mir ein sumsendes Geth\u00f6n,", "tokens": ["Da\u00df", "mir", "ein", "sum\u00b7sen\u00b7des", "Ge\u00b7th\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Mit leisen Murmeln dieses lehrte:", "tokens": ["Mit", "lei\u00b7sen", "Mur\u00b7meln", "die\u00b7ses", "lehr\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PDS", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "O! w\u00e4re in der Fr\u00fchlings-Zeit", "tokens": ["O", "!", "w\u00e4\u00b7re", "in", "der", "Fr\u00fch\u00b7lings\u00b7Zeit"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$.", "VAFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Des Lebens, jeder Mensch bereit", "tokens": ["Des", "Le\u00b7bens", ",", "je\u00b7der", "Mensch", "be\u00b7reit"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PIAT", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Zu sammlen, wie es sich geh\u00f6rte:", "tokens": ["Zu", "samm\u00b7len", ",", "wie", "es", "sich", "ge\u00b7h\u00f6r\u00b7te", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "PWAV", "PPER", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.33": {"text": "So w\u00fcrden in den Winter-Tagen", "tokens": ["So", "w\u00fcr\u00b7den", "in", "den", "Win\u00b7ter\u00b7Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.34": {"text": "Des Alters, sich nicht so viel plagen", "tokens": ["Des", "Al\u00b7ters", ",", "sich", "nicht", "so", "viel", "pla\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRF", "PTKNEG", "ADV", "ADV", "VVFIN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.35": {"text": "Mit Armuth und mit Ungemach,", "tokens": ["Mit", "Ar\u00b7muth", "und", "mit", "Un\u00b7ge\u00b7mach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "Wer seinen rechten Erndte-Tag,", "tokens": ["Wer", "sei\u00b7nen", "rech\u00b7ten", "Ernd\u00b7te\u00b7Tag", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.37": {"text": "Der in der Jugend ist, vers\u00e4umet,", "tokens": ["Der", "in", "der", "Ju\u00b7gend", "ist", ",", "ver\u00b7s\u00e4u\u00b7met", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VAFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.38": {"text": "Und alles, bis ins Alter schiebt,", "tokens": ["Und", "al\u00b7les", ",", "bis", "ins", "Al\u00b7ter", "schiebt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "$,", "KOUS", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "Der ist ein Mensch der Faulheit liebt,", "tokens": ["Der", "ist", "ein", "Mensch", "der", "Faul\u00b7heit", "liebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.40": {"text": "Der nur stets leere W\u00fcnsche tr\u00e4umet.", "tokens": ["Der", "nur", "stets", "lee\u00b7re", "W\u00fcn\u00b7sche", "tr\u00e4u\u00b7met", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.41": {"text": "Wer immer bei der Arbeit sizzet,", "tokens": ["Wer", "im\u00b7mer", "bei", "der", "Ar\u00b7beit", "siz\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.42": {"text": "Nur sich nicht andern dadurch n\u00fczzet,", "tokens": ["Nur", "sich", "nicht", "an\u00b7dern", "da\u00b7durch", "n\u00fcz\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PRF", "PTKNEG", "PIS", "PAV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.43": {"text": "Der seh uns rege Bienen an,", "tokens": ["Der", "seh", "uns", "re\u00b7ge", "Bie\u00b7nen", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.44": {"text": "Davon ein Mensche lernen kann", "tokens": ["Da\u00b7von", "ein", "Men\u00b7sche", "ler\u00b7nen", "kann"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "ART", "NN", "VVINF", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.45": {"text": "Da\u00df seine Pflicht auch das einschliesse,", "tokens": ["Da\u00df", "sei\u00b7ne", "Pflicht", "auch", "das", "ein\u00b7schlies\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.46": {"text": "Der Welt zugleich mit Nuz zu sein,", "tokens": ["Der", "Welt", "zu\u00b7gleich", "mit", "Nuz", "zu", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "NN", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.47": {"text": "Damit das Gute allgemein,", "tokens": ["Da\u00b7mit", "das", "Gu\u00b7te", "all\u00b7ge\u00b7mein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.48": {"text": "Auch mit von uns, aufs andre fliesse.", "tokens": ["Auch", "mit", "von", "uns", ",", "aufs", "and\u00b7re", "flies\u00b7se", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "APPR", "PPER", "$,", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}