{"textgrid.poem.33239": {"metadata": {"author": {"name": "Canitz, Friedrich Rudolph Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich schmiere nicht viel her, weil es zum Scheiden gehet,", "genre": "verse", "period": "N.A.", "pub_year": 1676, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich schmiere nicht viel her, weil es zum Scheiden gehet,", "tokens": ["Ich", "schmie\u00b7re", "nicht", "viel", "her", ",", "weil", "es", "zum", "Schei\u00b7den", "ge\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "PTKVZ", "$,", "KOUS", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und ich, Calenio, dich wieder lassen mu\u00df,", "tokens": ["Und", "ich", ",", "Ca\u00b7le\u00b7nio", ",", "dich", "wie\u00b7der", "las\u00b7sen", "mu\u00df", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "NE", "$,", "PPER", "ADV", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Da nunmehr dein Compa\u00df dem Nordpol n\u00e4her stehet,", "tokens": ["Da", "nun\u00b7mehr", "dein", "Com\u00b7pa\u00df", "dem", "Nord\u00b7pol", "n\u00e4\u00b7her", "ste\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPOSAT", "NN", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und dich dein Schicksal f\u00fchrt um kalten Pregel-Flu\u00df.", "tokens": ["Und", "dich", "dein", "Schick\u00b7sal", "f\u00fchrt", "um", "kal\u00b7ten", "Pre\u00b7gel\u00b7Flu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PPOSAT", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wohlan! es blicke dich in S\u00fcd- und Ost- und Westen,", "tokens": ["Wo\u00b7hlan", "!", "es", "bli\u00b7cke", "dich", "in", "S\u00fcd", "und", "Ost", "und", "Wes\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "PPER", "VVFIN", "PRF", "APPR", "TRUNC", "KON", "TRUNC", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So lang die Reise w\u00e4hrt, das Gl\u00fccke g\u00fcnstig an,", "tokens": ["So", "lang", "die", "Rei\u00b7se", "w\u00e4hrt", ",", "das", "Gl\u00fc\u00b7cke", "g\u00fcns\u00b7tig", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "VVFIN", "$,", "ART", "NN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Bi\u00df man dich wiederum, zu deinem eignen Besten,", "tokens": ["Bi\u00df", "man", "dich", "wie\u00b7de\u00b7rum", ",", "zu", "dei\u00b7nem", "eig\u00b7nen", "Bes\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PRF", "ADV", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und deiner Freunde Lust, willkommen heissen kan.", "tokens": ["Und", "dei\u00b7ner", "Freun\u00b7de", "Lust", ",", "will\u00b7kom\u00b7men", "heis\u00b7sen", "kan", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "$,", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Doch einen rechten Wunsch will ich auf k\u00fcnfftig sparen,", "tokens": ["Doch", "ei\u00b7nen", "rech\u00b7ten", "Wunsch", "will", "ich", "auf", "k\u00fcnff\u00b7tig", "spa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VMFIN", "PPER", "APPR", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Was heissers fl\u00f6\u00dft mir erst, nach dir, die Sehnsucht ein;", "tokens": ["Was", "heis\u00b7sers", "fl\u00f6\u00dft", "mir", "erst", ",", "nach", "dir", ",", "die", "Sehn\u00b7sucht", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "PPER", "ADV", "$,", "APPR", "PPER", "$,", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wann du verschwunden bist, dann werd ich erst erfahren,", "tokens": ["Wann", "du", "ver\u00b7schwun\u00b7den", "bist", ",", "dann", "werd", "ich", "erst", "er\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVPP", "VAFIN", "$,", "ADV", "VAFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Da\u00df Hoffen und Verdru\u00df die besten Musen seyn.", "tokens": ["Da\u00df", "Hof\u00b7fen", "und", "Ver\u00b7dru\u00df", "die", "bes\u00b7ten", "Mu\u00b7sen", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "ART", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Indessen sey bedacht, dein Reisen anzustellen,", "tokens": ["In\u00b7des\u00b7sen", "sey", "be\u00b7dacht", ",", "dein", "Rei\u00b7sen", "an\u00b7zu\u00b7stel\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "VVPP", "$,", "PPOSAT", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Da\u00df, eh man noch den Mertz in unsern Briefen schreibt,", "tokens": ["Da\u00df", ",", "eh", "man", "noch", "den", "Mertz", "in", "un\u00b7sern", "Brie\u00b7fen", "schreibt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "KOUS", "PIS", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Du deine Gegenwart mir m\u00f6gest zugesellen,", "tokens": ["Du", "dei\u00b7ne", "Ge\u00b7gen\u00b7wart", "mir", "m\u00f6\u00b7gest", "zu\u00b7ge\u00b7sel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Drauf geh, wohin der Wind dein leichtes Segel treibt.", "tokens": ["Drauf", "geh", ",", "wo\u00b7hin", "der", "Wind", "dein", "leich\u00b7tes", "Se\u00b7gel", "treibt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "$,", "PWAV", "ART", "NN", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Dein Anschlag werde dir nicht anfangs gleich zu nichte!", "tokens": ["Dein", "An\u00b7schlag", "wer\u00b7de", "dir", "nicht", "an\u00b7fangs", "gleich", "zu", "nich\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "PTKNEG", "ADV", "ADV", "PTKA", "PIS", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Doch, wann du unverhofft von l\u00e4ngerm bleiben h\u00f6rst,", "tokens": ["Doch", ",", "wann", "du", "un\u00b7ver\u00b7hofft", "von", "l\u00e4n\u00b7germ", "blei\u00b7ben", "h\u00f6rst", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWAV", "PPER", "ADJD", "APPR", "ADJD", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "So sprich mit solchem Thon und solchem Angesichte,", "tokens": ["So", "sprich", "mit", "sol\u00b7chem", "Thon", "und", "sol\u00b7chem", "An\u00b7ge\u00b7sich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "PIAT", "NN", "KON", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Wie du des Morgens fr\u00fch mich aus dem Schlaffe st\u00f6rst:", "tokens": ["Wie", "du", "des", "Mor\u00b7gens", "fr\u00fch", "mich", "aus", "dem", "Schlaf\u00b7fe", "st\u00f6rst", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "ADV", "ADJD", "PRF", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Soll Euer Sohn in Preussen bleiben,", "tokens": ["Soll", "Eu\u00b7er", "Sohn", "in", "Preus\u00b7sen", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Frau Oberj\u00e4germeisterin,", "tokens": ["Frau", "O\u00b7ber\u00b7j\u00e4\u00b7ger\u00b7meis\u00b7te\u00b7rin", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Warum habt ihr mir lassen schreiben,", "tokens": ["Wa\u00b7rum", "habt", "ihr", "mir", "las\u00b7sen", "schrei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "PPER", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mir, der ich kein Landstreicher bin?", "tokens": ["Mir", ",", "der", "ich", "kein", "Land\u00b7strei\u00b7cher", "bin", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "PPER", "PIAT", "NN", "VAFIN", "$."], "meter": "-+--++-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Und, ohne M\u00fch und viel Beschwerden,", "tokens": ["Und", ",", "oh\u00b7ne", "M\u00fch", "und", "viel", "Be\u00b7schwer\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUI", "NN", "KON", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wohl etwas gr\u00f6ssers k\u00f6nnen werden.", "tokens": ["Wohl", "et\u00b7was", "gr\u00f6s\u00b7sers", "k\u00f6n\u00b7nen", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJA", "VMFIN", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Lasst euer Kind, betr\u00fcbte Mutter,", "tokens": ["Lasst", "eu\u00b7er", "Kind", ",", "be\u00b7tr\u00fcb\u00b7te", "Mut\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Brecht nicht das schon gegebne Wort,", "tokens": ["Brecht", "nicht", "das", "schon", "ge\u00b7geb\u00b7ne", "Wort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ART", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und solte gleich kein Flaschen-Futter", "tokens": ["Und", "sol\u00b7te", "gleich", "kein", "Fla\u00b7schen\u00b7Fut\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zu finden seyn, so mu\u00df er fort.", "tokens": ["Zu", "fin\u00b7den", "seyn", ",", "so", "mu\u00df", "er", "fort", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "VAINF", "$,", "ADV", "VMFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Hat manche sich doch tr\u00f6sten m\u00fcssen,", "tokens": ["Hat", "man\u00b7che", "sich", "doch", "tr\u00f6s\u00b7ten", "m\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "PRF", "ADV", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die aus Adonis Arm gerissen.", "tokens": ["Die", "aus", "A\u00b7do\u00b7nis", "Arm", "ge\u00b7ris\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NE", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Der Weg ist einmahl vorgenommen,", "tokens": ["Der", "Weg", "ist", "ein\u00b7mahl", "vor\u00b7ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So sagt der Herr von Wallenrodt", "tokens": ["So", "sagt", "der", "Herr", "von", "Wal\u00b7len\u00b7rodt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Lasst mir nur den Gef\u00e4hrten kommen!", "tokens": ["Lasst", "mir", "nur", "den", "Ge\u00b7f\u00e4hr\u00b7ten", "kom\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Gen\u00e4dge Frau, im Fall der Noth,", "tokens": ["Ge\u00b7n\u00e4d\u00b7ge", "Frau", ",", "im", "Fall", "der", "Noth", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und, da mir alles sollt' entstehen,", "tokens": ["Und", ",", "da", "mir", "al\u00b7les", "sollt'", "ent\u00b7ste\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPER", "PIS", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "M\u00fcst ihr selbst mit nach Franckreich gehen.", "tokens": ["M\u00fcst", "ihr", "selbst", "mit", "nach", "Fran\u00b7ck\u00b7reich", "ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "APPR", "NE", "VVINF", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}}, "stanza.5": {"line.1": {"text": "Ich wei\u00df, ein Weiber Hertz ist leichtlich zu erbitten,", "tokens": ["Ich", "wei\u00df", ",", "ein", "Wei\u00b7ber", "Hertz", "ist", "leicht\u00b7lich", "zu", "er\u00b7bit\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ART", "NN", "NN", "VAFIN", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wann ein beredter Mund den Vortrag selbst gethan,", "tokens": ["Wann", "ein", "be\u00b7red\u00b7ter", "Mund", "den", "Vor\u00b7trag", "selbst", "ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wer ist auch, welcher wohl so angenehmen Sitten", "tokens": ["Wer", "ist", "auch", ",", "wel\u00b7cher", "wohl", "so", "an\u00b7ge\u00b7neh\u00b7men", "Sit\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "ADV", "$,", "PRELS", "ADV", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und deiner H\u00f6flichkeit leicht was versagen kan?", "tokens": ["Und", "dei\u00b7ner", "H\u00f6f\u00b7lich\u00b7keit", "leicht", "was", "ver\u00b7sa\u00b7gen", "kan", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADJD", "PIS", "VVINF", "VMFIN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Noch eins: du suchest war dein Heil in fremden L\u00e4ndern,", "tokens": ["Noch", "eins", ":", "du", "su\u00b7chest", "war", "dein", "Heil", "in", "frem\u00b7den", "L\u00e4n\u00b7dern", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "$.", "PPER", "VVFIN", "VAFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Doch glaub ich, da\u00df du fest in deiner Freundschafft bist,", "tokens": ["Doch", "glaub", "ich", ",", "da\u00df", "du", "fest", "in", "dei\u00b7ner", "Freund\u00b7schafft", "bist", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "KOUS", "PPER", 