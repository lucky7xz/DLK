{"textgrid.poem.33315": {"metadata": {"author": {"name": "Blumauer, Aloys", "birth": "N.A.", "death": "N.A."}, "title": "Eingang des zweiten Gesang's des M\u00e4dchens von Orleans", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dreimal begl\u00fcckt ist der, der eine Jungfrau findet,", "tokens": ["Drei\u00b7mal", "be\u00b7gl\u00fcckt", "ist", "der", ",", "der", "ei\u00b7ne", "Jung\u00b7frau", "fin\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "VAFIN", "ART", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Er hat ein grosses Gut; allein nach meinem Sinn", "tokens": ["Er", "hat", "ein", "gros\u00b7ses", "Gut", ";", "al\u00b7lein", "nach", "mei\u00b7nem", "Sinn"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$.", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ist ein uns liebend Herz ein s\u00fcsserer Gewinn,", "tokens": ["Ist", "ein", "uns", "lie\u00b7bend", "Herz", "ein", "s\u00fcs\u00b7se\u00b7rer", "Ge\u00b7winn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "PPER", "ADJD", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Weil wahres Gl\u00fcck sich nur auf Liebe gr\u00fcndet.", "tokens": ["Weil", "wah\u00b7res", "Gl\u00fcck", "sich", "nur", "auf", "Lie\u00b7be", "gr\u00fcn\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "PRF", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-