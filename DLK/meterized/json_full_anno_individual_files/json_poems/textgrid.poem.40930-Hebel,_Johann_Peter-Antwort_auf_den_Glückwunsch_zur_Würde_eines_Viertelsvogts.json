{"textgrid.poem.40930": {"metadata": {"author": {"name": "Hebel, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "Antwort auf den Gl\u00fcckwunsch zur W\u00fcrde eines Viertelsvogts", "genre": "verse", "period": "N.A.", "pub_year": 1793, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "'s isch frili wohr, e Viertelsvogt,", "tokens": ["'s", "isch", "fri\u00b7li", "wohr", ",", "e", "Vier\u00b7tels\u00b7vogt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "FM", "FM", "FM", "$,", "FM.la", "FM.la", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "wenn so ne Her im Sessel hockt,", "tokens": ["wenn", "so", "ne", "Her", "im", "Ses\u00b7sel", "hockt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADJA", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "und i\u00dft si Fleisch und trinkt si Wi,", "tokens": ["und", "i\u00dft", "si", "Fleisch", "und", "trinkt", "si", "Wi", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NE", "NN", "KON", "VVFIN", "NE", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "sel luegt e wenig anderst dri.", "tokens": ["sel", "luegt", "e", "we\u00b7nig", "an\u00b7derst", "dri", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "ADV", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sust hani wol zu Brot und Schunke", "tokens": ["Sust", "ha\u00b7ni", "wol", "zu", "Brot", "und", "Schun\u00b7ke"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "ADV", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "ne Moos, au anderthalbi trunke,", "tokens": ["ne", "Moos", ",", "au", "an\u00b7der\u00b7thal\u00b7bi", "trun\u00b7ke", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "NE", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "jez, wie's der Name mit em bringt,", "tokens": ["jez", ",", "wie's", "der", "Na\u00b7me", "mit", "em", "bringt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "ART", "NN", "APPR", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "der Viertelsvogt e Viertel zwingt.", "tokens": ["der", "Vier\u00b7tels\u00b7vogt", "e", "Vier\u00b7tel", "zwingt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Sust isch meng Eichli, ungvexirt,", "tokens": ["Sust", "isch", "meng", "Eich\u00b7li", ",", "ung\u00b7vex\u00b7irt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$,", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "z' Nacht usem Gmeiwald furt spazirt,", "tokens": ["z'", "Nacht", "u\u00b7sem", "Gmei\u00b7wald", "furt", "spa\u00b7zirt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "'s het glengt no zu de chleine Poste,", "tokens": ["'s", "het", "glengt", "no", "zu", "de", "chlei\u00b7ne", "Pos\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVFIN", "NE", "APPR", "NE", "NE", "NE", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.12": {"text": "jez cha's bi Gost e W\u00e4ldli choste.", "tokens": ["jez", "cha's", "bi", "Gost", "e", "W\u00e4ld\u00b7li", "chos\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Sust hani nit no Ehre gspannt,", "tokens": ["Sust", "ha\u00b7ni", "nit", "no", "Eh\u00b7re", "gs\u00b7pannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "PTKNEG", "NE", "NN", "VVFIN", "$,"], "meter": "---+-+--+", "measure": "iambic.tri.chol"}, "line.14": {"text": "ha's au nit gha, 's isch wohlbikannt,", "tokens": ["ha's", "au", "nit", "gha", ",", "'s", "isch", "wohl\u00b7bi\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "PTKNEG", "NE", "$,", "PPER", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.15": {"text": "jez hei\u00dft's: \u00bbTue d'Augen uf, du Stock,", "tokens": ["jez", "hei\u00dft's", ":", "\u00bb", "Tue", "d'\u00b7Au\u00b7gen", "uf", ",", "du", "Stock", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$.", "$(", "FM.la", "FM.la", "FM.la", "$,", "PPER", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.16": {"text": "siehsch nit, wer chunnt, der Viertelsvogt!\u00ab", "tokens": ["sieh\u00b7sch", "nit", ",", "wer", "chunnt", ",", "der", "Vier\u00b7tels\u00b7vogt", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["PPER", "PTKNEG", "$,", "PWS", "VVFIN", "$,", "ART", "NN", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.17": {"text": "Sust hani, wiene Burgersma,", "tokens": ["Sust", "ha\u00b7ni", ",", "wie\u00b7ne", "Bur\u00b7gers\u00b7ma", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "mi Laubi und mi Lusti gha", "tokens": ["mi", "Lau\u00b7bi", "und", "mi", "Lus\u00b7ti", "gha"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["FM", "FM", "KON", "NE", "NE", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "und bi mit Holz und andere Ware", "tokens": ["und", "bi", "mit", "Holz", "und", "an\u00b7de\u00b7re", "Wa\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.20": {"text": "go Basel und ins Rebland gfahre.", "tokens": ["go", "Ba\u00b7sel", "und", "ins", "Re\u00b7bland", "gfah\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "KON", "APPRART", "NN", "VVFIN", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.21": {"text": "Jez isch's verbei, sel isch f\u00fcr d'Chnecht,", "tokens": ["Jez", "isch's", "ver\u00b7bei", ",", "sel", "isch", "f\u00fcr", "d'Chnecht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "PTKVZ", "$,", "ADV", "ADJD", "APPR", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "die Lumperkerli, ebe recht.", "tokens": ["die", "Lum\u00b7per\u00b7ker\u00b7li", ",", "e\u00b7be", "recht", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Der Viertelsvogt den Gaul besteigt", "tokens": ["Der", "Vier\u00b7tels\u00b7vogt", "den", "Gaul", "be\u00b7steigt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "und drauf hinein nach Basel reit.", "tokens": ["und", "drauf", "hin\u00b7ein", "nach", "Ba\u00b7sel", "reit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "APZR", "APPR", "NE", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Ne brave Choli hani do,", "tokens": ["Ne", "bra\u00b7ve", "Cho\u00b7li", "ha\u00b7ni", "do", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "FM", "FM", "FM", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.26": {"text": "und isch mi zimli wolfel cho.", "tokens": ["und", "isch", "mi", "zim\u00b7li", "wol\u00b7fel", "cho", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "FM", "FM", "FM", "FM", "FM", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "I ha ne alte Esel gchauft,", "tokens": ["I", "ha", "ne", "al\u00b7te", "E\u00b7sel", "gchauft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "ADJA", "NN", "VVFIN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.28": {"text": "und vor der Hand zum R\u00f6\u00dfli tauft.", "tokens": ["und", "vor", "der", "Hand", "zum", "R\u00f6\u00df\u00b7li", "tauft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Zerst hani sini Ohre gstuzt,", "tokens": ["Zerst", "ha\u00b7ni", "si\u00b7ni", "Oh\u00b7re", "gs\u00b7tuzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.30": {"text": "druf hani en mit Chienrue\u00df puzt,", "tokens": ["druf", "ha\u00b7ni", "en", "mit", "Chien\u00b7rue\u00df", "puzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "FM", "FM", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.31": {"text": "e falsche Zopf ans F\u00fcdle ghenkt,", "tokens": ["e", "fal\u00b7sche", "Zopf", "ans", "F\u00fcd\u00b7le", "ghenkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "wo bis an Boden abe lengt,", "tokens": ["wo", "bis", "an", "Bo\u00b7den", "a\u00b7be", "lengt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "APPR", "NN", "PTKVZ", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "und riit jez druf in Stadt und Land,", "tokens": ["und", "riit", "jez", "druf", "in", "Stadt", "und", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PAV", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "und woni n\u00e4ume gang und stand.", "tokens": ["und", "wo\u00b7ni", "n\u00e4u\u00b7me", "gang", "und", "stand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "VVFIN", "ADJD", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "'s isch frili wohr, e Viertelsvogt,", "tokens": ["'s", "isch", "fri\u00b7li", "wohr", ",", "e", "Vier\u00b7tels\u00b7vogt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "FM", "FM", "FM", "$,", "FM.la", "FM.la", "$,"], "meter": "+-+-+-+-+", "measure": "troch