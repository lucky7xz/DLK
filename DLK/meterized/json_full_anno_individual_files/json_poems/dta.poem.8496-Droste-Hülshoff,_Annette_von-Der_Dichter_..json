{"dta.poem.8496": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Der Dichter .", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1860", "urn": "urn:nbn:de:kobv:b4-200905191007", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ihr, die beim frohen Mahle lacht,", "tokens": ["Ihr", ",", "die", "beim", "fro\u00b7hen", "Mah\u00b7le", "lacht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Euch eure Blumen zieht in Scherben,", "tokens": ["Euch", "eu\u00b7re", "Blu\u00b7men", "zieht", "in", "Scher\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und was an Gut euch zugedacht,", "tokens": ["Und", "was", "an", "Gut", "euch", "zu\u00b7ge\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "APPR", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Euch wohlbehaglich la\u00dft vererben,", "tokens": ["Euch", "wohl\u00b7be\u00b7hag\u00b7lich", "la\u00dft", "ver\u00b7er\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ihr starrt dem Dichter in\u2019s Gesicht,", "tokens": ["Ihr", "starrt", "dem", "Dich\u00b7ter", "in's", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Verwundert, da\u00df er Rosen bricht", "tokens": ["Ver\u00b7wun\u00b7dert", ",", "da\u00df", "er", "Ro\u00b7sen", "bricht"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVPP", "$,", "KOUS", "PPER", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Von Disteln, aus dem Quell der Augen", "tokens": ["Von", "Dis\u00b7teln", ",", "aus", "dem", "Quell", "der", "Au\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Korall\u2019 und Perle wei\u00df zu saugen;", "tokens": ["Ko\u00b7rall'", "und", "Per\u00b7le", "wei\u00df", "zu", "sau\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Da\u00df er den Blitz herniederlangt,", "tokens": ["Da\u00df", "er", "den", "Blitz", "her\u00b7nie\u00b7der\u00b7langt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Um seine Lampe zu entz\u00fcnden,", "tokens": ["Um", "sei\u00b7ne", "Lam\u00b7pe", "zu", "ent\u00b7z\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Im Wettertoben, wenn euch bangt,", "tokens": ["Im", "Wet\u00b7ter\u00b7to\u00b7ben", ",", "wenn", "euch", "bangt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den rechten Odem wei\u00df zu finden;", "tokens": ["Den", "rech\u00b7ten", "O\u00b7dem", "wei\u00df", "zu", "fin\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ihr starrt ihn an mit halbem Neid,", "tokens": ["Ihr", "starrt", "ihn", "an", "mit", "hal\u00b7bem", "Neid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den Geistes-Cr\u00f6sus seiner Zeit,", "tokens": ["Den", "Geis\u00b7tes\u00b7Cr\u00f6\u00b7sus", "sei\u00b7ner", "Zeit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und wi\u00dft es nicht, mit welchen Qualen", "tokens": ["Und", "wi\u00dft", "es", "nicht", ",", "mit", "wel\u00b7chen", "Qua\u00b7len"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "$,", "APPR", "PWAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Er seine Sch\u00e4tze mu\u00df bezahlen.", "tokens": ["Er", "sei\u00b7ne", "Sch\u00e4t\u00b7ze", "mu\u00df", "be\u00b7zah\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wi\u00dft nicht, da\u00df ihn, Verdammten gleich,", "tokens": ["Wi\u00dft", "nicht", ",", "da\u00df", "ihn", ",", "Ver\u00b7damm\u00b7ten", "gleich", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "$,", "KOUS", "PPER", "$,", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nur reines Feuer kann ern\u00e4hren,", "tokens": ["Nur", "rei\u00b7nes", "Feu\u00b7er", "kann", "er\u00b7n\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Nur der durchst\u00fcrmten Wolke Reich", "tokens": ["Nur", "der", "durchs\u00b7t\u00fcrm\u00b7ten", "Wol\u00b7ke", "Reich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Lebensodem kann gew\u00e4hren;", "tokens": ["Den", "Le\u00b7ben\u00b7so\u00b7dem", "kann", "ge\u00b7w\u00e4h\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Da\u00df, wo das Haupt ihm sinnend h\u00e4ngt,", "tokens": ["Da\u00df", ",", "wo", "das", "Haupt", "ihm", "sin\u00b7nend", "h\u00e4ngt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "PWAV", "ART", "NN", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sich blutig ihm die Thr\u00e4ne dr\u00e4ngt;", "tokens": ["Sich", "blu\u00b7tig", "ihm", "die", "Thr\u00e4\u00b7ne", "dr\u00e4ngt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Nur in des sch\u00e4rfsten Dornes Spalten", "tokens": ["Nur", "in", "des", "sch\u00e4rfs\u00b7ten", "Dor\u00b7nes", "Spal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Sich seine Blume kann entfalten.", "tokens": ["Sich", "sei\u00b7ne", "Blu\u00b7me", "kann", "ent\u00b7fal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PPOSAT", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Meint ihr das Wetter z\u00fcnde nicht?", "tokens": ["Meint", "ihr", "das", "Wet\u00b7ter", "z\u00fcn\u00b7de", "nicht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Meint ihr der Sturm ersch\u00fcttre nicht?", "tokens": ["Meint", "ihr", "der", "Sturm", "er\u00b7sch\u00fctt\u00b7re", "nicht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Meint ihr die Thr\u00e4ne brenne nicht?", "tokens": ["Meint", "ihr", "die", "Thr\u00e4\u00b7ne", "bren\u00b7ne", "nicht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Meint ihr die Dornen stechen nicht?", "tokens": ["Meint", "ihr", "die", "Dor\u00b7nen", "ste\u00b7chen", "nicht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ja eine Lamp\u2019 hat er entfacht,", "tokens": ["Ja", "ei\u00b7ne", "Lamp'", "hat", "er", "ent\u00b7facht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ART", "NN", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die nur das Mark ihm sieden macht;", "tokens": ["Die", "nur", "das", "Mark", "ihm", "sie\u00b7den", "macht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "PPER", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ja Perlen fischt er und Juwele,", "tokens": ["Ja", "Per\u00b7len", "fischt", "er", "und", "Ju\u00b7we\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "NN", "VVFIN", "PPER", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die kosten nichts \u2014 als seine Seele.", "tokens": ["Die", "kos\u00b7ten", "nichts", "als", "sei\u00b7ne", "See\u00b7le", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PIS", "$(", "KOUS", "PPOSAT", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}}}}