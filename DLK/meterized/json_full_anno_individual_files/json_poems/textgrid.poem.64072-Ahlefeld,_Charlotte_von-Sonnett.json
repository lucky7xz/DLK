{"textgrid.poem.64072": {"metadata": {"author": {"name": "Ahlefeld, Charlotte von", "birth": "N.A.", "death": "N.A."}, "title": "Sonnett", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Herrlichste, was uns der Himmel spendet,", "tokens": ["Das", "Herr\u00b7lichs\u00b7te", ",", "was", "uns", "der", "Him\u00b7mel", "spen\u00b7det", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Was er als Stab und St\u00fctze uns gegeben,", "tokens": ["Was", "er", "als", "Stab", "und", "St\u00fct\u00b7ze", "uns", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "KOUS", "NN", "KON", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Uns \u00fcber tr\u00fcbes Missgeschick zu heben, \u2013", "tokens": ["Uns", "\u00fc\u00b7ber", "tr\u00fc\u00b7bes", "Miss\u00b7ge\u00b7schick", "zu", "he\u00b7ben", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "APPR", "ADJA", "NN", "PTKZU", "VVINF", "$,", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Was nur allein das inn're Gl\u00fcck vollendet;", "tokens": ["Was", "nur", "al\u00b7lein", "das", "inn'\u00b7re", "Gl\u00fcck", "voll\u00b7en\u00b7det", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADV", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Sein G\u00f6tterdaseyn hat es nun geendet", "tokens": ["Sein", "G\u00f6t\u00b7ter\u00b7da\u00b7seyn", "hat", "es", "nu