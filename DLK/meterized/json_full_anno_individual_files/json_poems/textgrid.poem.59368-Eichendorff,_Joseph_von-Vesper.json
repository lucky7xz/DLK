{"textgrid.poem.59368": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Vesper", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Abendglocken klangen", "tokens": ["Die", "A\u00b7bend\u00b7glo\u00b7cken", "klan\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Schon durch das stille Tal,", "tokens": ["Schon", "durch", "das", "stil\u00b7le", "Tal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da sa\u00dfen wir zusammen", "tokens": ["Da", "sa\u00b7\u00dfen", "wir", "zu\u00b7sam\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da droben wohl hundertmal.", "tokens": ["Da", "dro\u00b7ben", "wohl", "hun\u00b7dert\u00b7mal", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "ADV", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Und unten war's so stille", "tokens": ["Und", "un\u00b7ten", "wa\u00b7r's", "so", "stil\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "ADV", "ADJA"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Im Lande weit und breit,", "tokens": ["Im", "Lan\u00b7de", "weit", "und", "breit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nur \u00fcber uns die Linde", "tokens": ["Nur", "\u00fc\u00b7ber", "uns", "die", "Lin\u