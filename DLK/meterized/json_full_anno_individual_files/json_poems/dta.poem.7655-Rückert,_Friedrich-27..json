{"dta.poem.7655": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "27.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn du den Muth verlierst, verlierest du die Kraft", "tokens": ["Wenn", "du", "den", "Muth", "ver\u00b7lierst", ",", "ver\u00b7lie\u00b7rest", "du", "die", "Kraft"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu wirken, und dein Werk verk\u00fcmmert kr\u00fcppelhaft.", "tokens": ["Zu", "wir\u00b7ken", ",", "und", "dein", "Werk", "ver\u00b7k\u00fcm\u00b7mert", "kr\u00fcp\u00b7pel\u00b7haft", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "KON", "PPOSAT", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wenn der gesunkne Muth auf einmal wieder steigt,", "tokens": ["Wenn", "der", "ge\u00b7sunk\u00b7ne", "Muth", "auf", "ein\u00b7mal", "wie\u00b7der", "steigt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "APPR", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu wilden Ranken ist alsbald der Trieb geneigt.", "tokens": ["Zu", "wil\u00b7den", "Ran\u00b7ken", "ist", "als\u00b7bald", "der", "Trieb", "ge\u00b7neigt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Drum bitte t\u00e4glich Gott, da\u00df er dich, streng wie g\u00fctig,", "tokens": ["Drum", "bit\u00b7te", "t\u00e4g\u00b7lich", "Gott", ",", "da\u00df", "er", "dich", ",", "streng", "wie", "g\u00fc\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "ADJD", "NN", "$,", "KOUS", "PPER", "PRF", "$,", "VVFIN", "KOKOM", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nie muthlos lasse seyn, noch werden \u00fcberm\u00fcthig.", "tokens": ["Nie", "muth\u00b7los", "las\u00b7se", "seyn", ",", "noch", "wer\u00b7den", "\u00fc\u00b7ber\u00b7m\u00fct\u00b7hig", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "VAINF", "$,", "ADV", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}