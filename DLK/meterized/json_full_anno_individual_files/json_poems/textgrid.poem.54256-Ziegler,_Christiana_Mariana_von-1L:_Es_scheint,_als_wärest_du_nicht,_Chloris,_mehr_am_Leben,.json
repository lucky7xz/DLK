{"textgrid.poem.54256": {"metadata": {"author": {"name": "Ziegler, Christiana Mariana von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es scheint, als w\u00e4rest du nicht, Chloris, mehr am Leben,", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es scheint, als w\u00e4rest du nicht, Chloris, mehr am Leben,", "tokens": ["Es", "scheint", ",", "als", "w\u00e4\u00b7rest", "du", "nicht", ",", "Chlo\u00b7ris", ",", "mehr", "am", "Le\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOKOM", "VAFIN", "PPER", "PTKNEG", "$,", "NE", "$,", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du bist, wie man ersieht, ein blosses Schatten-Bild,", "tokens": ["Du", "bist", ",", "wie", "man", "er\u00b7sieht", ",", "ein", "blos\u00b7ses", "Schat\u00b7ten\u00b7Bild", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "PWAV", "PIS", "VVFIN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dein Geist bed\u00fcnckt mich nur an Haut und Bein zu kleben,", "tokens": ["Dein", "Geist", "be\u00b7d\u00fcnckt", "mich", "nur", "an", "Haut", "und", "Bein", "zu", "kle\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADV", "APPR", "NN", "KON", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und deine Seele heist mit Wehmuth angef\u00fcllt.", "tokens": ["Und", "dei\u00b7ne", "See\u00b7le", "heist", "mit", "Weh\u00b7muth", "an\u00b7ge