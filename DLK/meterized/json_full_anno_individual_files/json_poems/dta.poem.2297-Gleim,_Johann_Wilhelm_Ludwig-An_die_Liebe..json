{"dta.poem.2297": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "An die Liebe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1744", "urn": "urn:nbn:de:kobv:b4-200905198996", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Liebe! allerliebste Liebe!", "tokens": ["Lie\u00b7be", "!", "al\u00b7ler\u00b7liebs\u00b7te", "Lie\u00b7be", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$.", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Seegne mich mit deinem Triebe.", "tokens": ["Seeg\u00b7ne", "mich", "mit", "dei\u00b7nem", "Trie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "La\u00df mir deinen Reitz empfinden,", "tokens": ["La\u00df", "mir", "dei\u00b7nen", "Reitz", "emp\u00b7fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "La\u00df mich deine Glut entz\u00fcnden,", "tokens": ["La\u00df", "mich", "dei\u00b7ne", "Glut", "ent\u00b7z\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "La\u00df mich deinen Zukker schmekken,", "tokens": ["La\u00df", "mich", "dei\u00b7nen", "Zuk\u00b7ker", "schmek\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "La\u00df mich durch ein Lied erwekken,", "tokens": ["La\u00df", "mich", "durch", "ein", "Lied", "er\u00b7wek\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wenn ich Zeit und Lust vers\u00e4ume", "tokens": ["Wenn", "ich", "Zeit", "und", "Lust", "ver\u00b7s\u00e4u\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "NN", "KON", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "M\u00fc\u00dfig wach\u2019, und m\u00fc\u00dfig tr\u00e4ume.", "tokens": ["M\u00fc\u00b7\u00dfig", "wach'", ",", "und", "m\u00fc\u00b7\u00dfig", "tr\u00e4u\u00b7me", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "KON", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "La\u00df mir h\u00fcbsch durch dein Geniessen", "tokens": ["La\u00df", "mir", "h\u00fcbsch", "durch", "dein", "Ge\u00b7nies\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Zeit und Stunden schueller fliessen.", "tokens": ["Zeit", "und", "Stun\u00b7den", "schu\u00b7el\u00b7ler", "flies\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ADJD", "VVINF", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.11": {"text": "La\u00df mirs an der M\u00fch zu w\u00e4hlen,", "tokens": ["La\u00df", "mirs", "an", "der", "M\u00fch", "zu", "w\u00e4h\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NE", "APPR", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Aber nie an Sch\u00f6nen fehlen,", "tokens": ["A\u00b7ber", "nie", "an", "Sch\u00f6\u00b7nen", "feh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Und damit auch viel Beschwerden,", "tokens": ["Und", "da\u00b7mit", "auch", "viel", "Be\u00b7schwer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "ADV", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Durch ein Mittel minder werden,", "tokens": ["Durch", "ein", "Mit\u00b7tel", "min\u00b7der", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "VAINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "La\u00df mir k\u00fcnftig nur von allen", "tokens": ["La\u00df", "mir", "k\u00fcnf\u00b7tig", "nur", "von", "al\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ADJD", "ADV", "APPR", "PIAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Eine sch\u00f6n seyn und gefallen.", "tokens": ["Ei\u00b7ne", "sch\u00f6n", "seyn", "und", "ge\u00b7fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VAINF", "KON", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Lehr ihr denn, sich gut zu schikken,", "tokens": ["Lehr", "ihr", "denn", ",", "sich", "gut", "zu", "schik\u00b7ken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "$,", "PRF", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Gut zu spielen, gut zu blikken,", "tokens": ["Gut", "zu", "spie\u00b7len", ",", "gut", "zu", "blik\u00b7ken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKZU", "VVINF", "$,", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Lehr ihr meine Neigung kennen,", "tokens": ["Lehr", "ihr", "mei\u00b7ne", "Nei\u00b7gung", "ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Klug zu frieren, klug zu brennen,", "tokens": ["Klug", "zu", "frie\u00b7ren", ",", "klug", "zu", "bren\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "$,", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Lehr ihr witzig abzuschlagen,", "tokens": ["Lehr", "ihr", "wit\u00b7zig", "ab\u00b7zu\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "VVIZU", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Lehr ihr reitzend Ja zu sagen.", "tokens": ["Lehr", "ihr", "reit\u00b7zend", "Ja", "zu", "sa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVPP", "PTKANT", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "Aus den Worten, aus den Werken", "tokens": ["Aus", "den", "Wor\u00b7ten", ",", "aus", "den", "Wer\u00b7ken"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "La\u00df ihr Wunsch und Willen merken?", "tokens": ["La\u00df", "ihr", "Wunsch", "und", "Wil\u00b7len", "mer\u00b7ken", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "KON", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "Aber lehr ihr Wunsch und Willen", "tokens": ["A\u00b7ber", "lehr", "ihr", "Wunsch", "und", "Wil\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "PPOSAT", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "Nicht zur Unzeit zu erf\u00fcllen,", "tokens": ["Nicht", "zur", "Un\u00b7zeit", "zu", "er\u00b7f\u00fcl\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.27": {"text": "Da\u00df sie sich erst artig sch\u00e4me", "tokens": ["Da\u00df", "sie", "sich", "erst", "ar\u00b7tig", "sch\u00e4\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "ADV", "ADJD", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Und sich nicht zu bald bequeme.", "tokens": ["Und", "sich", "nicht", "zu", "bald", "be\u00b7que\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "PTKNEG", "PTKA", "ADV", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.29": {"text": "Lehr ihr alle frohe Minen", "tokens": ["Lehr", "ihr", "al\u00b7le", "fro\u00b7he", "Mi\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "PIAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.30": {"text": "Die der Lust zum Vorteil dienen,", "tokens": ["Die", "der", "Lust", "zum", "Vor\u00b7teil", "die\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.31": {"text": "Lehr ihr alle Fr\u00f6lichkeiten", "tokens": ["Lehr", "ihr", "al\u00b7le", "Fr\u00f6\u00b7lich\u00b7kei\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.32": {"text": "Lehr ihr auch, was sie bedeuten,", "tokens": ["Lehr", "ihr", "auch", ",", "was", "sie", "be\u00b7deu\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "$,", "PRELS", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.33": {"text": "Da\u00df sie stets in Unschuld prange,", "tokens": ["Da\u00df", "sie", "stets", "in", "Un\u00b7schuld", "pran\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.34": {"text": "Da\u00df sie nie zuviel verlange,", "tokens": ["Da\u00df", "sie", "nie", "zu\u00b7viel", "ver\u00b7lan\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.35": {"text": "Da\u00df sie mirs vern\u00fcnftig klage,", "tokens": ["Da\u00df", "sie", "mirs", "ver\u00b7n\u00fcnf\u00b7tig", "kla\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NE", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.36": {"text": "Wenn ich ihr zuviel versage.", "tokens": ["Wenn", "ich", "ihr", "zu\u00b7viel", "ver\u00b7sa\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PIS", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.37": {"text": "Lehr ihr, wie man nie veralte", "tokens": ["Lehr", "ihr", ",", "wie", "man", "nie", "ver\u00b7al\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PPER", "$,", "PWAV", "PIS", "ADV", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.38": {"text": "Wie man Reitz und Wert behalte,", "tokens": ["Wie", "man", "Reitz", "und", "Wert", "be\u00b7hal\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "NN", "KON", "NE", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.39": {"text": "Wenn auch einst auf Brust und Wangen", "tokens": ["Wenn", "auch", "einst", "auf", "Brust", "und", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ADV", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.40": {"text": "Aller Rosen Schmukk vergangen.", "tokens": ["Al\u00b7ler", "Ro\u00b7sen", "Schmukk", "ver\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.41": {"text": "Lehr ihr, wenn wir uns vereinen,", "tokens": ["Lehr", "ihr", ",", "wenn", "wir", "uns", "ver\u00b7ei\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "KOUS", "PPER", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.42": {"text": "Treu zu seyn, und treu zu scheinen,", "tokens": ["Treu", "zu", "seyn", ",", "und", "treu", "zu", "schei\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VAINF", "$,", "KON", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.43": {"text": "Da\u00df sie mich mit nichts betr\u00fcbe", "tokens": ["Da\u00df", "sie", "mich", "mit", "nichts", "be\u00b7tr\u00fc\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "APPR", "PIS", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.44": {"text": "Und mich immer st\u00e4rker liebe.", "tokens": ["Und", "mich", "im\u00b7mer", "st\u00e4r\u00b7ker", "lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.45": {"text": "Lehr auch mich, durch deine Lehren,", "tokens": ["Lehr", "auch", "mich", ",", "durch", "dei\u00b7ne", "Leh\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPER", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.46": {"text": "Solchen Engel zu verehren,", "tokens": ["Sol\u00b7chen", "En\u00b7gel", "zu", "ver\u00b7eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PTKZU", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.47": {"text": "Da\u00df er, wenn ich ihn vergn\u00fcge,", "tokens": ["Da\u00df", "er", ",", "wenn", "ich", "ihn", "ver\u00b7gn\u00fc\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.48": {"text": "Keine Lust zum Wechsel kriege.", "tokens": ["Kei\u00b7ne", "Lust", "zum", "Wech\u00b7sel", "krie\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}