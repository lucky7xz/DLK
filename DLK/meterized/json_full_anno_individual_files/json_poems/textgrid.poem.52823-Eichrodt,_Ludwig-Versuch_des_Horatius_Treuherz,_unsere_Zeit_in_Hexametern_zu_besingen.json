{"textgrid.poem.52823": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Versuch des Horatius Treuherz, unsere Zeit in Hexametern zu besingen", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mitternacht war es vordem, jetzt ist es so ziemlich Mittagszeit,", "tokens": ["Mit\u00b7ter\u00b7nacht", "war", "es", "vor\u00b7dem", ",", "jetzt", "ist", "es", "so", "ziem\u00b7lich", "Mit\u00b7tags\u00b7zeit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADV", "$,", "ADV", "VAFIN", "PPER", "ADV", "ADV", "NN", "$,"], "meter": "--+--+--+--+-+-+", "measure": "anapaest.tetra.plus"}, "line.2": {"text": "Wenn nur die Reaktion nicht allzu fatal \u00fcber Hand nimmt,", "tokens": ["Wenn", "nur", "die", "Re\u00b7ak\u00b7ti\u00b7on", "nicht", "all\u00b7zu", "fa\u00b7tal", "\u00fc\u00b7ber", "Hand", "nimmt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "PTKNEG", "PTKA", "ADJD", "APPR", "NN", "VVFIN", "$,"], "meter": "+---+-+-+-+-+-+-", "measure": "dactylic.init"}, "line.3": {"text": "Zwar, und das tr\u00f6stet mich recht, sich dem Geiste der Zeit zu entziehen", "tokens": ["Zwar", ",", "und", "das", "tr\u00f6s\u00b7tet", "mich", "recht", ",", "sich", "dem", "Geis\u00b7te", "der", "Zeit", "zu", "ent\u00b7zie\u00b7hen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KON", "PDS", "VVFIN", "PPER", "ADJD", "$,", "PRF", "ART", "NN", "ART", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+--+--+-", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "Ist eine Schwierigkeit selbst f\u00fcr die finstern Gewalten des Stillstands,", "tokens": ["Ist", "ei\u00b7ne", "Schwie\u00b7rig\u00b7keit", "selbst", "f\u00fcr", "die", "fins\u00b7tern", "Ge\u00b7wal\u00b7ten", "des", "Still\u00b7stands", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "APPR", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+--+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Welche dem Fortschritte wehrt, stets vorw\u00e4rts zum Lichte zu schreiten;", "tokens": ["Wel\u00b7che", "dem", "Fort\u00b7schrit\u00b7te", "wehrt", ",", "stets", "vor\u00b7w\u00e4rts", "zum", "Lich\u00b7te", "zu", "schrei\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "ART", "NN", "VVFIN", "$,", "ADV", "ADV", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "+--++-+-+--+--+-", "measure": "trochaic.septa.relaxed"}, "line.6": {"text": "Freiheit, Licht und auch Recht, seit Guttenberg's edler Erfindung,", "tokens": ["Frei\u00b7heit", ",", "Licht", "und", "auch", "Recht", ",", "seit", "Gut\u00b7ten\u00b7ber\u00b7g's", "ed\u00b7ler", "Er\u00b7fin\u00b7dung", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "ADV", "NN", "$,", "APPR", "NE", "ADJA", "NN", "$,"], "meter": "+-+--+-+---+--+-", "measure": "trochaic.hexa.relaxed"}, "line.7": {"text": "Seit Amerika ward entdeckt von Christoph Columbus,", "tokens": ["Seit", "A\u00b7me\u00b7ri\u00b7ka", "ward", "ent\u00b7deckt", "von", "Chris\u00b7toph", "Co\u00b7lum\u00b7bus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VAFIN", "VVPP", "APPR", "NE", "NE", "$,"], "meter": "-+--+--+-+-+-+", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "Seit nun das Leinwandpapier und die Taschenuhr wurde erfunden,", "tokens": ["Seit", "nun", "das", "Lein\u00b7wand\u00b7pa\u00b7pier", "und", "die", "Ta\u00b7sche\u00b7nuhr", "wur\u00b7de", "er\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ART", "NN", "KON", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+--+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Ist keinem Angriff so leicht wie ehemals aus jetzt gesetzet;", "tokens": ["Ist", "kei\u00b7nem", "An\u00b7griff", "so", "leicht", "wie", "e\u00b7he\u00b7mals", "aus", "jetzt", "ge\u00b7set\u00b7zet", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "ADV", "ADJD", "KOKOM", "ADV", "APPR", "ADV", "VVPP", "$."], "meter": "-+-+--+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Aber erst seit man Censur abgeschafft und Geschwornengerichte", "tokens": ["A\u00b7ber", "erst", "seit", "man", "Cen\u00b7sur", "ab\u00b7ge\u00b7schafft", "und", "Ge\u00b7schwor\u00b7nen\u00b7ge\u00b7rich\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "PIS", "NN", "VVPP", "KON", "NN"], "meter": "+--+-+-+-+--+--+-", "measure": "iambic.septa.invert"}, "line.11": {"text": "Ueberall eingef\u00fchrt hat, erschrickt die Partei der Verdummung", "tokens": ["Ue\u00b7be\u00b7rall", "ein\u00b7ge\u00b7f\u00fchrt", "hat", ",", "er\u00b7schrickt", "die", "Par\u00b7tei", "der", "Ver\u00b7dum\u00b7mung"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVPP", "VAFIN", "$,", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.12": {"text": "Schier vor sich selbst, da sie sieht, wie beth\u00f6rt sie den Mi\u00dfbrauch gef\u00fcrchtet.", "tokens": ["Schier", "vor", "sich", "selbst", ",", "da", "sie", "sieht", ",", "wie", "be\u00b7th\u00f6rt", "sie", "den", "Mi\u00df\u00b7brauch", "ge\u00b7f\u00fcrch\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PRF", "ADV", "$,", "KOUS", "PPER", "VVFIN", "$,", "PWAV", "VVFIN", "PPER", "ART", "NN", "VVPP", "$."], "meter": "+--+--+--+--++-+-", "measure": "dactylic.tetra.plus"}, "line.13": {"text": "Darum lobsing' ich der Zeit, die gewi\u00df noch nicht da ist gewesen,", "tokens": ["Da\u00b7rum", "lob\u00b7sing'", "ich", "der", "Zeit", ",", "die", "ge\u00b7wi\u00df", "noch", "nicht", "da", "ist", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ART", "NN", "$,", "PRELS", "ADV", "ADV", "PTKNEG", "ADV", "VAFIN", "VAPP", "$,"], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.14": {"text": "Wo keine Inquisition, kein Autodafeh mehr ist m\u00f6glich,", "tokens": ["Wo", "kei\u00b7ne", "In\u00b7qui\u00b7si\u00b7ti\u00b7on", ",", "kein", "Au\u00b7to\u00b7da\u00b7feh", "mehr", "ist", "m\u00f6g\u00b7lich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "$,", "PIAT", "NN", "ADV", "VAFIN", "ADJD", "$,"], "meter": "-+--+-+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.15": {"text": "Weil schon die Staatspolizei sich selbst reformirt und gebessert.", "tokens": ["Weil", "schon", "die", "Staats\u00b7po\u00b7li\u00b7zei", "sich", "selbst", "re\u00b7for\u00b7mirt", "und", "ge\u00b7bes\u00b7sert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "PRF", "ADV", "VVPP", "KON", "VVPP", "$."], "meter": "-+--+-+-+-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.16": {"text": "Gl\u00fccklich der Mann, welcher lebt in der Zeit, die der Zukunft so nah' steht,", "tokens": ["Gl\u00fcck\u00b7lich", "der", "Mann", ",", "wel\u00b7cher", "lebt", "in", "der", "Zeit", ",", "die", "der", "Zu\u00b7kunft", "so", "nah'", "steht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "$,", "PRELS", "VVFIN", "APPR", "ART", "NN", "$,", "PRELS", "ART", "NN", "ADV", "ADJD", "VVFIN", "$,"], "meter": "+--+--+--+--+--++", "measure": "dactylic.tetra.plus"}, "line.17": {"text": "Wo sich sogar der Jesuit nicht scheut, mit dem Bahnzug zu fahren", "tokens": ["Wo", "sich", "so\u00b7gar", "der", "Je\u00b7su\u00b7it", "nicht", "scheut", ",", "mit", "dem", "Bahn\u00b7zug", "zu", "fah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PRF", "ADV", "ART", "NN", "PTKNEG", "VVFIN", "$,", "APPR", "ART", "NN", "PTKZU", "VVINF"], "meter": "-+-+--+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.18": {"text": "Und mit dem Telegraphist auf vertrautestem Fu\u00dfe zu stehen.", "tokens": ["Und", "mit", "dem", "Te\u00b7le\u00b7gra\u00b7phist", "auf", "ver\u00b7trau\u00b7tes\u00b7tem", "Fu\u00b7\u00dfe", "zu", "ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "APPR", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "+--+-+-+-+--+--+-", "measure": "iambic.septa.invert"}, "line.19": {"text": "Gl\u00fccklich die Zeit, wo der F\u00fcrst das Talent auch mit Orden versiehet,", "tokens": ["Gl\u00fcck\u00b7lich", "die", "Zeit", ",", "wo", "der", "F\u00fcrst", "das", "Ta\u00b7lent", "auch", "mit", "Or\u00b7den", "ver\u00b7sie\u00b7het", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "$,", "PWAV", "ART", "NN", "ART", "NN", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "+--+--+-+-+-+--+-", "measure": "dactylic.di.plus"}, "line.20": {"text": "Da\u00df es nicht b\u00f6slich verstimmt nur dem Wahne des P\u00f6bels sich hingibt", "tokens": ["Da\u00df", "es", "nicht", "b\u00f6s\u00b7lich", "ver\u00b7stimmt", "nur", "dem", "Wah\u00b7ne", "des", "P\u00f6\u00b7bels", "sich", "hin\u00b7gibt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PTKNEG", "ADJD", "VVFIN", "ADV", "ART", "NN", "ART", "NN", "PRF", "VVFIN"], "meter": "+--+--+--+--+-+-+", "measure": "dactylic.tetra.plus"}, "line.21": {"text": "Und an den S\u00e4ulen des Staats ehrgeizig r\u00fcttelt und krittelt.", "tokens": ["Und", "an", "den", "S\u00e4u\u00b7len", "des", "Staats", "ehr\u00b7gei\u00b7zig", "r\u00fct\u00b7telt", "und", "krit\u00b7telt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "ADJD", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+--+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.22": {"text": "Schm\u00e4ht nicht die jetzige Zeit, nicht dieses neunzehnte Jahrhundert,", "tokens": ["Schm\u00e4ht", "nicht", "die", "jet\u00b7zi\u00b7ge", "Zeit", ",", "nicht", "die\u00b7ses", "neun\u00b7zehn\u00b7te", "Jahr\u00b7hun\u00b7dert", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ART", "ADJA", "NN", "$,", "PTKNEG", "PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+--+--+-", "measure": "trochaic.septa.relaxed"}, "line.23": {"text": "Denn wo gab einst es, wie heut', Leihbibliotheken in Krautheim", "tokens": ["Denn", "wo", "gab", "einst", "es", ",", "wie", "heut'", ",", "Leih\u00b7bib\u00b7lio\u00b7the\u00b7ken", "in", "Kraut\u00b7heim"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PWAV", "VVFIN", "ADV", "PPER", "$,", "PWAV", "ADV", "$,", "NN", "APPR", "NN"], "meter": "-+-+-+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.24": {"text": "Oder in anderen Nestern, wodurch sich die Geister entwickeln,", "tokens": ["O\u00b7der", "in", "an\u00b7de\u00b7ren", "Nes\u00b7tern", ",", "wo\u00b7durch", "sich", "die", "Geis\u00b7ter", "ent\u00b7wi\u00b7ckeln", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "$,", "PWAV", "PRF", "ART", "NN", "VVINF", "$,"], "meter": "+--+--+-+-+-+--+-", "measure": "dactylic.di.plus"}, "line.25": {"text": "Da\u00df selbst das \u00e4rmste Genie schon als Kind nimmer braucht zu ersticken", "tokens": ["Da\u00df", "selbst", "das", "\u00e4rms\u00b7te", "Ge\u00b7nie", "schon", "als", "Kind", "nim\u00b7mer", "braucht", "zu", "er\u00b7sti\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "ADV", "KOUS", "NN", "ADV", "VVFIN", "PTKZU", "VVINF"], "meter": "-+-+--+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.26": {"text": "Und die mi\u00dfbildetste Frau durchaus nie als Hexe verbrannt wird.", "tokens": ["Und", "die", "mi\u00df\u00b7bil\u00b7dets\u00b7te", "Frau", "durc\u00b7haus", "nie", "als", "He\u00b7xe", "ver\u00b7brannt", "wird", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "ADV", "ADV", "KOUS", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+--+--+-+--+-", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Mitternacht war es vordem, jetzt ist es so ziemlich Mittagszeit,", "tokens": ["Mit\u00b7ter\u00b7nacht", "war", "es", "vor\u00b7dem", ",", "jetzt", "ist", "es", "so", "ziem\u00b7lich", "Mit\u00b7tags\u