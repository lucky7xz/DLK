{"textgrid.poem.33322": {"metadata": {"author": {"name": "Blumauer, Aloys", "birth": "N.A.", "death": "N.A."}, "title": "Der reiche Mann", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer immer hier auf dieser Welt", "tokens": ["Wer", "im\u00b7mer", "hier", "auf", "die\u00b7ser", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "ADV", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu faul zur Arbeit ist,", "tokens": ["Zu", "faul", "zur", "Ar\u00b7beit", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "APPRART", "NN", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und thun nur will, was ihm gef\u00e4llt,", "tokens": ["Und", "thun", "nur", "will", ",", "was", "ihm", "ge\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "VMFIN", "$,", "PWS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und Andere verdrie\u00dft;", "tokens": ["Und", "An\u00b7de\u00b7re", "ver\u00b7drie\u00dft", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der werde reich; ein reicher Mann", "tokens": ["Der", "wer\u00b7de", "reich", ";", "ein", "rei\u00b7cher", "Mann"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ADJD", "$.", "ART", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Darf alles, was er will und kann!", "tokens": ["Darf", "al\u00b7les", ",", "was", "er", "will", "und", "kann", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "$,", "PWS", "PPER", "VMFIN", "KON", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Er spricht in der gelehrten Welt", "tokens": ["Er", "spricht", "in", "der", "ge\u00b7lehr\u00b7ten", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Wissenschaften Hohn,", "tokens": ["Den", "Wis\u00b7sen\u00b7schaf\u00b7ten", "Hohn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und kauft sich f\u00fcr sein baares Geld,", "tokens": ["Und", "kauft", "sich", "f\u00fcr", "sein", "baa\u00b7res", "Geld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So viel er braucht, davon;", "tokens": ["So", "viel", "er", "braucht", ",", "da\u00b7von", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "VVFIN", "$,", "PAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Denn nur der Reiche kann allein", "tokens": ["Denn", "nur", "der", "Rei\u00b7che", "kann", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NE", "VMFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit guter Art ein Dummkopf sein!", "tokens": ["Mit", "gu\u00b7ter", "Art", "ein", "Dumm\u00b7kopf", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Den Wohlstand und die H\u00f6flichkeit,", "tokens": ["Den", "Wohl\u00b7stand", "und", "die", "H\u00f6f\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die \u00fcberl\u00e4\u00dft er ungescheut", "tokens": ["Die", "\u00fc\u00b7berl\u00b7\u00e4\u00dft", "er", "un\u00b7ge\u00b7scheut"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nur seinem Leiblakei;", "tokens": ["Nur", "sei\u00b7nem", "Leib\u00b7la\u00b7kei", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Denn nur der Reiche kann allein", "tokens": ["Denn", "nur", "der", "Rei\u00b7che", "kann", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NE", "VMFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein Grobian mit Ehren sein.", "tokens": ["Ein", "Gro\u00b7bi\u00b7an", "mit", "Eh\u00b7ren", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Er sieht der Menschen Arbeit zu,", "tokens": ["Er", "sieht", "der", "Men\u00b7schen", "Ar\u00b7beit", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und nennt es Spielerei,", "tokens": ["Und", "nennt", "es", "Spie\u00b7le\u00b7rei", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dehnt auf dem Sopha sich in Ruh,", "tokens": ["Dehnt", "auf", "dem", "So\u00b7pha", "sich", "in", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PRF", "APPR", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Und g\u00e4hnt und schnarcht dabei;", "tokens": ["Und", "g\u00e4hnt", "und", "schnarcht", "da\u00b7bei", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "PAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Denn nur der reiche Mann allein", "tokens": ["Denn", "nur", "der", "rei\u00b7che", "Mann", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Darf ungest\u00f6rt ein Tagdieb sein.", "tokens": ["Darf", "un\u00b7ge\u00b7st\u00f6rt", "ein", "Tag\u00b7dieb", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Er spottet der Religion,", "tokens": ["Er", "spot\u00b7tet", "der", "Re\u00b7li\u00b7gi\u00b7on", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Hei\u00dft nur den P\u00f6bel fromm,", "tokens": ["Hei\u00dft", "nur", "den", "P\u00f6\u00b7bel", "fromm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "ADJD", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Und kauft f\u00fcr eine Million", "tokens": ["Und", "kauft", "f\u00fcr", "ei\u00b7ne", "Mil\u00b7li\u00b7on"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sich einen Schein zu Rom;", "tokens": ["Sich", "ei\u00b7nen", "Schein", "zu", "Rom", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "APPR", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Denn nur der reiche Mann allein", "tokens": ["Denn", "nur", "der", "rei\u00b7che", "Mann", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Darf f\u00fcr sein Geld ein Freigeist sein.", "tokens": ["Darf", "f\u00fcr", "sein", "Geld", "ein", "Frei\u00b7geist", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "PPOSAT", "NN", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Er macht aus Schuldnerthr\u00e4nen Gold,", "tokens": ["Er", "macht", "aus", "Schuld\u00b7ner\u00b7thr\u00e4\u00b7nen", "Gold", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Raubt and'rer Leute Gut,", "tokens": ["Raubt", "an\u00b7d'\u00b7rer", "Leu\u00b7te", "Gut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "ADJD", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "H\u00e4lt die Gerechtigkeit im Sold,", "tokens": ["H\u00e4lt", "die", "Ge\u00b7rech\u00b7tig\u00b7keit", "im", "Sold", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Die nur, was er will, thut;", "tokens": ["Die", "nur", ",", "was", "er", "will", ",", "thut", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADV", "$,", "PWS", "PPER", "VMFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Denn nur der Reiche darf allein", "tokens": ["Denn", "nur", "der", "Rei\u00b7che", "darf", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NE", "VMFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein Schurke von Rechtswegen sein.", "tokens": ["Ein", "Schur\u00b7ke", "von", "Rechts\u00b7we\u00b7gen", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VAINF", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.7": {"line.1": {"text": "Und hat er seinen Lebenslauf", "tokens": ["Und", "hat", "er", "sei\u00b7nen", "Le\u00b7bens\u00b7lauf"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In M\u00fcssiggang vollbracht,", "tokens": ["In", "M\u00fcs\u00b7sig\u00b7gang", "voll\u00b7bracht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So nimmt er einen Dichter auf,", "tokens": ["So", "nimmt", "er", "ei\u00b7nen", "Dich\u00b7ter", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der ihn unsterblich macht;", "tokens": ["Der", "ihn", "uns\u00b7terb\u00b7lich", "macht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Denn nur der reiche Mann allein", "tokens": ["Denn", "nur", "der", "rei\u00b7che", "Mann", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Kann ohne Ruhm unsterblich sein.", "tokens": ["Kann", "oh\u00b7ne", "Ruhm", "uns\u00b7terb\u00b7lich", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "NN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Nur ein Gut ist, das in der Welt", "tokens": ["Nur", "ein", "Gut", "ist", ",", "das", "in", "der", "Welt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VAFIN", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Der Bettler oft genie\u00dft,", "tokens": ["Der", "Bett\u00b7ler", "oft", "ge\u00b7nie\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und das bei allem seinen Geld", "tokens": ["Und", "das", "bei", "al\u00b7lem", "sei\u00b7nen", "Geld"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PDS", "APPR", "PIS", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der reiche Mann vermi\u00dft:", "tokens": ["Der", "rei\u00b7che", "Mann", "ver\u00b7mi\u00dft", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mit sich zufrieden kann allein", "tokens": ["Mit", "sich", "zu\u00b7frie\u00b7den", "kann", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PRF", "ADJD", "VMFIN", "ADV"],