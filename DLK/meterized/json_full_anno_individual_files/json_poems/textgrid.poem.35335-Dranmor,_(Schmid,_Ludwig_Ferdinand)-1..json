{"textgrid.poem.35335": {"metadata": {"author": {"name": "Dranmor, (Schmid, Ludwig Ferdinand)", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Verg\u00e4nglich ist die Menschheit und, dem Staube", "tokens": ["Ver\u00b7g\u00e4ng\u00b7lich", "ist", "die", "Menschheit", "und", ",", "dem", "Stau\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "VAFIN", "ART", "NN", "KON", "$,", "ART", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "M\u00fchsam entwachsen, unrettbar verfallen", "tokens": ["M\u00fch\u00b7sam", "ent\u00b7wach\u00b7sen", ",", "un\u00b7rett\u00b7bar", "ver\u00b7fal\u00b7len"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NE", "VVPP", "$,", "ADJD", "VVINF"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Dem grauen Chaos. Keinen von uns allen", "tokens": ["Dem", "grau\u00b7en", "Chaos", ".", "Kei\u00b7nen", "von", "uns", "al\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "PIAT", "APPR", "PPER", "PIAT"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Befreit vom Erdenlos sein Himmelsglaube.", "tokens": ["Be\u00b7freit", "vom", "Er\u00b7den\u00b7los", "sein", "Him\u00b7mels\u00b7glau\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Arm ist die Menschheit; jeder lebt vom Raube", "tokens": ["Arm", "ist", "die", "Menschheit", ";", "je\u00b7der", "lebt", "vom", "Rau\u00b7be"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "ART", "NN", "$.", "PIS", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Und von Geschenken aus des Todes Krallen;", "tokens": ["Und", "von", "Ge\u00b7schen\u00b7ken", "aus", "des", "To\u00b7des", "Kral\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und was beherrscht Despoten wie Vasallen?", "tokens": ["Und", "was", "be\u00b7herrscht", "Des\u00b7po\u00b7ten", "wie", "Va\u00b7sal\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVFIN", "NN", "KOKOM", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die liebe S\u00fcnde nur, die blinde, taube.", "tokens": ["Die", "lie\u00b7be", "S\u00fcn\u00b7de", "nur", ",", "die", "blin\u00b7de", ",", "tau\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "$,", "ART", "ADJA", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Des Fleisches Wut, des Denkens finstre Macht,", "tokens": ["Des", "Flei\u00b7sches", "Wut", ",", "des", "Den\u00b7kens", "finst\u00b7re", "Macht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "O Papst! verbr\u00fcdern Sklaven mit C\u00e4saren", "tokens": ["O", "Papst", "!", "ver\u00b7br\u00fc\u00b7dern", "Skla\u00b7ven", "mit", "C\u00e4\u00b7sa\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NN", "$.", "ADJA", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure"