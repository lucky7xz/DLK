{"dta.poem.16295": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "31.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Geist ist als gesund und krank auch zu betrachten", "tokens": ["Der", "Geist", "ist", "als", "ge\u00b7sund", "und", "krank", "auch", "zu", "be\u00b7trach\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "KOKOM", "ADJD", "KON", "ADJD", "ADV", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Alswie der Leib; gesund ist \u00fcber krank zu achten.", "tokens": ["Als\u00b7wie", "der", "Leib", ";", "ge\u00b7sund", "ist", "\u00fc\u00b7ber", "krank", "zu", "ach\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$.", "VVPP", "VAFIN", "APPR", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wer nur das Gute thut, damit er B\u00f6ses meide,", "tokens": ["Wer", "nur", "das", "Gu\u00b7te", "thut", ",", "da\u00b7mit", "er", "B\u00f6\u00b7ses", "mei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "NN", "VVFIN", "$,", "KOUS", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ist krank, und werth da\u00df er, um zu genesen, leide;", "tokens": ["Ist", "krank", ",", "und", "werth", "da\u00df", "er", ",", "um", "zu", "ge\u00b7ne\u00b7sen", ",", "lei\u00b7de", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "ADJD", "$,", "KON", "ADJD", "KOUS", "PPER", "$,", "KOUI", "PTKZU", "VVINF", "$,", "VVFIN", "$."], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Dem leiblich-kranken gleich, der bittre Arzeneien", "tokens": ["Dem", "leib\u00b7lich\u00b7kran\u00b7ken", "gleich", ",", "der", "bitt\u00b7re", "Ar\u00b7ze\u00b7nei\u00b7en"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit Unlust nimmt, um sich vom Uebel zu befreien.", "tokens": ["Mit", "Un\u00b7lust", "nimmt", ",", "um", "sich", "vom", "Ue\u00b7bel", "zu", "be\u00b7frei\u00b7en", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$,", "KOUI", "PRF", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Doch ein Gesunder i\u00dft und trinkt, was ihm behagt,", "tokens": ["Doch", "ein", "Ge\u00b7sun\u00b7der", "i\u00dft", "und", "trinkt", ",", "was", "ihm", "be\u00b7hagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "KON", "VVFIN", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und ist gesund nicht weil er etwas sich versagt.", "tokens": ["Und", "ist", "ge\u00b7sund", "nicht", "weil", "er", "et\u00b7was", "sich", "ver\u00b7sagt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADJD", "PTKNEG", "KOUS", "PPER", "PIS", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "So der gesunde Geist thut was er will, und thut", "tokens": ["So", "der", "ge\u00b7sun\u00b7de", "Geist", "thut", "was", "er", "will", ",", "und", "thut"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "VVFIN", "PWS", "PPER", "VMFIN", "$,", "KON", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Deswegen B\u00f6ses nicht; denn was er will ist gut.", "tokens": ["Des\u00b7we\u00b7gen", "B\u00f6\u00b7ses", "nicht", ";", "denn", "was", "er", "will", "ist", "gut", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "NN", "PTKNEG", "$.", "KON", "PWS", "PPER", "VMFIN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}