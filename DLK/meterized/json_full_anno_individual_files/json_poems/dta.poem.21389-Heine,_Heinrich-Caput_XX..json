{"dta.poem.21389": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Caput XX.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-30602-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Von Harburg fuhr ich in einer Stund\u2019", "tokens": ["Von", "Har\u00b7burg", "fuhr", "ich", "in", "ei\u00b7ner", "Stund'"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Nach Hamburg. Es war schon Abend.", "tokens": ["Nach", "Ham\u00b7burg", ".", "Es", "war", "schon", "A\u00b7bend", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$.", "PPER", "VAFIN", "ADV", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die Sterne am Himmel gr\u00fc\u00dften mich,", "tokens": ["Die", "Ster\u00b7ne", "am", "Him\u00b7mel", "gr\u00fc\u00df\u00b7ten", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "PPER", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die Luft war lind und labend.", "tokens": ["Die", "Luft", "war", "lind", "und", "la\u00b7bend", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}