{"textgrid.poem.66766": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie das wilde Meer", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie das wilde Meer", "tokens": ["Wie", "das", "wil\u00b7de", "Meer"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": " \u00fcber die Bl\u00f6cke brandet!", "tokens": ["\u00fc\u00b7ber", "die", "Bl\u00f6\u00b7cke", "bran\u00b7det", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Doch ich warf mich hierher,", "tokens": ["Doch", "ich", "warf", "mich", "hier\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PRF", "PAV", "$,"], "meter": "-++--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "atemlos bin ich gelandet.", "tokens": ["a\u00b7tem\u00b7los", "bin", "ich", "ge\u00b7lan\u00b7det", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Soll's aufstrudelnd mich ziehn", "tokens": ["Soll's", "auf\u00b7stru\u00b7delnd", "mich", "ziehn"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVPP", "PPER", "VVINF"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.6": {"text": "abw\u00e4rts mit gierigen Krallen?", "tokens": ["ab\u00b7w\u00e4rts", "mit", "gie\u00b7ri\u00b7gen", "Kral\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.7": {"text": "Weltmeer, nicht will ich dich fliehn,", "tokens":