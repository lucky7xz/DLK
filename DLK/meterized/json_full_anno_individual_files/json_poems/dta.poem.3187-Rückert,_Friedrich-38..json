{"dta.poem.3187": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "38.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Seht, wann die Sonn' aufsteht, bis wann sie untergeht,", "tokens": ["Seht", ",", "wann", "die", "Sonn'", "auf\u00b7steht", ",", "bis", "wann", "sie", "un\u00b7ter\u00b7geht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "ART", "NN", "VVFIN", "$,", "KON", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie sich von Berg und Baum umher der Schatten dreht.", "tokens": ["Wie", "sich", "von", "Berg", "und", "Baum", "um\u00b7her", "der", "Schat\u00b7ten", "dreht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "APPR", "NN", "KON", "NE", "PTKVZ", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Weil ihre Starrheit nicht will Niederfall gestatten,", "tokens": ["Weil", "ih\u00b7re", "Star\u00b7rheit", "nicht", "will", "Nie\u00b7der\u00b7fall", "ge\u00b7stat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PTKNEG", "VMFIN", "NE", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Anbetend werfen sie zur Erde doch den Schatten.", "tokens": ["An\u00b7be\u00b7tend", "wer\u00b7fen", "sie", "zur", "Er\u00b7de", "doch", "den", "Schat\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPRART", "NN", "ADV", "ART", "NN", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}}, "stanza.3": {"line.1": {"text": "Wenn nicht wie Berg und Baum ihr starr seid, werfet nieder,", "tokens": ["Wenn", "nicht", "wie", "Berg", "und", "Baum", "ihr", "starr", "seid", ",", "wer\u00b7fet", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "KOKOM", "NN", "KON", "NE", "PPER", "ADJD", "VAFIN", "$,", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie ihren Schatten sie, anbetend, eure Glieder!", "tokens": ["Wie", "ih\u00b7ren", "Schat\u00b7ten", "sie", ",", "an\u00b7be\u00b7tend", ",", "eu\u00b7re", "Glie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "PPER", "$,", "VVPP", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}}}}}