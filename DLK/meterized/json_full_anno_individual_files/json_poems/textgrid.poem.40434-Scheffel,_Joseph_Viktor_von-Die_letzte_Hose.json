{"textgrid.poem.40434": {"metadata": {"author": {"name": "Scheffel, Joseph Viktor von", "birth": "N.A.", "death": "N.A."}, "title": "Die letzte Hose", "genre": "verse", "period": "N.A.", "pub_year": 1856, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Letzte Hose, die mich schm\u00fcckte,", "tokens": ["Letz\u00b7te", "Ho\u00b7se", ",", "die", "mich", "schm\u00fcck\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Fahre wohl! dein Amt ist aus,", "tokens": ["Fah\u00b7re", "wohl", "!", "dein", "Amt", "ist", "aus", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$.", "PPOSAT", "NN", "VAFIN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach auch dich, die mich entz\u00fcckte,", "tokens": ["Ach", "auch", "dich", ",", "die", "mich", "ent\u00b7z\u00fcck\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "PPER", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schleppt ein andrer nun nach Haus.", "tokens": ["Schleppt", "ein", "an\u00b7drer", "nun", "nach", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "ADV", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Selten hat an solchen Paares", "tokens": ["Sel\u00b7ten", "hat", "an", "sol\u00b7chen", "Paa\u00b7res"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Anblick sich ein Aug' erquickt;", "tokens": ["An\u00b7blick", "sich", "ein", "Aug'", "er\u00b7quickt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "ART", "NN", "VVPP", "$."], "meter": "+---+-+", "measure": "dactylic.init"}, "line.3": {"text": "Feinster Winterbuckskin war es,", "tokens": ["Feins\u00b7ter", "Win\u00b7ter\u00b7bucks\u00b7kin", "war", "es", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VAFIN", "PPER", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gro\u00df kariert \u2013 und nie geflickt!", "tokens": ["Gro\u00df", "ka\u00b7riert", "\u2013", "und", "nie", "ge\u00b7flickt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$(", "KON", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Mit Gesang und vollen Flaschen", "tokens": ["Mit", "Ge\u00b7sang", "und", "vol\u00b7len", "Fla\u00b7schen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gr\u00fc\u00dft' ich einst in dir die Welt;", "tokens": ["Gr\u00fc\u00dft'", "ich", "einst", "in", "dir", "die", "Welt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Zum Hausschl\u00fcssel in der Taschen", "tokens": ["Zum", "Haus\u00b7schl\u00fcs\u00b7sel", "in", "der", "Ta\u00b7schen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPR", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Klang noch froh das bare Geld.", "tokens": ["Klang", "noch", "froh", "das", "ba\u00b7re", "Geld", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Aber l\u00e4ngst kam das Verh\u00e4ngnis,", "tokens": ["A\u00b7ber", "l\u00e4ngst", "kam", "das", "Ver\u00b7h\u00e4ng\u00b7nis", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die Sechsb\u00e4tzner zogen fort,", "tokens": ["Die", "Sechs\u00b7b\u00e4tz\u00b7ner", "zo\u00b7gen", "fort", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Und das Br\u00fcckentorgef\u00e4ngnis", "tokens": ["Und", "das", "Br\u00fc\u00b7cken\u00b7tor\u00b7ge\u00b7f\u00e4ng\u00b7nis"], "token_info": ["word", "word", "word"], "pos": ["KON", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist ein dunkler stiller Ort ...", "tokens": ["Ist", "ein", "dunk\u00b7ler", "stil\u00b7ler", "Ort", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "L\u00e4ngst verschwand, was sonst versetzlich,", "tokens": ["L\u00e4ngst", "ver\u00b7schwand", ",", "was", "sonst", "ver\u00b7setz\u00b7lich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PRELS", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Frack \u2013 und Rock \u2013 und Mantels Pracht.", "tokens": ["Frack", "\u2013", "und", "Rock", "\u2013", "und", "Man\u00b7tels", "Pracht", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$(", "KON", "NN", "$(", "KON", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nun auch du! es ist entsetzlich!...", "tokens": ["Nun", "auch", "du", "!", "es", "ist", "ent\u00b7setz\u00b7lich", "!", "..."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADV", "PPER", "$.", "PPER", "VAFIN", "ADJD", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Letzte Hose, gute Nacht!", "tokens": ["Letz\u00b7te", "Ho\u00b7se", ",", "gu\u00b7te", "Nacht", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Tag der Pr\u00fcfung, o wie b\u00e4nglich", "tokens": ["Tag", "der", "Pr\u00fc\u00b7fung", ",", "o", "wie", "b\u00e4ng\u00b7lich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ART", "NN", "$,", "FM", "KOKOM", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schl\u00e4gt mein Herz und f\u00fchlt es hell:", "tokens": ["Schl\u00e4gt", "mein", "Herz", "und", "f\u00fchlt", "es", "hell", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "KON", "VVFIN", "PPER", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Alles Irdische ist verg\u00e4nglich", "tokens": ["Al\u00b7les", "Ir\u00b7di\u00b7sche", "ist", "ver\u00b7g\u00e4ng\u00b7lich"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "NN", "VAFIN", "ADJD"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Und das Pfandrecht schreitet schnell!", "tokens": ["Und", "das", "Pfand\u00b7recht", "schrei\u00b7tet", "schnell", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Nirgend winkt uns ein Erl\u00f6ser,", "tokens": ["Nir\u00b7gend", "winkt", "uns", "ein", "Er\u00b7l\u00f6\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Letzte Hose!... es mu\u00df sein!...", "tokens": ["Letz\u00b7te", "Ho\u00b7se", "!", "...", "es", "mu\u00df", "sein", "!", "..."], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ADJA", "NN", "$.", "$(", "PPER", "VMFIN", "VAINF", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Elkan Levi, dunkler, b\u00f6ser", "tokens": ["El\u00b7kan", "Le\u00b7vi", ",", "dunk\u00b7ler", ",", "b\u00f6\u00b7ser"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["NE", "NE", "$,", "ADJA", "$,", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Tr\u00f6dler, nimm sie!... Sie sei dein!", "tokens": ["Tr\u00f6d\u00b7ler", ",", "nimm", "sie", "!", "...", "Sie", "sei", "dein", "!"], "token_info": ["word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVIMP", "PPER", "$.", "$(", "PPER", "VAFIN", "PPOSAT", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Stiefelfuchs, du alter treuer,", "tokens": ["Stie\u00b7fel\u00b7fuchs", ",", "du", "al\u00b7ter", "treu\u00b7er", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "ADJA", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Komm und st\u00fctz' mein Dulderhaupt!", "tokens": ["Komm", "und", "st\u00fctz'", "mein", "Dul\u00b7der\u00b7haupt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Noch ein einziger Schoppen Neuer", "tokens": ["Noch", "ein", "ein\u00b7zi\u00b7ger", "Schop\u00b7pen", "Neu\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sei dem Trauernden erlaubt.", "tokens": ["Sei", "dem", "Trau\u00b7ern\u00b7den", "er\u00b7laubt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Dann will ich zu Bett mich legen", "tokens": ["Dann", "will", "ich", "zu", "Bett", "mich", "le\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "NN", "PPER", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und nicht aufstehn, wenn's auch klopft,", "tokens": ["Und", "nicht", "auf\u00b7stehn", ",", "wenn's", "auch", "klopft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "VVINF", "$,", "KOUS", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bis ein schwerer goldner Regen", "tokens": ["Bis", "ein", "schwe\u00b7rer", "gold\u00b7ner", "Re\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Unverhofft durchs Dach mir tropft.", "tokens": ["Un\u00b7ver\u00b7hofft", "durchs", "Dach", "mir", "tropft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Zeuch denn hin, die ich beweine,", "tokens": ["Zeuch", "denn", "hin", ",", "die", "ich", "be\u00b7wei\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "KON", "PTKVZ", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gr\u00fc\u00df den Rock und 's Kamisol!", "tokens": ["Gr\u00fc\u00df", "den", "Rock", "und", "'s", "Ka\u00b7mi\u00b7sol", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "KON", "PPER", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Weh! schon friert's mich an die Beine!...", "tokens": ["Weh", "!", "schon", "friert's", "mich", "an", "die", "Bei\u00b7ne", "!", "..."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "$.", "ADV", "VVFIN", "PRF", "APPR", "ART", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Letzte Hose, fahre wohl!!", "tokens": ["Letz\u00b7te", "Ho\u00b7se", ",", "fah\u00b7re", "wohl", "!!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Letzte Hose, die mich schm\u00fcckte,", "tokens": ["Letz\u00b7te", "Ho\u00b7se", ",", "die", "mich", "schm\u00fcck\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Fahre wohl! dein Amt ist aus,", "tokens": ["Fah\u00b7re", "wohl", "!", "dein", "Amt", "ist", "aus", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$.", "PPOSAT", "NN", "VAFIN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach auch dich, die mich entz\u00fcckte,", "tokens": ["Ach", "auch", "dich", ",", "die", "mich", "ent\u00b7z\u00f