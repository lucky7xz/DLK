{"textgrid.poem.54429": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "Trawrklage des Autoris/ in sehr schwerer Kranckheit", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich bin nicht/ der ich war/ Die kr\u00e4ffte sind verschwunden!", "tokens": ["Ich", "bin", "nicht", "/", "der", "ich", "war", "/", "Die", "kr\u00e4ff\u00b7te", "sind", "ver\u00b7schwun\u00b7den", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "$(", "PRELS", "PPER", "VAFIN", "$(", "ART", "ADJA", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Glieder sind verdorrt wie ein verbrandter Grau\u00df/", "tokens": ["Die", "Glie\u00b7der", "sind", "ver\u00b7dorrt", "wie", "ein", "ver\u00b7brand\u00b7ter", "Grau\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "KOKOM", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Hier schawt der schwartze Todt zu beyden Augen au\u00df/", "tokens": ["Hier", "schawt", "der", "schwart\u00b7ze", "Todt", "zu", "bey\u00b7den", "Au\u00b7gen", "au\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "APPR", "PIAT", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nichts wird als Haut v\u00f1 Bein mehr an mir vbrig funden.", "tokens": ["Nichts", "wird", "als", "Haut", "v\u00f1", "Bein", "mehr", "an", "mir", "vb\u00b7rig", "fun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "KOKOM", "NN", "APPR", "NN", "ADV", "APPR", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der Athem wil nicht fort; die Zung steht angebunden.", "tokens": ["Der", "A\u00b7them", "wil", "nicht", "fort", ";", "die", "Zung", "steht", "an\u00b7ge\u00b7bun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PTKNEG", "PTKVZ", "$.", "ART", "NN", "VVFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Mein Hertz das vbersteht numehr den letzten Strau\u00df/", "tokens": ["Mein", "Hertz", "das", "vbers\u00b7teht", "nu\u00b7mehr", "den", "letz\u00b7ten", "Strau\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PDS", "VVFIN", "ADV", "ART", "ADJA", "NE", "$("], "meter": "-+-+----+-+", "measure": "unknown.measure.tetra"}, "line.7": {"text": "Ein jeder/ der mich siht sp\u00fcrt da\u00df das schwache Hau\u00df", "tokens": ["Ein", "je\u00b7der", "/", "der", "mich", "siht", "sp\u00fcrt", "da\u00df", "das", "schwa\u00b7che", "Hau\u00df"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PIAT", "$(", "PRELS", "PPER", "VVFIN", "VVFIN", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Der Leib wird brechen ein/ gar jnner wenig Stunden/", "tokens": ["Der", "Leib", "wird", "bre\u00b7chen", "ein", "/", "gar", "jn\u00b7ner", "we\u00b7nig", "Stun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVINF", "ART", "$(", "ADV", "ADV", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Gleich wie die Wiesenblum fr\u00fch mit dem Liecht der Welt", "tokens": ["Gleich", "wie", "die", "Wie\u00b7sen\u00b7blum", "fr\u00fch", "mit", "dem", "Liecht", "der", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "ART", "NE", "ADJD", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Hervor kombt/ vnnd noch eh der Mittag weggeht/ f\u00e4lt;", "tokens": ["Her\u00b7vor", "kombt", "/", "vnnd", "noch", "eh", "der", "Mit\u00b7tag", "weg\u00b7geht", "/", "f\u00e4lt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "$(", "KON", "ADV", "KOUS", "ART", "NN", "VVFIN", "$(", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "So bin ich auch benetzt mit Thr\u00e4nentaw ankommen:", "tokens": ["So", "bin", "ich", "auch", "be\u00b7netzt", "mit", "Thr\u00e4\u00b7nen\u00b7taw", "an\u00b7kom\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "VVPP", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "So sterb ich vor der Zeit: O Erden gutte Nacht!", "tokens": ["So", "sterb", "ich", "vor", "der", "Zeit", ":", "O", "Er\u00b7den", "gut\u00b7te", "Nacht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$.", "NE", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Mein St\u00fcndlein laufft herbey! nun hab ich au\u00dfgewacht/", "tokens": ["Mein", "St\u00fcnd\u00b7lein", "laufft", "her\u00b7bey", "!", "nun", "hab", "ich", "au\u00df\u00b7ge\u00b7wacht", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKVZ", "$.", "ADV", "VAFIN", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Vnd werde von dem Schlaff des Todes eingenommen!", "tokens": ["Vnd", "wer\u00b7de", "von", "dem", "Schlaff", "des", "To\u00b7des", "ein\u00b7ge\u00b7nom\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ich bin nicht/ der ich war/ Die kr\u00e4ffte sind verschwunden!", "tokens": ["Ich", "bin", "nicht", "/", "der", "ich", "war", "/", "Die", "kr\u00e4ff\u00b7te", "sind", "ver\u00b7schwun\u00b7den", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "$(", "PRELS", "PPER", "VAFIN", "$(", "ART", "ADJA", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Glieder sind verdorrt wie ein verbrandter Grau\u00df/", "tokens": ["Die", "Glie\u00b7der", "sind", "ver\u00b7dorrt", "wie", "ein", "ver\u00b7brand\u00b7ter", "Grau\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "KOKOM", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Hier schawt der schwartze Todt zu beyden Augen au\u00df/", "tokens": ["Hier", "schawt", "der", "schwart\u00b7ze", "Todt", "zu", "bey\u00b7den", "Au\u00b7gen", "au\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "AD