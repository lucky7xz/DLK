{"dta.poem.344": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Sie liebet ihn.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Hertze/ das liebt dein sch\u00f6nes Gesicht/", "tokens": ["Mein", "Hert\u00b7ze", "/", "das", "liebt", "dein", "sch\u00f6\u00b7nes", "Ge\u00b7sicht", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVFIN", "$(", "PDS", "VVFIN", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und meine Gedancken sind auf dich gericht.", "tokens": ["Und", "mei\u00b7ne", "Ge\u00b7dan\u00b7cken", "sind", "auf", "dich", "ge\u00b7richt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "APPR", "PPER", "VVPP", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Mich lassen die Augen", "tokens": ["Mich", "las\u00b7sen", "die", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Den Liebes-Brand saugen.", "tokens": ["Den", "Lie\u00b7bes\u00b7Brand", "sau\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.5": {"text": "Du bist mir mein Licht/", "tokens": ["Du", "bist", "mir", "mein", "Licht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PPOSAT", "NN", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Du bist auch recht sch\u00f6n/", "tokens": ["Du", "bist", "auch", "recht", "sch\u00f6n", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ADJD", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Die Wangen die gleichen", "tokens": ["Die", "Wan\u00b7gen", "die", "glei\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "ADJA"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Den Rosen am Streuchen/", "tokens": ["Den", "Ro\u00b7sen", "am", "Streu\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Wenn sie sich auffbl\u00e4hn", "tokens": ["Wenn", "sie", "sich", "auff\u00b7bl\u00e4hn"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "VVINF"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.10": {"text": "Ich ehre dich sehr/", "tokens": ["Ich", "eh\u00b7re", "dich", "sehr", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.11": {"text": "Und lieb dich noch mehr;", "tokens": ["Und", "lieb", "dich", "noch", "mehr", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PPER", "ADV", "ADV", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.12": {"text": "Ich k\u00fcsse die Banden", "tokens": ["Ich", "k\u00fcs\u00b7se", "die", "Ban\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.13": {"text": "Die in mir verhanden", "tokens": ["Die", "in", "mir", "ver\u00b7han\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "PPER", "ADJD"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.14": {"text": "Als meinen Gewinn.", "tokens": ["Als", "mei\u00b7nen", "Ge\u00b7winn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.15": {"text": "Ich will mich nicht retten", "tokens": ["Ich", "will", "mich", "nicht", "ret\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.16": {"text": "Von Banden und Ketten", "tokens": ["Von", "Ban\u00b7den", "und", "Ket\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.17": {"text": "Die Freyheit ist hin.", "tokens": ["Die", "Frey\u00b7heit", "ist", "hin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Dir bin ich getreu/ ich gebe mein Hertz", "tokens": ["Dir", "bin", "ich", "ge\u00b7treu", "/", "ich", "ge\u00b7be", "mein", "Hertz"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "$(", "PPER", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Dir/ Sch\u00f6nster/ zu eigen/ komm linder den Schmertz/", "tokens": ["Dir", "/", "Sch\u00f6ns\u00b7ter", "/", "zu", "ei\u00b7gen", "/", "komm", "lin\u00b7der", "den", "Schmertz", "/"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$(", "NN", "$(", "PTKA", "ADJD", "$(", "VVFIN", "ADV", "ART", "NN", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Und mache durch Lieben", "tokens": ["Und", "ma\u00b7che", "durch", "Lie\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Mein bitters Betr\u00fcben", "tokens": ["Mein", "bit\u00b7ters", "Be\u00b7tr\u00fc\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Zum lieblichen Schertz.", "tokens": ["Zum", "lieb\u00b7li\u00b7chen", "Schertz", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Umfange die Brust", "tokens": ["Um\u00b7fan\u00b7ge", "die", "Brust"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Erg\u00f6tze die Seele", "tokens": ["Er\u00b7g\u00f6t\u00b7ze", "die", "See\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "In K\u00fcssen mit Oele", "tokens": ["In", "K\u00fcs\u00b7sen", "mit", "O\u00b7e\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Der gr\u00f6ssesten Lust", "tokens": ["Der", "gr\u00f6s\u00b7ses\u00b7ten", "Lust"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.10": {"text": "Erkenne die Brunst/", "tokens": ["Er\u00b7ken\u00b7ne", "die", "Brunst", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.11": {"text": "Und reiche mir Gunst/", "tokens": ["Und", "rei\u00b7che", "mir", "Gunst", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "NN", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.12": {"text": "Belohne die Liebe", "tokens": ["Be\u00b7loh\u00b7ne", "die", "Lie\u00b7be"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.13": {"text": "Durch liebliche Triebe", "tokens": ["Durch", "lieb\u00b7li\u00b7che", "Trie\u00b7be"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.14": {"text": "Und g\u00fctigen Blick.", "tokens": ["Und", "g\u00fc\u00b7ti\u00b7gen", "Blick", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.15": {"text": "Vermische die Flammen", "tokens": ["Ver\u00b7mi\u00b7sche", "die", "Flam\u00b7men"], "token_info": ["word", "word", "word"], "pos": ["NN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.16": {"text": "Mit deinen zusammen", "tokens": ["Mit", "dei\u00b7nen", "zu\u00b7sam\u00b7men"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.17": {"text": "Und kr\u00f6hne mein Gl\u00fcck.", "tokens": ["Und", "kr\u00f6h\u00b7ne", "mein", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Ach eile mein Schatz/ umfange die Brust/", "tokens": ["Ach", "ei\u00b7le", "mein", "Schatz", "/", "um\u00b7fan\u00b7ge", "die", "Brust", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "PPOSAT", "NN", "$(", "VVFIN", "ART", "NN", "$("], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und schencke der Seelen/ vergn\u00fcgliche Lust/", "tokens": ["Und", "schen\u00b7cke", "der", "See\u00b7len", "/", "ver\u00b7gn\u00fcg\u00b7li\u00b7che", "Lust", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$(", "ADJA", "NN", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "La\u00df liebliches Lieben", "tokens": ["La\u00df", "lieb\u00b7li\u00b7ches", "Lie\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["VVIMP", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Mein Liebster uns \u00fcben", "tokens": ["Mein", "Liebs\u00b7ter", "uns", "\u00fc\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "PPER", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Das keinem bewust", "tokens": ["Das", "kei\u00b7nem", "be\u00b7wust"], "token_info": ["word", "word", "word"], "pos": ["ART", "PIAT", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Komm k\u00fcsse den Mund", "tokens": ["Komm", "k\u00fcs\u00b7se", "den", "Mund"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "VVFIN", "ART", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Und mache den Bissen", "tokens": ["Und", "ma\u00b7che", "den", "Bis\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Jm safftigen K\u00fcssen", "tokens": ["Jm", "saff\u00b7ti\u00b7gen", "K\u00fcs\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Der Liebe mir kund/", "tokens": ["Der", "Lie\u00b7be", "mir", "kund", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "PTKVZ", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.10": {"text": "Ich warte mein Schatz", "tokens": ["Ich", "war\u00b7te", "mein", "Schatz"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.11": {"text": "Und mache dir Platz/", "tokens": ["Und", "ma\u00b7che", "dir", "Platz", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "NN", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.12": {"text": "Da\u00df von den Brust Klippen", "tokens": ["Da\u00df", "von", "den", "Brust", "Klip\u00b7pen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "NN"], "meter": "++-++-", "measure": "unknown.measure.tetra"}, "line.13": {"text": "Mit freudigen Lippen", "tokens": ["Mit", "freu\u00b7di\u00b7gen", "Lip\u00b7pen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.14": {"text": "Du kostest ", "tokens": ["Du", "kos\u00b7test"], "token_info": ["word", "word"], "pos": ["PPER", "VVFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.15": {"text": "Komm \u00fcbe das Lieben", "tokens": ["Komm", "\u00fc\u00b7be", "das", "Lie\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "VVFIN", "ART", "ADJA"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.16": {"text": "Das statt des Betr\u00fcben", "tokens": ["Das", "statt", "des", "Be\u00b7tr\u00fc\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "APPR", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.17": {"text": "Erg\u00f6tzung erweckt.", "tokens": ["Er\u00b7g\u00f6t\u00b7zung", "er\u00b7weckt", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}