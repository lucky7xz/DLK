{"dta.poem.8788": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "Ix.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Und von dir selber \u00fcberwunden bist?", "tokens": ["Und", "von", "dir", "sel\u00b7ber", "\u00fc\u00b7berw\u00b7un\u00b7den", "bist", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Verklage nicht das fromme Schwert der Zeit,", "tokens": ["Ver\u00b7kla\u00b7ge", "nicht", "das", "from\u00b7me", "Schwert", "der", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wenn du der Mann der tausend Wunden bist!", "tokens": ["Wenn", "du", "der", "Mann", "der", "tau\u00b7send", "Wun\u00b7den", "bist", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "CARD", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Bezeug' uns erst, da\u00df nichts in dir dich hemmt,", "tokens": ["Be\u00b7zeug'", "uns", "erst", ",", "da\u00df", "nichts", "in", "dir", "dich", "hemmt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "$,", "KOUS", "PIS", "APPR", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Da\u00df du ein Freund von allen Stunden bist!", "tokens": ["Da\u00df", "du", "ein", "Freund", "von", "al\u00b7len", "Stun\u00b7den", "bist", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "PIAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Sprich erst zur Rose, wenn sie welk erstirbt:", "tokens": ["Sprich", "erst", "zur", "Ro\u00b7se", ",", "wenn", "sie", "welk", "er\u00b7stirbt", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "APPRART", "NN", "$,", "KOUS", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Was k\u00fcmmert's mich, da\u00df du verschwunden bist?", "tokens": ["Was", "k\u00fcm\u00b7mert's", "mich", ",", "da\u00df", "du", "ver\u00b7schwun\u00b7den", "bist", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "KOUS", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Dann, Bruder, glauben wir, wie sehr auch du", "tokens": ["Dann", ",", "Bru\u00b7der", ",", "glau\u00b7ben", "wir", ",", "wie", "sehr", "auch", "du"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "NN", "$,", "VVFIN", "PPER", "$,", "PWAV", "ADV", "ADV", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Von uns, den Freyen, den Gesunden bist.", "tokens": ["Von", "uns", ",", "den", "Frey\u00b7en", ",", "den", "Ge\u00b7sun\u00b7den", "bist", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "ART", "NN", "$,", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}