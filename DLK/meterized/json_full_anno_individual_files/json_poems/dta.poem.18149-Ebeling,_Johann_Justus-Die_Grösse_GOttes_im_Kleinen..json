{"dta.poem.18149": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Die Gr\u00f6sse GOttes  \n im Kleinen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198805", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Unempfindlichkeit der menschlichen", "tokens": ["Die", "Un\u00b7emp\u00b7find\u00b7lich\u00b7keit", "der", "menschli\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Natur,", "tokens": ["Na\u00b7tur", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Sieht in dem Allmachtsreich des", "tokens": ["Sieht", "in", "dem", "All\u00b7machts\u00b7reich", "des"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "ART"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Sch\u00f6pfers keine Spur,", "tokens": ["Sch\u00f6p\u00b7fers", "kei\u00b7ne", "Spur", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Von seiner hohen Gr\u00f6\u00df, als nur", "tokens": ["Von", "sei\u00b7ner", "ho\u00b7hen", "Gr\u00f6\u00df", ",", "als", "nur"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,", "KOUS", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "in grossen Dingen,", "tokens": ["in", "gros\u00b7sen", "Din\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Die durch erhabnen Glantz, uns zur Verwundrung", "tokens": ["Die", "durch", "er\u00b7hab\u00b7nen", "Glantz", ",", "uns", "zur", "Ver\u00b7wund\u00b7rung"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "APPR", "ADJA", "NN", "$,", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "zwingen.", "tokens": ["zwin\u00b7gen", "."], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.9": {"text": "Der Sonnen g\u00fcldnes Rad, das an dem Firmament,", "tokens": ["Der", "Son\u00b7nen", "g\u00fcld\u00b7nes", "Rad", ",", "das", "an", "dem", "Fir\u00b7ma\u00b7ment", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,", "PRELS", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Bey stets geweltzten Lauf, durch seine Kreise rennt", "tokens": ["Bey", "stets", "ge\u00b7weltz\u00b7ten", "Lauf", ",", "durch", "sei\u00b7ne", "Krei\u00b7se", "rennt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "ADJA", "NN", "$,", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Kan unser Hertz noch woll, durch Reitzungvolles", "tokens": ["Kan", "un\u00b7ser", "Hertz", "noch", "woll", ",", "durch", "Reit\u00b7zung\u00b7vol\u00b7les"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VMFIN", "PPOSAT", "NN", "ADV", "ADV", "$,", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Blikken", "tokens": ["Blik\u00b7ken"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.13": {"text": "Das in die Augen strahlt, auf kurtze Zeit entz\u00fckken.", "tokens": ["Das", "in", "die", "Au\u00b7gen", "strahlt", ",", "auf", "kurt\u00b7ze", "Zeit", "ent\u00b7z\u00fck\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "VVFIN", "$,", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Der Farben Mannigfalt, so jenen Lufftkreis mahlt,", "tokens": ["Der", "Far\u00b7ben", "Man\u00b7nig\u00b7falt", ",", "so", "je\u00b7nen", "Lufft\u00b7kreis", "mahlt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ADV", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Und durch den blauen Grund erhellter Wolken strahlt,", "tokens": ["Und", "durch", "den", "blau\u00b7en", "Grund", "er\u00b7hell\u00b7ter", "Wol\u00b7ken", "strahlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Reitzt noch woll das Gesicht, die bunten Himmels-", "tokens": ["Reitzt", "noch", "woll", "das", "Ge\u00b7sicht", ",", "die", "bun\u00b7ten", "Him\u00b7mels"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ADV", "ART", "NN", "$,", "ART", "ADJA", "TRUNC"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.17": {"text": "auen,", "tokens": ["au\u00b7en", ","], "token_info": ["word", "punct"], "pos": ["VVINF", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.18": {"text": "Beym frohen Morgenroth, vergn\u00fcget anzuschauen.", "tokens": ["Beym", "fro\u00b7hen", "Mor\u00b7gen\u00b7roth", ",", "ver\u00b7gn\u00fc\u00b7get", "an\u00b7zu\u00b7schau\u00b7en", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "VVFIN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Was auf der Unterwelt, der Menschen Hertze r\u00fchrt,", "tokens": ["Was", "auf", "der", "Un\u00b7ter\u00b7welt", ",", "der", "Men\u00b7schen", "Hert\u00b7ze", "r\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "$,", "ART", "NN", "VVFIN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Mu\u00df durch das Aussenwerk und Gr\u00f6sse seyn geziert:", "tokens": ["Mu\u00df", "durch", "das", "Aus\u00b7sen\u00b7werk", "und", "Gr\u00f6s\u00b7se", "seyn", "ge\u00b7ziert", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "ART", "NN", "KON", "NN", "PPOSAT", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Jm weiten Pflantzenreich, wo Wald, Feld, G\u00e4r-", "tokens": ["Jm", "wei\u00b7ten", "Pflant\u00b7zen\u00b7reich", ",", "wo", "Wald", ",", "Feld", ",", "G\u00e4r"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word"], "pos": ["APPRART", "ADJA", "NN", "$,", "PWAV", "NN", "$,", "NN", "$,", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "ten gr\u00fcnen", "tokens": ["ten", "gr\u00fc\u00b7nen"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.23": {"text": "Pflegt was erhaben, sch\u00f6n, uns nur zur Lust zu", "tokens": ["Pflegt", "was", "er\u00b7ha\u00b7ben", ",", "sch\u00f6n", ",", "uns", "nur", "zur", "Lust", "zu"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "VVINF", "$,", "ADJD", "$,", "PPER", "ADV", "APPRART", "NN", "APPR"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "dienen.", "tokens": ["die\u00b7nen", "."], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.25": {"text": "Die Ceder Libanons, die mit der schlanken Pracht,", "tokens": ["Die", "Ce\u00b7der", "Li\u00b7ba\u00b7nons", ",", "die", "mit", "der", "schlan\u00b7ken", "Pracht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$,", "PRELS", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Bis in die Wolken steigt, wird noch woll hochgeacht;", "tokens": ["Bis", "in", "die", "Wol\u00b7ken", "steigt", ",", "wird", "noch", "woll", "hoch\u00b7ge\u00b7acht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "VVFIN", "$,", "VAFIN", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Und als ein Meisterst\u00fcck des Sch\u00f6pfers auch geprie-", "tokens": ["Und", "als", "ein", "Meis\u00b7ter\u00b7st\u00fcck", "des", "Sch\u00f6p\u00b7fers", "auch", "ge\u00b7prie"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "ART", "NN", "ADV", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "sen,", "tokens": ["sen", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-", "measure": "single.down"}, "line.29": {"text": "Wer blickt die Kr\u00e4uter an, die in dem gr\u00fcnen Wie-", "tokens": ["Wer", "blickt", "die", "Kr\u00e4u\u00b7ter", "an", ",", "die", "in", "dem", "gr\u00fc\u00b7nen", "Wie"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ART", "NN", "PTKVZ", "$,", "PRELS", "APPR", "ART", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "sen", "tokens": ["sen"], "token_info": ["word"], "pos": ["VVINF"], "meter": "-", "measure": "single.down"}, "line.31": {"text": "Aus zarten Keimen gehn? Wer ist woll recht bem\u00fcht,", "tokens": ["Aus", "zar\u00b7ten", "Kei\u00b7men", "gehn", "?", "Wer", "ist", "woll", "recht", "be\u00b7m\u00fcht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$.", "PWS", "VAFIN", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Den Jsop anzusehn, der nur an W\u00e4nden bl\u00fcht?", "tokens": ["Den", "Jsop", "an\u00b7zu\u00b7sehn", ",", "der", "nur", "an", "W\u00e4n\u00b7den", "bl\u00fcht", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVIZU", "$,", "PRELS", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.33": {"text": "Und doch des Sch\u00f6pfers Macht, zu seinen Preis", "tokens": ["Und", "doch", "des", "Sch\u00f6p\u00b7fers", "Macht", ",", "zu", "sei\u00b7nen", "Preis"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "NN", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.34": {"text": "erhebet;", "tokens": ["er\u00b7he\u00b7bet", ";"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.35": {"text": "Ob er in Niedrigkeit, gleich an der Erde klebet.", "tokens": ["Ob", "er", "in", "Nied\u00b7rig\u00b7keit", ",", "gleich", "an", "der", "Er\u00b7de", "kle\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "$,", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Jm Thierreich ist uns Nichts, was wir bewundern", "tokens": ["Jm", "Thier\u00b7reich", "ist", "uns", "Nichts", ",", "was", "wir", "be\u00b7wun\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "PIS", "$,", "PRELS", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.37": {"text": "sch\u00f6n,", "tokens": ["sch\u00f6n", ","], "token_info": ["word", "punct"], "pos": ["ADJD", "$,"], "meter": "+", "measure": "single.up"}, "line.38": {"text": "Als eine Kreatur, die wir gar selten sehn;", "tokens": ["Als", "ei\u00b7ne", "Kre\u00b7a\u00b7tur", ",", "die", "wir", "gar", "sel\u00b7ten", "sehn", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "PRELS", "PPER", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Als ein solch Ungeheur, f\u00fcr dessen Gang die Wellen,", "tokens": ["Als", "ein", "solch", "Un\u00b7ge\u00b7heur", ",", "f\u00fcr", "des\u00b7sen", "Gang", "die", "Wel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "PIAT", "NN", "$,", "APPR", "PRELAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Wenn es im Wasser wohnt, wie hohe Berge schwellen:", "tokens": ["Wenn", "es", "im", "Was\u00b7ser", "wohnt", ",", "wie", "ho\u00b7he", "Ber\u00b7ge", "schwel\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVFIN", "$,", "PWAV", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Als solch ein Wunderthier, das auf dem Troknen", "tokens": ["Als", "solch", "ein", "Wun\u00b7der\u00b7thier", ",", "das", "auf", "dem", "Trok\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PIAT", "ART", "NN", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.42": {"text": "lebt,", "tokens": ["lebt", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.43": {"text": "Und seiner Glieder Bau, vor andern hoch erhebt;", "tokens": ["Und", "sei\u00b7ner", "Glie\u00b7der", "Bau", ",", "vor", "an\u00b7dern", "hoch", "er\u00b7hebt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "$,", "APPR", "PIS", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Das wie ein Elephant sich mit dem R\u00fcssel br\u00fcstet", "tokens": ["Das", "wie", "ein", "E\u00b7le\u00b7phant", "sich", "mit", "dem", "R\u00fcs\u00b7sel", "br\u00fcs\u00b7tet"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "KOKOM", "ART", "NN", "PRF", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Und gleich wie ein Cameel mit Th\u00fcrmen ausger\u00fcstet.", "tokens": ["Und", "gleich", "wie", "ein", "Ca\u00b7meel", "mit", "Th\u00fcr\u00b7men", "aus\u00b7ge\u00b7r\u00fcs\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "KOKOM", "ART", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Ein Thiergen das da nur in faulen S\u00fcmpfen kreucht,", "tokens": ["Ein", "Thier\u00b7gen", "das", "da", "nur", "in", "fau\u00b7len", "S\u00fcmp\u00b7fen", "kreucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADV", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Und ein gefiedertes, das fast unsichtbahr fleucht,", "tokens": ["Und", "ein", "ge\u00b7fie\u00b7der\u00b7tes", ",", "das", "fast", "un\u00b7sicht\u00b7bahr", "fleucht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "$,", "PRELS", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Wird von dem wenigsten, f\u00fcr sonderbar geachtet", "tokens": ["Wird", "von", "dem", "we\u00b7nigs\u00b7ten", ",", "f\u00fcr", "son\u00b7der\u00b7bar", "ge\u00b7ach\u00b7tet"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "APPR", "ART", "VVFIN", "$,", "APPR", "ADJD", "VVPP"], "meter": "-+-+---+-+-+-", "measure": "unknown.measure.penta"}, "line.49": {"text": "Und noch viel weniger zu ", "tokens": ["Und", "noch", "viel", "we\u00b7ni\u00b7ger", "zu"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "PIS", "PTKZU"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.50": {"text": "Wie aber ist den ", "tokens": ["Wie", "a\u00b7ber", "ist", "den"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ADV", "VAFIN", "ART"], "meter": "-+-+-", "measure": "iambic.di"}, "line.51": {"text": "Schafft er die B\u00e4ume nur, nicht auch das zarte Moos?", "tokens": ["Schafft", "er", "die", "B\u00e4u\u00b7me", "nur", ",", "nicht", "auch", "das", "zar\u00b7te", "Moos", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADV", "$,", "PTKNEG", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.52": {"text": "Hat seine Macht sich nur im Adler abgespiegelt", "tokens": ["Hat", "sei\u00b7ne", "Macht", "sich", "nur", "im", "Ad\u00b7ler", "ab\u00b7ge\u00b7spie\u00b7gelt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "NN", "PRF", "ADV", "APPRART", "NN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.53": {"text": "Und nicht wenn er durch Kunst, ein M\u00fckken Heer", "tokens": ["Und", "nicht", "wenn", "er", "durch", "Kunst", ",", "ein", "M\u00fck\u00b7ken", "Heer"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PTKNEG", "KOUS", "PPER", "APPR", "NN", "$,", "ART", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.54": {"text": "befl\u00fcgelt?", "tokens": ["be\u00b7fl\u00fc\u00b7gelt", "?"], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.55": {"text": "Des Sch\u00f6pfers Kreatur, ist sie gleich noch so klein,", "tokens": ["Des", "Sch\u00f6p\u00b7fers", "Kre\u00b7a\u00b7tur", ",", "ist", "sie", "gleich", "noch", "so", "klein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "VAFIN", "PPER", "ADV", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.56": {"text": "Kan doch ein grosses Bild von seiner Allmacht seyn.", "tokens": ["Kan", "doch", "ein", "gros\u00b7ses", "Bild", "von", "sei\u00b7ner", "All\u00b7macht", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.57": {"t