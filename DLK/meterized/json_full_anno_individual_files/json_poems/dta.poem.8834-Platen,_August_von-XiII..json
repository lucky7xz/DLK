{"dta.poem.8834": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "XiII.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Es hadern die Partei'n, und jede Waffe klirrt:", "tokens": ["Es", "ha\u00b7dern", "die", "Par\u00b7tei'n", ",", "und", "je\u00b7de", "Waf\u00b7fe", "klirrt", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "KON", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wer achtet nun den Lenz, den \u00fcpp'gen Gast der Welt,", "tokens": ["Wer", "ach\u00b7tet", "nun", "den", "Lenz", ",", "den", "\u00fcpp'\u00b7gen", "Gast", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "ART", "NN", "$,", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der taumelnd und berauscht nach allen Seiten irrt?", "tokens": ["Der", "tau\u00b7melnd", "und", "be\u00b7rauscht", "nach", "al\u00b7len", "Sei\u00b7ten", "irrt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "KON", "VVFIN", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wer blickt den Himmel an, und saugt die reine Luft,", "tokens": ["Wer", "blickt", "den", "Him\u00b7mel", "an", ",", "und", "saugt", "die", "rei\u00b7ne", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "PTKVZ", "$,", "KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die br\u00fctend \u00fcber uns mit leisem Fl\u00fcgel schwirrt?", "tokens": ["Die", "br\u00fc\u00b7tend", "\u00fc\u00b7ber", "uns", "mit", "lei\u00b7sem", "Fl\u00fc\u00b7gel", "schwirrt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "PPER", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "D'rum sammle sich umher, wem noch der Lenz behagt,", "tokens": ["D'\u00b7rum", "samm\u00b7le", "sich", "um\u00b7her", ",", "wem", "noch", "der", "Lenz", "be\u00b7hagt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PRF", "PTKVZ", "$,", "PWS", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.7": {"text": "Wer noch des Weins begehrt, wer noch von Liebe girrt!", "tokens": ["Wer", "noch", "des", "Weins", "be\u00b7gehrt", ",", "wer", "noch", "von", "Lie\u00b7be", "girrt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "NN", "VVPP", "$,", "PWS", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ihm hat den Schleyer nicht umsonst gestickt die Nacht,", "tokens": ["Ihm", "hat", "den", "Schle\u00b7yer", "nicht", "um\u00b7sonst", "ge\u00b7stickt", "die", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PTKNEG", "ADV", "VVPP", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Und nicht umsonst der Tag die Zelter angeschirrt.", "tokens": ["Und", "nicht", "um\u00b7sonst", "der", "Tag", "die", "Zel\u00b7ter", "an\u00b7ge\u00b7schirrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ADV", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}