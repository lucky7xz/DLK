{"textgrid.poem.64156": {"metadata": {"author": {"name": "Dahn, Felix", "birth": "N.A.", "death": "N.A."}, "title": "Thamar", "genre": "verse", "period": "N.A.", "pub_year": 1873, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es ritt ein Ritter \u00fcber die Heide,", "tokens": ["Es", "ritt", "ein", "Rit\u00b7ter", "\u00fc\u00b7ber", "die", "Hei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sein Blick war tief und ernst sein Gesicht:", "tokens": ["Sein", "Blick", "war", "tief", "und", "ernst", "sein", "Ge\u00b7sicht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "KON", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Da h\u00f6rt' er schrei'n wie in t\u00f6dlichem Leide,", "tokens": ["Da", "h\u00f6rt'", "er", "schrei'n", "wie", "in", "t\u00f6d\u00b7li\u00b7chem", "Lei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "KOKOM", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Er jagte herzu an den Buschwald dicht.", "tokens": ["Er", "jag\u00b7te", "her\u00b7zu", "an", "den", "Busc\u00b7hwald", "dicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Dort hatten gebunden drei b\u00f6se Sch\u00e4cher", "tokens": ["Dort", "hat\u00b7ten", "ge\u00b7bun\u00b7den", "drei", "b\u00f6\u00b7se", "Sch\u00e4\u00b7cher"], "token_info