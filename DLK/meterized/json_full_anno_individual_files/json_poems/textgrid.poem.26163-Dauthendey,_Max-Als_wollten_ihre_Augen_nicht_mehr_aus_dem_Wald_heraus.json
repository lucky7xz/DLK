{"textgrid.poem.26163": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Als wollten ihre Augen nicht mehr aus dem Wald heraus", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nur einen kleinen Waldstrau\u00df,", "tokens": ["Nur", "ei\u00b7nen", "klei\u00b7nen", "Wald\u00b7strau\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nur einen Buchenzweig und den gedanklich blauen Fingerhut", "tokens": ["Nur", "ei\u00b7nen", "Bu\u00b7chen\u00b7zweig", "und", "den", "ge\u00b7dank\u00b7lich", "blau\u00b7en", "Fin\u00b7ger\u00b7hut"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "KON", "ART", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.3": {"text": "Nahm sie vom Waldweg mit nach Haus,", "tokens": ["Nahm", "sie", "vom", "Wald\u00b7weg", "mit", "nach", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "APPR", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es wollten ihre Augen nicht mehr aus dem Wald heraus,", "tokens": ["Es", "woll\u00b7ten", "ih\u00b7re", "Au\u00b7gen", "nicht", "mehr", "aus", "dem", "Wald", "he\u00b7raus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "PTKNEG", "ADV", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.5": {"text": "Es f\u00fchlten ihre H\u00e4nde sich bei k\u00fchlen Bl\u00e4ttern sorglos gut", "tokens": ["Es", "f\u00fchl\u00b7ten", "ih\u00b7re", "H\u00e4n\u0