{"dta.poem.13620": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Sommerschw\u00fcle .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "W\u00f6lbt hoch sich wieder des Gesanges Br\u00fccke,", "tokens": ["W\u00f6lbt", "hoch", "sich", "wie\u00b7der", "des", "Ge\u00b7san\u00b7ges", "Br\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PRF", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und k\u00fchn darf ich der alten Lust vertrauen,", "tokens": ["Und", "k\u00fchn", "darf", "ich", "der", "al\u00b7ten", "Lust", "ver\u00b7trau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VMFIN", "PPER", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Denn ew'ger Fr\u00fchling will bei ", "tokens": ["Denn", "ew'\u00b7ger", "Fr\u00fch\u00b7ling", "will", "bei"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "VMFIN", "APPR"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}