{"dta.poem.15884": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "89.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wird doch nicht \u00fcbers Kind der Vater ungeduldig,", "tokens": ["Wird", "doch", "nicht", "\u00fc\u00b7bers", "Kind", "der", "Va\u00b7ter", "un\u00b7ge\u00b7dul\u00b7dig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PTKNEG", "APPRART", "NN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das in der Arbeit ihn st\u00f6rt durch sein Spiel unschuldig.", "tokens": ["Das", "in", "der", "Ar\u00b7beit", "ihn", "st\u00f6rt", "durch", "sein", "Spiel", "un\u00b7schul\u00b7dig", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Es klinkt die Th\u00fcren auf und zu, kommt um zu gehn,", "tokens": ["Es", "klinkt", "die", "Th\u00fc\u00b7ren", "auf", "und", "zu", ",", "kommt", "um", "zu", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "KON", "PTKVZ", "$,", "VVFIN", "KOUI", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Geht um zu kommen, l\u00e4\u00dft kein Ding am Flecke stehn,", "tokens": ["Geht", "um", "zu", "kom\u00b7men", ",", "l\u00e4\u00dft", "kein", "Ding", "am", "Fle\u00b7cke", "stehn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOUI", "PTKZU", "VVINF", "$,", "VVFIN", "PIAT", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Schiebt hier am Stuhl, zerrt da am Buch, ruckt dort am Tisch,", "tokens": ["Schiebt", "hier", "am", "Stuhl", ",", "zerrt", "da", "am", "Buch", ",", "ruckt", "dort", "am", "Tisch", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPRART", "NN", "$,", "VVFIN", "ADV", "APPRART", "NN", "$,", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und die Schreibfeder selbst macht es zum Flederwisch.", "tokens": ["Und", "die", "Schreib\u00b7fe\u00b7der", "selbst", "macht", "es", "zum", "Fle\u00b7der\u00b7wisch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "--+-+-+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Der Vater, statt mit Macht zu wehren, droht und lacht,", "tokens": ["Der", "Va\u00b7ter", ",", "statt", "mit", "Macht", "zu", "weh\u00b7ren", ",", "droht", "und", "lacht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUI", "APPR", "NN", "PTKZU", "VVINF", "$,", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die St\u00f6rung freut ihn, die ihm Unterhaltung macht.", "tokens": ["Die", "St\u00f6\u00b7rung", "freut", "ihn", ",", "die", "ihm", "Un\u00b7ter\u00b7hal\u00b7tung", "macht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "PRELS", "PPER", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Die Welt ist auch ein Kind, und will ihr Spielwerk treiben;", "tokens": ["Die", "Welt", "ist", "auch", "ein", "Kind", ",", "und", "will", "ihr", "Spiel\u00b7werk", "trei\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ART", "NN", "$,", "KON", "VMFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn sie dich st\u00f6ret, mu\u00dft du fein geduldig bleiben,", "tokens": ["Wenn", "sie", "dich", "st\u00f6\u00b7ret", ",", "mu\u00dft", "du", "fein", "ge\u00b7dul\u00b7dig", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "$,", "VMFIN", "PPER", "ADJD", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Was schadets, l\u00e4\u00dft sie dich ein wenig wen'ger schreiben!", "tokens": ["Was", "scha\u00b7dets", ",", "l\u00e4\u00dft", "sie", "dich", "ein", "we\u00b7nig", "wen'\u00b7ger", "schrei\u00b7ben", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "VVFIN", "PPER", "PRF", "ART", "PIAT", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}