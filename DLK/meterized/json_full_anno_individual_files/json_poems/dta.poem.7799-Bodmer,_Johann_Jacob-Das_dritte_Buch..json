{"dta.poem.7799": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Das dritte Buch.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-200905198429", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ey dachte Saul, m\u00f6cht dich der Samuel so schauen,", "tokens": ["Ey", "dach\u00b7te", "Saul", ",", "m\u00f6cht", "dich", "der", "Sa\u00b7muel", "so", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NE", "$,", "VMFIN", "PRF", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Er w\u00fcrd, da\u00df ich von Gott verlassen, nimmer trauen,", "tokens": ["Er", "w\u00fcrd", ",", "da\u00df", "ich", "von", "Gott", "ver\u00b7las\u00b7sen", ",", "nim\u00b7mer", "trau\u00b7en", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "KOUS", "PPER", "APPR", "NN", "VVINF", "$,", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wie er mir wei\u00df gemacht, aus Ha\u00df, aus Eifersucht.", "tokens": ["Wie", "er", "mir", "wei\u00df", "ge\u00b7macht", ",", "aus", "Ha\u00df", ",", "aus", "Ei\u00b7fer\u00b7sucht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "VVFIN", "VVPP", "$,", "APPR", "NN", "$,", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wo sind wohl meine Feind, sind sie nicht in der Flucht?", "tokens": ["Wo", "sind", "wohl", "mei\u00b7ne", "Feind", ",", "sind", "sie", "nicht", "in", "der", "Flucht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ADV", "PPOSAT", "NN", "$,", "VAFIN", "PPER", "PTKNEG", "APPR", "ART", "NN", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Kan man aus meinem Sieg wohl Gottes Ha\u00df versp\u00fcren?", "tokens": ["Kan", "man", "aus", "mei\u00b7nem", "Sieg", "wohl", "Got\u00b7tes", "Ha\u00df", "ver\u00b7sp\u00fc\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "APPR", "PPOSAT", "NN", "ADV", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Gefiel ihm nicht, da\u00df ich sollt Jsrael regieren,", "tokens": ["Ge\u00b7fiel", "ihm", "nicht", ",", "da\u00df", "ich", "sollt", "Js\u00b7rael", "re\u00b7gie\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PTKNEG", "$,", "KOUS", "PPER", "VMFIN", "NE", "VVINF", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Wo w\u00e4re mein Triumph, wo w\u00e4re meine Ehr,", "tokens": ["Wo", "w\u00e4\u00b7re", "mein", "Tri\u00b7umph", ",", "wo", "w\u00e4\u00b7re", "mei\u00b7ne", "Ehr", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPOSAT", "NN", "$,", "PWAV", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die mich jetzunder kr\u00f6nt, f\u00fcr meinem gantzen Heer?", "tokens": ["Die", "mich", "je\u00b7tzun\u00b7der", "kr\u00f6nt", ",", "f\u00fcr", "mei\u00b7nem", "gant\u00b7zen", "Heer", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVFIN", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wer sollt nach diesem Sieg die Krone mir bewegen?", "tokens": ["Wer", "sollt", "nach", "die\u00b7sem", "Sieg", "die", "Kro\u00b7ne", "mir", "be\u00b7we\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "APPR", "PDAT", "NN", "ART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Wer Lust dazu gehabt mu\u00df nun sich niederlegen.", "tokens": ["Wer", "Lust", "da\u00b7zu", "ge\u00b7habt", "mu\u00df", "nun", "sich", "nie\u00b7der\u00b7le\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "PAV", "VAPP", "VMFIN", "ADV", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Was st\u00fctzet mehr den Thron als eine Siegeshand?", "tokens": ["Was", "st\u00fct\u00b7zet", "mehr", "den", "Thron", "als", "ei\u00b7ne", "Sie\u00b7ges\u00b7hand", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "ART", "NN", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Das Gl\u00fccke wenn es gut, macht fest den K\u00f6nigsstand.", "tokens": ["Das", "Gl\u00fc\u00b7cke", "wenn", "es", "gut", ",", "macht", "fest", "den", "K\u00f6\u00b7nigs\u00b7stand", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KOUS", "PPER", "ADJD", "$,", "VVFIN", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "So ware Saul getrost, er hofft es w\u00fcrde gl\u00fccken,", "tokens": ["So", "wa\u00b7re", "Saul", "ge\u00b7trost", ",", "er", "hofft", "es", "w\u00fcr\u00b7de", "gl\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NE", "VVPP", "$,", "PPER", "VVFIN", "PPER", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Indem sie insgesammt die Nymphenschaar erblicken,", "tokens": ["In\u00b7dem", "sie", "ins\u00b7ge\u00b7sammt", "die", "Nym\u00b7phen\u00b7schaar", "er\u00b7bli\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Dem K\u00f6nig war die\u00df lieb, die Helden wurden froh,", "tokens": ["Dem", "K\u00f6\u00b7nig", "war", "die\u00df", "lieb", ",", "die", "Hel\u00b7den", "wur\u00b7den", "froh", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PDS", "ADJD", "$,", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Da\u00df ihre Wiederkehr beehret ward also.", "tokens": ["Da\u00df", "ih\u00b7re", "Wie\u00b7der\u00b7kehr", "be\u00b7eh\u00b7ret", "ward", "al\u00b7so", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVPP", "VAFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Der Printz sah sein Gemahl verliebt an und vergn\u00fcget,", "tokens": ["Der", "Printz", "sah", "sein", "Ge\u00b7mahl", "ver\u00b7liebt", "an", "und", "ver\u00b7gn\u00fc\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "VVFIN", "PTKVZ", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Der andern Nymphen Schein hat manchen Held besieget;", "tokens": ["Der", "an\u00b7dern", "Nym\u00b7phen", "Schein", "hat", "man\u00b7chen", "Held", "be\u00b7sie\u00b7get", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VAFIN", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Der David nur allein findt das nicht was er sucht,", "tokens": ["Der", "Da\u00b7vid", "nur", "al\u00b7lein", "findt", "das", "nicht", "was", "er", "sucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "ADV", "ADV", "VVFIN", "PDS", "PTKNEG", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+--+-+-+--+", "measure": "iambic.penta.relaxed"}, "line.20": {"text": "Des Siegs versprochne Beut, der keuschen Liebe Frucht.", "tokens": ["Des", "Siegs", "ver\u00b7sproch\u00b7ne", "Beut", ",", "der", "keu\u00b7schen", "Lie\u00b7be", "Frucht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Er findet aber mehr als nicht war sein Ersinnen,", "tokens": ["Er", "fin\u00b7det", "a\u00b7ber", "mehr", "als", "nicht", "war", "sein", "Er\u00b7sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PIAT", "KOKOM", "PTKNEG", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Der Michal Lieblichkeit, der Michal ihr Beginnen,", "tokens": ["Der", "Mic\u00b7hal", "Lieb\u00b7lich\u00b7keit", ",", "der", "Mic\u00b7hal", "ihr", "Be\u00b7gin\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Der Michal Wunderstral, die schossen auf ihn zu,", "tokens": ["Der", "Mic\u00b7hal", "Wun\u00b7der\u00b7stral", ",", "die", "schos\u00b7sen", "auf", "ihn", "zu", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "PRELS", "VVFIN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Da\u00df Merob er vergist und seiner stillen Ruh.", "tokens": ["Da\u00df", "Me\u00b7rob", "er", "ver\u00b7gist", "und", "sei\u00b7ner", "stil\u00b7len", "Ruh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "PPER", "VVFIN", "KON", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Er wei\u00df nicht wie ihm ist, es kan kein Aug verlassen", "tokens": ["Er", "wei\u00df", "nicht", "wie", "ihm", "ist", ",", "es", "kan", "kein", "Aug", "ver\u00b7las\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "PWAV", "PPER", "VAFIN", "$,", "PPER", "VMFIN", "PIAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Die so ihn ebenfalls anschauet ohne Massen.", "tokens": ["Die", "so", "ihn", "e\u00b7ben\u00b7falls", "an\u00b7schau\u00b7et", "oh\u00b7ne", "Mas\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPER", "ADV", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Je mehr er sie nun schaut, je mehr wird er verblendt,", "tokens": ["Je", "mehr", "er", "sie", "nun", "schaut", ",", "je", "mehr", "wird", "er", "ver\u00b7blendt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "PPER", "ADV", "VVFIN", "$,", "ADV", "ADV", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-----+--+--+", "measure": "iambic.tri.relaxed"}, "line.28": {"text": "Je mehr er wird verblendt, je mehr sein Hertz entbrennt", "tokens": ["Je", "mehr", "er", "wird", "ver\u00b7blendt", ",", "je", "mehr", "sein", "Hertz", "ent\u00b7brennt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PPER", "VAFIN", "VVPP", "$,", "ADV", "ADV", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "In Liebe gegen ihr, ohn da\u00df er doch kan hoffen,", "tokens": ["In", "Lie\u00b7be", "ge\u00b7gen", "ihr", ",", "ohn", "da\u00df", "er", "doch", "kan", "hof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "PPER", "$,", "KOUI", "KOUS", "PPER", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Sie sey auch ebenfalls von keuscher Lieb getroffen.", "tokens": ["Sie", "sey", "auch", "e\u00b7ben\u00b7falls", "von", "keu\u00b7scher", "Lieb", "ge\u00b7trof\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Drum wann ihm Adriel kommt wieder in den Sinn,", "tokens": ["Drum", "wann", "ihm", "Ad\u00b7riel", "kommt", "wie\u00b7der", "in", "den", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PWAV", "PPER", "NE", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.32": {"text": "Den er geliebt zu seyn vermeinet, giebt ers hin.", "tokens": ["Den", "er", "ge\u00b7liebt", "zu", "seyn", "ver\u00b7mei\u00b7net", ",", "giebt", "ers", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "PTKZU", "VAINF", "VVFIN", "$,", "VVFIN", "PIS", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Er fasset wieder sich und kehrt die Augen abe,", "tokens": ["Er", "fas\u00b7set", "wie\u00b7der", "sich", "und", "kehrt", "die", "Au\u00b7gen", "a\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PRF", "KON", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Damit er nicht umsonst sich durch ihr Licht erlabe,", "tokens": ["Da\u00b7mit", "er", "nicht", "um\u00b7sonst", "sich", "durch", "ihr", "Licht", "er\u00b7la\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADV", "PRF", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Er will der Merob seyn die ihm versprochen war,", "tokens": ["Er", "will", "der", "Me\u00b7rob", "seyn", "die", "ihm", "ver\u00b7spro\u00b7chen", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "VAINF", "PRELS", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Und derer Liebe ihm schon schiene offenbar.", "tokens": ["Und", "de\u00b7rer", "Lie\u00b7be", "ihm", "schon", "schie\u00b7ne", "of\u00b7fen\u00b7bar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "NN", "PPER", "ADV", "ADJA", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Mitde\u00df hebt die Musik der Nymphen an zu klingen,", "tokens": ["Mit\u00b7de\u00df", "hebt", "die", "Mu\u00b7sik", "der", "Nym\u00b7phen", "an", "zu", "klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "ART", "NN", "APZR", "PTKZU", "VVINF", "$,"], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.38": {"text": "Davon die Lieblichkeit konnt aller Sinne zwingen,", "tokens": ["Da\u00b7von", "die", "Lieb\u00b7lich\u00b7keit", "konnt", "al\u00b7ler", "Sin\u00b7ne", "zwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "VMFIN", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Das gantze Heer wird still, die Ohren, das Gesicht", "tokens": ["Das", "gant\u00b7ze", "Heer", "wird", "still", ",", "die", "Oh\u00b7ren", ",", "das", "Ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADJD", "$,", "ART", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Ist nach dem sch\u00f6nen Volck verwundert hingericht.", "tokens": ["Ist", "nach", "dem", "sch\u00f6\u00b7nen", "Volck", "ver\u00b7wun\u00b7dert", "hin\u00b7ge\u00b7richt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "ADJA", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Die Paucken gro\u00df