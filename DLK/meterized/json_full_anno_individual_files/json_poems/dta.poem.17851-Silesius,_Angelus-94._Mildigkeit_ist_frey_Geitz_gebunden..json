{"dta.poem.17851": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "94.  Mildigkeit ist frey/ Geitz gebunden.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Milder breitt sich au\u00df/ ein Geitz-Hal\u00df krippt sich ein:", "tokens": ["Ein", "Mil\u00b7der", "breitt", "sich", "au\u00df", "/", "ein", "Geitz\u00b7Ha\u00b7l\u00df", "krippt", "sich", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "PTKVZ", "$(", "ART", "NN", "VVFIN", "PRF", "PTKVZ", "$."], "meter": "-+-+-+-++-+-+", "measure": "unknown.measure.septa"}, "line.2": {"text": "Der f\u00e4ht schon an bestrikt/ und jener frey zu seyn.", "tokens": ["Der", "f\u00e4ht", "schon", "an", "be\u00b7strikt", "/", "und", "je\u00b7ner", "frey", "zu", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "APPR", "VVPP", "$(", "KON", "PDS", "ADJD", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}