{"dta.poem.22489": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "36. Die \u00fcbertreffliche Liebe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1657", "urn": "urn:nbn:de:kobv:b4-200905198292", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Geistreiche Sinn- vnd Schlussrime. Wien, 1657."}, "poem": {"stanza.1": {"line.1": {"text": "Gantz unbegreiflich ist/ die Lieb\u2019 au\u00df der sich GOtt", "tokens": ["Gantz", "un\u00b7be\u00b7greif\u00b7lich", "ist", "/", "die", "Lieb'", "au\u00df", "der", "sich", "Gott"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VAFIN", "$(", "ART", "NN", "APPR", "PRELS", "PRF", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In eines M\u00e4gdlein Scho\u00df zum Br\u00e4utgam mir ent-", "tokens": ["In", "ei\u00b7nes", "M\u00e4gd\u00b7lein", "Scho\u00df", "zum", "Br\u00e4ut\u00b7gam", "mir", "ent"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN", "APPRART", "NE", "PPER", "TRUNC"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "both.", "tokens": ["both", "."], "token_info": ["word", "punct"], "pos": ["ADJD", "$."], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Doch gleichet diesem nichts da\u00df er auch Leib und Lebe\u0303/", "tokens": ["Doch", "glei\u00b7chet", "die\u00b7sem", "nichts", "da\u00df", "er", "auch", "Leib", "und", "Le\u00b7b\u1ebd", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PDAT", "PIS", "KOUS", "PPER", "ADV", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.5": {"text": "Am Creutze wie ein Schelm f\u00fcr mich hat hin gegeben.", "tokens": ["Am", "Creut\u00b7ze", "wie", "ein", "Schelm", "f\u00