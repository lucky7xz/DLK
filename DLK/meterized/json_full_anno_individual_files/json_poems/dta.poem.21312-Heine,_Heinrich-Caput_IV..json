{"dta.poem.21312": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Caput IV.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-30602-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201eund wird der Dom ein Pferdestall,", "tokens": ["\u201e", "und", "wird", "der", "Dom", "ein", "Pfer\u00b7de\u00b7stall", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was sollen wir dann beginnen", "tokens": ["Was", "sol\u00b7len", "wir", "dann", "be\u00b7gin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Mit den heil\u2019gen drey K\u00f6n\u2019gen, die da ruhn", "tokens": ["Mit", "den", "heil'\u00b7gen", "drey", "K\u00f6n'\u00b7gen", ",", "die", "da", "ruhn"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "CARD", "NN", "$,", "PRELS", "ADV", "VVINF"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Im Tabernakel da drinnen?\u201c", "tokens": ["Im", "Ta\u00b7ber\u00b7na\u00b7kel", "da", "drin\u00b7nen", "?", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "NN", "ADV", "ADV", "$.", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "So h\u00f6re ich fragen. Doch brauchen wir uns", "tokens": ["So", "h\u00f6\u00b7re", "ich", "fra\u00b7gen", ".", "Doch", "brau\u00b7chen", "wir", "uns"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "VVINF", "$.", "KON", "VVFIN", "PPER", "PRF"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "In unserer Zeit zu geniren?", "tokens": ["In", "un\u00b7se\u00b7rer", "Zeit", "zu", "ge\u00b7ni\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Die heil\u2019gen drey K\u00f6n\u2019ge aus Morgenland,", "tokens": ["Die", "heil'\u00b7gen", "drey", "K\u00f6n'\u00b7ge", "aus", "Mor\u00b7gen\u00b7land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "CARD", "NN", "APPR", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Sie k\u00f6nnen wo anders logiren.", "tokens": ["Sie", "k\u00f6n\u00b7nen", "wo", "an\u00b7ders", "lo\u00b7gi\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PWAV", "ADV", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.3": {"line.1": {"text": "Folgt meinem Rath und steckt sie hinein", "tokens": ["Folgt", "mei\u00b7nem", "Rath", "und", "steckt", "sie", "hin\u00b7ein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "KON", "VVFIN", "PPER", "PTKVZ"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "In jene drey K\u00f6rbe von Eisen,", "tokens": ["In", "je\u00b7ne", "drey", "K\u00f6r\u00b7be", "von", "Ei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "CARD", "NN", "APPR", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Die hoch zu M\u00fcnster h\u00e4ngen am Thurm,", "tokens": ["Die", "hoch", "zu", "M\u00fcns\u00b7ter", "h\u00e4n\u00b7gen", "am", "Thurm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "NE", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Der Sankt Lamberti gehei\u00dfen.", "tokens": ["Der", "Sankt", "Lam\u00b7ber\u00b7ti", "ge\u00b7hei\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "NE", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Fehlt etwa einer vom Triumvirat,", "tokens": ["Fehlt", "et\u00b7wa", "ei\u00b7ner", "vom", "Tri\u00b7um\u00b7vi\u00b7rat", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "So nehmt einen anderen Menschen,", "tokens": ["So", "nehmt", "ei\u00b7nen", "an\u00b7de\u00b7ren", "Men\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Ersetzt den K\u00f6nig des Morgenlands", "tokens": ["Er\u00b7setzt", "den", "K\u00f6\u00b7nig", "des", "Mor\u00b7gen\u00b7lands"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Durch einen abendl\u00e4ndschen.", "tokens": ["Durch", "ei\u00b7nen", "a\u00b7bend\u00b7l\u00e4nd\u00b7schen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}