.tetra"}}, "stanza.18": {"line.1": {"text": "Wie rauscht der Abend jetzt so k\u00fchl,", "tokens": ["Wie", "rauscht", "der", "A\u00b7bend", "jetzt", "so", "k\u00fchl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wo einst gebrannt der Kampf so schw\u00fcl,", "tokens": ["Wo", "einst", "ge\u00b7brannt", "der", "Kampf", "so", "schw\u00fcl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bei Sedan dort am Tannenb\u00fchl.", "tokens": ["Bei", "Se\u00b7dan", "dort", "am", "Tan\u00b7nen\u00b7b\u00fchl", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.19": {"line.1": {"text": "Die Fremde weilt dort wie es tagt,", "tokens": ["Die", "Frem\u00b7de", "weilt", "dort", "wie", "es", "tagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "KOKOM", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bis durch den Wald der Nachtwind klagt,", "tokens": ["Bis", "durch", "den", "Wald", "der", "Nacht\u00b7wind", "klagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wo hoch ein H\u00fcgel einsam ragt.", "tokens": ["Wo", "hoch", "ein", "H\u00fc\u00b7gel", "ein\u00b7sam", "ragt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.20": {"line.1": {"text": "Heil ihm, der dort den Tod gewann!", "tokens": ["Heil", "ihm", ",", "der", "dort", "den", "Tod", "ge\u00b7wann", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "PRELS", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Seit Lieb' und Liebesschmerz begann,", "tokens": ["Seit", "Lieb'", "und", "Lie\u00b7bes\u00b7schmerz", "be\u00b7gann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ward nicht gleich ihm geliebt ein Mann.", "tokens": ["Ward", "nicht", "gleich", "ihm", "ge\u00b7liebt", "ein", "Mann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ADV", "PPER", "VVPP", "ART", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}}}}