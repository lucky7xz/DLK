, "VVFIN", "KOKOM", "NN", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie lebt und wandelt wie im Traum.", "tokens": ["Sie", "lebt", "und", "wan\u00b7delt", "wie", "im", "Traum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "KOKOM", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Doch, ob sie redet, ob sie schweigt, \u2013", "tokens": ["Doch", ",", "ob", "sie", "re\u00b7det", ",", "ob", "sie", "schweigt", ",", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["KON", "$,", "KOUS", "PPER", "VVFIN", "$,", "KOUS", "PPER", "VVFIN", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ob sie das Haupt zum Busen neigt, \u2013", "tokens": ["Ob", "sie", "das", "Haupt", "zum", "Bu\u00b7sen", "neigt", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPRART", "NN", "VVFIN", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ob sie die sanften Augen zeigt: \u2013", "tokens": ["Ob", "sie", "die", "sanf\u00b7ten", "Au\u00b7gen", "zeigt", ":", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ob ohne Laut sie sinkt ins Knie: \u2013", "tokens": ["Ob", "oh\u00b7ne", "Laut", "sie", "sinkt", "ins", "Knie", ":", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "APPR", "APPR", "PPER", "VVFIN", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein leiser Glanz umflutet sie", "tok