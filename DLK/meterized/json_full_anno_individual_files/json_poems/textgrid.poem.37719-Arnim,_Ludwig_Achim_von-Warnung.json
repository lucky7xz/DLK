{"textgrid.poem.37719": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Warnung", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ufer von Sand, du rauschend verschwiegne \u00d6de", "tokens": ["U\u00b7fer", "von", "Sand", ",", "du", "rau\u00b7schend", "ver\u00b7schwieg\u00b7ne", "\u00d6\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "$,", "PPER", "ADJD", "ADJA", "NN"], "meter": "+--+-+--+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Zermalmeten Gebeins,", "tokens": ["Zer\u00b7mal\u00b7me\u00b7ten", "Ge\u00b7beins", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Ich flieh dich nicht so schn\u00f6de!", "tokens": ["Ich", "flieh", "dich", "nicht", "so", "schn\u00f6\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "ADV", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich w\u00e4hne und ich mein',", "tokens": ["Ich", "w\u00e4h\u00b7ne", "und", "ich", "mein'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da\u00df ich den Strom noch sehe,", "tokens": ["Da\u00df", "ich", "den", "Strom", "noch", "se\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Er war so hell und blau,", "tokens": ["Er", "war", "so", "hell", "und", "blau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Die Fischlein sah ich gehen,", "tokens": ["Die", "Fisc\u00b7hlein", "sah", "ich", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und was ich nunmehr schau! \u2013", "tokens": ["Und", "was", "ich", "nun\u00b7mehr", "schau", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PWS", "PPER", "ADV", "PTKVZ", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Felsen von Eis voll Wiederhall ohne Stimme!", "tokens": ["Fel\u00b7sen", "von", "Eis", "voll", "Wie\u00b7der\u00b7hall", "oh\u00b7ne", "Stim\u00b7me", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "ADJD", "NN", "APPR", "NN", "$."], "meter": "+--+-+-+--+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Wo ist mein altes Haus.", "tokens": ["Wo", "ist", "mein", "al\u00b7tes", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Verscholl'n in deinem Grimme,", "tokens": ["Ver\u00b7scholl'n", "in", "dei\u00b7nem", "Grim\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zerschmettert in deinem Graus!", "tokens": ["Zer\u00b7schmet\u00b7tert", "in", "dei\u00b7nem", "Graus", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Zerrissen flattert die Decke", "tokens": ["Zer\u00b7ris\u00b7sen", "flat\u00b7tert", "die", "De\u00b7cke"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Des blauen heitern Saals,", "tokens": ["Des", "blau\u00b7en", "hei\u00b7tern", "Saals", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Schwankt \u00fcber sich im Schrecke,", "tokens": ["Schwankt", "\u00fc\u00b7ber", "sich", "im", "Schre\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PRF", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Euch schaue ich zumal \u2013", "tokens": ["Euch", "schau\u00b7e", "ich", "zu\u00b7mal", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Hohl\u00e4ugig wei\u00dfe Gasse, die verstellte,", "tokens": ["Hoh\u00b7l\u00e4u\u00b7gig", "wei\u00b7\u00dfe", "Gas\u00b7se", ",", "die", "ver\u00b7stell\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "PRELS", "VVFIN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Du fl\u00fcssiger kalter Stein,", "tokens": ["Du", "fl\u00fcs\u00b7si\u00b7ger", "kal\u00b7ter", "Stein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Das Licht erlischt von K\u00e4lte,", "tokens": ["Das", "Licht", "er\u00b7lischt", "von", "K\u00e4l\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da giebt's nur leeren Schein,", "tokens": ["Da", "giebt's", "nur", "lee\u00b7ren", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Des Sandes K\u00f6rner sinken", "tokens": ["Des", "San\u00b7des", "K\u00f6r\u00b7ner", "sin\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wie helle Sternensaat,", "tokens": ["Wie", "hel\u00b7le", "Ster\u00b7nen\u00b7saat", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und allesammt ertrinken", "tokens": ["Und", "al\u00b7le\u00b7sammt", "er\u00b7trin\u00b7ken"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJD", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "In der get\u00e4uschten Stadt.", "tokens": ["In", "der", "ge\u00b7t\u00e4uschten", "Stadt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "--+-+", "measure": "anapaest.init"}}, "stanza.4": {"line.1": {"text": "Willst du im Zufall deine Hoffnung schauen,", "tokens": ["Willst", "du", "im", "Zu\u00b7fall", "dei\u00b7ne", "Hoff\u00b7nung", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPRART", "NN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So la\u00df die Kr\u00e4fte frei,", "tokens": ["So", "la\u00df", "die", "Kr\u00e4f\u00b7te", "frei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mu\u00dft eigner Kraft vertrauen,", "tokens": ["Mu\u00dft", "eig\u00b7ner", "Kraft", "ver\u00b7trau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die fremde ist nicht treu;", "tokens": ["Die", "frem\u00b7de", "ist", "nicht", "treu", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Willst du der fremden trauen,", "tokens": ["Willst", "du", "der", "frem\u00b7den", "trau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Saug ein den Siegerstrom,", "tokens": ["Saug", "ein", "den", "Sie\u00b7ger\u00b7strom", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Im neuen Land la\u00df bauen,", "tokens": ["Im", "neu\u00b7en", "Land", "la\u00df", "bau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Den neuen h\u00f6hern Thron.", "tokens": ["Den", "neu\u00b7en", "h\u00f6\u00b7hern", "Thron", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Willst du das Eis im Strom noch tr\u00e4ge w\u00e4hnen,", "tokens": ["Willst", "du", "das", "Eis", "im", "Strom", "noch", "tr\u00e4\u00b7ge", "w\u00e4h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "APPRART", "NN", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn schon der Strom erwacht,", "tokens": ["Wenn", "schon", "der", "Strom", "er\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Eis wird an's Land zu lehnen,", "tokens": ["Eis", "wird", "an's", "Land", "zu", "leh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "++-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Durchrei\u00dfen in die Nacht.", "tokens": ["Durc\u00b7hrei\u00b7\u00dfen", "in", "die", "Nacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Im Sturm kein ruhig Leben,", "tokens": ["Im", "Sturm", "kein", "ru\u00b7hig", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PIAT", "ADJD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Er w\u00fchlt dir schon ein Grab,", "tokens": ["Er", "w\u00fchlt", "dir", "schon", "ein", "Grab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Du mu\u00dft dir D\u00e4mme geben,", "tokens": ["Du", "mu\u00dft", "dir", "D\u00e4m\u00b7me", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Sonst rei\u00dft er dich hinab.", "tokens": ["Sonst", "rei\u00dft", "er", "dich", "hin\u00b7ab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Willst du vers\u00f6hnen, was im Sturme tobet,", "tokens": ["Willst", "du", "ver\u00b7s\u00f6h\u00b7nen", ",", "was", "im", "Stur\u00b7me", "to\u00b7bet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Sturm belebt die Welt,", "tokens": ["Der", "Sturm", "be\u00b7lebt", "die", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und wen die Welt nur lobet,", "tokens": ["Und", "wen", "die", "Welt", "nur", "lo\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ist der, der sie erh\u00e4lt;", "tokens": ["Ist", "der", ",", "der", "sie", "er\u00b7h\u00e4lt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Entschlag' dich den Gedanken,", "tokens": ["Ent\u00b7schlag'", "dich", "den", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Den Sturm l\u00f6scht Gegensturm,", "tokens": ["Den", "Sturm", "l\u00f6scht", "Ge\u00b7gen\u00b7sturm", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Die Sonne steigt ohn' Wanken,", "tokens": ["Die", "Son\u00b7ne", "steigt", "ohn'", "Wan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Gl\u00e4nzt hell am kleinsten Wurm.", "tokens": ["Gl\u00e4nzt", "hell", "am", "kleins\u00b7ten", "Wurm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Winke mir nicht zu gehn und still zu schweigen,", "tokens": ["Win\u00b7ke", "mir", "nicht", "zu", "gehn", "und", "still", "zu", "schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PTKNEG", "PTKZU", "VVINF", "KON", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Wirf keine Muschel hin,", "tokens": ["Wirf", "kei\u00b7ne", "Mu\u00b7schel", "hin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So lang' noch etwas dein, ich will nicht scheiden,", "tokens": ["So", "lang'", "noch", "et\u00b7was", "dein", ",", "ich", "will", "nicht", "schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "ADV", "PPOSAT", "$,", "PPER", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Es lieget mir im Sinn,", "tokens": ["Es", "lie\u00b7get", "mir", "im", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da\u00df m\u00fcde meiner Klagen,", "tokens": ["Da\u00df", "m\u00fc\u00b7de", "mei\u00b7ner", "Kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Erdr\u00fcckst den Treuen dein,", "tokens": ["Er\u00b7dr\u00fcckst", "den", "Treu\u00b7en", "dein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PPOSAT", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Dir Vaterland zu sagen:", "tokens": ["Dir", "Va\u00b7ter\u00b7land", "zu", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "\u00bbsiehst du des Wahnsinns Schein?\u00ab", "tokens": ["\u00bb", "siehst", "du", "des", "Wahn\u00b7sinns", "Schein", "?", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "PPER", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Ufer von Sand, du rauschend verschwiegne \u00d6de", "tokens": ["U\u00b7fer", "von", "Sand", ",", "du", "rau\u00b7schend", "ver\u00b7schwieg\u00b7ne", "\u00d6\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "$,", "PPER", "ADJD", "ADJA", "NN"], "meter": "+--+-+--+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Zermalmeten Gebeins,", "tokens": ["Zer\u00b7mal\u00b7me\u00b7ten", "Ge\u00b7beins", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Ich flieh dich nicht so schn\u00f6de!", "tokens": ["Ich", "flieh", "dich", "nicht", "so", "schn\u00f6\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "ADV", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich w\u00e4hne und ich mein',", "tokens": ["Ich", "w\u00e4h\u00b7ne", "und", "ich", "mein'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da\u00df ich den Strom noch sehe,", "tokens": ["Da\u00df", "ich", "den", "Strom", "noch", "se\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Er war so hell und blau,", "tokens": ["Er", "war", "so", "hell", "und", "blau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Die Fischlein sah ich gehen,", "tokens": ["Die", "Fisc\u00b7hlein", "sah", "ich", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und was ich nunmehr schau! \u2013", "tokens": ["Und", "was", "ich", "nun\u00b7mehr", "schau", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PWS", "PPER", "ADV", "PTKVZ", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Felsen von Eis voll Wiederhall ohne Stimme!", "tokens": ["Fel\u00b7sen", "von", "Eis", "voll", "Wie\u00b7der\u00b7hall", "oh\u00b7ne", "Stim\u00b7me", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "ADJD", "NN", "APPR", "NN", "$."], "meter": "+--+-+-+--+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Wo ist mein altes Haus.", "tokens": ["Wo", "ist", "mein", "al\u00b7tes", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Verscholl'n in deinem Grimme,", "tokens": ["Ver\u00b7scholl'n", "in", "dei\u00b7nem", "Grim\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zerschmettert in deinem Graus!", "tokens": ["Zer\u00b7schmet\u00b7tert", "in", "dei\u00b7nem", "Graus", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Zerrissen flattert die Decke", "tokens": ["Zer\u00b7ris\u00b7sen", "flat\u00b7tert", "die", "De\u00b7cke"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Des blauen heitern Saals,", "tokens": ["Des", "blau\u00b7en", "hei\u00b7tern", "Saals", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Schwankt \u00fcber sich im Schrecke,", "tokens": ["Schwankt", "\u00fc\u00b7ber", "sich", "im", "Schre\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PRF", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Euch schaue ich zumal \u2013", "tokens": ["Euch", "schau\u00b7e", "ich", "zu\u00b7mal", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Hohl\u00e4ugig wei\u00dfe Gasse, die verstellte,", "tokens": ["Hoh\u00b7l\u00e4u\u00b7gig", "wei\u00b7\u00dfe", "Gas\u00b7se", ",", "die", "ver\u00b7stell\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "PRELS", "VVFIN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Du fl\u00fcssiger kalter Stein,", "tokens": ["Du", "fl\u00fcs\u00b7si\u00b7ger", "kal\u00b7ter", "Stein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Das Licht erlischt von K\u00e4lte,", "tokens": ["Das", "Licht", "er\u00b7lischt", "von", "K\u00e4l\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da giebt's nur leeren Schein,", "tokens": ["Da", "giebt's", "nur", "lee\u00b7ren", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Des Sandes K\u00f6rner sinken", "tokens": ["Des", "San\u00b7des", "K\u00f6r\u00b7ner", "sin\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wie helle Sternensaat,", "tokens": ["Wie", "hel\u00b7le", "Ster\u00b7nen\u00b7saat", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und allesammt ertrinken", "tokens": ["Und", "al\u00b7le\u00b7sammt", "er\u00b7trin\u00b7ken"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJD", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "In der get\u00e4uschten Stadt.", "tokens": ["In", "der", "ge\u00b7t\u00e4uschten", "Stadt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "--+-+", "measure": "anapaest.init"}}, "stanza.11": {"line.1": {"text": "Willst du im Zufall deine Hoffnung schauen,", "tokens": ["Willst", "du", "im", "Zu\u00b7fall", "dei\u00b7ne", "Hoff\u00b7nung", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPRART", "NN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So la\u00df die Kr\u00e4fte frei,", "tokens": ["So", "la\u00df", "die", "Kr\u00e4f\u00b7te", "frei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mu\u00dft eigner Kraft vertrauen,", "tokens": ["Mu\u00dft", "eig\u00b7ner", "Kraft", "ver\u00b7trau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die fremde ist nicht treu;", "tokens": ["Die", "frem\u00b7de", "ist", "nicht", "treu", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Willst du der fremden trauen,", "tokens": ["Willst", "du", "der", "frem\u00b7den", "trau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Saug ein den Siegerstrom,", "tokens": ["Saug", "ein", "den", "Sie\u00b7ger\u00b7strom", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Im neuen Land la\u00df bauen,", "tokens": ["Im", "neu\u00b7en", "Land", "la\u00df", "bau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Den neuen h\u00f6hern Thron.", "tokens": ["Den", "neu\u00b7en", "h\u00f6\u00b7hern", "Thron", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Willst du das Eis im Strom noch tr\u00e4ge w\u00e4hnen,", "tokens": ["Willst", "du", "das", "Eis", "im", "Strom", "noch", "tr\u00e4\u00b7ge", "w\u00e4h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "APPRART", "NN", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn schon der Strom erwacht,", "tokens": ["Wenn", "schon", "der", "Strom", "er\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Eis wird an's Land zu lehnen,", "tokens": ["Eis", "wird", "an's", "Land", "zu", "leh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "++-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Durchrei\u00dfen in die Nacht.", "tokens": ["Durc\u00b7hrei\u00b7\u00dfen", "in", "die", "Nacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Im Sturm kein ruhig Leben,", "tokens": ["Im", "Sturm", "kein", "ru\u00b7hig", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PIAT", "ADJD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Er w\u00fchlt dir schon ein Grab,", "tokens": ["Er", "w\u00fchlt", "dir", "schon", "ein", "Grab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Du mu\u00dft dir D\u00e4mme geben,", "tokens": ["Du", "mu\u00dft", "dir", "D\u00e4m\u00b7me", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Sonst rei\u00dft er dich hinab.", "tokens": ["Sonst", "rei\u00dft", "er", "dich", "hin\u00b7ab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Willst du vers\u00f6hnen, was im Sturme tobet,", "tokens": ["Willst", "du", "ver\u00b7s\u00f6h\u00b7nen", ",", "was", "im", "Stur\u00b7me", "to\u00b7bet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Sturm belebt die Welt,", "tokens": ["Der", "Sturm", "be\u00b7lebt", "die", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und wen die Welt nur lobet,", "tokens": ["Und", "wen", "die", "Welt", "nur", "lo\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ist der, der sie erh\u00e4lt;", "tokens": ["Ist", "der", ",", "der", "sie", "er\u00b7h\u00e4lt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Entschlag' dich den Gedanken,", "tokens": ["Ent\u00b7schlag'", "dich", "den", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Den Sturm l\u00f6scht Gegensturm,", "tokens": ["Den", "Sturm", "l\u00f6scht", "Ge\u00b7gen\u00b7sturm", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Die Sonne steigt ohn' Wanken,", "tokens": ["Die", "Son\u00b7ne", "steigt", "ohn'", "Wan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Gl\u00e4nzt hell am kleinsten Wurm.", "tokens": ["Gl\u00e4nzt", "hell", "am", "kleins\u00b7ten", "Wurm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Winke mir nicht zu gehn und still zu schweigen,", "tokens": ["Win\u00b7ke", "mir", "nicht", "zu", "gehn", "und", "still", "zu", "schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PTKNEG", "PTKZU", "VVINF", "KON", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Wirf keine Muschel hin,", "tokens": ["Wirf", "kei\u00b7ne", "Mu\u00b7schel", "hin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So lang' noch etwas dein, ich will nicht scheiden,", "tokens": ["So", "lang'", "noch", "et\u00b7was", "dein", ",", "ich", "will", "nicht", "schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "ADV", "PPOSAT", "$,", "PPER", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Es lieget mir im Sinn,", "tokens": ["Es", "lie\u00b7get", "mir", "im", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da\u00df m\u00fcde meiner Klagen,", "tokens": ["Da\u00df", "m\u00fc\u00b7de", "mei\u00b7ner", "Kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Erdr\u00fcckst den Treuen dein,", "tokens": ["Er\u00b7dr\u00fcckst", "den", "Treu\u00b7en", "dein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PPOSAT", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Dir Vaterland zu sagen:", "tokens": ["Dir", "Va\u00b7ter\u00b7land", "zu", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "\u00bbsiehst du des Wahnsinns Schein?\u00ab", "tokens": ["\u00bb", "siehst", "du", "des", "Wahn\u00b7sinns", "Schein", "?", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "PPER", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}