{"dta.poem.3171": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "22.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Auch in der b\u00f6sen Zeit ist Gutes nicht verschwunden,", "tokens": ["Auch", "in", "der", "b\u00f6\u00b7sen", "Zeit", "ist", "Gu\u00b7tes", "nicht", "ver\u00b7schwun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "VAFIN", "NN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bei den Verfolgten wird es wenigstens gefunden.", "tokens": ["Bei", "den", "Ver\u00b7folg\u00b7ten", "wird", "es", "we\u00b7nigs\u00b7tens", "ge\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die Zeit ist aber gut, wo herrschend sich bezeugt", "tokens": ["Die", "Zeit", "ist", "a\u00b7ber", "gut", ",", "wo", "herr\u00b7schend", "sich", "be\u00b7zeugt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "$,", "PWAV", "VVPP", "PRF", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das Gute, und verzagt sich ihm das B\u00f6se beugt.", "tokens": ["Das", "Gu\u00b7te", ",", "und", "ver\u00b7zagt", "sich", "ihm", "das", "B\u00f6\u00b7se", "beugt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KON", "VVFIN", "PRF", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+