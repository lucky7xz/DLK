{"textgrid.poem.65749": {"metadata": {"author": {"name": "Shakespeare, William", "birth": "N.A.", "death": "N.A."}, "title": "23", "genre": "verse", "period": "N.A.", "pub_year": 1590, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie auf der B\u00fchn' ein unge\u00fcbter Held,", "tokens": ["Wie", "auf", "der", "B\u00fchn'", "ein", "un\u00b7ge\u00b7\u00fcb\u00b7ter", "Held", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wenn ihn die Furcht in seiner Rolle hindert,", "tokens": ["Wenn", "ihn", "die", "Furcht", "in", "sei\u00b7ner", "Rol\u00b7le", "hin\u00b7dert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Oder ein wild Gesch\u00f6pf, das Wut zu hitzig schwellt", "tokens": ["O\u00b7der", "ein", "wild", "Ge\u00b7sch\u00f6pf", ",", "das", "Wut", "zu", "hit\u00b7zig", "schwellt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJD", "NN", "$,", "ART", "NN", "PTKA", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und \u00fcberm\u00e4chtig ihm die eigne St\u00e4rke mindert:", "tokens": ["Und", "\u00fc\u00b7ber\u00b7m\u00e4ch\u00b7tig", "ihm", "die", "eig\u00b7ne", "St\u00e4r\u00b7ke", "min\u00b7dert", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PPER", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So ich vergess' es, zaghaft, auszusprechen,", "tokens": ["So", "ich", "ver\u00b7ge\u00b7ss'", "es", ",", "zag\u00b7haft", ",", "aus\u00b7zu\u00b7spre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "PPER", "$,", "ADJD", "$,", "VVIZU", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Was von mir fordert voller Liebe Pflicht;", "tokens": ["Was", "von", "mir", "for\u00b7dert", "vol\u00b7ler", "Lie\u00b7be", "Pflicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "PPER", "VVFIN", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "In eigner Liebe Macht schein' ich mich abzuschw\u00e4chen,", "tokens": ["In", "eig\u00b7ner", "Lie\u00b7be", "Macht", "schein'", "ich", "mich", "ab\u00b7zu\u00b7schw\u00e4\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "VVFIN", "PPER", "PRF", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Zu Boden dr\u00fcckt mich eigner Glut Gewicht.", "tokens": ["Zu", "Bo\u00b7den", "dr\u00fcckt", "mich", "eig\u00b7ner", "Glut", "Ge\u00b7wicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "O dann nimm meine Blick' als Redek\u00fcnste", "tokens": ["O", "dann", "nimm", "mei\u00b7ne", "Blick'", "als", "Re\u00b7de\u00b7k\u00fcns\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "ADV", "VVIMP", "PPOSAT", "NN", "KOUS", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.10": {"text": "Und stumme Deuter der beredten Brust!", "tokens": ["Und", "stum\u00b7me", "Deu\u00b7ter", "der", "be\u00b7red\u00b7ten", "