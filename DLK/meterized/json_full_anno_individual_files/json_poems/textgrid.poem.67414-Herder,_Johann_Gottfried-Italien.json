{"textgrid.poem.67414": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Italien", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Italien, Italien, o Du,", "tokens": ["I\u00b7ta\u00b7li\u00b7en", ",", "I\u00b7ta\u00b7li\u00b7en", ",", "o", "Du", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "$,", "FM", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Das seine Sch\u00f6nheit ungl\u00fcckselig macht,", "tokens": ["Das", "sei\u00b7ne", "Sch\u00f6n\u00b7heit", "un\u00b7gl\u00fcck\u00b7se\u00b7lig", "macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein traurig hartes Schicksal gaben Dir", "tokens": ["Ein", "trau\u00b7rig", "har\u00b7tes", "Schick\u00b7sal", "ga\u00b7ben", "Dir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "ADJA", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mit ihrer Gunst die G\u00f6tter. W\u00e4rest Du", "tokens": ["Mit", "ih\u00b7rer", "Gunst", "die", "G\u00f6t\u00b7ter", ".", "W\u00e4\u00b7rest", "Du"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "$.", "VAFIN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "An Sch\u00f6nheit \u00e4rmer, oder reicher nur", "tokens": ["An", "Sch\u00f6n\u00b7heit", "\u00e4r\u00b7mer", ",", "o\u00b7der", "rei\u00b7cher", "nur"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "ADJD", "$,", "KON", "ADJD", "ADV"], "meter": "-+-+-+-+-+", "measu