{"dta.poem.5362": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Danck-Gebeth nach dem Essen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ovater! der du uns nunmehr", "tokens": ["O\u00b7va\u00b7ter", "!", "der", "du", "uns", "nun\u00b7mehr"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$.", "ART", "PPER", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was unsern C\u00f6rper tr\u00e4nckt und n\u00e4hret,", "tokens": ["Was", "un\u00b7sern", "C\u00f6r\u00b7per", "tr\u00e4nckt", "und", "n\u00e4h\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So reichlich abermahl beschehret,", "tokens": ["So", "reich\u00b7lich", "a\u00b7ber\u00b7mahl", "be\u00b7scheh\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nur dir geb\u00fchret Danck und Ehr!", "tokens": ["Nur", "dir", "ge\u00b7b\u00fch\u00b7ret", "Danck", "und", "Ehr", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du hast aus Gnaden, uns nicht nur", "tokens": ["Du", "hast", "aus", "Gna\u00b7den", ",", "uns", "nicht", "nur"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "NN", "$,", "PPER", "PTKNEG", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Aus nichts gezogen, Leib und Leben,", "tokens": ["Aus", "nichts", "ge\u00b7zo\u00b7gen", ",", "Leib", "und", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VVPP", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und so verschiedne Kost gegeben;", "tokens": ["Und", "so", "ver\u00b7schied\u00b7ne", "Kost", "ge\u00b7ge\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Du hast, im Reiche der Natur,", "tokens": ["Du", "hast", ",", "im", "Rei\u00b7che", "der", "Na\u00b7tur", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "APPRART", "NE", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Der C\u00f6rper Theile so gef\u00fcget,", "tokens": ["Der", "C\u00f6r\u00b7per", "Thei\u00b7le", "so", "ge\u00b7f\u00fc\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Da\u00df, was uns n\u00e4hrt, uns auch vergn\u00fcget,", "tokens": ["Da\u00df", ",", "was", "uns", "n\u00e4hrt", ",", "uns", "auch", "ver\u00b7gn\u00fc\u00b7get", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "PRELS", "PPER", "VVFIN", "$,", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Und darzu sonderlich den Mund,", "tokens": ["Und", "dar\u00b7zu", "son\u00b7der\u00b7lich", "den", "Mund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Die Zunge, Z\u00e4hne, Gaum und Schlund,", "tokens": ["Die", "Zun\u00b7ge", ",", "Z\u00e4h\u00b7ne", ",", "Gaum", "und", "Schlund", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "So kunst- und wunder-reich formiret,", "tokens": ["So", "kunst", "und", "wun\u00b7der\u00b7reich", "for\u00b7mi\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "TRUNC", "KON", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Da\u00df unser Geist durch sie entdeckt", "tokens": ["Da\u00df", "un\u00b7ser", "Geist", "durch", "sie", "ent\u00b7deckt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "APPR", "PPER", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Die Kraft, die in den C\u00f6rpern steckt,", "tokens": ["Die", "Kraft", ",", "die", "in", "den", "C\u00f6r\u00b7pern", "steckt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Und im Genu\u00df Vergu\u00fcgen sp\u00fcret.", "tokens": ["Und", "im", "Ge\u00b7nu\u00df", "Ver\u00b7gu\u00fc\u00b7gen", "sp\u00fc\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Die\u00df Wunder im Getr\u00e4nck und Essen,", "tokens": ["Die\u00df", "Wun\u00b7der", "im", "Ge\u00b7tr\u00e4nck", "und", "Es\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "APPRART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Das uns zugleich auch Nahrung giebt,", "tokens": ["Das", "uns", "zu\u00b7gleich", "auch", "Nah\u00b7rung", "giebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADV", "ADV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Verdient ja wol, da\u00df wir ermessen,", "tokens": ["Ver\u00b7di\u00b7ent", "ja", "wol", ",", "da\u00df", "wir", "er\u00b7mes\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.20": {"text": "Wie sehr uns unser Sch\u00f6pfer liebt.", "tokens": ["Wie", "sehr", "uns", "un\u00b7ser", "Sch\u00f6p\u00b7fer", "liebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Ach, la\u00df denn, was von deinen Gaben,", "tokens": ["Ach", ",", "la\u00df", "denn", ",", "was", "von", "dei\u00b7nen", "Ga\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVIMP", "ADV", "$,", "PRELS", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "O GOtt! wir jetzt genossen haben,", "tokens": ["O", "Gott", "!", "wir", "jetzt", "ge\u00b7nos\u00b7sen", "ha\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "PPER", "ADV", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Woraus uns so viel Lust entstund,", "tokens": ["Wo\u00b7raus", "uns", "so", "viel", "Lust", "ent\u00b7stund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "PIAT", "NN", "VVFIN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.24": {"text": "Uns nicht allein den C\u00f6rper st\u00e4rcken;", "tokens": ["Uns", "nicht", "al\u00b7lein", "den", "C\u00f6r\u00b7per", "st\u00e4r\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "La\u00df uns, an Seel\u2019 und Leib gesund,", "tokens": ["La\u00df", "uns", ",", "an", "Seel'", "und", "Leib", "ge\u00b7sund", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "APPR", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Auch deine Macht und Liebe mercken,", "tokens": ["Auch", "dei\u00b7ne", "Macht", "und", "Lie\u00b7be", "mer\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Und, da wir nun ges\u00e4ttigt seyn,", "tokens": ["Und", ",", "da", "wir", "nun", "ge\u00b7s\u00e4t\u00b7tigt", "seyn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPER", "ADV", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Der Seelen Kraft zu dir allein,", "tokens": ["Der", "See\u00b7len", "Kraft", "zu", "dir", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "In froher Gegen-Liebe, lencken,", "tokens": ["In", "fro\u00b7her", "Ge\u00b7gen\u00b7Lie\u00b7be", ",", "len\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "Dir Ehrfurcht, Danck und Andacht schencken,", "tokens": ["Dir", "Ehr\u00b7furcht", ",", "Danck", "und", "An\u00b7dacht", "schen\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}