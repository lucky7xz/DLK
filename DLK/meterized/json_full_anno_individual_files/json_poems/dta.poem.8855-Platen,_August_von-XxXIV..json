{"dta.poem.8855": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "XxXIV.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df ohne R\u00fcckhalt er sein ganzes Selbst verleihe:", "tokens": ["Da\u00df", "oh\u00b7ne", "R\u00fcck\u00b7halt", "er", "sein", "gan\u00b7zes", "Selbst", "ver\u00b7lei\u00b7he", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NN", "PPER", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Erleuchten soll er klar der Seele tiefste Winkel,", "tokens": ["Er\u00b7leuch\u00b7ten", "soll", "er", "klar", "der", "See\u00b7le", "tiefs\u00b7te", "Win\u00b7kel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "ADJD", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ob auch ein Tadler ihn verlorner W\u00fcrde zeihe.", "tokens": ["Ob", "auch", "ein", "Tad\u00b7ler", "ihn", "ver\u00b7lor\u00b7ner", "W\u00fcr\u00b7de", "zei\u00b7he", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "PPER", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ihr Halben hofft umsonst, mit enger Furcht im Herzen,", "tokens": ["Ihr", "Hal\u00b7ben", "hofft", "um\u00b7sonst", ",", "mit", "en\u00b7ger", "Furcht", "im", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "$,", "APPR", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Da\u00df euer Lied man einst zu gro\u00dfen Liedern reihe:", "tokens": ["Da\u00df", "eu\u00b7er", "Lied", "man", "einst", "zu", "gro\u00b7\u00dfen", "Lie\u00b7dern", "rei\u00b7he", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PIS", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Stumpfsinnige, was w\u00e4hnt ihr rein zu seyn? Ich h\u00f6rte,", "tokens": ["Stumpf\u00b7sin\u00b7ni\u00b7ge", ",", "was", "w\u00e4hnt", "ihr", "rein", "zu", "seyn", "?", "Ich", "h\u00f6r\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PWS", "VVFIN", "PPER", "ADJD", "PTKZU", "VAINF", "$.", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Da\u00df keine Schuld so sehr, als solch ein Sinn entweihe;", "tokens": ["Da\u00df", "kei\u00b7ne", "Schuld", "so", "sehr", ",", "als", "solch", "ein", "Sinn", "ent\u00b7wei\u00b7he", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "ADV", "ADV", "$,", "KOUS", "PIAT", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ich f\u00fchlte, da\u00df die Schuld, die uns aus Eden bannte", "tokens": ["Ich", "f\u00fchl\u00b7te", ",", "da\u00df", "die", "Schuld", ",", "die", "uns", "aus", "E\u00b7den", "bann\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ART", "NN", "$,", "PRELS", "PPER", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Schwungfedern uns zum Flug nach h\u00f6hern Himmeln", "tokens": ["Schwung\u00b7fe\u00b7dern", "uns", "zum", "Flug", "nach", "h\u00f6\u00b7hern", "Him\u00b7meln"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "APPRART", "NN", "APPR", "ADJA", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.10": {"text": "Noch bin ich nicht so bleich, da\u00df ich der Schminke", "tokens": ["Noch", "bin", "ich", "nicht", "so", "bleich", ",", "da\u00df", "ich", "der", "Schmin\u00b7ke"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PTKNEG", "ADV", "ADJD", "$,", "KOUS", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Es kenne mich die Welt, auf da\u00df sie mir verzeihe!", "tokens": ["Es", "ken\u00b7ne", "mich", "die", "Welt", ",", "auf", "da\u00df", "sie", "mir", "ver\u00b7zei\u00b7he", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$,", "APPR", "KOUS", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}