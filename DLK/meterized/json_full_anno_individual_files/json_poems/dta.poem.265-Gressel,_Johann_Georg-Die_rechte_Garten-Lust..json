{"dta.poem.265": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Die rechte Garten-Lust.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Dem G\u00e4rtner mag das Hertze lachen", "tokens": ["Dem", "G\u00e4rt\u00b7ner", "mag", "das", "Hert\u00b7ze", "la\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "PDS", "VVFIN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn er in seinem Garten geht/", "tokens": ["Wenn", "er", "in", "sei\u00b7nem", "Gar\u00b7ten", "geht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wo ihm die Lufft von allen Sachen", "tokens": ["Wo", "ihm", "die", "Lufft", "von", "al\u00b7len", "Sa\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ART", "NN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Schon den Geruch entgegen weht.", "tokens": ["Schon", "den", "Ge\u00b7ruch", "ent\u00b7ge\u00b7gen", "weht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PTKVZ", "VVFIN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Da blicket er die Blumen-Felder", "tokens": ["Da", "bli\u00b7cket", "er", "die", "Blu\u00b7men\u00b7Fel\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Gewi\u00dflich recht vergn\u00fcget an/", "tokens": ["Ge\u00b7wi\u00df\u00b7lich", "recht", "ver\u00b7gn\u00fc\u00b7get", "an", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und freut sich wenn er seine W\u00e4lder", "tokens": ["Und", "freut", "sich", "wenn", "er", "sei\u00b7ne", "W\u00e4l\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "KOUS", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Voll Obst und Fr\u00fcchte schauen kan.", "tokens": ["Voll", "Obst", "und", "Fr\u00fcch\u00b7te", "schau\u00b7en", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "KON", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Er l\u00e4chelt/ wenn auf den ", "tokens": ["Er", "l\u00e4\u00b7chelt", "/", "wenn", "auf", "den"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$(", "KOUS", "APPR", "ART"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Er ein verirrtes Bienchen sieht.", "tokens": ["Er", "ein", "ver\u00b7irr\u00b7tes", "Bien\u00b7chen", "sieht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zu riechen mu\u00df die Ros\u2019 ihm dienen/", "tokens": ["Zu", "rie\u00b7chen", "mu\u00df", "die", "Ros'", "ihm", "die\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "VMFIN", "ART", "NN", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Er k\u00fc\u00dft die s\u00fcsse Nelcken-Bl\u00fcth;", "tokens": ["Er", "k\u00fc\u00dft", "die", "s\u00fcs\u00b7se", "Nel\u00b7cken\u00b7B\u00b7l\u00fcth", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Zur Liljen spricht er: Seht wie nette", "tokens": ["Zur", "Lil\u00b7jen", "spricht", "er", ":", "Seht", "wie", "net\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "$.", "VVFIN", "KOKOM", "NE"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df ihre weisse Bl\u00e4tter stehn/", "tokens": ["Da\u00df", "ih\u00b7re", "weis\u00b7se", "Bl\u00e4t\u00b7ter", "stehn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die Nacht-Viol setzt er vors Bette", "tokens": ["Die", "Nacht\u00b7Vi\u00b7ol", "setzt", "er", "vors", "Bet\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.8": {"text": "Wenn er jetzt will zur Ruhe gehn.", "tokens": ["Wenn", "er", "jetzt", "will", "zur", "Ru\u00b7he", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VMFIN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}