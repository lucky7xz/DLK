{"dta.poem.5244": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "Als die Wiederkunft des K\u00f6nigs  \n gew\u00fcnschet wurde.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-200905197778", "language": ["de:0.99"], "booktitle": "Karsch, Anna Luise: Auserlesene Gedichte. Berlin, 1764."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn unsern Feinden das Herze gesunken;", "tokens": ["Wenn", "un\u00b7sern", "Fein\u00b7den", "das", "Her\u00b7ze", "ge\u00b7sun\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Dann bringt uns der Friede den K\u00f6nig zur\u00fcck,", "tokens": ["Dann", "bringt", "uns", "der", "Frie\u00b7de", "den", "K\u00f6\u00b7nig", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Dann sagen wir n\u00fcchtern, dann stammeln wir trunkeu:", "tokens": ["Dann", "sa\u00b7gen", "wir", "n\u00fcch\u00b7tern", ",", "dann", "stam\u00b7meln", "wir", "trun\u00b7keu", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$,", "ADV", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+--+--+--++", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Sein Antlitz ist Himmel, und Sonne sein Blick!", "tokens": ["Sein", "Ant\u00b7litz", "ist", "Him\u00b7mel", ",", "und", "Son\u00b7ne", "sein", "Blick", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "NN", "$,", "KON", "NN", "PPOSAT", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}}}}