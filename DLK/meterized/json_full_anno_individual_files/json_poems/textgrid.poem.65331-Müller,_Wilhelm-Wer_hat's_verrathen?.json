{"textgrid.poem.65331": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Wer hat's verrathen?", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als wir uns k\u00fc\u00dften, war es Nacht. Wer hat es denn gesehn?", "tokens": ["Als", "wir", "uns", "k\u00fc\u00df\u00b7ten", ",", "war", "es", "Nacht", ".", "Wer", "hat", "es", "denn", "ge\u00b7sehn", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "$,", "VAFIN", "PPER", "NN", "$.", "PWS", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Ein kleiner Stern hat uns belauscht, den sahen wir nicht stehn.", "tokens": ["Ein", "klei\u00b7ner", "Stern", "hat", "uns", "be\u00b7lauscht", ",", "den", "sa\u00b7hen", "wir", "nicht", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "VVFIN", "$,", "ART", "VVFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "Der Stern stieg zu dem Meer herab und sagt' es diesem an,", "tokens": ["Der", "Stern", "stieg", "zu", "dem", "Meer", "her\u00b7ab", "und", "sagt'", "es", "die\u00b7sem", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "ADV", "KON", "VVFIN", "PPER", "PDS", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Das Meer verriet dem Ruder es, das Ruder seinem Mann;", "tokens": ["Das", "Meer", "ver\u00b7riet", "dem", "Ru\u00b7der", "es", ",", "das", "Ru\u00b7der", "sei\u00b7nem", "Mann", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PPER", "$,", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.5": {"text": "Und ach des schw\u00e4tzigen Verraths! so ist es nun geschehn,", "tokens": ["Und", "ach", "des", "schw\u00e4t\u00b7zi\u00b7gen", "Ver\u00b7raths", "!", "so", "ist", "es", "nun", "ge\u00b7schehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "$.", "ADV", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.6": {"text": "Da\u00df jeder Schiffer singt von dem, was keiner hat gesehn.", "tokens": ["Da\u00df", "je\u00b7der", "Schif\u00b7fer", "singt", "von", "dem", ",", "was", "kei\u00b7ner", "hat", "ge\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "VVFIN", "APPR", "ART", "$,", "PRELS", "PIS", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.2": {"line.1": {"text": "Als wir uns k\u00fc\u00dften, war es Nacht. Wer hat es denn gesehn?", "tokens": ["Als", "wir", "uns", "k\u00fc\u00df\u00b7ten", ",", "war", "es", "Nacht", ".", "Wer", "hat", "es", "denn", "ge\u00b7sehn", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "$,", "VAFIN", "PPER", "NN", "$.", "PWS", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Ein kleiner Stern hat uns belauscht, den sahen wir nicht stehn.", "tokens": ["Ein", "klei\u00b7ner", "Stern", "hat", "uns", "be\u00b7lauscht