{"textgrid.poem.61270": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Am Meere", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O leiser Wogenschlag, eint\u00f6nig Lied,", "tokens": ["O", "lei\u00b7ser", "Wo\u00b7gen\u00b7schlag", ",", "ein\u00b7t\u00f6\u00b7nig", "Lied", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Dazu die Harfe r\u00fchrt der m\u00fcde Wind,", "tokens": ["Da\u00b7zu", "die", "Har\u00b7fe", "r\u00fchrt", "der", "m\u00fc\u00b7de", "Wind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wenn Well' auf Welle blinkend strandw\u00e4rts zieht", "tokens": ["Wenn", "Well'", "auf", "Wel\u00b7le", "blin\u00b7kend", "strand\u00b7w\u00e4rts", "zieht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "APPR", "NN", "ADJD", "ADV", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und dann auf goldnem Ufersand verrinnt,", "tokens": ["Und", "dann", "auf", "gold\u00b7nem", "U\u00b7fer\u00b7sand", "ver\u00b7rinnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wie oft in m\u00e4rchenhaftes Traumgebiet", "tokens": ["Wie", "oft", "in", "m\u00e4r\u00b7chen\u00b7haf\u00b7tes", "Traum\u00b7ge\u00b7biet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Verlockte mich dein Wohllaut schon als Kind!", "tokens": ["Ver\u00b7lock\u00b7te", "mich", "dein", "Wohl\u00b7laut", "schon", "als", "Kind", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "ADV", "KOUS", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Versunken stand ich dann und lauschte tief,", "tokens": ["Ver\u00b7sun\u00b7ken", "stand", "ich", "dann", "und", "lauschte", "tief", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "KON", "VVFIN", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Bis mich die Nacht vom lieben Strande rief.", "tokens": ["Bis", "mich", "die", "Nacht", "vom", "lie\u00b7ben", "Stran\u00b7de", "rief", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und alles, was Geheimnisvolles je", "tokens": ["Und", "al\u00b7les", ",", "was", "Ge\u00b7heim\u00b7nis\u00b7vol\u00b7les", "je"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PIS", "$,", "PWS", "NN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mir kund ward, d\u00e4mmert' auf in meinen Sinnen:", "tokens": ["Mir", "kund", "ward", ",", "d\u00e4m\u00b7mert'", "auf", "in", "mei\u00b7nen", "Sin\u00b7nen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "VAFIN", "$,", "VVFIN", "APPR", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Durchsicht'ge Schl\u00f6sser auf dem Grund der See", "tokens": ["Durch\u00b7sicht'\u00b7ge", "Schl\u00f6s\u00b7ser", "auf", "dem", "Grund", "der", "See"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "ART", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "Mit Silberpfeilern und Korallenzinnen;", "tokens": ["Mit", "Sil\u00b7ber\u00b7pfei\u00b7lern", "und", "Ko\u00b7ral\u00b7len\u00b7zin\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Meerk\u00f6nig sa\u00df mit seinem Bart von Schnee", "tokens": ["Meer\u00b7k\u00f6\u00b7nig", "sa\u00df", "mit", "sei\u00b7nem", "Bart", "von", "Schnee"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "PPOSAT", "NN", "APPR", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.6": {"text": "Auf buntem Muschelstuhl und harfte drinnen,", "tokens": ["Auf", "bun\u00b7tem", "Mu\u00b7schel\u00b7stuhl", "und", "harf\u00b7te", "drin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und Nixen spannen zu dem s\u00fc\u00dfen Schall", "tokens": ["Und", "Ni\u00b7xen", "span\u00b7nen", "zu", "dem", "s\u00fc\u00b7\u00dfen", "Schall"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Von goldnen Spindeln F\u00e4den von Kristall.", "tokens": ["Von", "gold\u00b7nen", "Spin\u00b7deln", "F\u00e4\u00b7den", "von", "Kris\u00b7tall", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "APPR", "NN", "$."], "meter": "-+-+-+--++", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Doch als ich \u00e4lter ward, da lauscht' ich nicht", "tokens": ["Doch", "als", "ich", "\u00e4l\u00b7ter", "ward", ",", "da", "lauscht'", "ich", "nicht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ADJD", "VAFIN", "$,", "ADV", "VVFIN", "PPER", "PTKNEG"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Auf wei\u00dfe Nixen mehr noch auf Sirenen;", "tokens": ["Auf", "wei\u00b7\u00dfe", "Ni\u00b7xen", "mehr", "noch", "auf", "Si\u00b7re\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mein eigen Leben bl\u00fchte zum Gedicht,", "tokens": ["Mein", "ei\u00b7gen", "Le\u00b7ben", "bl\u00fch\u00b7te", "zum", "Ge\u00b7dicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und wieder trug zum Strand ich all mein Sehnen.", "tokens": ["Und", "wie\u00b7der", "trug", "zum", "Strand", "ich", "all", "mein", "Seh\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "APPRART", "NN", "PPER", "PIAT", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Dem Seewind bot ich mein erhitzt Gesicht,", "tokens": ["Dem", "See\u00b7wind", "bot", "ich", "mein", "er\u00b7hitzt", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PPOSAT", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Er k\u00fchlte mich und k\u00fc\u00dfte mir die Tr\u00e4nen", "tokens": ["Er", "k\u00fchl\u00b7te", "mich", "und", "k\u00fc\u00df\u00b7te", "mir", "die", "Tr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "KON", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Vom Auge fort - ich aber sprang ins Boot", "tokens": ["Vom", "Au\u00b7ge", "fort", "ich", "a\u00b7ber", "sprang", "ins", "Boot"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "PTKVZ", "$(", "PPER", "ADV", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und steuert' hei\u00df hinaus ins Abendrot.", "tokens": ["Und", "steu\u00b7ert'", "hei\u00df", "hin\u00b7aus", "ins", "A\u00b7ben\u00b7drot", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und \u00fcberm Wasser sang ich - mild und wild,", "tokens": ["Und", "\u00fc\u00b7berm", "Was\u00b7ser", "sang", "ich", "mild", "und", "wild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVFIN", "PPER", "$(", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Reimlose Weisen, wie des Herzens Drang", "tokens": ["Reim\u00b7lo\u00b7se", "Wei\u00b7sen", ",", "wie", "des", "Her\u00b7zens", "Drang"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PWAV", "ART", "NN", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Sie eingibt, wenn's bis zum Zerspringen schwillt,", "tokens": ["Sie", "ein\u00b7gibt", ",", "wenn's", "bis", "zum", "Zer\u00b7sprin\u00b7gen", "schwillt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "APPR", "APPRART", "NN", "VVFIN", "$,"], "meter": "-++-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Nun jauchzend, nun in Sehnsucht todesbang;", "tokens": ["Nun", "jauch\u00b7zend", ",", "nun", "in", "Sehn\u00b7sucht", "to\u00b7des\u00b7bang", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "$,", "ADV", "APPR", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Hei\u00df wie die Tr\u00e4ne, die bewu\u00dftlos quillt,", "tokens": ["Hei\u00df", "wie", "die", "Tr\u00e4\u00b7ne", ",", "die", "be\u00b7wu\u00dft\u00b7los", "quillt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "So flutet' aus der Seele mein Gesang,", "tokens": ["So", "flu\u00b7tet'", "aus", "der", "See\u00b7le", "mein", "Ge\u00b7sang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Der jungen Liebe kunstlos rauhes Lied,", "tokens": ["Der", "jun\u00b7gen", "Lie\u00b7be", "kunst\u00b7los", "rau\u00b7hes", "Lied", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Das erste, das die Muse mir beschied.", "tokens": ["Das", "ers\u00b7te", ",", "das", "die", "Mu\u00b7se", "mir", "be\u00b7schied", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "PRELS", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Und wenn des Mondes klares Auge dann", "tokens": ["Und", "wenn", "des", "Mon\u00b7des", "kla\u00b7res", "Au\u00b7ge", "dann"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Im Blauen aufging, und auf weiter Flut", "tokens": ["Im", "Blau\u00b7en", "auf\u00b7ging", ",", "und", "auf", "wei\u00b7ter", "Flut"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "$,", "KON", "APPR", "ADV", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sein k\u00fchles Silber irren Scheines rann,", "tokens": ["Sein", "k\u00fch\u00b7les", "Sil\u00b7ber", "ir\u00b7ren", "Schei\u00b7nes", "rann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Da ward mir still und friedensvoll zumut.", "tokens": ["Da", "ward", "mir", "still", "und", "frie\u00b7dens\u00b7voll", "zu\u00b7mut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "KON", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Das Ruder zog ich ein und sa\u00df und sann", "tokens": ["Das", "Ru\u00b7der", "zog", "ich", "ein", "und", "sa\u00df", "und", "sann"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ", "KON", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Von goldner Zukunft. O, es sinnt sich gut", "tokens": ["Von", "gold\u00b7ner", "Zu\u00b7kunft", ".", "O", ",", "es", "sinnt", "sich", "gut"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$.", "NE", "$,", "PPER", "VVFIN", "PRF", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Im Kahne - nichts umher in N\u00e4h' und Ferne", "tokens": ["Im", "Kah\u00b7ne", "nichts", "um\u00b7her", "in", "N\u00e4h'", "und", "Fer\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$(", "PIS", "PTKVZ", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Als Lieb' und Meer und \u00fcber uns die Sterne.", "tokens": ["Als", "Lieb'", "und", "Meer", "und", "\u00fc\u00b7ber", "uns", "die", "Ster\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "KON", "APPR", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Einst kehrt' ich heim - O, wie ich da sie fand,", "tokens": ["Einst", "kehrt'", "ich", "heim", "O", ",", "wie", "ich", "da", "sie", "fand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$(", "NE", "$,", "PWAV", "PPER", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mein lockig Kind, das sp\u00e4t zum Strand gegangen,", "tokens": ["Mein", "lo\u00b7ckig", "Kind", ",", "das", "sp\u00e4t", "zum", "Strand", "ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "$,", "PRELS", "ADJD", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und wie ich schwieg, und sie mich doch verstand", "tokens": ["Und", "wie", "ich", "schwieg", ",", "und", "sie", "mich", "doch", "ver\u00b7stand"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "$,", "KON", "PPER", "PRF", "ADV", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und selig gl\u00fcht' und doch verstummt' in Bangen,", "tokens": ["Und", "se\u00b7lig", "gl\u00fcht'", "und", "doch", "ver\u00b7stummt'", "in", "Ban\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "KON", "ADV", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wie meine Lippe brannt' auf ihrer Hand", "tokens": ["Wie", "mei\u00b7ne", "Lip\u00b7pe", "brannt'", "auf", "ih\u00b7rer", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPOSAT", "NN", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Gleich Flamm' auf Schnee und dann auf ihren Wangen,", "tokens": ["Gleich", "Flamm'", "auf", "Schnee", "und", "dann", "auf", "ih\u00b7ren", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "KON", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und dann in wonn'gen Z\u00e4hren all ihr Stolz,", "tokens": ["Und", "dann", "in", "wonn'\u00b7gen", "Z\u00e4h\u00b7ren", "all", "ihr", "Stolz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ADJA", "NN", "PIAT", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "In langen K\u00fcssen all ihr Wesen schmolz:", "tokens": ["In", "lan\u00b7gen", "K\u00fcs\u00b7sen", "all", "