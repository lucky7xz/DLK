{"dta.poem.8817": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "5.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Freund, es soll auch mir die Jugend ohne Liebgekose", "tokens": ["Freund", ",", "es", "soll", "auch", "mir", "die", "Ju\u00b7gend", "oh\u00b7ne", "Lieb\u00b7ge\u00b7ko\u00b7se"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PPER", "VMFIN", "ADV", "PPER", "ART", "NN", "APPR", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.2": {"text": "Wenn die Blumen einst im Garten nach den Tafeln", "tokens": ["Wenn", "die", "Blu\u00b7men", "einst", "im", "Gar\u00b7ten", "nach", "den", "Ta\u00b7feln"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADV", "APPRART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Doch es ist der Lenz gekommen, unsre Wege sind ge-", "tokens": ["Doch", "es", "ist", "der", "Lenz", "ge\u00b7kom\u00b7men", ",", "uns\u00b7re", "We\u00b7ge", "sind", "ge"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "ART", "NN", "VVPP", "$,", "PPOSAT", "NN", "VAFIN", "TRUNC"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.4": {"text": "Bl\u00fche wie die keusche Lilje, la\u00df mich wie die Rose", "tokens": ["Bl\u00fc\u00b7he", "wie", "die", "keu\u00b7sche", "Lil\u00b7je", ",", "la\u00df", "mich", "wie", "die", "Ro\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "KOKOM", "ART", "ADJA", "NN", "$,", "VVIMP", "PPER", "KOKOM",