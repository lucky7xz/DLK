{"dta.poem.3110": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "155.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Dichter w\u00e4r' ein Gott, und zu begl\u00fcckt sein Lo\u00df,", "tokens": ["Der", "Dich\u00b7ter", "w\u00e4r'", "ein", "Gott", ",", "und", "zu", "be\u00b7gl\u00fcckt", "sein", "Lo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$,", "KON", "PTKZU", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der kleine Welten schafft, wie Gott schuf Welten gro\u00df;", "tokens": ["Der", "klei\u00b7ne", "Wel\u00b7ten", "schafft", ",", "wie", "Gott", "schuf", "Wel\u00b7ten", "gro\u00df", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "PWAV", "NN", "VVFIN", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Zu gl\u00fccklich w\u00e4r' er, wenn das was er schuf im Spiele,", "tokens": ["Zu", "gl\u00fcck\u00b7lich", "w\u00e4r'", "er", ",", "wenn", "das", "was", "er", "schuf", "im", "Spie\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "VAFIN", "PPER", "$,", "KOUS", "PDS", "PRELS", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ihm auf die Dauer so, wie Gott sein Werk, gefiele.", "tokens": ["Ihm", "auf", "die", "Dau\u00b7er", "so", ",", "wie", "Gott", "sein", "Werk", ",", "ge\u00b7fie\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "ADV", "$,", "PWAV", "NN", "PPOSAT", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Am Abend meint er zwar, da\u00df wohlgemacht es sei,", "tokens": ["Am", "A\u00b7bend", "meint", "er", "zwar", ",", "da\u00df", "wohl\u00b7ge\u00b7macht", "es", "sei", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ADV", "$,", "KOUS", "VVFIN", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch die Zufriedenheit ist \u00fcber Nacht vorbei.", "tokens": ["Doch", "die", "Zu\u00b7frie\u00b7den\u00b7heit", "ist", "\u00fc\u00b7ber", "Nacht", "vor\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Dann wendet er sich ab dem, was er abgethan;", "tokens": ["Dann", "wen\u00b7det", "er", "sich", "ab", "dem", ",", "was", "er", "ab\u00b7ge\u00b7than", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "ART", "$,", "PWS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gott aber sieht sein Werk mit neuer Lust stets an,", "tokens": ["Gott", "a\u00b7ber", "sieht", "sein", "Werk", "mit", "neu\u00b7er", "Lust", "stets", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Und Neues schaffend, will er Altes nicht vergessen,", "tokens": ["Und", "Neu\u00b7es", "schaf\u00b7fend", ",", "will", "er", "Al\u00b7tes", "nicht", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADJD", "$,", "VMFIN", "PPER", "NN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nur seiner Liebesmacht Unme\u00dfbarkeit ermessen.", "tokens": ["Nur", "sei\u00b7ner", "Lie\u00b7bes\u00b7macht", "Un\u00b7me\u00df\u00b7bar\u00b7keit", "er\u00b7mes\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}