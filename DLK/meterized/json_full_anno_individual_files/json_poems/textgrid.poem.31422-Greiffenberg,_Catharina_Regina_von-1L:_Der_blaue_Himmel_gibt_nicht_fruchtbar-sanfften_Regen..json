{"textgrid.poem.31422": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der blaue Himmel gibt nicht fruchtbar-sanfften Regen.", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der blaue Himmel gibt nicht fruchtbar-sanfften Regen.", "tokens": ["Der", "blau\u00b7e", "Him\u00b7mel", "gibt", "nicht", "frucht\u00b7ba\u00b7rsanf\u00b7ften", "Re\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKNEG", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es treuffet keinen Thau der strahlende Mittag.", "tokens": ["Es", "treuf\u00b7fet", "kei\u00b7nen", "Thau", "der", "strah\u00b7len\u00b7de", "Mit\u00b7tag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Der sch\u00f6ne Demant auch zu nehren nicht vermag.", "tokens": ["Der", "sch\u00f6\u00b7ne", "De\u00b7mant", "auch", "zu", "neh\u00b7ren", "nicht", "ver\u00b7mag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "PTKZU", "VVINF", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "man mu\u00df / will man zum Port / das Wasser ja bewegen.", "tokens": ["man", "mu\u00df", "/", "will", "man", "zum", "Port", "/", "das", "Was\u00b7ser", "ja", "be\u00b7we\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "$(", "VMFIN", "PIS", "APPRART", "NE", "$(", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+---+-+-+-+-", "measure": "dactylic.init"}, "line.5": {"text": "Die Traid-bekleidten Berg / nit Gold und Silber hegen.", "tokens": ["Die", "Trai\u00b7dbe\u00b7kleid\u00b7ten", "Berg", "/", "nit", "Gold", "und", "Sil\u00b7ber", "he\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$(", "PTKNEG", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So kan die Tugend auch nit bl\u00fchen sonder Plag.", "tokens": ["So", "kan", "die", "Tu\u00b7gend", "auch", "nit", "bl\u00fc\u00b7hen", "son\u00b7der", "Plag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "ADV", "PTKNEG", "VVINF", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "in gutem Gl\u00fcck sie grob ohn' allen Glanz da lag /", "tokens": ["in", "gu\u00b7tem", "Gl\u00fcck", "sie", "grob", "ohn'", "al\u00b7len", "Glanz", "da", "lag", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "ADJD", "APPR", "PIAT", "NN", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "in M\u00fch und Arbeit wolt der H\u00f6chst den Segen legen.", "tokens": ["in", "M\u00fch", "und", "Ar\u00b7beit", "wolt", "der", "H\u00f6chst", "den", "Se\u00b7gen", "le\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VMFIN", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "im sauren Meer / und nicht im s\u00fcssen wachs Palast /", "tokens": ["im", "sau\u00b7ren", "Meer", "/", "und", "nicht", "im", "s\u00fcs\u00b7sen", "wachs", "Pa\u00b7last", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$(", "KON", "PTKNEG", "APPRART", "ADJA", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "die theuren Perlein seyn. Also / in vollen Freuden", "tokens": ["die", "theu\u00b7ren", "Per\u00b7lein", "seyn", ".", "Al\u00b7so", "/", "in", "vol\u00b7len", "Freu\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAINF", "$.", "ADV", "$(", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "wird keine Himmels Zier / kein Tugend / nicht gefasst:", "tokens": ["wird", "kei\u00b7ne", "Him\u00b7mels", "Zier", "/", "kein", "Tu\u00b7gend", "/", "nicht", "ge\u00b7fasst", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "NN", "$(", "PIAT", "NN", "$(", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Ihr Balsam-Geist riecht nur im Schmerz-geritzten Leiden.", "tokens": ["Ihr", "Bal\u00b7sam\u00b7Geist", "riecht", "nur", "im", "Schmer\u00b7zge\u00b7ritz\u00b7ten", "Lei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Die Sonn m\u00fcst / solt ein Land sie stets bescheinen / stehn.", "tokens": ["Die", "Sonn", "m\u00fcst", "/", "solt", "ein", "Land", "sie", "stets", "be\u00b7schei\u00b7nen", "/", "stehn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "p