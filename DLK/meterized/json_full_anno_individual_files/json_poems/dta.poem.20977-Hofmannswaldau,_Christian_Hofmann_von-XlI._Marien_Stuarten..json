{"dta.poem.20977": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "XlI.  \n Marien Stuarten.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Mir hat Elisabeth die Freyheit weggenommen/", "tokens": ["Mir", "hat", "E\u00b7lis\u00b7a\u00b7beth", "die", "Frey\u00b7heit", "weg\u00b7ge\u00b7nom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NE", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich bin durchs Henckers Hand von meinem Leben", "tokens": ["Ich", "bin", "durchs", "Hen\u00b7ckers", "Hand", "von", "mei\u00b7nem", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "kommen.", "tokens": ["kom\u00b7men", "."], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Was der und jener klagt/ ist mehrentheils erdacht/", "tokens": ["Was", "der", "und", "je\u00b7ner", "klagt", "/", "ist", "meh\u00b7ren\u00b7theils", "er\u00b7dacht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "KON", "PDS", "VVFIN", "$(", "VAFIN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Mich hat ein guter Kopff umb meinen Kopff ge-", "tokens": ["Mich", "hat", "ein", "gu\u00b7ter", "Kopff", "umb", "mei\u00b7nen", "Kopff", "ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "(bracht.", "tokens": ["(", "bracht", "."], "token_info": ["punct", "word", "punct"], "pos": ["$(", "VVFIN", "$."], "meter": "+", "measure": "single.up"}}}}}