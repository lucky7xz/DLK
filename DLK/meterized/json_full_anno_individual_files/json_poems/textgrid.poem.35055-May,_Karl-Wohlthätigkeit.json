{"textgrid.poem.35055": {"metadata": {"author": {"name": "May, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Wohlth\u00e4tigkeit", "genre": "verse", "period": "N.A.", "pub_year": 1877, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Streckt sich bittend dir entgegen", "tokens": ["Streckt", "sich", "bit\u00b7tend", "dir", "ent\u00b7ge\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "ADJD", "PPER", "PTKVZ"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Eines Bettlers arme Hand,", "tokens": ["Ei\u00b7nes", "Bett\u00b7lers", "ar\u00b7me", "Hand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sei ein Theil ihr von dem Segen,", "tokens": ["Sei", "ein", "Theil", "ihr", "von", "dem", "Se\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der dir wurde, zugewandt.", "tokens": ["Der", "dir", "wur\u00b7de", ",", "zu\u00b7ge\u00b7wandt", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PPER", "VAFIN", "$,", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "G\u00fcter, die dir Gott gegeben,", "tokens": ["G\u00fc\u00b7ter", ",", "die", "dir", "Gott", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sind f\u00fcr Andre dir geliehn,", "tokens": ["Sind", "f\u00fcr", "And\u00b7re", "dir", "ge\u00b7liehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PIS", "PPER", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und nur was du f\u00fcr das Leben", "tokens": ["Und", "nur", "was", "du", "f\u00fcr", "das", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "PWS", "PPER", "APPR", "ART", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Brauchst, sollst du davon beziehn.", "tokens": ["Brauchst", ",", "sollst", "du", "da\u00b7von", "be\u00b7ziehn", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VMFIN", "PPER", "PAV", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Gehst du dennoch da vor\u00fcber,", "tokens": ["Gehst", "du", "den\u00b7noch", "da", "vor\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo Erbarmung n\u00f6thig ist,", "tokens": ["Wo", "Er\u00b7bar\u00b7mung", "n\u00f6\u00b7thig", "ist", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADJD", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "O, so denke dort hin\u00fcber,", "tokens": ["O", ",", "so", "den\u00b7ke", "dort", "hin\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "VVFIN", "ADV", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo du auch nur Bettler bist!", "tokens": ["Wo", "du", "auch", "nur", "Bett\u00b7ler", "bist", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADV", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1":