{"dta.poem.16863": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "222.  Es mu\u00df Gewuchert seyn.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Knecht wuchre da\u00df du hast: denn wann der HErr wird", "tokens": ["Knecht", "wuch\u00b7re", "da\u00df", "du", "hast", ":", "denn", "wann", "der", "Herr", "wird"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "KOUS", "PPER", "VAFIN", "$.", "KON", "PWAV", "ART", "NN", "VAFIN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "kommen:", "tokens": ["kom\u00b7men", ":"], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "So wird von jhm allein der Wuchrer angenommen.", "tokens": ["So", "wird", "von", "jhm", "al\u00b7lein", "der", "Wuch\u00b7rer", "an\u00b7ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "PPER", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}