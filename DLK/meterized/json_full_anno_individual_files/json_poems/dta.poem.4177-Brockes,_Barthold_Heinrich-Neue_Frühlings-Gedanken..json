{"dta.poem.4177": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Neue Fr\u00fchlings-Gedanken.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wir nahen von neuem den Stralen der Sonnen;", "tokens": ["Wir", "na\u00b7hen", "von", "neu\u00b7em", "den", "Stra\u00b7len", "der", "Son\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "APPR", "ADJA", "ART", "NN", "ART", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Von Frost und Eise sind wir frey.", "tokens": ["Von", "Frost", "und", "Ei\u00b7se", "sind", "wir", "frey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "An Tagen hat unsere Fl\u00e4che gewonnen,", "tokens": ["An", "Ta\u00b7gen", "hat", "un\u00b7se\u00b7re", "Fl\u00e4\u00b7che", "ge\u00b7won\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Gott Lob! der Winter ist vorbey.", "tokens": ["Gott", "Lob", "!", "der", "Win\u00b7ter", "ist", "vor\u00b7bey", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$.", "ART", "NN", "VAFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wir sehen dem fr\u00f6hlichen Fr\u00fchling entgegen,", "tokens": ["Wir", "se\u00b7hen", "dem", "fr\u00f6h\u00b7li\u00b7chen", "Fr\u00fch\u00b7ling", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.6": {"text": "Es lacht die tr\u00e4chtige Natur.", "tokens": ["Es", "lacht", "die", "tr\u00e4ch\u00b7ti\u00b7ge", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wir sehen die g\u00e4hrende Kr\u00e4fte sich regen;", "tokens": ["Wir", "se\u00b7hen", "die", "g\u00e4h\u00b7ren\u00b7de", "Kr\u00e4f\u00b7te", "sich", "re\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "PRF", "ADJA", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.8": {"text": "Es f\u00e4rbet sich der Felder Flur.", "tokens": ["Es", "f\u00e4r\u00b7bet", "sich", "der", "Fel\u00b7der", "Flur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Jtzt werden die Augen best\u00e4ndig erfreuet;", "tokens": ["Jtzt", "wer\u00b7den", "die", "Au\u00b7gen", "be\u00b7st\u00e4n\u00b7dig", "er\u00b7freu\u00b7et", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.10": {"text": "Es treibet der gedrungne Klee,", "tokens": ["Es", "trei\u00b7bet", "der", "ge\u00b7drung\u00b7ne", "Klee", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Mit gl\u00e4nzenden lieblichen Bluhmen bestreuet,", "tokens": ["Mit", "gl\u00e4n\u00b7zen\u00b7den", "lieb\u00b7li\u00b7chen", "Bluh\u00b7men", "be\u00b7streu\u00b7et", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.12": {"text": "Fast sichtbar itzt sich in die H\u00f6h.", "tokens": ["Fast", "sicht\u00b7bar", "itzt", "sich", "in", "die", "H\u00f6h", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "PRF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Die Beeten der schimmernden G\u00e4rten bedecket", "tokens": ["Die", "Bee\u00b7ten", "der", "schim\u00b7mern\u00b7den", "G\u00e4r\u00b7ten", "be\u00b7de\u00b7cket"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.14": {"text": "Ein bunt-gef\u00e4rbtes Bluhmen-Heer.", "tokens": ["Ein", "bunt\u00b7ge\u00b7f\u00e4rb\u00b7tes", "Bluh\u00b7men\u00b7Heer", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Von Hecken, durchs wachsende Gr\u00fcne verstecket,", "tokens": ["Von", "He\u00b7cken", ",", "durchs", "wach\u00b7sen\u00b7de", "Gr\u00fc\u00b7ne", "ver\u00b7ste\u00b7cket", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.16": {"text": "Sieht man den d\u00fcrren Strauch nicht mehr.", "tokens": ["Sieht", "man", "den", "d\u00fcr\u00b7ren", "Strauch", "nicht", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ART", "ADJA", "NN", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Es kr\u00f6net die gl\u00e4nzende Bl\u00fchte die Wipfel", "tokens": ["Es", "kr\u00f6\u00b7net", "die", "gl\u00e4n\u00b7zen\u00b7de", "Bl\u00fch\u00b7te", "die", "Wip\u00b7fel"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "ART", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.18": {"text": "Der fruchtbarn B\u00e4ume sonder Zahl.", "tokens": ["Der", "frucht\u00b7barn", "B\u00e4u\u00b7me", "son\u00b7der", "Zahl", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Es kleiden unz\u00e4hlige Kr\u00e4uter die Gipfel", "tokens": ["Es", "klei\u00b7den", "un\u00b7z\u00e4h\u00b7li\u00b7ge", "Kr\u00e4u\u00b7ter", "die", "Gip\u00b7fel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "ART", "NN"], "meter": "-+-+---+--+-", "measure": "iambic.tetra.relaxed"}, "line.20": {"text": "Der Berge, die noch gestern kahl.", "tokens": ["Der", "Ber\u00b7ge", ",", "die", "noch", "ge\u00b7stern", "kahl", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+---+", "measure": "unknown.measure.tri"}, "line.21": {"text": "Aus berstenden Knospen entspriessende Bl\u00e4tter", "tokens": ["Aus", "bers\u00b7ten\u00b7den", "Knos\u00b7pen", "ent\u00b7spries\u00b7sen\u00b7de", "Bl\u00e4t\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.22": {"text": "Erf\u00fcllen \u00fcberall die Luft;", "tokens": ["Er\u00b7f\u00fcl\u00b7len", "\u00fc\u00b7be\u00b7rall", "die", "Luft", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Es schwebt um die B\u00e4ume bey heiterem Wetter", "tokens": ["Es", "schwebt", "um", "die", "B\u00e4u\u00b7me", "bey", "hei\u00b7te\u00b7rem", "Wet\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.24": {"text": "Ein allgemeiner gr\u00fcner Duft.", "tokens": ["Ein", "all\u00b7ge\u00b7mei\u00b7ner", "gr\u00fc\u00b7ner", "Duft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Gef\u00e4rbte befiederte V\u00f6gel durchstreichen", "tokens": ["Ge\u00b7f\u00e4rb\u00b7te", "be\u00b7fie\u00b7der\u00b7te", "V\u00f6\u00b7gel", "durch\u00b7strei\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "NN", "VVINF"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.26": {"text": "Die\u00df gr\u00fcn Gewebe, Paar bey Paar;", "tokens": ["Die\u00df", "gr\u00fcn", "Ge\u00b7we\u00b7be", ",", "Paar", "bey", "Paar", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "NN", "$,", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Wir sehen auf Linden, auf Buchen und Eichen", "tokens": ["Wir", "se\u00b7hen", "auf", "Lin\u00b7den", ",", "auf", "Bu\u00b7chen", "und", "Ei\u00b7chen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NE", "$,", "APPR", "NN", "KON", "NN"], "meter": "-+--+-++--+-", "measure": "iambic.penta.relaxed"}, "line.28": {"text": "Die lange nicht gesehne Schaar.", "tokens": ["Die", "lan\u00b7ge", "nicht", "ge\u00b7seh\u00b7ne", "Schaar", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PTKNEG", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wir sehen sie h\u00fcpfen und springen. Wir h\u00f6ren", "tokens": ["Wir", "se\u00b7hen", "sie", "h\u00fcp\u00b7fen", "und", "sprin\u00b7gen", ".", "Wir", "h\u00f6\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "VVINF", "KON", "VVFIN", "$.", "PPER", "VVINF"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Den lange nicht geh\u00f6rten Schall;", "tokens": ["Den", "lan\u00b7ge", "nicht", "ge\u00b7h\u00f6r\u00b7ten", "Schall", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PTKNEG", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von ihren hell-pfeifenden gurgelnden Ch\u00f6ren", "tokens": ["Von", "ih\u00b7ren", "hell\u00b7pfei\u00b7fen\u00b7den", "gur\u00b7geln\u00b7den", "Ch\u00f6\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "ADJA", "NN"], "meter": "-+-++--+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Ert\u00f6nt der rege Wiederhall.", "tokens": ["Er\u00b7t\u00f6nt", "der", "re\u00b7ge", "Wie\u00b7der\u00b7hall", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Vor andern bezaubert mit wirbelndem Klingen", "tokens": ["Vor", "an\u00b7dern", "be\u00b7zau\u00b7bert", "mit", "wir\u00b7beln\u00b7dem", "Klin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIS", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Die angenehme Nachtigall;", "tokens": ["Die", "an\u00b7ge\u00b7neh\u00b7me", "Nach\u00b7ti\u00b7gall", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Es f\u00fcllt ihr durchdringendes schmetterndes Singen", "tokens": ["Es", "f\u00fcllt", "ihr", "durch\u00b7drin\u00b7gen\u00b7des", "schmet\u00b7tern\u00b7des", "Sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "ADJA", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.8": {"text": "Die B\u00fcsch und W\u00e4lder \u00fcberall.", "tokens": ["Die", "B\u00fcsch", "und", "W\u00e4l\u00b7der", "\u00fc\u00b7be\u00b7rall", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Jtzt wallen von neuem die sprudlenden Quellen,", "tokens": ["Jtzt", "wal\u00b7len", "von", "neu\u00b7em", "die", "sprud\u00b7len\u00b7den", "Quel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "APPR", "ADJA", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.10": {"text": "Von Eis und Schlamm nicht mehr verd\u00e4mmt,", "tokens": ["Von", "Eis", "und", "Schlamm", "nicht", "mehr", "ver\u00b7d\u00e4mmt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PTKNEG", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "In hurtigen, cirkelnden, wirbelnden F\u00e4llen,", "tokens": ["In", "hur\u00b7ti\u00b7gen", ",", "cir\u00b7keln\u00b7den", ",", "wir\u00b7beln\u00b7den", "F\u00e4l\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "VVINF", "$,", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Dur