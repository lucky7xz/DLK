 ["APPR", "PPOSAT", "ADJA", "ADJA", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.18": {"text": "L\u00e4\u00dft sich der bunte Boden sehn.", "tokens": ["L\u00e4\u00dft", "sich", "der", "bun\u00b7te", "Bo\u00b7den", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Man h\u00f6ret ein rauschendes murmelndes Schallen,", "tokens": ["Man", "h\u00f6\u00b7ret", "ein", "rau\u00b7schen\u00b7des", "mur\u00b7meln\u00b7des", "Schal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.20": {"text": "Durch manche kleine F\u00e4ll\u2019, entstehn.", "tokens": ["Durch", "man\u00b7che", "klei\u00b7ne", "F\u00e4ll'", ",", "ent\u00b7stehn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "$,", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Sie rinnen gesch\u00e4ftig, sie rieseln, sie eilen,", "tokens": ["Sie", "rin\u00b7nen", "ge\u00b7sch\u00e4f\u00b7tig", ",", "sie", "rie\u00b7seln", ",", "sie", "ei\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$,", "PPER", "VVINF", "$,", "PPER", "VVFIN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.22": {"text": "Bis da\u00df sich endlich nach und nach,", "tokens": ["Bis", "da\u00df", "sich", "end\u00b7lich", "nach", "und", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PRF", "ADV", "APPR", "KON", "APPR", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Durch ebenern Bode