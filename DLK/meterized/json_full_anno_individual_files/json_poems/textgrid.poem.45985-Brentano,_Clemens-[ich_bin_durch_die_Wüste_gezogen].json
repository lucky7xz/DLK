en Quell ist versunken,", "tokens": ["Der", "Au\u00b7gen", "Quell", "ist", "ver\u00b7sun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Oase wie liegst du so fern!", "tokens": ["O\u00b7a\u00b7se", "wie", "liegst", "du", "so", "fern", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "VVFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Vor Glut ist das Herz mir verglommen,", "tokens": ["Vor", "Glut", "ist", "das", "Herz", "mir", "ver\u00b7glom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Das Ziel, ich f\u00fchl' es gekommen,", "tokens": ["Das", "Ziel", ",", "ich", "f\u00fchl'", "es", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "VVFIN", "PPER", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Ich rufe zum sinkenden Stern:", "tokens": ["Ich", "ru\u00b7fe", "zum", "sin\u00b7ken\u00b7den", "Stern", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "Der Herr hat gegeben, genommen,", "tokens": ["Der", "Herr", "hat", "ge\u00b7ge\u00b7ben", ",", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.8": {"text": "Gelobt sei der Name des Herrn!", "tokens": ["Ge\u00b7lobt", "sei", "der", "Na\u00b7me", "des", "Herrn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}