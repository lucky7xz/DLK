{"textgrid.poem.64050": {"metadata": {"author": {"name": "Ahlefeld, Charlotte von", "birth": "N.A.", "death": "N.A."}, "title": "Mein D\u00f6rfchen", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn im Weltger\u00e4usch, das mich umgiebt,", "tokens": ["Wenn", "im", "Welt\u00b7ge\u00b7r\u00e4usch", ",", "das", "mich", "um\u00b7giebt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "APPRART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Noch mein Herz Dich, trautes D\u00f6rfchen, liebt,", "tokens": ["Noch", "mein", "Herz", "Dich", ",", "trau\u00b7tes", "D\u00f6rf\u00b7chen", ",", "liebt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "PPER", "$,", "ADJA", "NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "O so sinds der Kindheit Fr\u00fchlingskr\u00e4nze", "tokens": ["O", "so", "sinds", "der", "Kind\u00b7heit", "Fr\u00fch\u00b7lings\u00b7kr\u00e4n\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "VAFIN", "ART", "NN", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und die Reize l\u00e4ndlicher Natur,", "tokens": ["Und", "die", "Rei\u00b7ze", "l\u00e4nd\u00b7li\u00b7cher", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Die Erinn'rung meiner ersten Lenze,", "tokens": ["Die", "Er\u00b7inn'\u00b7rung", "mei\u00b7ner", "ers\u00b7ten", "Len\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Was mich kn\u00fcpft an Deine stille Flur.", "tokens": ["Was", "mich", "kn\u00fcpft", "an", "Dei\u00b7ne", "stil\u00b7le", "Flur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Nie konnt' ich im Glanz der St\u00e4dte finden,", "tokens": ["Nie", "konnt'", "ich", "im", "Glanz", "der", "St\u00e4d\u00b7te", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPRART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "+---+-+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Was im D\u00e4mmerschatten Deiner Linden", "tokens": ["Was", "im", "D\u00e4m\u00b7mer\u00b7schat\u00b7ten", "Dei\u00b7ner", "Lin\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "APPRART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Mir die heitre Einsamkeit verlieh.", "tokens": ["Mir", "die", "heit\u00b7re", "Ein\u00b7sam\u00b7keit", "ver\u00b7lieh", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Ach ich fand in lauten, bunten Freuden", "tokens": ["Ach", "ich", "fand", "in", "lau\u00b7ten", ",", "bun\u00b7ten", "Freu\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ITJ", "PPER", "VVFIN", "APPR", "ADJA", "$,", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Jenes Gl\u00fcck der fr\u00fchern Jugend nie \u2013", "tokens": ["Je\u00b7nes", "Gl\u00fcck", "der", "fr\u00fc\u00b7hern", "Ju\u00b7gend", "nie", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ART", "ADJA", "NN", "ADV", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Fand nur Sorgen, Bangigkeit und Leiden.", "tokens": ["Fand", "nur", "Sor\u00b7gen", ",", "Ban\u00b7gig\u00b7keit", "und", "Lei\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Doch in Dir \u2013 froh ging mir auf und unter", "tokens": ["Doch", "in", "Dir", "\u2013", "froh", "ging", "mir", "auf", "und", "un\u00b7ter"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "$(", "ADJD", "VVFIN", "PPER", "PTKVZ", "KON", "APPR"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Stets der Sonnenschein, und mild und munter", "tokens": ["Stets", "der", "Son\u00b7nen\u00b7schein", ",", "und", "mild", "und", "mun\u00b7ter"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "$,", "KON", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Strahlt' in Dir mir Lunas Silberlicht.", "tokens": ["Strahlt'", "in", "Dir", "mir", "Lu\u00b7nas", "Sil\u00b7ber\u00b7licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "PPER", "NE", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Immer, wenn es durch des Himmels Bl\u00e4ue", "tokens": ["Im\u00b7mer", ",", "wenn", "es", "durch", "des", "Him\u00b7mels", "Bl\u00e4u\u00b7e"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "PPER", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "L\u00e4chelnd wie der Blick der Liebe bricht,", "tokens": ["L\u00e4\u00b7chelnd", "wie", "der", "Blick", "der", "Lie\u00b7be", "bricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Mahnt es schmerzlich mich an Dich aufs neue.", "tokens": ["Mahnt", "es", "schmerz\u00b7lich", "mich", "an", "Dich", "aufs", "neu\u00b7e", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "PRF", "APPR", "PPER", "APPRART", "ADJA", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "R\u00f6thete wie Purpur sich der Morgen,", "tokens": ["R\u00f6\u00b7the\u00b7te", "wie", "Pur\u00b7pur", "sich", "der", "Mor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "NN", "PRF", "ART", "NN", "$,"], "meter": "+---+-+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Sang die Nachtigall im Hain verborgen,", "tokens": ["Sang", "die", "Nach\u00b7ti\u00b7gall", "im", "Hain", "ver\u00b7bor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "So begr\u00fcsst' ich freudig die Natur;", "tokens": ["So", "be\u00b7gr\u00fcsst'", "ich", "freu\u00b7dig", "die", "Na\u00b7tur", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Und die Brust, geschwellt von Dank und Liebe,", "tokens": ["Und", "die", "Brust", ",", "ge\u00b7schwellt", "von", "Dank", "und", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "VVPP", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Schuf zum Tempel mir die weite Flur,", "tokens": ["Schuf", "zum", "Tem\u00b7pel", "mir", "die", "wei\u00b7te", "Flur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Wo ich opferte der Andacht reine Triebe.", "tokens": ["Wo", "ich", "op\u00b7fer\u00b7te", "der", "An\u00b7dacht", "rei\u00b7ne", "Trie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.5": {"line.1": {"text": "Wehten dann die Morgenwinde leiser,", "tokens": ["Weh\u00b7ten", "dann", "die", "Mor\u00b7gen\u00b7win\u00b7de", "lei\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ward der Blick der Sonne immer heisser,", "tokens": ["Ward", "der", "Blick", "der", "Son\u00b7ne", "im\u00b7mer", "heis\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Floh ich gern ins freundliche Gemach,", "tokens": ["Floh", "ich", "gern", "ins", "freund\u00b7li\u00b7che", "Ge\u00b7mach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Dachte still besch\u00e4ftigt dann mit Freuden", "tokens": ["Dach\u00b7te", "still", "be\u00b7sch\u00e4f\u00b7tigt", "dann", "mit", "Freu\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "VVFIN", "ADV", "APPR", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Der vergangnen frohen Tage nach,", "tokens": ["Der", "ver\u00b7gang\u00b7nen", "fro\u00b7hen", "Ta\u00b7ge", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Und mein einsam Loos war zu beneiden.", "tokens": ["Und", "mein", "ein\u00b7sam", "Loos", "war", "zu", "be\u00b7nei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJD", "NE", "VAFIN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Schwebte sp\u00e4t auf rosigem Gefieder", "tokens": ["Schweb\u00b7te", "sp\u00e4t", "auf", "ro\u00b7si\u00b7gem", "Ge\u00b7fie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Lind und k\u00fchl die Abendd\u00e4mm'rung nieder,", "tokens": ["Lind", "und", "k\u00fchl", "die", "A\u00b7bend\u00b7d\u00e4m\u00b7m'\u00b7rung", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJD", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "Netzte sie mit Thau die stille Flur,", "tokens": ["Netz\u00b7te", "sie", "mit", "Thau", "die", "stil\u00b7le", "Flur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "O wie eilt' ich dann, sie zu begr\u00fcssen", "tokens": ["O", "wie", "eilt'", "ich", "dann", ",", "sie", "zu", "be\u00b7gr\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "KOKOM", "VVFIN", "PPER", "ADV", "$,", "PPER", "PTKZU", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Und der Sehnsucht leis' erwachte Spur", "tokens": ["Und", "der", "Sehn\u00b7sucht", "leis'", "er\u00b7wach\u00b7te", "Spur"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJD", "VVFIN", "NN"], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.6": {"text": "Wusste froh die Hoffnung zu vers\u00fcssen.", "tokens": ["Wuss\u00b7te", "froh", "die", "Hoff\u00b7nung", "zu", "ver\u00b7s\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "So entwich im eng beschr\u00e4nkten Kreise", "tokens": ["So", "ent\u00b7wich", "im", "eng", "be\u00b7schr\u00e4nk\u00b7ten", "Krei\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "APPRART", "ADJD", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Mir der Fr\u00fchling meiner Jugend leise,", "tokens": ["Mir", "der", "Fr\u00fch\u00b7ling", "mei\u00b7ner", "Ju\u00b7gend", "lei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "PPOSAT", "NN", "ADJD", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Bis das Schicksal finster mich ergriff;", "tokens": ["Bis", "das", "Schick\u00b7sal", "fins\u00b7ter", "mich", "er\u00b7griff", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Ach nun schwankt auf wild erz\u00fcrnten Meeren", "tokens": ["Ach", "nun", "schwankt", "auf", "wild", "er\u00b7z\u00fcrn\u00b7ten", "Mee\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "ADV", "VVFIN", "APPR", "ADJD", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Meines Lebens unbeschirmtes Schiff,", "tokens": ["Mei\u00b7nes", "Le\u00b7bens", "un\u00b7be\u00b7schirm\u00b7tes", "Schiff", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Und die St\u00fcrme werden es verheeren.", "tokens": ["Und", "die", "St\u00fcr\u00b7me", "wer\u00b7den", "es", "ver\u00b7hee\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "PPER", "VVINF", "$."], "meter": "--+-+-+-+-", "measure": "anapaest.init"}}, "stanza.8": {"line.1": {"text": "Unschuldsvolle, nie vergels'ne Stunden,", "tokens": ["Un\u00b7schulds\u00b7vol\u00b7le", ",", "nie", "ver\u00b7gels'\u00b7ne", "Stun\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Warum seid Ihr mir so schnell verschwunden?", "tokens": ["Wa\u00b7rum", "seid", "Ihr", "mir", "so", "schnell", "ver\u00b7schwun\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "PPER", "ADV", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Ruft kein Flehen jemals Euch zur\u00fcck?", "tokens": ["Ruft", "kein", "Fle\u00b7hen", "je\u00b7mals", "Euch", "zu\u00b7r\u00fcck", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "ADV", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Ach die leisen T\u00f6ne meiner Lieder", "tokens": ["Ach", "die", "lei\u00b7sen", "T\u00f6\u00b7ne", "mei\u00b7ner", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "ART", "ADJA", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Klagen um das fr\u00fch verlor'ne Gl\u00fcck,", "tokens": ["Kla\u00b7gen", "um", "das", "fr\u00fch", "ver\u00b7lor'\u00b7ne", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Doch umsonst \u2013 es kehrt mir niemals wieder!", "tokens": ["Doch", "um\u00b7sonst", "\u2013", "es", "kehrt", "mir", "nie\u00b7mals", "wie\u00b7der", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$(", "PPER", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.9": {"line.1": {"text": "Wenn im Weltger\u00e4usch, das mich umgiebt,", "tokens": ["Wenn", "im", "Welt\u00b7ge\u00b7r\u00e4usch", ",", "das", "mich", "um\u00b7giebt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "APPRART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Noch mein Herz Dich, trautes D\u00f6rfchen, liebt,", "tokens": ["Noch", "mein", "Herz", "Dich", ",", "trau\u00b7tes", "D\u00f6rf\u00b7chen", ",", "liebt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "PPER", "$,", "ADJA", "NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "O so sinds der Kindheit Fr\u00fchlingskr\u00e4nze", "tokens": ["O", "so", "sinds", "der", "Kind\u00b7heit", "Fr\u00fch\u00b7lings\u00b7kr\u00e4n\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "VAFIN", "ART", "NN", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und die Reize l\u00e4ndlicher Natur,", "tokens": ["Und", "die", "Rei\u00b7ze", "l\u00e4nd\u00b7li\u00b7cher", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Die Erinn'rung meiner ersten Lenze,", "tokens": ["Die", "Er\u00b7inn'\u00b7rung", "mei\u00b7ner", "ers\u00b7ten", "Len\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Was mich kn\u00fcpft an Deine stille Flur.", "tokens": ["Was", "mich", "kn\u00fcpft", "an", "Dei\u00b7ne", "stil\u00b7le", "Flur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.10": {"line.1": {"text": "Nie konnt' ich im Glanz der St\u00e4dte finden,", "tokens": ["Nie", "konnt'", "ich", "im", "Glanz", "der", "St\u00e4d\u00b7te", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPRART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "+---+-+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Was im D\u00e4mmerschatten Deiner Linden", "tokens": ["Was", "im", "D\u00e4m\u00b7mer\u00b7schat\u00b7ten", "Dei\u00b7ner", "Lin\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "APPRART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Mir die heitre Einsamkeit verlieh.", "tokens": ["Mir", "die", "heit\u00b7re", "Ein\u00b7sam\u00b7keit", "ver\u00b7lieh", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Ach ich fand in lauten, bunten Freuden", "tokens": ["Ach", "ich", "fand", "in", "lau\u00b7ten", ",", "bun\u00b7ten", "Freu\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ITJ", "PPER", "VVFIN", "APPR", "ADJA", "$,", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Jenes Gl\u00fcck der fr\u00fchern Jugend nie \u2013", "tokens": ["Je\u00b7nes", "Gl\u00fcck", "der", "fr\u00fc\u00b7hern", "Ju\u00b7gend", "nie", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ART", "ADJA", "NN", "ADV", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Fand nur Sorgen, Bangigkeit und Leiden.", "tokens": ["Fand", "nur", "Sor\u00b7gen", ",", "Ban\u00b7gig\u00b7keit", "und", "Lei\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.11": {"line.1": {"text": "Doch in Dir \u2013 froh ging mir auf und unter", "tokens": ["Doch", "in", "Dir", "\u2013", "froh", "ging", "mir", "auf", "und", "un\u00b7ter"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "$(", "ADJD", "VVFIN", "PPER", "PTKVZ", "KON", "APPR"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Stets der Sonnenschein