{"line.1": {"text": "Schwebte sp\u00e4t auf rosigem Gefieder", "tokens": ["Schweb\u00b7te", "sp\u00e4t", "auf", "ro\u00b7si\u00b7gem", "Ge\u00b7fie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Lind und k\u00fchl die Abendd\u00e4mm'rung nieder,", "tokens": ["Lind", "und", "k\u00fchl", "die", "A\u00b7bend\u00b7d\u00e4m\u00b7m'\u00b7rung", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJD", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "Netzte sie mit Thau die stille Flur,", "tokens": ["Netz\u00b7te", "sie", "mit", "Thau", "die", "stil\u00b7le", "Flur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "O wie eilt' ich dann, sie zu begr\u00fcssen", "tokens": ["O", "wie", "eilt'", "ich", "dann", ",", "sie", "zu", "be\u00b7gr\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "KOKOM", "VVFIN", "PPER", "ADV", "$,", "PPER", "PTKZU", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Und der Sehnsucht leis' erwachte Spur", "tokens": ["Und", "der", "Sehn\u00b7sucht", "leis'", "er\u00b7wach\u00b7te", "Spur"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJD", "VVFIN", "NN"], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.6": {"text": "Wusste froh die Hoffnung zu vers\u00fcssen.", "tokens": ["Wuss\u00b7te", "froh", "die", "Hoff\u00b7nung", "zu", "ver\u00b7s\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.15": {"line.1": {"text": "So entwich im eng beschr\u00e4nkten Kreise", "tokens": ["So", "ent\u00b7wich", "im", "eng", "be\u00b7schr\u00e4nk\u00b7ten", "Krei\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "APPRART", "ADJD", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Mir der Fr\u00fchling meiner Jugend leise,", "tokens": ["Mir", "der", "Fr\u00fch\u00b7ling", "mei\u00b7ner", "Ju\u00b7gend", "lei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "PPOSAT", "NN", "ADJD", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Bis das Schicksal finster mich ergriff;", "tokens": ["Bis", "das", "Schick\u00b7sal", "fins\u00b7ter", "mich", "er\u00b7griff", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Ach nun schwankt auf wild erz\u00fcrnten Meeren", "tokens": ["Ach", "nun", "schwankt", "auf", "wild", "er\u00b7z\u00fcrn\u00b7ten", "Mee\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "ADV", "VVFIN", "APPR", "ADJD", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Meines Lebens unbeschirmtes Schiff,", "tokens": ["Mei\u00b7nes", "Le\u00b7bens", "un\u00b7be\u00b7schirm\u00b7tes", "Schiff", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Und die St\u00fcrme werden es verheeren.", "tokens": ["Und", "die", "St\u00fcr\u00b7me", "wer\u00b7den", "es", "ver\u00b7hee\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "PPER", "VVINF", "$."], "meter": "--+-+-+-+-", "measure": "anapaest.init"}}, "stanza.16": {"line.1": {"text": "Unschuldsvolle, nie vergels'ne Stunden,", "tokens": ["Un\u00b7schulds\u00b7vol\u00b7le", ",", "nie", "ver\u00b7gels'\u00b7ne", "Stun\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Warum seid Ihr mir so schnell verschwunden?", "tokens": ["Wa\u00b7rum", "seid", "Ihr", "mir", "so", "schnell", "ver\u00b7schwun\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "PPER", "ADV", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Ruft kein Flehen jemals Euch zur\u00fcck?", "tokens": ["Ruft", "kein", "Fle\u00b7hen", "je\u00b7mals", "Euch", "zu\u00b7r\u00fcck", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "ADV", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Ach die leisen T\u00f6ne meiner Lieder", "tokens": ["Ach", "die", "lei\u00b7sen", "T\u00f6\u00b7ne", "mei\u00b7ner", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "ART", "ADJA", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Klagen um das fr\u00fch verlor'ne Gl\u00fcck,", "tokens": ["Kla\u00b7gen", "um", "das", "fr\u00fch", "ver\u00b7lor'\u00b7ne", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Doch umsonst \u2013 es kehrt mir niemals wieder!", "tokens": ["Doch", "um\u00b7sonst", "\u2013", "es",