{"dta.poem.12428": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Geh du nur hin ,  ich hab mein Theil .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1806", "urn": "urn:nbn:de:kobv:b4-20090519157", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Husar. Wohlan die Zeit ist kommen,               ", "tokens": ["Hu\u00b7sar", ".", "Wo\u00b7hlan", "die", "Zeit", "ist", "kom\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ADV", "ART", "NN", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Pferd das mu\u00df gesattelt seyn,", "tokens": ["Mein", "Pferd", "das", "mu\u00df", "ge\u00b7sat\u00b7telt", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PDS", "VMFIN", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich hab mirs vorgenommen,", "tokens": ["Ich", "hab", "mirs", "vor\u00b7ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NE", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Geritten mu\u00df es seyn.", "tokens": ["Ge\u00b7rit\u00b7ten", "mu\u00df", "es", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Geh du nur hin, ich hab mein Theil,", "tokens": ["Geh", "du", "nur", "hin", ",", "ich", "hab", "mein", "Theil", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADV", "PTKVZ", "$,", "PPER", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ich lieb dich nur aus Narrethei;", "tokens": ["Ich", "lieb", "dich", "nur", "aus", "Nar\u00b7re\u00b7thei", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ohne dich kann ich wohl leben,", "tokens": ["Oh\u00b7ne", "dich", "kann", "ich", "wohl", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ohne dich kann ich schon seyn.", "tokens": ["Oh\u00b7ne", "dich", "kann", "ich", "schon", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VMFIN", "PPER", "ADV", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "So setz ich mich aufs Pferdchen,", "tokens": ["So", "setz", "ich", "mich", "aufs", "Pferd\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und trink ein Gl\u00e4schen k\u00fchlen Wein,", "tokens": ["Und", "trink", "ein", "Gl\u00e4s\u00b7chen", "k\u00fch\u00b7len", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und schw\u00f6r bey meinem B\u00e4rtchen,", "tokens": ["Und", "schw\u00f6r", "bey", "mei\u00b7nem", "B\u00e4rt\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dir ewig treu zu seyn: Geh du u. s. w.", "tokens": ["Dir", "e\u00b7wig", "treu", "zu", "seyn", ":", "Geh", "du", "u.", "s.", "w."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "abbreviation", "abbreviation", "abbreviation"], "pos": ["PPER", "ADJD", "ADJD", "PTKZU", "VAINF", "$.", "NE", "PPER", "APPR", "VVIMP", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "M\u00e4dchen. Du glaubst, du bist der Sch\u00f6nste,               ", "tokens": ["M\u00e4d\u00b7chen", ".", "Du", "glaubst", ",", "du", "bist", "der", "Sch\u00f6ns\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "VVFIN", "$,", "PPER", "VAFIN", "ART", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Wohl auf der ganzen weiten Welt,", "tokens": ["Wohl", "auf", "der", "gan\u00b7zen", "wei\u00b7ten", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und auch der Angenehmste,", "tokens": ["Und", "auch", "der", "An\u00b7ge\u00b7nehms\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ist aber weit gefehlt: Geh du nur hin u. s. w.", "tokens": ["Ist", "a\u00b7ber", "weit", "ge\u00b7fehlt", ":", "Geh", "du", "nur", "hin", "u.", "s.", "w."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "abbreviation", "abbreviation", "abbreviation"], "pos": ["VAFIN", "ADV", "ADJD", "VVPP", "$.", "NE", "PPER", "ADV", "ADV", "APPR", "VVIMP", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "In meines Vaters Garten,", "tokens": ["In", "mei\u00b7nes", "Va\u00b7ters", "Gar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "W\u00e4chst eine sch\u00f6ne Blume drin,", "tokens": ["W\u00e4chst", "ei\u00b7ne", "sch\u00f6\u00b7ne", "Blu\u00b7me", "drin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So lang will ich noch warten,", "tokens": ["So", "lang", "will", "ich", "noch", "war\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bis die noch gr\u00f6\u00dfer ist. Geh du nur u. s. w.", "tokens": ["Bis", "die", "noch", "gr\u00f6\u00b7\u00dfer", "ist", ".", "Geh", "du", "nur", "u.", "s.", "w."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "abbreviation", "abbreviation", "abbreviation"], "pos": ["APPR", "ART", "ADV", "ADJD", "VAFIN", "$.", "NE", "PPER", "ADV", "APPR", "VVIMP", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Beyde. Du denkst ich werd dich nehmen,               ", "tokens": ["Bey\u00b7de", ".", "Du", "denkst", "ich", "werd", "dich", "neh\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$.", "PPER", "VVFIN", "PPER", "VAFIN", "PRF", "VVINF", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Ich habs noch nicht im Sinn,", "tokens": ["Ich", "habs", "noch", "nicht", "im", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NE", "ADV", "PTKNEG", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich mu\u00df mich deiner sch\u00e4men,", "tokens": ["Ich", "mu\u00df", "mich", "dei\u00b7ner", "sch\u00e4\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PPOSAT", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wenn ich in Gesellschaft bin;", "tokens": ["Wenn", "ich", "in", "Ge\u00b7sell\u00b7schaft", "bin", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Geh du nur hin, ich hab mein. u. s. w.", "tokens": ["Geh", "du", "nur", "hin", ",", "ich", "hab", "mein", ".", "u.", "s.", "w."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "abbreviation", "abbreviation", "abbreviation"], "pos": ["NE", "PPER", "ADV", "PTKVZ", "$,", "PPER", "VAFIN", "PPOSAT", "$.", "KON", "VVIMP", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}