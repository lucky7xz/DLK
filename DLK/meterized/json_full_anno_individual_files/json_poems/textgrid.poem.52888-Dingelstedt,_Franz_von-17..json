{"textgrid.poem.52888": {"metadata": {"author": {"name": "Dingelstedt, Franz von", "birth": "N.A.", "death": "N.A."}, "title": "17.", "genre": "verse", "period": "N.A.", "pub_year": 1847, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aber nein! Ich ziehe mit leisem Schritt", "tokens": ["A\u00b7ber", "nein", "!", "Ich", "zie\u00b7he", "mit", "lei\u00b7sem", "Schritt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PTKANT", "$.", "PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Vorbei der verfluchten St\u00e4tte.", "tokens": ["Vor\u00b7bei", "der", "ver\u00b7fluch\u00b7ten", "St\u00e4t\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ich weckte ja sie zum Leiden mit,", "tokens": ["Ich", "weck\u00b7te", "ja", "sie", "zum", "Lei\u00b7den", "mit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPER", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sie droben im Witwen-Bette.", "tokens": ["Sie", "dro\u00b7ben", "im", "Wit\u00b7wen\u00b7Bet\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPRART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Du armes, junges, s\u00fc\u00dfes Weib,", "tokens": ["Du", "ar\u00b7mes", ",", "jun\u00b7ges", ",", "s\u00fc\u00b7\u00dfes", "Weib", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADJA", "$,", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zum Schatten umgewandelt,", "tokens": ["Zum", "Schat\u00b7ten", "um\u00b7ge\u00b7wan\u00b7delt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Seit du den bl\u00fchenden, sch\u00f6nen Leib", "tokens": ["Seit", "du", "den", "bl\u00fc\u00b7hen\u00b7den", ",", "sch\u00f6\u00b7nen", "Leib"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "An jenen Toten verhandelt!", "tokens": ["An", "je\u00b7nen", "To\u00b7ten", "ver\u00b7han\u00b7delt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Nun hast du dein beneidet' Gl\u00fcck,", "tokens": ["Nun", "hast", "du", "dein", "be\u00b7nei\u00b7det'", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Titel, die Mittel, die Gr\u00f6\u00dfen,", "tokens": ["Die", "Ti\u00b7tel", ",", "die", "Mit\u00b7tel", ",", "die", "Gr\u00f6\u00b7\u00dfen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Und g\u00e4bst es mit tausend Freuden zur\u00fcck", "tokens": ["Und", "g\u00e4bst", "es", "mit", "tau\u00b7send", "Freu\u00b7den", "zu\u00b7r\u00fcck"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPR", "CARD", "NN", "PTKVZ"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "F\u00fcr deiner Arm