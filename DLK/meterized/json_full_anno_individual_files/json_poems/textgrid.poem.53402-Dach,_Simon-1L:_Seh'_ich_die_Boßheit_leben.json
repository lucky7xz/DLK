", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Giebst mir zwar Liebes-Schl\u00e4ge", "tokens": ["Giebst", "mir", "zwar", "Lie\u00b7bes\u00b7Schl\u00e4\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und ziehest mich dein Kind,", "tokens": ["Und", "zie\u00b7hest", "mich", "dein", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du leitest mich durch Wege", "tokens": ["Du", "lei\u00b7test", "mich", "durch", "We\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die mir verborgen sind,", "tokens": ["Die", "mir", "ver\u00b7bor\u00b7gen", "sind", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Durch Raht den ich nicht h\u00f6ren", "tokens": ["Durch", "Raht", "den", "ich", "nicht", "h\u00f6\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "ART", "PPER", "PTKNEG", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Nicht sehn noch fassen kan,", "tokens": ["Nicht", "sehn", "noch", "fas\u00b7sen", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Jedoch nimmst Du mit Ehren", "tokens": ["Je\u00b7doch", "nimmst", "Du", "mit", "Eh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.8": {"text": "Mich endlich wieder an.", "tokens": ["Mich", "end\u00b7lich", "wie\u00b7der", "an", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Du Vrsprung aller Gaben", "tokens": ["Du", "Vr\u00b7sprung", "al\u00b7ler", "Ga\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "NN", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Erd' und Himmel hat,", "tokens": ["Die", "Erd'", "und", "Him\u00b7mel", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wenn ich nur Dich kan haben", "tokens": ["Wenn", "ich", "nur", "Dich", "kan", "ha\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "PPER", "VMFIN", "VAFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So hab' ich alles satt:", "tokens": ["So", "hab'", "ich", "al\u00b7les", "satt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIS", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Den Himmel lass' ich fahren", "tokens": ["Den", "Him\u00b7mel", "lass'", "ich", "fah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mit seiner Zierlicheit", "tokens": ["Mit", "sei\u00b7ner", "Zier\u00b7lic\u00b7heit"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Kan ich nur Dich bewahren", "tokens": ["Kan", "ich", "nur", "Dich", "be\u00b7wah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Im Hertzen allezeit.", "tokens": ["Im", "Hert\u00b7zen", "al\u00b7le\u00b7zeit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Der Erden Krei\u00df mag prangen,", "tokens": ["Der", "Er\u00b7den", "Krei\u00df", "mag", "pran\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VMFIN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Er irrt nicht meinen Muth,", "tokens": ["Er", "irrt", "nicht", "mei\u00b7nen", "Muth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wenn ich nur Dich kan fangen", "tokens": ["Wenn", "ich", "nur", "Dich", "kan", "fan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "PPER", "VMFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Du unersch\u00f6pfftes Gut.", "tokens": ["Du", "un\u00b7er\u00b7sch\u00f6pff\u00b7tes", "Gut", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Solt' ich dar\u00fcber kommen", "tokens": ["Solt'", "ich", "da\u00b7r\u00fc\u00b7ber", "kom\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PAV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Auch in Gefahr und Noht,", "tokens": ["Auch", "in", "Ge\u00b7fahr", "und", "Noht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Vnd w\u00fcrde mitgenommen", "tokens": ["Vnd", "w\u00fcr\u00b7de", "mit\u00b7ge\u00b7nom\u00b7men"], "token_info": ["word", "word", "word"], "pos": ["KON", "VAFIN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Durch Armut, Tr\u00fcbsal, Tod,", "tokens": ["Durch", "Ar\u00b7mut", ",", "Tr\u00fcb\u00b7sal", ",", "Tod", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Es wolt' in mir verschmachten", "tokens": ["Es", "wolt'", "in", "mir", "ver\u00b7schmach\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Leib, Seel' und was ich bin,", "tokens": ["Leib", ",", "Seel'", "und", "was", "ich", "bin", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "PWS", "PPER", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wil ich es doch nicht achten", "tokens": ["Wil", "ich", "es", "doch", "nicht", "ach\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PPER", "ADV", "PTKNEG", "VVINF"], "meter": "---+-+-", "measure": "unknown.measure.di"}, "line.4": {"text": "Aus unverwandtem Sinn:", "tokens": ["Aus", "un\u00b7ver\u00b7wand\u00b7tem", "Sinn", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die\u00df nimmt mir alle Schmertzen,", "tokens": ["Die\u00df", "nimmt", "mir", "al\u00b7le", "Schmert\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Da\u00df Du, mein s\u00fcsses Heil,", "tokens": ["Da\u00df", "Du", ",", "mein", "s\u00fcs\u00b7ses", "Heil", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Der Trost bist meinem Hertzen", "tokens": ["Der", "Trost", "bist", "mei\u00b7nem", "Hert\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Vnd mein gew\u00fcnschtes Theil.", "tokens": ["Vnd", "mein", "ge\u00b7w\u00fcnschtes", "Theil", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}