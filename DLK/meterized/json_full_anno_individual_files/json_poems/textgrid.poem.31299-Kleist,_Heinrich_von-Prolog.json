{"textgrid.poem.31299": {"metadata": {"author": {"name": "Kleist, Heinrich von", "birth": "N.A.", "death": "N.A."}, "title": "Prolog", "genre": "verse", "period": "N.A.", "pub_year": 1794, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wettre hinein, o du, mit deinen flammenden Rossen,", "tokens": ["Wett\u00b7re", "hin\u00b7ein", ",", "o", "du", ",", "mit", "dei\u00b7nen", "flam\u00b7men\u00b7den", "Ros\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "FM", "PPER", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Ph\u00f6bus, Bringer des Tags, in den unendlichen Raum!", "tokens": ["Ph\u00f6\u00b7bus", ",", "Brin\u00b7ger", "des", "Tags", ",", "in", "den", "un\u00b7end\u00b7li\u00b7chen", "Raum", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "ART", "NN", "$,", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+--+---+--+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Gib den Horen dich hin! Nicht um dich, neben, noch r\u00fcckw\u00e4rts,", "tokens": ["Gib", "den", "Ho\u00b7ren", "dich", "hin", "!", "Nicht", "um", "dich", ",", "ne\u00b7ben", ",", "noch", "r\u00fcck\u00b7w\u00e4rts", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "PPER", "PTKVZ", "$.", "PTKNEG", "APPR", "PPER", "$,", "APPR", "$,", "ADV", "ADV", "$,"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.4": {"text": "Vorw\u00e4rts wende den Blick, wo das Geschwader sich regt!", "tokens": ["Vor\u00b7w\u00e4rts", "wen\u00b7de", "den", "Blick", ",", "wo", "das", "Ge\u00b7schwa\u00b7der", "sich", "regt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "PWAV", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "+-+--+---+--+", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Donnr' einher, gleichviel, ob \u00fcber die L\u00e4nder der Menschen,", "tokens": ["Donn\u00b7r'", "ein\u00b7her", ",", "gleich\u00b7viel", ",", "ob", "\u00fc\u00b7ber", "die", "L\u00e4n\u00b7der", "der", "Men\u00b7schen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "VVFIN", "$,", "KOUS", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Achtlos, welchem du steigst, welchem Geschlecht du versinkst,", "tokens": ["Acht\u00b7los", ",", "wel\u00b7chem", "du", "steigst", ",", "wel\u00b7chem", "Ge\u00b7schlecht", "du", "ver\u00b7sinkst", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "PPER", "VVFIN", "$,", "PWAT", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.7": {"text": "Hier jetzt lenke, jetzt dort, so wie die Faust sich dir stellet,", "tokens": ["Hier", "jetzt", "len\u00b7ke", ",", "jetzt", "dort", ",", "so", "wie", "die", "Faust", "sich", "dir", "stel\u00b7let", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "$,", "ADV", "ADV", "$,", "ADV", "KOKOM", "ART", "NN", "PRF", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Weil die Kraft dich, der Kraft spielende \u00dcbung, erfreut.", "tokens": ["Weil", "die", "Kraft", "dich", ",", "der", "Kraft", "spie\u00b7len\u00b7de", "\u00dc\u00b7bung", ",", "er\u00b7freut", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "$,", "ART", "NN", "ADJA", "NN", "$,", "VVFIN", "$."], "meter": "--+--++--+--+", "measure": "anapaest.di.plus"}, "line.9": {"text": "Fehlen nicht wirst du, du triffst, es ist der Tanz um die Erde,", "tokens": ["Feh\u00b7len", "nicht", "wirst", "du", ",", "du", "triffst", ",", "es", "ist", "der", "Tanz", "um", "die", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "VAFIN", "PPER", "$,", "PPER", "VVFIN", "$,", "PPER", "VAFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+--++-+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.10": {"text": "Und auch vom Wartturm entdeckt unten ein Sp\u00e4her das Ma\u00df.", "tokens": ["Und", "auch", "vom", "Wart\u00b7turm", "ent\u00b7deckt", "un\u00b7ten", "ein", "Sp\u00e4\u00b7her", "das", "Ma\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "NN", "VVPP", "ADV", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+--+---+--+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Wettre hinein, o du, mit deinen flammenden Rossen,", "tokens": ["Wett\u00b7re", "hin\u00b7ein", ",", "o", "du", ",", "mit", "dei\u00b7nen", "flam\u00b7men\u00b7den", "Ros\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "FM", "PPER", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Ph\u00f6bus, Bringer des Tags, in den unendlichen Raum!", "tokens": ["Ph\u00f6\u00b7bus", ",", "Brin\u00b7ger", "des", "Tags", ",", "in", "den", "un\u00b7end\u00b7li\u00b7chen", "Raum", "!"], "