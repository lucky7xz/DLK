{"textgrid.poem.41938": {"metadata": {"author": {"name": "Schulze, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Entschwunden ist der Pfad, den ich erkohr;", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Entschwunden ist der Pfad, den ich erkohr;", "tokens": ["Ent\u00b7schwun\u00b7den", "ist", "der", "Pfad", ",", "den", "ich", "er\u00b7kohr", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der Wald verschr\u00e4nkt sich stets mit dichtern Zweigen;", "tokens": ["Der", "Wald", "ver\u00b7schr\u00e4nkt", "sich", "stets", "mit", "dich\u00b7tern", "Zwei\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Kein Ausgang aus der W\u00fcste will sich zeigen,", "tokens": ["Kein", "Aus\u00b7gang", "aus", "der", "W\u00fcs\u00b7te", "will", "sich", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "ART", "NN", "VMFIN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und finstrer sinkt der n\u00e4cht'ge Wolkenflor.", "tokens": ["Und", "finst\u00b7rer", "sinkt", "der", "n\u00e4cht'\u00b7ge", "Wol\u00b7ken\u00b7flor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und tret' ich oft auch aus dem Hain hervor,", "tokens": ["Und", "tret'", "ich", "oft", "auch