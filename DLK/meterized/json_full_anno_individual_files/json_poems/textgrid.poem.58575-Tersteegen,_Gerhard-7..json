{"textgrid.poem.58575": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "7.", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Buchstablehrer und Vernunft sind die Toten, die man fraget,", "tokens": ["Buch\u00b7stab\u00b7leh\u00b7rer", "und", "Ver\u00b7nunft", "sind", "die", "To\u00b7ten", ",", "die", "man", "fra\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VAFIN", "ART", "NN", "$,", "PRELS", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Man hat Gott nicht n\u00f6tig mehr, ob der Mund gleich anders saget;", "tokens": ["Man", "hat", "Gott", "nicht", "n\u00f6\u00b7tig", "mehr", ",", "ob", "der", "Mund", "gleich", "an\u00b7ders", "sa\u00b7get", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "NN", "PTKNEG", "ADJD", "ADV", "$,", "KOUS", "ART", "NN", "ADV", "ADV", "VVFIN", "$."], "meter": "-+--+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Immer fragt man ohne Tun, \u00e4u\u00dfres Lehren acht't man g'nug,", "tokens": ["Im\u00b7mer", "fragt", "man", "oh\u00b7ne", "Tun", ",", "\u00e4u\u00df\u00b7res", "Leh\u00b7ren", "acht't", "man", "g'\u00b7nug", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "APPR", "NN", "$,", "ADJA", "NN", "VVFIN", "PIS", "ADV", "$,"], "meter": "+-+-+-+--+-+--+", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Wenn ein Herz Gott selber fragt, ruft Vernunft, es sei Betrug.", "tokens": ["Wenn", "ein", "Herz", "Gott", "sel\u00b7ber", "fragt", ",", "ruft", "Ver\u00b7nunft",