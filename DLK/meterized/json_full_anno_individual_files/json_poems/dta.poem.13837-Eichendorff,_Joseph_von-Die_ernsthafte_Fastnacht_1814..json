{"dta.poem.13837": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Die ernsthafte Fastnacht  1814.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "\u201esieh, wir werben lang' verstohlen", "tokens": ["\u201e", "sieh", ",", "wir", "wer\u00b7ben", "lang'", "ver\u00b7stoh\u00b7len"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "VVIMP", "$,", "PPER", "VAFIN", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schon um Dich in Noth und Tod,", "tokens": ["Schon", "um", "Dich", "in", "Noth", "und", "Tod", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PRF", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Komm! sonst wollen wir Dich holen,", "tokens": ["Komm", "!", "sonst", "wol\u00b7len", "wir", "Dich", "ho\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "ADV", "VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wann der Mond scheint blutigroth!\u201c", "tokens": ["Wann", "der", "Mond", "scheint", "blu\u00b7ti\u00b7groth", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "ADJD", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}