{"textgrid.poem.56460": {"metadata": {"author": {"name": "Heym, Georg", "birth": "N.A.", "death": "N.A."}, "title": "1L: Zu vieren sitzen sie am gr\u00fcnen Tische,", "genre": "verse", "period": "N.A.", "pub_year": 1899, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zu vieren sitzen sie am gr\u00fcnen Tische,", "tokens": ["Zu", "vie\u00b7ren", "sit\u00b7zen", "sie", "am", "gr\u00fc\u00b7nen", "Ti\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Verschanzt in seines Daches hohe Kanten.", "tokens": ["Ver\u00b7schanzt", "in", "sei\u00b7nes", "Da\u00b7ches", "ho\u00b7he", "Kan\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Kahlk\u00f6pfig hocken sie in den Folianten,", "tokens": ["Kahl\u00b7k\u00f6p\u00b7fig", "ho\u00b7cken", "sie", "in", "den", "Fo\u00b7li\u00b7an\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Wie auf dem Aas die alten Tintenfische.", "tokens": ["Wie", "auf", "dem", "Aas", "die", "al\u00b7ten", "Tin\u00b7ten\u00b7fi\u00b7sche", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Manchmal erscheinen H\u00e4nde, die bedreckten", "tokens": ["Manch\u00b7mal", "er\u00b7schei\u00b7nen", "H\u00e4n\u00b7de", ",", "die", "be\u00b7dreck\u00b7ten"], "token_info":