{"textgrid.poem.61991": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "Der Leuchtturm", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Morgen, ein sch\u00f6ner Morgen bricht an,", "tokens": ["Ein", "Mor\u00b7gen", ",", "ein", "sch\u00f6\u00b7ner", "Mor\u00b7gen", "bricht", "an", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ein Morgen voll goldener Sonnen!", "tokens": ["Ein", "Mor\u00b7gen", "voll", "gol\u00b7de\u00b7ner", "Son\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Es reifen die herrlichsten Fr\u00fcchte alsdann,", "tokens": ["Es", "rei\u00b7fen", "die", "herr\u00b7lichs\u00b7ten", "Fr\u00fcch\u00b7te", "als\u00b7dann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Von ewiger Dauer umsponnen.", "tokens": ["Von", "e\u00b7wi\u00b7ger", "Dau\u00b7er", "um\u00b7spon\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "Ein Morgen, ein sch\u00f6ner Morgen bricht an,", "tokens": ["Ein", "Mor\u00b7gen", ",", "ein", "sch\u00f6\u00b7ner", "Mor\u00b7gen", "bricht", "an", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "VVFIN",