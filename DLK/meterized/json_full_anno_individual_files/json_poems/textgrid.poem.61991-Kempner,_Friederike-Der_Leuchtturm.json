, "tokens": ["D'\u00b7ran", "zweif\u00b7le", "ich", "\u2013", "Gott", "wei\u00df", "es", "\u2013", "nicht", "ger\u00b7ne", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "$(", "NN", "VVFIN", "PPER", "$(", "PTKNEG", "ADV", "$."], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}}, "stanza.13": {"line.1": {"text": "Doch, da\u00df ihn dereinst \u2013 ja, da\u00df ihn dereinst", "tokens": ["Doch", ",", "da\u00df", "ihn", "de\u00b7reinst", "\u2013", "ja", ",", "da\u00df", "ihn", "de\u00b7reinst"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KON", "$,", "KOUS", "PPER", "ADV", "$(", "PTKANT", "$,", "KOUS", "PPER", "ADV"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Das Schifflein noch jubelnd begr\u00fc\u00dfe, \u2013", "tokens": ["Das", "Schif\u00b7flein", "noch", "ju\u00b7belnd", "be\u00b7gr\u00fc\u00b7\u00dfe", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "VVFIN", "$,", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "O k\u00fcnftige Mannschaft, ich wei\u00df es \u2013 Du weinst \u2013", "tokens": ["O", "k\u00fcnf\u00b7ti\u00b7ge", "Mann\u00b7schaft", ",", "ich", "wei\u00df", "es", "\u2013", "Du", "weinst", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "PPER", "VVFIN", "PPER", "$(", "PPER", "VVFIN", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Alsdann erst die Tr\u00e4ne, die s\u00fc\u00dfe.", "tokens": ["Als\u00b7dann", "erst", "die", "Tr\u00e4\u00b7ne", ",", "die", "s\u00fc\u00b7\u00dfe", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "$,", "ART", "ADJA", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.14": {"line.1": {"text": "Der fr\u00fcheren, vorigen Mannschaft geweiht,", "tokens": ["Der", "fr\u00fc\u00b7he\u00b7ren", ",", "vo\u00b7ri\u00b7gen", "Mann\u00b7schaft", "ge\u00b7weiht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Die strandend das Licht noch erblickte;", "tokens": ["Die", "stran\u00b7dend", "das", "Licht", "noch", "er\u00b7blick\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Das herrliche Licht der Br\u00fcderlichkeit,", "tokens": ["Das", "herr\u00b7li\u00b7che", "Licht", "der", "Br\u00fc\u00b7der\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Trotzdem sie die Finsternis dr\u00fcckte.", "tokens": ["Trotz\u00b7dem", "sie", "die", "Fins\u00b7ter\u00b7nis", "dr\u00fcck\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}}}}}