{"textgrid.poem.53058": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "Christoff Schimmelfennig und Anna von Weinbeer", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich lobe, die allhie der Zeit", "tokens": ["Ich", "lo\u00b7be", ",", "die", "all\u00b7hie", "der", "Zeit"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PRELS", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In Fr\u00f6ligkeit geniessen,", "tokens": ["In", "Fr\u00f6\u00b7lig\u00b7keit", "ge\u00b7nies\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Daferne nur kein heimlich Leid", "tokens": ["Da\u00b7fer\u00b7ne", "nur", "kein", "heim\u00b7lich", "Leid"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PIAT", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Beschweret das Gewissen,", "tokens": ["Be\u00b7schwe\u00b7ret", "das", "Ge\u00b7wis\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sie haben vber Geld vnd Gutt", "tokens": ["Sie", "ha\u00b7ben", "vber", "Geld", "vnd", "Gutt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "NN", "KON", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Ein h\u00f6hers sich erwehlet,", "tokens": ["Ein", "h\u00f6\u00b7hers", "sich", "er\u00b7weh\u00b7let", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "PRF", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Sind noch so wol, al\u00df welcher Muth", "tokens": ["Sind", "noch", "so", "wol", ",", "al\u00df", "wel\u00b7cher", "Muth"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ADV", "ADV", "$,", "KOUS", "PWAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Sich stets mit Sorgen quelet.", "tokens": ["Sich", "stets", "mit", "Sor\u00b7gen", "que\u00b7let", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Es finden sich schon gar zu viel", "tokens": ["Es", "fin\u00b7den", "sich", "schon", "gar", "zu", "viel"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "ADV", "PTKA", "PIS"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der tr\u00fcben Trawer-Tage,", "tokens": ["Der", "tr\u00fc\u00b7ben", "Tra\u00b7wer\u00b7Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das Vngl\u00fcck seet au\u00df ohne Ziel", "tokens": ["Das", "Vn\u00b7gl\u00fcck", "seet", "au\u00df", "oh\u00b7ne", "Ziel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Saamen seiner Plage,", "tokens": ["Den", "Saa\u00b7men", "sei\u00b7ner", "Pla\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wer dennoch ausser dieser Zeit", "tokens": ["Wer", "den\u00b7noch", "aus\u00b7ser", "die\u00b7ser", "Zeit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sich stets mit Hummeln schl\u00e4get,", "tokens": ["Sich", "stets", "mit", "Hum\u00b7meln", "schl\u00e4\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ist der nicht th\u00f6richt, weil er Leid,", "tokens": ["Ist", "der", "nicht", "th\u00f6\u00b7richt", ",", "weil", "er", "Leid", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "PTKNEG", "ADJD", "$,", "KOUS", "PPER", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Da nicht von n\u00f6then, tr\u00e4get?", "tokens": ["Da", "nicht", "von", "n\u00f6\u00b7then", ",", "tr\u00e4\u00b7get", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "PTKNEG", "APPR", "ADJA", "$,", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Die Jahre wissen keinen halt,", "tokens": ["Die", "Jah\u00b7re", "wis\u00b7sen", "kei\u00b7nen", "halt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIAT", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie achten keiner Z\u00fcgel,", "tokens": ["Sie", "ach\u00b7ten", "kei\u00b7ner", "Z\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Mensch wird vnversehens alt,", "tokens": ["Der", "Mensch", "wird", "vn\u00b7ver\u00b7se\u00b7hens", "alt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Al\u00df hett er schnelle Fl\u00fcgel,", "tokens": ["Al\u00df", "hett", "er", "schnel\u00b7le", "Fl\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Was schon der Todt hinweg gebracht,", "tokens": ["Was", "schon", "der", "Todt", "hin\u00b7weg", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "NN", "APZR", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Kan nimmer wieder kommen,", "tokens": ["Kan", "nim\u00b7mer", "wie\u00b7der", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Wird dan in jenner langen Nacht", "tokens": ["Wird", "dan", "in", "jen\u00b7ner", "lan\u00b7gen", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Dein trawrig sein dir frommen?", "tokens": ["Dein", "traw\u00b7rig", "sein", "dir", "from\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "VAINF", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Durch trawren wird der Mensch geschw\u00e4cht,", "tokens": ["Durch", "traw\u00b7ren", "wird", "der", "Mensch", "ge\u00b7schw\u00e4cht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVINF", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Kr\u00e4ffte fallen nieder,", "tokens": ["Die", "Kr\u00e4ff\u00b7te", "fal\u00b7len", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Fr\u00f6ligkeit macht alles recht,", "tokens": ["Die", "Fr\u00f6\u00b7lig\u00b7keit", "macht", "al\u00b7les", "recht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIS", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dann leben erst die Glieder,", "tokens": ["Dann", "le\u00b7ben", "erst", "die", "Glie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Es geht vns niemahls der Gesang", "tokens": ["Es", "geht", "vns", "nie\u00b7mahls", "der", "Ge\u00b7sang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Anmutiger von statten,", "tokens": ["An\u00b7mu\u00b7ti\u00b7ger", "von", "stat\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "So kan der Instrumenten Klang", "tokens": ["So", "kan", "der", "Inst\u00b7ru\u00b7men\u00b7ten", "Klang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ART", "NN", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.8": {"text": "Auch besser nie gerathen.", "tokens": ["Auch", "bes\u00b7ser", "nie", "ge\u00b7ra\u00b7then", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Wer Gott zu forderst sich ergiebt,", "tokens": ["Wer", "Gott", "zu", "for\u00b7derst", "sich", "er\u00b7giebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "PTKZU", "VVFIN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vnd nimmer von ihm weichet,", "tokens": ["Vnd", "nim\u00b7mer", "von", "ihm", "wei\u00b7chet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Auch nachmahls eine Seele liebt,", "tokens": ["Auch", "nach\u00b7mahls", "ei\u00b7ne", "See\u00b7le", "liebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die seiner Seelen gleichet,", "tokens": ["Die", "sei\u00b7ner", "See\u00b7len", "glei\u00b7chet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Mag wol gewehnen sein Gem\u00fcth", "tokens": ["Mag", "wol", "ge\u00b7weh\u00b7nen", "sein", "Ge\u00b7m\u00fcth"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "ADJA", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In Fr\u00f6ligkeit zu leben,", "tokens": ["In", "Fr\u00f6\u00b7lig\u00b7keit", "zu", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Die dan ein Gla\u00df vnd ein sch\u00f6n Lied", "tokens": ["Die", "dan", "ein", "Gla\u00df", "vnd", "ein", "sch\u00f6n", "Lied"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ART", "NN", "KON", "ART", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Vollauff vn\u00df k\u00f6nnen geben.", "tokens": ["Vol\u00b7lauff", "vn\u00df", "k\u00f6n\u00b7nen", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Weg, weg, du rauhe Trawrigkeit,", "tokens": ["Weg", ",", "weg", ",", "du", "rau\u00b7he", "Traw\u00b7rig\u00b7keit", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PTKVZ", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du Hertz-Bedrengerinne,", "tokens": ["Du", "Hertz\u00b7Bed\u00b7ren\u00b7ge\u00b7rin\u00b7ne", ","], "token_info": ["word", "word", "punct"], "pos": ["PPER", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Komm, angenehme Fr\u00f6ligkeit,", "tokens": ["Komm", ",", "an\u00b7ge\u00b7neh\u00b7me", "Fr\u00f6\u00b7lig\u00b7keit", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du Sonne meiner Sinne,", "tokens": ["Du", "Son\u00b7ne", "mei\u00b7ner", "Sin\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Mein Leben soll in dir bestehn,", "tokens": ["Mein", "Le\u00b7ben", "soll", "in", "dir", "be\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Es lieben die Gem\u00fcther,", "tokens": ["Es", "lie\u00b7ben", "die", "Ge\u00b7m\u00fc\u00b7ther", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Die nicht nur an der Erden gehn,", "tokens": ["Die", "nicht", "nur", "an", "der", "Er\u00b7den", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "ADV", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Dich vber alle G\u00fcter.", "tokens": ["Dich", "vber", "al\u00b7le", "G\u00fc\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "Ich lobe, die allhie der Zeit", "tokens": ["Ich", "lo\u00b7be", ",", "die", "all\u00b7hie", "der", "Zeit"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PRELS", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In Fr\u00f6ligkeit geniessen,", "tokens": ["In", "Fr\u00f6\u00b7lig\u00b7keit", "ge\u00b7nies\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Daferne nur kein heimlich Leid", "tokens": ["Da\u00b7fer\u00b7ne", "nur", "kein", "heim\u00b7lich", "Leid"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PIAT", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Beschweret das Gewissen,", "tokens": ["Be\u00b7schwe\u00b7ret", "das", "Ge\u00b7wis\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sie haben vber Geld vnd Gutt", "tokens": ["Sie", "ha\u00b7ben", "vber", "Geld", "vnd", "Gutt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "NN", "KON", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Ein h\u00f6hers sich erwehlet,", "tokens": ["Ein", "h\u