{"dta.poem.21392": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Caput XX.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-30602-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Mutter aber fing wieder an", "tokens": ["Die", "Mut\u00b7ter", "a\u00b7ber", "fing", "wie\u00b7der", "an"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VVFIN", "ADV", "PTKVZ"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Zu fragen sehr vergn\u00fcglich,", "tokens": ["Zu", "fra\u00b7gen", "sehr", "ver\u00b7gn\u00fcg\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ADV", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Nach tausend Dingen, mitunter sogar", "tokens": ["Nach", "tau\u00b7send", "Din\u00b7gen", ",", "mi\u00b7tun\u00b7ter", "so\u00b7gar"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "CARD", "NN", "$,", "ADV", "ADV"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Nach Dingen die sehr anz\u00fcglich.", "tokens": ["Nach", "Din\u00b7gen", "die", "sehr", "an\u00b7z\u00fcg\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "ADV", "ADJD", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "\u201emein liebes Kind! wie denkst du jetzt?", "tokens": ["\u201e", "mein", "lie\u00b7bes", "Kind", "!", "wie", "denkst", "du", "jetzt", "?"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "ADJA", "NN", "$.", "PWAV", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Treibst du noch immer aus Neigung", "tokens": ["Treibst", "du", "noch", "im\u00b7mer", "aus", "Nei\u00b7gung"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "APPR", "NN"], "meter": "+-++--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Die Politik? Zu welcher Parthey", "tokens": ["Die", "Po\u00b7li\u00b7tik", "?", "Zu", "wel\u00b7cher", "Par\u00b7they"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$.", "APPR", "PWAT", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Geh\u00f6rst du mit Ueberzeugung?\u201c", "tokens": ["Ge\u00b7h\u00f6rst", "du", "mit", "Ue\u00b7ber\u00b7zeu\u00b7gung", "?", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "$.", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Die Apfelsinen, lieb M\u00fctterlein,", "tokens": ["Die", "Ap\u00b7fel\u00b7si\u00b7nen", ",", "lieb", "M\u00fct\u00b7ter\u00b7lein", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sind gut, und mit wahrem Vergn\u00fcgen", "tokens": ["Sind", "gut", ",", "und", "mit", "wah\u00b7rem", "Ver\u00b7gn\u00fc\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "ADJD", "$,", "KON", "APPR", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Verschlucke ich den s\u00fc\u00dfen Saft,", "tokens": ["Ver\u00b7schlu\u00b7cke", "ich", "den", "s\u00fc\u00b7\u00dfen", "Saft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und ich lasse die Schaalen liegen.", "tokens": ["Und", "ich", "las\u00b7se", "die", "Schaa\u00b7len", "lie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "VVFIN", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}}}}}