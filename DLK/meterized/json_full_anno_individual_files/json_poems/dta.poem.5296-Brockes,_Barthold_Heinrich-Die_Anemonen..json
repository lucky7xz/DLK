"$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.6": {"text": "Woran wir die Sinne laben,", "tokens": ["Wo\u00b7ran", "wir", "die", "Sin\u00b7ne", "la\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Welche wir von dir allein,", "tokens": ["Wel\u00b7che", "wir", "von", "dir", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "PPER", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Grosser GOTT, empfangen haben,", "tokens": ["Gros\u00b7ser", "GoTT", ",", "emp\u00b7fan\u00b7gen", "ha\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "VVPP", "VAINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Danckbar und erk\u00e4nntlich seyn!", "tokens": ["Dan\u00b7ck\u00b7bar", "und", "er\u00b7k\u00e4nnt\u00b7lich", "seyn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "VAINF", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.10": {"text": "M\u00f6gt ich doch, o ew\u2019ge G\u00fcte,", "tokens": ["M\u00f6gt", "ich", "doch", ",", "o", "ew'\u00b7ge", "G\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "$,", "FM", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Die mir so viel Guts gegeben,", "tokens": ["Die", "mir", "so", "viel", "Guts", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "PIAT", "NN", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.12": {"text": "Mit erk\u00e4nntlichem Gem\u00fchte", "tokens": ["Mit", "er\u00b7k\u00e4nnt\u00b7li\u00b7chem", "Ge\u00b7m\u00fch\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Dir gef\u00e4llig hier zu leben", "tokens": ["Dir", "ge\u00b7f\u00e4l\u00b7lig", "hier", "zu", "le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADJD", "ADV", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Recht von Hertzen mich bestreben!", "tokens": ["Recht", "von", "Hert\u00b7zen", "mich", "be\u00b7stre\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}