i"}, "line.25": {"text": "Als wenn, von buntem Licht,", "tokens": ["Als", "wenn", ",", "von", "bun\u00b7tem", "Licht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.26": {"text": "Auf gr\u00fcnen sammtenen Tapeten,", "tokens": ["Auf", "gr\u00fc\u00b7nen", "samm\u00b7te\u00b7nen", "Ta\u00b7pe\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Ein Blum-Werck sch\u00f6n gewircket w\u00e4r.", "tokens": ["Ein", "Blum\u00b7\u00b7Werck", "sch\u00f6n", "ge\u00b7wir\u00b7cket", "w\u00e4r", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Die allerreichsten Kaufmanns-Laden,", "tokens": ["Die", "al\u00b7ler\u00b7reichs\u00b7ten", "Kauf\u00b7manns\u00b7La\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "Voll g\u00fcld- und silberner Brocaden,", "tokens": ["Voll", "g\u00fcld", "und", "sil\u00b7ber\u00b7ner", "Bro\u00b7ca\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "TRUNC", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "Und wenn sie noch so sch\u00f6n gestickt, durchwirckt, bebr\u00e4mt,", "tokens": ["Und", "wenn", "sie", "noch", "so", "sch\u00f6n", "ge\u00b7stickt", ",", "durch\u00b7wirckt", ",", "be\u00b7br\u00e4mt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "ADV", "ADJD", "VVPP", "$,", "VVPP", "$,", "VVFIN", "$,"], "