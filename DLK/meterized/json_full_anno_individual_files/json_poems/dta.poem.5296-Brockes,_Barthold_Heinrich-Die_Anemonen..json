{"dta.poem.5296": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Anemonen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mir hat, GOtt Lob! der Schmcuk, der bunte Blu-\nmen ziert,", "tokens": ["Mir", "hat", ",", "Gott", "Lob", "!", "der", "Schm\u00b7cuk", ",", "der", "bun\u00b7te", "Blu", "men", "ziert", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "NN", "NN", "$.", "ART", "NN", "$,", "ART", "ADJA", "TRUNC", "NN", "VVFIN", "$,"], "meter": "+-+--+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Das innerste der Seelen oft ger\u00fchrt;", "tokens": ["Das", "in\u00b7ners\u00b7te", "der", "See\u00b7len", "oft", "ge\u00b7r\u00fchrt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ART", "NN", "ADV", "VVPP", "$."], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Allein, die\u00df ist gewi\u00df: noch nimmer", "tokens": ["Al\u00b7lein", ",", "die\u00df", "ist", "ge\u00b7wi\u00df", ":", "noch", "nim\u00b7mer"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "$,", "PDS", "VAFIN", "ADV", "$.", "ADV", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bin ich von ihrem Glantz und angenehmen Schimmer,", "tokens": ["Bin", "ich", "von", "ih\u00b7rem", "Glantz", "und", "an\u00b7ge\u00b7neh\u00b7men", "Schim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "PPOSAT", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Von ihrer Zierlichkeit und tausendfacher Pracht", "tokens": ["Von", "ih\u00b7rer", "Zier\u00b7lich\u00b7keit", "und", "tau\u00b7send\u00b7fa\u00b7cher", "Pracht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So lieblich angestrahlt und fast best\u00fcrtzt gemacht,", "tokens": ["So", "lieb\u00b7lich", "an\u00b7ge\u00b7strahlt", "und", "fast", "be\u00b7st\u00fcrtzt", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "KON", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Als heute, da ich kaum im Garten eingetreten,", "tokens": ["Als", "heu\u00b7te", ",", "da", "ich", "kaum", "im", "Gar\u00b7ten", "ein\u00b7ge\u00b7tre\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "$,", "KOUS", "PPER", "ADV", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und mir ein buntes Feur von Anemonen-Beeten", "tokens": ["Und", "mir", "ein", "bun\u00b7tes", "Feur", "von", "A\u00b7ne\u00b7mo\u00b7nen\u00b7Bee\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "ART", "ADJA", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Das Aug\u2019 auf einmahl traf. Ich stutzt\u2019! Es blieb mein", "tokens": ["Das", "Aug'", "auf", "ein\u00b7mahl", "traf", ".", "Ich", "stutzt'", "!", "Es", "blieb", "mein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADV", "VVFIN", "$.", "PPER", "VVFIN", "$.", "PPER", "VVFIN", "PPOSAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Der halb gehoben war, so, halb gehoben, stehn,", "tokens": ["Der", "halb", "ge\u00b7ho\u00b7ben", "war", ",", "so", ",", "halb", "ge\u00b7ho\u00b7ben", ",", "stehn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJD", "VVPP", "VAFIN", "$,", "ADV", "$,", "ADJD", "VVPP", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und kont ich, vor der Pracht und Sch\u00f6nheit Ueberflu\u00df,", "tokens": ["Und", "kont", "ich", ",", "vor", "der", "Pracht", "und", "Sch\u00f6n\u00b7heit", "Ue\u00b7berf\u00b7lu\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "$,", "APPR", "ART", "NN", "KON", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Der mich recht blendete, nicht vor- nicht r\u00fcckw\u00e4rts gehn.", "tokens": ["Der", "mich", "recht", "blen\u00b7de\u00b7te", ",", "nicht", "vor", "nicht", "r\u00fcck\u00b7w\u00e4rts", "gehn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVFIN", "$,", "PTKNEG", "TRUNC", "PTKNEG", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "\u201eliebster GOTT! kan wol auf Erden", "tokens": ["\u201e", "liebs\u00b7ter", "GoTT", "!", "kan", "wol", "auf", "Er\u00b7den"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "ADJA", "NN", "$.", "VMFIN", "ADV", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "\u201eetwas, das so wundersch\u00f6n,", "tokens": ["\u201e", "et\u00b7was", ",", "das", "so", "wun\u00b7der\u00b7sch\u00f6n", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "$,", "PRELS", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "\u201ebunt und zierlich ist, gesehn;", "tokens": ["\u201e", "bunt", "und", "zier\u00b7lich", "ist", ",", "ge\u00b7sehn", ";"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "ADJD", "KON", "ADJD", "VAFIN", "$,", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.16": {"text": "\u201elieblichers gefunden werden?", "tokens": ["\u201e", "lieb\u00b7li\u00b7chers", "ge\u00b7fun\u00b7den", "wer\u00b7den", "?"], "token_info": ["punct", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVPP", "VAINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "So rief ich, halb entz\u00fcckt vor \u00fcberh\u00e4ufter Lust,", "tokens": ["So", "rief", "ich", ",", "halb", "ent\u00b7z\u00fcckt", "vor", "\u00fc\u00b7berh\u00b7\u00e4uf\u00b7ter", "Lust", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADJD", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Und lie\u00df den frohen Blick den bunt-gef\u00e4rbten Hauffen", "tokens": ["Und", "lie\u00df", "den", "fro\u00b7hen", "Blick", "den", "bunt\u00b7ge\u00b7f\u00e4rb\u00b7ten", "Hauf\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Auf einmahl \u00fcberlauffen.", "tokens": ["Auf", "ein\u00b7mahl", "\u00fc\u00b7berl\u00b7auf\u00b7fen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.20": {"text": "Ein gleichsam bunter Schwall drang in die rege Brust,", "tokens": ["Ein", "gleich\u00b7sam", "bun\u00b7ter", "Schwall", "drang", "in", "die", "re\u00b7ge", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.21": {"text": "Mit einem tausendfach-gef\u00e4rbten Glantz und Schein,", "tokens": ["Mit", "ei\u00b7nem", "tau\u00b7send\u00b7fach\u00b7ge\u00b7f\u00e4rb\u00b7ten", "Glantz", "und", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "So heftig und so kr\u00e4ftig ein,", "tokens": ["So", "hef\u00b7tig", "und", "so", "kr\u00e4f\u00b7tig", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADV", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Da\u00df fast die Seele selbst, von Anmuth \u00fcberh\u00e4uft,", "tokens": ["Da\u00df", "fast", "die", "See\u00b7le", "selbst", ",", "von", "An\u00b7muth", "\u00fc\u00b7berh\u00b7\u00e4uft", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "ADV", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Vor \u00fcberm\u00e4\u00dfigem Vergn\u00fcgen, nichts begreift,", "tokens": ["Vor", "\u00fc\u00b7berm\u00b7\u00e4\u00b7\u00dfi\u00b7gem", "Ver\u00b7gn\u00fc\u00b7gen", ",", "nichts", "be\u00b7greift", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Und nur, wie alles hier bunt durch einander spielet,", "tokens": ["Und", "nur", ",", "wie", "al\u00b7les", "hier", "bunt", "durch", "ein\u00b7an\u00b7der", "spie\u00b7let", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "PWAV", "PIS", "ADV", "ADJD", "APPR", "PRF", "VVFIN", "$,"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.26": {"text": "In einer lieblichen Verwirrung, fr\u00f6lich f\u00fchlet.", "tokens": ["In", "ei\u00b7ner", "lieb\u00b7li\u00b7chen", "Ver\u00b7wir\u00b7rung", ",", "fr\u00f6\u00b7lich", "f\u00fch\u00b7let", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Es war der Anemonen Zier", "tokens": ["Es", "war", "der", "A\u00b7ne\u00b7mo\u00b7nen", "Zier"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So lieblich bunt, so w\u00fcrdig sch\u00f6n,", "tokens": ["So", "lieb\u00b7lich", "bunt", ",", "so", "w\u00fcr\u00b7dig", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJD", "$,", "ADV", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df ich, indem ich sie betrachte", "tokens": ["Da\u00df", "ich", ",", "in\u00b7dem", "ich", "sie", "be\u00b7trach\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "$,", "KOUS", "PPER", "PPER", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit inniglichen Freuden dachte,", "tokens": ["Mit", "in\u00b7nig\u00b7li\u00b7chen", "Freu\u00b7den", "dach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Mein GOTT, wie fr\u00f6lich danck ich dir,", "tokens": ["Mein", "GoTT", ",", "wie", "fr\u00f6\u00b7lich", "danck", "ich", "dir", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PWAV", "ADJD", "APPR", "PPER", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df du mir Augen gabst zu sehn!", "tokens": ["Da\u00df", "du", "mir", "Au\u00b7gen", "gabst", "zu", "sehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "NN", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wenn ich nichts anders sonst erblicket", "tokens": ["Wenn", "ich", "nichts", "an\u00b7ders", "sonst", "er\u00b7bli\u00b7cket"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PIS", "ADV", "ADV", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Auf dieser Welt, als blos allein", "tokens": ["Auf", "die\u00b7ser", "Welt", ",", "als", "blos", "al\u00b7lein"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "$,", "KOUS", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Den tausendfachen Wunder-Schein,", "tokens": ["Den", "tau\u00b7send\u00b7fa\u00b7chen", "Wun\u00b7der\u00b7Schein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Womit die\u00df Fr\u00fchlings-Kind geschm\u00fccket,", "tokens": ["Wo\u00b7mit", "die\u00df", "Fr\u00fch\u00b7lings\u00b7Kind", "ge\u00b7schm\u00fc\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDS", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "M\u00fcsst\u2019 ich dir billig danckbar seyn.", "tokens": ["M\u00fcsst'", "ich", "dir", "bil\u00b7lig", "dan\u00b7ck\u00b7bar", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "ADJD", "ADJD", "VAINF", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.12": {"text": "Ich zog darauf den nimmer festen Blick", "tokens": ["Ich", "zog", "da\u00b7rauf", "den", "nim\u00b7mer", "fes\u00b7ten", "Blick"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PAV", "ART", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Der um zu viel zu sehn, fast nichtes sah,", "tokens": ["Der", "um", "zu", "viel", "zu", "sehn", ",", "fast", "nich\u00b7tes", "sah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PTKA", "PIS", "PTKZU", "VVINF", "$,", "ADV", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Aus seiner bunten Fahrt, fast mit Gewalt zur\u00fcck,", "tokens": ["Aus", "sei\u00b7ner", "bun\u00b7ten", "Fahrt", ",", "fast", "mit", "Ge\u00b7walt", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,", "ADV", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Um einige genau zu sehen, die mir nah,", "tokens": ["Um", "ei\u00b7ni\u00b7ge", "ge\u00b7nau", "zu", "se\u00b7hen", ",", "die", "mir", "nah", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUI", "PIAT", "ADJD", "PTKZU", "VVINF", "$,", "PRELS", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und fast vor meinen F\u00fcssen stunden.", "tokens": ["Und", "fast", "vor", "mei\u00b7nen", "F\u00fcs\u00b7sen", "stun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Mein GOtt, was hab ich da f\u00fcr Zierlichkeit gefunden,", "tokens": ["Mein", "Gott", ",", "was", "hab", "ich", "da", "f\u00fcr", "Zier\u00b7lich\u00b7keit", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PWS", "VAFIN", "PPER", "ADV", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "F\u00fcr Farben, und f\u00fcr Glantz!", "tokens": ["F\u00fcr", "Far\u00b7ben", ",", "und", "f\u00fcr", "Glantz", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "KON", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.19": {"text": "Es w\u00fcrden selbst aus Edelsteinen", "tokens": ["Es", "w\u00fcr\u00b7den", "selbst", "aus", "E\u00b7del\u00b7stei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Geschnittene Gef\u00e4sse sch\u00f6ner nicht", "tokens": ["Ge\u00b7schnit\u00b7te\u00b7ne", "Ge\u00b7f\u00e4s\u00b7se", "sch\u00f6\u00b7ner", "nicht"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "ADJA", "PTKNEG"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "In mehrem Glantz, in mehrem Licht,", "tokens": ["In", "meh\u00b7rem", "Glantz", ",", "in", "meh\u00b7rem", "Licht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "In bunterm Schmuck verm\u00f6gend seyn zu scheinen,", "tokens": ["In", "bun\u00b7term", "Schmuck", "ver\u00b7m\u00f6\u00b7gend", "seyn", "zu", "schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "VAINF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Als die von der Natur in solcher Zier", "tokens": ["Als", "die", "von", "der", "Na\u00b7tur", "in", "sol\u00b7cher", "Zier"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "APPR", "ART", "NN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Gekleideten gef\u00e4rbten Blumen hier.", "tokens": ["Ge\u00b7klei\u00b7de\u00b7ten", "ge\u00b7f\u00e4rb\u00b7ten", "Blu\u00b7men", "hier", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Zumahlen wenn in heitern Wetter", "tokens": ["Zu\u00b7mah\u00b7len", "wenn", "in", "hei\u00b7tern", "Wet\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Der Sonnen fr\u00fch- und sp\u00e4te Strahlen", "tokens": ["Der", "Son\u00b7nen", "fr\u00fch", "und", "sp\u00e4\u00b7te", "Strah\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "TRUNC", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Die klaren Bl\u00e4slein ihrer Bl\u00e4tter", "tokens": ["Die", "kla\u00b7ren", "Bl\u00e4s\u00b7lein", "ih\u00b7rer", "Bl\u00e4t\u0