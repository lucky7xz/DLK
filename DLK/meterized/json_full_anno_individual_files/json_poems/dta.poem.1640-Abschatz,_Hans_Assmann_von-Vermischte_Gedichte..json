{"dta.poem.1640": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Vermischte  \n Gedichte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Es rede wer was wei\u00df/ und welcher reden soll/", "tokens": ["Es", "re\u00b7de", "wer", "was", "wei\u00df", "/", "und", "wel\u00b7cher", "re\u00b7den", "soll", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PWS", "PWS", "VVFIN", "$(", "KON", "PWAT", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von was er reden mag/ schau ob es stehe wohl/", "tokens": ["Von", "was", "er", "re\u00b7den", "mag", "/", "schau", "ob", "es", "ste\u00b7he", "wohl", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "VVINF", "VMFIN", "$(", "ADJD", "KOUS", "PPER", "VVFIN", "ADV", "$("], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Obs Ort und Zeit verg\u00f6nnt/ obs Ehr und Recht ihn heist/", "tokens": ["Obs", "Ort", "und", "Zeit", "ver\u00b7g\u00f6nnt", "/", "obs", "Ehr", "und", "Recht", "ihn", "heist", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "VVPP", "$(", "KOUS", "NN", "KON", "NN", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Obs ihm die Treu erlaubt. Wer Schweigens sich befleist/", "tokens": ["Obs", "ihm", "die", "Treu", "er\u00b7laubt", ".", "Wer", "Schwei\u00b7gens", "sich", "be\u00b7fleist", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVPP", "$.", "PWS", "NN", "PRF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Heist sittsam/ wo er jung; wofern er alt/ gescheu", "tokens": ["Heist", "sitt\u00b7sam", "/", "wo", "er", "jung", ";", "wo\u00b7fern", "er", "alt", "/", "ge\u00b7scheu"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["VAFIN", "ADJD", "$(", "PWAV", "PPER", "ADJD", "$.", "KOUS", "PPER", "ADJD", "$(", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wo er in Diensten steht/ voll Ehrerbietigkeit/", "tokens": ["Wo", "er", "in", "Diens\u00b7ten", "steht", "/", "voll", "Ehr\u00b7er\u00b7bie\u00b7tig\u00b7keit", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "NN", "VVFIN", "$(", "ADJD", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "(betrifft es eine Frau/ ein Wunder ihrer Zeit/)", "tokens": ["(", "be\u00b7tr\u00b7ifft", "es", "ei\u00b7ne", "Frau", "/", "ein", "Wun\u00b7der", "ih\u00b7rer", "Zeit", "/", ")"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "PPER", "ART", "NN", "$(", "ART", "NN", "PPOSAT", "NN", "$(", "$("], "meter": "--+-+-+-+-+-+", "measure": "anapaest.init"}, "line.8": {"text": "Bey F\u00fcrsten heist es Witz/ bey Hof-Bedienten/ Pflicht/", "tokens": ["Bey", "F\u00fcrs\u00b7ten", "heist", "es", "Witz", "/", "bey", "Hof\u00b7Be\u00b7dien\u00b7ten", "/", "Pflicht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "NN", "$(", "APPR", "NN", "$(", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wen also ziert und hilfft das kluge Schweigen nicht?", "tokens": ["Wen", "al\u00b7so", "ziert", "und", "hilfft", "das", "klu\u00b7ge", "Schwei\u00b7gen", "nicht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "KON", "VVFIN", "ART", "ADJA", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Den Edlen Scelen steht vern\u00fcnfftigs Schweigen an/", "tokens": ["Den", "Ed\u00b7len", "Sce\u00b7len", "steht", "ver\u00b7n\u00fcnff\u00b7tigs", "Schwei\u00b7gen", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJA", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Weil fast kein schlechter Geist das Schwatzen lassen kan/", "tokens": ["Weil", "fast", "kein", "schlech\u00b7ter", "Geist", "das", "Schwat\u00b7zen", "las\u00b7sen", "kan", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PIAT", "ADJA", "NN", "ART", "NN", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "So rauscht durch Ki\u00df und Sand die offt halb trockne Bach;", "tokens": ["So", "rauscht", "durch", "Ki\u00df", "und", "Sand", "die", "offt", "halb", "trock\u00b7ne", "Bach", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "KON", "NN", "ART", "ADV", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Der allertieffste Flu\u00df hingegen laufft gemach.", "tokens": ["Der", "al\u00b7ler\u00b7tieffs\u00b7te", "Flu\u00df", "hin\u00b7ge\u00b7gen", "laufft", "ge\u00b7mach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Es weist der ", "tokens": ["Es", "weist", "der"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.15": {"text": "Da\u00df H\u00f6rn und Sehen mehr als Reden n\u00fctzlich war.", "tokens": ["Da\u00df", "H\u00f6rn", "und", "Se\u00b7hen", "mehr", "als", "Re\u00b7den", "n\u00fctz\u00b7lich", "war", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "ADV", "KOUS", "NN", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Das Wort verr\u00e4th den Mann/ auch wider sein Vermeynen/", "tokens": ["Das", "Wort", "ver\u00b7r\u00e4\u00b7th", "den", "Mann", "/", "auch", "wi\u00b7der", "sein", "Ver\u00b7mey\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$(", "ADV", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.17": {"text": "Doch kan ein schweigend Narr zu Zeiten weise scheinen.", "tokens": ["Doch", "kan", "ein", "schwei\u00b7gend", "Narr", "zu", "Zei\u00b7ten", "wei\u00b7se", "schei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "ADJD", "NN", "APPR", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}