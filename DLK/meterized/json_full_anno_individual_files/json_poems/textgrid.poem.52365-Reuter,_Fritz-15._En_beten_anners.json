{"textgrid.poem.52365": {"metadata": {"author": {"name": "Reuter, Fritz", "birth": "N.A.", "death": "N.A."}, "title": "15. En beten anners", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.71", "nl:0.28"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbna, Jochen, segg, wo is dat nu mit di?", "tokens": ["\u00bb", "na", ",", "Jo\u00b7chen", ",", "segg", ",", "wo", "is", "dat", "nu", "mit", "di", "?"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ITJ", "$,", "NE", "$,", "VVFIN", "$,", "PWAV", "FM", "ART", "ADV", "APPR", "NE", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Kannst mit den Leutnant di nu all verdragen?\u00ab", "tokens": ["Kannst", "mit", "den", "Leut\u00b7nant", "di", "nu", "all", "ver\u00b7dra\u00b7gen", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "APPR", "ART", "NN", "NE", "ADV", "PIAT", "VVINF", "$.", "$("], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "\u00bbih, ja, dat geiht; dat Gr\u00e4wst, dat is v\u00f6rbi,", "tokens": ["\u00bb", "ih", ",", "ja", ",", "dat", "geiht", ";", "dat", "Gr\u00e4wst", ",", "dat", "is", "v\u00f6r\u00b7bi", ","], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "$,", "PTKANT", "$,", "PRELS", "VVPP", "$.", "ART", "NN", "$,", "FM.nl", "FM.nl", "FM.nl", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Doch alle Dag' des Morrens fr\u00fch", "tokens": ["Doch", "al\u00b7le", "Dag'", "des", "Mor\u00b7rens", "fr\u00fch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "ART", "NN", "ADJD"], "meter": "-+-+-+-+", "m