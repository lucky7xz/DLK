{"dta.poem.518": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Auf das sch\u00f6ne Frauen-Zimmer/ so im g\u00fcld-  \n nen Engel  logir te.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Der Engel K\u00f6nigin zieht in den Engel ein/", "tokens": ["Der", "En\u00b7gel", "K\u00f6\u00b7ni\u00b7gin", "zieht", "in", "den", "En\u00b7gel", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "APPR", "ART", "NN", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Denn Engel m\u00fcssen blo\u00df bey ihres Gleichen seyn/", "tokens": ["Denn", "En\u00b7gel", "m\u00fcs\u00b7sen", "blo\u00df", "bey", "ih\u00b7res", "Glei\u00b7chen", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VMFIN", "ADV", "APPR", "PPOSAT", "NN", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ach k\u00f6nnt ich nur einmahl dich Engels-Kind/ umfassen/", "tokens": ["Ach", "k\u00f6nnt", "ich", "nur", "ein\u00b7mahl", "dich", "En\u00b7gels\u00b7Kind", "/", "um\u00b7fas\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ITJ", "VVFIN", "PPER", "ADV", "ADV", "PPER", "NE", "$(", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ich wolte dich gewi\u00df nicht ungek\u00fc\u00dft verlassen.", "tokens": ["Ich", "wol\u00b7te", "dich", "ge\u00b7wi\u00df", "nicht", "un\u00b7ge\u00b7k\u00fc\u00dft", "ver\u00b7las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "PTKNEG", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Es solte meine Hand sich um den sch\u00f6nen Leib", "tokens": ["Es", "sol\u00b7te", "mei\u00b7ne", "Hand", "sich", "um", "den", "sch\u00f6\u00b7nen", "Leib"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "PRF", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Als wie ein Reben-Stock um seinen Ulmbaum schliessen/", "tokens": ["Als", "wie", "ein", "Re\u00b7ben\u00b7Stock", "um", "sei\u00b7nen", "Ulm\u00b7baum", "schlies\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wir wolten uns zum Zeit-Vertreib", "tokens": ["Wir", "wol\u00b7ten", "uns", "zum", "Zeit\u00b7Ver\u00b7treib"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Recht zu erg\u00f6tzen wissen.", "tokens": ["Recht", "zu", "er\u00b7g\u00f6t\u00b7zen", "wis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}