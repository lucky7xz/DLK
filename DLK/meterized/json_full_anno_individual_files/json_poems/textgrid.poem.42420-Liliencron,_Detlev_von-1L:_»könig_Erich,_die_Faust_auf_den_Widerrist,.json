{"textgrid.poem.42420": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "1L: \u00bbk\u00f6nig Erich, die Faust auf den Widerrist,", "genre": "verse", "period": "N.A.", "pub_year": 1876, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbk\u00f6nig Erich, die Faust auf den Widerrist,", "tokens": ["\u00bb", "k\u00f6\u00b7nig", "E\u00b7rich", ",", "die", "Faust", "auf", "den", "Wi\u00b7der\u00b7rist", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VMFIN", "NE", "$,", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.2": {"text": "La\u00df tanzen den Hengst im Grase.", "tokens": ["La\u00df", "tan\u00b7zen", "den", "Hengst", "im", "Gra\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Vergi\u00df den alten Bruderzwist,", "tokens": ["Ver\u00b7gi\u00df", "den", "al\u00b7ten", "Bru\u00b7der\u00b7zwist", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wir trinken aus einem Glase.\u00ab", "tokens": ["Wir", "trin\u00b7ken", "aus", "ei\u00b7nem", "Gla\u00b7se", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$.", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Herzog Abel schrieb das. K\u00f6nig Erich ritt ein,", "tokens": ["Her\u00b7zog", "A\u00b7bel", "schrieb", "das", ".", "K\u00f6\u00b7nig", "E\u00b7rich", "ritt", "ein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "PDS", "$.", "NN", "NE", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Und lag im Bruderarme.", "tokens": ["Und", "lag", "im", "Bru\u00b7der\u00b7ar\u00b7me", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Viel Jauchzen der Ritter im Abendschein,", "tokens": ["Viel", "Jauch\u00b7zen", "der", "Rit\u00b7ter", "im", "A\u00b7bend\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Lauge Gudmundson schwieg im Schwarme.", "tokens": ["Lau\u00b7ge", "Gud\u00b7mund\u00b7son", "schwieg", "im", "Schwar\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Am Morgen fr\u00fch weckt Hornsto\u00df und Tusch,", "tokens": ["Am", "Mor\u00b7gen", "fr\u00fch", "weckt", "Horn\u00b7sto\u00df", "und", "Tusch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Zu hetzen Wolf und Elche.", "tokens": ["Zu", "het\u00b7zen", "Wolf", "und", "El\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NE", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Br\u00fcder zusammen im Heidebusch,", "tokens": ["Die", "Br\u00fc\u00b7der", "zu\u00b7sam\u00b7men", "im", "Hei\u00b7de\u00b7busch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Sie trinken aus einem Kelche.", "tokens": ["Sie", "trin\u00b7ken", "aus", "ei\u00b7nem", "Kel\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Der Herzog allein. Zur Seiten nur", "tokens": ["Der", "Her\u00b7zog", "al\u00b7lein", ".", "Zur", "Sei\u00b7ten", "nur"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NE", "ADV", "$.", "APPRART", "NN", "ADV"], "meter": "-++-+-+-+", "measure": "unknown.measure.penta"}, "line.2": {"text": "Ritter Lauge mit Speer und Pfeilen.", "tokens": ["Rit\u00b7ter", "Lau\u00b7ge", "mit", "Speer", "und", "Pfei\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "\u00bbsprich, Lauge, wo blieb Wieb Stures Spur,", "tokens": ["\u00bb", "sprich", ",", "Lau\u00b7ge", ",", "wo", "blieb", "Wieb", "Stu\u00b7res", "Spur", ","], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "$,", "NN", "$,", "PWAV", "VVFIN", "NN", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wem hilft sie die Freuden teilen?\u00ab", "tokens": ["Wem", "hilft", "sie", "die", "Freu\u00b7den", "tei\u00b7len", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ART", "NN", "VVINF", "$.", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Der K\u00f6nig allein. Zur Seiten nur", "tokens": ["Der", "K\u00f6\u00b7nig", "al\u00b7lein", ".", "Zur", "Sei\u00b7ten", "nur"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "$.", "APPRART", "NN", "ADV"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ritter Lauge mit Speer und Pfeilen.", "tokens": ["Rit\u00b7ter", "Lau\u00b7ge", "mit", "Speer", "und", "Pfei\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "\u00bbk\u00f6nig Erich, wo blieb Wieb Stures Spur,", "tokens": ["\u00bb", "k\u00f6\u00b7nig", "E\u00b7rich", ",", "wo", "blieb", "Wieb", "Stu\u00b7res", "Spur", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VMFIN", "NE", "$,", "PWAV", "VVFIN", "NN", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "Wem hilft sie das Leben teilen?\u00ab", "tokens": ["Wem", "hilft", "sie", "das", "Le\u00b7ben", "tei\u00b7len", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ART", "NN", "VVINF", "$.", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Erich Plogpenning zischt. Den Stachel sticht", "tokens": ["E\u00b7rich", "Plog\u00b7pen\u00b7ning", "zischt", ".", "Den", "Sta\u00b7chel", "sticht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NE", "VVFIN", "$.", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Dem Rothengst er in die Weichen.", "tokens": ["Dem", "Ro\u00b7thengst", "er", "in", "die", "Wei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-++-+-+-", "measure": "unknown.measure.tetra"}, "line.3": {"text": "\u00bbbei Sanct J\u00fcrgen, ich wei\u00df es nicht,\u00ab", "tokens": ["\u00bb", "bei", "Sanct", "J\u00fcr\u00b7gen", ",", "ich", "wei\u00df", "es", "nicht", ",", "\u00ab"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "APPR", "NE", "NE", "$,", "PPER", "VVFIN", "PPER", "PTKNEG", "$,", "$("], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Und sucht die Jagd zu erreichen.", "tokens": ["Und", "sucht", "die", "Jagd", "zu", "er\u00b7rei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "Am Abend Humpenaus, Zinken und Tanz,", "tokens": ["Am", "A\u00b7bend", "Hum\u00b7pe\u00b7naus", ",", "Zin\u00b7ken", "und", "Tanz", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NE", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Beim Brettspiel K\u00f6nig und Knappen.", "tokens": ["Beim", "Brett\u00b7spiel", "K\u00f6\u00b7nig", "und", "Knap\u00b7pen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Der Mond flicht drau\u00dfen den alten Kranz", "tokens": ["Der", "Mond", "flicht", "drau\u00b7\u00dfen", "den", "al\u00b7ten", "Kranz"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Um Lauben und steinerne Wappen.", "tokens": ["Um", "Lau\u00b7ben", "und", "stei\u00b7ner\u00b7ne", "Wap\u00b7pen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.8": {"line.1": {"text": "Der Herzog allein. Zur Seiten nur", "tokens": ["Der", "Her\u00b7zog", "al\u00b7lein", ".", "Zur", "Sei\u00b7ten", "nur"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NE", "ADV", "$.", "APPRART", "NN", "ADV"], "meter": "-++-+-+-+", "measure": "unknown.measure.penta"}, "line.2": {"text": "Ritter Laug' im Wams von Seiden.", "tokens": ["Rit\u00b7ter", "Laug'", "im", "Wams", "von", "Sei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "APPRART", "NN", "APPR", "NE", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00bbsprich, Lauge, wo blieb Wieb Stures Spur,", "tokens": ["\u00bb", "sprich", ",", "Lau\u00b7ge", ",", "wo", "blieb", "Wieb", "Stu\u00b7res", "Spur", ","], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "$,", "NN", "$,", "PWAV", "VVFIN", "NN", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wen k\u00fc\u00dft sie von euch beiden?\u00ab", "tokens": ["Wen", "k\u00fc\u00dft", "sie", "von", "euch", "bei\u00b7den", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "PPER", "PIAT", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "\u00bbvom Trinken ist dir die Stirne hei\u00df,", "tokens": ["\u00bb", "vom", "Trin\u00b7ken", "ist", "dir", "die", "Stir\u00b7ne", "hei\u00df", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPRART", "NN", "VAFIN", "PPER", "ART", "NN", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "K\u00f6nig Erich, die Luft ist trocken.", "tokens": ["K\u00f6\u00b7nig", "E\u00b7rich", ",", "die", "Luft", "ist", "tro\u00b7cken", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "ART", "NN", "VAFIN", "ADJD", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Mein Segel wiegt unten, scharlach und wei\u00df,", "tokens": ["Mein", "Se\u00b7gel", "wiegt", "un\u00b7ten", ",", "schar\u00b7lach", "und", "wei\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "$,", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Steig' ein, und k\u00fchle die Locken.\u00ab", "tokens": ["Steig'", "ein", ",", "und", "k\u00fch\u00b7le", "die", "Lo\u00b7cken", ".", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "PTKVZ", "$,", "KON", "VVFIN", "ART", "NN", "$.", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.10": {"line.1": {"text": "Schlo\u00dfknechte spannen den Baldachin,", "tokens": ["Schlo\u00df\u00b7knech\u00b7te", "span\u00b7nen", "den", "Bal\u00b7da\u00b7ch\u00b7in", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Vom S\u00f6ller winkt der Bruder.", "tokens": ["Vom", "S\u00f6l\u00b7ler", "winkt", "der", "Bru\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der K\u00f6nig schl\u00e4ft auf dem Hermelin,", "tokens": ["Der", "K\u00f6\u00b7nig", "schl\u00e4ft", "auf", "dem", "Her\u00b7me\u00b7lin", ","], "token_info": ["word",