", "word", "word"], "pos": ["$(", "VVFIN", "$,", "VVFIN", "$,", "NN", "NE", "$.", "$(", "$(", "NN", "APPRART", "NN"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Flackern um schwarze Figuren.", "tokens": ["Fla\u00b7ckern", "um", "schwar\u00b7ze", "Fi\u00b7gu\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "\u00bbwo blieb Wieb Sture, gieb Antwort, geschwind,", "tokens": ["\u00bb", "wo", "blieb", "Wieb", "Stu\u00b7re", ",", "gieb", "Ant\u00b7wort", ",", "ge\u00b7schwind", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["$(", "PWAV", "VVFIN", "NN", "NN", "$,", "VVIMP", "NN", "$,", "ADJD", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Gieb Antwort, wo blieb Wieb Sturen?\u00ab", "tokens": ["Gieb", "Ant\u00b7wort", ",", "wo", "blieb", "Wieb", "Stu\u00b7ren", "?", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVIMP", "NN", "$,", "PWAV", "VVFIN", "NN", "NN", "$.", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.13": {"line.1": {"text": "\u00bbbei Sanct J\u00fcrgen, ich ri\u00df sie dir Hund vom Leib,\u00ab", "tokens": ["\u00bb", "bei", "Sanct", "J\u00fcr\u00b7gen", ",", "ich", "ri\u00df", "sie", "dir", "Hund", "vom", "Leib", ",", "\u00ab"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "APPR", "NE", "NE", "$,", "PPER", "VVFIN", "PPER", "PPER", "NN", "APPRART", "NN", "$,", "$("], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Schreit der K\u00f6nig, die Lippen beben.", "tokens": ["Schreit", "der", "K\u00f6\u00b7nig", ",", "die", "Lip\u00b7pen", "be\u00b7ben", "."], "token_info": ["wo