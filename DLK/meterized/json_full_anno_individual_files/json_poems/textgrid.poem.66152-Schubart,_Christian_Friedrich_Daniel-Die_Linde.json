{"textgrid.poem.66152": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "Die Linde", "genre": "verse", "period": "N.A.", "pub_year": 1783, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Warst so sch\u00f6n, breitwipflichter Baum,", "tokens": ["Warst", "so", "sch\u00f6n", ",", "breit\u00b7wipf\u00b7lich\u00b7ter", "Baum", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Als dir schwollen die Knospen,", "tokens": ["Als", "dir", "schwol\u00b7len", "die", "Knos\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Als du Bl\u00fctend\u00fcfte verhauchtest;", "tokens": ["Als", "du", "Bl\u00fc\u00b7ten\u00b7d\u00fcf\u00b7te", "ver\u00b7hauch\u00b7test", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "VVFIN", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Warst so sch\u00f6n!", "tokens": ["Warst", "so", "sch\u00f6n", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Dich umsummt' im Lenzabend der K\u00e4fer,", "tokens": ["Dich", "um\u00b7summt'", "im", "Lenz\u00b7a\u00b7bend", "der", "K\u00e4\u00b7fer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+-+---+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Gefl\u00fcgelte Ameisen schw\u00e4rmten", "tokens": ["Ge\u00b7fl\u00fc\u00b7gel\u00b7te", "A\u00b7mei\u00b7sen", "schw\u00e4rm\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN"], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Wie Mittagsw\u00f6lkchen, die die Sonne", "tokens": ["Wie", "Mit\u00b7tags\u00b7w\u00f6lk\u00b7chen", ",", "die", "die", "Son\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "NN", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Versilbert, um deinen Bl\u00fctenzweig.", "tokens": ["Ver\u00b7sil\u00b7bert", ",", "um", "dei\u00b7nen", "Bl\u00fc\u00b7ten\u00b7zweig", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "KOUI", "PPOSAT", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Die Bl\u00fcte fiel; da warst du gr\u00fcn", "tokens": ["Die", "Bl\u00fc\u00b7te", "fiel", ";", "da", "warst", "du", "gr\u00fcn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "ADV", "VAFIN", "PPER", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und st\u00e4rktest mein Auge,", "tokens": ["Und", "st\u00e4rk\u00b7test", "mein", "Au\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Das ans falsche Dunkel meines Kerkers", "tokens": ["Das", "ans", "fal\u00b7sche", "Dun\u00b7kel", "mei\u00b7nes", "Ker\u00b7kers"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "APPRART", "ADJA", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Gew\u00f6hnt, blinzt' im Sonnenstrahl.", "tokens": ["Ge\u00b7w\u00f6hnt", ",", "blinzt'", "im", "Son\u00b7nen\u00b7strahl", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "VVFIN", "APPRART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Und nun bist du halbnackt;", "tokens": ["Und", "nun", "bist", "du", "halb\u00b7nackt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Der Herbststurm blies um deinen Scheitel,", "tokens": ["Der", "Herbst\u00b7sturm", "blies", "um", "dei\u00b7nen", "Schei\u00b7tel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und deinen Schmuck; die goldnen Bl\u00e4tter", "tokens": ["Und", "dei\u00b7nen", "Schmuck", ";", "die", "gold\u00b7nen", "Bl\u00e4t\u00b7ter"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "W\u00e4lzt nun wogend der Odem des Sturms.", "tokens": ["W\u00e4lzt", "nun", "wo\u00b7gend", "der", "O\u00b7dem", "des", "Sturms", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "ART", "NN", "ART", "NN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Die schwarzen Aeste starren trauernd,", "tokens": ["Die", "schwar\u00b7zen", "A\u00b7es\u00b7te", "star\u00b7ren", "trau\u00b7ernd", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ihrer Decke beraubt, in die Luft.", "tokens": ["Ih\u00b7rer", "De\u00b7cke", "be\u00b7raubt", ",", "in", "die", "Luft", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$,", "APPR", "ART", "NN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Dich flieht der Sperling, denn du bist", "tokens": ["Dich", "flieht", "der", "Sper\u00b7ling", ",", "denn", "du", "bist"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "KON", "PPER", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihm nicht mehr H\u00fclle gegen den Sperber.", "tokens": ["Ihm", "nicht", "mehr", "H\u00fcl\u00b7le", "ge\u00b7gen", "den", "Sper\u00b7ber", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "PIAT", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Einst knospete ich, o Linde!", "tokens": ["Einst", "knos\u00b7pe\u00b7te", "ich", ",", "o", "Lin\u00b7de", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "FM", "NE", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sch\u00f6ner, als du. Trug Bl\u00fcten", "tokens": ["Sch\u00f6\u00b7ner", ",", "als", "du", ".", "Trug", "Bl\u00fc\u00b7ten"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "$,", "KOUS", "PPER", "$.", "NN", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Des Knaben, des J\u00fcnglings, die s\u00fc\u00dfer", "tokens": ["Des", "Kna\u00b7ben", ",", "des", "J\u00fcng\u00b7lings", ",", "die", "s\u00fc\u00b7\u00dfer"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ART", "ADJA"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Dufteten, als du im Fr\u00fchlingsschmuck.", "tokens": ["Duf\u00b7te\u00b7ten", ",", "als", "du", "im", "Fr\u00fch\u00b7lings\u00b7schmuck", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Meine geringelten Seidenlocken", "tokens": ["Mei\u00b7ne", "ge\u00b7rin\u00b7gel\u00b7ten", "Sei\u00b7den\u00b7lo\u00b7cken"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Waren sch\u00f6ner, als dein gr\u00fcnes Haar.", "tokens": ["Wa\u00b7ren", "sch\u00f6\u00b7ner", ",", "als", "dein", "gr\u00fc\u00b7nes", "Haar", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$,", "KOUS", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Sch\u00f6ner, als deines Finken und Distelvogels,", "tokens": ["Sch\u00f6\u00b7ner", ",", "als", "dei\u00b7nes", "Fin\u00b7ken", "und", "Dis\u00b7tel\u00b7vo\u00b7gels", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "+--+-+--+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Scholl mein Gesang und Fl\u00fcgelspiel.", "tokens": ["Scholl", "mein", "Ge\u00b7sang", "und", "Fl\u00fc\u00b7gel\u00b7spiel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Ich war ein Mann, breitwipflig", "tokens": ["Ich", "war", "ein", "Mann", ",", "breit\u00b7wipf\u00b7lig"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und lieblich im Sonnenstrahl spielend.", "tokens": ["Und", "lieb\u00b7lich", "im", "Son\u00b7nen\u00b7strahl", "spie\u00b7lend", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPRART", "NN", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Meines Geistes Fittig deckte die Meinen,", "tokens": ["Mei\u00b7nes", "Geis\u00b7tes", "Fit\u00b7tig", "deck\u00b7te", "die", "Mei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Wie dein schattender Wipfel den Pilger.", "tokens": ["Wie", "dein", "schat\u00b7ten\u00b7der", "Wip\u00b7fel", "den", "Pil\u00b7ger", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "ADJA", "NN", "ART", "NN", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}}, "stanza.9": {"line.1": {"text": "Aber ach! mein Herbst ist gekommen;", "tokens": ["A\u00b7ber", "ach", "!", "mein", "Herbst", "ist", "ge\u00b7kom\u00b7men", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$.", "PPOSAT", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "So fr\u00fch ist schon mein Herbst gekommen!", "tokens": ["So", "fr\u00fch", "ist", "schon", "mein", "Herbst", "ge\u00b7kom\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Das Schicksal blies mit kaltem st\u00fcrmendem Odem;", "tokens": ["Das", "Schick\u00b7sal", "blies", "mit", "kal\u00b7tem", "st\u00fcr\u00b7men\u00b7dem", "O\u00b7dem", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Und meine Bl\u00e4tter fielen.", "tokens": ["Und", "mei\u00b7ne", "Bl\u00e4t\u00b7ter", "fie\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Heiser ist mein Gesang;", "tokens": ["Hei\u00b7ser", "ist", "mein", "Ge\u00b7sang", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Die gefl\u00fcgelte Rechte lahmt", "tokens": ["Die", "ge\u00b7fl\u00fc\u00b7gel\u00b7te", "Rech\u00b7te", "lahmt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Auf den braunen Tasten", "tokens": ["Auf", "den", "brau\u00b7nen", "Tas\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Des goldnen Saitenspiels.", "tokens": ["Des", "gold\u00b7nen", "Sai\u00b7ten\u00b7spiels", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Meine Phantasie, der Riese,", "tokens": ["Mei\u00b7ne", "Phan\u00b7ta\u00b7sie", ",", "der", "Rie\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zuckt ausgestreckt, wie ein Geripp'", "tokens": ["Zuckt", "aus\u00b7ge\u00b7streckt", ",", "wie", "ein", "Ge\u00b7ripp'"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV"