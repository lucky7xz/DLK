{"textgrid.poem.59376": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Viel Lenze waren lange schon vergangen,", "tokens": ["Viel", "Len\u00b7ze", "wa\u00b7ren", "lan\u00b7ge", "schon", "ver\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vor\u00fcber zogen wunderbare Lieder,", "tokens": ["Vor\u00b7\u00fc\u00b7ber", "zo\u00b7gen", "wun\u00b7der\u00b7ba\u00b7re", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Sterne gingen ewig auf und nieder,", "tokens": ["Die", "Ster\u00b7ne", "gin\u00b7gen", "e\u00b7wig", "auf", "und", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die selbst vor gro\u00dfer Sehnsucht golden klangen.", "tokens": ["Die", "selbst", "vor", "gro\u00b7\u00dfer", "Sehn\u00b7sucht", "gol\u00b7den", "klan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "ADJA", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und wie so tausend Stimmen ferne sangen,", "tokens": ["Und", "wie", "so", "tau\u00b7send", "Stim\u00b7men", "fer\u00b7ne", "san\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ADV", "CARD", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als riefen mich von hinnen sel'ge Br\u00fcder,", "tokens": ["Als", "rie\u00b7fen", "mich", "von", "hin\u00b7nen", "sel'\u00b7ge", "Br\u00fc\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PRF", "APPR", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "F\u00fchlt ich die alten Schmerzen immer wieder,", "tokens": ["F\u00fchlt", "ich", "die", "al\u00b7ten", "Schmer\u00b7zen", "im\u00b7mer", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Seit deine Blicke, Jungfrau, mich bezwangen.", "tokens": ["Seit", "dei\u00b7ne", "Bli\u00b7cke", ",", "Jung\u00b7frau", ",", "mich", "be\u00b7zwan\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "NN", "$,", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Da war's, als ob sich still dein Auge h\u00fcbe,", "tokens": ["Da", "wa\u00b7r's", ",", "als", "ob", "sich", "still", "dein", "Au\u00b7ge", "h\u00fc\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "$,", "KOKOM", "KOUS", "PRF", "ADJD", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Langst sehnsuchtsvoll nach mir mit offnen Armen,", "tokens": ["Langst", "sehn\u00b7suchts\u00b7voll", "nach", "mir", "mit", "off\u00b7nen", "Ar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "P