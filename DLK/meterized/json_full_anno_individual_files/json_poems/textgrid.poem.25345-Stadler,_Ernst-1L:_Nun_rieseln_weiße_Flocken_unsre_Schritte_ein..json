{"textgrid.poem.25345": {"metadata": {"author": {"name": "Stadler, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nun rieseln wei\u00dfe Flocken unsre Schritte ein.", "genre": "verse", "period": "N.A.", "pub_year": 1898, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun rieseln wei\u00dfe Flocken unsre Schritte ein.", "tokens": ["Nun", "rie\u00b7seln", "wei\u00b7\u00dfe", "Flo\u00b7cken", "uns\u00b7re", "Schrit\u00b7te", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJA", "NN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Weidenstrich l\u00e4\u00dft fr\u00f6stelnd letzte Farben sinken,", "tokens": ["Der", "Wei\u00b7den\u00b7strich", "l\u00e4\u00dft", "fr\u00f6s\u00b7telnd", "letz\u00b7te", "Far\u00b7ben", "sin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das Dunkel steigt vom Flu\u00df, um den versprengte", "tokens": ["Das", "Dun\u00b7kel", "steigt", "vom", "Flu\u00df", ",", "um", "den", "ver\u00b7spreng\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,", "KOUI", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Lichter blinken,", "tokens": ["Lich\u00b7ter", "blin\u00b7ken", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Mit Schnee und bleicher Stille weht die Nacht herein.", "tokens": ["Mit", "Schnee", "und", "blei\u00b7cher", "Stil\u00b7le", "weht", "die", "Nacht", "her\u00b7ein", "."], "token_info": ["word