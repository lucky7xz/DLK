{"textgrid.poem.39102": {"metadata": {"author": {"name": "Prutz, Robert Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Bescheidner Wunsch", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "W\u00e4ren lieber wir Chinesen!", "tokens": ["W\u00e4\u00b7ren", "lie\u00b7ber", "wir", "Chi\u00b7ne\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPER", "NN", "$."], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.2": {"text": "Leichter w\u00fcrd' ich es ertragen,", "tokens": ["Leich\u00b7ter", "w\u00fcrd'", "ich", "es", "er\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "da\u00df wir erst so stolz gewesen", "tokens": ["da\u00df", "wir", "erst", "so", "stolz", "ge\u00b7we\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ADJD", "VAPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "und nun so zugrund geschlagen.", "tokens": ["und", "nun", "so", "zu\u00b7grund", "ge\u00b7schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ADJD", "VVPP", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Blieb' uns doch der Opiumbecher,", "tokens": ["Blieb'", "uns", "doch", "der", "O\u00b7pium\u00b7be\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.2": {"text": "drin Vergessenheit zu suchen,", "tokens": ["drin", "Ver\u00b7ges\u00b7sen\u00b7heit", "zu", "su\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "blieb uns doch, als tolle Zecher,", "tokens": ["blieb", "uns", "doch", ",", "als", "tol\u00b7le", "Ze\u00b7cher", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "KOUS", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "unsern \u00c4rger auszufluchen!", "tokens": ["un\u00b7sern", "\u00c4r\u00b7ger", "aus\u00b7zu\u00b7flu\u00b7chen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Blieb' uns doch der Schmuck der K\u00f6pfe,", "tokens": ["Blieb'", "uns", "doch", "der", "Schmuck", "der", "K\u00f6p\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "jene zarten, jene langen,", "tokens": ["je\u00b7ne", "zar\u00b7ten", ",", "je\u00b7ne", "lan\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "$,", "PDAT", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "blieben uns ja doch die Z\u00f6pfe,", "tokens": ["blie\u00b7ben", "uns", "ja", "doch", "die", "Z\u00f6p\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "selber uns dran aufzuhangen!", "tokens": ["sel\u00b7ber", "uns", "dran", "auf\u00b7zu\u00b7han\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "PAV", "VVINF", "$."], "meter": "+-+-+-+-", "m