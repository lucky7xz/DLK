{"textgrid.poem.59481": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Valet", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ade nun, liebe Lieder,", "tokens": ["A\u00b7de", "nun", ",", "lie\u00b7be", "Lie\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "ADJA", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Ade, du sch\u00f6ner Sang!", "tokens": ["A\u00b7de", ",", "du", "sch\u00f6\u00b7ner", "Sang", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "ADJA", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Nun sing ich wohl nicht wieder", "tokens": ["Nun", "sing", "ich", "wohl", "nicht", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PTKNEG", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Vielleicht mein Leben lang.", "tokens": ["Viel\u00b7leicht", "mein", "Le\u00b7ben", "lang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Einst bl\u00fcht' von Gottes Odem", "tokens": ["Einst", "bl\u00fcht'", "von", "Got\u00b7tes", "O\u00b7dem"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Welt so wunderreich,", "tokens": ["Die", "Welt", "so", "wun\u00b7der\u00b7reich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da in den gr\u00fcnen Boden", "tokens": ["Da", "in", "den", "gr\u00fc\u00b7nen", "Bo\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Senkt ich als Reiser euch.", "tokens": ["Senkt", "ich", "als", "Rei\u00b7ser", "euch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KOUS", "NN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Jetzt eure Wipfel schwanken", "tokens": ["Jetzt", "eu\u00b7re", "Wip\u00b7fel", "schwan\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So k\u00fchle \u00fcber mir,", "tokens": ["So", "k\u00fch\u00b7le", "\u00fc\u00b7ber", "mir", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich stehe in Gedanken", "tokens": ["Ich", "ste\u00b7he", "in", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Gleichwie im Walde hier.", "tokens": ["Gleich\u00b7wie", "im", "Wal\u00b7de", "hier", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Da mu\u00df ich oft noch lauschen", "tokens": ["Da", "mu\u00df", "ich", "oft", "noch", "lau\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In meiner Einsamkeit,", "tokens": ["In", "mei\u00b7ner", "Ein\u00b7sam\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und denk bei eurem Rauschen", "tokens": ["Und", "denk", "bei", "eu\u00b7rem", "Rau\u00b7schen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der sch\u00f6nen Jugendzeit.", "tokens": ["Der", "sch\u00f6\u00b7nen", "Ju\u00b7gend\u00b7zeit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ade nun, liebe Lieder,", "tokens": ["A\u00b7de", "nun", ",", "lie\u00b7be", "Lie\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "ADJA", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Ade, du sch\u00f6ner Sang!", "tokens": ["A\u00b7de", ",", "du", "sch\u00f6\u00b7ner", "Sang", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "ADJA", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Nun sing ich wohl nicht wieder", "tokens": ["Nun", "sing", "ich", "wohl", "nicht", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PTKNEG", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Vielleicht mein Leben lang.", "tokens": ["Viel\u00b7leicht", "mein", "Le\u00b7ben", "lang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Einst bl\u00fcht' von Gottes Odem", "tokens": ["Einst", "bl\u00fcht'", "von", "Got\u00b7tes", "O\u00b7dem"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "NN", "NN"], "meter