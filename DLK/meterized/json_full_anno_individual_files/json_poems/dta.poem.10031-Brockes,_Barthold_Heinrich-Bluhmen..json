{"dta.poem.10031": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Bluhmen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20086-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "A. Es w\u00e4hrt der Bluhmen Zier ja nur so kurtze Zeit;", "tokens": ["A.", "Es", "w\u00e4hrt", "der", "Bluh\u00b7men", "Zier", "ja", "nur", "so", "kurt\u00b7ze", "Zeit", ";"], "token_info": ["abbreviation", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "PPER", "VVFIN", "ART", "NN", "NN", "ADV", "ADV", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und dennoch wird derselben Pracht", "tokens": ["Und", "den\u00b7noch", "wird", "der\u00b7sel\u00b7ben", "Pracht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Uns alle Nacht", "tokens": ["Uns", "al\u00b7le", "Nacht"], "token_info": ["word", "word", "word"], "pos": ["PPER", "PIAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Geraubt, und k\u00f6nnen wir an ihnen das, was sch\u00f6n,", "tokens": ["Ge\u00b7raubt", ",", "und", "k\u00f6n\u00b7nen", "wir", "an", "ih\u00b7nen", "das", ",", "was", "sch\u00f6n", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVPP", "$,", "KON", "VMFIN", "PPER", "APPR", "PPER", "PDS", "$,", "PRELS", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die halbe Zeit von ihrer Daur, nicht sehn.", "tokens": ["Die", "hal\u00b7be", "Zeit", "von", "ih\u00b7rer", "Daur", ",", "nicht", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$,", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Mich deucht es scheint hieraus zu fliessen,", "tokens": ["Mich", "deucht", "es", "scheint", "hier\u00b7aus", "zu", "flies\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVFIN", "PAV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Da\u00df sie f\u00fcr uns nicht, wie man glaubt, gemacht.", "tokens": ["Da\u00df", "sie", "f\u00fcr", "uns", "nicht", ",", "wie", "man", "glaubt", ",", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "PTKNEG", "$,", "PWAV", "PIS", "VVFIN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Wie, nach des Winters Sturm und rauhem Frost im Lentzen", "tokens": ["Wie", ",", "nach", "des", "Win\u00b7ters", "Sturm", "und", "rau\u00b7hem", "Frost", "im", "Lent\u00b7zen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "$,", "APPR", "ART", "NN", "NN", "KON", "ADJA", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Die Vorw\u00fcrff\u2019 alle sch\u00f6ner gl\u00e4ntzen;", "tokens": ["Die", "Vor\u00b7w\u00fcrff'", "al\u00b7le", "sch\u00f6\u00b7ner", "gl\u00e4nt\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Und wie, wenn unsre Lust uns die Gewohnheit st\u00f6hrt,", "tokens": ["Und", "wie", ",", "wenn", "uns\u00b7re", "Lust", "uns", "die", "Ge\u00b7wohn\u00b7heit", "st\u00f6hrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "$,", "KOUS", "PPOSAT", "NN", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Der Wechsel sie uns wieder schencket,", "tokens": ["Der", "Wech\u00b7sel", "sie", "uns", "wie\u00b7der", "schen\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Sie schmacksam macht, und noch vermehrt;", "tokens": ["Sie", "schmack\u00b7sam", "macht", ",", "und", "noch", "ver\u00b7mehrt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVFIN", "$,", "KON", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "So deucht mich, wenn die Nacht sich zu uns lencket,", "tokens": ["So", "deucht", "mich", ",", "wenn", "die", "Nacht", "sich", "zu", "uns", "len\u00b7cket", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "ART", "NN", "PRF", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Da\u00df es nur blo\u00df zu diesem Zweck geschehe,", "tokens": ["Da\u00df", "es", "nur", "blo\u00df", "zu", "die\u00b7sem", "Zweck", "ge\u00b7sche\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "APPR", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Damit man, mit noch gr\u00f6ssrer Wonne,", "tokens": ["Da\u00b7mit", "man", ",", "mit", "noch", "gr\u00f6ss\u00b7rer", "Won\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "$,", "APPR", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Bey heller Wiederkunft der Sonne,", "tokens": ["Bey", "hel\u00b7ler", "Wie\u00b7der\u00b7kunft", "der", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Sie, nach dem scheiden, wieder sehe.", "tokens": ["Sie", ",", "nach", "dem", "schei\u00b7den", ",", "wie\u00b7der", "se\u00b7he", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "APPR", "ART", "ADJA", "$,", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}