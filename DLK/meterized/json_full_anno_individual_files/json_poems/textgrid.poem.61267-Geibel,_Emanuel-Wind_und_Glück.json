{"textgrid.poem.61267": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Wind und Gl\u00fcck", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Stets, wenn das Segel zur Fahrt nur schlaff hing, h\u00f6rt' ich den Bootsmann", "tokens": ["Stets", ",", "wenn", "das", "Se\u00b7gel", "zur", "Fahrt", "nur", "schlaff", "hing", ",", "h\u00f6rt'", "ich", "den", "Boots\u00b7mann"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "ART", "NN", "APPRART", "NN", "ADV", "ADJD", "VVFIN", "$,", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+--+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Pfeifen; begierig gemacht, fragt' ich ihn einst um den Grund.", "tokens": ["Pfei\u00b7fen", ";", "be\u00b7gie\u00b7rig", "ge\u00b7macht", ",", "fragt'", "ich", "ihn", "einst", "um", "den", "Grund", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ADJD", "VVPP", "$,", "VVFIN", "PPER", "PPER", "ADV", "APPR", "ART", "NN", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.3": {"text": "Doch er bedeutete mich schlau l\u00e4chelnd: \u00bbDer Wind ist ein Vogel,", "tokens": ["Doch", "er", "be\u00b7deu\u00b7te\u00b7te", "mich", "schlau", "l\u00e4\u00b7chelnd", ":", "\u00bb", "Der", "Wind", "ist", "ein", "Vo\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADJD", "ADJD", "$.", "$(", "ART", "NN", "VAFIN", "ART", "NE", "$,"], "meter": "-+-+-+-+---+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Welcher gelockt sein will.\u00ab Sagt' es und fl\u00f6tete fort.", "tokens": ["Wel\u00b7cher", "ge\u00b7lockt", "sein", "will", ".", "\u00ab", "Sagt'", "es", "und", "fl\u00f6\u00b7te\u00b7te", "fort", "."], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "VVPP", "VAINF", "VMFIN", "$.", "$(", "VVFIN", "PPER", "KON", "VVFIN", "PTKVZ", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.5": {"text": "Und so sing' ich gefa\u00dft mein Lied in schwererer Zeit nun,", "tokens": ["Und", "so", "sing'", "ich", "ge\u00b7fa\u00dft", "mein", "Lied", "in", "schwe\u00b7re\u00b7rer", "Zeit", "nun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "VVPP", "PPOSAT", "NN", "APPR", "ADJA", "NN", "ADV", "$,"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Da mich das Leben bedr\u00fcckt. Ist doch das Gl\u00fcck wie der Wind,", "tokens": ["Da", "mich", "das", "Le\u00b7ben", "be\u00b7dr\u00fcckt", ".", "Ist", "doch", "das", "Gl\u00fcck", "wie", "der", "Wind", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVPP", "$.", "VAFIN", "ADV", "ART", "NN", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Flattert gefl\u00fcgelt umher in der Luft und harret des Lockrufs;", "tokens": ["Flat\u00b7tert", "ge\u00b7fl\u00fc\u00b7gelt", "um\u00b7her", "in", "der", "Luft", "und", "har\u00b7ret", "des", "Lock\u00b7rufs", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVPP", "PTKVZ", "APPR", "ART", "NN", "KON", "VVFIN", "ART", "NN", "$."], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.8": {"text": "Komm, Gl\u00fccksvogel! Den Weg zeigt dir der leise Gesang.", "tokens": ["Komm", ",", "Gl\u00fccks\u00b7vo\u00b7gel", "!", "Den", "Weg", "zeigt", "dir", "der", "lei\u00b7se", "Ge\u00b7sang", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$.", "ART", "NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}}, "stanza.2": {"line.1": {"text": "Stets, wenn das Segel zur Fahrt nur schlaff hing, h\u00f6rt' ich den Bootsmann", "tokens": ["Stets", ",", "wenn", "das", "Se\u00b7gel", "zur", "Fahrt", "nur", "schlaff", "hing", ",", "h\u00f6rt'", "ich", "den", "Boots\u00b7mann"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "ART", "NN", "APPRART", "NN", "ADV", "ADJD", "VVFIN", "$,", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+--+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Pfeifen; begierig gemacht, fragt' ich ihn einst um den Grund.", "tokens": ["Pfei\u00b7fen", ";", "be\u00b7gie\u00b7rig", "ge\u00b7macht", ",", "fragt'", "ich", "ihn", "einst", "um", "den", "Grund", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ADJD", "VVPP", "$,", "VVFIN", "PPER", "PPER", "ADV", "APPR", "ART", "NN", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.3": {"text": "Doch er bedeutete mich schlau l\u00e4chelnd: \u00bbDer Wind ist ein Vogel,", "tokens": ["Doch", "er", "be\u00b7deu\u00b7te\u00b7te", "mich", "schlau", "l\u00e4\u00b7chelnd", ":", "\u00bb", "Der", "Wind", "ist", "ein", "Vo\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADJD", "ADJD", "$.", "$(", "ART", "NN",