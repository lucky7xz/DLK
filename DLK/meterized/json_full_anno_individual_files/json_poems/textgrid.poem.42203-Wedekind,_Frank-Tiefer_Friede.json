{"textgrid.poem.42203": {"metadata": {"author": {"name": "Wedekind, Frank", "birth": "N.A.", "death": "N.A."}, "title": "Tiefer Friede", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Tage verblassen, die Stunden zergehn,", "tokens": ["Die", "Ta\u00b7ge", "ver\u00b7blas\u00b7sen", ",", "die", "Stun\u00b7den", "zer\u00b7gehn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "ART", "NN", "VVINF", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Die Waffen rasten und rosten;", "tokens": ["Die", "Waf\u00b7fen", "ras\u00b7ten", "und", "ros\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ich bin von vorn und von hinten besehn", "tokens": ["Ich", "bin", "von", "vorn", "und", "von", "hin\u00b7ten", "be\u00b7sehn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ADV", "KON", "APPR", "ADV", "VVINF"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ein armer verlorener Posten.", "tokens": ["Ein", "ar\u00b7mer", "ver\u00b7lo\u00b7re\u00b7ner", "Pos\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "Es kreisen die Dohlen, es kriecht das Gew\u00fcrm,", "tokens": ["Es", "krei\u00b7sen", "die", "Doh\u00b7len", ",", "es", "kriecht", "das", "Ge\u00b7w\u00fcrm", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "m