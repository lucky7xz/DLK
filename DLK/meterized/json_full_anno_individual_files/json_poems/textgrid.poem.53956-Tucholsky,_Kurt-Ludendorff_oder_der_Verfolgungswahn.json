{"textgrid.poem.53956": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Ludendorff oder der Verfolgungswahn", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hast du Angst, Erich? Bist du bange, Erich?", "tokens": ["Hast", "du", "Angst", ",", "E\u00b7rich", "?", "Bist", "du", "ban\u00b7ge", ",", "E\u00b7rich", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "$,", "NE", "$.", "VAFIN", "PPER", "ADV", "$,", "NE", "$."], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Klopft dein Herz, Erich? L\u00e4ufst du weg?", "tokens": ["Klopft", "dein", "Herz", ",", "E\u00b7rich", "?", "L\u00e4ufst", "du", "weg", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,", "NE", "$.", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Wolln die Maurer, Erich \u2013 und die Jesuiten, Erich,", "tokens": ["Wolln", "die", "Mau\u00b7rer", ",", "E\u00b7rich", "\u2013", "und", "die", "Je\u00b7su\u00b7i\u00b7ten", ",", "E\u00b7rich", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "ART", "NN", "$,", "NE", "$(", "KON", "ART", "NN", "$,", "NE", "$,"], "meter": "+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.4": {"text": "dich erdolchen, Erich \u2013 welch ein Schreck!", "tokens": ["dich", "er\u00b7dol\u00b7chen", ",", "E\u00b7rich", "\u2013", "welch", "ein", "Schreck", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "$,", "NE", "$(", "PWAT", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Diese Juden werden immer r\u00fcder.", "tokens": ["Die\u00b7se", "Ju\u00b7den", "wer\u00b7den", "im\u00b7mer", "r\u00fc\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VAFIN", "ADV", "ADJD", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Alles Unheil ist das Werk der .\u00b7..\u00b7. Br\u00fcder.", "tokens": ["Al\u00b7les", "Un\u00b7heil", "ist", "das", "Werk", "der", ".", "..", ".", "Br\u00fc\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ART", "NN", "ART", "$.", "XY", "XY", "XY", "$.", "NN", "$."], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Denn die Jesuiten, Erich \u2013 und die Maurer, Erich \u2013", "tokens": ["Denn", "die", "Je\u00b7su\u00b7i\u00b7ten", ",", "E\u00b7rich", "\u2013", "und", "die", "Mau\u00b7rer", ",", "E\u00b7rich", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "NE", "$(", "KON", "ART", "NN", "$,", "NE", "$("], "meter": "+-+-+--+--+---", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "und die Radfahrer \u2013 die sind schuld", "tokens": ["und", "die", "Rad\u00b7fah\u00b7rer", "\u2013", "die", "sind", "schuld"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ART", "NN", "$(", "PDS", "VAFIN", "ADJD"], "meter": "--++-+-+", "measure": "anapaest.init"}, "line.3": {"text": "an der Marne, Erich \u2013 und am Dolchsto\u00df, Erich \u2013", "tokens": ["an", "der", "Mar\u00b7ne", ",", "E\u00b7rich", "\u2013", "und", "am", "Dolch\u00b7sto\u00df", ",", "E\u00b7rich", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "NE", "$(", "KON", "APPRART", "NN", "$,", "NE", "$("], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.4": {"text": "ohne die g\u00e4bs keinen Welttumult.", "tokens": ["oh\u00b7ne", "die", "g\u00e4bs", "kei\u00b7nen", "Welt\u00b7tu\u00b7mult", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PIAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Jeden Freitag abend spielt ein Kapuziner", "tokens": ["Je\u00b7den", "Frei\u00b7tag", "a\u00b7bend", "spielt", "ein", "Ka\u00b7pu\u00b7zi\u00b7ner"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVPP", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.6": {"text": "mit dem Papste Skat \u2013 dazu ein Feldrabbiner;", "tokens": ["mit", "dem", "Paps\u00b7te", "Skat", "\u2013", "da\u00b7zu", "ein", "Fel\u00b7drab\u00b7bi\u00b7ner", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$(", "PAV", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.7": {"text": "auf dem Tische liegt ein Grand mit Vieren \u2013", "tokens": ["auf", "dem", "Ti\u00b7sche", "liegt", "ein", "Grand", "mit", "Vie\u00b7ren", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "APPR", "NN", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "dabei tun sie gegen Deutschland konspirieren . . .", "tokens": ["da\u00b7bei", "tun", "sie", "ge\u00b7gen", "Deutschland", "kons\u00b7pi\u00b7rie\u00b7ren", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PAV", "VVFIN", "PPER", "APPR", "NE", "VVINF", "$.", "$.", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.9": {"text": "Hindenburg wird \u00e4lter und auch m\u00fcder . . .", "tokens": ["Hin\u00b7den\u00b7burg", "wird", "\u00e4l\u00b7ter", "und", "auch", "m\u00fc\u00b7der", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["NE", "VAFIN", "ADJD", "KON", "ADV", "ADJD", "$.", "$.", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Alles Unheil ist das Werk der .\u00b7..\u00b7. Br\u00fcder.", "tokens": ["Al\u00b7les", "Un\u00b7heil", "ist", "das", "Werk", "der", ".", "..", ".", "Br\u00fc\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ART", "NN", "ART", "$.", "XY", "XY", "XY", "$.", "NN", "$."], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "F\u00e4hrst du aus dem Schlaf? Die blaue Brille", "tokens": ["F\u00e4hrst", "du", "aus", "dem", "Schlaf", "?", "Die", "blau\u00b7e", "Bril\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$.", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "liegt auf deinem Nachttisch wohl bereit?", "tokens": ["liegt", "auf", "dei\u00b7nem", "Nacht\u00b7tisch", "wohl", "be\u00b7reit", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "ADV", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "H\u00f6rst du Stimmen?", "tokens": ["H\u00f6rst", "du", "Stim\u00b7men", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Das ist Gottes Wille,", "tokens": ["Das", "ist", "Got\u00b7tes", "Wil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "NN", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Ludendorff, und wei\u00dft du, wer da schreit \u2013?", "tokens": ["Lu\u00b7den\u00b7dorff", ",", "und", "wei\u00dft", "du", ",", "wer", "da", "schreit", "\u2013", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NE", "$,", "KON", "VVFIN", "PPER", "$,", "PWS", "ADV", "VVFIN", "$(", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Hunderttausende, die jung und edel", "tokens": ["Hun\u00b7dert\u00b7tau\u00b7sen\u00b7de", ",", "die", "jung", "und", "e\u00b7del"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "ADJD", "KON", "ADJD"], "meter": "+-+---+-+-", "measure": "unknown.measure.tetra"}, "line.7": {"text": "sterben mu\u00dften, weil dein dicker Sch\u00e4del", "tokens": ["ster\u00b7ben", "mu\u00df\u00b7ten", ",", "weil", "dein", "di\u00b7cker", "Sch\u00e4\u00b7del"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVINF", "VMFIN", "$,", "KOUS", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "sie von Grabenst\u00fcck zu Grabenst\u00fcck gehetzt", "tokens": ["sie", "von", "Gra\u00b7ben\u00b7st\u00fcck", "zu", "Gra\u00b7ben\u00b7st\u00fcck", "ge\u00b7hetzt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "NN", "APPR", "NN", "VVPP"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.9": {"text": "bis zuletzt.", "tokens": ["bis", "zu\u00b7letzt", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADV", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.10": {"text": "Ackerkrume sind, die Deutschlands Kraft gewesen.", "tokens": ["A\u00b7cker\u00b7kru\u00b7me", "sind", ",", "die", "Deutschlands", "Kraft", "ge\u00b7we\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "$,", "ART", "ADJA", "NN", "VAPP", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.11": {"text": "Pack die Koffer! Geh zu den Chinesen!", "tokens": ["Pack", "die", "Kof\u00b7fer", "!", "Geh", "zu", "den", "Chi\u00b7ne\u00b7sen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$.", "NE", "APPR", "ART", "NN", "$."], "meter": "+-+-+--+--", "measure": "trochaic.tetra.relaxed"}, "line.12": {"text": "F\u00fchr auch die bei ihren Kriegen!", "tokens": ["F\u00fchr", "auch", "die", "bei", "ih\u00b7ren", "Krie\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Ohne Juden wirst du gleichfalls unterliegen.", "tokens": ["Oh\u00b7ne", "Ju\u00b7den", "wirst", "du", "gleich\u00b7falls", "un\u00b7ter\u00b7lie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "ADV", "VVINF", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.14": {"text": "Geh nach China! Und komm nie mehr wieder \u2013!", "tokens": ["Geh", "nach", "Chi\u00b7na", "!", "Und", "komm", "nie", "mehr", "wie\u00b7der", "\u2013", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "APPR", "NE", "$.", "KON", "VVFIN", "ADV", "ADV", "ADV", "$(", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.15": {"text": "Alles Unheil ist das Werk der Heeresbr\u00fcder.", "tokens": ["Al\u00b7les", "Un\u00b7heil", "ist", "das", "Werk", "der", "Hee\u00b7res\u00b7br\u00fc\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["