{"dta.poem.17128": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "229.  Der Ha\u00df seiner selbst.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Ich lieb und hasse mich/ ich f\u00fchre mit mir Kriege/", "tokens": ["Ich", "lieb", "und", "has\u00b7se", "mich", "/", "ich", "f\u00fch\u00b7re", "mit", "mir", "Krie\u00b7ge", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "KON", "VVFIN", "PPER", "$(", "PPER", "VVFIN", "APPR", "PPER", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich brauche List und Macht/ da\u00df ich mich selbst besiege:", "tokens": ["Ich", "brau\u00b7che", "List", "und", "Macht", "/", "da\u00df", "ich", "mich", "selbst", "be\u00b7sie\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "KON", "NN", "$(", "KOUS", "PPER", "PRF", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.3": {"text": "Ich schlag\u2019 und t\u00f6dte mich/ ich mach\u2019 es wie ich kan", "tokens": ["Ich", "schlag'", "und", "t\u00f6d\u00b7te", "mich", "/", "ich", "mach'", "es", "wie", "ich", "kan"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PPER", "$(", "PPER", "VVFIN", "PPER", "PWAV", "PPER", "VMFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df ich nicht ich mehr bin: rath was ich f\u00fcr ein Mann?", "tokens": ["Da\u00df", "ich", "nicht", "ich", "mehr", "bin", ":", "rath", "was", "ich", "f\u00fcr", "ein", "Mann", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "PPER", "ADV", "VAFIN", "$.", "NN", "PWS", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}