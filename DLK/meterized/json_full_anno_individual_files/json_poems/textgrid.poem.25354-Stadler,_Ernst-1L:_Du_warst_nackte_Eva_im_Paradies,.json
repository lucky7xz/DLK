{"textgrid.poem.25354": {"metadata": {"author": {"name": "Stadler, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du warst nackte Eva im Paradies,", "genre": "verse", "period": "N.A.", "pub_year": 1898, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du warst nackte Eva im Paradies,", "tokens": ["Du", "warst", "nack\u00b7te", "E\u00b7va", "im", "Pa\u00b7ra\u00b7dies", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "blank, windumspielt und ohne Scham.", "tokens": ["blank", ",", "win\u00b7dum\u00b7spielt", "und", "oh\u00b7ne", "Scham", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKVZ", "$,", "VVFIN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du wuchsest mit den Fr\u00fcchten und Tieren.", "tokens": ["Du", "wuch\u00b7sest", "mit", "den", "Fr\u00fcch\u00b7ten", "und", "Tie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "KON", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Der Morgen nahm", "tokens": ["Der", "Mor\u00b7gen", "nahm"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Dich aus dem Arm der Nacht,", "tokens": ["Dich", "aus", "dem", "Arm", "der", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "---+-+", "measure": "unknown.measure.di"}, "line.6": {"text": "und Abend bettete dich weich", "tokens": ["und", "A\u00b7bend", "bet\u00b7te\u00b7te", "dich", "weich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VVFIN", "PPER", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Zur m\u00fctterlichen Erde. Du warst wild und sch\u00f6n.", "tokens": ["Zur", "m\u00fct\u00b7ter\u00b7li\u00b7chen", "Er\u00b7de", ".", "Du", "warst", "wild", "und", "sch\u00f6n", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$.", "PPER", "VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Du warst den Tieren gleich.", "tokens": ["Du", "warst", "den", "Tie\u00b7ren", "gleich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Warst Rauschen gr\u00fcner Wipfel.", "tokens": ["Warst", "Rau\u00b7schen", "gr\u00fc\u00b7ner", "Wip\u00b7fel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Warst Krume des Bodens, der dich trug.", "tokens": ["Warst", "Kru\u00b7me", "des", "Bo\u00b7dens", ",", "der", "dich", "trug", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Dein Schicksal klopfte mit dem Blut,", "tokens": ["Dein", "Schick\u00b7sal", "klopf\u00b7te", "mit", "dem", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "das leicht und stark durch deine Adern schlug.", "tokens": ["das", "leicht", "und", "stark", "durch", "dei\u00b7ne", "A\u00b7dern", "schlug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "KON", "ADJD", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Aber dann kamen sie mit", "tokens": ["A\u00b7ber", "dann", "ka\u00b7men", "sie", "mit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "APPR"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.2": {"text": "Netzen und Zangen", "tokens": ["Net\u00b7zen", "und", "Zan\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Und haben dich", "tokens": ["Und", "ha\u00b7ben", "dich"], "token_info": ["word", "word", "word"], "pos": ["KON", "VAFIN", "PPER"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "eingefangen.", "tokens": ["ein\u00b7ge\u00b7fan\u00b7gen", "."], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Und wollten", "tokens": ["Und", "woll\u00b7ten"], "token_info": ["word", "word"], "pos": ["KON", "VMFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "von ihren schlechten S\u00e4ften", "tokens": ["von", "ih\u00b7ren", "schlech\u00b7ten", "S\u00e4f\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "In dich verspritzen,", "tokens": ["In", "dich", "ver\u00b7sprit\u00b7zen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVINF", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "dein Raubtierblut zu entkr\u00e4ften.", "tokens": ["dein", "Raub\u00b7tier\u00b7blut", "zu", "ent\u00b7kr\u00e4f\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Du hast sie abgesch\u00fcttelt.", "tokens": ["Du", "hast", "sie", "ab\u00b7ge\u00b7sch\u00fct\u00b7telt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Aber eine gro\u00dfe Traurigkeit", "tokens": ["A\u00b7ber", "ei\u00b7ne", "gro\u00b7\u00dfe", "Trau\u00b7rig\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.11": {"text": "Kam \u00fcber dich und schwamm in deinen Blicken,", "tokens": ["Kam", "\u00fc\u00b7ber", "dich", "und", "schwamm", "in", "dei\u00b7nen", "Bli\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPER", "KON", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "die die Herrlichkeit", "tokens": ["die", "die", "Herr\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word"], "pos": ["ART", "ART", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.13": {"text": "Noch hielten jener schweigend jungen Sch\u00f6pfungslust.", "tokens": ["Noch", "hiel\u00b7ten", "je\u00b7ner", "schwei\u00b7gend", "jun\u00b7gen", "Sch\u00f6p\u00b7fungs\u00b7lust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PDAT", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Du trugst", "tokens": ["Du", "trugst"], "token_info": ["word", "word"], "pos": ["PPER", "VVFIN"], "meter": "-+", "measure": "iambic.single"}, "line.15": {"text": "Die Ketten, die sie dir geschmiedet.", "tokens": ["Die", "Ket\u00b7ten", ",", "die", "sie", "dir", "ge\u00b7schmie\u00b7det", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Schlugst", "tokens": ["Schlugst"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.17": {"text": "Sie nicht zu Boden, da sie dich in ihre Zellen schlossen.", "tokens": ["Sie", "nicht", "zu", "Bo\u00b7den", ",", "da", "sie", "dich", "in", "ih\u00b7re", "Zel\u00b7len", "schlos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "APPR", "NN", "$,", "KOUS", "PPER", "PRF", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.18": {"text": "Spiest ihnen nicht,", "tokens": ["Spiest", "ih\u00b7nen", "nicht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.19": {"text": "Da sie den Schacherpreis betasteten,", "tokens": ["Da", "sie", "den", "Scha\u00b7cher\u00b7preis", "be\u00b7tas\u00b7te\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "ins schmatzende Gesicht.", "tokens": ["ins", "schmat\u00b7zen\u00b7de", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+---+", "measure": "dactylic.init"}, "line.21": {"text": "Du kauertest vor deinem Weh", "tokens": ["Du", "kau\u00b7er\u00b7test", "vor", "dei\u00b7nem", "Weh"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "und horchtest auf der Sterne Lauf ...", "tokens": ["und", "horch\u00b7test", "auf", "der", "Ster\u00b7ne", "Lauf", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Aber immer noch st\u00fcrzt dein Blut,", "tokens": ["A\u00b7ber", "im\u00b7mer", "noch", "st\u00fcrzt", "dein", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.24": {"text": "wie heftige Str\u00f6mung, ab und auf,", "tokens": ["wie", "hef\u00b7ti\u00b7ge", "Str\u00f6\u00b7mung", ",", "ab", "und", "auf", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "$,", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.25": {"text": "Und deine Augen, wie zwei ruhelose Tiere schweifen", "tokens": ["Und", "dei\u00b7ne", "Au\u00b7gen", ",", "wie", "zwei", "ru\u00b7he\u00b7lo\u00b7se", "Tie\u00b7re", "schwei\u00b7fen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "$,", "KOKOM", "CARD", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.26": {"text": "In die Welt hinaus und greifen", "tokens": ["In", "die", "Welt", "hin\u00b7aus", "und", "grei\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PTKVZ", "KON", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.27": {"text": "Ins Gew\u00fchl, als wollten sie das Schicksal packen,", "tokens": ["Ins", "Ge\u00b7w\u00fchl", ",", "als", "woll\u00b7ten", "sie", "das", "Schick\u00b7sal", "pa\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "KOUS", "VMFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.28": {"text": "Und dein schwarzes Haar schl\u00e4gt herrisch dir im Nacken,", "tokens": ["Und", "dein", "schwar\u00b7zes", "Haar", "schl\u00e4gt", "her\u00b7risch", "dir", "im", "Na\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVFIN", "ADJD", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.29": {"text": "Eine windentrollte Fahne, die zum Sturme weht \u2013", "tokens": ["Ei\u00b7ne", "win\u00b7dent\u00b7roll\u00b7te", "Fah\u00b7ne", ",", "die", "zum", "Stur\u00b7me", "weht", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$("], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.30": {"text": "Auf! Rei\u00dfe dich empor! Die Barrikade steht!", "tokens": ["Auf", "!", "Rei\u00b7\u00dfe", "dich", "em\u00b7por", "!", "Die", "Bar\u00b7ri\u00b7ka\u00b7de", "steht", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "$.", "VVFIN", "PPER", "PTKVZ", "$.", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Der Himmel ist von tausend Freiheitsfackeln aufgehellt \u2013", "tokens": ["Der", "Him\u00b7mel", "ist", "von", "tau\u00b7send", "Frei\u00b7heits\u00b7fa\u00b7ckeln", "auf\u00b7ge\u00b7hellt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "CARD", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.32": {"text": "Brich aus, Raubtier,", "tokens": ["Brich", "aus", ",", "Raub\u00b7tier", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADJD", "PTKVZ", "$,", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.33": {"text": "St\u00fcrme an ihren erstarrten Reihen,", "tokens": ["St\u00fcr\u00b7me", "an", "ih\u00b7ren", "er\u00b7starr\u00b7ten", "Rei\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.34": {"text": "Aufgerissnen M\u00e4ulern, schreckerstickten Schreien", "tokens": ["Auf\u00b7ge\u00b7riss\u00b7nen", "M\u00e4u\u00b7lern", ",", "schre\u00b7cker\u00b7stick\u00b7ten", "Schrei\u00b7en"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.35": {"text": "Vorbei", "tokens": ["Vor\u00b7bei"], "token_info": ["word"], "pos": ["NN"], "meter": "-+", "measure": "iambic.single"}, "line.36": {"text": "In deine Welt!", "tokens": ["In", "dei\u00b7ne", "Welt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.37": {"text": "Brich aus, Raubtier!", "tokens": ["Brich", "aus", ",", "Raub\u00b7tier", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADJD", "PTKVZ", "$,", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.38": {"text": "Brich aus!", "tokens": ["Brich", "aus", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.3": {"line.1": {"text": "Du warst nackte Eva im Paradies,", "tokens": ["Du", "warst", "nack\u00b7te", "E\u00b7va", "im", "Pa\u00b7ra\u00b7dies", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "blank, windumspielt und ohne Scham.", "tokens": ["blank", ",", "win\u00b7dum\u00b7spielt", "und", "oh\u00b7ne", "Scham", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKVZ", "$,", "VVFIN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du wuchsest mit den Fr\u00fcchten und Tieren.", "tokens": ["Du", "wuch\u00b7sest", "mit", "den", "Fr\u00fcch\u00b7ten", "und", "Tie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "KON", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Der Morgen nahm", "tokens": ["Der", "Mor\u00b7gen", "nahm"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Dich aus dem Arm der Nacht,", "tokens": ["Dich", "aus", "dem", "Arm", "der", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "---+-+", "measure": "unknown.measure.di"}, "line.6": {"text": "und Abend bettete dich weich", "tokens": ["und", "A\u00b7bend", "bet\u00b7te\u00b7te", "dich", "weich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VVFIN", "PPER", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Zur m\u00fctterlichen Erde. Du warst wild und sch\u00f6n.", "tokens": ["Zur", "m\u00fct\u00b7ter\u00b7li\u00b7chen", "Er\u00b7de", ".", "Du", "warst", "wild", "und", "sch\u00f6n", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$.", "PPER", "VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Du warst den Tieren gleich.", "tokens": ["Du", "warst", "den", "Tie\u00b7ren", "gleich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Warst Rauschen gr\u00fcner Wipfel.", "tokens": ["Warst", "Rau\u00b7schen", "gr\u00fc\u00b7ner", "Wip\u00b7fel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Warst Krume des Bodens, der dich trug.", "tokens": ["Warst", "Kru\u00b7me", "des", "Bo\u00b7dens", ",", "der", "dich", "trug", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Dein Schicksal klopfte mit dem Blut,", "tokens": ["Dein", "Schick\u00b7sal", "klopf\u00b7te", "mit", "dem", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "das leicht und stark durch deine Adern schlug.", "tokens": ["das", "leicht", "und", "stark", "durch", "dei\u00b7ne", "A\u00b7dern", "schlug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "KON", "ADJD", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Aber dann kamen sie mit", "tokens": ["A\u00b7ber", "dann", "ka\u00b7men", "sie", "mit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "APPR"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.2": {"text": "Netzen und Zangen", "tokens": ["Net\u00b7zen", "und", "Zan\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "