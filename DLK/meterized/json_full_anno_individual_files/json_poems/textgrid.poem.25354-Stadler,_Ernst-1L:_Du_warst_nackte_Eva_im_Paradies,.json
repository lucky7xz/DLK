, "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.18": {"text": "Spiest ihnen nicht,", "tokens": ["Spiest", "ih\u00b7nen", "nicht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.19": {"text": "Da sie den Schacherpreis betasteten,", "tokens": ["Da", "sie", "den", "Scha\u00b7cher\u00b7preis", "be\u00b7tas\u00b7te\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "ins schmatzende Gesicht.", "tokens": ["ins", "schmat\u00b7zen\u00b7de", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+---+", "measure": "dactylic.init"}, "line.21": {"text": "Du kauertest vor deinem Weh", "tokens": ["Du", "kau\u00b7er\u00b7test", "vor", "dei\u00b7nem", "Weh"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "und horchtest auf der Sterne Lauf ...", "tokens": ["und", "horch\u00b7test", "auf", "der", "Ster\u00b7ne", "Lauf", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Aber immer noch st\u00fcrzt dein Blut,", "tokens": ["A\u00b7ber", "im\u00b7mer", "noch", "st\u00fcrzt", "dein", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.24": {"text": "wie heftige Str\u00f6mung, ab und auf,", "tokens": ["wie", "hef\u00b7ti\u00b7ge", "Str\u00f6\u00b7mung", ",", "ab", "und", "auf", ","], "token_info": ["