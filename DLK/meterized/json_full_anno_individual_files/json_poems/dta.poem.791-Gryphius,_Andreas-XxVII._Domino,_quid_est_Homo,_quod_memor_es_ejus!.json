{"dta.poem.791": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "XxVII.  \n Domino, quid est Homo, quod memor es ejus!", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Wjr armen! ach wie ists so bald mit vns geschehn! ", "tokens": ["Wjr", "ar\u00b7men", "!", "ach", "wie", "ists", "so", "bald", "mit", "vns", "ge\u00b7schehn", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "$.", "XY", "KOKOM", "VAFIN", "ADV", "ADV", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie pl\u00f6tzlich gehn wir fort/ offt/ eh wir vns besinnen", "tokens": ["Wie", "pl\u00f6tz\u00b7lich", "gehn", "wir", "fort", "/", "offt", "/", "eh", "wir", "vns", "be\u00b7sin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VVFIN", "PPER", "PTKVZ", "$(", "ADV", "$(", "KOUS", "PPER", "PRF", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Rufft vn\u00df der schnelle Todt: komm\u2019t Menschen/ kommt von", "tokens": ["Rufft", "vn\u00df", "der", "schnel\u00b7le", "Todt", ":", "kom\u00b7m't", "Men\u00b7schen", "/", "kommt", "von"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$.", "VVFIN", "NN", "$(", "VVFIN", "APPR"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "hinnen", "tokens": ["hin\u00b7nen"], "token_info": ["word"], "pos": ["ADV"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Kan jemand was jhm drew\u2019t/ was jtzt gleich anbricht", "tokens": ["Kan", "je\u00b7mand", "was", "jhm", "dre\u00b7w't", "/", "was", "jtzt", "gleich", "an\u00b7bricht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VMFIN", "PIS", "PWS", "PPER", "VVFIN", "$(", "PWS", "ADV", "ADV", "VVFIN"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "sehn?", "tokens": ["sehn", "?"], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+", "measure": "single.up"}}, "stanza.2": {"line.1": {"text": "Wir spielen sorgenfrey/ wir schimpffen/ lachen/ schmehn!", "tokens": ["Wir", "spie\u00b7len", "sor\u00b7gen\u00b7frey", "/", "wir", "schimpf\u00b7fen", "/", "la\u00b7chen", "/", "schmehn", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$(", "PPER", "VVFIN", "$(", "VVINF", "$(", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch vnser End ist dar. Wir werden gantz nicht jnnen", "tokens": ["Doch", "vn\u00b7ser", "End", "ist", "dar", ".", "Wir", "wer\u00b7den", "gantz", "nicht", "jn\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "PTKVZ", "$.", "PPER", "VAFIN", "ADV", "PTKNEG", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wie nahe wir der grufft. di\u00df Leben mu\u00df zerrinnen", "tokens": ["Wie", "na\u00b7he", "wir", "der", "grufft", ".", "di\u00df", "Le\u00b7ben", "mu\u00df", "zer\u00b7rin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "PPER", "ART", "NN", "$.", "PDS", "NN", "VMFIN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wenn Gott nicht beystand schickt/ ehr wir vmb beyftand", "tokens": ["Wenn", "Gott", "nicht", "beys\u00b7tand", "schickt", "/", "ehr", "wir", "vmb", "beyf\u00b7tand"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "PTKNEG", "PIS", "VVFIN", "$(", "KOUS", "PPER", "APPR", "PIS"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "flehn.", "tokens": ["flehn", "."], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+", "measure": "single.up"}}, "stanza.3": {"line.1": {"text": "Ein Stein/ ein st\u00fccklin Bley/ vnd ein vergifftend schnauben.", "tokens": ["Ein", "Stein", "/", "ein", "st\u00fcck\u00b7lin", "Bley", "/", "vnd", "ein", "ver\u00b7giff\u00b7tend", "schnau\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "ADJA", "NN", "$(", "KON", "ART", "VVPP", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein fall/ ein Wassertropff/ kan vn\u00df di\u00df leben rauben", "tokens": ["Ein", "fall", "/", "ein", "Was\u00b7ser\u00b7tropff", "/", "kan", "vn\u00df", "di\u00df", "le\u00b7ben", "rau\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$(", "ART", "NN", "$(", "VMFIN", "APPR", "PDS", "VVINF", "VVINF"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Geschwinder de\u00f1 es Pest/ v\u00f1 frost/ vnd Schwindsuchtthut.", "tokens": ["Ge\u00b7schwin\u00b7der", "de\u00f1", "es", "Pest", "/", "v\u00f1", "frost", "/", "vnd", "Schwind\u00b7sucht\u00b7thut", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$(", "FM", "FM", "$(", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Wir sorgen nur vmbsonst/ wenn Gott nicht f\u00fcr vns wachet/", "tokens": ["Wir", "sor\u00b7gen", "nur", "vmbsonst", "/", "wenn", "Gott", "nicht", "f\u00fcr", "vns", "wa\u00b7chet", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "$(", "KOUS", "NN", "PTKNEG", "APPR", "PPER", "VVFIN", "$("], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wenn er nicht Wall vnd Burg vnd L\u00e4ger vmb vns machet.", "tokens": ["Wenn", "er", "nicht", "Wall", "vnd", "Burg", "vnd", "L\u00e4\u00b7ger", "vmb", "vns", "ma\u00b7chet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "NE", "KON", "NN", "KON", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der ist schon lebend-todt der nicht in seiner Hut.", "tokens": ["Der", "ist", "schon", "le\u00b7ben\u00b7dtodt", "der", "nicht", "in", "sei\u00b7ner", "Hut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADJD", "ART", "PTKNEG", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}