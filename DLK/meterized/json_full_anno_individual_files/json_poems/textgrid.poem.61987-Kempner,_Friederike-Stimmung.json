{"textgrid.poem.61987": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "Stimmung", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "D\u00fcster liegt die Welt mir da,", "tokens": ["D\u00fcs\u00b7ter", "liegt", "die", "Welt", "mir", "da", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "PPER", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie ein \u00f6des Meer,", "tokens": ["Wie", "ein", "\u00f6\u00b7des", "Meer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Und der Abgrund ist so nah,", "tokens": ["Und", "der", "Ab\u00b7grund", "ist", "so", "nah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und er reizt mich sehr.", "tokens": ["Und", "er", "reizt", "mich", "sehr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Drin vergessen und versenken,", "tokens": ["Drin", "ver\u00b7ges\u00b7sen", "und", "ver\u00b7sen\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "KON", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Selbst das Sch\u00f6ne mit,", "tokens": ["Selbst", "das", "Sch\u00f6\u00b7ne", "mit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Nichts mehr f\u00fchlen, nichts mehr denken,", "tokens": ["Nichts", "mehr", "f\u00fch\u00b7len", ",", "nichts", "mehr", "den\u00b7ken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "VVINF", "$,", "PIS", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Erde, wir sind quitt!", "tokens": ["Er\u00b7de", ",", "wir", "sind", "quitt", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VAFIN", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Keine Lust ist's, keine Wonne,", "tokens": ["Kei\u00b7ne", "Lust", "ist's", ",", "kei\u00b7ne", "Won\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "$,", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aber mehr als Das,", "tokens": ["A\u00b7ber", "mehr", "als", "Das", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "KOKOM", "PDS", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Keinen Schatten, keine Sonne, \u2013", "tokens": ["Kei\u00b7nen", "Schat\u00b7ten", ",", "kei\u00b7ne", "Son\u00b7ne", ",", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "$,", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Keine Lieb', kein Ha\u00df! \u2013", "tokens": ["Kei\u00b7ne", "Lieb'", ",", "kein", "Ha\u00df", "!", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "$.", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Denn im Nichts die Freiheit lieget,", "tokens": ["Denn", "im", "Nichts", "die", "Frei\u00b7heit", "lie\u00b7get", ","], "token_info": ["