{"textgrid.poem.60423": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "1L: Im tiefen Walde ging die Poesie", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im tiefen Walde ging die Poesie", "tokens": ["Im", "tie\u00b7fen", "Wal\u00b7de", "ging", "die", "Poe\u00b7sie"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Pfade heilger Abgeschiedenheit,", "tokens": ["Die", "Pfa\u00b7de", "heil\u00b7ger", "Ab\u00b7ge\u00b7schie\u00b7den\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da bricht ein lauter Schwarm herein und schreit", "tokens": ["Da", "bricht", "ein", "lau\u00b7ter", "Schwarm", "her\u00b7ein", "und", "schreit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "PIAT", "NN", "PTKVZ", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der Selbstversunknen zu: \u00bbWas suchst du hie?", "tokens": ["Der", "Selbst\u00b7ver\u00b7sun\u00b7knen", "zu", ":", "\u00bb", "Was", "suchst", "du", "hie", "?"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$.", "$(", "PWS", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "La\u00df doch die Blumen bl\u00fchn, die B\u00e4ume rauschen,", "tokens": ["La\u00df", "doch", "die", "Blu\u00b7men", "bl\u00fchn", ",", "die", "B\u00e4u\u00b7me", "rau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ART", "NN", "VVINF", "$,", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und schw\u00e4rme nicht unpraktisch weiche Klage,", "tokens": ["Und", "schw\u00e4r\u00b7me", "nicht", "un\u00b7prak\u00b7tisch", "wei\u00b7che", "Kla\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Denn mannhaftwehrhaft sind nunmehr die Tage,", "tokens": ["Denn", "mann\u00b7haft\u00b7wehr\u00b7haft", "sind", "nun\u00b7mehr", "die", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Du wirst dem Wald kein wirksam Lied entlauschen.", "tokens": ["Du", "wirst", "dem", "Wald", "kein", "wirk\u00b7sam", "Lied", "ent\u00b7lau\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PIAT", "ADJD", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Komm, komm mit uns, verding uns deine Kr\u00e4fte;", "tokens": ["Komm", ",", "komm", "mit", "uns", ",", "ver\u00b7ding", "uns", "dei\u00b7ne", "Kr\u00e4f\u00b7te", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "APPR", "PPER", "$,", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Wir wollen reich dir jeden Schritt bezahlen", "tokens": ["Wir", "wol\u00b7len", "reich", "dir", "je\u00b7den", "Schritt", "be\u00b7zah\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADJD", "PPER", "PIAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Mit blankgem\u00fcnztem Lobe in Journalen,", "tokens": ["Mit", "blank\u00b7ge\u00b7m\u00fcnz\u00b7tem", "Lo\u00b7be", "in", "Jour\u00b7na\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Heb dich zum weltbegl\u00fcckenden Gesch\u00e4fte! \u2013", "tokens": ["Heb", "dich", "zum", "welt\u00b7be\u00b7gl\u00fc\u00b7cken\u00b7den", "Ge\u00b7sch\u00e4f\u00b7te", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVIMP", "PPER", "APPRART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "La\u00df nicht dein Herz in Einsamkeit verdumpfen,", "tokens": ["La\u00df", "nicht", "dein", "Herz", "in", "Ein\u00b7sam\u00b7keit", "ver\u00b7dump\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKNEG", "PPOSAT", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Erwach aus Tr\u00e4umen, werde sozial,", "tokens": ["Er\u00b7wach", "aus", "Tr\u00e4u\u00b7men", ",", "wer\u00b7de", "so\u00b7zi\u00b7al", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,", "VAFIN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Weih dich dem Tatendrange zum Gemahl;", "tokens": ["Weih", "dich", "dem", "Ta\u00b7ten\u00b7dran\u00b7ge", "zum", "Ge\u00b7mahl", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "APPRART", "NN", "$."], "meter": "++-+-+-+-+", "measure": "iambic.penta.spondeus"}, "line.16": {"text": "Zur alten Jungfer wirst du sonst verschrumpfen!\u00ab", "tokens": ["Zur", "al\u00b7ten", "Jung\u00b7fer", "wirst", "du", "sonst", "ver\u00b7schrump\u00b7fen", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "ADJA", "NN", "VAFIN", "PPER", "ADV", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Die Poesie dem Schwarm antwortend spricht:", "tokens": ["Die", "Poe\u00b7sie", "dem", "Schwarm", "ant\u00b7wor\u00b7tend", "spricht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVPP", "VVFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.18": {"text": "\u00bbla\u00dft mich! verd\u00e4chtig ist mir euer Streben;", "tokens": ["\u00bb", "la\u00dft", "mich", "!", "ver\u00b7d\u00e4ch\u00b7tig", "ist", "mir", "eu\u00b7er", "Stre\u00b7ben", ";"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "PPER", "$.", "ADJD", "VAFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Befreien wollt ihr das gejochte Leben", "tokens": ["Be\u00b7frei\u00b7en", "wollt", "ihr", "das", "ge\u00b7joch\u00b7te", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VMFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Und g\u00f6nnt sogar der Kunst die Freiheit nicht?", "tokens": ["Und", "g\u00f6nnt", "so\u00b7gar", "der", "Kunst", "die", "Frei\u00b7heit", "nicht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "ART", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Euch sank zu tief ins Aug die Nebelkappe,", "tokens": ["Euch", "sank", "zu", "tief", "ins", "Aug", "die", "Ne\u00b7bel\u00b7kap\u00b7pe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKA", "ADJD", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Wenn euer Blick nicht stra\u00dfen\u00fcber sieht,", "tokens": ["Wenn", "eu\u00b7er", "Blick", "nicht", "stra\u00b7\u00dfen\u00b7\u00fc\u00b7ber", "sieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PTKNEG", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Und wenn ihr heischt vom freigebornen Lied,", "tokens": ["Und", "wenn", "ihr", "heischt", "vom", "frei\u00b7ge\u00b7bor\u00b7nen", "Lied", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Da\u00df es dienstbar nur eure Gleise tappe.", "tokens": ["Da\u00df", "es", "dienst\u00b7bar", "nur", "eu\u00b7re", "Glei\u00b7se", "tap\u00b7pe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "Ein Blumenantlitz hat noch nie gelogen,", "tokens": ["Ein", "Blu\u00b7men\u00b7ant\u00b7litz", "hat", "noch", "nie", "ge\u00b7lo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "Und sichrer bl\u00fcht es mir ins Herz die Kunde,", "tokens": ["Und", "sich\u00b7rer", "bl\u00fcht", "es", "mir", "ins", "Herz", "die", "Kun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "VVFIN", "PPER", "PPER", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Da\u00df heilen wird der Menschheit tiefe Wunde,", "tokens": ["Da\u00df", "hei\u00b7len", "wird", "der", "Menschheit", "tie\u00b7fe", "Wun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVINF", "VAFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.28": {"text": "Als euer wirres Antlitz, wutverzogen.", "tokens": ["Als", "eu\u00b7er", "wir\u00b7res", "Ant\u00b7litz", ",", "wut\u00b7ver\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "Prophetisch rauscht der Wald: die Welt wird freit!", "tokens": ["Pro\u00b7phe\u00b7tisch", "rauscht", "der", "Wald", ":", "die", "Welt", "wird", "freit", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$.", "ART", "NN", "VAFIN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.30": {"text": "Er rauscht es lauter mir als ", "tokens": ["Er", "rauscht", "es", "lau\u00b7ter", "mir", "als"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "PPER", "KOKOM"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.31": {"text": "Mit all dem seelenlosen Wortgeschmetter,", "tokens": ["Mit", "all", "dem", "see\u00b7len\u00b7lo\u00b7sen", "Wort\u00b7ge\u00b7schmet\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.32": {"text": "Mit all der matten Eisenfresserei.", "tokens": ["Mit", "all", "der", "mat\u00b7ten", "Ei\u00b7sen\u00b7fres\u00b7se\u00b7rei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.33": {"text": "Wenn mirs beliebt, werd ich hier Blumen pfl\u00fccken;", "tokens": ["Wenn", "mirs", "be\u00b7liebt", ",", "werd", "ich", "hier", "Blu\u00b7men", "pfl\u00fc\u00b7cken", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "ADJD", "$,", "VAFIN", "PPER", "ADV", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.34": {"text": "Wenn mirs beliebt, werd ich von Freiheit singen;", "tokens": ["Wenn", "mirs", "be\u00b7liebt", ",", "werd", "ich", "von", "Frei\u00b7heit", "sin\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "ADJD", "$,", "VAFIN", "PPER", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.35": {"text": "Doch nimmermehr la\u00df ich von euch mich dingen!\u00ab", "tokens": ["Doch", "nim\u00b7mer\u00b7mehr", "la\u00df", "ich", "von", "euch", "mich", "din\u00b7gen", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADV", "VVIMP", "PPER", "APPR", "PPER", "PRF", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.36": {"text": "Sie sprichts und kehrt dem rohen Schwarm den R\u00fccken.", "tokens": ["Sie", "sprichts", "und", "kehrt", "dem", "ro\u00b7hen", "Schwarm", "den", "R\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Im tiefen Walde ging die Poesie", "tokens": ["Im", "tie\u00