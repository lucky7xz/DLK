{"dta.poem.11384": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Der mit geitz verm\u00e4hlte eigen-sinn.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.85", "en:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mein freund! ich frage dich: Ists wohl der rede werth,", "tokens": ["Mein", "freund", "!", "ich", "fra\u00b7ge", "dich", ":", "Ists", "wohl", "der", "re\u00b7de", "werth", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "PPER", "VVFIN", "PPER", "$.", "NE", "ADV", "ART", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df die erz\u00fcrnte laus dir in die leber f\u00e4hrt,", "tokens": ["Da\u00df", "die", "er\u00b7z\u00fcrn\u00b7te", "laus", "dir", "in", "die", "le\u00b7ber", "f\u00e4hrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Indem ich blos allein zum zeit-vertreib geschrieben,", "tokens": ["In\u00b7dem", "ich", "blos", "al\u00b7lein", "zum", "zeit\u00b7ver\u00b7treib", "ge\u00b7schrie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und du an ehr und gut gantz unbesch\u00e4digt blieben?", "tokens": ["Und", "du", "an", "ehr", "und", "gut", "gantz", "un\u00b7be\u00b7sch\u00e4\u00b7digt", "blie\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "NN", "KON", "ADJD", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Denn was in solcher schrifft zum possen vorgebracht,", "tokens": ["Denn", "was", "in", "sol\u00b7cher", "schrifft", "zum", "pos\u00b7sen", "vor\u00b7ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "APPR", "PIAT", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Hat dich gewi\u00df nicht kranck, geschweige todt, gemacht.", "tokens": ["Hat", "dich", "ge\u00b7wi\u00df", "nicht", "kranck", ",", "ge\u00b7schwei\u00b7ge", "todt", ",", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "PTKNEG", "ADJD", "$,", "ADJA", "ADJD", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Was gilts? wenn andern man dergleichen schreiben wollen,", "tokens": ["Was", "gilts", "?", "wenn", "an\u00b7dern", "man", "derg\u00b7lei\u00b7chen", "schrei\u00b7ben", "wol\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$.", "KOUS", "PIS", "PIS", "PIS", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wie h\u00e4ttest du alsbald so treulich helffen sollen!", "tokens": ["Wie", "h\u00e4t\u00b7test", "du", "als\u00b7bald", "so", "treu\u00b7lich", "helf\u00b7fen", "sol\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ich glaub auch sicherlich, da\u00df keinem ausser dir", "tokens": ["Ich", "glaub", "auch", "si\u00b7cher\u00b7lich", ",", "da\u00df", "kei\u00b7nem", "aus\u00b7ser", "dir"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "$,", "KOUS", "PIS", "APPR", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Dergleichen lapperey empfindlich k\u00e4me f\u00fcr.", "tokens": ["Derg\u00b7lei\u00b7chen", "lap\u00b7pe\u00b7rey", "emp\u00b7find\u00b7lich", "k\u00e4\u00b7me", "f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "ADJD", "VVFIN", "APPR", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "So ists, wer gerne tantzt, dem ist gar leicht zu pfeiffen:", "tokens": ["So", "ists", ",", "wer", "ger\u00b7ne", "tantzt", ",", "dem", "ist", "gar", "leicht", "zu", "pfeif\u00b7fen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "$,", "PWS", "ADV", "VVFIN", "$,", "PDS", "VAFIN", "ADV", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Dein gantzes naturell ist murren, klagen, keiffen,", "tokens": ["Dein", "gant\u00b7zes", "na\u00b7tu\u00b7rell", "ist", "mur\u00b7ren", ",", "kla\u00b7gen", ",", "keif\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "VVINF", "$,", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Drum mag bald etwas seyn, das dein gebl\u00fcth erhitzt.", "tokens": ["Drum", "mag", "bald", "et\u00b7was", "seyn", ",", "das", "dein", "ge\u00b7bl\u00fcth", "er\u00b7hitzt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "ADV", "PIS", "VAINF", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Wenn eine fliege nur etwas zu lange sitzt,", "tokens": ["Wenn", "ei\u00b7ne", "flie\u00b7ge", "nur", "et\u00b7was", "zu", "lan\u00b7ge", "sitzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "ADV", "ADV", "PTKA", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Bist du schon sorgen-voll, da\u00df bey so langem harren", "tokens": ["Bist", "du", "schon", "sor\u00b7gen\u00b7voll", ",", "da\u00df", "bey", "so", "lan\u00b7gem", "har\u00b7ren"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "$,", "KOUS", "APPR", "ADV", "ADJA", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Sie m\u00f6ge dir den kalck von deinen w\u00e4nden scharren.", "tokens": ["Sie", "m\u00f6\u00b7ge", "dir", "den", "kalck", "von", "dei\u00b7nen", "w\u00e4n\u00b7den", "schar\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Ja, brummst du bey dir selbst, es kan gar licht ge-", "tokens": ["Ja", ",", "brummst", "du", "bey", "dir", "selbst", ",", "es", "kan", "gar", "licht", "ge"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "VVFIN", "PPER", "APPR", "PPER", "ADV", "$,", "PPER", "VMFIN", "ADV", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "schehn,", "tokens": ["schehn", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.19": {"text": "Da\u00df man auf solche art die wand mu\u00df l\u00f6chricht sehn.", "tokens": ["Da\u00df", "man", "auf", "sol\u00b7che", "art", "die", "wand", "mu\u00df", "l\u00f6ch\u00b7richt", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "PIAT", "NN", "ART", "NN", "VMFIN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Du z\u00fcrnest bitterlich, wenn molcken um die lichte", "tokens": ["Du", "z\u00fcr\u00b7nest", "bit\u00b7ter\u00b7lich", ",", "wenn", "mol\u00b7cken", "um", "die", "lich\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "$,", "KOUS", "NN", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Umschw\u00e4rmend kommen an, dir fliegen ins gesichte,", "tokens": ["Um\u00b7schw\u00e4r\u00b7mend", "kom\u00b7men", "an", ",", "dir", "flie\u00b7gen", "ins", "ge\u00b7sich\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "PTKVZ", "$,", "PPER", "VVFIN", "APPRART", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Du schl\u00e4gest hefftig zu, und sprichst: Wenn man be-", "tokens": ["Du", "schl\u00e4\u00b7gest", "heff\u00b7tig", "zu", ",", "und", "sprichst", ":", "Wenn", "man", "be"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "PTKVZ", "$,", "KON", "VVFIN", "$.", "KOUS", "PIS", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "denckt,", "tokens": ["denckt", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.24": {"text": "Wie sehr die nahrung wird durch solch ein aas ge-", "tokens": ["Wie", "sehr", "die", "nah\u00b7rung", "wird", "durch", "solch", "ein", "aas", "ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ART", "NN", "VAFIN", "APPR", "PIAT", "ART", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "kr\u00e4nckt,", "tokens": ["kr\u00e4nckt", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-", "measure": "single.down"}, "line.26": {"text": "Und wie viel talg nicht bleibt an seinen fl\u00fcgeln hangen.", "tokens": ["Und", "wie", "viel", "talg", "nicht", "bleibt", "an", "sei\u00b7nen", "fl\u00fc\u00b7geln", "han\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PIAT", "NN", "PTKNEG", "VVFIN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Was man bey einem licht kan sehen und erlangen,", "tokens": ["Was", "man", "bey", "ei\u00b7nem", "licht", "kan", "se\u00b7hen", "und", "er\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "APPR", "ART", "NN", "VMFIN", "VVINF", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Da braucht man solcher zwey. Legst du dich dann zur", "tokens": ["Da", "braucht", "man", "sol\u00b7cher", "zwey", ".", "Legst", "du", "dich", "dann", "zur"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "PIAT", "CARD", "$.", "VVFIN", "PPER", "PRF", "ADV", "APPRART"], "meter": "-+-+--+-++-", "measure": "iambic.penta.relaxed"}, "line.29": {"text": "ruh,", "tokens": ["ruh", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.30": {"text": "Bringst du die gantze nacht mit solchen dingen zu,", "tokens": ["Bringst", "du", "die", "gant\u00b7ze", "nacht", "mit", "sol\u00b7chen", "din\u00b7gen", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "APPR", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Die nicht der m\u00fche werth: Denckst, was in drey\u00dfig jahren", "tokens": ["Die", "nicht", "der", "m\u00fc\u00b7he", "werth", ":", "Denckst", ",", "was", "in", "drey\u00b7\u00dfig", "jah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "PTKNEG", "ART", "NN", "ADJD", "$.", "VVFIN", "$,", "PRELS", "APPR", "CARD", "NN"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.32": {"text": "Dir noch vor ungemach einst k\u00f6nne wiederfahren:", "tokens": ["Dir", "noch", "vor", "un\u00b7ge\u00b7mach", "einst", "k\u00f6n\u00b7ne", "wie\u00b7der\u00b7fah\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "ADJD", "ADV", "VMFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Und was ein ander l\u00e4\u00dft auf GOtt und zeit beruhn,", "tokens": ["Und", "was", "ein", "an\u00b7der", "l\u00e4\u00dft", "auf", "Gott", "und", "zeit", "be\u00b7ruhn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "ADJD", "VVFIN", "APPR", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Wilst du durch aberwitz und eigne sorgfalt thun.", "tokens": ["Wilst", "du", "durch", "a\u00b7ber\u00b7witz", "und", "eig\u00b7ne", "sorg\u00b7falt", "thun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "NE", "KON", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Was, sprichst du bey dir selbst, ist aus des nachbars garten", "tokens": ["Was", ",", "sprichst", "du", "bey", "dir", "selbst", ",", "ist", "aus", "des", "nach\u00b7bars", "gar\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "$,", "VVFIN", "PPER", "APPR", "PPER", "ADV", "$,", "VAFIN", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Vor mich nicht vor verlust und schade zu gewarten?", "tokens": ["Vor", "mich", "nicht", "vor", "ver\u00b7lust", "und", "scha\u00b7de", "zu", "ge\u00b7war\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PTKNEG", "APPR", "NN", "KON", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Die reiser, so er hat nicht weit von meiner wand", "tokens": ["Die", "rei\u00b7ser", ",", "so", "er", "hat", "nicht", "weit", "von", "mei\u00b7ner", "wand"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "$,", "ADV", "PPER", "VAFIN", "PTKNEG", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Gar ordentlich gepflantzt, die k\u00f6nnen nach der hand", "tokens": ["Gar", "or\u00b7dent\u00b7lich", "ge\u00b7pflantzt", ",", "die", "k\u00f6n\u00b7nen", "nach", "der", "hand"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVPP", "$,", "PRELS", "VMFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Als b\u00e4ume, die belaubt, in vollem wachsthum stehen:", "tokens": ["Als", "b\u00e4u\u00b7me", ",", "die", "be\u00b7laubt", ",", "in", "vol\u00b7lem", "wach\u00b7sthum", "ste\u00b7hen", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "PRELS", "VVPP", "$,", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Ey, wenn sie nun den thau und regen lassen gehen,", "tokens": ["Ey", ",", "wenn", "sie", "nun", "den", "thau", "und", "re\u00b7gen", "las\u00b7sen", "ge\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "ADV", "ART", "NN", "KON", "ADJA", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Auf meine gute wand, so mags nicht anders seyn,", "tokens": ["Auf", "mei\u00b7ne", "gu\u00b7te", "wand", ",", "so", "mags", "nicht", "an\u00b7ders", "seyn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "VVFIN", "$,", "ADV", "VMFIN", "PTKNEG", "ADV", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Sie f\u00e4llt in kurtzer zeit bi\u00df auf den grundstein ein.", "tokens": ["Sie", "f\u00e4llt", "in", "kurt\u00b7zer", "zeit", "bi\u00df", "auf", "den", "grund\u00b7stein", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "APPR", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Was leiden und gefahr hab ich noch zu vermuthen,", "tokens": ["Was", "lei\u00b7den", "und", "ge\u00b7fahr", "hab", "ich", "noch", "zu", "ver\u00b7mu\u00b7then", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVINF", "KON", "NN", "VAFIN", "PPER", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Wenn grosser regen k\u00f6mmt, da\u00df mich die strengen fluthen", "tokens": ["Wenn", "gros\u00b7ser", "re\u00b7gen", "k\u00f6mmt", ",", "da\u00df", "mich", "die", "stren\u00b7gen", "flut\u00b7hen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJA", "ADJA", "VVFIN", "$,", "KOUS", "PPER", "ART", "ADJA", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Nicht nehmen gar dahin? Schon zweymahl ists ge-", "tokens": ["Nicht", "neh\u00b7men", "gar", "da\u00b7hin", "?", "Schon", "zwey\u00b7mahl", "ists", "ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKNEG", "VVFIN", "ADV", "PAV", "$.", "ADV", "ADV", "VAFIN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.46": {"text": "schehn,", "tokens": ["schehn", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.47": {"text": "Da\u00df man bey einem haar mich untergehen sehn.", "tokens": ["Da\u00df", "man", "bey", "ei\u00b7nem", "haar", "mich", "un\u00b7ter\u00b7ge\u00b7hen", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "ART", "NN", "PPER", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Der hencker hat sein spiel, wie balde kan sichs f\u00fcgen,", "tokens": ["Der", "hen\u00b7cker", "hat", "sein", "spiel", ",", "wie", "bal\u00b7de", "kan", "sichs", "f\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "VVFIN", "$,", "PWAV", "ADV", "VMFIN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.49": {"text": "Da\u00df mir ein grosser storch kan an die scheune fliegen.", "tokens": ["Da\u00df", "mir", "ein", "gros\u00b7ser", "storch", "kan", "an", "die", "scheu\u00b7ne", "flie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VMFIN", "APPR", "ART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.50": {"text": "Wie wenn mein kleinster sohn ein schwalben-nest zer-", "tokens": ["Wie", "wenn", "mein", "kleins\u00b7ter", "sohn", "ein", "schwal\u00b7ben\u00b7nest", "zer"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOKOM", "KOUS", "PPOSAT", "ADJA", "NN", "ART", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.51": {"text": "bricht,", "tokens": ["bricht", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-", "measure": "single.down"}, "line.52": {"text": "Und diese schwalbe mir hernach die k\u00fche sticht:", "tokens": ["Und", "die\u00b7se", "schwal\u00b7be", "mir", "her\u00b7nach", "die", "k\u00fc\u00b7he", "sticht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "PPER", "ADV", "ART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.53": {"text": "So k\u00f6nt ich ja f\u00fcrwahr bey so gestalten sachen", "tokens": ["So", "k\u00f6nt", "ich", "ja", "f\u00fcr\u00b7wahr", "bey", "so", "ge\u00b7stal\u00b7ten", "sa\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "APPR", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.54": {"text": "Gar keine butter nicht, und wenig k\u00e4se machen.", "tokens": ["Gar", "kei\u00b7ne", "but\u00b7ter", "nicht", ",", "und", "we\u00b7nig", "k\u00e4\u00b7se", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "PTKNEG", "$,", "KON", "PIS", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.55": {"text": "Di\u00df sey von diesem gnung. Kommt dann die erndte-zeit,", "tokens": ["Di\u00df", "sey", "von", "die\u00b7sem", "gnung", ".", "Kommt", "dann", "die", "ernd\u00b7te\u00b7zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "PDAT", "NN", "$.", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.56": {"text": "Die sonsten jedermann vergn\u00fcget und erfreut,", "tokens": ["Die", "sons\u00b7ten", "je\u00b7der\u00b7mann", "ver\u00b7gn\u00fc\u00b7get", "und", "er\u00b7freut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PIS", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.57": {"text": "So hast du deine last, durchrennest felder, fluhren,", "tokens": ["So", "hast", "du", "dei\u00b7ne", "last", ",", "durch\u00b7ren\u00b7nest", "fel\u00b7der", ",", "fluh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "VVFIN", "$,", "VVFIN", "PTKVZ", "$,", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.58": {"text": "Besiehst der menschen tritt, und zehlst der m\u00e4use spuhren.", "tokens": ["Be\u00b7siehst", "der", "men\u00b7schen", "tritt", ",", "und", "zehlst", "der", "m\u00e4u\u00b7se", "spuh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "VVFIN", "$,", "KON", "VVFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.59": {"text": "An deinem acker ist f\u00fcrwahr kein leerer rein,", "tokens": ["An", "dei\u00b7nem", "ac\u00b7ker", "ist", "f\u00fcr\u00b7wahr", "kein", "lee\u00b7rer", "rein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJD", "VAFIN", "ADV", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.60": {"text": "Er mu\u00df, wie du vermeinst, gantz abgeschnitten seyn;", "tokens": ["Er", "mu\u00df", ",", "wie", "du", "ver\u00b7meinst", ",", "gantz", "ab\u00b7ge\u00b7schnit\u00b7ten", "seyn", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "PWAV", "PPER", "VVFIN", "$,", "ADV", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.61": {"text": "Wird denn nach deinem wunsch, und \u00fcber sein verhoffen,", "tokens": ["Wird", "denn", "nach", "dei\u00b7nem", "wunsch", ",", "und", "\u00fc\u00b7ber", "sein", "ver\u00b7hof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "PPOSAT", "ADJD", "$,", "KON", "APPR", "PPOSAT", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.62": {"text": "Ein solcher armer dieb erhascht und angetroffen,", "tokens": ["Ein", "sol\u00b7cher", "ar\u00b7mer", "dieb", "er\u00b7hascht", "und", "an\u00b7ge\u00b7trof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ADJA", "NN", "VVPP", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.63": {"text": "Ist keine gnade da, er mu\u00df mit dir herein,", "tokens": ["Ist", "kei\u00b7ne", "gna\u00b7de", "da", ",", "er", "mu\u00df", "mit", "dir", "her\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "VVFIN", "ADV", "$,", "PPER", "VMFIN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.64": {"text": "Und kan nach deinem sinn nicht gnung bestrafet seyn.", "tokens": ["Und", "kan", "nach", "dei\u00b7nem", "sinn", "nicht", "gnung", "be\u00b7stra\u00b7fet", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "APPR", "PPOSAT", "NN", "PTKNEG", "ADV", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.65": {"text": "Will sich das feder-vieh nur etwan unterstehen,", "tokens": ["Will", "sich", "das", "fe\u00b7der\u00b7vieh", "nur", "et\u00b7wan", "un\u00b7ter\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "ART", "NN", "ADV", "ADV", "VVINF", "$,"], "meter": "+----+-+-+-+-", "measure": "dactylic.init"}, "line.66": {"text": "Aus bloser unvernunfft auf deine saat zu gehen:", "tokens": ["Aus", "blo\u00b7ser", "un\u00b7ver\u00b7nunfft", "auf", "dei\u00b7ne", "saat", "zu", "ge\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.67": {"text": "So schwingest du den stock, und wirffst aus aller macht,", "tokens": ["So", "schwin\u00b7gest", "du", "den", "stock", ",", "und", "wirffst", "aus", "al\u00b7ler", "macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,", "KON", "VVFIN", "APPR", "PIAT", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.68": {"text": "So lange, bi\u00df davon etwas ist umgebracht:", "tokens": ["So", "lan\u00b7ge", ",", "bi\u00df", "da\u00b7von", "et\u00b7was", "ist", "um\u00b7ge\u00b7bracht", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "KON", "PAV", "PIS", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.69": {"text": "Du rupffst die federn aus, die rache zu vollenden,", "tokens": ["Du", "rupffst", "die", "fe\u00b7dern", "aus", ",", "die", "ra\u00b7che", "zu", "voll\u00b7en\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.70": {"text": "Und streust sie hier und da herum an allen enden,", "tokens": ["Und", "streust", "sie", "hier", "und", "da", "he\u00b7rum", "an", "al\u00b7len", "en\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "KON", "ADV", "APZR", "APPR", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.71": {"text": "Damit da\u00df huhn und gan\u00df zu ihrem schrecken sehn,", "tokens": ["Da\u00b7mit", "da\u00df", "huhn", "und", "gan\u00df", "zu", "ih\u00b7rem", "schre\u00b7cken", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "KOUS", "NE", "KON", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.72": {"text": "Was hier mit ihnen sey vor eine schlacht geschehn.", "tokens": ["Was", "hier", "mit", "ih\u00b7nen", "sey", "vor", "ei\u00b7ne", "schlacht", "ge\u00b7schehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "APPR", "PPER", "VAFIN", "APPR", "ART", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.73": {"text": "F\u00e4ngt das getreide nun allm\u00e4hlich an zu reiffen,", "tokens": ["F\u00e4ngt", "das", "ge\u00b7trei\u00b7de", "nun", "all\u00b7m\u00e4h\u00b7lich", "an", "zu", "reif\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "ADJD", "PTKVZ", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.74": {"text": "Dann pflegst du st\u00fcck vor st\u00fcck gar flei\u00dfig durchzustreiffen:", "tokens": ["Dann", "pflegst", "du", "st\u00fcck", "vor", "st\u00fcck", "gar", "flei\u00b7\u00dfig", "durch\u00b7zu\u00b7streif\u00b7fen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "APPR", "NN", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.75": {"text": "Und da\u00df du wissen kanst, ob halm und korn gesund,", "tokens": ["Und", "da\u00df", "du", "wis\u00b7sen", "kanst", ",", "ob", "halm", "und", "korn", "ge\u00b7sund", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVINF", "VMFIN", "$,", "KOUS", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.76": {"text": "So ziehst du beyde sie gar zierlich durch den mund.", "tokens": ["So", "ziehst", "du", "bey\u00b7de", "sie", "gar", "zier\u00b7lich", "durch", "den", "mund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIS", "PPER", "ADV", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.77": {"text": "Kommt denn die zeit heran, zur erndte selbst zu schreiten,", "tokens": ["Kommt", "denn", "die", "zeit", "he\u00b7ran", ",", "zur", "ernd\u00b7te", "selbst", "zu", "schrei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "PTKVZ", "$,", "APPRART", "ADJA", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.78": {"text": "Wie bist du denn bem\u00fcht, die seile auszubreiten:", "tokens": ["Wie", "bist", "du", "denn", "be\u00b7m\u00fcht", ",", "die", "sei\u00b7le", "aus\u00b7zu\u00b7brei\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "VVPP", "$,", "PRELS", "ADV", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.79": {"text": "Und wenn nun di\u00df geschehn, vermeinest du, du hast", "tokens": ["Und", "wenn", "nun", "di\u00df", "ge\u00b7schehn", ",", "ver\u00b7mei\u00b7nest", "du", ",", "du", "hast"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KON", "KOUS", "ADV", "PDS", "VVPP", "$,", "VVFIN", "PPER", "$,", "PPER", "VAFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.80": {"text": 