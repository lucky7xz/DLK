word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "PPOSAT", "NN", "ART", "ADJA", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.85": {"text": "Zeigt denn der sch\u00f6ne herbst die reichen garten-fr\u00fcchte,", "tokens": ["Zeigt", "denn", "der", "sch\u00f6\u00b7ne", "herbst", "die", "rei\u00b7chen", "gar\u00b7ten\u00b7fr\u00fcch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.86": {"text": "So wird dein kurtzer schlaf um so viel mehr zu nichte:", "tokens": ["So", "wird", "dein", "kurt\u00b7zer", "schlaf", "um", "so", "viel", "mehr", "zu", "nich\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "ADJA", "NN", "APPR", "ADV", "ADV", "ADV", "PTKA", "PIS", "$."], "meter": "-+-+-+-++--+-", "measure": "iambic.hexa.relaxed"}, "line.87": {"text": "Ey dieses kan den kopff dir gar zu sehr verwirrn!", "tokens": ["Ey", "die\u00b7ses", "kan", "den", "kopff", "dir", "gar", "zu", "sehr", "ver\u00b7wirrn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PDS", "VMFIN", "ART", "NN", "PPER", "ADV", "PTKA", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.88": {"text": "Da zehlst du eigentlich die \u00e4pffel und die birn.", "tokens": ["Da", "zehlst", "du", "ei\u00b7gent\u00b7lich", "die", "\u00e4pf\u00b7fel", "und", "die", "birn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.89": {"text": "Und wenn ein wenig wind die leichten bl\u00e4tter r\u00fchret,", "tokens": ["Und", "wenn", "ein", "we\u00b7nig", "wind", "die", "leich\u00b7ten", "bl\u00e4t\u00b7ter", "r\u00fch\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "PIS", "VAFIN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.90": {"text": "Was wird vor grosse noth so gleich bey dir gesp\u00fchret!", "tokens": ["Was", "wird", "vor", "gros\u00b7se", "noth", "so", "gleich", "bey", "dir", "ge\u00b7sp\u00fch\u00b7ret", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "APPR", "ADJA", "NN", "ADV", "ADV", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.91": {"text": "F\u00e4llt dann ein pfl\u00e4umgen ab, durch maden weich ge-", "tokens": ["F\u00e4llt", "dann", "ein", "pfl\u00e4um\u00b7gen", "ab", ",", "durch", "ma\u00b7den", "weich", "ge"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "NN", "PTKVZ", "$,", "APPR", "ADJA", "ADJD", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.92": {"text": "macht,", "tokens": ["macht", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.93": {"text": "Hast du dich allbereit an bettel-stab gebracht.", "tokens": ["Hast", "du", "dich", "all\u00b7be\u00b7reit", "an", "bet\u00b7tel\u00b7stab", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PRF", "ADV", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.94": {"text": "Und wenn es nur alsdenn in deiner macht best\u00fcnde,", "tokens": ["Und", "wenn", "es", "nur", "als\u0