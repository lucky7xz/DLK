{"textgrid.poem.56293": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "Roland ein Ro\u00dfkamm", "genre": "verse", "period": "N.A.", "pub_year": 1832, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herr Roland ein seltsamer Ro\u00dfkamm,", "tokens": ["Herr", "Ro\u00b7land", "ein", "selt\u00b7sa\u00b7mer", "Ro\u00df\u00b7kamm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Als feil er die Stute bot.", "tokens": ["Als", "feil", "er", "die", "Stu\u00b7te", "bot", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ausnehmend sch\u00f6n war die Stute,", "tokens": ["Aus\u00b7neh\u00b7mend", "sch\u00f6n", "war", "die", "Stu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Sie aber war leider tot.", "tokens": ["Sie", "a\u00b7ber", "war", "lei\u00b7der", "tot", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "\u00bbsieh her, die vortreffliche Stute,", "tokens": ["\u00bb", "sieh", "her", ",", "die", "vor\u00b7treff\u00b7li\u00b7che", "Stu\u00b7te", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "PTKVZ", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Du kaufst sie, das sag ich dir!", "tokens": ["Du", "kaufst", "