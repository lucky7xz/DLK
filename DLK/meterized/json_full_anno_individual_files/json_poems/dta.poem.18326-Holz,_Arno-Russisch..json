{"dta.poem.18326": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "Russisch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1886", "urn": "urn:nbn:de:kobv:b4-200905192665", "language": ["de:0.99"], "booktitle": "Holz, Arno: Das Buch der Zeit. Lieder eines Modernen. Z\u00fcrich, 1886."}, "poem": {"stanza.1": {"line.1": {"text": "Steck ein das lumpige Milli\u00f6nchen!", "tokens": ["Steck", "ein", "das", "lum\u00b7pi\u00b7ge", "Mil\u00b7li\u00b7\u00f6n\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Du kennst ja die Moral der Zeit:", "tokens": ["Du", "kennst", "ja", "die", "Mo\u00b7ral", "der", "Zeit", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u201eder Himmel ist hoch und der Czar ist weit!\u201c", "tokens": ["\u201e", "der", "Him\u00b7mel", "ist", "hoch", "und", "der", "Czar", "ist", "weit", "!", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "NN", "VAFIN", "ADJD", "KON", "ART", "NN", "VAFIN", "ADJD", "$.", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}}}}