{"dta.poem.21367": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Caput XV.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-30602-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201ero\u00dfk\u00e4mme hab\u2019 ich ausgeschickt", "tokens": ["\u201e", "ro\u00df\u00b7k\u00e4m\u00b7me", "hab'", "ich", "aus\u00b7ge\u00b7schickt"], "token_info": ["punct", "word", "word", "word", "word"], "pos": ["$(", "VVFIN", "VAFIN", "PPER", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In alle Welt, die kaufen", "tokens": ["In", "al\u00b7le", "Welt", ",", "die", "kau\u00b7fen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PIAT", "NN", "$,", "PRELS", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "F\u00fcr mich die besten Pferde ein,", "tokens": ["F\u00fcr", "mich", "die", "bes\u00b7ten", "Pfer\u00b7de", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hab\u2019 schon einen guten Haufen.", "tokens": ["Hab'", "schon", "ei\u00b7nen", "gu\u00b7ten", "Hau\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "\u201eich warte bis die Zahl komplet,", "tokens": ["\u201e", "ich", "war\u00b7te", "bis", "die", "Zahl", "kom\u00b7plet", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dann schlag\u2019 ich los und befreye", "tokens": ["Dann", "schlag'", "ich", "los", "und", "be\u00b7fre\u00b7ye"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mein Vaterland, mein deutsches Volk,", "tokens": ["Mein", "Va\u00b7ter\u00b7land", ",", "mein", "deut\u00b7sches", "Volk", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das meiner harret mit Treue.\u201c", "tokens": ["Das", "mei\u00b7ner", "har\u00b7ret", "mit", "Treu\u00b7e", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "PPOSAT", "VVFIN", "APPR", "NN", "$.", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "So sprach der Kaiser, ich aber rief:", "tokens": ["So", "sprach", "der", "Kai\u00b7ser", ",", "ich", "a\u00b7ber", "rief", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Schlag\u2019 los, du alter Geselle,", "tokens": ["Schlag'", "los", ",", "du", "al\u00b7ter", "Ge\u00b7sel\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Schlag\u2019 los, und hast du nicht Pferde genug,", "tokens": ["Schlag'", "los", ",", "und", "hast", "du", "nicht", "Pfer\u00b7de", "ge\u00b7nug", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "KON", "VAFIN", "PPER", "PTKNEG", "NN", "ADV", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Nimm Esel an ihrer Stelle.", "tokens": ["Nimm", "E\u00b7sel", "an", "ih\u00b7rer", "Stel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Der Rothbart erwiederte l\u00e4chelnd: \u201eEs hat", "tokens": ["Der", "Roth\u00b7bart", "er\u00b7wie\u00b7der\u00b7te", "l\u00e4\u00b7chelnd", ":", "\u201e", "Es", "hat"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$.", "$(", "PPER", "VAFIN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Mit dem Schlagen gar keine Eile,", "tokens": ["Mit", "dem", "Schla\u00b7gen", "gar", "kei\u00b7ne", "Ei\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "PIAT", "NN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Man baute nicht Rom in einem Tag,", "tokens": ["Man", "bau\u00b7te", "nicht", "Rom", "in", "ei\u00b7nem", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PTKNEG", "NE", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Gut Ding will haben Weile.", "tokens": ["Gut", "Ding", "will", "ha\u00b7ben", "Wei\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "VMFIN", "VAFIN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}