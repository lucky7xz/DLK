{"dta.poem.17849": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "92.  Thue was du dir gethan wilt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Mensch weil du gerne siehst da\u00df man dir Gaben giebt;", "tokens": ["Mensch", "weil", "du", "ger\u00b7ne", "siehst", "da\u00df", "man", "dir", "Ga\u00b7ben", "giebt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOUS", "PPER", "ADV", "VVFIN", "KOUS", "PIS", "PPER", "NN", "VVFIN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "So mache doch auch dich im geben wol ge\u00fcbt.", "tokens": ["So", "ma\u00b7che", "doch", "auch", "dich", "im", "ge\u00b7ben", "wol", "ge\u00b7\u00fcbt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "PPER", "APPRART", "VVFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}