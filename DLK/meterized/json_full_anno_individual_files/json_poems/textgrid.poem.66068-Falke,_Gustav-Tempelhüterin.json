{"textgrid.poem.66068": {"metadata": {"author": {"name": "Falke, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Tempelh\u00fcterin", "genre": "verse", "period": "N.A.", "pub_year": 1884, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das hab ich dir zu danken,", "tokens": ["Das", "hab", "ich", "dir", "zu", "dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da\u00df du die gr\u00fcnen Ranken", "tokens": ["Da\u00df", "du", "die", "gr\u00fc\u00b7nen", "Ran\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Des Gl\u00fccks zu einem stillen Zelt mir biegst,", "tokens": ["Des", "Gl\u00fccks", "zu", "ei\u00b7nem", "stil\u00b7len", "Zelt", "mir", "biegst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Davor du ohne Klagen", "tokens": ["Da\u00b7vor", "du", "oh\u00b7ne", "Kla\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "PPER", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Getreu an allen Tagen", "tokens": ["Ge\u00b7treu", "an", "al\u00b7len", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Als meines Friedens wache H\u00fcterin liegst.", "tokens": ["Als", "mei\u00b7nes", "Frie\u00b7dens", "wa\u00b7che", "H\u00fc\u00b7te\u00b7rin", "liegst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["