ta"}}, "stanza.3": {"line.1": {"text": "So l\u00e4\u00dft du mich gew\u00e4hren", "tokens": ["So", "l\u00e4\u00dft", "du", "mich", "ge\u00b7w\u00e4h\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und wei\u00dft den Gott zu ehren,", "tokens": ["Und", "wei\u00dft", "den", "Gott", "zu", "eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der herrisch dich von meiner Seite scheucht,", "tokens": ["Der", "her\u00b7risch", "dich", "von", "mei\u00b7ner", "Sei\u00b7te", "scheucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PRF", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und tr\u00e4umst von Ruhmessternen", "tokens": ["Und", "tr\u00e4umst", "von", "Ruh\u00b7mess\u00b7ter\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und siehst in goldne Fernen", "tokens": ["Und", "siehst", "in", "gold\u00b7ne", "Fer\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mit einem stillen, seligen Geleucht.", "tokens": ["Mit", "ei\u00b7nem", "stil\u00b7len", ",", "se\u00b7li\u00b7gen", "Ge\u00b7leucht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Das hab ich dir zu danken,", "tokens": ["Das", "hab", "ich", "dir", "zu", "dan\u00b7ken", ","], "