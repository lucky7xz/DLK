{"dta.poem.18655": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "Vier Gnaden .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Dass Araber an ihrem Theil", "tokens": ["Dass", "A\u00b7ra\u00b7ber", "an", "ih\u00b7rem", "Theil"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Die Weite froh durchziehen", "tokens": ["Die", "Wei\u00b7te", "froh", "durch\u00b7zie\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Hat Allah zu gemeinem Heil", "tokens": ["Hat", "Al\u00b7lah", "zu", "ge\u00b7mei\u00b7nem", "Heil"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "NN", "APPR", "ADJA", "NN"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.4": {"text": "Der Gnaden vier verliehen.", "tokens": ["Der", "Gna\u00b7den", "vier", "ver\u00b7lie\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "CARD", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Den Turban erst, der besser schm\u00fcckt", "tokens": ["Den", "Tur\u00b7ban", "erst", ",", "der", "bes\u00b7ser", "schm\u00fcckt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "$,", "PRELS", "ADJD", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als alle Kaiserkronen,", "tokens": ["Als", "al\u00b7le", "Kai\u00b7ser\u00b7kro\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein Zelt, das man vom Orte r\u00fcckt", "tokens": ["Ein", "Zelt", ",", "das", "man", "vom", "Or\u00b7te", "r\u00fcckt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PIS", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Um \u00fcberall zu wohnen.", "tokens": ["Um", "\u00fc\u00b7be\u00b7rall", "zu", "woh\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ein Schwerdt, das t\u00fcchtiger besch\u00fctzt", "tokens": ["Ein", "Schwerdt", ",", "das", "t\u00fcch\u00b7ti\u00b7ger", "be\u00b7sch\u00fctzt"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als Fels und hohe Mauern,", "tokens": ["Als", "Fels", "und", "ho\u00b7he", "Mau\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein Liedchen, das gef\u00e4llt und n\u00fctzt,", "tokens": ["Ein", "Lied\u00b7chen", ",", "das", "ge\u00b7f\u00e4llt", "und", "n\u00fctzt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVPP", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Worauf die M\u00e4dchen lauern.", "tokens": ["Wo\u00b7rauf", "die", "M\u00e4d\u00b7chen", "lau\u00b7ern", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und Blumen sing\u2019 ich ungest\u00f6rt", "tokens": ["Und", "Blu\u00b7men", "sing'", "ich", "un\u00b7ge\u00b7st\u00f6rt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VVFIN", "PPER", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von Ihrem Schawl herunter,", "tokens": ["Von", "Ih\u00b7rem", "Schawl", "her\u00b7un\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie weiss recht wohl was Ihr geh\u00f6rt", "tokens": ["Sie", "weiss", "recht", "wohl", "was", "Ihr", "ge\u00b7h\u00f6rt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "PWS", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und bleibt mir hold und munter.", "tokens": ["Und", "bleibt", "mir", "hold", "und", "mun\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und Blum\u2019 und Fr\u00fcchte weiss ich euch", "tokens": ["Und", "Blum'", "und", "Fr\u00fcch\u00b7te", "weiss", "ich", "euch"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN", "VVFIN", "PPER", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gar zierlich aufzutischen,", "tokens": ["Gar", "zier\u00b7lich", "auf\u00b7zu\u00b7ti\u00b7schen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wollt ihr Moralien zugleich,", "tokens": ["Wollt", "ihr", "Mo\u00b7ra\u00b7li\u00b7en", "zu\u00b7gleich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So geb\u2019 ich von den frischen.", "tokens": ["So", "geb'", "ich", "von", "den", "fri\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}