{"dta.poem.1306": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Nach dreymahl \u00fcberstandener  \n Feuers-Gefahr.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Ach GOtt/ ein schweres Jahr ist wieder nun zum Ende/", "tokens": ["Ach", "Gott", "/", "ein", "schwe\u00b7res", "Jahr", "ist", "wie\u00b7der", "nun", "zum", "En\u00b7de", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$(", "ART", "ADJA", "NN", "VAFIN", "ADV", "ADV", "APPRART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df Hau\u00df und Hoff noch steht/ da\u00df Haab und Gutt noch", "tokens": ["Da\u00df", "Hau\u00df", "und", "Hoff", "noch", "steht", "/", "da\u00df", "Ha\u00b7ab", "und", "Gutt", "noch"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "KON", "VVFIN", "ADV", "VVFIN", "$(", "KOUS", "NN", "KON", "ADJD", "ADV"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "w\u00e4hrt/", "tokens": ["w\u00e4hrt", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Von keiner grimmen Macht des Feuers auffgezehrt/", "tokens": ["Von", "kei\u00b7ner", "grim\u00b7men", "Macht", "des", "Feu\u00b7ers", "auff\u00b7ge\u00b7zehrt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ist einig und allein das Wunder deiner H\u00e4nde.", "tokens": ["Ist", "ei\u00b7nig", "und", "al\u00b7lein", "das", "Wun\u00b7der", "dei\u00b7ner", "H\u00e4n\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "KON", "ADV", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wir sehn mit Zittern zu/ da\u00df unsrer Andacht Flammen", "tokens": ["Wir", "sehn", "mit", "Zit\u00b7tern", "zu", "/", "da\u00df", "uns\u00b7rer", "An\u00b7dacht", "Flam\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "PTKZU", "$(", "KOUS", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "In kalten Hertzen lau/ und fast erstorben seyn.", "tokens": ["In", "kal\u00b7ten", "Hert\u00b7zen", "lau", "/", "und", "fast", "ers\u00b7tor\u00b7ben", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "$(", "KON", "ADV", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wolt uns dein strenges Recht zur Straffe laden ein/", "tokens": ["Wolt", "uns", "dein", "stren\u00b7ges", "Recht", "zur", "Straf\u00b7fe", "la\u00b7den", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "ADJA", "NN", "APPRART", "NN", "VVFIN", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "So w\u00e4re/ was man hat/ vorl\u00e4ngst geschmeltzt zusammen.", "tokens": ["So", "w\u00e4\u00b7re", "/", "was", "man", "hat", "/", "vor\u00b7l\u00e4ngst", "ge\u00b7schmeltzt", "zu\u00b7sam\u00b7men", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "$(", "PWS", "PIS", "VAFIN", "$(", "ADV", "VVPP", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Wir h\u00e4tten selbst an Leib und Seele brennen sollen/", "tokens": ["Wir", "h\u00e4t\u00b7ten", "selbst", "an", "Leib", "und", "See\u00b7le", "bren\u00b7nen", "sol\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "NN", "KON", "NN", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Doch deine G\u00fctt ohn End\u2019 ist gr\u00f6sser als die Schuld.", "tokens": ["Doch", "dei\u00b7ne", "G\u00fctt", "ohn", "End'", "ist", "gr\u00f6s\u00b7ser", "als", "die", "Schuld", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPR", "NN", "VAFIN", "ADJD", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Du tr\u00e4gest noch di\u00df Jahr nach Vaters-Art Geduld/", "tokens": ["Du", "tr\u00e4\u00b7gest", "noch", "di\u00df", "Jahr", "nach", "Va\u00b7ter\u00b7sArt", "Ge\u00b7duld", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PDS", "NN", "APPR", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Und siehest/ ob wir uns auff Warnung bessern wollen.", "tokens": ["Und", "sie\u00b7hest", "/", "ob", "wir", "uns", "auff", "War\u00b7nung", "bes\u00b7sern", "wol\u00b7len", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$(", "KOUS", "PPER", "PRF", "APPR", "NN", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Du l\u00e4st der Flammen Glantz uns in die Augen scheinen/", "tokens": ["Du", "l\u00e4st", "der", "Flam\u00b7men", "Glantz", "uns", "in", "die", "Au\u00b7gen", "schei\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "PPER", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Dadurch zu z\u00fcnden an der Andacht todte Glutt/", "tokens": ["Da\u00b7durch", "zu", "z\u00fcn\u00b7den", "an", "der", "An\u00b7dacht", "tod\u00b7te", "Glutt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PTKZU", "VVINF", "APPR", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Du weisest/ wie du uns kanst rauben Haab und Gutt/", "tokens": ["Du", "wei\u00b7sest", "/", "wie", "du", "uns", "kanst", "rau\u00b7ben", "Ha\u00b7ab", "und", "Gutt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "PWAV", "PPER", "PPER", "VMFIN", "VVINF", "NN", "KON", "NN", "$("], "meter": "-+-++--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.17": {"text": "Wie langer Jahre Schwei\u00df ein Abend macht beweinen.", "tokens": ["Wie", "lan\u00b7ger", "Jah\u00b7re", "Schwei\u00df", "ein", "A\u00b7bend", "macht", "be\u00b7wei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "NN", "ART", "NN", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Du lehrest uns dadurch das Irrdische verachten/", "tokens": ["Du", "leh\u00b7rest", "uns", "da\u00b7durch", "das", "Irr\u00b7di\u00b7sche", "ver\u00b7ach\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PAV", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Und von der Welt getrennt nach Himmels-Sch\u00e4tzen stehn/", "tokens": ["Und", "von", "der", "Welt", "ge\u00b7trennt", "nach", "Him\u00b7mels\u00b7Sch\u00e4t\u00b7zen", "stehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVPP", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Di\u00df Vorbild zeiget uns/ wie alles soll vergehn/", "tokens": ["Di\u00df", "Vor\u00b7bild", "zei\u00b7get", "uns", "/", "wie", "al\u00b7les", "soll", "ver\u00b7gehn", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "VVFIN", "PPER", "$(", "PWAV", "PIS", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Und der Verdammten Schaar in stetem Feuer schmachten.", "tokens": ["Und", "der", "Ver\u00b7damm\u00b7ten", "Schaar", "in", "ste\u00b7tem", "Feu\u00b7er", "schmach\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Doch wenn der strenge Brand sich will zu weit erstrecken/", "tokens": ["Doch", "wenn", "der", "stren\u00b7ge", "Brand", "sich", "will", "zu", "weit", "er\u00b7stre\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN", "PRF", "VMFIN", "PTKA", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "So schickest du das Heer der treuen W\u00e4chter aus/", "tokens": ["So", "schi\u00b7ckest", "du", "das", "Heer", "der", "treu\u00b7en", "W\u00e4ch\u00b7ter", "aus", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "ART", "ADJA", "NE", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Bewahrest unsern Hof/ versicherst unser Hau\u00df/", "tokens": ["Be\u00b7wah\u00b7rest", "un\u00b7sern", "Hof", "/", "ver\u00b7si\u00b7cherst", "un\u00b7ser", "Hau\u00df", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$(", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Und l\u00e4st uns um und um mit deinem Schutze decken.", "tokens": ["Und", "l\u00e4st", "uns", "um", "und", "um", "mit", "dei\u00b7nem", "Schut\u00b7ze", "de\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "KON", "APPR", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Ein Regen voller Glutt/ getrieben von dem Winde/", "tokens": ["Ein", "Re\u00b7gen", "vol\u00b7ler", "Glutt", "/", "ge\u00b7trie\u00b7ben", "von", "dem", "Win\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$(", "VVPP", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Macht einen lichten Tag aus dunckel-tr\u00fcber Nacht/", "tokens": ["Macht", "ei\u00b7nen", "lich\u00b7ten", "Tag", "aus", "dun\u00b7ckel\u00b7tr\u00fcber", "Nacht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.28": {"text": "Es wird das Trockne doch von Hitze warm gemacht/", "tokens": ["Es", "wird", "das", "Trock\u00b7ne", "doch", "von", "Hit\u00b7ze", "warm", "ge\u00b7macht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "APPR", "NN", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Man sieht mit Furchten zu/ wie bald es sich entz\u00fcnde.", "tokens": ["Man", "sieht", "mit", "Furch\u00b7ten", "zu", "/", "wie", "bald", "es", "sich", "ent\u00b7z\u00fcn\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "NN", "PTKZU", "$(", "KOKOM", "ADV", "PPER", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Durch Frevel frecher Hand wird uns zur Brunst gebettet/", "tokens": ["Durch", "Fre\u00b7vel", "fre\u00b7cher", "Hand", "wird", "uns", "zur", "Brunst", "ge\u00b7bet\u00b7tet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "VAFIN", "PPER", "APPRART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Und unser eigen Hau\u00df soll unser Holtz-Sto\u00df seyn/", "tokens": ["Und", "un\u00b7ser", "ei\u00b7gen", "Hau\u00df", "soll", "un\u00b7ser", "Holtz\u00b7Sto\u00df", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VMFIN", "PPOSAT", "NN", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Du aber siehst/ o HErr/ mit Vaters-Augen drein/", "tokens": ["Du", "a\u00b7ber", "siehst", "/", "o", "Herr", "/", "mit", "Va\u00b7ter\u00b7sAu\u00b7gen", "drein", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "$(", "FM", "NN", "$(", "APPR", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Wir werden unverdient durch deine Gunst gerettet/", "tokens": ["Wir", "wer\u00b7den", "un\u00b7ver\u00b7di\u00b7ent", "durch", "dei\u00b7ne", "Gunst", "ge\u00b7ret\u00b7tet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "APPR", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.34": {"text": "Der Wind ver\u00e4ndert sich/ die Funcken m\u00fcssen sterben/", "tokens": ["Der", "Wind", "ver\u00b7\u00e4n\u00b7dert", "sich", "/", "die", "Fun\u00b7cken", "m\u00fcs\u00b7sen", "ster\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "$(", "ART", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Man wird zur rechten Zeit der stillen Glutt gewahr/", "tokens": ["Man", "wird", "zur", "rech\u00b7ten", "Zeit", "der", "stil\u00b7len", "Glutt", "ge\u00b7wahr", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "APPRART", "ADJA", "NN", "ART", "ADJA", "NN", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "D\u00e4mpfft in der Kohle noch die bl\u00fchende Gefahr/", "tokens": ["D\u00e4mpfft", "in", "der", "Koh\u00b7le", "noch", "die", "bl\u00fc\u00b7hen\u00b7de", "Ge\u00b7fahr", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ADV", "ART", "ADJA", "NN", "$("], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.37": {"text": "So kan dein starcker Arm befreyen vom Verderben.", "tokens": ["So", "kan", "dein", "star\u00b7cker", "Arm", "be\u00b7fre\u00b7yen", "vom", "Ver\u00b7der\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPOSAT", "ADJA", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Herr/ la\u00df uns dir darvor von Hertzen Danck erweisen/", "tokens": ["Herr", "/", "la\u00df", "uns", "dir", "dar\u00b7vor", "von", "Hert\u00b7zen", "Danck", "er\u00b7wei\u00b7sen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "VVIMP", "PPER", "PPER", "PAV", "APPR", "NN", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Und auch di\u00df neue Jahr in deinem Schutze seyn/", "tokens": ["Und", "auch", "di\u00df", "neu\u00b7e", "Jahr", "in", "dei\u00b7nem", "Schut\u00b7ze", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PDS", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Wend alles Ubel ab/ f\u00fchr allen Seegen ein/", "tokens": ["Wend", "al\u00b7les", "U\u00b7bel", "ab", "/", "f\u00fchr", "al\u00b7len", "See\u00b7gen", "ein", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "NN", "PTKVZ", "$(", "VVFIN", "PIAT", "NN", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Bi\u00df wir dich dermahleinst ohn Ende k\u00f6nnen preisen.", "tokens": ["Bi\u00df", "wir", "dich", "der\u00b7mah\u00b7le\u00b7inst", "ohn", "En\u00b7de", "k\u00f6n\u00b7nen", "prei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "APPR", "NN", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}}}}}