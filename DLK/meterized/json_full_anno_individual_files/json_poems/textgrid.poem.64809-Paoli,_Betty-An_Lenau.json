{"textgrid.poem.64809": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "An Lenau", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In Hella's morgenfrischen Tagen,", "tokens": ["In", "Hel\u00b7la's", "mor\u00b7gen\u00b7fri\u00b7schen", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Umwebt von ros'ger Tr\u00e4ume Flor,", "tokens": ["Um\u00b7webt", "von", "ros'\u00b7ger", "Tr\u00e4u\u00b7me", "Flor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da lieh das Volk den heitern Sagen", "tokens": ["Da", "lieh", "das", "Volk", "den", "hei\u00b7tern", "Sa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Dichter gern ein gl\u00e4ubig Ohr.", "tokens": ["Der", "Dich\u00b7ter", "gern", "ein", "gl\u00e4u\u00b7big", "Ohr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der jungen Menschheit Seele, offen,", "tokens": ["Der", "jun\u00b7gen", "Menschheit", "See\u00b7le", ",", "of\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$,", "ADJD", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Lauscht' freudig auf den s\u00fc\u00dfen Ton;", "tokens": ["Lauscht'", "freu\u00b7dig", "auf", "den", "s\u00fc\u00b7\u00dfen", "Ton", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Dichter schuf aus seinem Hoffen", "tokens": ["Der", "Dich\u00b7ter", "schuf", "aus", "sei\u00b7nem", "Hof\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und seinem Traum die Religion.", "tokens": ["Und", "sei\u00b7nem", "Traum", "die", "Re\u00b7li\u00b7gi\u00b7on", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Allein seit in des Nichtseins Leere", "tokens": ["Al\u00b7lein", "seit", "in", "des", "Nich\u00b7tseins", "Lee\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der G\u00f6tter bunte Schaar zerstob,", "tokens": ["Der", "G\u00f6t\u00b7ter", "bun\u00b7te", "Schaar", "zer\u00b7stob", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Seit sich die heil'ge Gotteslehre", "tokens": ["Seit", "sich", "die", "heil'\u00b7ge", "Got\u00b7tes\u00b7leh\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PRF", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Am Kreuz auf Golgatha erhob;", "tokens": ["Am", "Kreuz", "auf", "Gol\u00b7ga\u00b7tha", "er\u00b7hob", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "NE", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Seit Christus Wort dem Erdenstaube", "tokens": ["Seit", "Chris\u00b7tus", "Wort", "dem", "Er\u00b7dens\u00b7tau\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Auf Engelschwingen uns entrafft,", "tokens": ["Auf", "En\u00b7gel\u00b7schwin\u00b7gen", "uns", "en\u00b7trafft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ist es des Herzens tiefer Glaube,", "tokens": ["Ist", "es", "des", "Her\u00b7zens", "tie\u00b7fer", "Glau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der nun hinf\u00fcr den Dichter schafft.", "tokens": ["Der", "nun", "hin\u00b7f\u00fcr", "den", "Dich\u00b7ter", "schafft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Und statt des Heidenthumes Musen", "tokens": ["Und", "statt", "des", "Hei\u00b7den\u00b7thu\u00b7mes", "Mu\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und ihrer l\u00e4ngstversunknen Spur,", "tokens": ["Und", "ih\u00b7rer", "l\u00e4ngst\u00b7ver\u00b7sun\u00b7knen", "Spur", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bl\u00fch'n jetzo in des K\u00fcnstlers Busen", "tokens": ["Bl\u00fch'n", "jet\u00b7zo", "in", "des", "K\u00fcnst\u00b7lers", "Bu\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Des Christenthums drei Engel nur.", "tokens": ["Des", "Chris\u00b7ten\u00b7thums", "drei", "En\u00b7gel", "nur", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "CARD", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Drei Engel, die in ew'ger Neuheit,", "tokens": ["Drei", "En\u00b7gel", ",", "die", "in", "ew'\u00b7ger", "Neu\u00b7heit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "$,", "PRELS", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn auch nicht allw\u00e4rts noch erkannt,", "tokens": ["Wenn", "auch", "nicht", "all\u00b7w\u00e4rts", "noch", "er\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PTKNEG", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als Glaube, Liebe und als Freiheit", "tokens": ["Als", "Glau\u00b7be", ",", "Lie\u00b7be", "und", "als", "Frei\u00b7heit"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "$,", "NN", "KON", "KOUS", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Durchgeistigen das Erdenland!", "tokens": ["Durch\u00b7geis\u00b7ti\u00b7gen", "das", "Er\u00b7den\u00b7land", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Die sich in jedem Bilde malen,", "tokens": ["Die", "sich", "in", "je\u00b7dem", "Bil\u00b7de", "ma\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die t\u00f6nen aus jedwedem Lied,", "tokens": ["Die", "t\u00f6\u00b7nen", "aus", "jed\u00b7we\u00b7dem", "Lied", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die aus jedwedem Kunstwerk strahlen,", "tokens": ["Die", "aus", "jed\u00b7we\u00b7dem", "Kunst\u00b7werk", "strah\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das aus berufnem Geiste zieht.", "tokens": ["Das", "aus", "be\u00b7ruf\u00b7nem", "Geis\u00b7te", "zieht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "So la\u00df mich denn verehrend gr\u00fc\u00dfen", "tokens": ["So", "la\u00df", "mich", "denn", "ver\u00b7eh\u00b7rend", "gr\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVIMP", "PPER", "ADV", "VVPP", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Aus meiner Seele Innigkeit", "tokens": ["Aus", "mei\u00b7ner", "See\u00b7le", "In\u00b7nig\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dich, den mit himmelreinen K\u00fcssen", "tokens": ["Dich", ",", "den", "mit", "him\u00b7mel\u00b7rei\u00b7nen", "K\u00fcs\u00b7sen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Zu ihrem Dichter sie geweiht!", "tokens": ["Zu", "ih\u00b7rem", "Dich\u00b7ter", "sie", "ge\u00b7weiht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Dich, den vor Vielen sie erkoren,", "tokens": ["Dich", ",", "den", "vor", "Vie\u00b7len", "sie", "er\u00b7ko\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "APPR", "NN", "PPER", "VVINF", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Zu k\u00fcnden ihr erhabnes Wort,", "tokens": ["Zu", "k\u00fcn\u00b7den", "ihr", "er\u00b7hab\u00b7nes", "Wort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und der sich ihnen zugeschworen", "tokens": ["Und", "der", "sich", "ih\u00b7nen", "zu\u00b7ge\u00b7schwo\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "PRF", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Als Streiter f\u00fcr der Menschheit Hort!", "tokens": ["Als", "Strei\u00b7ter", "f\u00fcr", "der", "Menschheit", "Hort", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "ART", "NN", "NE", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.10": {"line.1": {"text": "O wirke fort, wie Du begonnen,", "tokens": ["O", "wir\u00b7ke", "fort", ",", "wie", "Du", "be\u00b7gon\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKVZ", "$,", "PWAV", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Voll Liebes- und voll Freiheitsdrang,", "tokens": ["Voll", "Lie\u00b7bes", "und", "voll", "Frei\u00b7heits\u00b7drang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "TRUNC", "KON", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und la\u00df die Strahlen dreier Sonnen", "tokens": ["Und", "la\u00df", "die", "Strah\u00b7len", "drei\u00b7er", "Son\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "ART", "NN", "CARD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Verein'gen sich in Deinen Sang.", "tokens": ["Ver\u00b7ein'\u00b7gen", "sich", "in", "Dei\u00b7nen", "Sang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "O leuchte fort als heil'ge Flamme,", "tokens": ["O", "leuch\u00b7te", "fort", "als", "heil'\u00b7ge", "Flam\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKVZ", "KOKOM", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Verkl\u00e4rend diese dunkle Bahn,", "tokens": ["Ver\u00b7kl\u00e4\u00b7rend", "die\u00b7se", "dunk\u00b7le", "Bahn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und richte, segne und verdamme,", "tokens": ["Und", "rich\u00b7te", ",", "seg\u00b7ne", "und", "ver\u00b7dam\u00b7me", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie du es k\u00fchn bis jetzt gethan.", "tokens": ["Wie", "du", "es", "k\u00fchn", "bis", "j