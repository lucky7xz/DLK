{"textgrid.poem.57240": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Seraph stammelt, und die Unendlichkeit", "genre": "verse", "period": "N.A.", "pub_year": 1750, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Seraph stammelt, und die Unendlichkeit", "tokens": ["Der", "Se\u00b7raph", "stam\u00b7melt", ",", "und", "die", "Un\u00b7end\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "KON", "ART", "NN"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Bebt durch den Umkreis ihrer Gefilde nach", "tokens": ["Bebt", "durch", "den", "Um\u00b7kreis", "ih\u00b7rer", "Ge\u00b7fil\u00b7de", "nach"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "PPOSAT", "NN", "APPR"], "meter": "+---+-+-+-+", "measure": "dactylic.init"}, "line.3": {"text": "Dein hohes Lob, o Sohn! wer bin ich,", "tokens": ["Dein", "ho\u00b7hes", "Lob", ",", "o", "Sohn", "!", "wer", "bin", "ich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "FM", "NN", "$.", "PWS", "VAFIN", "PPER", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Dass ich mich auch in die Jubel dr\u00e4nge?", "tokens": ["Dass", "ich", "mich", "auch", "in", "die", "Ju\u00b7bel", "dr\u00e4n\u00b7ge", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Vom Staube Staub! Doch wohnt ein Unsterblicher", "tokens": ["Vom", "Stau\u00b7be", "Staub", "!", "Doch", "wohnt", "ein", "U\u00b7nsterb\u00b7li\u00b7cher"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "NN", "$.", "KON", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von hoher Abkunft in den Verwesungen!", "tokens": ["Von", "ho\u00b7her", "Ab\u00b7kunft", "in", "den", "Ver\u00b7we\u00b7sun\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und denkt Gedanken, dass Entz\u00fcckung", "tokens": ["Und", "denkt", "Ge\u00b7dan\u00b7ken", ",", "dass", "Ent\u00b7z\u00fc\u00b7ckung"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "NN", "$,", "KOUS", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Durch die ersch\u00fctterte Nerve schauert!", "tokens": ["Durch", "die", "er\u00b7sch\u00fct\u00b7ter\u00b7te", "Ner\u00b7ve", "schau\u00b7ert", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Auch du wirst einmal mehr wie Verwesung seyn,", "tokens": ["Auch", "du", "wirst", "ein\u00b7mal", "mehr", "wie", "Ver\u00b7we\u00b7sung", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VAFIN", "ADV", "ADV", "KOKOM", "NN", "VAINF", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Der Seele Schatten, H\u00fctte, von Erd' erbaut,", "tokens": ["Der", "See\u00b7le", "Schat\u00b7ten", ",", "H\u00fct\u00b7te", ",", "von", "Erd'", "er\u00b7baut", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "NN", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und andrer Schauer Trunkenbeiten", "tokens": ["Und", "an\u00b7drer", "Schau\u00b7er", "Trun\u00b7ken\u00b7bei\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Werden dich dort, wo du schlummerst, wecken.", "tokens": ["Wer\u00b7den", "dich", "dort", ",", "wo", "du", "schlum\u00b7merst", ",", "we\u00b7cken", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "$,", "PWAV", "PPER", "VVFIN", "$,", "VVINF", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Der Leben Schauplatz, Feld, wo wir schlummerten,", "tokens": ["Der", "Le\u00b7ben", "Schau\u00b7platz", ",", "Feld", ",", "wo", "wir", "schlum\u00b7mer\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "NN", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wo Adams Enkel wird, was sein Vater war,", "tokens": ["Wo", "A\u00b7dams", "En\u00b7kel", "wird", ",", "was", "sein", "Va\u00b7ter", "war", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "NN", "VAFIN", "$,", "PRELS", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Als er sich jetzt der Sch\u00f6pfung Armen", "tokens": ["Als", "er", "sich", "jetzt", "der", "Sch\u00f6p\u00b7fung", "Ar\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "ADV", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Jauchzend entriss, und ein Leben dastand!", "tokens": ["Jauch\u00b7zend", "ent\u00b7riss", ",", "und", "ein", "Le\u00b7ben", "das\u00b7tand", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$,", "KON", "ART", "NN", "VVFIN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.5": {"line.1": {"text": "O Feld vom Aufgang bis, wo sie untergeht", "tokens": ["O", "Feld", "vom", "Auf\u00b7gang", "bis", ",", "wo", "sie", "un\u00b7ter\u00b7geht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "APPRART", "NN", "ADV", "$,", "PWAV", "PPER", "VVFIN"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der Sonnen letzte, heiliger Todter voll,", "tokens": ["Der", "Son\u00b7nen", "letz\u00b7te", ",", "hei\u00b7li\u00b7ger", "Tod\u00b7ter", "voll", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "$,", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Wenn seh ich dich? wenn weint mein Auge", "tokens": ["Wenn", "seh", "ich", "dich", "?", "wenn", "weint", "mein", "Au\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "VVFIN", "PPER", "PRF", "$.", "KOUS", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Unter den tausendmal tausend Thr\u00e4nen?", "tokens": ["Un\u00b7ter", "den", "tau\u00b7send\u00b7mal", "tau\u00b7send", "Thr\u00e4\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADV", "CARD", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.6": {"line.1": {"text": "Des Schlafes Stunden, oder Jahrhunderte,", "tokens": ["Des", "Schla\u00b7fes", "Stun\u00b7den", ",", "o\u00b7der", "Jahr\u00b7hun\u00b7der\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Fliesst schnell vor\u00fcber, fliesst, dass ich aufersteh!", "tokens": ["Fliesst", "schnell", "vor\u00b7\u00fc\u00b7ber", ",", "fliesst", ",", "dass", "ich", "auf\u00b7er\u00b7steh", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PTKVZ", "$,", "VVFIN", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Allein sie s\u00e4umen, und ich bin noch", "tokens": ["Al\u00b7lein", "sie", "s\u00e4u\u00b7men", ",", "und", "ich", "bin", "noch"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "VVFIN", "$,", "KON", "PPER", "VAFIN", "ADV"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Diesseit am Grabe! O helle Stunde,", "tokens": ["Dies\u00b7seit", "am", "Gra\u00b7be", "!", "O", "hel\u00b7le", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$.", "NE", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "Der Ruh Gespielin, Stunde des Todes, kom!", "tokens": ["Der", "Ruh", "Ge\u00b7spie\u00b7lin", ",", "Stun\u00b7de", "des", "To\u00b7des", ",", "kom", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "NN", "ART", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "O du Gefilde, wo der Unsterblichkeit", "tokens": ["O", "du", "Ge\u00b7fil\u00b7de", ",", "wo", "der", "U\u00b7nsterb\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NE", "NN", "$,", "PWAV", "ART", "NN"], "meter": "++-+-+-+--+", "measure": "iambic.hexa.chol"}, "line.3": {"text": "Diess Leben reift, noch nie besuchter", "tokens": ["Diess", "Le\u00b7ben", "reift", ",", "noch", "nie", "be\u00b7such\u00b7ter"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDAT", "NN", "VVFIN", "$,", "ADV", "ADV", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Acker f\u00fcr ewige Saat, wo bist du?", "tokens": ["A\u00b7cker", "f\u00fcr", "e\u00b7wi\u00b7ge", "Saat", ",", "wo", "bist", "du", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,", "PWAV", "VAFIN", "PPER", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.8": {"line.1": {"text": "Lass mich dort hingehn, dass ich die St\u00e4te seh!", "tokens": ["Lass", "mich", "dort", "hin\u00b7gehn", ",", "dass", "ich", "die", "St\u00e4\u00b7te", "seh", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "VVINF", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Mit hingesenktem trunkenen Blick sie seh!", "tokens": ["Mit", "hin\u00b7ge\u00b7senk\u00b7tem", "trun\u00b7ke\u00b7nen", "Blick", "sie", "seh", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Der Erndte Blumen dr\u00fcber streue,", "tokens": ["Der", "Ernd\u00b7te", "Blu\u00b7men", "dr\u00fc\u00b7ber", "streu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PAV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Unter die Blumen mich leg', und sterbe!", "tokens": ["Un\u00b7ter", "die", "Blu\u00b7men", "mich", "leg'", ",", "und", "ster\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "VVFIN", "$,", "KON", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.9": {"line.1": {"text": "Wunsch grosser Aussicht, aber nur Gl\u00fccklichen,", "tokens": ["Wunsch", "gros\u00b7ser", "Aus\u00b7sicht", ",", "a\u00b7ber", "nur", "Gl\u00fcck\u00b7li\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$,", "ADV", "ADV", "NN", "$,"], "meter": "+--+-+--+--", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Wenn du die s\u00fcsse Stunde der Seligkeit,", "tokens": ["Wenn", "du", "die", "s\u00fcs\u00b7se", "Stun\u00b7de", "der", "Se\u00b7lig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Da wir dich w\u00fcnschen, k\u00e4mst; wer gliche", "tokens": ["Da", "wir", "dich", "w\u00fcn\u00b7schen", ",", "k\u00e4mst", ";", "wer", "gli\u00b7che"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "VVINF", "$,", "VVFIN", "$.", "PWS", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dem, der alsdann mit dem Tode r\u00e4nge?", "tokens": ["Dem", ",", "der", "als\u00b7dann", "mit", "dem", "To\u00b7de", "r\u00e4n\u00b7ge", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.10": {"line.1": {"text": "Dann mischt' ich k\u00fchner unter den Throngesang", "tokens": ["Dann", "mischt'", "ich", "k\u00fch\u00b7ner", "un\u00b7ter", "den", "Thron\u00b7ge\u00b7sang"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Des Menschen Stimme, s\u00e4nge dann heiliger", "tokens": ["Des", "Men\u00b7schen", "Stim\u00b7me", ",", "s\u00e4n\u00b7ge", "dann", "hei\u00b7li\u00b7ger"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$,", "VVFIN", "ADV", "ADJA"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Den meine Seele liebt! den Besten", "tokens": ["Den", "mei\u00b7ne", "See\u00b7le", "liebt", "!", "den", "Bes\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "$.", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Aller gebohrnen, den Sohn des Vaters!", "tokens": ["Al\u00b7ler", "ge\u00b7bohr\u00b7nen", ",", "den", "Sohn", "des", "Va\u00b7ters", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ART", "NN", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.11": {"line.1": {"text": "Doch lass mich leben, dass am erreichten Ziel", "tokens": ["Doch", "lass", "mich", "le\u00b7ben", ",", "dass", "am", "er\u00b7reich\u00b7ten", "Ziel"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "VVINF", "$,", "KOUS", "APPRART", "ADJA", "NN"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ich sterbe! Dass erst, wenn es gesungen ist", "tokens": ["Ich", "ster\u00b7be", "!", "Dass", "erst", ",", "wenn", "es", "ge\u00b7sun\u00b7gen", "ist"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "KOUS", "ADV", "$,", "KOUS", "PPER", "VVPP", "VAFIN"], "meter": "-+-+-++-+-+", "measure": "unknown.measure.hexa"}, "line.3": {"text": "Das Lied von dir, ich triumphirend", "tokens": ["Das", "Lied", "von", "dir", ",", "ich", "tri\u00b7um\u00b7phi\u00b7rend"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "APPR", "PPER", "$,", "PPER", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "\u00dcber das Grab den erhabnen Weg geh!", "tokens": ["\u00dc\u00b7ber", "das", "Grab", "den", "er\u00b7hab\u00b7nen", "Weg", "geh", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.12": {"line.1": {"text": "O du mein Meister, der du gewaltiger", "tokens": ["O", "du", "mein", "Meis\u00b7ter", ",", "der", "du", "ge\u00b7wal\u00b7ti\u00b7ger"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NE", "PPOSAT", "NN", "$,", "PRELS", "PPER", "ADJA"], "meter": "+--+-++-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Die Gottheit lehrtest! zeige die Wege mir,", "tokens": ["Die", "Got\u00b7theit", "lehr\u00b7test", "!", "zei\u00b7ge", "die", "We\u00b7ge", "mir", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "VVFIN", "ART", "NN", "PPER", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Die du da gingst! worauf die Seher,", "tokens": ["Die", "du", "da", "gingst", "!", "wo\u00b7rauf", "die", "Se\u00b7her", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVFIN", "$.", "PWAV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {