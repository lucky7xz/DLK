{"dta.poem.20625": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "Bei  \n dem jubelvollen Empfange  \n der K\u00f6nigin .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die ihr euch nie den Nordwind la\u00dft entkleiden,                 ", "tokens": ["Die", "ihr", "euch", "nie", "den", "Nord\u00b7wind", "la\u00dft", "ent\u00b7klei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "ADV", "ART", "NN", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ihr Tannenwipfel, b\u00fccket euch;", "tokens": ["Ihr", "Tan\u00b7nen\u00b7wip\u00b7fel", ",", "b\u00fc\u00b7cket", "euch", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Empfangt die ", "tokens": ["Emp\u00b7fangt", "die"], "token_info": ["word", "word"], "pos": ["VVFIN", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Und rauscht mit ihm zugleich!", "tokens": ["Und", "rauscht", "mit", "ihm", "zu\u00b7gleich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Un\u00fcberz\u00e4hlbar w\u00e4lzet sich die Menge", "tokens": ["Un\u00b7\u00fc\u00b7berz\u00b7\u00e4hl\u00b7bar", "w\u00e4l\u00b7zet", "sich", "die", "Men\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PRF", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dicht aneinander durch das Thor.", "tokens": ["Dicht", "an\u00b7ein\u00b7an\u00b7der", "durch", "das", "Thor", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von zehen tausend Zungen steigt, wie Lobges\u00e4nge,", "tokens": ["Von", "ze\u00b7hen", "tau\u00b7send", "Zun\u00b7gen", "steigt", ",", "wie", "Lob\u00b7ge\u00b7s\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "CARD", "CARD", "NN", "VVFIN", "$,", "PWAV", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ein frommer Zank empor.", "tokens": ["Ein", "from\u00b7mer", "Zank", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Welch ein Get\u00fcmmel! Aller Blicke wollen", "tokens": ["Welch", "ein", "Ge\u00b7t\u00fcm\u00b7mel", "!", "Al\u00b7ler", "Bli\u00b7cke", "wol\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIAT", "ART", "NN", "$.", "ADJA", "NN", "VMFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auf einmal in Ihr Angesicht,", "tokens": ["Auf", "ein\u00b7mal", "in", "Ihr", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Jetzt kommt Sie; sehet da den Strom von Thr\u00e4nen", "tokens": ["Jetzt", "kommt", "Sie", ";", "se\u00b7het", "da", "den", "Strom", "von", "Thr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$.", "VVFIN", "ADV", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "rollen,", "tokens": ["rol\u00b7len", ","], "token_info": ["word", "punct"], "pos": ["VVINF", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Der mehr als Worte spricht!", "tokens": ["Der", "mehr", "als", "Wor\u00b7te", "spricht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "KOKOM", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Vermischte Stimmen rufen unaufh\u00f6rlich:", "tokens": ["Ver\u00b7mischte", "Stim\u00b7men", "ru\u00b7fen", "un\u00b7auf\u00b7h\u00f6r\u00b7lich", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "\u201eder Friede schwebet \u00fcber Ihr;", "tokens": ["\u201e", "der", "Frie\u00b7de", "schwe\u00b7bet", "\u00fc\u00b7ber", "Ihr", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "APPR", "PPOSAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u201eder Sieger kommt; Sein Thron, Sein Land bleibt", "tokens": ["\u201e", "der", "Sie\u00b7ger", "kommt", ";", "Sein", "Thron", ",", "Sein", "Land", "bleibt"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "ART", "NN", "VVFIN", "$.", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "unzerst\u00f6hrlich;", "tokens": ["un\u00b7zer\u00b7st\u00f6hr\u00b7lich", ";"], "token_info": ["word", "punct"], "pos": ["ADJD", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "\u201egl\u00fcckselig bleiben wir!\u201e \u2014", "tokens": ["\u201e", "gl\u00fcck\u00b7se\u00b7lig", "blei\u00b7ben", "wir", "!", "\u201e"], "token_info": ["punct", "word", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "ADJD", "VVFIN", "PPER", "$.", "$(", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.5": {"line.1": {"text": "Und ich vergesse meines Saitenspieles", "tokens": ["Und", "ich", "ver\u00b7ges\u00b7se", "mei\u00b7nes", "Sai\u00b7ten\u00b7spie\u00b7les"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gewohnte T\u00f6ne; mich entrei\u00dft", "tokens": ["Ge\u00b7wohn\u00b7te", "T\u00f6\u00b7ne", ";", "mich", "ent\u00b7rei\u00dft"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$.", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den Musen, dem Apoll, der Aufruhr des Gef\u00fchles.", "tokens": ["Den", "Mu\u00b7sen", ",", "dem", "A\u00b7poll", ",", "der", "Auf\u00b7ruhr", "des", "Ge\u00b7f\u00fch\u00b7les", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "ART", "NN", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Ich werde lauter Geist,", "tokens": ["Ich", "wer\u00b7de", "lau\u00b7ter", "Geist", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Und schwimme durch die Wellen des Gedr\u00e4nges,", "tokens": ["Und", "schwim\u00b7me", "durch", "die", "Wel\u00b7len", "des", "Ge\u00b7dr\u00e4n\u00b7ges", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zum Purpursessel, der Sie tr\u00e4gt:", "tokens": ["Zum", "Pur\u00b7pur\u00b7ses\u00b7sel", ",", "der", "Sie", "tr\u00e4gt", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Sie l\u00e4chelt mir, und horcht, wie stark mein viel zu enges", "tokens": ["Sie", "l\u00e4\u00b7chelt", "mir", ",", "und", "horcht", ",", "wie", "stark", "mein", "viel", "zu", "en\u00b7ges"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KON", "VVFIN", "$,", "PWAV", "ADJD", "PPOSAT", "ADV", "APPR", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Durchdrungnes Herze schl\u00e4gt.", "tokens": ["Durch\u00b7drung\u00b7nes", "Her\u00b7ze", "schl\u00e4gt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}