{"dta.poem.13321": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Z w\u00f6lfftes  B uch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Bey diesem Spruch und Schlu\u00df erregte sich der Saal", "tokens": ["Bey", "die\u00b7sem", "Spruch", "und", "Schlu\u00df", "er\u00b7reg\u00b7te", "sich", "der", "Saal"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "KON", "NN", "VVFIN", "PRF", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "450Und rieff: \u201dJa dieses ist die vorgesehne Wahl!", "tokens": ["rieff", ":", "\"", "Ja", "die\u00b7ses", "ist", "die", "vor\u00b7ge\u00b7seh\u00b7ne", "Wahl", "!"], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "$(", "PTKANT", "PDS", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "\u201eso siegen wir zugleich!\u201e .... Wir sahn in allen Reihen,", "tokens": ["\u201e", "so", "sie\u00b7gen", "wir", "zu\u00b7gleich", "!", "\u201e", "....", "Wir", "sahn", "in", "al\u00b7len", "Rei\u00b7hen", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "ADV", "$.", "$(", "$.", "PPER", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wie sehr die Tugenden damit vergn\u00fcget seyen;", "tokens": ["Wie", "sehr", "die", "Tu\u00b7gen\u00b7den", "da\u00b7mit", "ver\u00b7gn\u00fc\u00b7get", "se\u00b7yen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "PAV", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.5": {"text": "Man stund in Freuden auf. Die Wahrheit wich zur\u00fcck;", "tokens": ["Man", "stund", "in", "Freu\u00b7den", "auf", ".", "Die", "Wahr\u00b7heit", "wich", "zu\u00b7r\u00fcck", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "NN", "PTKVZ", "$.", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Mithin verweilte man kaum einen Augenblick;", "tokens": ["Mi\u00b7thin", "ver\u00b7weil\u00b7te", "man", "kaum", "ei\u00b7nen", "Au\u00b7gen\u00b7blick", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "455Sie eilten, flogen hin, woher sie leztlich kamen,", "tokens": ["eil\u00b7ten", ",", "flo\u00b7gen", "hin", ",", "wo\u00b7her", "sie", "lezt\u00b7lich", "ka\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "PTKVZ", "$,", "PWAV", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.8": {"text": "Und nennten tausend Mahl die K\u00f6niglichen Nahmen.", "tokens": ["Und", "nenn\u00b7ten", "tau\u00b7send", "Mahl", "die", "K\u00f6\u00b7nig\u00b7li\u00b7chen", "Nah\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "CARD", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}