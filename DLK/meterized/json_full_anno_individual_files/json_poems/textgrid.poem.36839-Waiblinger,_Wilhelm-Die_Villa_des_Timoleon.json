{"textgrid.poem.36839": {"metadata": {"author": {"name": "Waiblinger, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Die Villa des Timoleon", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "W\u00e4r's eine Nymphe, die in der Einsamkeit", "tokens": ["W\u00e4r's", "ei\u00b7ne", "Nym\u00b7phe", ",", "die", "in", "der", "Ein\u00b7sam\u00b7keit"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Dem Wandrer sich verr\u00e4th? Im Geb\u00fcsch vielleicht", "tokens": ["Dem", "Wand\u00b7rer", "sich", "ver\u00b7r\u00e4\u00b7th", "?", "Im", "Ge\u00b7b\u00fcsch", "viel\u00b7leicht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "PRF", "VVFIN", "$.", "APPRART", "NN", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Verborgen lauscht das holde Wesen", "tokens": ["Ver\u00b7bor\u00b7gen", "lauscht", "das", "hol\u00b7de", "We\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und dem Ersch\u00f6pften ert\u00f6nt die Stimme:", "tokens": ["Und", "dem", "Er\u00b7sch\u00f6pf\u00b7ten", "er\u00b7t\u00f6nt", "die", "Stim\u00b7me", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Komm, labe, Wandrer, dich und Epipoli", "tokens": ["Komm", ",", "la\u00b7be", ",", "Wand\u00b7rer", ",", "dich", "und", "E\u00b7pi\u00b7po\u00b7li"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "VVFIN", "$,", "NN", "$,", "PPER", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gest\u00e4rkt besteigst du! T\u00e4uscht' ich mich nicht, es quillt", "tokens": ["Ge\u00b7st\u00e4rkt", "be\u00b7steigst", "du", "!", "T\u00e4uscht'", "ich", "mich", "nicht", ",", "es", "quillt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVPP", "VVFIN", "PPER", "$.", "VVFIN", "PPER", "PRF", "PTKNEG", "$,", "PPER", "VVFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Vom Felsen sprudelnd und der B\u00e4ume", "tokens": ["Vom", "Fel\u00b7sen", "spru\u00b7delnd", "und", "der", "B\u00e4u\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVPP", "KON", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Freundliche Schatten verbreiten K\u00fchlung.", "tokens": ["Freund\u00b7li\u00b7che", "Schat\u00b7ten", "ver\u00b7brei\u00b7ten", "K\u00fch\u00b7lung", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Dem Berg entspro\u00dft gro\u00dfbl\u00e4tterig Indiens Frucht", "tokens": ["Dem", "Berg", "ent\u00b7spro\u00dft", "gro\u00df\u00b7bl\u00e4t\u00b7te\u00b7rig", "In\u00b7di\u00b7ens", "Frucht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Voll Purpurfeigen, auch die Cypresse ragt,", "tokens": ["Voll", "Pur\u00b7pur\u00b7fei\u00b7gen", ",", "auch", "die", "Cyp\u00b7res\u00b7se", "ragt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "$,", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Es reift die Goldorang' und lieblich", "tokens": ["Es", "reift", "die", "Gol\u00b7dorang'", "und", "lieb\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "KON", "ADJD"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Birgt sich im ewigen Gr\u00fcn die M\u00fchle.", "tokens": ["Birgt", "sich", "im", "e\u00b7wi\u00b7gen", "Gr\u00fcn", "die", "M\u00fch\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPRART", "ADJA", "NN", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "Ich trinke; dankt' ich's, lauschende Nymphe, dir?", "tokens": ["Ich", "trin\u00b7ke", ";", "dankt'", "ich's", ",", "lau\u00b7schen\u00b7de", "Nym\u00b7phe", ",", "dir", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "VVFIN", "PIS", "$,", "ADJA", "NN", "$,", "PPER", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "O welche Stille! Wohnte die Schwermuth hier,", "tokens": ["O", "wel\u00b7che", "Stil\u00b7le", "!", "Wohn\u00b7te", "die", "Schwer\u00b7muth", "hier", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PWAT", "NN", "$.", "NN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Der Schmerz, vielleicht verkannte Tugend,", "tokens": ["Der", "Schmerz", ",", "viel\u00b7leicht", "ver\u00b7kann\u00b7te", "Tu\u00b7gend", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Oder die Weisheit, die V\u00f6lkern Heil bringt?", "tokens": ["O\u00b7der", "die", "Weis\u00b7heit", ",", "die", "V\u00f6l\u00b7kern", "Heil", "bringt", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "ART", "NN", "NN", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.5": {"line.1": {"text": "Timoleon, o Name mir werther selbst", "tokens": ["Ti\u00b7mo\u00b7le\u00b7on", ",", "o", "Na\u00b7me", "mir", "wert\u00b7her", "selbst"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "FM", "NN", "PPER", "ADV", "ADV"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Als Recht und Tugend, Wort und Gedanke nur!", "tokens": ["Als", "Recht", "und", "Tu\u00b7gend", ",", "Wort", "und", "Ge\u00b7dan\u00b7ke", "nur", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "$,", "NN", "KON", "NN", "ADV", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Du bist die That! Es schuf den Menschen,", "tokens": ["Du", "bist", "die", "That", "!", "Es", "schuf", "den", "Men\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$.", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Schuf auch die Erde des Gottes That nur.", "tokens": ["Schuf", "auch", "die", "Er\u00b7de", "des", "Got\u00b7tes", "That", "nur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "ART", "NN", "NN", "ADV", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.6": {"line.1": {"text": "Timoleon, dir bietet der Denker selbst,", "tokens": ["Ti\u00b7mo\u00b7le\u00b7on", ",", "dir", "bie\u00b7tet", "der", "Den\u00b7ker", "selbst", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der Seher des Cefi\u00df, der unsterbliche,", "tokens": ["Der", "Se\u00b7her", "des", "Ce\u00b7fi\u00df", ",", "der", "uns\u00b7terb\u00b7li\u00b7che", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+--+-+-+--", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Das Haupt; was er im Geiste getr\u00e4umet,", "tokens": ["Das", "Haupt", ";", "was", "er", "im", "Geis\u00b7te", "ge\u00b7tr\u00e4u\u00b7met", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "PWS", "PPER", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Doppelt hast du's in der That geschaffen.", "tokens": ["Dop\u00b7pelt", "hast", "du's", "in", "der", "That", "ge\u00b7schaf\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PIS", "APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Sah je im Tempel gr\u00f6\u00dferen Sterblichen", "tokens": ["Sah", "je", "im", "Tem\u00b7pel", "gr\u00f6\u00b7\u00dfe\u00b7ren", "Sterb\u00b7li\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPRART", "NN", "ADJA", "NN"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ortygias Gottheit? Gelon, der Alte, nicht,", "tokens": ["Or\u00b7ty\u00b7gi\u00b7as", "Got\u00b7theit", "?", "Ge\u00b7lon", ",", "der", "Al\u00b7te", ",", "nicht", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NE", "NN", "$.", "NN", "$,", "ART", "NN", "$,", "PTKNEG", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Nicht Hermokrat, nur Einer ist hier,", "tokens": ["Nicht", "Her\u00b7mo\u00b7krat", ",", "nur", "Ei\u00b7ner", "ist", "hier", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "$,", "ADV", "PIS", "VAFIN", "ADV", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Nur Aristomaches Bruder \u00e4hnlich,", "tokens": ["Nur", "A\u00b7ris\u00b7to\u00b7ma\u00b7ches", "Bru\u00b7der", "\u00e4hn\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "ADJD", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.8": {"line.1": {"text": "Der Mann, der einst den Weisen von Griechenland", "tokens": ["Der", "Mann", ",", "der", "einst", "den", "Wei\u00b7sen", "von", "Grie\u00b7chen\u00b7land"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "ART", "NN", "APPR", "NE"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Das Schwert umg\u00fcrtet und den Tyrannen schlug,", "tokens": ["Das", "Schwert", "um\u00b7g\u00fcr\u00b7tet", "und", "den", "Ty\u00b7ran\u00b7nen", "schlug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "KON", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ein Gott und Retter heut gefeiert,", "tokens": ["Ein", "Gott", "und", "Ret\u00b7ter", "heut", "ge\u00b7fei\u00b7ert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Morgen gemordet von schn\u00f6der Habsucht.", "tokens": ["Mor\u00b7gen", "ge\u00b7mor\u00b7det", "von", "schn\u00f6\u00b7der", "Hab\u00b7sucht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "APPR", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.9": {"line.1": {"text": "Timoleon, ert\u00f6ne dein Name mir", "tokens": ["Ti\u00b7mo\u00b7le\u00b7on", ",", "er\u00b7t\u00f6\u00b7ne", "dein", "Na\u00b7me", "mir"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "VVFIN", "PPOSAT", "NN", "PPER"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Noch einmal! Gro\u00dfer Vater des Volks, du hast", "tokens": ["Noch", "ein\u00b7mal", "!", "Gro\u00b7\u00dfer", "Va\u00b7ter", "des", "Volks", ",", "du", "hast"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ADV", "$.", "ADJA", "NN", "ART", "NN", "$,", "PPER", "VAFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Zertr\u00fcmmert des Tyrannen Burg und", "tokens": ["Zer\u00b7tr\u00fcm\u00b7mert", "des", "Ty\u00b7ran\u00b7nen", "Burg", "und"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "NN", "KON"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hast auf den Tr\u00fcmmern gest\u00fcrzter Herrschaft", "tokens": ["Hast", "auf", "den", "Tr\u00fcm\u00b7mern", "ge\u00b7st\u00fcrz\u00b7ter", "Herr\u00b7schaft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.10": {"line.1": {"text": "Dir selbst den Thron, Gro\u00dfm\u00fcthigster, nicht gebaut,", "tokens": ["Dir", "selbst", "den", "Thron", ",", "Gro\u00df\u00b7m\u00fct\u00b7higs\u00b7ter", ",", "nicht", "ge\u00b7baut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "NN", "$,", "NN", "$,", "PTKNEG", "VVPP", "$,"], "meter": "-+-++-+-+-+", "measure": "unknown.measure.hexa"}, "line.2": {"text": "Wie Menschen pflegen, hast den Entfesselten", "tokens": ["Wie", "Men\u00b7schen", "pfle\u00b7gen", ",", "hast", "den", "Ent\u00b7fes\u00b7sel\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "NN", "VVINF", "$,", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Freiheit Haus und seine S\u00e4ulen,", "tokens": ["Der", "Frei\u00b7heit", "Haus", "und", "sei\u00b7ne", "S\u00e4u\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Weiser Gesetze Geschenk verliehen.", "tokens": ["Wei\u00b7ser", "Ge\u00b7set\u00b7ze", "Ge\u00b7schenk", "ver\u00b7lie\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "VVPP", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.11": {"line.1": {"text": "So, nach vollbrachtem Werke, du blinder Greis,", "tokens": ["So", ",", "nach", "voll\u00b7brach\u00b7tem", "Wer\u00b7ke", ",", "du", "blin\u00b7der", "Greis", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPR", "ADJA", "NN", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Rathgeber, angebeteter stets des Volks,", "tokens": ["Rath\u00b7ge\u00b7ber", ",", "an\u00b7ge\u00b7be\u00b7te\u00b7ter", "stets", "des", "Volks", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "ADV", "ART", "NN", "$,"], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Tratst du in Einsamkeit und Ruhe,", "tokens": ["Tratst", "du", "in", "Ein\u00b7sam\u00b7keit", "und", "Ru\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ruhe genie\u00dfend, denn Ruhe schufst du.", "tokens": ["Ru\u00b7he", "ge\u00b7nie\u00b7\u00dfend", ",", "denn", "Ru\u00b7he", "schufst", "du", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "$,", "KON", "NN", "VVFIN", "PPER", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.12": {"line.1": {"text": "O Bruderm\u00f6rder, wie doch erhabener", "tokens": ["O", "Bru\u00b7der\u00b7m\u00f6r\u00b7der", ",", "wie", "doch", "er\u00b7ha\u00b7be\u00b7ner"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "$,", "PWAV", "ADV", "ADJA"], "meter": "-+-+--+-+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Bist du als jener R\u00f6mer, der Sieger, doch", "tokens": ["Bist", "du", "als", "je\u00b7ner", "R\u00f6\u00b7mer", ",", "der", "Sie\u00b7ger", ",", "doch"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["VAFIN", "PPER", "KOUS", "PDAT", "NN", "$,", "ART", "NN", "$,", "ADV"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Zerst\u00f6rer ist. Zweimal gestritten,", "tokens": ["Zer\u00b7st\u00f6\u00b7rer", "ist", ".", "Zwei\u00b7mal", "ge\u00b7strit\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VAFIN", "$.", "ADV", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Zweimal entsagt und befreiet hast du.", "tokens": ["Zwei\u00b7mal", "ent\u00b7sagt", "und", "be\u00b7frei\u00b7et", "hast", "du", "."], "token