{"dta.poem.1008": {"metadata": {"author": {"name": "George, Stefan", "birth": "N.A.", "death": "N.A."}, "title": "T RAURIGE   T \u00c4NZE", "genre": "Lyrik", "period": "N.A.", "pub_year": "1897", "urn": "urn:nbn:de:kobv:b4-200905191530", "language": ["de:0.99"], "booktitle": "George, Stefan: Das Jahr der Seele. Berlin, 1897."}, "poem": {"stanza.1": {"line.1": {"text": "Gef&#252;hrt vom sang der leis sich schlang               ", "tokens": ["Gef", "&#252;", "hrt", "vom", "sang", "der", "leis", "sich", "schlang"], "token_info": ["word", "XML_entity", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$(", "VVFIN", "APPRART", "VVFIN", "ART", "ADJD", "PRF", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dir ward er leicht der ufergang               ", "tokens": ["Dir", "ward", "er", "leicht", "der", "u\u00b7fer\u00b7gang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich sah der h\u00f6hen dichten rauch               ", "tokens": ["Ich", "sah", "der", "h\u00f6\u00b7hen", "dich\u00b7ten", "rauch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Verj&#228;hrtes laub und distelstrauch             ", "tokens": ["Verj", "&#228;", "hr\u00b7tes", "laub", "und", "dis\u00b7tel\u00b7strauch"], "token_info": ["word", "XML_entity", "word", "word", "word", "word"], "pos": ["NE", "$(", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Dein auge schweift schon tr&#228;umerisch               ", "tokens": ["Dein", "au\u00b7ge", "schweift", "schon", "tr", "&#228;", "u\u00b7me\u00b7risch"], "token_info": ["word", "word", "word", "word", "word", "XML_entity", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "ADJD", "$(", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Auf eine erde gabenfrisch               ", "tokens": ["Auf", "ei\u00b7ne", "er\u00b7de", "ga\u00b7ben\u00b7frisch"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Denn dein gedanke flattert fort               ", "tokens": ["Denn", "dein", "ge\u00b7dan\u00b7ke", "flat\u00b7tert", "fort"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Voraus zu einem sichern hort             ", "tokens": ["Vo\u00b7raus", "zu", "ei\u00b7nem", "si\u00b7chern", "hort"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ich frage noch wer kommt wenn sanft               ", "tokens": ["Ich", "fra\u00b7ge", "noch", "wer", "kommt", "wenn", "sanft"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PWS", "VVFIN", "KOUS", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die gelbe primel nickt am ranft               ", "tokens": ["Die", "gel\u00b7be", "pri\u00b7mel", "nickt", "am", "ranft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und sich das wasser gr&#252;n umschilft               ", "tokens": ["Und", "sich", "das", "was\u00b7ser", "gr", "&#252;", "n", "um\u00b7schilft"], "token_info": ["word", "word", "word", "word", "word", "XML_entity", "word", "word"], "pos": ["KON", "PRF", "ART", "ADJA", "NN", "$(", "XY", "XY"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Der mir den mai beginnen hilft?             ", "tokens": ["Der", "mir", "den", "mai", "be\u00b7gin\u00b7nen", "hilft", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}