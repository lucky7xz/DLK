{"dta.poem.12779": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Die  \n Allerbeste reise in einem seeligen tode,  \n Fr. R. M. S. geb. C.  \n  D.  V. A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wo ist ihr edler geist", "tokens": ["Wo", "ist", "ihr", "ed\u00b7ler", "geist"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Und himmel-volle seele", "tokens": ["Und", "him\u00b7mel\u00b7vol\u00b7le", "see\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Aus ihrer leibes-h\u00f6le,", "tokens": ["Aus", "ih\u00b7rer", "lei\u00b7bes\u00b7h\u00f6le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "So pl\u00f6tzlich bingereist?", "tokens": ["So", "pl\u00f6tz\u00b7lich", "bin\u00b7ge\u00b7reist", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wie hat sie sich erhoben?", "tokens": ["Wie", "hat", "sie", "sich", "er\u00b7ho\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "PRF", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Jhr hingang ist zu loben.", "tokens": ["Ihr", "hin\u00b7gang", "ist", "zu", "lo\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VAFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wer wird es nicht gestehn,", "tokens": ["Wer", "wird", "es", "nicht", "ge\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Herr Schamberg kan erweisen,", "tokens": ["Herr", "Scham\u00b7berg", "kan", "er\u00b7wei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df seiner Liebsten reisen", "tokens": ["Da\u00df", "sei\u00b7ner", "Liebs\u00b7ten", "rei\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Weit \u00fcber seine gehn.", "tokens": ["Weit", "\u00fc\u00b7ber", "sei\u00b7ne", "gehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPOSAT", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ost-Indien ist ferne;", "tokens": ["Ost\u00b7In\u00b7di\u00b7en", "ist", "fer\u00b7ne", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADV", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.6": {"text": "Noch weiter sind die sterne.", "tokens": ["Noch", "wei\u00b7ter", "sind", "die", "ster\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "ART", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Was hat er nicht gesehn", "tokens": ["Was", "hat", "er", "nicht", "ge\u00b7sehn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "PPER", "PTKNEG", "VVINF"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Zu wasser und zu lande?", "tokens": ["Zu", "was\u00b7ser", "und", "zu", "lan\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Was ist in seinem stande", "tokens": ["Was", "ist", "in", "sei\u00b7nem", "stan\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "APPR", "PPOSAT", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Vor nutz darbey geschehn?", "tokens": ["Vor", "nutz", "dar\u00b7bey", "ge\u00b7schehn", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PAV", "VVPP", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Es brauchen edle geister", "tokens": ["Es", "brau\u00b7chen", "ed\u00b7le", "geis\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Meist die natur zum meister.", "tokens": ["Meist", "die", "na\u00b7tur", "zum", "meis\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.4": {"line.1": {"text": "Was aber vor gefahr", "tokens": ["Was", "a\u00b7ber", "vor", "ge\u00b7fahr"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "ADV", "APPR", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Ist auch dabey gewesen?", "tokens": ["Ist", "auch", "da\u00b7bey", "ge\u00b7we\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PAV", "VAPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Es l\u00e4st sich leichter lesen,", "tokens": ["Es", "l\u00e4st", "sich", "leich\u00b7ter", "le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Als selber stellen dar;", "tokens": ["Als", "sel\u00b7ber", "stel\u00b7len", "dar", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wo kranckheit, sturm und wellen", "tokens": ["Wo", "kran\u00b7ck\u00b7heit", ",", "sturm", "und", "wel\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "NN", "$,", "ADJD", "KON", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Zu barbarn sich gesellen.", "tokens": ["Zu", "bar\u00b7barn", "sich", "ge\u00b7sel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "PRF", "VVPP", "$."], "meter": "-+---+-", "measure": "dactylic.init"}}, "stanza.5": {"line.1": {"text": "O aber! wie so gut", "tokens": ["O", "a\u00b7ber", "!", "wie", "so", "gut"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "ADV", "$.", "PWAV", "ADV", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Und gar von andrer weise", "tokens": ["Und", "gar", "von", "an\u00b7drer", "wei\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ist seiner Liebsten reise,", "tokens": ["Ist", "sei\u00b7ner", "Liebs\u00b7ten", "rei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die sie in himmel thut!", "tokens": ["Die", "sie", "in", "him\u00b7mel", "thut", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sie zieht zu ihrer sonne", "tokens": ["Sie", "zieht", "zu", "ih\u00b7rer", "son\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mit lauter freud und wonne.", "tokens": ["Mit", "lau\u00b7ter", "freud", "und", "won\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "KON", "PWAV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Dort folget nun die that,", "tokens": ["Dort", "fol\u00b7get", "nun", "die", "that", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Da unter viel beschwerden,", "tokens": ["Da", "un\u00b7ter", "viel", "be\u00b7schwer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PIAT", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die hoffnung hier auf erden,", "tokens": ["Die", "hoff\u00b7nung", "hier", "auf", "er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Jhr vorgebirge hat.", "tokens": ["Ihr", "vor\u00b7ge\u00b7bir\u00b7ge", "hat", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Kein sturm kan ihr mehr schaden,", "tokens": ["Kein", "sturm", "kan", "ihr", "mehr", "scha\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Jhr schiff ist ausgeladen.", "tokens": ["Ihr", "schiff", "ist", "aus\u00b7ge\u00b7la\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Was geht sie Zeylan an?", "tokens": ["Was", "geht", "sie", "Zey\u00b7lan", "an", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Hat Holland sein ergetzen", "tokens": ["Hat", "Hol\u00b7land", "sein", "er\u00b7get\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "NE", "PPOSAT", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "An Java reichen sch\u00e4tzen?", "tokens": ["An", "Ja\u00b7va", "rei\u00b7chen", "sch\u00e4t\u00b7zen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADJA", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Damit ists nicht gethan.", "tokens": ["Da\u00b7mit", "ists", "nicht", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Das paradie\u00df vor allen,", "tokens": ["Das", "pa\u00b7ra\u00b7die\u00df", "vor", "al\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "PIAT", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Hat ihr allein gefallen.", "tokens": ["Hat", "ihr", "al\u00b7lein", "ge\u00b7fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Sie l\u00e4st zwar auf der see", "tokens": ["Sie", "l\u00e4st", "zwar", "auf", "der", "see"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "So mann als kinder schweben,", "tokens": ["So", "mann", "als", "kin\u00b7der", "schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "KOKOM", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und ihr betr\u00fcbtes leben", "tokens": ["Und", "ihr", "be\u00b7tr\u00fcb\u00b7tes", "le\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "F\u00fchlt lauter ach und weh:", "tokens": ["F\u00fchlt", "lau\u00b7ter", "ach", "und", "weh", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "XY", "KON", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und dennoch wird ihr segen", "tokens": ["Und", "den\u00b7noch", "wird", "ihr", "se\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Auch diesen sturm bald legen.", "tokens": ["Auch", "die\u00b7sen", "sturm", "bald", "le\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDAT", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Sie aber bleibt in ruh,", "tokens": ["Sie", "a\u00b7ber", "bleibt", "in", "ruh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Und au\u00dfer allem leide,", "tokens": ["Und", "au\u00b7\u00dfer", "al\u00b7lem", "lei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "In steter lust und freude;", "tokens": ["In", "ste\u00b7ter", "lust", "und", "freu\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wir w\u00fcnschen gl\u00fcck darzu:", "tokens": ["Wir", "w\u00fcn\u00b7schen", "gl\u00fcck", "dar\u00b7zu", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und stehn am ufer stille,", "tokens": ["Und", "stehn", "am", "u\u00b7fer", "stil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Herr! bis geschieht dein wille.", "tokens": ["Herr", "!", "bis", "ge\u00b7schieht", "dein", "wil\u00b7le", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}