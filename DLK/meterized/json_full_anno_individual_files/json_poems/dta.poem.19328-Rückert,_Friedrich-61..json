{"dta.poem.19328": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "61.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Verzweifelst du, der Welt zu schaun ins innre Wesen,", "tokens": ["Ver\u00b7zwei\u00b7felst", "du", ",", "der", "Welt", "zu", "schaun", "ins", "inn\u00b7re", "We\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ART", "NN", "PTKZU", "VVINF", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So schau umher auf ihr, wie viele sind gewesen,", "tokens": ["So", "schau", "um\u00b7her", "auf", "ihr", ",", "wie", "vie\u00b7le", "sind", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PTKVZ", "APPR", "PPER", "$,", "PWAV", "PIS", "VAFIN", "VAPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wie viele werden seyn, wie viele sind um dich,", "tokens": ["Wie", "vie\u00b7le", "wer\u00b7den", "seyn", ",", "wie", "vie\u00b7le", "sind", "um", "dich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VAINF", "VAINF", "$,", "PWAV", "PIS", "VAFIN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die ihren Stand zur Welt, den Stand der Welt zu sich", "tokens": ["Die", "ih\u00b7ren", "Stand", "zur", "Welt", ",", "den", "Stand", "der", "Welt", "zu", "sich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPOSAT", "NN", "APPRART", "NN", "$,", "ART", "NN", "ART", "NN", "APPR", "PRF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Begreifen, und mit ihr wohl wissen auszukommen,", "tokens": ["Be\u00b7grei\u00b7fen", ",", "und", "mit", "ihr", "wohl", "wis\u00b7sen", "aus\u00b7zu\u00b7kom\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "APPR", "PPER", "ADV", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch haben nie die H\u00f6hn des Weltbegriffs erklommen.", "tokens": ["Doch", "ha\u00b7ben", "nie", "die", "H\u00f6hn", "des", "Welt\u00b7be\u00b7griffs", "er\u00b7klom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Drum m\u00fc\u00dfen andere Erkenntnisquellen flie\u00dfen,", "tokens": ["Drum", "m\u00fc\u00b7\u00dfen", "an\u00b7de\u00b7re", "Er\u00b7kennt\u00b7nis\u00b7quel\u00b7len", "flie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die dir kein Schl\u00fcssel braucht des Gr\u00fcbelns aufzuschlie\u00dfen.", "tokens": ["Die", "dir", "kein", "Schl\u00fcs\u00b7sel", "braucht", "des", "Gr\u00fc\u00b7belns", "auf\u00b7zu\u00b7schlie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PIAT", "NN", "VVFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Aus diesen sch\u00f6pfe, so, da\u00df Vorwitz nie sie stopft;", "tokens": ["Aus", "die\u00b7sen", "sch\u00f6p\u00b7fe", ",", "so", ",", "da\u00df", "Vor\u00b7witz", "nie", "sie", "stopft", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "$,", "ADV", "$,", "KOUS", "NN", "ADV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Aus Felsen springt ein Quell, wo nur der Glaub' anklopft.", "tokens": ["Aus", "Fel\u00b7sen", "springt", "ein", "Quell", ",", "wo", "nur", "der", "Glaub'", "an\u00b7klopft", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "$,", "PWAV", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}