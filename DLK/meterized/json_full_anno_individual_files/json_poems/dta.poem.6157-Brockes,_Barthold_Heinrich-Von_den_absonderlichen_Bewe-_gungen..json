{"dta.poem.6157": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den absonderlichen Bewe-  \n gungen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die unterschiedne Macht", "tokens": ["Die", "un\u00b7ter\u00b7schied\u00b7ne", "Macht"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Von einer jeglichen Bewegung stammt allein", "tokens": ["Von", "ei\u00b7ner", "jeg\u00b7li\u00b7chen", "Be\u00b7we\u00b7gung", "stammt", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "PIAT", "NN", "VVFIN", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Von der Bewegung her, die allgemein.", "tokens": ["Von", "der", "Be\u00b7we\u00b7gung", "her", ",", "die", "all\u00b7ge\u00b7mein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$,", "PRELS", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und ob es gleich gantz anders scheint zu seyn,", "tokens": ["Und", "ob", "es", "gleich", "gantz", "an\u00b7ders", "scheint", "zu", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "ADV", "ADV", "VVFIN", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ist es dennoch, wenn wir es \u00fcberlegen,", "tokens": ["Ist", "es", "den\u00b7noch", ",", "wenn", "wir", "es", "\u00fc\u00b7berl\u00b7e\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "$,", "KOUS", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Richts, als von einem Ort zum andern, ein Bewegen.", "tokens": ["Richts", ",", "als", "von", "ei\u00b7nem", "Ort", "zum", "an\u00b7dern", ",", "ein", "Be\u00b7we\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "APPR", "ART", "NN", "APPRART", "ADJA", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Kan unser Sinn nun gleich hievon nicht Nachricht kriegen:", "tokens": ["Kan", "un\u00b7ser", "Sinn", "nun", "gleich", "hie\u00b7von", "nicht", "Nach\u00b7richt", "krie\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "ADV", "ADV", "PAV", "PTKNEG", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So mu\u00df sich unser Geist doch nicht damit betr\u00fcgen.", "tokens": ["So", "mu\u00df", "sich", "un\u00b7ser", "Geist", "doch", "nicht", "da\u00b7mit", "be\u00b7tr\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PRF", "PPOSAT", "NN", "ADV", "PTKNEG", "PAV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}