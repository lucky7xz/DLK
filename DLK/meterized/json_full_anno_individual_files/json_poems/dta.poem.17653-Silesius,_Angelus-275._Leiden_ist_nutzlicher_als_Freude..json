{"dta.poem.17653": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "275.  Leiden ist nutzlicher als Freude.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Mensch w\u00fcstestu wie gut und nutzlich\u2019s Leiden ist/", "tokens": ["Mensch", "w\u00fcs\u00b7te\u00b7stu", "wie", "gut", "und", "nutz\u00b7lich's", "Lei\u00b7den", "ist", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "KOKOM", "ADJD", "KON", "ADJA", "NN", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du h\u00e4ttest\u2019s dir vorl\u00e4ngsi f\u00fcr aller Lust erkiest.", "tokens": ["Du", "h\u00e4t\u00b7test's", "dir", "vor\u00b7l\u00e4n\u00b7gsi", "f\u00fcr", "al\u00b7ler", "Lust", "er\u00b7kiest", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}}}}}