{"dta.poem.19542": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Nicola Pesce.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "Ein halbes J\u00e4hrchen hab' ich nun geschwommen", "tokens": ["Ein", "hal\u00b7bes", "J\u00e4hr\u00b7chen", "hab'", "ich", "nun", "ge\u00b7schwom\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "ADV", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und noch behagt mir dieses k\u00fchle Gleiten,", "tokens": ["Und", "noch", "be\u00b7hagt", "mir", "die\u00b7ses", "k\u00fch\u00b7le", "Glei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Arme l\u00e4ssig Auseinanderbreiten \u2014", "tokens": ["Der", "Ar\u00b7me", "l\u00e4s\u00b7sig", "Aus\u00b7ein\u00b7an\u00b7der\u00b7brei\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Fastenspeise mag der Seele frommen!", "tokens": ["Die", "Fas\u00b7ten\u00b7spei\u00b7se", "mag", "der", "See\u00b7le", "from\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Halb schlummernd lieg' ich stundenlang, umglommen", "tokens": ["Halb", "schlum\u00b7mernd", "lieg'", "ich", "stun\u00b7den\u00b7lang", ",", "um\u00b7glom\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADJD", "ADJD", "VVFIN", "PPER", "VVFIN", "$,", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von Wetterleuchten, bis auf allen Seiten", "tokens": ["Von", "Wet\u00b7ter\u00b7leuch\u00b7ten", ",", "bis", "auf", "al\u00b7len", "Sei\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "KOUS", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sich Wogen th\u00fcrmen. M\u00e4nnlich gilt's zu streiten.", "tokens": ["Sich", "Wo\u00b7gen", "th\u00fcr\u00b7men", ".", "M\u00e4nn\u00b7lich", "gilt's", "zu", "strei\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PRF", "NN", "VVINF", "$.", "NN", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ich freue mich. Stets bin ich durchgekommen.", "tokens": ["Ich", "freu\u00b7e", "mich", ".", "Stets", "bin", "ich", "durch\u00b7ge\u00b7kom\u00b7men", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$.", "ADV", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Was machte mich zum Fisch? Ein Mi\u00dfverst\u00e4ndni\u00df", "tokens": ["Was", "mach\u00b7te", "mich", "zum", "Fisch", "?", "Ein", "Mi\u00df\u00b7ver\u00b7st\u00e4nd\u00b7ni\u00df"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "APPRART", "NN", "$.", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit meinem Weib. Vermehrte Menschenkenntni\u00df.", "tokens": ["Mit", "mei\u00b7nem", "Weib", ".", "Ver\u00b7mehr\u00b7te", "Men\u00b7schen\u00b7kennt\u00b7ni\u00df", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$.", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mein Wanderdrang und meine Farbenlust.", "tokens": ["Mein", "Wan\u00b7der\u00b7drang", "und", "mei\u00b7ne", "Far\u00b7ben\u00b7lust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Die Furcht verlernt' ich \u00fcber Todestiefen,", "tokens": ["Die", "Furcht", "ver\u00b7lernt'", "ich", "\u00fc\u00b7ber", "To\u00b7des\u00b7tie\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Fast bis zum Frieren k\u00fchlt' ich mir die Brust \u2014", "tokens": ["Fast", "bis", "zum", "Frie\u00b7ren", "k\u00fchlt'", "ich", "mir", "die", "Brust"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "APPRART", "NN", "VVFIN", "PPER", "PPER", "ART", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich ", "tokens": ["Ich"], "token_info": ["word"], "pos": ["PPER"], "meter": "-", "measure": "single.down"}}}}}