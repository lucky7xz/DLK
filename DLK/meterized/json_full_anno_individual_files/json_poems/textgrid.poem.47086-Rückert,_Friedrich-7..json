{"textgrid.poem.47086": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "7.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "W\u00e4r' ich wie ihr, ihr sommerlichen Schwalben,", "tokens": ["W\u00e4r'", "ich", "wie", "ihr", ",", "ihr", "som\u00b7mer\u00b7li\u00b7chen", "Schwal\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PWAV", "PPER", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich wandert' aus von dieser \u00f6den Heide;", "tokens": ["Ich", "wan\u00b7dert'", "aus", "von", "die\u00b7ser", "\u00f6\u00b7den", "Hei\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich schw\u00f6r' es euch bei meines Herzens Leide,", "tokens": ["Ich", "schw\u00f6r'", "es", "euch", "bei", "mei\u00b7nes", "Her\u00b7zens", "Lei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PRF", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ihr seht's nur nicht, der Herbst ist allenthalben.", "tokens": ["Ihr", "seht's", "nur", "nicht", ",", "der", "Herbst", "ist", "al\u00b7len\u00b7thal\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKNEG", "$,", "ART", "NN", "VAFIN", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und ihr, die ihr noch leben wollt mit halben", "tokens": ["Und", "ihr", ",", "die", "ihr", "noch", "le\u00b7ben", "wollt", "mit", "hal\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "$,", "PRELS", "PPER", "ADV", "VVINF", "VMFIN", "APPR", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Scheinleben, Birke, Buche, Lind' und Weide,", "tokens": ["Schein\u00b7le\u00b7ben", ",", "Bir\u00b7ke", ",", "Bu\u00b7che", ",", "Lind'", "und", "Wei\u00b7de", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NE", "KON", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Ich rat' es euch, la\u00dft ab vom gr\u00fcnen Kleide", "tokens": ["Ich", "rat'", "es", "euch", ",", "la\u00dft", "ab", "vom", "gr\u00fc\u00b7nen", "Klei\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PPER", "$,", "VVIMP", "PTKVZ", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und kleidet ohne Scheu euch mit dem falben.", "tokens": ["Und", "klei\u00b7det", "oh\u00b7ne", "Scheu", "euch", "mit", "dem", "fal\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "PPER", "APPR", "ART", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Fragt nicht, warum? Fragt nicht, was denn im Gange", "tokens": ["Fragt", "nicht", ",", "wa\u00b7rum", "?", "Fragt", "nicht", ",", "was", "denn", "im", "Gan\u00b7ge"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKNEG", "$,", "PWAV", "$.", "VVFIN", "PTKNEG", "$,", "PRELS", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Natur, die alte Mutter, pl\u00f6tzlich st\u00f6rte,", "tokens": ["Na\u00b7tur", ",", "die", "al\u00b7te", "Mut\u00b7ter", ",", "pl\u00f6tz\u00b7lich", "st\u00f6r\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "ADJA", "NN", "$,", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df Herbst kommt in den Fr\u00fchling eingebrochen?", "tokens": ["Da\u00df", "Herbst", "kommt", "in", "den", "Fr\u00fch\u00b7ling", "ein\u00b7ge\u00b7bro\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Nicht erst seit heut ist's ja, es ist seit lange;", "tokens": ["Nicht", "erst", "seit", "heut", "ist's", "ja", ",", "es", "ist", "seit", "lan\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "APPR", "ADV", "VAFIN", "ADV", "$,", "PPER", "VAFIN", "APPR", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Denn ", "tokens": ["Denn"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Schl\u00e4ft ihren Winterschlaf schon sieben Wochen.", "tokens": ["Schl\u00e4ft", "ih\u00b7ren", "Win\u00b7ter\u00b7schlaf", "schon", "sie\u00b7ben", "Wo\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADV", "CARD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "W\u00e4r' ich wie ihr, ihr sommerlichen Schwalben,", "tokens": ["W\u00e4r'", "ich", "wie", "ihr", ",", "ihr", "som\u00b7mer\u00b7li\u00b7chen", "Schwal\u00b7ben", ","], "token_info": [