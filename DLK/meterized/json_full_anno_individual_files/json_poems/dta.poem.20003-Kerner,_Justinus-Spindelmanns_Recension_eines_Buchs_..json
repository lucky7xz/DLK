{"dta.poem.20003": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Spindelmanns Recension eines Buchs .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905193111", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "'s ist kein ganz schlechtes Lesen drum,", "tokens": ["'s", "ist", "kein", "ganz", "schlech\u00b7tes", "Le\u00b7sen", "drum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "ADV", "ADJA", "NN", "PAV", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "'s ist aber noch nicht aufgeschnitten,", "tokens": ["'s", "ist", "a\u00b7ber", "noch", "nicht", "auf\u00b7ge\u00b7schnit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Wenn man die Nase reibt drauf 'rum,", "tokens": ["Wenn", "man", "die", "Na\u00b7se", "reibt", "drauf", "'rum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "NN", "VVFIN", "PAV", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So riecht's nach was, \u2014 ich mein', nach Quitten.", "tokens": ["So", "riecht's", "nach", "was", ",", "ich", "mein'", ",", "nach", "Quit\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PRELS", "$,", "$(", "PPER", "VVFIN", "$,", "APPR", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}}}}