{"textgrid.poem.46233": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "An Herren Hans Jacob Grob, meinen alten werten gelehrten freind", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O dessen werte werk und weisheit wol bezeugen,", "tokens": ["O", "des\u00b7sen", "wer\u00b7te", "werk", "und", "weis\u00b7heit", "wol", "be\u00b7zeu\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PDS", "VVFIN", "NN", "KON", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "da\u00df nichts, dan nur dein nam, an dir zu nennen grob;", "tokens": ["da\u00df", "nichts", ",", "dan", "nur", "dein", "nam", ",", "an", "dir", "zu", "nen\u00b7nen", "grob", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "$,", "ADV", "ADV", "PPOSAT", "VVFIN", "$,", "APPR", "PPER", "PTKZU", "VVINF", "ADJD", "$."], "meter": "+-+-+-+-+--+", "measure": "iambic.hexa.chol"}, "line.3": {"text": "grob w\u00e4r ich selbs und b\u00f6s, lang f\u00fcr der welt dein lob,", "tokens": ["grob", "w\u00e4r", "ich", "selbs", "und", "b\u00f6s", ",", "lang", "f\u00fcr", "der", "welt", "dein", "lob", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "ADV", "KON", "ADJD", "$,", "ADJD", "APPR", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "welches der Musen zunft vermeldet, zu verschweigen;", "tokens": ["wel\u00b7ches", "der", "Mu\u00b7sen", "zunft", "ver\u00b7mel\u00b7det", ",", "zu", "ver\u00b7schwei\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "ADJD", "VVPP", "$,", "PTKZU", "VVINF", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.5": {"text": "Zwar dein verdienst bedarf nu weiter keine zeugen,", "tokens": ["Zwar", "dein", "ver\u00b7dienst", "be\u00b7darf", "nu", "wei\u00b7ter", "kei\u00b7ne", "zeu\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN", "ADV", "ADV", "PIAT", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "weil landgraf Moritz schon (den zwar, wie ich dich lob", "tokens": ["weil", "land\u00b7graf", "Mo\u00b7ritz", "schon", "(", "den", "zwar", ",", "wie", "ich", "dich", "lob"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "NE", "NE", "ADV", "$(", "ART", "ADV", "$,", "PWAV", "PPER", "PRF", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "durch ihn, ich mit dir ehr) auf unfehlbare prob", "tokens": ["durch", "ihn", ",", "ich", "mit", "dir", "ehr", ")", "auf", "un\u00b7fehl\u00b7ba\u00b7re", "prob"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPER", "$,", "PPER", "APPR", "PPER", "NN", "$(", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "dein haupt gekr\u00f6net hat mit seinen gnadenzweigen.", "tokens": ["dein", "haupt", "ge\u00b7kr\u00f6\u00b7net", "hat", "mit", "sei\u00b7nen", "gna\u00b7den\u00b7zwei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "VAFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Darum