{"dta.poem.23701": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Seufzer an den Fr\u00fcling.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-200905197682", "language": ["de:0.85", "da:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Kehre wieder, holder Fr&#252;ling,", "tokens": ["Keh\u00b7re", "wie\u00b7der", ",", "hol\u00b7der", "Fr", "&#252;", "ling", ","], "token_info": ["word", "word", "punct", "word", "word", "XML_entity", "word", "punct"], "pos": ["NN", "ADV", "$,", "ADJA", "NN", "$(", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kehre wieder, Kind des Himmels!", "tokens": ["Keh\u00b7re", "wie\u00b7der", ",", "Kind", "des", "Him\u00b7mels", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Doris will, wenn du es siehest,", "tokens": ["Do\u00b7ris", "will", ",", "wenn", "du", "es", "sie\u00b7hest", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Wenn sie Nachtigallen lokken,", "tokens": ["Wenn", "sie", "Nach\u00b7ti\u00b7gal\u00b7len", "lok\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Immer k&#252;ssen.", "tokens": ["Im\u00b7mer", "k", "&#252;", "ssen", "."], "token_info": ["word", "word", "XML_entity", "word", "punct"], "pos": ["ADV", "ADJD", "$(", "VVINF", "$."], "meter": "+---", "measure": "dactylic.init"}}}}}