{"dta.poem.13469": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "IiI .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Ich reise \u00fcber's gr\u00fcne Land,", "tokens": ["Ich", "rei\u00b7se", "\u00fc\u00b7ber's", "gr\u00fc\u00b7ne", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Winter ist vergangen,", "tokens": ["Der", "Win\u00b7ter", "ist", "ver\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Hab' um den Hals ein g\u00fclden Band,", "tokens": ["Hab'", "um", "den", "Hals", "ein", "g\u00fcl\u00b7den", "Band", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Daran die Laute hangen.", "tokens": ["Da\u00b7ran", "die", "Lau\u00b7te", "han\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der Morgen thut ein'n rothen Schein,", "tokens": ["Der", "Mor\u00b7gen", "thut", "ein'n", "ro\u00b7then", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den recht mein Herze sp\u00fcret,", "tokens": ["Den", "recht", "mein", "Her\u00b7ze", "sp\u00fc\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da greif ich in die Saiten ein,", "tokens": ["Da", "greif", "ich", "in", "die", "Sai\u00b7ten", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der liebe Gott mich f\u00fchret.", "tokens": ["Der", "lie\u00b7be", "Gott", "mich", "f\u00fch\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "So silbern geht der Str\u00f6me Lauf,", "tokens": ["So", "sil\u00b7bern", "geht", "der", "Str\u00f6\u00b7me", "Lauf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Fern\u00fcber schallt Gel\u00e4ute,", "tokens": ["Fern\u00b7\u00fc\u00b7ber", "schallt", "Ge\u00b7l\u00e4u\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Seele ruft in sich: Gl\u00fcck auf!", "tokens": ["Die", "See\u00b7le", "ruft", "in", "sich", ":", "Gl\u00fcck", "auf", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PRF", "$.", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Rings gr\u00fc\u00dfen frohe Leute.", "tokens": ["Rings", "gr\u00fc\u00b7\u00dfen", "fro\u00b7he", "Leu\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Mein Herz ist recht von Diamant,", "tokens": ["Mein", "Herz", "ist", "recht", "von", "Di\u00b7a\u00b7mant", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Blum' von Edelsteinen,", "tokens": ["Ein", "Blum'", "von", "E\u00b7del\u00b7stei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die funkelt lustig \u00fcber's Land", "tokens": ["Die", "fun\u00b7kelt", "lus\u00b7tig", "\u00fc\u00b7ber's", "Land"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In tausend sch\u00f6nen Scheinen.", "tokens": ["In", "tau\u00b7send", "sch\u00f6\u00b7nen", "Schei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Vom Schlosse in die weite Welt", "tokens": ["Vom", "Schlos\u00b7se", "in", "die", "wei\u00b7te", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schaut eine Jungfrau runter,", "tokens": ["Schaut", "ei\u00b7ne", "Jung\u00b7frau", "run\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Liebste sie im Arme h\u00e4lt,", "tokens": ["Der", "Liebs\u00b7te", "sie", "im", "Ar\u00b7me", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die seh'n nach mir herunter.", "tokens": ["Die", "seh'n", "nach", "mir", "her\u00b7un\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Wie bist du sch\u00f6n! \u2014 Hinaus, im Wald", "tokens": ["Wie", "bist", "du", "sch\u00f6n", "!", "Hin\u00b7aus", ",", "im", "Wald"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "punct", "word", "word"], "pos": ["PWAV", "VAFIN", "PPER", "ADJD", "$.", "$(", "ADV", "$,", "APPRART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Gehn Wasser auf und unter,", "tokens": ["Gehn", "Was\u00b7ser", "auf", "und", "un\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "PTKVZ", "KON", "APPR", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Im gr\u00fcnen Wald sing' da\u00df es schallt,", "tokens": ["Im", "gr\u00fc\u00b7nen", "Wald", "sing'", "da\u00df", "es", "schallt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mein Herz, bleib' frei und munter!", "tokens": ["Mein", "Herz", ",", "bleib'", "frei", "und", "mun\u00b7ter", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Die Sonne uns im Dunklen l\u00e4\u00dft,", "tokens": ["Die", "Son\u00b7ne", "uns", "im", "Dunk\u00b7len", "l\u00e4\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im Meere sich zu sp\u00fclen,", "tokens": ["Im", "Mee\u00b7re", "sich", "zu", "sp\u00fc\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PRF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da ruh' ich aus vom Tages-Fest", "tokens": ["Da", "ruh'", "ich", "aus", "vom", "Ta\u00b7ges\u00b7Fest"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Fromm in der rothen K\u00fchle.", "tokens": ["Fromm", "in", "der", "ro\u00b7then", "K\u00fch\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Hoch f\u00fchret durch die stille Nacht", "tokens": ["Hoch", "f\u00fch\u00b7ret", "durch", "die", "stil\u00b7le", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Mond die goldnen Schaafe,", "tokens": ["Der", "Mond", "die", "gold\u00b7nen", "Schaa\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Den Kreis der Erden Gott bewacht,", "tokens": ["Den", "Kreis", "der", "Er\u00b7den", "Gott", "be\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wo ich tief unten schlafe.", "tokens": ["Wo", "ich", "tief", "un\u00b7ten", "schla\u00b7fe", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "ADV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Wie liegt all' falsche Pracht so weit!", "tokens": ["Wie", "liegt", "all'", "fal\u00b7sche", "Pracht", "so", "weit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PIAT", "ADJA", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schlaf wohl auf stiller Erde,", "tokens": ["Schlaf", "wohl", "auf", "stil\u00b7ler", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Gott sch\u00fctz' dein Herz in Ewigkeit,", "tokens": ["Gott", "sch\u00fctz'", "dein", "Herz", "in", "E\u00b7wig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df es nie traurig werde!", "tokens": ["Da\u00df", "es", "nie", "trau\u00b7rig", "wer\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}