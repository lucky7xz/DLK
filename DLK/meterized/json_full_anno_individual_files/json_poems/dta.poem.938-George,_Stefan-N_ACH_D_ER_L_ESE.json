{"dta.poem.938": {"metadata": {"author": {"name": "George, Stefan", "birth": "N.A.", "death": "N.A."}, "title": "N ACH   D ER   L ESE", "genre": "Lyrik", "period": "N.A.", "pub_year": "1897", "urn": "urn:nbn:de:kobv:b4-200905191530", "language": ["de:0.99"], "booktitle": "George, Stefan: Das Jahr der Seele. Berlin, 1897."}, "poem": {"stanza.1": {"line.1": {"text": "Ich schrieb es auf: nicht l&#228;nger sei verhehlt               ", "tokens": ["Ich", "schrieb", "es", "auf", ":", "nicht", "l", "&#228;", "nger", "sei", "ver\u00b7hehlt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "XML_entity", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$.", "PTKNEG", "NE", "$(", "ADJD", "VAFIN", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Was als gedanken ich nicht mehr verbanne               ", "tokens": ["Was", "als", "ge\u00b7dan\u00b7ken", "ich", "nicht", "mehr", "ver\u00b7ban\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "KOUS", "NN", "PPER", "PTKNEG", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Was ich nicht sage du nicht f&#252;hlst: uns fehlt               ", "tokens": ["Was", "ich", "nicht", "sa\u00b7ge", "du", "nicht", "f", "&#252;", "hlst", ":", "uns", "fehlt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "XML_entity", "word", "punct", "word", "word"], "pos": ["PWS", "PPER", "PTKNEG", "VVFIN", "PPER", "PTKNEG", "NE", "$(", "VVFIN", "$.", "PPER", "VVFIN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Bis an das gl&#252;ck noch eine weite spanne             ", "tokens": ["Bis", "an", "das", "gl", "&#252;", "ck", "noch", "ei\u00b7ne", "wei\u00b7te", "span\u00b7ne"], "token_info": ["word", "word", "word", "word", "XML_entity", "word", "word", "word", "word", "word"], "pos": ["APPR", "APPR", "ART", "NN", "$(", "NE", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "An einer hohen blume welkem stiel               ", "tokens": ["An", "ei\u00b7ner", "ho\u00b7hen", "blu\u00b7me", "wel\u00b7kem", "stiel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "PWAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Entfaltest du's &#183; ich stehe fern und ahne &#183; &#183;               ", "tokens": ["Ent\u00b7fal\u00b7test", "du's", "&#183;", "ich", "ste\u00b7he", "fern", "und", "ah\u00b7ne", "&#183;", "&#183;"], "token_info": ["word", "word", "XML_entity", "word", "word", "word", "word", "word", "XML_entity", "XML_entity"], "pos": ["VVFIN", "PIS", "$(", "PPER", "VVFIN", "ADJD", "KON", "VVFIN", "$(", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es war das weisse blatt das dir entfiel               ", "tokens": ["Es", "war", "das", "weis\u00b7se", "blatt", "das", "dir", "ent\u00b7fiel"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "ART", "PPER", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die grellste farbe auf dem fahlen plane.             ", "tokens": ["Die", "grells\u00b7te", "far\u00b7be", "auf", "dem", "fah\u00b7len", "pla\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}