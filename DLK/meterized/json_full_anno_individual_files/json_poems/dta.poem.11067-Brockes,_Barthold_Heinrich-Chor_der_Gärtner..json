{"dta.poem.11067": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Chor der G\u00e4rtner.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.71", "en:0.28"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ruhe wohl, ", "tokens": ["Ru\u00b7he", "wohl", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "ADV", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Bis du dort den Himmels-Garten,", "tokens": ["Bis", "du", "dort", "den", "Him\u00b7mels\u00b7Gar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Des die Seraphinen warten,", "tokens": ["Des", "die", "Se\u00b7ra\u00b7phi\u00b7nen", "war\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auch erstanden, bauen wirst;", "tokens": ["Auch", "er\u00b7stan\u00b7den", ",", "bau\u00b7en", "wirst", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVPP", "$,", "VVINF", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ruhe wohl!", "tokens": ["Ru\u00b7he", "wohl", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "ADV", "$."], "meter": "+-+", "measure": "trochaic.di"}}}}}