{"dta.poem.2073": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Das Kaninchen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dieses Thier gleicht fast den Hasen; aber wohnt in", "tokens": ["Die\u00b7ses", "Thier", "gleicht", "fast", "den", "Ha\u00b7sen", ";", "a\u00b7ber", "wohnt", "in"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDAT", "NN", "VVFIN", "ADV", "ART", "NN", "$.", "ADV", "VVFIN", "APPR"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Gruben meist,", "tokens": ["Gru\u00b7ben", "meist", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Wie es denn fast jeden Boden theilt, zerw\u00fchlet und zer-", "tokens": ["Wie", "es", "denn", "fast", "je\u00b7den", "Bo\u00b7den", "theilt", ",", "zer\u00b7w\u00fch\u00b7let", "und", "zer"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "ADV", "PIAT", "NN", "VVFIN", "$,", "VVFIN", "KON", "TRUNC"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.4": {"text": "rei\u00dft.", "tokens": ["rei\u00dft", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-", "measure": "single.down"}, "line.5": {"text": "Sie bereiten gleichfalls k\u00fcnstlich ihren unterirdschen Bau,", "tokens": ["Sie", "be\u00b7rei\u00b7ten", "gleich\u00b7falls", "k\u00fcnst\u00b7lich", "ih\u00b7ren", "un\u00b7ter\u00b7ird\u00b7schen", "Bau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.6": {"text": "Jhr Farb ist wei\u00df entweder, oder sprenklicht, oder grau.", "tokens": ["Ihr", "Farb", "ist", "wei\u00df", "ent\u00b7we\u00b7der", ",", "o\u00b7der", "sprenk\u00b7licht", ",", "o\u00b7der", "grau", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVFIN", "PTKVZ", "$,", "KON", "VVFIN", "$,", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.7": {"text": "Sie sind mehr als andre Thiere von so gro\u00dfer Frucht-", "tokens": ["Sie", "sind", "mehr", "als", "and\u00b7re", "Thie\u00b7re", "von", "so", "gro\u00b7\u00dfer", "Frucht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "KOUS", "ADJA", "NN", "APPR", "ADV", "ADJA", "TRUNC"], "meter": "----+-+-+-+-+", "measure": "unknown.measure.penta"}, "line.8": {"text": "barkeit,", "tokens": ["bar\u00b7keit", ","], "token_info": ["word", "punct"], "pos": ["ADV", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.9": {"text": "Da\u00df sie zw\u00f6lfmal Junge hecken nur in eines Jahres Zeit,", "tokens": ["Da\u00df", "sie", "zw\u00f6lf\u00b7mal", "Jun\u00b7ge", "he\u00b7cken", "nur", "in", "ei\u00b7nes", "Jah\u00b7res", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "NN", "VVFIN", "ADV", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.10": {"text": "Und fast immer f\u00fcnf bis sechs. Es giebt ihrer zweyer-", "tokens": ["Und", "fast", "im\u00b7mer", "f\u00fcnf", "bis", "sechs", ".", "Es", "giebt", "ih\u00b7rer", "zweyer"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "CARD", "APPR", "CARD", "$.", "PPER", "VVFIN", "PPOSAT", "TRUNC"], "meter": "-+--+-+-+--+", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "ley,", "tokens": ["ley", ","], "token_info": ["word", "punct"], "pos": ["ADV", "$,"], "meter": "+", "measure": "single.up"}, "line.12": {"text": "Wilde n\u00e4mlich, und auch zahme. Doch ein gro\u00dfer Un-", "tokens": ["Wil\u00b7de", "n\u00e4m\u00b7lich", ",", "und", "auch", "zah\u00b7me", ".", "Doch", "ein", "gro\u00b7\u00dfer", "Un"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "ADV", "$,", "KON", "ADV", "VVFIN", "$.", "KON", "ART", "ADJA", "TRUNC"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.13": {"text": "terscheid", "tokens": ["ter\u00b7scheid"], "token_info": ["word"], "pos": ["NE"], "meter": "-+", "measure": "iambic.single"}, "line.14": {"text": "Ist an ihrem Fleisch zu finden. An Geschmack und Nied-", "tokens": ["Ist", "an", "ih\u00b7rem", "Fleisch", "zu", "fin\u00b7den", ".", "An", "Ge\u00b7schmack", "und", "Nie\u00b7d"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$.", "APPR", "NN", "KON", "TRUNC"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.15": {"text": "lichkeit", "tokens": ["lich\u00b7keit"], "token_info": ["word"], "pos": ["NN"], "meter": "-+", "measure": "iambic.single"}, "line.16": {"text": "K\u00f6mmet der gez\u00e4hmten Fleisch nie dem Fleisch der wil-", "tokens": ["K\u00f6m\u00b7met", "der", "ge\u00b7z\u00e4hm\u00b7ten", "Fleisch", "nie", "dem", "Fleisch", "der", "wil"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "ART", "ADJA", "NN", "ADV", "ART", "NN", "ART", "TRUNC"], "meter": "+---+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.17": {"text": "den bey.", "tokens": ["den", "bey", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "PTKVZ", "$."], "meter": "-+", "measure": "iambic.single"}, "line.18": {"text": "Von den wilden wissen K\u00f6che, wenn sie sie kunstm\u00e4\u00dfig", "tokens": ["Von", "den", "wil\u00b7den", "wis\u00b7sen", "K\u00f6\u00b7che", ",", "wenn", "sie", "sie", "kunst\u00b7m\u00e4\u00b7\u00dfig"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "$,", "KOUS", "PPER", "PPER", "ADJD"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.19": {"text": "mischen,", "tokens": ["mi\u00b7schen", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.20": {"text": "Uns so mancherley Gerichte, die sehr niedlich, aufzuti-", "tokens": ["Uns", "so", "man\u00b7cher\u00b7ley", "Ge\u00b7rich\u00b7te", ",", "die", "sehr", "nied\u00b7lich", ",", "auf\u00b7zu\u00b7ti"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["PPER", "ADV", "PIAT", "NN", "$,", "PRELS", "ADV", "ADJD", "$,", "TRUNC"], "meter": "+-+-+-+-+-+-+--", "measure": "unknown.measure.septa"}, "line.21": {"text": "schen,", "tokens": ["schen", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-", "measure": "single.down"}, "line.22": {"text": "Und von ihrem Fell die K\u00fcrschner uns im Frost verschied-", "tokens": ["Und", "von", "ih\u00b7rem", "Fell", "die", "K\u00fcrschner", "uns", "im", "Frost", "ver\u00b7schie\u00b7d"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN", "ART", "NN", "PPER", "APPRART", "NN", "TRUNC"], "meter": "--+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.23": {"text": "ne Sachen", "tokens": ["ne", "Sa\u00b7chen"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.24": {"text": "So zur W\u00e4rm\u2019 als auch zur Zierde, zur Bequemlichkeit", "tokens": ["So", "zur", "W\u00e4rm'", "als", "auch", "zur", "Zier\u00b7de", ",", "zur", "Be\u00b7quem\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "APPRART", "NN", "KOKOM", "ADV", "APPRART", "NN", "$,", "APPRART", "NN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.25": {"text": "zu machen.", "tokens": ["zu", "ma\u00b7chen", "."], "token_info": ["word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.26": {"text": "Da\u00df demnach auch dieses Thier, wie auch Gott in ihm,", "tokens": ["Da\u00df", "dem\u00b7nach", "auch", "die\u00b7ses", "Thier", ",", "wie", "auch", "Gott", "in", "ihm", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PAV", "ADV", "PDAT", "NN", "$,", "PWAV", "ADV", "NN", "APPR", "PPER", "$,"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.27": {"text": "uns liebt,", "tokens": ["uns", "liebt", ","], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.28": {"text": "Da es uns erw\u00e4rmt und n\u00e4hret, eine klare Probe giebt.", "tokens": ["Da", "es", "uns", "er\u00b7w\u00e4rmt", "und", "n\u00e4h\u00b7ret", ",", "ei\u00b7ne", "kla\u00b7re", "Pro\u00b7be", "giebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVPP", "KON", "VVFIN", "$,", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}}}}