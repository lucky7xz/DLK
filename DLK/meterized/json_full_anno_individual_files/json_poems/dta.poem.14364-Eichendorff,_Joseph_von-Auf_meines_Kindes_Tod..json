{"dta.poem.14364": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Auf meines Kindes Tod.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Und Jahre nah'n und geh'n,", "tokens": ["Und", "Jah\u00b7re", "nah'n", "und", "geh'n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADJD", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Wie bald bin ich verstoben \u2014", "tokens": ["Wie", "bald", "bin", "ich", "ver\u00b7sto\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VAFIN", "PPER", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "O bitt' f\u00fcr mich da droben,", "tokens": ["O", "bitt'", "f\u00fcr", "mich", "da", "dro\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPR", "PPER", "ADV", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df wir uns wiederseh'n!", "tokens": ["Da\u00df", "wir", "uns", "wie\u00b7der\u00b7seh'n", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}