{"textgrid.poem.53765": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Anatole France in Pantoffeln", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Frau France: \u203aDer Mann ist da von der ", "tokens": ["Frau", "Fran\u00b7ce", ":", "\u203a", "Der", "Mann", "ist", "da", "von", "der"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "NE", "$.", "$(", "ART", "NN", "VAFIN", "ADV", "APPR", "ART"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Herr France: \u203aWas will der Mann von der", "tokens": ["Herr", "Fran\u00b7ce", ":", "\u203a", "Was", "will", "der", "Mann", "von", "der"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "NE", "$.", "$(", "PWS", "VMFIN", "ART", "NN", "APPR", "ART"], "meter": "+---+-+--", "measure": "dactylic.init"}, "line.3": {"text": "\u203aer will den Artikel. Er mu\u00df heute abend noch nach Wien weg, sonst kommt er nicht rechtzeitig an.\u2039", "tokens": ["\u203a", "er", "will", "den", "Ar\u00b7ti\u00b7kel", ".", "Er", "mu\u00df", "heu\u00b7te", "a\u00b7bend", "noch", "nach", "Wi\u00b7en", "weg", ",", "sonst", "kommt", "er", "nicht", "recht\u00b7zei\u00b7tig", "an", ".", "\u2039"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VMFIN", "ART", "NN", "$.", "PPER", "VMFIN", "ADV", "ADV", "ADV", "APPR", "NE", "PTKVZ", "$,", "ADV", "VVFIN", "PPER", "PTKNEG", "ADJD", "PTKVZ", "$.", "$("], "meter": "-+-+--+---+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.4": {"text": "\u203aheute abend noch? Ja, wer h\u00e4lt ihn denn zur\u00fcck?\u2039", "tokens": ["\u203a", "heu\u00b7te", "a\u00b7bend", "noch", "?", "Ja", ",", "wer", "h\u00e4lt", "ihn", "denn", "zu\u00b7r\u00fcck", "?", "\u2039"], "token_info": ["punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "ADV", "ADV", "$.", "PTKANT", "$,", "PWS", "VVFIN", "PPER", "ADV", "PTKVZ", "$.", "$("], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "\u203adu. Ich habe dich schon ein paarmal daran erinnert, aber du h\u00f6rst ja nicht auf mich.\u2039", "tokens": ["\u203a", "du", ".", "Ich", "ha\u00b7be", "dich", "schon", "ein", "paar\u00b7mal", "da\u00b7ran", "e\u00b7rin\u00b7nert", ",", "a\u00b7ber", "du", "h\u00f6rst", "ja", "nicht", "auf", "mich", ".", "\u2039"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "$.", "PPER", "VAFIN", "PPER", "ADV", "ART", "ADV", "PAV", "VVFIN", "$,", "KON", "PPER", "VVFIN", "ADV", "PTKNEG", "APPR", "PPER", "$.", "$("], "meter": "+-+-+-+--+-+--+--+-+-+", "measure": "trochaic.octa.plus.relaxed"}}, "stanza.2": {"line.1": {"text": "N\u00e4chste Szene. Die Vorigen. Fran\u00e7ois, der Kammerdiener.", "tokens": ["N\u00e4chs\u00b7te", "Sze\u00b7ne", ".", "Die", "Vo\u00b7ri\u00b7gen", ".", "Fran\u00e7ois", ",", "der", "Kam\u00b7mer\u00b7die\u00b7ner", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "ART", "NN", "$.", "NE", "$,", "ART", "NN", "$."], "meter": "+-+--+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "\u203agn\u00e4dige Frau, der Bote von der ", "tokens": ["\u203a", "gn\u00e4\u00b7di\u00b7ge", "Frau", ",", "der", "Bo\u00b7te", "von", "der"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "ADJA", "NN", "$,", "ART", "NN", "APPR", "ART"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "\u203ageben Sie ihm noch ein Glas Wein.\u2039", "tokens": ["\u203a", "ge\u00b7ben", "Sie", "ihm", "noch", "ein", "Glas", "Wein", ".", "\u2039"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "PPER", "PPER", "ADV", "ART", "NN", "NN", "$.", "$("], "meter": "+---+-++", "measure": "dactylic.init"}}, "stanza.3": {"line.1": {"text": "\u203aer soll noch ein Glas Wein . . . \u2039", "tokens": ["\u203a", "er", "soll", "noch", "ein", "Glas", "Wein", ".", ".", ".", "\u2039"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct", "punct"], "pos": ["$(", "PPER", "VMFIN", "ADV", "ART", "NN", "NN", "$.", "$.", "$.", "$("], "meter": "-+-+++", "measure": "unknown.measure.tetra"}, "line.2": {"text": "\u203agn\u00e4dige Frau, er hat schon die ganze Flasche ausgetrunken.\u2039", "tokens": ["\u203a", "gn\u00e4\u00b7di\u00b7ge", "Frau", ",", "er", "hat", "schon", "die", "gan\u00b7ze", "Fla\u00b7sche", "aus\u00b7ge\u00b7trun\u00b7ken", ".", "\u2039"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADJA", "NN", "$,", "PPER", "VAFIN", "ADV", "ART", "ADJA", "NN", "VVIZU", "$.", "$("], "meter": "+--+--+-+-+-+-+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "\u203awas war es f\u00fcr welcher? Wei\u00dfer oder roter?\u2039", "tokens": ["\u203a", "was", "war", "es", "f\u00fcr", "wel\u00b7cher", "?", "Wei\u00b7\u00dfer", "o\u00b7der", "ro\u00b7ter", "?", "\u2039"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PWS", "VAFIN", "PPER", "APPR", "PWAT", "$.", "NN", "KON", "ADJD", "$.", "$("], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": 