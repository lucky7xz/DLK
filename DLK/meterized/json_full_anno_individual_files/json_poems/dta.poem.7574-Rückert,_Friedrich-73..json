{"dta.poem.7574": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "73.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich habe, seit, o Freund, die G\u00f6tter uns verbanden,", "tokens": ["Ich", "ha\u00b7be", ",", "seit", ",", "o", "Freund", ",", "die", "G\u00f6t\u00b7ter", "uns", "ver\u00b7ban\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "KOUS", "$,", "FM", "NN", "$,", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nie deine Weise so, wie meine du, verstanden.", "tokens": ["Nie", "dei\u00b7ne", "Wei\u00b7se", "so", ",", "wie", "mei\u00b7ne", "du", ",", "ver\u00b7stan\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "ADV", "$,", "PWAV", "VVFIN", "PPER", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Du bist nach deiner Art ge\u00fcbt, an sich zu denken,", "tokens": ["Du", "bist", "nach", "dei\u00b7ner", "Art", "ge\u00b7\u00fcbt", ",", "an", "sich", "zu", "den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "NN", "VVPP", "$,", "APPR", "PRF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und ich, Gedanken nur in Bilder zu versenken.", "tokens": ["Und", "ich", ",", "Ge\u00b7dan\u00b7ken", "nur", "in", "Bil\u00b7der", "zu", "ver\u00b7sen\u00b7ken", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "NN", "ADV", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Du hast mir nach und nach geholfen aus dem Traum,", "tokens": ["Du", "hast", "mir", "nach", "und", "nach", "ge\u00b7hol\u00b7fen", "aus", "dem", "Traum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "APPR", "KON", "APPR", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Im innern auch zu schaun alswie im \u00e4u\u00dfern Raum.", "tokens": ["Im", "in\u00b7nern", "auch", "zu", "schaun", "als\u00b7wie", "im", "\u00e4u\u00b7\u00dfern", "Raum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "ADV", "PTKZU", "VVINF", "KOKOM", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Und manches, was ich sonst gethan, weil ich gemu\u00dft,", "tokens": ["Und", "man\u00b7ches", ",", "was", "ich", "sonst", "ge\u00b7than", ",", "weil", "ich", "ge\u00b7mu\u00dft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "$,", "PWS", "PPER", "ADV", "VVPP", "$,", "KOUS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Thu' ich mit h\u00f6herem Genusse nun bewu\u00dft.", "tokens": ["Thu'", "ich", "mit", "h\u00f6\u00b7he\u00b7rem", "Ge\u00b7nus\u00b7se", "nun", "be\u00b7wu\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "ADJA", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}