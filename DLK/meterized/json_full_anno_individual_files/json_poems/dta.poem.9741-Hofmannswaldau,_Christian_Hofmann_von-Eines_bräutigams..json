{"dta.poem.9741": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Eines br\u00e4utigams.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wje meiner hoffnung schiff gleich gl\u00fccklich solte landen/", "tokens": ["Wie", "mei\u00b7ner", "hoff\u00b7nung", "schiff", "gleich", "gl\u00fcck\u00b7lich", "sol\u00b7te", "lan\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "VVFIN", "ADV", "ADJD", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So must es bey dem port ohn alle h\u00fclffe stranden.", "tokens": ["So", "must", "es", "bey", "dem", "port", "ohn", "al\u00b7le", "h\u00fclf\u00b7fe", "stran\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "ART", "NN", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der tod nahms leben mir/ wie ich auff hochzeit dachte/", "tokens": ["Der", "tod", "nahms", "le\u00b7ben", "mir", "/", "wie", "ich", "auff", "hoch\u00b7zeit", "dach\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VVFIN", "PPER", "$(", "PWAV", "PPER", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "In lauter freuden schwamm/ und braut-caleuder machte.", "tokens": ["In", "lau\u00b7ter", "freu\u00b7den", "schwamm", "/", "und", "braut\u00b7ca\u00b7leu\u00b7der", "mach\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punc