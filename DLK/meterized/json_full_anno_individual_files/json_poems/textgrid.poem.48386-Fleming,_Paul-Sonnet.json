{"textgrid.poem.48386": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "Sonnet", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was uns den Himmel sperrt, die Welt zu enge macht,", "tokens": ["Was", "uns", "den", "Him\u00b7mel", "sperrt", ",", "die", "Welt", "zu", "en\u00b7ge", "macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ART", "NN", "VVFIN", "$,", "ART", "NN", "APPR", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "die lasse Seele zw\u00e4ngt, den kranken Leib verzehret,", "tokens": ["die", "las\u00b7se", "See\u00b7le", "zw\u00e4ngt", ",", "den", "kran\u00b7ken", "Leib", "ver\u00b7zeh\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "was uns bei Freuden Lust, bei Lachen Lachen wehret,", "tokens": ["was", "uns", "bei", "Freu\u00b7den", "Lust", ",", "bei", "La\u00b7chen", "La\u00b7chen", "weh\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "NN", "NN", "$,", "APPR", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "den langen Tag entf\u00e4rbt, erschreckt bei Mitternacht,", "tokens": ["den", "lan\u00b7gen", "Tag", "ent\u00b7f\u00e4rbt", ",", "er\u00b7schreckt", "bei", "Mit\u00b7ter\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "was mit uns geht zur Kost, steht, sitzt, entschl\u00e4ft, erwacht,", "tokens": ["was", "mit", "uns", "geht", "zur", "Kost", ",", "steht", ",", "sitzt", ",", "ent\u00b7schl\u00e4ft", ",", "er\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PWS", "APPR", "PPER", "VVFIN", "APPRART", "NN", "$,", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "das erste lange Leid, das Eva auf uns kehret,", "tokens": ["das", "ers\u00b7te", "lan\u00b7ge", "Leid", ",", "das", "E\u00b7va", "auf", "uns", "keh\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,", "ART", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "und was das arge Fleisch noch t\u00e4glich \u00fcben lehret,", "tokens": ["und", "was", "das", "ar\u00b7ge", "Fleisch", "noch", "t\u00e4g\u00b7lich", "\u00fc\u00b7ben", "leh\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "ADJA", "NN", "ADV", "ADJD", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "auch wie wir armes Volk zu Rechte werden bracht:", "tokens": ["auch", "wie", "wir", "ar\u00b7mes", "Volk", "zu", "Rech\u00b7te", "wer\u00b7den", "bracht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "PPER", "ADJA", "NN", "APPR", "NN", "VAINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "das klagt und lehrt di\u00df Buch. Wenn ihr denn, Ruhmb der Frauen,", "tokens": ["das", "klagt", "und", "lehrt", "di\u00df", "Buch", ".", "Wenn", "ihr", "denn", ",", "Ruhmb", "der", "Frau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "KON", "VVFIN", "PDS", "NN", "$.", "KOUS", "PPER", "ADV", "$,", "KOUI", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "das gn\u00e4dige Gesicht' in diese Schrift la\u00dft schauen,", "tokens": ["das", "gn\u00e4\u00b7di\u00b7ge", "Ge\u00b7sicht'", "in", "die\u00b7se", "Schrift", "la\u00dft", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PDAT", "NN", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "so hoffet euch nur nicht der W\u00f6rter sch\u00f6nen Schein!", "tokens": ["so", "hof\u00b7fet", "euch", "nur", "nicht", "der", "W\u00f6r\u00b7ter", "sch\u00f6\u00b7nen", "Schein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PTKNEG", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Denkt, Mutter, denkt viel mehr, da\u00df keine b\u00f6se Sache", "tokens": ["Denkt", ",", "Mut\u00b7ter", ",", "denkt", "viel", "mehr", ",", "da\u00df", "kei\u00b7ne", "b\u00f6\u00b7se", "Sa\u00b7che"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "NN", "$,", "VVFIN", "ADV", "ADV", "$,", "KOUS", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "der angeschminkte Glanz der Reden besser mache!", "tokens": ["der", "an\u00b7ge\u00b7schmink\u00b7te", "Glanz", "der", "Re\u00b7den", "bes\u00b7ser", "ma\u00b7che", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Der Richter siht hier nicht, was wir von au\u00dfen sein.", "tokens": ["Der", "Rich\u00b7ter", "siht", "hier", "nicht", ",", "was", "wir", "von", "au\u00b7\u00dfen", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "PTKNEG", "$,", "PRELS", "PPER", "APPR", "ADV", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Was uns den Himmel sperrt, die Welt zu enge macht,", "tokens": ["Was", "uns", "den", "Him\u00b7mel", "sperrt", ",", "die", "Welt", "zu", "en\u00b7ge", "macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ART", "NN", "VVFIN", "$,", "ART", "NN", "APPR", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "die lasse Seele zw\u00e4ngt, den kranken Leib verzehret,", "tokens": ["die", "las\u00b7se", "See\u00b7le", "zw\u00e4ngt", ",", "den", "kran\u00b7ken", "Leib", "ver\u00b7zeh\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "was uns bei Freuden Lust, bei Lachen Lachen wehret,", "tokens": ["was", "uns", "bei", "Freu\u00b7den", "Lust", ",", "bei", "La\u00b7chen", "La\u00b7chen", "weh\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", 