{"dta.poem.20898": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Der  \n Sterbende  \n  Socrates.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Der glaubet/ da\u00df die Welt sich einer Kugel gleichet/", "tokens": ["Der", "glau\u00b7bet", "/", "da\u00df", "die", "Welt", "sich", "ei\u00b7ner", "Ku\u00b7gel", "glei\u00b7chet", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$(", "KOUS", "ART", "NN", "PRF", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So frey von aller Ruh/ stets in die Runde streichet/", "tokens": ["So", "frey", "von", "al\u00b7ler", "Ruh", "/", "stets", "in", "die", "Run\u00b7de", "strei\u00b7chet", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "PIAT", "NN", "$(", "ADV", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und unterm Himmel ihr hat ihren Ort erkiest;", "tokens": ["Und", "un\u00b7term", "Him\u00b7mel", "ihr", "hat", "ih\u00b7ren", "Ort", "er\u00b7kiest", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "PPER", "VAFIN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ein ander so sie uns wie einen Trog beschreibet/", "tokens": ["Ein", "an\u00b7der", "so", "sie", "uns", "wie", "ei\u00b7nen", "Trog", "be\u00b7schrei\u00b7bet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADV", "PPER", "PRF", "KOKOM", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der fu\u00dft auf einen Wahn der nicht viel besser ist/", "tokens": ["Der", "fu\u00dft", "auf", "ei\u00b7nen", "Wahn", "der", "nicht", "viel", "bes\u00b7ser", "ist", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "ART", "NN", "ART", "PTKNEG", "ADV", "ADJD", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da\u00df sie auf freyer Lufft als einem Grunde bleibet.", "tokens": ["Da\u00df", "sie", "auf", "frey\u00b7er", "Lufft", "als", "ei\u00b7nem", "Grun\u00b7de", "blei\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "KOKOM", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}