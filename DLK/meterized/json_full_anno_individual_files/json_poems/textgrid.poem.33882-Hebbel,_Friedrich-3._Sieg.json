{"textgrid.poem.33882": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "3. Sieg", "genre": "verse", "period": "N.A.", "pub_year": 1856, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zum ersten Male ist sie heut' gegangen", "tokens": ["Zum", "ers\u00b7ten", "Ma\u00b7le", "ist", "sie", "heut'", "ge\u00b7gan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "VAFIN", "PPER", "ADV", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als junge Christin zum Altar des Herrn;", "tokens": ["Als", "jun\u00b7ge", "Chris\u00b7tin", "zum", "Al\u00b7tar", "des", "Herrn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die dunklen Worte, die vorher erklangen,", "tokens": ["Die", "dunk\u00b7len", "Wor\u00b7te", ",", "die", "vor\u00b7her", "er\u00b7klan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sie hielten ihr die ganze Erde fern;", "tokens": ["Sie", "hiel\u00b7ten", "ihr", "die", "gan\u00b7ze", "Er\u00b7de", "fern", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ein Todesschauer bleichte ihre Wangen", "tokens": ["Ein", "To\u00b7des\u00b7schau\u00b7er", "bleich\u00b7te", "ih\u00b7re", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und fast verglimmte ihres Auges Stern,", "tokens": ["Und", "fast", "ver\u00b7glimm\u00b7te", "ih\u00b7res", "Au\u00b7ges", "Stern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Denn, wer nicht w\u00fcrdig i\u00dft und trinkt, so spricht", "tokens": ["Denn", ",", "wer", "nicht", "w\u00fcr\u00b7dig", "i\u00dft", "und", "trinkt", ",", "so", "spricht"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "$,", "PWS", "PTKNEG", "ADJD", "VVFIN", "KON", "VVFIN", "$,", "ADV", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Gott selbst, der i\u00dft und trinkt sich das Gericht.", "tokens": ["Gott", "selbst", ",", "der", "i\u00dft", "und", "trinkt", "sich", "das", "Ge\u00b7richt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "PRELS", "VVFIN", "KON", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und dennoch hat sie heut' sich mir ergeben,", "tokens": ["Und", "den\u00b7noch", "hat", "sie", "heut'", "sich", "mir", "er\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ADV", "PRF", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wo jegliche Empfindung ihr's verbot;", "tokens": ["Wo", "jeg\u00b7li\u00b7che", "Emp\u00b7fin\u00b7dung", "ih\u00b7r's", "ver\u00b7bot", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "NE", "VVFIN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Sie wagte einmal, ihren Blick zu heben,", "tokens": ["Sie", "wag\u00b7te", "ein\u00b7mal", ",", "ih\u00b7ren", "Blick", "zu", "he\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da sah sie mich und wurde wieder roth;", "tokens": ["Da", "sah", "sie", "mich", "und", "wur\u00b7de", "wie\u00b7der", "roth", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "KON", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Nun nahte sie sich dem Altar mit Beben", "tokens": ["Nun", "nah\u00b7te", "sie", "sich", "dem", "Al\u00b7tar", "mit", "Be\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ART", "NN", "APPR", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Und nahm nur noch mit Angst das heil'ge Brot,", "tokens": ["Und", "nahm", "nur", "noch", "mit", "Angst", "das", "heil'\u00b7ge", "Brot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "APPR", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und als sie auch versch\u00fcttete den Wein,", "tokens": ["Und", "als", "sie", "auch", "ver\u00b7sch\u00fct\u00b7te\u00b7te", "den", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Da jauchzte ich: sie ist auf ewig mein!", "tokens": ["Da", "jauchz\u00b7te", "ich", ":", "sie", "ist", "auf", "e\u00b7wig", "mein", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "PPER", "VAFIN", "APPR", "ADJD", "PPOSAT", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Zum ersten Male ist sie heut' gegangen", "tokens": ["Zum", "ers\u00b7ten", "Ma\u00b7le", "ist", "sie", "heut'", "ge\u00b7gan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "VAFIN", "PPER", "ADV", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als junge Christin zum Altar des Herrn;", "tokens": ["Als", "jun\u00b7ge", "Chris\u00b7tin", "zum", "Al\u00b7tar", "des", "Herrn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die dunklen Worte, die vorher erklangen,", "tokens": ["Die", "dunk\u00b7len", "Wor\u00b7te", ",", "die", "vor\u00b7her", "er\u00b7klan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sie hielten ihr die ganze Erde fern;", "tokens": ["Sie", "hiel\u00b7ten", "ihr", "die", "gan\u00b7ze", "Er\u00b7de", "fern", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ein Todesschauer bleichte ihre Wangen", "tokens": ["Ein", "To\u00b7des\u00b7schau\u00b7er", "bleich\u00b7te", "ih\u00b7re", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "i