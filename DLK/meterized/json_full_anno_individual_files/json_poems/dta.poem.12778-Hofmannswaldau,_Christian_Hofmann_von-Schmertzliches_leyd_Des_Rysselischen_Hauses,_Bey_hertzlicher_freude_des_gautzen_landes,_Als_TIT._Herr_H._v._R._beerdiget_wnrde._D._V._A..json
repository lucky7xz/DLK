{"dta.poem.12778": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Schmertzliches leyd  \n Des Rysselischen Hauses,  \n Bey hertzlicher freude des gautzen landes,  \n Als  TIT.  Herr H. v. R.  \n  beerdiget wnrde.  \n  D.  V. A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was ist geschehn, wie sieht ihr werthes hau\u00df", "tokens": ["Was", "ist", "ge\u00b7schehn", ",", "wie", "sieht", "ihr", "wert\u00b7hes", "hau\u00df"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "VVPP", "$,", "PWAV", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ach leyder! itzund aus?", "tokens": ["Ach", "ley\u00b7der", "!", "it\u00b7zund", "aus", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "ADV", "$.", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Was macht allhier die menge der eypressen?", "tokens": ["Was", "macht", "all\u00b7hier", "die", "men\u00b7ge", "der", "ey\u00b7pres\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "ART", "NN", "ART", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Der Sachse triumphirt,", "tokens": ["Der", "Sach\u00b7se", "tri\u00b7um\u00b7phirt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und weil es sich geb\u00fchrt,", "tokens": ["Und", "weil", "es", "sich", "ge\u00b7b\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PRF", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Mu\u00df Leipzig itzt der palmen nicht vergessett.", "tokens": ["Mu\u00df", "Leip\u00b7zig", "itzt", "der", "pal\u00b7men", "nicht", "ver\u00b7ges\u00b7sett", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NE", "ADV", "ART", "NN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+---", "measure": "unknown.measure.tetra"}}, "stanza.2": {"line.1": {"text": "Ein ieder rufft: Du Landes-Vater, du!", "tokens": ["Ein", "ie\u00b7der", "rufft", ":", "Du", "Lan\u00b7des\u00b7Va\u00b7ter", ",", "du", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PIAT", "NN", "$.", "PPER", "NN", "$,", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Victorie! gl\u00fcck zu!", "tokens": ["Vic\u00b7to\u00b7rie", "!", "gl\u00fcck", "zu", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$.", "ADJD", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Die raut ist dir zum lorbeer-krantze worden,", "tokens": ["Die", "raut", "ist", "dir", "zum", "lor\u00b7beer\u00b7krant\u00b7ze", "wor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VAFIN", "PPER", "APPRART", "NN", "VAPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein ieder schm\u00fcckt sein hau\u00df", "tokens": ["Ein", "ie\u00b7der", "schm\u00fcckt", "sein", "hau\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PIS", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mit sieges-palmen aus,", "tokens": ["Mit", "sie\u00b7ges\u00b7pal\u00b7men", "aus", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Weil du vertreibst der T\u00fcrcken raub und morden.", "tokens": ["Weil", "du", "ver\u00b7treibst", "der", "T\u00fcr\u00b7cken", "raub", "und", "mor\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ART", "NN", "PTKVZ", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wie stellst du dann, du Edles Eltern-Paar!", "tokens": ["Wie", "stellst", "du", "dann", ",", "du", "Ed\u00b7les", "El\u00b7tern\u00b7Paar", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Dich itzt so traurig dar?", "tokens": ["Dich", "itzt", "so", "trau\u00b7rig", "dar", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Was weinest du, wenn andre fr\u00f6lich singen?", "tokens": ["Was", "wei\u00b7nest", "du", ",", "wenn", "and\u00b7re", "fr\u00f6\u00b7lich", "sin\u00b7gen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "KOUS", "PIS", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wirff die cypressen hin,", "tokens": ["Wirff", "die", "cyp\u00b7res\u00b7sen", "hin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Die palmen sind noch gr\u00fcn,", "tokens": ["Die", "pal\u00b7men", "sind", "noch", "gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Du kanst sie itzt dem Sachsen-Helden bringen.", "tokens": ["Du", "kanst", "sie", "itzt", "dem", "Sach\u00b7sen\u00b7Hel\u00b7den", "brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ach aber! ach! wenn T\u00fcrck und Tartar weicht,", "tokens": ["Ach", "a\u00b7ber", "!", "ach", "!", "wenn", "T\u00fcrck", "und", "Tar\u00b7tar", "weicht", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "$.", "XY", "$.", "KOUS", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "So k\u00f6mmt der tod und schleicht", "tokens": ["So", "k\u00f6mmt", "der", "tod", "und", "schleicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "KON", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In euer hau\u00df, und raubt das allerbeste;", "tokens": ["In", "eu\u00b7er", "hau\u00df", ",", "und", "raubt", "das", "al\u00b7ler\u00b7bes\u00b7te", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "KON", "VVFIN", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der allerliebste Sohn,", "tokens": ["Der", "al\u00b7ler\u00b7liebs\u00b7te", "Sohn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mu\u00df mit ihm selbst davon,", "tokens": ["Mu\u00df", "mit", "ihm", "selbst", "da\u00b7von", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "PPER", "ADV", "PAV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Und was noch mehr, kein ander bleibt im reste.", "tokens": ["Und", "was", "noch", "mehr", ",", "kein", "an\u00b7der", "bleibt", "im", "res\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "ADV", "$,", "PIAT", "ADJD", "VVFIN", "APPRART", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "O ungel\u00fcck! o schaden und verlust!", "tokens": ["O", "un\u00b7ge\u00b7l\u00fcck", "!", "o", "scha\u00b7den", "und", "ver\u00b7lust", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$.", "FM", "ADJA", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "O wunden einer brust,", "tokens": ["O", "wun\u00b7den", "ei\u00b7ner", "brust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die euch der tod, durch seinen tod, geschlagen!", "tokens": ["Die", "euch", "der", "tod", ",", "durch", "sei\u00b7nen", "tod", ",", "ge\u00b7schla\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "$,", "APPR", "PPOSAT", "NN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der alters-stab zubricht,", "tokens": ["Der", "al\u00b7ter\u00b7sstab", "zu\u00b7bricht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und des geschlechtes licht", "tokens": ["Und", "des", "ge\u00b7schlech\u00b7tes", "licht"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Wird ausgel\u00f6scht, und in die grufft getragen.", "tokens": ["Wird", "aus\u00b7ge\u00b7l\u00f6scht", ",", "und", "in", "die", "grufft", "ge\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "$,", "KON", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Es bleibt darbey, da\u00df eltern traurig sind,", "tokens": ["Es", "bleibt", "dar\u00b7bey", ",", "da\u00df", "el\u00b7tern", "trau\u00b7rig", "sind", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "$,", "KOUS", "NN", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wenn auch ein kleines kind", "tokens": ["Wenn", "auch", "ein", "klei\u00b7nes", "kind"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mu\u00df in der wieg und an der brust verderben.", "tokens": ["Mu\u00df", "in", "der", "wieg", "und", "an", "der", "brust", "ver\u00b7der\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "ART", "NN", "KON", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ach aber! was vor schmertz", "tokens": ["Ach", "a\u00b7ber", "!", "was", "vor", "schmertz"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "ADV", "$.", "PWS", "APPR", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Trifft denn der eltern hertz,", "tokens": ["Trifft", "denn", "der", "el\u00b7tern", "hertz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Wenn, wie allhier, ein gro\u00dfer sohn mu\u00df sterben.", "tokens": ["Wenn", ",", "wie", "all\u00b7hier", ",", "ein", "gro\u00b7\u00dfer", "sohn", "mu\u00df", "ster\u00b7ben", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "PWAV", "ADV", "$,", "ART", "ADJA", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Er war ein baum, da man nach voller bl\u00fct", "tokens": ["Er", "war", "ein", "baum", ",", "da", "man", "nach", "vol\u00b7ler", "bl\u00fct"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "KOUS", "PIS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Viel sch\u00f6ne fr\u00fcchte sieht,", "tokens": ["Viel", "sch\u00f6\u00b7ne", "fr\u00fcch\u00b7te", "sieht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und allbereits kan auf den nutzen dencken.", "tokens": ["Und", "all\u00b7be\u00b7reits", "kan", "auf", "den", "nut\u00b7zen", "den\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Er war ein reiches schiff,", "tokens": ["Er", "war", "ein", "rei\u00b7ches", "schiff", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Das schon in hafen lief;", "tokens": ["Das", "schon", "in", "ha\u00b7fen", "lief", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Was k\u00f6nte mehr die frommen eltern kr\u00e4ncken?", "tokens": ["Was", "k\u00f6n\u00b7te", "mehr", "die", "from\u00b7men", "el\u00b7tern", "kr\u00e4n\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ADV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Doch nur getrost! wo dient allhier ein ort,", "tokens": ["Doch", "nur", "ge\u00b7trost", "!", "wo", "dient", "all\u00b7hier", "ein", "ort", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVPP", "$.", "PWAV", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Zu einem sichern port?", "tokens": ["Zu", "ei\u00b7nem", "si\u00b7chern", "port", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Schaut \u00fcber euch, da seht ihr ihn anl\u00e4nden,", "tokens": ["Schaut", "\u00fc\u00b7ber", "euch", ",", "da", "seht", "ihr", "ihn", "an\u00b7l\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "$,", "ADV", "VVFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dort bl\u00fcht er f\u00fcr und f\u00fcr,", "tokens": ["Dort", "bl\u00fcht", "er", "f\u00fcr", "und", "f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "KON", "APPR", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und seiner fr\u00fcchte zier", "tokens": ["Und", "sei\u00b7ner", "fr\u00fcch\u00b7te", "zier"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Bleibt wohl verwahrt in JEsus treuen h\u00e4nden.", "tokens": ["Bleibt", "wohl", "ver\u00b7wahrt", "in", "Je\u00b7sus", "treu\u00b7en", "h\u00e4n\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "VVPP", "APPR", "NE", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Er triumphirt, nachdem er selber siegt,", "tokens": ["Er", "tri\u00b7um\u00b7phirt", ",", "nach\u00b7dem", "er", "sel\u00b7ber", "siegt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und die, so ihn bekriegt,", "tokens": ["Und", "die", ",", "so", "ihn", "be\u00b7kriegt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "$,", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Tod, s\u00fcnd und welt hat in die flucht geschlagen;", "tokens": ["Tod", ",", "s\u00fcnd", "und", "welt", "hat", "in", "die", "flucht", "ge\u00b7schla\u00b7gen", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "KON", "NN", "VAFIN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Er f\u00fchrt ein sieges-pfand", "tokens": ["Er", "f\u00fchrt", "ein", "sie\u00b7ges\u00b7pfand"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Von palmen in der hand;", "tokens": ["Von", "pal\u00b7men", "in", "der", "hand", ";"], "to