{"textgrid.poem.61427": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Der \u00c4ther", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.85", "en:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hoher \u00c4ther, hoher \u00c4ther,", "tokens": ["Ho\u00b7her", "\u00c4\u00b7ther", ",", "ho\u00b7her", "\u00c4\u00b7ther", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gestern sonnig, heut mit sanften", "tokens": ["Ge\u00b7stern", "son\u00b7nig", ",", "heut", "mit", "sanf\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ADJD", "$,", "ADV", "APPR", "ADJA"], "meter": "-++-+-+-", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Schatten meine Schl\u00e4fe k\u00fchlend,", "tokens": ["Schat\u00b7ten", "mei\u00b7ne", "Schl\u00e4\u00b7fe", "k\u00fch\u00b7lend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "O wie preis' ich deine Wunder!", "tokens": ["O", "wie", "preis'", "ich", "dei\u00b7ne", "Wun\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie ein Vater ruhig-heiter", "tokens": ["Wie", "ein", "Va\u00b7ter", "ru\u00b7hig\u00b7hei\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Tr\u00e4gst am Busen du den Erdkreis,", "tokens": ["Tr\u00e4gst", "am", "Bu\u00b7sen", "du", "den", "Erd\u00b7kreis", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und er l\u00e4chelt dir und l\u00e4\u00dft dich", "tokens": ["Und", "er", "l\u00e4\u00b7chelt", "dir", "und", "l\u00e4\u00dft", "dich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPER", "KON", "VVFIN", "PPER"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.8": {"text": "Seines Wesens Duft und Bl\u00fcte,", "tokens": ["Sei\u00b7nes", "We\u00b7sens", "Duft", "und", "Bl\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Seine ganze Sch\u00f6nheit saugen;", "tokens": ["Sei\u00b7ne", "gan\u00b7ze", "Sch\u00f6n\u00b7heit", "sau\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Denn die hohen Berge atmen", "tokens": ["Denn", "die", "ho\u00b7hen", "Ber\u00b7ge", "at\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Zu dir auf, die W\u00e4lder streun dir", "tokens": ["Zu", "dir", "auf", ",", "die", "W\u00e4l\u00b7der", "streun", "dir"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "PTKVZ", "$,", "ART", "NN", "VVFIN", "PPER"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.12": {"text": "Rauschend ihren besten Weihrauch,", "tokens": ["Rau\u00b7schend", "ih\u00b7ren", "bes\u00b7ten", "Weih\u00b7rauch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Tal und Flu\u00df und Quelle dampfen", "tokens": ["Tal", "und", "Flu\u00df", "und", "Quel\u00b7le", "damp\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "KON", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Dir ihr t\u00e4glich Morgenopfer,", "tokens": ["Dir", "ihr", "t\u00e4g\u00b7lich", "Mor\u00b7ge\u00b7nop\u00b7fer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPER", "ADJD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Und die Menschen \u2013 gleich als z\u00f6g' es", "tokens": ["Und", "die", "Men\u00b7schen", "\u2013", "gleich", "als", "z\u00f6g'", "es"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "$(", "ADV", "KOKOM", "VVFIN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Ewig sie zu deiner Stille \u2013", "tokens": ["E\u00b7wig", "sie", "zu", "dei\u00b7ner", "Stil\u00b7le", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "APPR", "PPOSAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Senden dir zu jeder Stunde", "tokens": ["Sen\u00b7den", "dir", "zu", "je\u00b7der", "Stun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Ihrer Brust lebend'gen Odem,", "tokens": ["Ih\u00b7rer", "Brust", "le\u00b7ben\u00b7d'\u00b7gen", "O\u00b7dem", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.19": {"text": "Ihre Lieder, ihre Seufzer.", "tokens": ["Ih\u00b7re", "Lie\u00b7der", ",", "ih\u00b7re", "Seuf\u00b7zer", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Und du nimmst die reichen Gaben", "tokens": ["Und", "du", "nimmst", "die", "rei\u00b7chen", "Ga\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Willig hin und sammelst alle;", "tokens": ["Wil\u00b7lig", "hin", "und", "sam\u00b7melst", "al\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "KON", "VVFIN", "PIS", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Aber nicht f\u00fcr dich \u2013 in Wolken", "tokens": ["A\u00b7ber", "nicht", "f\u00fcr", "dich", "\u2013", "in", "Wol\u00b7ken"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PTKNEG", "APPR", "PPER", "$(", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "Deine Stirn verh\u00fcllend wandelst", "tokens": ["Dei\u00b7ne", "Stirn", "ver\u00b7h\u00fcl\u00b7lend", "wan\u00b7delst"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVPP", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Du den Schatz in lautern Segen,", "tokens": ["Du", "den", "Schatz", "in", "lau\u00b7tern", "Se\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "Und in lichten Feuerflammen", "tokens": ["Und", "in", "lich\u00b7ten", "Feu\u00b7er\u00b7flam\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.26": {"text": "Und in Tropfen und in G\u00fcssen", "tokens": ["Und", "in", "Trop\u00b7fen", "und", "in", "G\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "KON", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.27": {"text": "Gibst du wonniglich befruchtend", "tokens": ["Gibst", "du", "won\u00b7nig\u00b7lich", "be\u00b7fruch\u00b7tend"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJD", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Ihn der durst'gen Erde wieder.", "tokens": ["Ihn", "der", "dur\u00b7st'\u00b7gen", "Er\u00b7de", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Hoher \u00c4ther, hoher \u00c4ther,", "tokens": ["Ho\u00b7her", "\u00c4\u00b7ther", ",", "ho\u00b7her", "\u00c4\u00b7ther", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie der Geist des Dichters bist du,", "tokens": ["Wie", "der", "Geist", "des", "Dich\u00b7ters", "bist", "du", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "VAFIN", "PPER", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Der, auf Fl\u00fcgeln \u00fcberm bunten", "tokens": ["Der", ",", "auf", "Fl\u00fc\u00b7geln", "\u00fc\u00b7berm", "bun\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ART", "$,", "APPR", "NN", "APPRART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Farbenspiel des Lebens schwebend,", "tokens": ["Far\u00b7ben\u00b7spiel", "des", "Le\u00b7bens", "schwe\u00b7bend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Seine Sch\u00f6nheit selig einsaugt.", "tokens": ["Sei\u00b7ne", "Sch\u00f6n\u00b7heit", "se\u00b7lig", "ein\u00b7saugt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "VVPP", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Und dann wogt's in ihm, dann w\u00f6lkt sich's", "tokens": ["Und", "dann", "wogt's", "in", "ihm", ",", "dann", "w\u00f6lkt", "sich's"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "APPR", "PPER", "$,", "ADV", "VVFIN", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wunderbar, er kann die F\u00fclle", "tokens": ["Wun\u00b7der\u00b7bar", ",", "er", "kann", "die", "F\u00fcl\u00b7le"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PPER", "VMFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Seiner Sch\u00e4tze nimmer halten,", "tokens": ["Sei\u00b7ner", "Sch\u00e4t\u00b7ze", "nim\u00b7mer", "hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Und wie du in Blitz und Regen", "tokens": ["Und", "wie", "du", "in", "Blitz", "und", "Re\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "APPR", "NN", "KON", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "Steigt er nieder im Gesang.", "tokens": ["Steigt", "er", "nie\u00b7der", "im", "Ge\u00b7sang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "t