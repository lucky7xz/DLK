{"textgrid.poem.34890": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Schelm von Bergen", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Schlo\u00df zu D\u00fcsseldorf am Rhein", "tokens": ["Im", "Schlo\u00df", "zu", "D\u00fcs\u00b7sel\u00b7dorf", "am", "Rhein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPR", "NE", "APPRART", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wird Mummenschanz gehalten;", "tokens": ["Wird", "Mum\u00b7men\u00b7schanz", "ge\u00b7hal\u00b7ten", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da flimmern die Kerzen, da rauscht die Musik,", "tokens": ["Da", "flim\u00b7mern", "die", "Ker\u00b7zen", ",", "da", "rauscht", "die", "Mu\u00b7sik", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Da tanzen die bunten Gestalten.", "tokens": ["Da", "tan\u00b7zen", "die", "bun\u00b7ten", "Ge\u00b7stal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "Da tanzt die sch\u00f6ne Herzogin,", "tokens": ["Da", "tanzt", "die", "sch\u00f6\u00b7ne", "Her\u00b7zo\u00b7gin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie lacht laut auf best\u00e4ndig;", "tokens": ["Sie", "lacht", "laut", "auf", "be\u00b7st\u00e4n\u00b7dig", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ihr T\u00e4nzer ist ein schlanker Fant,", "tokens": ["Ihr", "T\u00e4n\u00b7zer", "ist", "ein", "schlan\u00b7ker", "Fant", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gar h\u00f6fisch und behendig.", "tokens": ["Gar", "h\u00f6\u00b7fisch", "und", "be\u00b7hen\u00b7dig", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+--+--", "measure": "iambic.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Er tr\u00e4gt eine Maske von schwarzem Samt,", "tokens": ["Er", "tr\u00e4gt", "ei\u00b7ne", "Mas\u00b7ke", "von", "schwar\u00b7zem", "Samt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Daraus gar freudig blicket", "tokens": ["Da\u00b7raus", "gar", "freu\u00b7dig", "bli\u00b7cket"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "ADV", "ADJD", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein Auge, wie ein blanker Dolch,", "tokens": ["Ein", "Au\u00b7ge", ",", "wie", "ein", "blan\u00b7ker", "Dolch", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Halb aus der Scheide gez\u00fccket.", "tokens": ["Halb", "aus", "der", "Schei\u00b7de", "ge\u00b7z\u00fc\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVPP", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.4": {"line.1": {"text": "Es jubelt die Fastnachtsgeckenschar,", "tokens": ["Es", "ju\u00b7belt", "die", "Fast\u00b7nachts\u00b7ge\u00b7cken\u00b7schar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wenn jene vor\u00fcberwalzen.", "tokens": ["Wenn", "je\u00b7ne", "vor\u00b7\u00fc\u00b7berw\u00b7al\u00b7zen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Der Drickes und die Marizzebill", "tokens": ["Der", "Dri\u00b7ckes", "und", "die", "Ma\u00b7riz\u00b7ze\u00b7bill"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Gr\u00fc\u00dfen mit Schnarren und Schnalzen.", "tokens": ["Gr\u00fc\u00b7\u00dfen", "mit", "Schnar\u00b7ren", "und", "Schnal\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.5": {"line.1": {"text": "Und die Trompeten schmettern drein,", "tokens": ["Und", "die", "Trom\u00b7pe\u00b7ten", "schmet\u00b7tern", "drein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "--++-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Der n\u00e4rrische Brummba\u00df brummet,", "tokens": ["Der", "n\u00e4r\u00b7ri\u00b7sche", "Brumm\u00b7ba\u00df", "brum\u00b7met", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Bis endlich der Tanz ein Ende nimmt", "tokens": ["Bis", "end\u00b7lich", "der", "Tanz", "ein", "En\u00b7de", "nimmt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "ART", "NN", "ART", "NN", "VVFIN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und die Musik verstummet.", "tokens": ["Und", "die", "Mu\u00b7sik", "ver\u00b7stum\u00b7met", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.6": {"line.1": {"text": "\u00bbdurchlauchtigste Frau, gebt Urlaub mir,", "tokens": ["\u00bb", "durch\u00b7lauch\u00b7tigs\u00b7te", "Frau", ",", "gebt", "Ur\u00b7laub", "mir", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADJA", "NN", "$,", "VVFIN", "NN", "PPER", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ich mu\u00df nach Hause gehen \u2013\u00ab", "tokens": ["Ich", "mu\u00df", "nach", "Hau\u00b7se", "ge\u00b7hen", "\u2013", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VMFIN", "APPR", "NN", "VVINF", "$(", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Herzogin lacht: \u00bbIch la\u00df dich nicht fort,", "tokens": ["Die", "Her\u00b7zo\u00b7gin", "lacht", ":", "\u00bb", "Ich", "la\u00df", "dich", "nicht", "fort", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "$(", "PPER", "VVFIN", "PPER", "PTKNEG", "PTKVZ", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Bevor ich dein Antlitz gesehen.\u00ab", "tokens": ["Be\u00b7vor", "ich", "dein", "Ant\u00b7litz", "ge\u00b7se\u00b7hen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVPP", "$.", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.7": {"line.1": {"text": "\u00bbdurchlauchtigste Frau, gebt Urlaub mir,", "tokens": ["\u00bb", "durch\u00b7lauch\u00b7tigs\u00b7te", "Frau", ",", "gebt", "Ur\u00b7laub", "mir", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADJA", "NN", "$,", "VVFIN", "NN", "PPER", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mein Anblick bringt Schrecken und Grauen \u2013\u00ab", "tokens": ["Mein", "An\u00b7blick", "bringt", "Schre\u00b7cken", "und", "Grau\u00b7en", "\u2013", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "NN", "KON", "NN", "$(", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Die Herzogin lacht: \u00bbIch f\u00fcrchte mich nicht,", "tokens": ["Die", "Her\u00b7zo\u00b7gin", "lacht", ":", "\u00bb", "Ich", "f\u00fcrch\u00b7te", "mich", "nicht", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "$(", "PPER", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ich will dein Antlitz schauen.\u00ab", "tokens": ["Ich", "will", "dein", "Ant\u00b7litz", "schau\u00b7en", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "\u00bbdurchlauchtigste Frau, gebt Urlaub mir,", "tokens": ["\u00bb", "durch\u00b7lauch\u00b7tigs\u00b7te", "Frau", ",", "gebt", "Ur\u00b7laub", "mir", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADJA", "NN", "$,", "VVFIN", "NN", "PPER", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der Nacht und dem Tode geh\u00f6r ich \u2013\u00ab", "tokens": ["Der", "Nacht", "und", "dem", "To\u00b7de", "ge\u00b7h\u00f6r", "ich", "\u2013", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "VVFIN", "PPER", "$(", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Die Herzogin lacht: \u00bbIch lasse dich nicht,", "tokens": ["Die", "Her\u00b7zo\u00b7gin", "lacht", ":", "\u00bb", "Ich", "las\u00b7se", "dich", "nicht", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "$(", "PPER", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Dein Antlitz zu schauen begehr ich.\u00ab", "tokens": ["Dein", "Ant\u00b7litz", "zu", "schau\u00b7en", "be\u00b7gehr", "ich", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "VVFIN", "PPER", "$.", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.9": {"line.1": {"text": "Wohl str\u00e4ubt sich der Mann mit finsterm Wort,", "tokens": ["Wohl", "str\u00e4ubt", "sich", "der", "Mann", "mit", "fins\u00b7term", "Wort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Das Weib nicht z\u00e4hmen kunnt er;", "tokens": ["Das", "Weib", "nicht", "z\u00e4h\u00b7men", "kunnt", "er", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "VVINF", "VMFIN", "PPER", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie ri\u00df zuletzt ihm mit Gewalt", "tokens": ["Sie", "ri\u00df", "zu\u00b7letzt", "ihm", "mit", "Ge\u00b7walt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PPER", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Maske vom Antlitz herunter.", "tokens": ["Die", "Mas\u00b7ke", "vom", "Ant\u00b7litz", "her\u00b7un\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.10": {"line.1": {"text": "\u00bbdas ist der Scharfrichter von Bergen!\u00ab so schreit", "tokens": ["\u00bb", "das", "ist", "der", "Schar\u00b7frich\u00b7ter", "von", "Ber\u00b7gen", "!", "\u00ab", "so", "schreit"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word"], "pos": ["$(", "PDS", "VAFIN", "ART", "NN", "APPR", "NN", "$.", "$(", "ADV", "NN"], "meter": "-+-++--+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Entsetzt die Menge im Saale", "tokens": ["Ent\u00b7setzt", "die", "Men\u00b7ge", "im", "Saa\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "APPRART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und weichet scheusam \u2013 die Herzogin", "tokens": ["Und", "wei\u00b7chet", "scheu\u00b7sam", "\u2013", "die", "Her\u00b7zo\u00b7gin"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "$(", "ART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "St\u00fcrzt fort zu ihrem Gemahle.", "tokens": ["St\u00fcrzt", "fort", "zu", "ih\u00b7rem", "Ge\u00b7mah\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.11": {"line.1": {"text": "Der Herzog ist klug, er tilgte die Schmach", "tokens": ["Der", "Her\u00b7zog", "ist", "klug", ",", "er", "tilg\u00b7te", "die", "Schmach"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NE", "VAFIN", "ADJD", "$,", "PPER", "VVFIN", "ART", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der Gattin auf der Stelle.", "tokens": ["Der", "Gat\u00b7tin", "auf", "der", "Stel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er zog sein blankes Schwert und sprach:", "tokens": ["Er", "zog", "sein", "blan\u00b7kes", "Schwert", "und", "sprach", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u00bbknie vor mir nieder, Geselle!", "tokens": ["\u00bb", "knie", "vor", "mir", "nie\u00b7der", ",", "Ge\u00b7sel\u00b7le", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "NN", "APPR", "PPER", "PTKVZ", "$,", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.12": {"line.1": {"text": "Mit diesem Schwertschlag mach ich dich", "tokens": ["Mit", "die\u00b7sem", "Schwert\u00b7schlag", "mach", "ich", "dich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "PPER", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Jetzt ehrlich und ritterz\u00fcnftig,", "tokens": ["Jetzt", "ehr\u00b7lich", "und", "rit\u00b7ter\u00b7z\u00fcnf\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und weil du ein Schelm, so nenne dich", "tokens": ["Und", "weil", "du", "ein", "Schelm", ",", "so", "nen\u00b7ne", "dich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ART", "NN", "$,", "ADV", "VVFIN", "PPER"], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Herr Schelm von Bergen k\u00fcnftig.\u00ab", "tokens": ["Herr", "Schelm", "von", "Ber\u00b7gen", "k\u00fcnf\u00b7tig", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "NN", "APPR", "NN", "ADJD", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "So ward der Henker ein Edelmann", "tokens": ["So", "ward", "der", "Hen\u00b7ker", "ein", "E\u00b7del\u00b7mann"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und Ahnherr der Schelme von Bergen.", "tokens": ["Und", "Ahn\u00b7herr", "der", "Schel\u00b7me", "von", "Ber\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Ein stolzes Geschlecht! es bl\u00fchte am Rhein.", "tokens": ["Ein", "stol\u00b7zes", "Ge\u00b7schlecht", "!", "es", "bl\u00fch\u00b7te", "am", "Rhein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "PPER", "VVFIN", "APPRART", "NE", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Jetzt schl\u00e4ft es in steinernen S\u00e4rgen.", "tokens": ["Jetzt", "schl\u00e4ft", "es", "in", "stei\u00b7ner\u00b7nen", "S\u00e4r\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.14": {"line.1": {"text": "Im Schlo\u00df zu D\u00fcsseldorf am Rhein", "tokens": ["Im", "Schlo\u00df", "zu", "D\u00fcs\u00b7sel\u00b7dorf", "am", "Rhein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPR", "NE", "APPRART", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wird Mummenschanz gehalten;", "tokens": ["Wird", "Mum\u00b7men\u00b7schanz", "ge\u00b7hal\u00b7ten", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da flimmern die Kerzen, da rauscht die Musik,", "tokens": ["Da", "flim\u00b7mern", "die", "Ker\u00b7zen", ",", "da", "rauscht", "die", "Mu\u00b7sik", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Da tanzen die bunten Gestalten.", "tokens": ["Da", "tan\u00b7zen", "die", "bun\u00b7ten", "Ge\u00b7stal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.15": {"line.1": {"text": "Da tanzt die sch\u00f6ne Herzogin,", "tokens": ["Da", "tanzt", "die", "sch\u00f6\u00b7ne", "Her\u00b7zo\u00b7gin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie lacht laut auf best\u00e4ndig;", "tokens": ["Sie", "lacht", "laut", "auf", "be\u00b7st\u00e4n\u00b7dig", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.