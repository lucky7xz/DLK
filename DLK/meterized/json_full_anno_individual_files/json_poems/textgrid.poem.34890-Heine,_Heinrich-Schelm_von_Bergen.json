, "pos": ["KON", "KOUS", "PPER", "ART", "NN", "$,", "ADV", "VVFIN", "PPER"], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Herr Schelm von Bergen k\u00fcnftig.\u00ab", "tokens": ["Herr", "Schelm", "von", "Ber\u00b7gen", "k\u00fcnf\u00b7tig", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "NN", "APPR", "NN", "ADJD", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.26": {"line.1": {"text": "So ward der Henker ein Edelmann", "tokens": ["So", "ward", "der", "Hen\u00b7ker", "ein", "E\u00b7del\u00b7mann"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und Ahnherr der Schelme von Bergen.", "tokens": ["Und", "Ahn\u00b7herr", "der", "Schel\u00b7me", "von", "Ber\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Ein stolzes Geschlecht! es bl\u00fchte am Rhein.", "tokens": ["Ein", "stol\u00b7zes", "Ge\u00b7schlecht", "!", "es", "bl\u00fch\u00b7te", "am", "Rhein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "PPER", "VVFIN", "APPRART", "NE", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Jetzt schl\u00e4ft es in steinernen S\u00e4rgen.", "tokens": ["Jetzt", "schl\u00e4ft", "es", "in", "stei\u00b7ner\u00b7nen", "S\u00e4r\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}}}}