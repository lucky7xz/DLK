{"dta.poem.3083": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "128.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Gar viele Wege gehn zu Gott, auch deiner geht", "tokens": ["Gar", "vie\u00b7le", "We\u00b7ge", "gehn", "zu", "Gott", ",", "auch", "dei\u00b7ner", "geht"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "PIAT", "NN", "VVFIN", "APPR", "NN", "$,", "ADV", "PPOSAT", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu Gott, geh ihn getrost mit Preisen und Gebet.", "tokens": ["Zu", "Gott", ",", "geh", "ihn", "ge\u00b7trost", "mit", "Prei\u00b7sen", "und", "Ge\u00b7bet", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "VVFIN", "PPER", "VVPP", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Und la\u00df dich nicht darin von denen irre machen,", "tokens": ["Und", "la\u00df", "dich", "nicht", "da\u00b7rin", "von", "de\u00b7nen", "ir\u00b7re", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "PTKNEG", "PAV", "APPR", "PRELS", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die andre Wege gehn, und mach nicht irr die Schwachen.", "tokens": ["Die", "and\u00b7re", "We\u00b7ge", "gehn", ",", "und", "mach", "nicht", "irr", "die", "Schwa\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,", "KON", "VVFIN", "PTKNEG", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wer mit auf meinem Weg will gehn, der sei willkommen;", "tokens": ["Wer", "mit", "auf", "mei\u00b7nem", "Weg", "will", "gehn", ",", "der", "sei", "will\u00b7kom\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "APPR", "PPOSAT", "NN", "VMFIN", "VVINF", "$,", "PRELS", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und geh' ich auch allein, doch geh' ich unbeklommen.", "tokens": ["Und", "geh'", "ich", "auch", "al\u00b7lein", ",", "doch", "geh'", "ich", "un\u00b7be\u00b7klom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADV", "$,", "ADV", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}