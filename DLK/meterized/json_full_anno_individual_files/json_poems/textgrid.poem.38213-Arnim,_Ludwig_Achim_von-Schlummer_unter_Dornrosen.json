{"textgrid.poem.38213": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Schlummer unter Dornrosen", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich legte mich nieder ins gr\u00fcne Gras,", "tokens": ["Ich", "leg\u00b7te", "mich", "nie\u00b7der", "ins", "gr\u00fc\u00b7ne", "Gras", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Und lauert auf meinen Herzliebsten Schatz,", "tokens": ["Und", "lau\u00b7ert", "auf", "mei\u00b7nen", "Herz\u00b7liebs\u00b7ten", "Schatz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Ich lauert so lange bis mich es verdro\u00df,", "tokens": ["Ich", "lau\u00b7ert", "so", "lan\u00b7ge", "bis", "mich", "es", "ver\u00b7dro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "APPR", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Da fielen zwei R\u00f6selein mir in den Schoos.", "tokens": ["Da", "fie\u00b7len", "zwei", "R\u00f6\u00b7se\u00b7lein", "mir", "in", "den", "Schoos", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "CARD", "NN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Die R\u00f6selein, die waren wie Blut so roth,", "tokens": ["Die", "R\u00f6\u00b7se\u00b7lein", ",", "die", "wa\u00b7ren", "wie", "Blut", "so", "roth", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VAFIN", "KOKOM", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Jezt schl\u00e4ft ja mein Schatz oder er ist todt,", "tokens": ["Jezt", "schl\u00e4ft", "ja", "mein", "Schatz", "o\u00b7der", "er", "ist", "todt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPOSAT", "NN", "KON", "PPER", "VAFIN", "ADJD", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Er schl\u00e4ft ja nicht, er schlummert ja nur,", "tokens": ["Er", "schl\u00e4ft", "ja", "nicht", ",", "er", "schlum\u00b7mert", "ja", "nur", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKNEG", "$,", "PPER", "VVFIN", "ADV", "ADV", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Es blinken seine Aeuglein, es l\u00e4chelt sein Mund,", "tokens": ["Es", "blin\u00b7ken", "sei\u00b7ne", "A\u00b7e\u00b7u\u00b7glein", ",", "es", "l\u00e4\u00b7chelt", "sein", "Mund", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Da lie\u00df ich meine Augen herummer gehn,", "tokens": ["Da", "lie\u00df", "ich", "mei\u00b7ne", "Au\u00b7gen", "her\u00b7um\u00b7mer", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Da sah ich mein Sch\u00e4tzlein bei einem andern stehn,", "tokens": ["Da", "sah", "ich", "mein", "Sch\u00e4tz\u00b7lein", "bei", "ei\u00b7nem", "an\u00b7dern", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "APPR", "ART", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Bei einem andern stehn,", "tokens": ["Bei", "ei\u00b7nem", "an\u00b7dern", "stehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Das hab ich gesehn.", "tokens": ["Das", "hab", "ich", "ge\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Ich legte mich nieder ins gr\u00fcne Gras,", "tokens": ["Ich", "leg\u00b7te", "mich", "nie\u00b7der", "ins", "gr\u00fc\u00b7ne", "Gras", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Und lauert auf meinen Herzliebsten Schatz,", "tokens": ["Und", "lau\u00b7ert", "auf", "mei\u00b7nen", "Herz\u00b7liebs\u00b7ten", "Schatz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Ich lauert so lange bis mich es verdro\u00df,", "tokens": ["Ich", "lau\u00b7ert", "so", "lan\u00b7ge", "bis", "mich", "es", "ver\u00b7dro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "APPR", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+--+-+-+-+", "measure"