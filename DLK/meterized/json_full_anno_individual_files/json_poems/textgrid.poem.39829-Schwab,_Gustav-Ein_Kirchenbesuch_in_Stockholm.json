 ["Du", "zie\u00b7hest", "vor", "uns", "her", "im", "schlich\u00b7ten", "Rei\u00b7ter\u00b7kleid", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "ADV", "APPRART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.12": {"line.1": {"text": "Der Schatten winkt, entschwebt, und Nacht bedeckt den Stein.", "tokens": ["Der", "Schat\u00b7ten", "winkt", ",", "ent\u00b7schwebt", ",", "und", "Nacht", "be\u00b7deckt", "den", "Stein", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "VVFIN", "$,", "KON", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch drau\u00dfen ist es Tag, und wehen die Standarten.", "tokens": ["Doch", "drau\u00b7\u00dfen", "ist", "es", "Tag", ",", "und", "we\u00b7hen", "die", "Stand\u00b7ar\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "NN", "$,", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Hinaus, wo Br\u00fcder uns in Drang und Not erwarten!", "tokens": ["Hin\u00b7aus", ",", "wo", "Br\u00fc\u00b7der", "uns", "in", "Drang", "und", "Not", "er\u00b7war\u00b7ten", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "NN", "PPER", "APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und unser Feldgeschrei? ", "tokens": ["Und", "un\u00b7ser", "Feld\u00b7ge\u00b7schrei", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$."], "meter": "-+-+-