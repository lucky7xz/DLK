{"textgrid.poem.39829": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Ein Kirchenbesuch in Stockholm", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vom rauschenden Gespr\u00e4ch, von der Begeistrung Glut", "tokens": ["Vom", "rau\u00b7schen\u00b7den", "Ge\u00b7spr\u00e4ch", ",", "von", "der", "Be\u00b7geis\u00b7trung", "Glut"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "$,", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Auf, folgt in's K\u00fchle mir, zur feierlichen Stille,", "tokens": ["Auf", ",", "folgt", "in's", "K\u00fch\u00b7le", "mir", ",", "zur", "fei\u00b7er\u00b7li\u00b7chen", "Stil\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "VVFIN", "APPRART", "NN", "PPER", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wo fern, im ", "tokens": ["Wo", "fern", ",", "im"], "token_info": ["word", "word", "punct", "word"], "pos": ["PWAV", "ADJD", "$,", "APPRART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Der unser Banner tr\u00e4gt, l\u00e4ngst unterm Boden ruht.", "tokens": ["Der", "un\u00b7ser", "Ban\u00b7ner", "tr\u00e4gt", ",", "l\u00e4ngst", "un\u00b7term", "Bo\u00b7den", "ruht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "$,", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Im S\u00fcden d\u00e4mmert's bald; hier z\u00f6gert lang der Stral,", "tokens": ["Im", "S\u00fc\u00b7den", "d\u00e4m\u00b7mert's", "bald", ";", "hier", "z\u00f6\u00b7gert", "lang", "der", "Stral", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NE", "ADV", "$.", "ADV", "VVFIN", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Schwedensonne brennt mit ihrer n\u00e4cht'gen Helle,", "tokens": ["Die", "Schwe\u00b7den\u00b7son\u00b7ne", "brennt", "mit", "ih\u00b7rer", "n\u00e4cht'\u00b7gen", "Hel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sie senkt sich in des Doms verschwiegenste Kapelle,", "tokens": ["Sie", "senkt", "sich", "in", "des", "Doms", "ver\u00b7schwie\u00b7gens\u00b7te", "Ka\u00b7pel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Verkl\u00e4rt, die Wand entlang, Troph\u00e4en ohne Zahl.", "tokens": ["Ver\u00b7kl\u00e4rt", ",", "die", "Wand", "ent\u00b7lang", ",", "Trop\u00b7h\u00e4\u00b7en", "oh\u00b7ne", "Zahl", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "ART", "NN", "APPO", "$,", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Verbleichte Fahnen bl\u00fchn auf's Neu' in ihrem Gold,", "tokens": ["Ver\u00b7bleich\u00b7te", "Fah\u00b7nen", "bl\u00fchn", "auf's", "Neu'", "in", "ih\u00b7rem", "Gold", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "APPRART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und wie Geschmeide blinkt ein Strau\u00df von Lanzenspitzen,", "tokens": ["Und", "wie", "Ge\u00b7schmei\u00b7de", "blinkt", "ein", "Strau\u00df", "von", "Lan\u00b7zens\u00b7pit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "NN", "VVFIN", "ART", "NE", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Des K\u00f6nigs Schwerter spr\u00fchn, von Leipzig und von L\u00fctzen,", "tokens": ["Des", "K\u00f6\u00b7nigs", "Schwer\u00b7ter", "spr\u00fchn", ",", "von", "Leip\u00b7zig", "und", "von", "L\u00fct\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVINF", "$,", "APPR", "NE", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und Trommeln winken euch, drauf einst die Schlacht gerollt.", "tokens": ["Und", "Trom\u00b7meln", "win\u00b7ken", "euch", ",", "drauf", "einst", "die", "Schlacht", "ge\u00b7rollt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPER", "$,", "PAV", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Kein andres Mal; die Gruft h\u00fcllt glattes Estrich ein;", "tokens": ["Kein", "and\u00b7res", "Mal", ";", "die", "Gruft", "h\u00fcllt", "glat\u00b7tes", "Est\u00b7rich", "ein", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$.", "ART", "NN", "VVFIN", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der graue Marmor spricht, geborsten und zerschlissen,", "tokens": ["Der", "grau\u00b7e", "Mar\u00b7mor", "spricht", ",", "ge\u00b7bors\u00b7ten", "und", "zer\u00b7schlis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "ADJA", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mit kurzer Schrift euch an, aus seinen Bodenrissen", "tokens": ["Mit", "kur\u00b7zer", "Schrift", "euch", "an", ",", "aus", "sei\u00b7nen", "Bo\u00b7den\u00b7ris\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "PPER", "PTKVZ", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Steigt mahnend Staub empor und spielt im Abendschein.", "tokens": ["Steigt", "mah\u00b7nend", "Staub", "em\u00b7por", "und", "spielt", "im", "A\u00b7bend\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "NN", "PTKVZ", "KON", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "O schaut, o schauet hin! Die Sonne geht hinab,", "tokens": ["O", "schaut", ",", "o", "schau\u00b7et", "hin", "!", "Die", "Son\u00b7ne", "geht", "hin\u00b7ab", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "FM", "VVFIN", "PTKVZ", "$.", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch weilt die S\u00e4ule Staubs, aufsteigend aus den Platten;", "tokens": ["Doch", "weilt", "die", "S\u00e4u\u00b7le", "Staubs", ",", "auf\u00b7stei\u00b7gend", "aus", "den", "Plat\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "NE", "$,", "VVPP", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Seht, sie verdichtet sich, sie wird Gestalt und Schatten,", "tokens": ["Seht", ",", "sie", "ver\u00b7dich\u00b7tet", "sich", ",", "sie", "wird", "Ge\u00b7stalt", "und", "Schat\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPER", "VVFIN", "PRF", "$,", "PPER", "VAFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Leibhaftig steht der F\u00fcrst vor euch auf seinem Grab.", "tokens": ["Leib\u00b7haf\u00b7tig", "steht", "der", "F\u00fcrst", "vor", "euch", "auf", "sei\u00b7nem", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "APPR", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}}, "stanza.6": {"line.1": {"text": "Er ist's, der Glaubensheld; so ritterlich, so gro\u00df", "tokens": ["Er", "ist's", ",", "der", "Glau\u00b7bens\u00b7held", ";", "so", "rit\u00b7ter\u00b7lich", ",", "so", "gro\u00df"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "ART", "NN", "$.", "ADV", "ADJD", "$,", "ADV", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sah ich ihn j\u00fcngst von Stein in N\u00fcrnbergs Burghof lehnen,", "tokens": ["Sah", "ich", "ihn", "j\u00fcngst", "von", "Stein", "in", "N\u00fcrn\u00b7bergs", "Burg\u00b7hof", "leh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "APPR", "NN", "APPR", "NE", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Den Koller um die Brust; er streckt die nerv'gen Sehnen \u2013", "tokens": ["Den", "Kol\u00b7ler", "um", "die", "Brust", ";", "er", "streckt", "die", "ner\u00b7v'\u00b7gen", "Seh\u00b7nen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$.", "PPER", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Doch nur zum Handschlag \u2013 aus: denn er ist", "tokens": ["Doch", "nur", "zum", "Hand\u00b7schlag", "\u2013", "aus", ":", "denn", "er", "ist"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "APPRART", "NN", "$(", "PTKVZ", "$.", "KON", "PPER", "VAFIN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "Ja sanft erscheinest du, du L\u00f6w' aus Mitternacht,", "tokens": ["Ja", "sanft", "er\u00b7schei\u00b7nest", "du", ",", "du", "L\u00f6\u00b7w'", "aus", "Mit\u00b7ter\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADJD", "VVFIN", "PPER", "$,", "PPER", "NN", "APPR", "NN", "$,"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "K\u00f6nig im Goldgelock, entkleidet von dem Schrecken,", "tokens": ["K\u00f6\u00b7nig", "im", "Gold\u00b7ge\u00b7lock", ",", "ent\u00b7klei\u00b7det", "von", "dem", "Schre\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Der an den W\u00e4nden h\u00e4ngt; die Waffen, so dich decken,", "tokens": ["Der", "an", "den", "W\u00e4n\u00b7den", "h\u00e4ngt", ";", "die", "Waf\u00b7fen", ",", "so", "dich", "de\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "$.", "ART", "NN", "$,", "ADV", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+--+--", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Sind ganz Gerechtigkeit, ganz Wahrheit, Friedenstracht.", "tokens": ["Sind", "ganz", "Ge\u00b7rech\u00b7tig\u00b7keit", ",", "ganz", "Wahr\u00b7heit", ",", "Frie\u00b7dens\u00b7tracht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "ADV", "NN", "$,", "ADV", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Jahrhundertkl\u00fcfte weit liegt hinter dir der Krieg,", "tokens": ["Jahr\u00b7hun\u00b7dert\u00b7kl\u00fcf\u00b7te", "weit", "liegt", "hin\u00b7ter", "dir", "der", "Krieg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VVFIN", "APPR", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die unbewehrte Hand will unsre Sache st\u00fctzen;", "tokens": ["Die", "un\u00b7be\u00b7wehr\u00b7te", "Hand", "will", "uns\u00b7re", "Sa\u00b7che", "st\u00fct\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Kein Schwert, dein Herzblut nur bringst du uns mit von L\u00fctzen.", "tokens": ["Kein", "Schwert", ",", "dein", "Herz\u00b7blut", "nur", "bringst", "du", "uns", "mit", "von", "L\u00fct\u00b7zen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PPOSAT", "NN", "ADV", "VVFIN", "PPER", "PRF", "APPR", "APPR", "NN", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Das ist noch nicht verraucht, das b\u00fcrgt uns f\u00fcr den Sieg.", "tokens": ["Das", "ist", "noch", "nicht", "ver\u00b7raucht", ",", "das", "b\u00fcrgt", "uns", "f\u00fcr", "den", "Sieg", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "PTKNEG", "VVPP", "$,", "PDS", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Zum allerfr\u00f6mmsten Werk begleitet uns dein Geist:", "tokens": ["Zum", "al\u00b7ler\u00b7fr\u00b7\u00f6mms\u00b7ten", "Werk", "be\u00b7glei\u00b7tet", "uns", "dein", "Geist", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-++-+-+-+-+", "measure": "unknown.measure.septa"}, "line.2": {"text": "Dem Docht, der einsam glimmt, Oel zu der Flamme spenden,", "tokens": ["Dem", "Docht", ",", "der", "ein\u00b7sam", "glimmt", ",", "O\u00b7el", "zu", "der", "Flam\u00b7me", "spen\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$,", "NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Dem Glauben H\u00fctten baun, dem Glauben Lehrer senden,", "tokens": ["Dem", "Glau\u00b7ben", "H\u00fct\u00b7ten", "baun", ",", "dem", "Glau\u00b7ben", "Leh\u00b7rer", "sen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVINF", "$,", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Den Frieden bringen, den des Meisters Mund verhei\u00dft.", "tokens": ["Den", "Frie\u00b7den", "brin\u00b7gen", ",", "den", "des", "Meis\u00b7ters", "Mund", "ver\u00b7hei\u00dft", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "PRELS", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "\u00bbin Gottes Namen dran!\u00ab so riefst du vor dem Tod,", "tokens": ["\u00bb", "in", "Got\u00b7tes", "Na\u00b7men", "dran", "!", "\u00ab", "so", "riefst", "du", "vor", "dem", "Tod", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "NN", "NN", "PTKVZ", "$.", "$(", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und dreimal lie\u00dfest du den heil'gen Namen h\u00f6ren,", "tokens": ["Und", "drei\u00b7mal", "lie\u00b7\u00dfest", "du", "den", "heil'\u00b7gen", "Na\u00b7men", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Den Namen unsres Herrn, der in viel tausend Ch\u00f6ren", "tokens": ["Den", "Na\u00b7men", "uns\u00b7res", "Herrn", ",", "der", "in", "viel", "tau\u00b7send", "Ch\u00f6\u00b7ren"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,", "PRELS", "APPR", "PIAT", "CARD", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Durch Erd' und Himmel scha