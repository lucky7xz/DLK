word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "ADJA", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mit kurzer Schrift euch an, aus seinen Bodenrissen", "tokens": ["Mit", "kur\u00b7zer", "Schrift", "euch", "an", ",", "aus", "sei\u00b7nen", "Bo\u00b7den\u00b7ris\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "PPER", "PTKVZ", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Steigt mahnend Staub empor und spielt im Abendschein.", "tokens": ["Steigt", "mah\u00b7nend", "Staub", "em\u00b7por", "und", "spielt", "im", "A\u00b7bend\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "NN", "PTKVZ", "KON", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.17": {"line.1": {"text": "O schaut, o schauet hin! Die Sonne geht hinab,", "tokens": ["O", "schaut", ",", "o", "schau\u00b7et", "hin", "!", "Die", "Son\u00b7ne", "geht", "hin\u00b7ab", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "FM", "VVFIN", "PTKVZ", "$.", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch weilt die S\u00e4ule Staubs, aufsteigend aus den Platten;", "tokens": ["Doch", "weilt", "die", "S\u00e4u\u00b7le", "Staubs", ",", "auf\u00b7stei\u00b7gend", "aus", "den", "Plat\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "NE", "$,", "VVPP", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Seht, sie verdichtet