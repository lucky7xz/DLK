, "streckt", "die", "ner\u00b7v'\u00b7gen", "Seh\u00b7nen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$.", "PPER", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Doch nur zum Handschlag \u2013 aus: denn er ist", "tokens": ["Doch", "nur", "zum", "Hand\u00b7schlag", "\u2013", "aus", ":", "denn", "er", "ist"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "APPRART", "NN", "$(", "PTKVZ", "$.", "KON", "PPER", "VAFIN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.19": {"line.1": {"text": "Ja sanft erscheinest du, du L\u00f6w' aus Mitternacht,", "tokens": ["Ja", "sanft", "er\u00b7schei\u00b7nest", "du", ",", "du", "L\u00f6\u00b7w'", "aus", "Mit\u00b7ter\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADJD", "VVFIN", "PPER", "$,", "PPER", "NN", "APPR", "NN", "$,"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "K\u00f6nig im Goldgelock, entkleidet von dem Schrecken,", "tokens": ["K\u00f6\u00b7nig", "im", "Gold\u00b7ge\u00b7lock", ",", "ent\u00b7klei\u00b7det", "von", "dem", "Schre\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Der an den W\u00e4nden h\u00e4ngt; die Waffen, so dich decken,", "tokens": ["Der", "an", "den", "W\u00e4n\u00b7den", "h\u00e4ngt", ";", "die", "Waf\u00b7fen", ",", "so", "dich", "de\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "$.", "ART", "NN", "$,", "ADV", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+--+--", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Sind ganz Gerechtigkeit, ganz Wahrheit, Friedenstracht.", "tokens": ["Sind", "ganz", "Ge\u00b7rech\u00b7tig\u00b7keit", ",", "ganz", "Wahr\u00b7heit", ",", "Frie\u00b7dens\u00b7tracht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "ADV", "NN", "$,", "ADV", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.20": {"line.1": {"text": "Jahrhundertkl\u00fcfte weit liegt hinter dir der Krieg,", "tokens": ["Jahr\u00b7hun\u00b7dert\u00b7kl\u00fcf\u00b7te", "weit", "liegt", "hin\u00b7ter", "dir", "der", "Krieg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VVFIN", "APPR", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die unbewehrte Hand will unsre Sache st\u00fctzen;", "tokens": ["Die", "un\u00b7be\u00b7wehr\u00b7te", "Hand", "will", "uns\u00b7re", "Sa\u00b7che", "st\u00fct\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Kein Schwert, dein Herzblut nur bringst du uns mit von L\u00fctzen.", "tokens": ["Kein", "Schwert", ",", "dein", "Herz\u00b7blut", "nur", "bringst", "du", "uns", "mit", "von", "L\u00fct\u00b7zen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PPOSAT", "NN", "ADV", "VVFIN", "PPER", "PRF", "APPR", "APPR", "NN", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Das ist noch nicht verraucht, das b\u00fcrgt uns f\u00fcr den Sieg.", "tokens": ["Das", "ist", "noch", "nicht", "ver\u00b7raucht", ",", "das", "b\u00fcrgt", "uns", "f\u00fcr", "den", "Sieg", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "PTKNEG", "VVPP", "$,", "PDS", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.21": {"line.1": {"text": "Zum allerfr\u00f6mmsten Werk begleitet uns dein Geist:", "tokens": ["Zum", "al\u00b7ler\u00b7fr\u00b7\u00f6mms\u00b7ten", "Werk", "be\u00b7glei\u00b7tet", "uns", "dein", "Geist", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-++-+-+-+-+", "measure": "unknown.measure.septa"}, "line.2": {"text": "Dem Docht, der einsam glimmt, Oel zu der Flamme spenden,", "tokens": ["Dem", "Docht", ",", "der", "ein\u00b7sam", "glimmt", ",", "O\u00b7el", "zu", "der", "Flam\u00b7me", "spen\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$,", "NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Dem Glauben H\u00fctten baun, dem Glauben Lehrer senden,", "tokens": ["Dem", "Glau\u00b7ben", "H\u00fct\u00b7ten", "baun", ",", "dem", "Glau\u00b7ben", "Leh\u00b7rer", "sen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVINF", "$,", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Den Frieden bringen, den des Meisters Mund verhei\u00dft.", "tokens": ["Den", "Frie\u00b7den", "brin\u00b7gen", ",", "den", "des", "Meis\u00b7ters", "Mund", "ver\u00b7hei\u00dft", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "PRELS", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.22": {"line.1": {"text": "\u00bbin Gottes Namen dran!\u00ab so riefst du vor dem Tod,", "tokens": ["\u00bb", "in", "Got\u00b7tes", "Na\u00b7men", "dran", "!", "\u00ab", "so", "riefst", "du", "vor", "dem", "Tod", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "NN", "NN", "PTKVZ", "$.", "$(", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und dreimal lie\u00dfest du den heil'gen Namen h\u00f6ren,", "tokens": ["Und", "drei\u00b7mal", "lie\u00b7\u00dfest", "du", "den", "heil'\u00b7gen", "Na\u00b7men", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Den Namen unsres Herrn, der in viel tausend Ch\u00f6ren", "tokens": ["Den", "Na\u00b7men", "uns\u00b7res", "Herrn", ",", "der", "in", "viel", "tau\u00b7send", "Ch\u00f6\u00b7ren"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,", "PRELS", "APPR", "PIAT", "CARD", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Durch Erd' und Himmel schallt, ein Trost f\u00fcr jede Not.", "tokens": ["Durch", "Erd'", "und", "Him\u00b7mel", "schallt", ",", "ein", "Trost", "f\u00fcr", "je\u00b7de", "Not", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "$,", "ART", "NN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.23": {"line.1": {"text": "In Jesu Namen dran! an's Werk der Einigkeit!", "tokens": ["In", "Je\u00b7su", "Na\u00b7men", "dran", "!", "an's", "Werk", "der", "Ei\u00b7nig\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "PTKVZ", "$.", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du willst der F\u00fchrer sein \u2013 ein Jeder wird es kennen \u2013", "tokens": ["Du", "willst", "der", "F\u00fch\u00b7rer", "sein", "\u2013", "ein", "Je\u00b7der", "wird", "es", "ken\u00b7nen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "VAINF", "$(", "ART", "PIS", "VAFIN", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Du willst der F\u00fchrer sein \u2013 man braucht dich nicht zu nennen \u2013", "tokens": ["Du", "willst", "der", "F\u00fch\u00b7rer", "sein", "\u2013", "man", "braucht", "dich", "nicht", "zu", "nen\u00b7nen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "VAINF", "$(", "PIS", "VVFIN", "PPER", "PTKNEG", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Du ziehest vor uns her im schlichten Reiterkleid. \u2013", "tokens": ["Du", "zie\u00b7hest", "vor", "uns", "her", "im", "schlich\u00b7ten", "Rei\u00b7ter\u00b7kleid", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "ADV", "APPRART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.24": {"line.1": {"text": "Der Schatten winkt, entschwebt, und Nacht bedeckt den Stein.", "tokens": ["Der", "Schat\u00b7ten", "winkt", ",", "ent\u00b7schwebt", ",", "und", "Nacht", "be\u00b7deckt", "den", "Stein", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "VVFIN", "$,", "KON", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch drau\u00dfen ist es Tag, und wehen die Standarten.", "tokens": ["Doch", "drau\u00b7\u00dfen", "ist", "es", "Tag", ",", "und", "we\u00b7hen", "die", "Stand\u00b7ar\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "NN", "$,", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Hinaus, wo Br\u00fcder uns in Drang und Not erwarten!", "tokens": ["Hin\u00b7aus", ",", "wo", "Br\u00fc\u00b7der", "uns", "in", "Drang", "und", "Not", "er\u00b7war\u00b7ten", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "NN", "PPER", "APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und unser Feldgeschrei? ", "tokens": ["Und", "un\u00b7ser", "Feld\u00b7ge\u00b7schrei", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}