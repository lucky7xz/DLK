{"textgrid.poem.33290": {"metadata": {"author": {"name": "Blumauer, Aloys", "birth": "N.A.", "death": "N.A."}, "title": "1L: H\u00f6rt Br\u00fcder, die Zeit ist ein Becher,", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "H\u00f6rt Br\u00fcder, die Zeit ist ein Becher,", "tokens": ["H\u00f6rt", "Br\u00fc\u00b7der", ",", "die", "Zeit", "ist", "ein", "Be\u00b7cher", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "$,", "ART", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "D'rein giesset das Schicksal dem Zecher", "tokens": ["D'\u00b7rein", "gies\u00b7set", "das", "Schick\u00b7sal", "dem", "Ze\u00b7cher"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Bald Galle, bald Wasser, bald Wein.", "tokens": ["Bald", "Gal\u00b7le", ",", "bald", "Was\u00b7ser", ",", "bald", "Wein", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "ADV", "NN", "$,", "ADV", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Was gestern als Wein uns erfreute,", "tokens": ["Was", "ge\u00b7stern", "als", "Wein", "uns", "er\u00b7freu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "KOUS", "NN", "PPER", "VVFIN", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Verwandelt in Wasser sich heute,", "tokens": ["Ver\u00b7wan\u00b7delt", "in", "Was\u00b7ser", "sich", "heu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "PRF", "ADV", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Und morgen kann Galle d'rin sein.", "tokens": ["Und", "mor\u00b7gen", "kann", "Gal\u00b7le", "d'\u00b7rin", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "NN", "ADV", "VAINF", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Doch weisere Zecher verstehen", "tokens": ["Doch", "wei\u00b7se\u00b7re", "Ze\u00b7cher", "ver\u00b7ste\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "VVINF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Mit Klugheit zu trinken, und sehen", "tokens": ["Mit", "Klug\u00b7heit", "zu", "trin\u00b7ken", ",", "und", "se\u00b7hen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "PTKZU", "VVINF", "$,", "KON", "VVINF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Zuvor in den Becher hinein:", "tokens": ["Zu\u00b7vor", "in", "den", "Be\u00b7cher", "hin\u00b7ein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Und blinket es golden, so trinken", "tokens": ["Und", "blin\u00b7ket", "es", "gol\u00b7den", ",", "so", "trin\u00b7ken"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "$,", "ADV", "VVINF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Sie hastigen Zuges, und d\u00fcnken", "tokens": ["Sie", "has\u00b7ti\u00b7gen", "Zu\u00b7ges", ",", "und", "d\u00fcn\u00b7ken"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "$,", "KON", "VVINF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Sich heute nur durstig zu sein.", "tokens": ["Sich", "heu\u00b7te", "nur", "durs\u00b7tig", "zu", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "ADV", "ADJD", "PTKZU", "VAINF", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.3": {"line.1": {"text": "D'rum f\u00fcllt euch das Schicksal, ihr Zecher,", "tokens": ["D'\u00b7rum", "f\u00fcllt", "euch", "das", "Schick\u00b7sal", ",", "ihr", "Ze\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ART", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Mit flie\u00dfendem Golde den Becher,", "tokens": ["Mit", "flie\u00b7\u00dfen\u00b7dem", "Gol\u00b7de", "den", "Be\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Und ladet zum Trinken euch ein;", "tokens": ["Und", "la\u00b7det", "zum", "Trin\u00b7ken", "euch", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "PPER", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "So la\u00dft euch das Wasser von morgen,", "tokens": ["So", "la\u00dft", "euch", "das", "Was\u00b7ser", "von", "mor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "ART", "NN", "APPR", "ADV", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Die Galle von gestern nicht sorgen,", "tokens": ["Die", "Gal\u00b7le", "von", "ge\u00b7stern", "nicht", "sor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6"