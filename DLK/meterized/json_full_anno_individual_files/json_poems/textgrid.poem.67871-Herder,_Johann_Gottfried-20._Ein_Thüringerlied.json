{"textgrid.poem.67871": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "20. Ein Th\u00fcringerlied", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aber so wolln wirs heben an,", "tokens": ["A\u00b7ber", "so", "wolln", "wirs", "he\u00b7ben", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PIS", "VVFIN", "PTKVZ", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Wie sich's hat angespunnen,", "tokens": ["Wie", "sich's", "hat", "an\u00b7ge\u00b7spun\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Es ist in unser Herrn Land also gestalt,", "tokens": ["Es", "ist", "in", "un\u00b7ser", "Herrn", "Land", "al\u00b7so", "ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "NN", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Da\u00df der Herren R\u00e4the treib'n gro\u00df' Gewalt,", "tokens": ["Da\u00df", "der", "Her\u00b7ren", "R\u00e4\u00b7the", "treib'n", "gro\u00df'", "Ge\u00b7walt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.5": {"text": "Drauf haben sie gesunnen.", "tokens": ["Drauf", "ha\u00b7ben", "sie", "ge\u00b7sun\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Th\u00fcringerland, du bist ein fein gut Land,", "tokens": ["Th\u00fc\u00b7rin\u00b7ger\u00b7land", ",", "du", "bist", "ein", "fein", "gut", "Land", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VAFIN", "ART", "ADJD", "ADJD", "NN", "$,"], "meter": "---+-+-+-+", "measure": "zehnsilber"}, "line.2": {"text": "Wer dich mit Treun th\u00e4t meinen,", "tokens": ["Wer", "dich", "mit", "Treun", "th\u00e4t", "mei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "NN", "VVFIN", "PPOSAT", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Du gibst uns des Waizen und des Weins so viel,", "tokens": ["Du", "gibst", "uns", "des", "Wai\u00b7zen", "und", "des", "Weins", "so", "viel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "KON", "ART", "NN", "ADV", "ADV", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Du k\u00f6nnt'st einen Land'sherrn wohl ern\u00e4hr'n,", "tokens": ["Du", "k\u00f6nnt'st", "ei\u00b7nen", "Lan\u00b7d'\u00b7sherrn", "wohl", "er\u00b7n\u00e4hr'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Und bist ein L\u00e4ndlein so kleine.", "tokens": ["Und", "bist", "ein", "L\u00e4nd\u00b7lein", "so", "klei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "ADV", "ADJA", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Wo der Geier auf dem Gatter sizt,", "tokens": ["Wo", "der", "Gei\u00b7er", "auf", "dem", "Gat\u00b7ter", "sizt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Da deihen die K\u00fcchlein selten;", "tokens": ["Da", "dei\u00b7hen", "die", "K\u00fcch\u00b7lein", "sel\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJD", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Es d\u00fcnkt mich ein seltsam Narrenspiel,", "tokens": ["Es", "d\u00fcnkt", "mich", "ein", "selt\u00b7sam", "Nar\u00b7ren\u00b7spiel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "ADJD", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Welcher Herr sein'n R\u00e4then gehorcht so viel,", "tokens": ["Wel\u00b7cher", "Herr", "sein'n", "R\u00e4\u00b7then", "ge\u00b7horcht", "so", "viel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "PPOSAT", "NN", "VVFIN", "ADV", "ADV", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Mu\u00df mancher armer Mann entgelten.", "tokens": ["Mu\u00df", "man\u00b7cher", "ar\u00b7mer", "Mann", "ent\u00b7gel\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ein edler Herr aus Th\u00fcringerland,", "tokens": ["Ein", "ed\u00b7ler", "Herr", "aus", "Th\u00fc\u00b7rin\u00b7ger\u00b7land", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Herzog Wilhelm von Sachsen,", "tokens": ["Her\u00b7zog", "Wil\u00b7helm", "von", "Sach\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "APPR", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Liesset ihr die alten Schwertgroschen wieder schla'n,", "tokens": ["Lies\u00b7set", "ihr", "die", "al\u00b7ten", "Schwert\u00b7gro\u00b7schen", "wie\u00b7der", "schla'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "ADV", "VVINF", "$,"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Als euer Voreltern hab'n gethan,", "tokens": ["Als", "eu\u00b7er", "Vo\u00b7rel\u00b7tern", "hab'n", "ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "So m\u00f6cht' eur Heil wohl wieder wachsen.", "tokens": ["So", "m\u00f6cht'", "eur", "Heil", "wohl", "wie\u00b7der", "wach\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPOSAT", "NN", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "So w\u00fcrden die St\u00e4dt' von Gelde reich,", "tokens": ["So", "w\u00fcr\u00b7den", "die", "St\u00e4dt'", "von", "Gel\u00b7de", "reich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "APPR", "NN", "ADJD", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "So w\u00fcrden wieder gute Zeiten,", "tokens": ["So", "w\u00fcr\u00b7den", "wie\u00b7der", "gu\u00b7te", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So k\u00f6nnten euch eur arme Leut beistahn,", "tokens": ["So", "k\u00f6nn\u00b7ten", "euch", "eur", "ar\u00b7me", "Leut", "bei\u00b7stahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPOSAT", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wenn ihr sie in N\u00f6then th\u00e4t rufen an,", "tokens": ["Wenn", "ihr", "sie", "in", "N\u00f6\u00b7then", "th\u00e4t", "ru\u00b7fen", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "APPR", "NN", "VVFIN", "VVFIN", "PTKVZ", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Es w\u00e4r zu st\u00fcrmen, oder zu streiten.", "tokens": ["Es", "w\u00e4r", "zu", "st\u00fcr\u00b7men", ",", "o\u00b7der", "zu", "strei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKZU", "VVINF", "$,", "KON", "PTKZU", "VVINF", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Wo das gut Geld im Land umf\u00e4hrt,", "tokens": ["Wo", "das", "gut", "Geld", "im", "Land", "um\u00b7f\u00e4hrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJD", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das haben die Pfaffen und Juden,", "tokens": ["Das", "ha\u00b7ben", "die", "Pfaf\u00b7fen", "und", "Ju\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "KON", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Es ist dem reichen Mann alles unterthan,", "tokens": ["Es", "ist", "dem", "rei\u00b7chen", "Mann", "al\u00b7les", "un\u00b7ter\u00b7than", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "PIS", "ADJD", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Die den Wucher mit den Juden ha'n,", "tokens": ["Die", "den", "Wu\u00b7cher", "mit", "den", "Ju\u00b7den", "ha'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Man vergleicht sie einem Stockruthen.", "tokens": ["Man", "ver\u00b7gleicht", "sie", "ei\u00b7nem", "Stock\u00b7ru\u00b7then", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+--", "measure": "unknown.measure.tetra"}}, "stanza.7": {"line.1": {"text": "Hat einer dann der Pfennige nicht,", "tokens": ["Hat", "ei\u00b7ner", "dann", "der", "Pfen\u00b7ni\u00b7ge", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADV", "ART", "NN", "PTKNEG", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Er mu\u00df sie wahrlich schicken,", "tokens": ["Er", "mu\u00df", "sie", "wahr\u00b7lich", "schi\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der reiche Mann, der hats daheim in seinem Haus,", "tokens": ["Der", "rei\u00b7che", "Mann", ",", "der", "hats", "da\u00b7heim", "in", "sei\u00b7nem", "Haus", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "VAFIN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Er sieht gleich wie eine Steineule heraus,", "tokens": ["Er", "sieht", "gleich", "wie", "ei\u00b7ne", "Stei\u00b7neu\u00b7le", "he\u00b7raus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "KOKOM", "ART", "NN", "PTKVZ", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "So geschieht manchem Armen oft und dicke.", "tokens": ["So", "ge\u00b7schieht", "man\u00b7chem", "Ar\u00b7men", "oft", "und", "di\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "ADV", "KON", "ADJA", "$."], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}}, "stanza.8": {"line.1": {"text": "Aber so wolln wirs heben an,", "tokens": ["A\u00b7ber", "so", "wolln", "wirs", "he\u00b7ben", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PIS", "VVFIN", "PTKVZ", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Wie sich's hat angespunnen,", "tokens": ["Wie", "sich's", "hat", "an\u00b7ge\u00b7spun\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Es ist in unser Herrn Land also gestalt,", "tokens": ["Es", "ist", "in", "un\u00b7ser", "Herrn", "Land", "al\u00b7so", "ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "NN", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Da\u00df der Herren R\u00e4the treib'n gro\u00df' Gewalt,", "tokens": ["Da\u00df", "der", "Her\u00b7ren", "R\u00e4\u00b7the", "treib'n", "gro\u00df'", "Ge\u00b7walt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "VVFIN", "ADJA", "NN