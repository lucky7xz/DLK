okens": ["Ein", "ed\u00b7ler", "Herr", "aus", "Th\u00fc\u00b7rin\u00b7ger\u00b7land", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Herzog Wilhelm von Sachsen,", "tokens": ["Her\u00b7zog", "Wil\u00b7helm", "von", "Sach\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "APPR", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Liesset ihr die alten Schwertgroschen wieder schla'n,", "tokens": ["Lies\u00b7set", "ihr", "die", "al\u00b7ten", "Schwert\u00b7gro\u00b7schen", "wie\u00b7der", "schla'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "ADV", "VVINF", "$,"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Als euer Voreltern hab'n gethan,", "tokens": ["Als", "eu\u00b7er", "Vo\u00b7rel\u00b7tern", "hab'n", "ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "So m\u00f6cht' eur Heil wohl wieder wachsen.", "tokens": ["So", "m\u00f6cht'", "eur", "Heil", "wohl", "wie\u00b7der", "wach\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPOSAT", "NN", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "So w\u00fcrden die St\u00e4dt' von Gelde reich,", "tokens": ["So", "w\u00fcr\u00b7den", "die", "St\u00e4dt'", "von", "Gel\u00b7de", "reich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "APPR", "NN", "ADJD", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "So w\u00fcrden wieder gute Zeiten,", "tokens": ["So", "w\u00fcr\u00b7den", "wie\u00b7der", "gu\u00b7te", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So k\u00f6nnten euch eur arme Leut beistahn,", "tokens": ["So", "k\u00f6nn\u00b7ten", "euch", "eur", "ar\u00b7me", "Leut", "bei\u00b7stahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPOSAT", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wenn ihr sie in N\u00f6then th\u00e4t rufen an,", "tokens": ["Wenn", "ihr", "sie", "in", "N\u00f6\u00b7then", "th\u00e4t", "ru\u00b7fen", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "APPR", "NN", "VVFIN", "VVFIN", "PTKVZ", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Es w\u00e4r zu st\u00fcrmen, oder zu streiten.", "tokens": ["Es", "w\u00e4r", "zu", "st\u00fcr\u00b7men", ",", "o\u00b7der", "zu", "strei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKZU", "VVINF", "$,", "KON", "PTKZU", "VVINF", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.13": {"line.1": {"text": "Wo das gut Geld im Land umf\u00e4hrt,", "tokens": ["Wo", "das", "gut", "Geld", "im", "Land", "um\u00b7f\u00e4hrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJD", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das haben die Pfaffen und Juden,", "tokens": ["Das", "ha\u00b7ben", "die", "Pfaf\u00b7fen", "und", "Ju\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "KON", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Es ist dem reichen Mann alles unterthan,", "tokens": ["Es", "ist", "dem", "rei\u00b7chen", "Mann", "al\u00b7les", "un\u00b7ter\u00b7than", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "PIS", "ADJD", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Die den Wucher mit den Juden ha'n,", "tokens": ["Die", "den", "Wu\u00b7cher", "mit", "den", "Ju\u00b7den", "ha'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Man vergleicht sie einem Stockruthen.", "tokens": ["Man", "ver\u00b7gleicht", "sie", "ei\u00b7nem", "Stock\u00b7ru\u00b7then", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+--", "measure": "unknown.measure.tetra"}}, "stanza.14": {"line.1": {"text": "Hat einer dann der Pfennige nicht,", "tokens": ["Hat", "ei\u00b7ner", "dann", "der", "Pfen\u00b7ni\u00b7ge", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADV", "ART", "NN", "PTKNEG", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Er mu\u00df sie wahrlich schicken,", "tokens": ["Er", "mu\u00df", "sie", "wahr\u00b7lich", "schi\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der reiche Mann, der hats daheim in seinem Haus,", "tokens": ["Der", "rei\u00b7che", "Mann", ",", "der", "hats", "da\u00b7heim", "in", "sei\u00b7nem", "Haus", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "VAFIN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Er sieht gleich wie eine Steineule heraus,", "tokens": ["Er", "sieht", "gleich", "wie", "ei\u00b7ne", "Stei\u00b7neu\u00b7le", "he\u00b7raus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "KOKOM", "ART", "NN", "PTKVZ", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "So geschieht manchem Armen oft und dicke.", "tokens": ["So", "ge\u00b7schieht", "man\u00b7chem", "Ar\u00b7men", "oft", "und", "di\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "ADV", "KON", "ADJA", "$."], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}}}}}