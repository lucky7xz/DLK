{"textgrid.poem.42535": {"metadata": {"author": {"name": "Cronegk, Johann Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Undankbare Chloris, leb wohl! nun will ich in traurige W\u00e4lder", "genre": "verse", "period": "N.A.", "pub_year": 1744, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Undankbare Chloris, leb wohl! nun will ich in traurige W\u00e4lder", "tokens": ["Un\u00b7dank\u00b7ba\u00b7re", "Chlo\u00b7ris", ",", "leb", "wohl", "!", "nun", "will", "ich", "in", "trau\u00b7ri\u00b7ge", "W\u00e4l\u00b7der"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "$,", "VVFIN", "ADV", "$.", "ADV", "VMFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+--+-+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Zum Wohnplatz schwerm\u00fctiger Einsamkeit fliehn.", "tokens": ["Zum", "Wohn\u00b7platz", "schwer\u00b7m\u00fc\u00b7ti\u00b7ger", "Ein\u00b7sam\u00b7keit", "fliehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJA", "NN", "VVINF", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Sei gl\u00fccklich! ich segne dich noch! dich segnet die letzte der Tr\u00e4nen,", "tokens": ["Sei", "gl\u00fcck\u00b7lich", "!", "ich", "seg\u00b7ne", "dich", "noch", "!", "dich", "seg\u00b7net", "die", "letz\u00b7te", "der", "Tr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "$.", "PPER", "VVFIN", "PPER", "ADV", "$.", "PPER", "VVFIN", "ART", "ADJA", "ART", "NN", "$,"], "meter": "-+--+-+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Die einst auf den Wangen der Sterbenden steht.", "tokens": ["Die", "einst", "auf", "den", "Wan\u00b7gen", "der", "Ster\u00b7ben\u00b7den", "steht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.2": {"line.1": {"text": "Nun f\u00fchlst du nicht meinen Verlust: von fr\u00f6hlichen Toren umgeben", "tokens": ["Nun", "f\u00fchlst", "du", "nicht", "mei\u00b7nen", "Ver\u00b7lust", ":", "von", "fr\u00f6h\u00b7li\u00b7chen", "To\u00b7ren", "um\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "PPOSAT", "NN", "$.", "APPR", "ADJA", "NN", "VVPP"], "meter": "-+--+--+-+--+--+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Vergi\u00dft du, wie z\u00e4rtlich dich Thyrsis geliebt.", "tokens": ["Ver\u00b7gi\u00dft", "du", ",", "wie", "z\u00e4rt\u00b7lich", "dich", "Thyr\u00b7sis", "ge\u00b7liebt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "ADJD", "PPER", "NE", "VVPP", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Dereinst (ich w\u00fcnsch' es dir nicht!) doch ach! die Stunde wird kommen,", "tokens": ["De\u00b7reinst", "(", "ich", "w\u00fcn\u00b7sch'", "es", "dir", "nicht", "!", ")", "doch", "ach", "!", "die", "Stun\u00b7de", "wird", "kom\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "PPER", "VVFIN", "PPER", "PPER", "PTKNEG", "$.", "$(", "ADV", "ADV", "$.", "ART", "NN", "VAFIN", "VVINF", "$,"], "meter": "-+-+---+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Wo du mich bedaurest und nach mir dich sehnst.", "tokens": ["Wo", "du", "mich", "be\u00b7dau\u00b7rest", "und", "nach", "mir", "dich", "sehnst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "ADJD", "KON", "APPR", "PPER", "PRF", "ADV", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.3": {"line.1": {"text": "Verzeih mir den dichtrischen Stolz! Es sendet der Himmel nur selten", "tokens": ["Ver\u00b7zeih", "mir", "den", "dich\u00b7tri\u00b7schen", "Stolz", "!", "Es", "sen\u00b7det", "der", "Him\u00b7mel", "nur", "sel\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ART", "ADJA", "NN", "$.", "PPER", "VVFIN", "ART", "NN", "ADV", "ADJD"], "meter": "-+-+-+-+-+--+--+-", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "So z\u00e4rtliche Seelen zum Erdball herab,", "tokens": ["So", "z\u00e4rt\u00b7li\u00b7che", "See\u00b7len", "zum", "Erd\u00b7ball", "her\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "APPRART", "NN", "ADV", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Wie die! die mein Gl\u00fcck mir verliehn, so z\u00e4rtlich empfindende Herzen,", "tokens": ["Wie", "die", "!", "die", "mein", "Gl\u00fcck", "mir", "ver\u00b7liehn", ",", "so", "z\u00e4rt\u00b7lich", "emp\u00b7fin\u00b7den\u00b7de", "Her\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "$.", "ART", "PPOSAT", "NN", "PPER", "VVINF", "$,", "ADV", "ADJD", "ADJA", "NN", "$,"], "meter": "-+--+--+-+--+--+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Wie dies, das dein Stolz an dem Thyrsis verschm\u00e4ht.", "tokens": ["Wie", "dies", ",", "das", "dein", "Stolz", "an", "dem", "Thyr\u00b7sis", "ver\u00b7schm\u00e4ht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDS", "$,", "PRELS", "PPOSAT", "NN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.4": {"line.1": {"text": "Einst, wenn diese Seele befreit sich wieder zum Himmel geschwungen,", "tokens": ["Einst", ",", "wenn", "die\u00b7se", "See\u00b7le", "be\u00b7freit", "sich", "wie\u00b7der", "zum", "Him\u00b7mel", "ge\u00b7schwun\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PDAT", "NN", "VVFIN", "PRF", "ADV", "APPRART", "NN", "VVPP", "$,"], "meter": "----+--+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Da k\u00f6mmst du, gezwungen von heimlicher Reu',", "tokens": ["Da", "k\u00f6mmst", "du", ",", "ge\u00b7zwun\u00b7gen", "von", "heim\u00b7li\u00b7cher", "Reu'", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Hieher in das friedsame Tal und fragst die unschuldigen Hirten:", "tokens": ["Hie\u00b7her", "in", "das", "fried\u00b7sa\u00b7me", "Tal", "und", "fragst", "die", "un\u00b7schul\u00b7di\u00b7gen", "Hir\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-+--+--+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Ihr Hirten, sprecht, habt ihr den Thyrsis gesehn?", "tokens": ["Ihr", "Hir\u00b7ten", ",", "sprecht", ",", "habt", "ihr", "den", "Thyr\u00b7sis", "ge\u00b7sehn", "?"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "$,", "VAFIN", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.5": {"line.1": {"text": "Wir sahn ihn vor einiger Zeit, antwortet der eine von ihnen:", "tokens": ["Wir", "sahn", "ihn", "vor", "ei\u00b7ni\u00b7ger", "Zeit", ",", "ant\u00b7wor\u00b7tet", "der", "ei\u00b7ne", "von", "ih\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PIAT", "NN", "$,", "VVFIN", "ART", "ART", "APPR", "PPER", "$."], "meter": "-+--+--+-+--+--+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Hier ging er tiefsinnig und schweigend herum.", "tokens": ["Hier", "ging", "er", "tief\u00b7sin\u00b7nig", "und", "schwei\u00b7gend", "he\u00b7rum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "PTKVZ", "$."], "meter": "-+-++--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Oft sahn unsre M\u00e4gdchen ihm nach und seufzten; o gl\u00fcckliches M\u00e4gdchen,", "tokens": ["Oft", "sahn", "uns\u00b7re", "M\u00e4gd\u00b7chen", "ihm", "nach", "und", "seufz\u00b7ten", ";", "o", "gl\u00fcck\u00b7li\u00b7ches", "M\u00e4gd\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "PPER", "APPR", "KON", "VVFIN", "$.", "FM", "ADJA", "NN", "$,"], "meter": "-+--+--+-+--+--+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Dem Amor denselben zum Sch\u00e4fer bestimmt!", "tokens": ["Dem", "A\u00b7mor", "den\u00b7sel\u00b7ben", "zum", "Sch\u00e4\u00b7fer", "be\u00b7stimmt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "PDAT", "APPRART", "NN", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.6": {"line.1": {"text": "Oft sah man ihn einsam am Bach die murmelnden Wellen betrachten;", "tokens": ["Oft", "sah", "man", "ihn", "ein\u00b7sam", "am", "Bach", "die", "mur\u00b7meln\u00b7den", "Wel\u00b7len", "be\u00b7trach\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PPER", "ADJD", "APPRART", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+--+--+-+--+--+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Oft sah man ihn hier im beschatteten Tal;", "tokens": ["Oft", "sah", "man", "ihn", "hier", "im", "be\u00b7schat\u00b7te\u00b7ten", "Tal", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PPER", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+--++-+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Hier klagten bei heiterer Nacht die T\u00f6ne der z\u00e4rtlichen Laute;", "tokens": ["Hier", "klag\u00b7ten", "bei", "hei\u00b7te\u00b7rer", "Nacht", "die", "T\u00f6\u00b7ne", "der", "z\u00e4rt\u00b7li\u00b7chen", "Lau\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-+--+--+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Wir h\u00f6rten bewundernd den sanften Gesang.", "tokens": ["Wir", "h\u00f6r\u00b7ten", "be\u00b7wun\u00b7dernd", "den", "sanf\u00b7ten", "Ge\u00b7sang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.7": {"line.1": {"text": "Jetzt sieht man ihn nicht mehr am Bach die murmelnden Wellen betrachten.", "tokens": ["Jetzt", "sieht", "man", "ihn", "nicht", "mehr", "am", "Bach", "die", "mur\u00b7meln\u00b7den", "Wel\u00b7len", "be\u00b7trach\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PPER", "PTKNEG", "ADV", "APPRART", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+--++-+-+--+--+-", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Man sieht ihn nicht mehr im beschatteten Tal;", "tokens": ["Man", "sieht", "ihn", "nicht", "mehr", "im", "be\u00b7schat\u00b7te\u00b7ten", "Tal", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "PTKNEG", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Wir h\u00f6ren bei heiterer Nacht die T\u00f6ne der z\u00e4rtlichen Laute,", "tokens": ["Wir", "h\u00f6\u00b7ren", "bei", "hei\u00b7te\u00b7rer", "Nacht", "die", "T\u00f6\u00b7ne", "der", "z\u00e4rt\u00b7li\u00b7chen", "Lau\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+--+--+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Die sanften Ges\u00e4nge bewundernd nicht mehr.", "tokens": ["Die", "sanf\u00b7ten", "Ge\u00b7s\u00e4n\u00b7ge", "be\u00b7wun\u00b7dernd", "nicht", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "PTKNEG", "ADV", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.8": {"line.1": {"text": "Dort fern, in dem einsamsten Busch, dort liegt nun sein Leichnam begraben.", "tokens": ["Dort", "fern", ",", "in", "dem", "ein\u00b7sams\u00b7ten", "Busch", ",", "dort", "liegt", "nun", "sein", "Leich\u00b7nam", "be\u00b7gra\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "APPR", "ART", "ADJA", "NN", "$,", "ADV", "VVFIN", "ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.2": {"text": "Man saget, da\u00df dorten sein Schatten noch irrt:", "tokens": ["Man", "sa\u00b7get", ",", "da\u00df", "dor\u00b7ten", "sein", "Schat\u00b7ten", "noch", "irrt", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "KOUS", "ADV", "PPOSAT", "NN", "ADV", "VVFIN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Dort h\u00f6rt, wie man furchtsam erz\u00e4hlt, der Wandrer die Reihen der Nymphen", "tokens": ["Dort", "h\u00f6rt", ",", "wie", "man", "furcht\u00b7sam", "er\u00b7z\u00e4hlt", ",", "der", "Wand\u00b7rer", "die", "Rei\u00b7hen", "der", "Nym\u00b7phen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "PWAV", "PIS", "ADJD", "VVFIN", "$,", "ART", "NN", "ART", "NN", "ART", "NN"], "meter": "-+--+--+-+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Beim Scheine des Monden sich h\u00fcpfend erfreun.", "tokens": ["Beim", "Schei\u00b7ne", "des", "Mon\u00b7den", "sich", "h\u00fcp\u00b7fend", "er\u00b7freun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "PRF", "ADJD", "VVINF", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.9": {"line.1": {"text": "Dann, Chloris, besuche mein Grab und sprich: ungl\u00fccklicher J\u00fcngling!", "tokens": ["Dann", ",", "Chlo\u00b7ris", ",", "be\u00b7su\u00b7che", "mein", "Grab", "und", "sprich", ":", "un\u00b7gl\u00fcck\u00b7li\u00b7cher", "J\u00fcng\u00b7ling", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "NE", "$,", "VVFIN", "PPOSAT", "NN", "KON", "ADJD", "$.", "ADJA", "NN", "$."], "meter": "-+--+--+-+-+--+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Ruh sanfte! so sprichst du mit Seufzen vielleicht;", "tokens": ["Ruh", "sanf\u00b7te", "!", "so", "sprichst", "du", "mit", "Seuf\u00b7zen", "viel\u00b7leicht", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$.", "ADV", "VVFIN", "PPER", "APPR", "NN", "ADV", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Ruh sanft! o warum hat mein Stolz das Z\u00e4rtlichste unter den Herzen,", "tokens": ["Ruh", "sanft", "!", "o", "wa\u00b7rum", "hat", "mein", "Stolz", "das", "Z\u00e4rt\u00b7lichs\u00b7te", "un\u00b7ter", "den", "Her\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "$.", "FM", "PWAV", "VAFIN", "PPOSAT", "NN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+--+--+-", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "Das Herz des ungl\u00fccklichen Thyrsis verschm\u00e4ht!", "tokens": ["Das", "Herz", "des", "un\u00b7gl\u00fcck\u00b7li\u00b7chen", "Thyr\u00b7sis", "ver\u00b7schm\u00e4ht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.10": {"line.1": {"text": "Undankbare Chloris, leb wohl! nun will ich in traurige W\u00e4lder", "tokens": ["Un\u00b7dank\u00b7ba\u00b7re", "Chlo\u00b7ris", ",", "leb", "wohl", "!", "nun", "will", "ich", "in", "trau\u00b7ri\u00b7ge", "W\u00e4l\u00b7der"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "$,", "VVFIN", "ADV", "$.", "ADV", "VMFIN", "PPER", "APPR", "ADJA", "NN"], "mete