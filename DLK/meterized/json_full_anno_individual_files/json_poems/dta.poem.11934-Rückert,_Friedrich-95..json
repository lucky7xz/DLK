{"dta.poem.11934": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "95.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich sage dir, mein Sohn, von welchen Lehrern lernen", "tokens": ["Ich", "sa\u00b7ge", "dir", ",", "mein", "Sohn", ",", "von", "wel\u00b7chen", "Leh\u00b7rern", "ler\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "$,", "APPR", "PWAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du sollst soviel du kannst, von welchem dich entfernen.", "tokens": ["Du", "sollst", "so\u00b7viel", "du", "kannst", ",", "von", "wel\u00b7chem", "dich", "ent\u00b7fer\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIS", "PPER", "VMFIN", "$,", "APPR", "PRELS", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Einer bescheiden ist des Stoffes treu beflissen,", "tokens": ["Ei\u00b7ner", "be\u00b7schei\u00b7den", "ist", "des", "Stof\u00b7fes", "treu", "be\u00b7flis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVPP", "VAFIN", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Des andern h\u00f6hrer Sinn erhebt den Stoff ins Wissen.", "tokens": ["Des", "an\u00b7dern", "h\u00f6h\u00b7rer", "Sinn", "er\u00b7hebt", "den", "Stoff", "ins", "Wis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Der dritte d\u00fcnkelhaft will nicht die ew'gen Sachen", "tokens": ["Der", "drit\u00b7te", "d\u00fcn\u00b7kel\u00b7haft", "will", "nicht", "die", "ew'\u00b7gen", "Sa\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJD", "VMFIN", "PTKNEG", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So nehmen wie sie sind, will wie er denkt sie machen.", "tokens": ["So", "neh\u00b7men", "wie", "sie", "sind", ",", "will", "wie", "er", "denkt", "sie", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "KOKOM", "PPER", "VAFIN", "$,", "VMFIN", "KOKOM", "PPER", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Der eine wird mit Flei\u00df das Einzle weiter bringen,", "tokens": ["Der", "ei\u00b7ne", "wird", "mit", "Flei\u00df", "das", "Einz\u00b7le", "wei\u00b7ter", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VAFIN", "APPR", "NN", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der andre sucht mit Geist das Ganze zu durchdringen.", "tokens": ["Der", "and\u00b7re", "sucht", "mit", "Geist", "das", "Gan\u00b7ze", "zu", "durch\u00b7drin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "APPR", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Der dritte d\u00fcnkelhaft will ein System nur baun,", "tokens": ["Der", "drit\u00b7te", "d\u00fcn\u00b7kel\u00b7haft", "will", "ein", "Sys\u00b7tem", "nur", "baun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJD", "VMFIN", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Um wohlgef\u00e4llig sich als Sch\u00f6pfer zu beschaun.", "tokens": ["Um", "wohl\u00b7ge\u00b7f\u00e4l\u00b7lig", "sich", "als", "Sch\u00f6p\u00b7fer", "zu", "be\u00b7schaun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADJD", "PRF", "KOUS", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Vom einen kannst du viel, vom andern alles lernen,", "tokens": ["Vom", "ei\u00b7nen", "kannst", "du", "viel", ",", "vom", "an\u00b7dern", "al\u00b7les", "ler\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "PIS", "VMFIN", "PPER", "ADV", "$,", "APPRART", "ADJA", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vom dritten nichts; von dem sollst du dich, Sohn, entfernen.", "tokens": ["Vom", "drit\u00b7ten", "nichts", ";", "von", "dem", "sollst", "du", "dich", ",", "Sohn", ",", "ent\u00b7fer\u00b7nen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPRART", "ADJA", "PIS", "$.", "APPR", "ART", "VMFIN", "PPER", "PRF", "$,", "NN", "$,", "VVINF", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.7": {"line.1": {"text": "Beim ersten magst du Fu\u00df auf festem Grunde fassen,", "tokens": ["Beim", "ers\u00b7ten", "magst", "du", "Fu\u00df", "auf", "fes\u00b7tem", "Grun\u00b7de", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "VMFIN", "PPER", "NN", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vom andern dir zum Flug die Richte geben lassen.", "tokens": ["Vom", "an\u00b7dern", "dir", "zum", "Flug", "die", "Rich\u00b7te", "ge\u00b7ben", "las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "PPER", "APPRART", "NN", "ART", "NN", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Vorm dritten h\u00fcte dich! es ist um dich gethan,", "tokens": ["Vorm", "drit\u00b7ten", "h\u00fc\u00b7te", "dich", "!", "es", "ist", "um", "dich", "ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "VVFIN", "PPER", "$.", "PPER", "VAFIN", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "F\u00fcllt er mit D\u00fcnkel dich und leerem Fachwerk an.", "tokens": ["F\u00fcllt", "er", "mit", "D\u00fcn\u00b7kel", "dich", "und", "lee\u00b7rem", "Fach\u00b7werk", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "PPER", "KON", "ADJA", "NN", "PTKVZ", "$."], "meter": "++-+-+-+-+-+", "measure": "unknown.measure.septa"}}}}}