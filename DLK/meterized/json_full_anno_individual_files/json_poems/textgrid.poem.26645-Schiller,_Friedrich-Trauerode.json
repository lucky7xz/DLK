{"textgrid.poem.26645": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Trauerode", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Grimmig wirgt der Tod durch unsre Glieder! \u2013", "tokens": ["Grim\u00b7mig", "wirgt", "der", "Tod", "durch", "uns\u00b7re", "Glie\u00b7der", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Dumpfig heult die Leichendrummel wieder,", "tokens": ["Dump\u00b7fig", "heult", "die", "Lei\u00b7chen\u00b7drum\u00b7mel", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Schon ein neuer ist hinweggerafft;", "tokens": ["Schon", "ein", "neu\u00b7er", "ist", "hin\u00b7weg\u00b7ge\u00b7rafft", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Mit gesenktem Schie\u00dfgewehre wanken", "tokens": ["Mit", "ge\u00b7senk\u00b7tem", "Schie\u00df\u00b7ge\u00b7weh\u00b7re", "wan\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Graue Krieger nach des Kirchhofs Schranken,", "tokens": ["Grau\u00b7e", "Krie\u00b7ger", "nach", "des", "Kirch\u00b7hofs", "Schran\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Wo der tapfre, brave M\u00fcller schlaft.", "tokens": ["Wo", "der", "tapf\u00b7re", ",", "bra\u00b7ve", "M\u00fcl\u00b7ler", "schlaft", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "$,", "VVFIN", "NE", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Br\u00fcder, kommt! \u2013 erblasset! \u2013 schauert! zittert!", "tokens": ["Br\u00fc\u00b7der", ",", "kommt", "!", "\u2013", "er\u00b7blas\u00b7set", "!", "\u2013", "schau\u00b7ert", "!", "zit\u00b7tert", "!"], "token_info": ["word", "punct", "word", "punct", "punct", "word", "punct", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$.", "$(", "VVFIN", "$.", "$(", "VVFIN", "$.", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Bebe jetzt, den niemals nichts ersch\u00fcttert,", "tokens": ["Be\u00b7be", "jetzt", ",", "den", "nie\u00b7mals", "nichts", "er\u00b7sch\u00fct\u00b7tert", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "PRELS", "ADV", "PIS", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Grabgef\u00fchle schauern durch sein Mark.", "tokens": ["Grab\u00b7ge\u00b7f\u00fch\u00b7le", "schau\u00b7ern", "durch", "sein", "Mark", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Sehet! Alles, was wir Leben hie\u00dfen,", "tokens": ["Se\u00b7het", "!", "Al\u00b7les", ",", "was", "wir", "Le\u00b7ben", "hie\u00b7\u00dfen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "PIS", "$,", "PRELS", "PPER", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Was wir liebten, was wir selig priesen,", "tokens": ["Was", "wir", "lieb\u00b7ten", ",", "was", "wir", "se\u00b7lig", "prie\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "PRELS", "PPER", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Liegt vereitelt in dem schmalen Sarg.", "tokens": ["Liegt", "ver\u00b7ei\u00b7telt", "in", "dem", "schma\u00b7len", "Sarg", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Von dem Antlitz alles Rot gesunken,", "tokens": ["Von", "dem", "Ant\u00b7litz", "al\u00b7les", "Rot", "ge\u00b7sun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PIAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Aus den Augen alle Lebensfunken", "tokens": ["Aus", "den", "Au\u00b7gen", "al\u00b7le", "Le\u00b7bens\u00b7fun\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PIAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Weggel\u00f6schet in chaotsche Nacht \u2013", "tokens": ["Weg\u00b7ge\u00b7l\u00f6\u00b7schet", "in", "chaot\u00b7sche", "Nacht", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$("], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Seine Mienen, sein holdselig L\u00e4cheln", "tokens": ["Sei\u00b7ne", "Mie\u00b7nen", ",", "sein", "hold\u00b7se\u00b7lig", "L\u00e4\u00b7cheln"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "ADJD", "NN"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Weggeblasen mit dem Sterber\u00f6cheln,", "tokens": ["Weg\u00b7ge\u00b7bla\u00b7sen", "mit", "dem", "Ster\u00b7be\u00b7r\u00f6\u00b7cheln", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Ewig, ewig nimmer angefacht! \u2013", "tokens": ["E\u00b7wig", ",", "e\u00b7wig", "nim\u00b7mer", "an\u00b7ge\u00b7facht", "!", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "$,", "ADJD", "ADV", "VVPP", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Nie vom Sturm der Leidenschaft durchw\u00fchlet,", "tokens": ["Nie", "vom", "Sturm", "der", "Lei\u00b7den\u00b7schaft", "durch\u00b7w\u00fch\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wie ein Bach durch Blumenbette spielet,", "tokens": ["Wie", "ein", "Bach", "durch", "Blu\u00b7men\u00b7bet\u00b7te", "spie\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Flo\u00df sein Leben hin in Melodie \u2013", "tokens": ["Flo\u00df", "sein", "Le\u00b7ben", "hin", "in", "Me\u00b7lo\u00b7die", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "ADV", "APPR", "NE", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Ha! was ist nun, was am sch\u00f6nsten schmeichelt?", "tokens": ["Ha", "!", "was", "ist", "nun", ",", "was", "am", "sch\u00f6ns\u00b7ten", "schmei\u00b7chelt", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PWS", "VAFIN", "ADV", "$,", "PRELS", "APPRART", "ADJA", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Nichts als Larve, die der Tod uns heuchelt \u2013", "tokens": ["Nichts", "als", "Lar\u00b7ve", ",", "die", "der", "Tod", "uns", "heu\u00b7chelt", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "KOKOM", "NE", "$,", "PRELS", "ART", "NN", "PPER", "VVFIN", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Und dann auf dem Sarg zerrei\u00dft er sie.", "tokens": ["Und", "dann", "auf", "dem", "Sarg", "zer\u00b7rei\u00dft", "er", "sie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "VVFIN", "PPER", "PPER", "$."], "meter": "----+-+-+", "measure": "unknown.measure.tri"}}, "stanza.5": {"line.1": {"text": "Auf des Menschen kaltem, starrem Rumpfe", "tokens": ["Auf", "des", "Men\u00b7schen", "kal\u00b7tem", ",", "star\u00b7rem", "Rump\u00b7fe"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "$,", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Sterben seine wirblende Triumphe,", "tokens": ["Ster\u00b7ben", "sei\u00b7ne", "wir\u00b7blen\u00b7de", "Tri\u00b7um\u00b7phe", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "R\u00f6chlen all in ein Gewimmer aus \u2013", "tokens": ["R\u00f6ch\u00b7len", "all", "in", "ein", "Ge\u00b7wim\u00b7mer", "aus", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "APPR", "ART", "NN", "PTKVZ", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Gl\u00fcck und Ruhm zerflattern auf dem Sarge,", "tokens": ["Gl\u00fcck", "und", "Ruhm", "zer\u00b7flat\u00b7tern", "auf", "dem", "Sar\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "K\u00f6nige und Bettler, Feige, Starke", "tokens": ["K\u00f6\u00b7ni\u00b7ge", "und", "Bett\u00b7ler", ",", "Fei\u00b7ge", ",", "Star\u00b7ke"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["NN", "KON", "NN", "$,", "NN", "$,", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Ziehn hinunter in das Totenhaus.", "tokens": ["Ziehn", "hin\u00b7un\u00b7ter", "in", "das", "To\u00b7ten\u00b7haus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Aber frei erhoben \u00fcber Gr\u00fcfte", "tokens": ["A\u00b7ber", "frei", "er\u00b7ho\u00b7ben", "\u00fc\u00b7ber", "Gr\u00fcf\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVPP", "APPR", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Fliegt der Geist in des Olympus L\u00fcfte,", "tokens": ["Fliegt", "der", "Geist", "in", "des", "O\u00b7lym\u00b7pus", "L\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NE", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Triumphierend, wie ein Adler steigt,", "tokens": ["Tri\u00b7um\u00b7phie\u00b7rend", ",", "wie", "ein", "Ad\u00b7ler", "steigt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Wann sein Wohnsitz, die erhabne Tanne,", "tokens": ["Wann", "sein", "Wohn\u00b7sitz", ",", "die", "er\u00b7hab\u00b7ne", "Tan\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Niederkracht im tobenden Orkane", "tokens": ["Nie\u00b7der\u00b7kracht", "im", "to\u00b7ben\u00b7den", "Or\u00b7ka\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["PTKNEG", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Und der Nordsturm W\u00e4lder niederbeugt.", "tokens": ["Und", "der", "Nord\u00b7sturm", "W\u00e4l\u00b7der", "nie\u00b7der\u00b7beugt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Zieh auch du, geliebter, teurer Streiter,", "tokens": ["Zieh", "auch", "du", ",", "ge\u00b7lieb\u00b7ter", ",", "teu\u00b7rer", "Strei\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPER", "$,", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Auf den Fl\u00fcgeln unsrer Donner weiter,", "tokens": ["Auf", "den", "Fl\u00fc\u00b7geln", "uns\u00b7rer", "Don\u00b7ner", "wei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Keine Tr\u00e4nen schicken wir dir mit \u2013", "tokens": ["Kei\u00b7ne", "Tr\u00e4\u00b7nen", "schi\u00b7cken", "wir", "dir", "mit", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "PPER", "APPR", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Mit Geheule und mit Weiberklagen", "tokens": ["Mit", "Ge\u00b7heu\u00b7le", "und", "mit", "Wei\u00b7ber\u00b7kla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "APPR", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Mag man andre zu dem Grabe tragen,", "tokens": ["Mag", "man", "and\u00b7re", "zu", "dem", "Gra\u00b7be", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "PIS", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Pulverdonner ist der Krieger Wiegenlied. \u2013", "tokens": ["Pul\u00b7ver\u00b7don\u00b7ner", "ist", "der", "Krie\u00b7ger", "Wie\u00b7gen\u00b7lied", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "NN", "$.", "$("], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.8": {"line.1": {"text": "Weinend geht man deinen Sarg vor\u00fcber,", "tokens": ["Wei\u00b7nend", "geht", "man", "dei\u00b7nen", "Sarg", "vor\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PIS", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Selbst des Mannes Auge wird jetzt tr\u00fcber,", "tokens": ["Selbst", "des", "Man\u00b7nes", "Au\u00b7ge", "wird", "jetzt", "tr\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Und die Helden Carls betrauren dich. \u2013", "tokens": ["Und", "die", "Hel\u00b7den", "Carls", "be\u00b7trau\u00b7ren", "dich", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ART", "NN", "NE", "VVFIN", "PPER", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Geh dahin mit dieser stolzen Ehre,", "tokens": ["Geh", "da\u00b7hin", "mit", "die\u00b7ser", "stol\u00b7zen", "Eh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PAV", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Prahle dort in der Verkl\u00e4rten Heere:", "tokens": ["Prah\u00b7le", "dort", "in", "der", "Ver\u00b7kl\u00e4r\u00b7ten", "Hee\u00b7re", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Sie, die Helden Carls, betrauren mich!", "tokens": ["Sie", ",", "die", "Hel\u00b7den", "Carls", ",", "be\u00b7trau\u00b7ren", "mich", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "ART", "NN", "NE", "$,", "VVFIN", "PPER", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.9": {"line.1": {"text": "Sie, die Helden, eilen dir entgegen", "tokens": ["Sie", ",", "die", "Hel\u00b7den", ",", "ei\u00b7len", "dir", "ent\u00b7ge\u00b7gen"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "$,", "ART", "NN", "$,", "VVFIN", "PPER", "PTKVZ"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Unter Donner und der Kugeln Regen,", "tokens": ["Un\u00b7ter", "Don\u00b7ner", "und", "der", "Ku\u00b7geln", "Re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Krieger zittern vor dem Tode nicht \u2013", "tokens": ["Krie\u00b7ger", "zit\u00b7tern", "vor", "dem", "To\u00b7de", "nicht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "PTKNEG", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Ihm entgegen gehen wir mit Hohne", "tokens": ["Ihm", "ent\u00b7ge\u00b7gen", "ge\u00b7hen", "wir", "mit", "Hoh\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "PTKVZ", "VVFIN", "PPER", "APPR", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Unterm Dampf der br\u00fcllenden Kanone,", "tokens": ["Un\u00b7term", "Dampf", "der", "br\u00fcl\u00b7len\u00b7den", "Ka\u00b7no\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Wann er rei\u00dfend durch die Glieder bricht \u2013", "tokens": ["Wann", "er", "rei\u00b7\u00dfend", "durch", "die", "Glie\u00b7der", "bricht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.10": {"line.1": {"text": "Und dann droben finden wir dich wieder,", "tokens": ["Und", "dann", "dro\u00b7ben", "fin\u00b7den", "wir", "dich", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "VVFIN", "PPER", "PRF", "ADV", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Legen dort das m\u00fcde Eisen nieder,", "tokens": ["Le\u00b7gen", "dort", "das", "m\u00fc\u00b7de", "Ei\u00b7sen", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Dr\u00fccken dich an unsre warme Brust,", "tokens": ["Dr\u00fc\u00b7cken", "dich", "an", "uns\u00b7re", "war\u00b7me", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Dann wird alles, wie von Morgenwinden", "tokens": ["Dann", "wird", "al\u00b7les", ",", "wie", "von", "Mor\u00b7gen\u00b7win\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PIS", "$,", "PWAV", "APPR", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Weggeweht, ein leichter Traum, verschwinden", "tokens": ["Weg\u00b7ge\u00b7weht", ",", "ein", "leich\u00b7ter", "Traum", ",", "ver\u00b7schwin\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "$,", "ART", "ADJA", "NN", "$,", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Und nichts bleiben als die Lust.", "tokens": ["Und", "nichts", "blei\u00b7ben", "als", "die", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVINF", "KOKOM", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Grimmig wirgt der Tod durch unsre Glieder! \u2013", "tokens": ["Grim\u00b7mig", "wirgt", "der", "Tod", "durch", "uns\u00b7re", "Glie\u00b7der", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Dumpfig heult die Leichendrummel wieder,", "tokens": ["Dump\u00b7fig", "heult", "die", "Lei\u00b7chen\u00b7drum\u00b7mel", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Schon ein neuer ist hinweggerafft;", "tokens": ["Schon", "ein", "neu\u00b7er", "ist", "hin\u00b7weg\u00b7ge\u00b7rafft", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Mit gesenktem Schie\u00dfgewehre wanken", "tokens": ["Mit", "ge\u00b7senk\u00b7tem", "Schie\u00df\u00b7ge\u00b7weh\u00b7re", "wan\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Graue Krieger nach des Kirchhofs Schranken,", "tokens": ["Grau\u00b7e", "Krie\u00b7ger", "nach", "des", "Kirch\u00b7hofs", "Schran\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Wo der tapfre, brave M\u00fcller schlaft.", "tokens": ["Wo", "der", "tapf\u00b7re", ",", "bra\u00b7ve", "M\u00fcl\u00b7ler", "schlaft", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "$,", "VVFIN", "NE", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.12": {"line.1": {"text": "Br\u00fcder, kommt! \u2013 erblasset! \u2013 schauert! zittert!", "tokens": ["Br\u00fc\u00b7der", ",", "kommt", "!", "\u2013", "er\u00b7blas\u00b7set", "!", "\u2013", "schau\u00b7ert", "!", "zit\u00b7tert", "!"], "token_info": ["word", "punct", "word", "punct", "punct", "word", "punct", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$.", "$(", "VVFIN", "$.", "$(", "VVFIN", "$.", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Bebe jetzt, den niemals nichts ersch\u00fcttert,", "tokens": ["Be\u00b7be", "jetzt", ",", "den", "nie\u00b7mals", "nichts", "er\u00b7sch\u00fct\u00b7tert", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "PRELS", "ADV", "PIS", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Grabgef\u00fchle schauern durch sein Mark.", "tokens": ["Grab\u00b7ge\u00b7f\u00fch\u00b7le", "schau\u00b7ern", "durch", "sein", "Mark", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Sehet! Alles, was wir Leben hie\u00dfen,", "tokens": ["Se\u00b7het", "!", "Al\u00b7les", ",", "was", "wir", "Le\u00b7ben", "hie\u00b7\u00dfen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "PIS", "$,", "PRELS", "PPER", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Was wir liebten, was wir selig priesen,", "tokens": ["Was", "wir", "lieb\u00b7ten", ",", "was", "wir", "se\u00b7lig", "prie\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "PRELS", "PPER", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Liegt vereitelt in dem schmalen Sarg.", "tokens": ["Liegt", "ver\u00b7ei\u00b7telt", "in", "dem", "schma\u00b7len", "Sarg", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.13": {"line.1": {"text": "Von dem Antlitz alles Rot gesunken,", "tokens": ["Von", "dem", "Ant\u00b7litz", "al\u00b7les", "Rot", "ge\u00b7sun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PIAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Aus den Augen alle Lebensfunken", "tokens": ["Aus", "den", "Au\u00b7gen", "al\u00b7le", "Le\u00b7bens\u00b7fun\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PIAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Weggel\u00f6schet in chaotsche Nacht \u2013", "tokens": ["Weg\u00b7ge\u00b7l\u00f6\u00b7schet", "in", "chaot\u00b7sche", "Nacht", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$("], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Seine Mienen, sein holdselig L\u00e4cheln", "tokens": ["Sei\u00b7ne", "Mie\u00b7nen", ",", "sein", "hold\u00b7se\u00b7lig", "L\u00e4\u00b7cheln"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "ADJD", "NN"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Weggeblasen mit dem Sterber\u00f6cheln,", "tokens": ["Weg\u00b7ge\u00b7bla\u00b7sen", "mit", "dem", "Ster\u00b7be\u00b7r\u00f6\u00b7cheln", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Ewig, ewig nimmer angefacht! \u2013", "tokens": ["E\u00b7wig", ",", "e\u00b7wig", "nim\u00b7mer", "an\u00b7ge\u00b7facht", "!", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "$,", "ADJD", "ADV", "VVPP", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.14": {"line.1": {"text": "Nie vom Sturm der Leidenschaft durchw\u00fchlet,", "tokens": ["Nie", "vom", "Sturm", "der", "Lei\u00b7den\u00b7schaft", "durch\u00b7w\u0