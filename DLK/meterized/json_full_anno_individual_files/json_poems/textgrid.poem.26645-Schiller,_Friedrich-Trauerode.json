KON", "ADV", "APPR", "ART", "NN", "VVFIN", "PPER", "PPER", "$."], "meter": "----+-+-+", "measure": "unknown.measure.tri"}}, "stanza.15": {"line.1": {"text": "Auf des Menschen kaltem, starrem Rumpfe", "tokens": ["Auf", "des", "Men\u00b7schen", "kal\u00b7tem", ",", "star\u00b7rem", "Rump\u00b7fe"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "$,", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Sterben seine wirblende Triumphe,", "tokens": ["Ster\u00b7ben", "sei\u00b7ne", "wir\u00b7blen\u00b7de", "Tri\u00b7um\u00b7phe", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "R\u00f6chlen all in ein Gewimmer aus \u2013", "tokens": ["R\u00f6ch\u00b7len", "all", "in", "ein", "Ge\u00b7wim\u00b7mer", "aus", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "APPR", "ART", "NN", "PTKVZ", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Gl\u00fcck und Ruhm zerflattern auf dem Sarge,", "tokens": ["Gl\u00fcck", "und", "Ruhm", "zer\u00b7flat\u00b7tern", "auf", "dem", "Sar\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "K\u00f6nige und Bettler, Feige, Starke", "tokens": ["K\u00f6\u00b7ni\u00b7ge", "und", "Bett\u00b7ler", ",", "Fei\u00b7ge", ",", "Star\u00b7ke"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["NN", "KON", "NN", "$,", "NN", "$,", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Ziehn hinunter in das Totenhaus.", "tokens": ["Ziehn", "hin\u00b7un\u00b7ter", "in", "das", "To\u00b7ten\u00b7haus", "."], 