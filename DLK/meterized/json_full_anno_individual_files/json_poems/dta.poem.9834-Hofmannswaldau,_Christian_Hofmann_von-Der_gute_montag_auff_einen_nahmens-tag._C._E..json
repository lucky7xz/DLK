{"dta.poem.9834": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Der gute montag auff einen  \n nahmens-tag.  \n C. E.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Verg\u00f6nne/ da\u00df wir uns bey deinem nahmens-fest/", "tokens": ["Ver\u00b7g\u00f6n\u00b7ne", "/", "da\u00df", "wir", "uns", "bey", "dei\u00b7nem", "nah\u00b7mens\u00b7fest", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "KOUS", "PPER", "PRF", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du f\u00f6rdrer unsers gl\u00fccks/ ein wenig lustig machen/", "tokens": ["Du", "f\u00f6rd\u00b7rer", "un\u00b7sers", "gl\u00fccks", "/", "ein", "we\u00b7nig", "lus\u00b7tig", "ma\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "PPOSAT", "NN", "$(", "ART", "PIS", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und mit erfreute", "tokens": ["Und", "mit", "er\u00b7freu\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["KON", "APPR", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Die heute \u00fcber uns der himmel kommen l\u00e4\u00dft.", "tokens": ["Die", "heu\u00b7te", "\u00fc\u00b7ber", "uns", "der", "him\u00b7mel", "kom\u00b7men", "l\u00e4\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "PPER", "ART", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Denn da wir gantz vergn\u00fcgt/ von keiner noth gepre\u00dft/", "tokens": ["Denn", "da", "wir", "gantz", "ver\u00b7gn\u00fcgt", "/", "von", "kei\u00b7ner", "noth", "ge\u00b7pre\u00dft", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "VVPP", "$(", "APPR", "PIAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Noch \u00fcber unserm kopff sehn blitz und donner krachen;", "tokens": ["Noch", "\u00fc\u00b7ber", "un\u00b7serm", "kopff", "sehn", "blitz", "und", "don\u00b7ner", "kra\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "VVINF", "VVIMP", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wie solten wir denn nicht bey so gestalten sachen", "tokens": ["Wie", "sol\u00b7ten", "wir", "denn", "nicht", "bey", "so", "ge\u00b7stal\u00b7ten", "sa\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "PPER", "ADV", "PTKNEG", "APPR", "ADV", "ADJA", "NN"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Heut deinen nahmens-tag begehn auffs allerbest?", "tokens": ["Heut", "dei\u00b7nen", "nah\u00b7mens\u00b7tag", "be\u00b7gehn", "auffs", "al\u00b7ler\u00b7best", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN", "APPRART", "ADV", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Es kan nicht anders seyn: wir legen wunsch und sinn", "tokens": ["Es", "kan", "nicht", "an\u00b7ders", "seyn", ":", "wir", "le\u00b7gen", "wunsch", "und", "sinn"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PTKNEG", "ADV", "VAINF", "$.", "PPER", "VVFIN", "ADJD", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "F\u00fcr deines hauses gl\u00fcck/ und deine wolfahrt hin:", "tokens": ["F\u00fcr", "dei\u00b7nes", "hau\u00b7ses", "gl\u00fcck", "/", "und", "dei\u00b7ne", "wol\u00b7fahrt", "hin", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$(", "KON", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Der montag m\u00fcsse sich dir \u00f6ffters noch verneuren/", "tokens": ["Der", "mon\u00b7tag", "m\u00fcs\u00b7se", "sich", "dir", "\u00f6ff\u00b7ters", "noch", "ver\u00b7neu\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PRF", "PPER", "ADV", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und wie der volle mond/ dir zeigen licht und schein;", "tokens": ["Und", "wie", "der", "vol\u00b7le", "mond", "/", "dir", "zei\u00b7gen", "licht", "und", "schein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "ADJA", "NN", "$(", "PPER", "VVFIN", "NN", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Er m\u00fcsse dir und uns ein guter montag seyn/", "tokens": ["Er", "m\u00fcs\u00b7se", "dir", "und", "uns", "ein", "gu\u00b7ter", "mon\u00b7tag", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "KON", "PPER", "ART", "ADJA", "NN", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Der sich/ so offt er kommt/ mit jauchzen lasse feyren.", "tokens": ["Der", "sich", "/", "so", "offt", "er", "kommt", "/", "mit", "jauch\u00b7zen", "las\u00b7se", "fey\u00b7ren", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "$(", "ADV", "ADV", "PPER", "VVFIN", "$(", "APPR", "VVINF", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}