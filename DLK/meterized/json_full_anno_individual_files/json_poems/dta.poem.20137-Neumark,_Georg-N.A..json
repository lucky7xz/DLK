 "token_info": ["word", "word", "XML_entity", "word", "word", "word", "word"], "pos": ["ART", "NE", "$(", "ADJD", "NN", "VVFIN", "PTKNEG"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "darinnen.", "tokens": ["da\u00b7rin\u00b7nen", "."], "token_info": ["word", "punct"], "pos": ["ADV", "$."], "meter": "-+-", "measure": "amphibrach.single"}}}}}