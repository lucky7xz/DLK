{"dta.poem.6387": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Geschmack.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Allein, indem wir immer weiter", "tokens": ["Al\u00b7lein", ",", "in\u00b7dem", "wir", "im\u00b7mer", "wei\u00b7ter"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "PPER", "ADV", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Auf unsrer einst erkohrnen Bahne gehn,", "tokens": ["Auf", "uns\u00b7rer", "einst", "er\u00b7kohr\u00b7nen", "Bah\u00b7ne", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADV", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So scheint, es werden glatt und eben alle H\u00f6hn.", "tokens": ["So", "scheint", ",", "es", "wer\u00b7den", "glatt", "und", "e\u00b7ben", "al\u00b7le", "H\u00f6hn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PPER", "VAFIN", "ADJD", "KON", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Was schwer ist, l\u00f6st sich auf. Um uns wird alles heiter,", "tokens": ["Was", "schwer", "ist", ",", "l\u00f6st", "sich", "auf", ".", "Um", "uns", "wird", "al\u00b7les", "hei\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VAFIN", "$,", "VVFIN", "PRF", "PTKVZ", "$.", "KOUI", "PPER", "VAFIN", "PIS", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Lasst uns die S\u00e4ffte doch betrachten und probiren,", "tokens": ["Lasst", "uns", "die", "S\u00e4ff\u00b7te", "doch", "be\u00b7trach\u00b7ten", "und", "pro\u00b7bi\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADV", "VVINF", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Woher die W\u00fcrcknngen, die so verschiedlich, r\u00fchren.", "tokens": ["Wo\u00b7her", "die", "W\u00fcr\u00b7cknn\u00b7gen", ",", "die", "so", "ver\u00b7schied\u00b7lich", ",", "r\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "PRELS", "ADV", "ADJD", "$,", "VVINF", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}}}}}