{"textgrid.poem.65533": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "6. Narrenstolz", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O sagt, warum die stolzen Narren so m\u00fcrrisch durch die Stra\u00dfen gehn,", "tokens": ["O", "sagt", ",", "wa\u00b7rum", "die", "stol\u00b7zen", "Nar\u00b7ren", "so", "m\u00fcr\u00b7risch", "durch", "die", "Stra\u00b7\u00dfen", "gehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "PWAV", "ART", "ADJA", "NN", "ADV", "ADJD", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Warum sie bald erbo\u00dft zu Boden, und bald ergrimmt gen Himmel sehn?", "tokens": ["Wa\u00b7rum", "sie", "bald", "er\u00b7bo\u00dft", "zu", "Bo\u00b7den", ",", "und", "bald", "er\u00b7grimmt", "gen", "Him\u00b7mel", "sehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VVFIN", "APPR", "NN", "$,", "KON", "ADV", "VVPP", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.3": {"text": "\u00bbdort will das Pflaster sich vor ihnen noch nicht erheben mit Respekt,", "tokens": ["\u00bb", "dort", "will", "das", "Pflas\u00b7ter", "sich", "vor", "ih\u00b7nen", "noch", "nicht", "er\u00b7he\u00b7ben", "mit", "Res\u00b7pekt", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VMFIN", "ART", "NN", "PRF", "APPR", "PPER", "ADV", "PTKNEG", "VVINF", "APPR", "NN", "$,"], "meter": "-+-+-+-+--+-+--+-", "measure": "iambic.septa.relaxed