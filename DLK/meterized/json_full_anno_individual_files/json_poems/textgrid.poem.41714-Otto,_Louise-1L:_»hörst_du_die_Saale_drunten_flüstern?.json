sure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "Ach, wie wir damals uns umschlungen", "tokens": ["Ach", ",", "wie", "wir", "da\u00b7mals", "uns", "um\u00b7schlun\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PWAV", "PPER", "ADV", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "H\u00e4ttst \u00bbSchw\u00e4rmrin\u00ab! du mich nicht genannt", "tokens": ["H\u00e4ttst", "\u00bb", "Schw\u00e4rm\u00b7rin", "\u00ab", "!", "du", "mich", "nicht", "ge\u00b7nannt"], "token_info": ["word", "punct", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$(", "NN", "$(", "$.", "PPER", "PRF", "PTKNEG", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So w\u00e4ren wir hinabgesprungen", "tokens": ["So", "w\u00e4\u00b7ren", "wir", "hin\u00b7ab\u00b7ge\u00b7sprun\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und h\u00e4tten Trennung nie gekannt.", "tokens": ["Und", "h\u00e4t\u00b7ten", "Tren\u00b7nung", "nie", "ge\u00b7kannt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.15": {"line.1": {"text": "So w\u00e4r ich nicht allein geblieben", "tokens": ["So", "w\u00e4r", "ich", "nicht", "al\u00b7lein", "ge\u00b7blie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PTKNEG", "ADV", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In dieser kalten, \u00f6den Welt,", "tokens": ["In", "die\u00b7ser", "kal\u00b7ten", ",", "\u00f6\u00b7den", "Welt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "$,", "ADJA", "NN", "$,"], "mete