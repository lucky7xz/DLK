{"textgrid.poem.48871": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "96. An seinen Ring", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der sch\u00f6ne Namenstag der Liebsten ist erschienen,", "tokens": ["Der", "sch\u00f6\u00b7ne", "Na\u00b7mens\u00b7tag", "der", "Liebs\u00b7ten", "ist", "er\u00b7schie\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+---+-+-+-", "measure": "unknown.measure.penta"}, "line.2": {"text": "die Anmut macht mich froh, die aus der halben Nacht", "tokens": ["die", "An\u00b7mut", "macht", "mich", "froh", ",", "die", "aus", "der", "hal\u00b7ben", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "$,", "PRELS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "ganz wie der Lilgen Milch und Blut der Rosen lacht,", "tokens": ["ganz", "wie", "der", "Lil\u00b7gen", "Milch", "und", "Blut", "der", "Ro\u00b7sen", "lacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "NN", "NN", "KON", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "mit Safran angemischt. Ihr m\u00fcsset euch erk\u00fchnen", "tokens": ["mit", "Saf\u00b7ran", "an\u00b7ge\u00b7mischt", ".", "Ihr", "m\u00fcs\u00b7set", "euch", "er\u00b7k\u00fch\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVPP", "$.", "PPER", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "zu wagen einen Gang, ihr funkelnden Rubinen,", "tokens": ["zu", "wa\u00b7gen", "ei\u00b7nen", "Gang", ",", "ihr", "fun\u00b7keln\u00b7den", "Ru\u00b7bi\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ART", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "eilt, eh das sch\u00f6ne Kind von ihrer Ruh' erwacht,", "tokens": ["eilt", ",", "eh", "das", "sch\u00f6\u00b7ne", "Kind", "von", "ih\u00b7rer", "Ruh'", "er\u00b7wacht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "und sehet, wie ihr euch an ihren Fingern macht!", "tokens": ["und", "se\u00b7het", ",", "wie", "ihr", "euch", "an", "ih\u00b7ren", "Fin\u00b7gern", "macht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWAV", "PPER", "PRF", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So wird ihr sanfter Schlaf zu eurem Vorteil dienen.", "tokens": ["So", "wird", "ihr", "sanf\u00b7ter", "Schlaf", "zu", "eu\u00b7rem", "Vor\u00b7teil", "die\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Geht, bindet sie also! Wie aber, wollt ihr nicht?", "tokens": ["Geht", ",", "bin\u00b7det", "sie", "al\u00b7so", "!", "Wie", "a\u00b7ber", ",", "wollt", "ihr", "nicht", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "PPER", "ADV", "$.", "PWAV", "ADV", "$,", "VMFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie werdet ihr so bla\u00df um euer Angesicht'", "tokens": ["Wie", "wer\u00b7det", "ihr", "so", "bla\u00df", "um", "eu\u00b7er", "An\u00b7ge\u00b7sicht'"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "und was verstellt ihr euch in sterbende Geberden?", "tokens": ["und", "was", "ver\u00b7stellt", "ihr", "euch", "in", "ster\u00b7ben\u00b7de", "Ge\u00b7ber\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVFIN", "PPER", "PRF", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+--", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Ists etwan, da\u00df ihr meint, wo sie schon sei erwacht,", "tokens": ["Ists", "et\u00b7wan", ",", "da\u00df", "ihr", "meint", ",", "wo", "sie", "schon", "sei", "er\u00b7wacht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "$,", "KOUS", "PPER", "VVFIN", "$,", "PWAV", "PPER", "ADV", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "ihr m\u00f6chtet schamrot stehn f\u00fcr ihrer Lippen Pracht", "tokens": ["ihr", "m\u00f6ch\u00b7tet", "scham\u00b7rot", "stehn", "f\u00fcr", "ih\u00b7rer", "Lip\u00b7pen", "Pracht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADJD", "VVFIN", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-