{"dta.poem.643": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Eines ungl\u00fccklichen Verliebten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Leser stelle doch das Gehen etwas ein/", "tokens": ["Mein", "Le\u00b7ser", "stel\u00b7le", "doch", "das", "Ge\u00b7hen", "et\u00b7was", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "ART", "NN", "ADV", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mir raubte/ da ich war/ ein steinern Hertz die Ruh/", "tokens": ["Mir", "raub\u00b7te", "/", "da", "ich", "war", "/", "ein", "stei\u00b7nern", "Hertz", "die", "Ruh", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "KOUS", "PPER", "VAFIN", "$(", "ART", "ADJA", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und jetzo giebt der Tod mir einen Stein dazu/", "tokens": ["Und", "jet\u00b7zo", "giebt", "der", "Tod", "mir", "ei\u00b7nen", "Stein", "da\u00b7zu", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "PPER", "ART", "NN", "PAV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So mu\u00df ich/ weil ich war/ und todt bey Steinen seyn.", "tokens": ["So", "mu\u00df", "ich", "/", "weil", "ich", "war", "/", "und", "todt", "bey", "Stei\u00b7nen", "seyn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "$(", "KOUS", "PPER", "VAFIN", "$(", "KON", "ADJD", "APPR", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}