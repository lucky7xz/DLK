, "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Als er nach schwerem Leyden", "tokens": ["Als", "er", "nach", "schwe\u00b7rem", "Ley\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Auch mit dem Tode rangk,", "tokens": ["Auch", "mit", "dem", "To\u00b7de", "rangk", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "Und, must' er gleich verscheiden,", "tokens": ["Und", ",", "must'", "er", "gleich", "ver\u00b7schei\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der H\u00f6llen Reich bezwangk.", "tokens": ["Der", "H\u00f6l\u00b7len", "Reich", "be\u00b7zwangk", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Herr Jesu, da\u00df ich lebe", "tokens": ["Herr", "Je\u00b7su", ",", "da\u00df", "ich", "le\u00b7be"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "NE", "$,", "KOUS", "PPER", "VVFIN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Ohn Schrecken Furcht und Pein", "tokens": ["Ohn", "Schre\u00b7cken", "Furcht", "und", "Pein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und ewig umb dich schwebe,", "tokens": ["Und", "e\u00b7wig", "umb", "dich", "schwe\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das ist dein Werck allein,", "tokens": ["Das", "ist", "dein", "Werck", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "O sey hief\u00fcr gesungen,", "tokens": ["O", "sey", "hie\u00b7f\u00fcr", "ge\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PAV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Dich loben weit und breit", "tokens": ["Dich", "lo\u00b7ben", "weit", "und", "breit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Der Au\u00dferwehlten Zungen", "tokens": ["Der", "Au\u00b7\u00dfer\u00b7wehl\u00b7ten", "Zun\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "In alle Ewigheit.", "tokens": ["In", "al\u00b7le", "E\u00b7wig\u00b7heit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}