{"dta.poem.18826": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Sie haben wegen der Trunkenheit", "tokens": ["Sie", "ha\u00b7ben", "we\u00b7gen", "der", "Trun\u00b7ken\u00b7heit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Vielf\u00e4ltig uns verklagt,", "tokens": ["Viel\u00b7f\u00e4l\u00b7tig", "uns", "ver\u00b7klagt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und haben von der Trunkenheit", "tokens": ["Und", "ha\u00b7ben", "von", "der", "Trun\u00b7ken\u00b7heit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Lange nicht genug gesagt.", "tokens": ["Lan\u00b7ge", "nicht", "ge\u00b7nug", "ge\u00b7sagt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Gew\u00f6hnlich die Betrunkenheit", "tokens": ["Ge\u00b7w\u00f6hn\u00b7lich", "die", "Be\u00b7trun\u00b7ken\u00b7heit"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Verschwindet so wie es tagt;", "tokens": ["Ver\u00b7schwin\u00b7det", "so", "wie", "es", "tagt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "KOKOM", "PPER", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Doch hat mich meine Betrunkenheit", "tokens": ["Doch", "hat", "mich", "mei\u00b7ne", "Be\u00b7trun\u00b7ken\u00b7heit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "In der Nacht umher gejagt.", "tokens": ["In", "der", "Nacht", "um\u00b7her", "ge\u00b7jagt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Es ist die Liebestrunkenheit", "tokens": ["Es", "ist", "die", "Lie\u00b7be\u00b7strun\u00b7ken\u00b7heit"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Die mich erb\u00e4rmlich plagt,", "tokens": ["Die", "mich", "er\u00b7b\u00e4rm\u00b7lich", "plagt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Von Tag zu Nacht, von Nacht zu Tag", "tokens": ["Von", "Tag", "zu", "Nacht", ",", "von", "Nacht", "zu", "Tag"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "APPR", "NN", "$,", "APPR", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "In meinem Herzen zagt.", "tokens": ["In", "mei\u00b7nem", "Her\u00b7zen", "zagt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Dem Herzen das in Trunkenheit", "tokens": ["Dem", "Her\u00b7zen", "das", "in", "Trun\u00b7ken\u00b7heit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Der Lieder schwillt und ragt,", "tokens": ["Der", "Lie\u00b7der", "schwillt", "und", "ragt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.15": {"text": "Dass keine n\u00fcchterne Trunkenheit", "tokens": ["Dass", "kei\u00b7ne", "n\u00fcch\u00b7ter\u00b7ne", "Trun\u00b7ken\u00b7heit"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PIAT", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.16": {"text": "Sich gleich zu heben wagt.", "tokens": ["Sich", "gleich", "zu", "he\u00b7ben", "wagt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.17": {"text": "Lieb\u2019, Lied und Weines Trunkenheit,", "tokens": ["Lieb'", ",", "Lied", "und", "Wei\u00b7nes", "Trun\u00b7ken\u00b7heit", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Ob\u2019s nachtet oder tagt,", "tokens": ["Ob's", "nach\u00b7tet", "o\u00b7der", "tagt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVPP", "KON", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.19": {"text": "Die g\u00f6ttlichste Betrunkenheit", "tokens": ["Die", "g\u00f6tt\u00b7lichs\u00b7te", "Be\u00b7trun\u00b7ken\u00b7heit"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Die mich entz\u00fcckt und plagt.", "tokens": ["Die", "mich", "ent\u00b7z\u00fcckt", "und", "plagt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}