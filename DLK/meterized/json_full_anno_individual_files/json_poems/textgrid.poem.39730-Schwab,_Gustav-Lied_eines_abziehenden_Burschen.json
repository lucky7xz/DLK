{"textgrid.poem.39730": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Lied eines abziehenden Burschen", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bemooster Bursche zieh' ich aus,", "tokens": ["Be\u00b7moos\u00b7ter", "Bur\u00b7sche", "zieh'", "ich", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Beh\u00fct dich Gott, Philisters Haus!", "tokens": ["Be\u00b7h\u00fct", "dich", "Gott", ",", "Phi\u00b7lis\u00b7ters", "Haus", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$,", "NE", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zur alten Heimat geh' ich ein,", "tokens": ["Zur", "al\u00b7ten", "Hei\u00b7mat", "geh'", "ich", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mu\u00df selber nun Philister sein.", "tokens": ["Mu\u00df", "sel\u00b7ber", "nun", "Phi\u00b7lis\u00b7ter", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ADV", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Fahrt wohl, ihr Stra\u00dfen grad und krumm,", "tokens": ["Fahrt", "wohl", ",", "ihr", "Stra\u00b7\u00dfen", "grad", "und", "krumm", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "PPOSAT", "NN", "ADV", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich zieh' nicht mehr in euch herum,", "tokens": ["Ich", "zieh'", "nicht", "mehr", "in", "euch", "he\u00b7rum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Durcht\u00f6n' euch nicht mehr mit Gesang,", "tokens": ["Durch\u00b7t\u00f6n'", "euch", "nicht", "mehr", "mit", "Ge\u00b7sang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKNEG", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit L\u00e4rm nicht mehr und Sporenklang.", "tokens": ["Mit", "L\u00e4rm", "nicht", "mehr", "und", "Spo\u00b7ren\u00b7klang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKNEG", "ADV", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Was wollt ihr Kneipen all' von mir?", "tokens": ["Was", "wollt", "ihr", "Knei\u00b7pen", "all'", "von", "mir", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPOSAT", "NN", "PIS", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Bleiben ist nicht mehr allhier,", "tokens": ["Mein", "Blei\u00b7ben", "ist", "nicht", "mehr", "all\u00b7hier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PTKNEG", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Winkt nicht mit eurem langen Arm,", "tokens": ["Winkt", "nicht", "mit", "eu\u00b7rem", "lan\u00b7gen", "Arm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Macht mir mein durstig Herz nicht warm.", "tokens": ["Macht", "mir", "mein", "durs\u00b7tig", "Herz", "nicht", "warm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPOSAT", "ADJD", "NN", "PTKNEG", "ADJD", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.4": {"line.1": {"text": "Ei gr\u00fc\u00df' euch Gott, Collegia!", "tokens": ["Ei", "gr\u00fc\u00df'", "euch", "Gott", ",", "Col\u00b7le\u00b7gia", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "NN", "$,", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie steht ihr in Parade da.", "tokens": ["Wie", "steht", "ihr", "in", "Pa\u00b7ra\u00b7de", "da", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr dumpfen S\u00e4le gro\u00df und klein,", "tokens": ["Ihr", "dum\u00b7pfen", "S\u00e4\u00b7le", "gro\u00df", "und", "klein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Jetzt kriegt ihr mich nicht mehr herein.", "tokens": ["Jetzt", "kriegt", "ihr", "mich", "nicht", "mehr", "her\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "PTKNEG", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Auch du von deinem Giebeldach", "tokens": ["Auch", "du", "von", "dei\u00b7nem", "Gie\u00b7bel\u00b7dach"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Siehst mir umsonst, o Carcer, nach.", "tokens": ["Siehst", "mir", "um\u00b7sonst", ",", "o", "Car\u00b7cer", ",", "nach", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "FM", "NN", "$,", "APPR", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "F\u00fcr schlechte Herberg, Tag und Nacht,", "tokens": ["F\u00fcr", "schlech\u00b7te", "Her\u00b7berg", ",", "Tag", "und", "Nacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sei dir ein Pereat gebracht!", "tokens": ["Sei", "dir", "ein", "Pe\u00b7reat", "ge\u00b7bracht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Du aber bl\u00fch' und schalle noch,", "tokens": ["Du", "a\u00b7ber", "bl\u00fch'", "und", "schal\u00b7le", "noch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "KON", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Leb' alter Waffenboden hoch!", "tokens": ["Leb'", "al\u00b7ter", "Waf\u00b7fen\u00b7bo\u00b7den", "hoch", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es st\u00e4rkt den Geist die Wissenschaft,", "tokens": ["Es", "st\u00e4rkt", "den", "Geist", "die", "Wis\u00b7sen\u00b7schaft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So st\u00e4rke du des Armes Kraft.", "tokens": ["So", "st\u00e4r\u00b7ke", "du", "des", "Ar\u00b7mes", "Kraft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Da komm' ich, ach an Liebchens Haus:", "tokens": ["Da", "komm'", "ich", ",", "ach", "an", "Lieb\u00b7chens", "Haus", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADV", "APPR", "NE", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "O Kind, schau noch einmal heraus!", "tokens": ["O", "Kind", ",", "schau", "noch", "ein\u00b7mal", "he\u00b7raus", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ADJD", "ADV", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Heraus mit deinen Aeuglein klar,", "tokens": ["He\u00b7raus", "mit", "dei\u00b7nen", "A\u00b7e\u00b7u\u00b7glein", "klar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mit deinem dunkeln Lockenhaar!", "tokens": ["Mit", "dei\u00b7nem", "dun\u00b7keln", "Lo\u00b7cken\u00b7haar", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Und hast du mich vergessen schon,", "tokens": ["Und", "hast", "du", "mich", "ver\u00b7ges\u00b7sen", "schon", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PRF", "VVPP", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So w\u00fcnsch' ich dir nicht b\u00f6sen Lohn;", "tokens": ["So", "w\u00fcn\u00b7sch'", "ich", "dir", "nicht", "b\u00f6\u00b7sen", "Lohn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "PTKNEG", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Such' dir nur einen Buhlen neu,", "tokens": ["Such'", "dir", "nur", "ei\u00b7nen", "Buh\u00b7len", "neu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch sei er flott gleich mir und treu!", "tokens": ["Doch", "sei", "er", "flott", "gleich", "mir", "und", "treu", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "VVFIN", "ADV", "PPER", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Und weiter, weiter geht mein Lauf,", "tokens": ["Und", "wei\u00b7ter", ",", "wei\u00b7ter", "geht", "mein", "Lauf", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Thut euch, ihr alten Thore, auf!", "tokens": ["Thut", "euch", ",", "ihr", "al\u00b7ten", "Tho\u00b7re", ",", "auf", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPOSAT", "ADJA", "NN", "$,", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Leicht ist mein Sinn, und frei mein Pfad,", "tokens": ["Leicht", "ist", "mein", "Sinn", ",", "und", "frei", "mein", "Pfad", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPOSAT", "NN", "$,", "KON", "ADJD", "PPOSAT", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Gehab dich wohl, du Musenstadt!", "tokens": ["Ge\u00b7hab", "dich", "wohl", ",", "du", "Mu\u00b7sen\u00b7stadt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "$,", "PPER", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Ihr Freunde, dr\u00e4ngt euch um mich her,", "tokens": ["Ihr", "Freun\u00b7de", ",", "dr\u00e4ngt", "euch", "um", "mich", "her", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "PPER", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Macht mir mein leichtes Herz nicht schwer,", "tokens": ["Macht", "mir", "mein", "leich\u00b7tes", "Herz", "nicht", "schwer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPOSAT", "ADJA", "NN", "PTKNEG", "ADJD", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Auf frischem Ro\u00df, mit frohem Sang", "tokens": ["Auf", "fri\u00b7schem", "Ro\u00df", ",", "mit", "fro\u00b7hem", "Sang"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Geleitet mich den Weg entlang.", "tokens": ["Ge\u00b7lei\u00b7tet", "mich", "den", "Weg", "ent\u00b7lang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Im n\u00e4chsten Dorfe kehret ein,", "tokens": ["Im", "n\u00e4chs\u00b7ten", "Dor\u00b7fe", "keh\u00b7ret", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Trinkt noch mit mir von einem Wein. \u2013", "tokens": ["Trinkt", "noch", "mit", "mir", "von", "ei\u00b7nem", "Wein", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PPER", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und nun denn, Br\u00fcder, sei's weil's mu\u00df!", "tokens": ["Und", "nun", "denn", ",", "Br\u00fc\u00b7der", ",", "sei's", "weil's", "mu\u00df", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "$,", "NN", "$,", "VAFIN", "PIS", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das letzte Glas, den letzten Ku\u00df!", "tokens": ["Das", "letz\u00b7te", "Glas", ",", "den", "letz\u00b7ten", "Ku\u00df", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Bemooster Bursche zieh' ich aus,", "tokens": ["Be\u00b7moos\u00b7ter", "Bur\u00b7sche", "zieh'", "ich", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Beh\u00fct dich Gott, Philisters Haus!", "tokens": ["Be\u00b7h\u00fct", "dich", "Gott", ",", "Phi\u00b7lis\u00b7ters", "Haus", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$,", "NE", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zur alten Heimat geh' ich ein,", "tokens": ["Zur", "al\u00b7ten", "Hei\u00b7mat", "geh'", "ich", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word