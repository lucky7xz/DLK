{"dta.poem.6953": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Vereinigung und dem Unter-  \n scheid der Seelen und des C\u00f6rpers.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Inde\u00df setzt Epieur die Seel' aus einem Dufft,", "tokens": ["In\u00b7de\u00df", "setzt", "E\u00b7pie\u00b7ur", "die", "Seel'", "aus", "ei\u00b7nem", "Dufft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Auch, wie es ihm gef\u00e4llet, aus der Lufft,", "tokens": ["Auch", ",", "wie", "es", "ihm", "ge\u00b7f\u00e4l\u00b7let", ",", "aus", "der", "Lufft", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "PPER", "PPER", "VVPP", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Aus Wind und Flammen,", "tokens": ["Aus", "Wind", "und", "Flam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Aus einem Etwas, so ihm unbekannt, zusammen.", "tokens": ["Aus", "ei\u00b7nem", "Et\u00b7was", ",", "so", "ihm", "un\u00b7be\u00b7kannt", ",", "zu\u00b7sam\u00b7men", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "ADV", "$,", "ADV", "PPER", "ADJD", "$,", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ohn das man weiter geht und sinnt.", "tokens": ["Ohn", "das", "man", "wei\u00b7ter", "geht", "und", "sinnt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PIS", "ADV", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In einer Zuversicht, die ja so stoltz, als blind,", "tokens": ["In", "ei\u00b7ner", "Zu\u00b7ver\u00b7sicht", ",", "die", "ja", "so", "stoltz", ",", "als", "blind", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "ADV", "ADV", "ADJD", "$,", "KOUS", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Verficht man ohne Scheu:", "tokens": ["Ver\u00b7ficht", "man", "oh\u00b7ne", "Scheu", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Da\u00df blos Materie, und nichts als C\u00f6rper sey.", "tokens": ["Da\u00df", "blos", "Ma\u00b7te\u00b7rie", ",", "und", "nichts", "als", "C\u00f6r\u00b7per", "sey", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "NN", "$,", "KON", "PIS", "KOKOM", "NE", "VAFIN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.9": {"text": "An den verwegnen S\u00e4tzen,", "tokens": ["An", "den", "ver\u00b7weg\u00b7nen", "S\u00e4t\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "In des Lucretius hochtrabenden Gedichten,", "tokens": ["In", "des", "Lu\u00b7cre\u00b7ti\u00b7us", "hoch\u00b7tra\u00b7ben\u00b7den", "Ge\u00b7dich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NE", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Sucht man begierig sich zu n\u00e4hren, zu erg\u00f6tzen.", "tokens": ["Sucht", "man", "be\u00b7gie\u00b7rig", "sich", "zu", "n\u00e4h\u00b7ren", ",", "zu", "er\u00b7g\u00f6t\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADJD", "PRF", "PTKZU", "VVINF", "$,", "PTKZU", "VVINF", "$."], "meter": "++-+-+-+-+-+-", "measure": "unknown.measure.septa"}, "line.12": {"text": "Man wirfft uns vor den Schlaf, die Trunckenheit.", "tokens": ["Man", "wirfft", "uns", "vor", "den", "Schlaf", ",", "die", "Trun\u00b7cken\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "APPR", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Man spricht: Der Geist-Leib, der sich mit dem C\u00f6rper paart,", "tokens": ["Man", "spricht", ":", "Der", "Geist\u00b7Leib", ",", "der", "sich", "mit", "dem", "C\u00f6r\u00b7per", "paart", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$.", "ART", "NN", "$,", "PRELS", "PRF", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Sey nichts, als d\u00fcnner nur, und von derselben Art,", "tokens": ["Sey", "nichts", ",", "als", "d\u00fcn\u00b7ner", "nur", ",", "und", "von", "der\u00b7sel\u00b7ben", "Art", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "$,", "KOUS", "ADJD", "ADV", "$,", "KON", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Da\u00df er mit ihm zugleich vergeh\u2019 und sich zerft\u00f6hre,", "tokens": ["Da\u00df", "er", "mit", "ihm", "zu\u00b7gleich", "ver\u00b7geh'", "und", "sich", "zer\u00b7ft\u00f6h\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "ADV", "VVFIN", "KON", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Da\u00df ihn desselben Last beschwere,", "tokens": ["Da\u00df", "ihn", "des\u00b7sel\u00b7ben", "Last", "be\u00b7schwe\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Da\u00df er in Kindern wachs\u2019, im Alter sich verliehre,", "tokens": ["Da\u00df", "er", "in", "Kin\u00b7dern", "wachs'", ",", "im", "Al\u00b7ter", "sich", "ver\u00b7lieh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "PTKVZ", "$,", "APPRART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Da\u00df durch des C\u00f6rpers Pein, er Pein und Unmuht sp\u00fcre,", "tokens": ["Da\u00df", "durch", "des", "C\u00f6r\u00b7pers", "Pein", ",", "er", "Pein", "und", "Un\u00b7muht", "sp\u00fc\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN", "$,", "PPER", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Da\u00df einerley Getrieb, auch beyder Thun regiere,", "tokens": ["Da\u00df", "ei\u00b7ner\u00b7ley", "Ge\u00b7trieb", ",", "auch", "bey\u00b7der", "Thun", "re\u00b7gie\u00b7re", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "$,", "ADV", "PIAT", "NN", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Da\u00df gleiche F\u00e4ll\u2019 ihr Amt verhindern und verderben,", "tokens": ["Da\u00df", "glei\u00b7che", "F\u00e4ll'", "ihr", "Amt", "ver\u00b7hin\u00b7dern", "und", "ver\u00b7der\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "PPOSAT", "NN", "VVINF", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Da\u00df sie zu gleicher Zeit auch alle beyde sterben.", "tokens": ["Da\u00df", "sie", "zu", "glei\u00b7cher", "Zeit", "auch", "al\u00b7le", "bey\u00b7de", "ster\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "ADV", "PIAT", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}