{"textgrid.poem.39685": {"metadata": {"author": {"name": "G\u00fcnderode, Karoline von", "birth": "N.A.", "death": "N.A."}, "title": "Zueignung", "genre": "verse", "period": "N.A.", "pub_year": 1793, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich habe Dir in ernsten stillen Stunden,", "tokens": ["Ich", "ha\u00b7be", "Dir", "in", "erns\u00b7ten", "stil\u00b7len", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "APPR", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Betrachtungsvoll in heil'ger Einsamkeit,", "tokens": ["Be\u00b7trach\u00b7tungs\u00b7voll", "in", "heil'\u00b7ger", "Ein\u00b7sam\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Blumen dieser und vergangner Zeit,", "tokens": ["Die", "Blu\u00b7men", "die\u00b7ser", "und", "ver\u00b7gang\u00b7ner", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PDAT", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die mir erbl\u00fcht, zu einem Kranz gewunden.", "tokens": ["Die", "mir", "er\u00b7bl\u00fcht", ",", "zu", "ei\u00b7nem", "Kranz", "ge\u00b7wun\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$,", "APPR", "ART", "NN", "VAPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Von Dir, ich wei\u00df es, wird der Sinn empfunden,", "tokens": ["Von", "Dir", ",", "ich", "wei\u00df", "es", ",", "wird", "der", "Sinn", "emp\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "PPER", "VVFIN", "PPER", "$,", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der in des Bl\u00fcthenkelchs Verschwiegenheit", "tokens": ["Der", "in", "des", "Bl\u00fc\u00b7then\u00b7kelchs", "Ver\u00b7schwie\u00b7gen\u00b7heit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nur sichtbar wird dem Auge, das geweiht", "tokens": ["Nur", "sicht\u00b7bar", "wird", "dem", "Au\u00b7ge", ",", "das", "ge\u00b7weiht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ADJD", "VAFIN", "ART", "NN", "$,", "PRELS", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Im Farbenspiel den stillen Geist gefunden.", "tokens": ["Im", "Far\u00b7ben\u00b7spiel", "den", "stil\u00b7len", "Geist", "ge\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Es flechten M\u00e4dchen so im Orient", "tokens": ["Es", "flech\u00b7ten", "M\u00e4d\u00b7chen", "so", "im", "O\u00b7rient"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "ADV", "APPRART", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Den bunten Kranz; da\u00df vielen er gefalle,", "tokens": ["Den", "bun\u00b7ten", "Kranz", ";", "da\u00df", "vie\u00b7len", "er", "ge\u00b7fal\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "KOUS", "PIS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wetteifern unter sich die Blumen alle.", "tokens": ["Wett\u00b7ei\u00b7fern", "un\u00b7ter", "sich", "die", "Blu\u00b7men", "al\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PRF", "ART", "NN", "PIAT", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Doch Einer ihren tiefern Sinn erkennt,", "tokens": ["Doch", "Ei\u00b7ner", "ih\u00b7ren", "tie\u00b7fern", "Sinn", "er\u00b7kennt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ihm sind Symbole sie nur, \u00e4u\u00dfre Zeichen;", "tokens": ["Ihm", "sind", "Sym\u00b7bo\u00b7le", "sie", "nur", ",", "\u00e4u\u00df\u00b7re", "Zei\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "PPER", "ADV", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sie reden ihm, obgleich sie alle schweigen.", "tokens": ["Sie", "re\u00b7den", "ihm", ",", "ob\u00b7gleich", "sie", "al\u00b7le", "schwei\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KOUS", "PPER", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Ich habe Dir in ernsten stillen Stunden,", "tokens": ["Ich", "ha\u00b7be", "Dir", "in", "erns\u00b7ten", "stil\u00b7len", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "APPR", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Betrachtungsvoll in heil'ger Einsamkeit,", "tokens": ["Be\u00b7trach\u00b7tungs\u00b7voll", "in", "heil'\u00b7ger", "Ein\u00b7sam\u00b7