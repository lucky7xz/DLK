{"textgrid.poem.33720": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Noch spr\u00fcht des l\u00e4ngsten Tages warme Quelle", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Noch spr\u00fcht des l\u00e4ngsten Tages warme Quelle", "tokens": ["Noch", "spr\u00fcht", "des", "l\u00e4ngs\u00b7ten", "Ta\u00b7ges", "war\u00b7me", "Quel\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Lebendig fort, es wagen sich verstohlen", "tokens": ["Le\u00b7ben\u00b7dig", "fort", ",", "es", "wa\u00b7gen", "sich", "ver\u00b7stoh\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "PTKVZ", "$,", "PPER", "VVFIN", "PRF", "VVINF"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Die Tr\u00e4ume nur und nur mit scheuen Sohlen", "tokens": ["Die", "Tr\u00e4u\u00b7me", "nur", "und", "nur", "mit", "scheu\u00b7en", "Soh\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "KON", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Stern' auf dieser Nacht saphirne Schwelle.", "tokens": ["Die", "Stern'", "auf", "die\u00b7ser", "Nacht", "sa\u00b7phir\u00b7ne", "Schwel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PDAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Kaum sank der Abend in die D\u00e4mmerwelle,", "tokens": ["Kaum", "sank", "der", "A\u00b7bend", "in", "die", "D\u00e4m\u00b7mer\u00b7wel\u00b7le", ","], "t