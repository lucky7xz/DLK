{"textgrid.poem.40827": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Im Klosterdome prangt, aus Stein gehauen,", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Klosterdome prangt, aus Stein gehauen,", "tokens": ["Im", "Klos\u00b7ter\u00b7do\u00b7me", "prangt", ",", "aus", "Stein", "ge\u00b7hau\u00b7en", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des Stifters Grabmal, kl\u00e4glich anzuschauen:", "tokens": ["Des", "Stif\u00b7ters", "Grab\u00b7mal", ",", "kl\u00e4g\u00b7lich", "an\u00b7zu\u00b7schau\u00b7en", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ADJD", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein Ritter knieend, Stahl um Brust und Lenden,", "tokens": ["Ein", "Rit\u00b7ter", "kni\u00b7e\u00b7end", ",", "Stahl", "um", "Brust", "und", "Len\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Den Rosenkranz fest in gefalt'nen H\u00e4nden.", "tokens": ["Den", "Ro\u00b7sen\u00b7kranz", "fest", "in", "ge\u00b7falt'\u00b7nen", "H\u00e4n\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Vor ihm liegt ein Brevier und Todtensch\u00e4del,", "tokens": ["Vor", "ihm", "liegt", "ein", "Bre\u00b7vier", "und", "Tod\u00b7ten\u00b7sch\u00e4\u00b7del", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "ART", "NN", "KON", "NN", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "Ein Kruzifix und dran ein Weihbrunnwedel", "tokens": ["Ein", "Kru\u00b7zi\u00b7fix", "und", "dran", "ein", "Weih\u00b7brunn\u00b7we\u00b7del"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "PAV", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und eine Gei\u00dfel, da\u00df den Leib er schlage!", "tokens": ["Und", "ei\u00b7ne", "Gei\u00b7\u00dfel", ",", "da\u00df", "den", "Leib", "er", "schla\u00b7ge", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "KOUS", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ei, ob er drum wohl Schien' und Panzer trage?", "tokens": ["Ei", ",", "ob", "er", "drum", "wohl", "Schien'", "und", "Pan\u00b7zer", "tra\u00b7ge", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "PAV", "ADV", "NN", "KON", "NN", "VVFIN", "$."], "meter": "---+-+-+-+-", "measure": "unknown.measure.tetra"}}, "stanza.3": {"line.1": {"text": "Und was noch Tr\u00fcbes fehlt, der Stein mag's k\u00fcnden:", "tokens": ["Und", "was", "noch", "Tr\u00fc\u00b7bes", "fehlt", ",", "der", "Stein", "mag's", "k\u00fcn\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "NN", "VVFIN", "$,", "ART", "NN", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie er gen Sion zog, sich zu ents\u00fcnden,", "tokens": ["Wie", "er", "gen", "Si\u00b7on", "zog", ",", "sich", "zu", "ent\u00b7s\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "NE", "VVFIN", "$,", "PRF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie er die Fasten hielt und sich kasteite", "tokens": ["Wie", "er", "die", "Fas\u00b7ten", "hielt", "und", "sich", "kas\u00b7tei\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ART", "NN", "VVFIN", "KON", "PRF", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und keine Mess' vers\u00e4umt' und niemals freite.", "tokens": ["Und", "kei\u00b7ne", "Mess'", "ver\u00b7s\u00e4umt'", "und", "nie\u00b7mals", "frei\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "KON", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Doch mu\u00df ich dieser Marmorl\u00fcge lachen,", "tokens": ["Doch", "mu\u00df", "ich", "die\u00b7ser", "Mar\u00b7mor\u00b7l\u00fc\u00b7ge", "la\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PDAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Denn mir erz\u00e4hlt mein Herz ganz andre Sachen,", "tokens": ["Denn", "mir", "er\u00b7z\u00e4hlt", "mein", "Herz", "ganz", "and\u00b7re", "Sa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "NN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Als sei's mit dir, du theurer Held, vor Jahren", "tokens": ["Als", "sei's", "mit", "dir", ",", "du", "theu\u00b7rer", "Held", ",", "vor", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "NE", "APPR", "PPER", "$,", "PPER", "ADJD", "NN", "$,", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In lust'gem Zug froh durch die Welt gefahren.", "tokens": ["In", "lust'\u00b7gem", "Zug", "froh", "durch", "die", "Welt", "ge\u00b7fah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Ich seh' dich zwar nach Sch\u00e4deln noch verlangen", "tokens": ["Ich", "seh'", "dich", "zwar", "nach", "Sch\u00e4\u00b7deln", "noch", "ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "NN", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch ist noch Goldhaar dran und ros'ge Wangen!", "tokens": ["Doch", "ist", "noch", "Gold\u00b7haar", "dran", "und", "ros'\u00b7ge", "Wan\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "NN", "PTKVZ", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zwar noch den Rosenkranz, doch aufgezwungen", "tokens": ["Zwar", "noch", "den", "Ro\u00b7sen\u00b7kranz", ",", "doch", "auf\u00b7ge\u00b7zwun\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ADV", "ART", "NN", "$,", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den H\u00e4nden nicht, nein, frei ums Haupt geschlungen!", "tokens": ["Den", "H\u00e4n\u00b7den", "nicht", ",", "nein", ",", "frei", "ums", "Haupt", "ge\u00b7schlun\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "$,", "PTKANT", "$,", "ADJD", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Ich sehe dich an Bord, die Fluth durchjagend,", "tokens": ["Ich", "se\u00b7he", "dich", "an", "Bord", ",", "die", "Fluth", "durch\u00b7ja\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "NN", "$,", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du stehst am Deck, die Harfe fr\u00f6hlich schlagend,", "tokens": ["Du", "stehst", "am", "Deck", ",", "die", "Har\u00b7fe", "fr\u00f6h\u00b7lich", "schla\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,", "ART", "NN", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df selbst das Schiff im Tanz durchflog die Wogen,", "tokens": ["Da\u00df", "selbst", "das", "Schiff", "im", "Tanz", "durch\u00b7flog", "die", "Wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "APPRART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und hinterdrein Delphine walzend zogen!", "tokens": ["Und", "hin\u00b7ter\u00b7drein", "Del\u00b7phi\u00b7ne", "wal\u00b7zend", "zo\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Seh' deiner Sehnsucht heilig Grab dich finden", "tokens": ["Seh'", "dei\u00b7ner", "Sehn\u00b7sucht", "hei\u00b7lig", "Grab", "dich", "fin\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "ADJD", "NN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In Salem in zwei Armen, weichen, linden;", "tokens": ["In", "Sa\u00b7lem", "in", "zwei", "Ar\u00b7men", ",", "wei\u00b7chen", ",", "lin\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "NE", "APPR", "CARD", "NN", "$,", "VVINF", "$,", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es neigen schattend sich, wie seine Fahnen", "tokens": ["Es", "nei\u00b7gen", "schat\u00b7tend", "sich", ",", "wie", "sei\u00b7ne", "Fah\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "PRF", "$,", "PWAV", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Rings um den Sieger, Palmen und Platanen.", "tokens": ["Rings", "um", "den", "Sie\u00b7ger", ",", "Pal\u00b7men", "und", "Pla\u00b7ta\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Ich find' auf Burgen s\u00fc\u00dfer Heimat wieder", "tokens": ["Ich", "find'", "auf", "Bur\u00b7gen", "s\u00fc\u00b7\u00dfer", "Hei\u00b7mat", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In Lauben k\u00fchl dich und die Waffenbr\u00fcder,", "tokens": ["In", "Lau\u00b7ben", "k\u00fchl", "dich", "und", "die", "Waf\u00b7fen\u00b7br\u00fc\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "PPER", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die klingenden Pokale frisch erneuend", "tokens": ["Die", "klin\u00b7gen\u00b7den", "Po\u00b7ka\u00b7le", "frisch", "er\u00b7neu\u00b7end"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJD", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und Scherz und Lied drein als Gew\u00fcrze streuend;", "tokens": ["Und", "Scherz", "und", "Lied", "drein", "als", "Ge\u00b7w\u00fcr\u00b7ze", "streu\u00b7end", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "ADV", "KOUS", "NN", "VVPP", "$."], "meter": "-+-+-+-+-++", "measure": "unknown.measure.hexa"}}, "stanza.9": {"line.1": {"text": "Seh' auf der Kl\u00e4nge Meer im Saal dich wiegen,", "tokens": ["Seh'", "auf", "der", "Kl\u00e4n\u00b7ge", "Meer", "im", "Saal", "dich", "wie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "APPRART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als r\u00fcst'gen Segler tanzend es durchfliegen;", "tokens": ["Als", "r\u00fcst'\u00b7gen", "Seg\u00b7ler", "tan\u00b7zend", "es", "durch\u00b7flie\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "ADJD", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So h\u00e4lt der Sprosser nimmerm\u00fcden Reigen", "tokens": ["So", "h\u00e4lt", "der", "Spros\u00b7ser", "nim\u00b7mer\u00b7m\u00fc\u00b7den", "Rei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Im sel'gen Festessaal von Bl\u00fcthenzweigen.", "tokens": ["Im", "sel'\u00b7gen", "Fes\u00b7tes\u00b7saal", "von", "Bl\u00fc\u00b7then\u00b7zwei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Da\u00df sie am Grab dir lesen so viel Messen,", "tokens": ["Da\u00df", "sie", "am", "Grab", "dir", "le\u00b7sen", "so", "viel", "Mes\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "PPER", "VVFIN", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ist, weil du lebend ihrer oft vergessen!", "tokens": ["Ist", ",", "weil", "du", "le\u00b7bend", "ih\u00b7rer", "oft", "ver\u00b7ges\u00b7sen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "KOUS", "PPER", "ADJD", "PPOSAT", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch log etwa die Kunde meines Herzens?", "tokens": ["Doch", "log", "et\u00b7wa", "die", "Kun\u00b7de", "mei\u00b7nes", "Her\u00b7zens", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und warst du doch ein tr\u00fcber Sohn des Schmerzens?", "tokens": ["Und", "warst", "du", "doch", "ein", "tr\u00fc\u00b7ber", "Sohn", "des", "Schmer\u00b7zens", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Dann, kraft des Dichterrechts, das mir gegeben,", "tokens": ["Dann", ",", "kraft", "des", "Dich\u00b7ter\u00b7rechts", ",", "das", "mir", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "NN", "ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In meinem Herzen leb' ein sch\u00f6n'res Leben!", "tokens": ["In", "mei\u00b7nem", "Her\u00b7zen", "leb'", "ein", "sch\u00f6n'\u00b7res", "Le\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In meinem Herzen wirst du neu geboren,", "tokens": ["In", "mei\u00b7nem", "Her\u00b7zen", "wirst", "du", "neu", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und Alles dir erweckt, was du verloren.", "tokens": ["Und", "Al\u00b7les", "dir", "er\u00b7weckt", ",", "was", "du", "ver\u00b7lo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "PPER", "VVPP", "$,", "PWS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.12": {"line.1": {"text": "Der Freund, da\u00df du ihn liebend magst umschlie\u00dfen,", "tokens": ["Der", "Freund", ",", "da\u00df", "du", "ihn", "lie\u00b7bend", "magst", "um\u00b7schlie\u00b7\u00dfen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "PPER", "ADJD", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die s\u00fc\u00d