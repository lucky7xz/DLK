{"dta.poem.21184": {"metadata": {"author": {"name": "Zachariae, Justus Friedrich Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-20676-2", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ehrw\u00fcrdger Einsiedler! Wie gl\u00fccklich bist du nicht!", "tokens": ["Ehr\u00b7w\u00fcrd\u00b7ger", "Ein\u00b7sied\u00b7ler", "!", "Wie", "gl\u00fcck\u00b7lich", "bist", "du", "nicht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "PWAV", "ADJD", "VAFIN", "PPER", "PTKNEG", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Fern von der Welt aufr\u00fchrischem Get\u00fcmmel,", "tokens": ["Fern", "von", "der", "Welt", "auf\u00b7r\u00fch\u00b7ri\u00b7schem", "Ge\u00b7t\u00fcm\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.3": {"text": "Zeigt uns dein ruhiges Gesicht,", "tokens": ["Zeigt", "uns", "dein", "ru\u00b7hi\u00b7ges", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Von g\u00f6ttlicher Zufriedenheit", "tokens": ["Von", "g\u00f6tt\u00b7li\u00b7cher", "Zu\u00b7frie\u00b7den\u00b7heit"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und hoher Andacht, einen ganzen Himmel.", "tokens": ["Und", "ho\u00b7her", "An\u00b7dacht", ",", "ei\u00b7nen", "gan\u00b7zen", "Him\u00b7mel", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die tiefe Nacht der Einsamkeit,", "tokens": ["Die", "tie\u00b7fe", "Nacht", "der", "Ein\u00b7sam\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "In deiner rauhen H\u00f6hle,", "tokens": ["In", "dei\u00b7ner", "rau\u00b7hen", "H\u00f6h\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wird von verwerflichen Gedanken", "tokens": ["Wird", "von", "ver\u00b7werf\u00b7li\u00b7chen", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Niemals entweiht.", "tokens": ["Nie\u00b7mals", "ent\u00b7weiht", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVFIN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.10": {"text": "Der ganze feyerliche Golgatha", "tokens": ["Der", "gan\u00b7ze", "fey\u00b7er\u00b7li\u00b7che", "Gol\u00b7ga\u00b7tha"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Liegt stets vor deinen Augen da,", "tokens": ["Liegt", "stets", "vor", "dei\u00b7nen", "Au\u00b7gen", "da", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Und bringt vor deine fromme Seele", "tokens": ["Und", "bringt", "vor", "dei\u00b7ne", "from\u00b7me", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Den Tod des G\u00f6ttlichen, der hier f\u00fcr Menschen starb,", "tokens": ["Den", "Tod", "des", "G\u00f6tt\u00b7li\u00b7chen", ",", "der", "hier", "f\u00fcr", "Men\u00b7schen", "starb", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "PRELS", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und Eden uns aufs neu erwarb.", "tokens": ["Und", "E\u00b7den", "uns", "aufs", "neu", "er\u00b7warb", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PPER", "APPRART", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Wir kommen hier zu dieser H\u00f6h,", "tokens": ["Wir", "kom\u00b7men", "hier", "zu", "die\u00b7ser", "H\u00f6h", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Nach einer Reise voll Beschwerde;", "tokens": ["Nach", "ei\u00b7ner", "Rei\u00b7se", "voll", "Be\u00b7schwer\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Und wollen dieser heilgen Erde", "tokens": ["Und", "wol\u00b7len", "die\u00b7ser", "heil\u00b7gen", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Voll Innbrunst, doch von Aberglauben rein,", "tokens": ["Voll", "I\u00b7nnbrunst", ",", "doch", "von", "A\u00b7berg\u00b7lau\u00b7ben", "rein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "$,", "ADV", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Auch unsre Thr\u00e4nen weihn.", "tokens": ["Auch", "uns\u00b7re", "Thr\u00e4\u00b7nen", "weihn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}