{"textgrid.poem.40595": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "Junggesellentod", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der unbeweibte Ritter liegt", "tokens": ["Der", "un\u00b7be\u00b7weib\u00b7te", "Rit\u00b7ter", "liegt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im Sterbepf\u00fchl voll Gram,", "tokens": ["Im", "Ster\u00b7be\u00b7pf\u00fchl", "voll", "Gram", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Kein Weib sich weinend an ihn schmiegt,", "tokens": ["Kein", "Weib", "sich", "wei\u00b7nend", "an", "ihn", "schmiegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PRF", "ADJD", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kein Sohn um Segen kam.", "tokens": ["Kein", "Sohn", "um", "Se\u00b7gen", "kam", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Im Vorgemach der M\u00e4gde Schaar", "tokens": ["Im", "Vor\u00b7ge\u00b7mach", "der", "M\u00e4g\u00b7de", "Schaar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Flicht mit Gesang den Kranz,", "tokens": ["Flicht", "mit", "Ge\u00b7sang", "den", "Kranz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "ART", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Zu schm\u00fccken seine Todtenbahr'", "tokens": ["Zu", "schm\u00fc\u00b7cken", "sei\u00b7ne", "Tod\u00b7ten\u00b7bahr'"], "token_info": ["word", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit reiner Lilien Glanz.", "tokens": ["Mit", "rei\u00b7ner", "Li\u00b7li\u00b7en", "Glanz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Da fa\u00dft den Ritter herbes Weh:", "tokens": ["Da", "fa\u00dft", "den", "Rit\u00b7ter", "her\u00b7bes", "Weh", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbo da\u00df ich hier allein,", "tokens": ["\u00bb", "o", "da\u00df", "ich", "hier", "al\u00b7lein", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "KOUS", "PPER", "ADV", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der letzte meines Stamms, vergeh'", "tokens": ["Der", "letz\u00b7te", "mei\u00b7nes", "Stamms", ",", "ver\u00b7geh'"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ART", "ADJA", "PPOSAT", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und sink' ins Nichts hinein!", "tokens": ["Und", "sink'", "ins", "Nichts", "hin\u00b7ein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "PIS", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Es spro\u00dft der Baum, vermodert schon,", "tokens": ["Es", "spro\u00dft", "der", "Baum", ",", "ver\u00b7mo\u00b7dert", "schon", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In Sam' und Wurzeln fort!", "tokens": ["In", "Sam'", "und", "Wur\u00b7zeln", "fort", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die fl\u00fccht'ge Wolke ist der Sohn", "tokens": ["Die", "fl\u00fccht'\u00b7ge", "Wol\u00b7ke", "ist", "der", "Sohn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des Stroms, im Sand verdorrt!\u00ab", "tokens": ["Des", "Stroms", ",", "im", "Sand", "ver\u00b7dorrt", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "$,", "APPRART", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Da reicht der Schlo\u00dfkaplan zum Ku\u00df", "tokens": ["Da", "reicht", "der", "Schlo\u00df\u00b7kap\u00b7lan", "zum", "Ku\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Demantkreuz ihm dar:", "tokens": ["Ein", "De\u00b7mant\u00b7kreuz", "ihm", "dar", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbdie\u00df Kreuz schickt Hedwig euch zum Gru\u00df,", "tokens": ["\u00bb", "die\u00df", "Kreuz", "schickt", "Hed\u00b7wig", "euch", "zum", "Gru\u00df", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PDS", "NN", "VVFIN", "NE", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die meine Mutter war.\u00ab", "tokens": ["Die", "mei\u00b7ne", "Mut\u00b7ter", "war", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "PPOSAT", "NN", "VAFIN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "\u00bbund wenn dir Hedwig Mutter hei\u00dft,", "tokens": ["\u00bb", "und", "wenn", "dir", "Hed\u00b7wig", "Mut\u00b7ter", "hei\u00dft", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "KOUS", "PPER", "NE", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nenn' ich lieb S\u00f6hnlein dich!", "tokens": ["Nenn'", "ich", "lieb", "S\u00f6hn\u00b7lein", "dich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "NN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es senke tief in deinen Geist", "tokens": ["Es", "sen\u00b7ke", "tief", "in", "dei\u00b7nen", "Geist"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Segen Gottes sich!", "tokens": ["Der", "Se\u00b7gen", "Got\u00b7tes", "sich", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PRF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Die\u00df Schlo\u00df mit Burgkapell' und Wart',", "tokens": ["Die\u00df", "Schlo\u00df", "mit", "Burg\u00b7ka\u00b7pell'", "und", "Wart'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als Erbtheil fall's dir zu:", "tokens": ["Als", "E\u00b7rbtheil", "fall's", "dir", "zu", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nicht mit Gebet und Mess' gespart", "tokens": ["Nicht", "mit", "Ge\u00b7bet", "und", "Mess'", "ge\u00b7spart"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "APPR", "NN", "KON", "NN", "VVPP"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.4": {"text": "F\u00fcr meiner Seele Ruh'!\u00ab", "tokens": ["F\u00fcr", "mei\u00b7ner", "See\u00b7le", "Ruh'", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Ein R\u00f6slein von Rubinen rein", "tokens": ["Ein", "R\u00f6s\u00b7lein", "von", "Ru\u00b7bi\u00b7nen", "rein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN", "ADJD"], "meter": "-+-+---+", "measure": "unknown.measure.tri"}, "line.2": {"text": "Beut ihm des G\u00e4rtners Hand:", "tokens": ["Beut", "ihm", "des", "G\u00e4rt\u00b7ners", "Hand", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "$."], "meter": "---+-+", "measure": "unknown.measure.di"}, "line.3": {"text": "\u00bbfrau Adelheid, mein M\u00fctterlein,", "tokens": ["\u00bb", "frau", "A\u00b7del\u00b7heid", ",", "mein", "M\u00fct\u00b7ter\u00b7lein", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "VVFIN", "NE", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Entsendet euch die\u00df Pfand!\u00ab", "tokens": ["Ent\u00b7sen\u00b7det", "euch", "die\u00df", "Pfand", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "PDS", "NN", "$.", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.9": {"line.1": {"text": "\u00bbist Adelheid dein M\u00fctterlein,", "tokens": ["\u00bb", "ist", "A\u00b7del\u00b7heid", "dein", "M\u00fct\u00b7ter\u00b7lein", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "NE", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mir an die Brust, mein Kind!", "tokens": ["Mir", "an", "die", "Brust", ",", "mein", "Kind", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ins Herz und auf die Blumen dein", "tokens": ["Ins", "Herz", "und", "auf", "die", "Blu\u00b7men", "dein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "KON", "APPR", "ART", "NN", "PPOSAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Fleu\u00df' Gottes Segen lind!", "tokens": ["Fleu\u00df'", "Got\u00b7tes", "Se\u00b7gen", "lind", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Dir schenk' ich Garten, Wies' und Hain", "tokens": ["Dir", "schenk'", "ich", "Gar\u00b7ten", ",", "Wies'", "und", "Hain"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "NN", "$,", "NE", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und dort das Winzerhaus;", "tokens": ["Und", "dort", "das", "Win\u00b7zer\u00b7haus", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du sorgst wohl, da\u00df auf meinem Stein", "tokens": ["Du", "sorgst", "wohl", ",", "da\u00df", "auf", "mei\u00b7nem", "Stein"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "ADV", "$,", "KOUS", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nie gehn die Blumen aus.\u00ab", "tokens": ["Nie", "gehn", "die", "Blu\u00b7men", "aus", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKVZ", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Es trat sein Page drauf vor ihn", "tokens": ["Es", "trat", "sein", "Pa\u00b7ge", "drauf", "vor", "ihn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "PAV", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit einem Ring von Gold:", "tokens": ["Mit", "ei\u00b7nem", "Ring", "von", "Gold", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbdie\u00df schickt euch Mutter Melusin',", "tokens": ["\u00bb", "die\u00df", "schickt", "euch", "Mut\u00b7ter", "Me\u00b7lu\u00b7sin'", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PDS", "VVFIN", "PPER", "NN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ob ihr's erkennen wollt?\u00ab", "tokens": ["Ob", "ih\u00b7r's", "er\u00b7ken\u00b7nen", "wollt", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PIS", "VVINF", "VMFIN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "\u00bbo Melusinens Sohn, sei mir", "tokens": ["\u00bb", "o", "Me\u00b7lu\u00b7si\u00b7nens", "Sohn", ",", "sei", "mir"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word"], "pos": ["$(", "FM", "NE", "NN", "$,", "VAFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein liebstes Kind genannt!", "tokens": ["Mein", "liebs\u00b7tes", "Kind", "ge\u00b7nannt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gott's Segen st\u00e4hle f\u00fcr und f\u00fcr", "tokens": ["Gott's", "Se\u00b7gen", "st\u00e4h\u00b7le", "f\u00fcr", "und", "f\u00fcr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "VVFIN", "APPR", "KON", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dir Brust und Mark und Hand!", "tokens": ["Dir", "Brust", "und", "Mark", "und", "Hand", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Das sch\u00f6nste R\u00f6\u00dflein, das mich trug,", "tokens": ["Das", "sch\u00f6ns\u00b7te", "R\u00f6\u00df\u00b7lein", ",", "das", "mich", "trug", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein bestes Schwert sei dein:", "tokens": ["Mein", "bes\u00b7tes", "Schwert", "sei", "dein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "PPOSAT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das tr\u00e4gt noch meinen Namenszug,", "tokens": ["Das", "tr\u00e4gt", "noch", "mei\u00b7nen", "Na\u00b7mens\u00b7zug", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "F\u00fchr's w\u00fcrdig dein und mein.\u00ab", "tokens": ["F\u00fchr's", "w\u00fcr\u00b7dig", "dein", "und", "mein", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "ADJD", "PPOSAT", "KON", "PPOSAT", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Da rauschen Tritte vor dem Schlo\u00df,", "tokens": ["Da", "rau\u00b7schen", "Trit\u00b7te", "vor", "dem", "Schlo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da h\u00f6rt er Kinderschrei:", "tokens": ["Da", "h\u00f6rt", "er", "Kin\u00b7der\u00b7schrei", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbo Gott, dein Segen ist zu gro\u00df!\u00ab", "tokens": ["\u00bb", "o", "Gott", ",", "dein", "Se\u00b7gen", "ist", "zu", "gro\u00df", "!", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "FM", "NN", "$,", "PPOSAT", "NN", "VAFIN", "PTKA", "ADJD", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da bricht sein Herz entzwei.", "tokens": ["Da", "bricht", "sein", "Herz", "ent\u00b7zwei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Dem Glockenklang, dem Sarge nach", "tokens": ["Dem", "Glo\u00b7cken\u00b7klang", ",", "dem", "Sar\u00b7ge", "nach"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text