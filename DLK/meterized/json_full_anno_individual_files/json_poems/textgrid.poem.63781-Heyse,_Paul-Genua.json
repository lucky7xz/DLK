{"textgrid.poem.63781": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "Genua", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dein Puls, du stolzes Genua, ist erschlafft.", "tokens": ["Dein", "Puls", ",", "du", "stol\u00b7zes", "Ge\u00b7nua", ",", "ist", "er\u00b7schlafft", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPER", "ADJA", "NN", "$,", "VAFIN", "VVPP", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Noch sieht man herrlich dich im Halbrund thronen,", "tokens": ["Noch", "sieht", "man", "herr\u00b7lich", "dich", "im", "Hal\u00b7brund", "thro\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADJD", "PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Als g\u00e4lt's dem hehren Schauspiel beizuwohnen", "tokens": ["Als", "g\u00e4lt's", "dem", "heh\u00b7ren", "Schau\u00b7spiel", "bei\u00b7zu\u00b7woh\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NE", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Siegreicher Flotten, hoher Heldenkraft.", "tokens": ["Sieg\u00b7rei\u00b7cher", "Flot\u00b7ten", ",", "ho\u00b7her", "Hel\u00b7den\u00b7kraft", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Doch statt zu handeln, treibst du Handelschaft.", "tokens": ["Doch", "statt", "zu", "han\u00b7deln", ",", "treibst", "du", "Han\u00b7del\u00b7schaft", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PTKZU", "VVINF", "$,", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Heut gelten Aktien statt der Staatsaktionen,", "tokens": ["Heut", "gel\u00b7ten", "Ak\u00b7ti\u00b7en", "statt", "der", "Staats\u00b7ak\u00b7ti\u00b7o\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "+--+--+--+-+-", "measure": "dactylic.tri.plus"}, "line.3": {"text": "Die Schiffe bringen Waren fremder Zonen,", "tokens": ["Die", "Schif\u00b7fe", "brin\u00b7gen", "Wa\u00b7ren", "frem\u00b7der", "Zo\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VAFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nicht mehr Troph\u00e4'n, dem Sarazen entrafft.", "tokens": ["Nicht", "mehr", "Tro\u00b7ph\u00e4'n", ",", "dem", "Sa\u00b7ra\u00b7zen", "en\u00b7trafft", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PIAT", "NN", "$,", "ART", "NN", "VVFIN", "$."], "meter": "+-+---+--+", "measure": "iambic.tetra.chol"}}, "stanza.3": {"line.1": {"text": "Vom Geist der Zeit hast du dich b\u00e4nd'gen lassen.", "tokens": ["Vom", "Geist", "der", "Zeit", "hast", "du", "dich", "b\u00e4n\u00b7d'\u00b7gen", "las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VAFIN", "PPER", "PRF", "VVINF", "VVINF", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ward doch die B\u00fchne, die ihn spiegelt, heute", "tokens": ["Ward", "doch", "die", "B\u00fch\u00b7ne", ",", "die", "ihn", "spie\u00b7gelt", ",", "heu\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word"