{"dta.poem.8484": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Mondesaufgang.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1860", "urn": "urn:nbn:de:kobv:b4-200905191007", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "An des Balkones Gitter lehnte ich", "tokens": ["An", "des", "Bal\u00b7ko\u00b7nes", "Git\u00b7ter", "lehn\u00b7te", "ich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und wartete, du mildes Licht, auf dich;", "tokens": ["Und", "war\u00b7te\u00b7te", ",", "du", "mil\u00b7des", "Licht", ",", "auf", "dich", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PPER", "ADJA", "NN", "$,", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Hoch \u00fcber mir gleich tr\u00fcbem Eiskristalle", "tokens": ["Hoch", "\u00fc\u00b7ber", "mir", "gleich", "tr\u00fc\u00b7bem", "Eis\u00b7kris\u00b7tal\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "PPER", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zerschmolzen schwamm des Firmamentes Halle;", "tokens": ["Zer\u00b7schmol\u00b7zen", "schwamm", "des", "Fir\u00b7ma\u00b7men\u00b7tes", "Hal\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "NE", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Grauschimmernd lag der See mit leisem St\u00f6hnen,", "tokens": ["Grau\u00b7schim\u00b7mernd", "lag", "der", "See", "mit", "lei\u00b7sem", "St\u00f6h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Zerflo\u00dfne Perlen, oder Wolkenthr\u00e4nen?", "tokens": ["Zer\u00b7flo\u00df\u00b7ne", "Per\u00b7len", ",", "o\u00b7der", "Wol\u00b7ken\u00b7thr\u00e4\u00b7nen", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Es rieselte, es d\u00e4mmerte um mich;", "tokens": ["Es", "rie\u00b7sel\u00b7te", ",", "es", "d\u00e4m\u00b7mer\u00b7te", "um", "mich", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Du mildes Licht, ich wartete auf dich.", "tokens": ["Du", "mil\u00b7des", "Licht", ",", "ich", "war\u00b7te\u00b7te", "auf", "dich", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "PPER", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Hoch stand ich, neben mir der Linden Kamm,", "tokens": ["Hoch", "stand", "ich", ",", "ne\u00b7ben", "mir", "der", "Lin\u00b7den", "Kamm", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "$,", "APPR", "PPER", "ART", "NE", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Tief unter mir Gezweige, Ast und Stamm,", "tokens": ["Tief", "un\u00b7ter", "mir", "Ge\u00b7zwei\u00b7ge", ",", "Ast", "und", "Stamm", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPER", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Im Laube summte der Phal\u00e4nen Reigen;", "tokens": ["Im", "Lau\u00b7be", "summ\u00b7te", "der", "Pha\u00b7l\u00e4\u00b7nen", "Rei\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Feuerfliege sah ich zieh\u2019n und steigen,", "tokens": ["Die", "Feu\u00b7er\u00b7flie\u00b7ge", "sah", "ich", "zieh'n", "und", "stei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVINF", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und Bl\u00fcten taumelten wie halb entschlafen;", "tokens": ["Und", "Bl\u00fc\u00b7ten", "tau\u00b7mel\u00b7ten", "wie", "halb", "ent\u00b7schla\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "KOKOM", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Mir war, als treibe hier ein Herz zum Hafen,", "tokens": ["Mir", "war", ",", "als", "trei\u00b7be", "hier", "ein", "Herz", "zum", "Ha\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "KOUS", "VVFIN", "ADV", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Ein Herz, das \u00fcbervoll von Gl\u00fcck und Leid", "tokens": ["Ein", "Herz", ",", "das", "\u00fc\u00b7ber\u00b7voll", "von", "Gl\u00fcck", "und", "Leid"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und Bildern seliger Vergangenheit.", "tokens": ["Und", "Bil\u00b7dern", "se\u00b7li\u00b7ger", "Ver\u00b7gan\u00b7gen\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Die Schatten stiegen, dr\u00e4ngten finster ein;", "tokens": ["Die", "Schat\u00b7ten", "stie\u00b7gen", ",", "dr\u00e4ng\u00b7ten", "fins\u00b7ter", "ein", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wo weilst du, weilst du denn mein milder Schein?", "tokens": ["Wo", "weilst", "du", ",", "weilst", "du", "denn", "mein", "mil\u00b7der", "Schein", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sie drangen ein wie s\u00fcndige Gedanken,", "tokens": ["Sie", "dran\u00b7gen", "ein", "wie", "s\u00fcn\u00b7di\u00b7ge", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "KOKOM", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Des Firmamentes Woge schien zu schwanken;", "tokens": ["Des", "Fir\u00b7ma\u00b7men\u00b7tes", "Wo\u00b7ge", "schien", "zu", "schwan\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Verzitternd losch der Feuerfliege Funken,", "tokens": ["Ver\u00b7zit\u00b7ternd", "losch", "der", "Feu\u00b7er\u00b7flie\u00b7ge", "Fun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "ADJD", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "L\u00e4ngst die Phal\u00e4ne war zum Grund gesunken;", "tokens": ["L\u00e4ngst", "die", "Pha\u00b7l\u00e4\u00b7ne", "war", "zum", "Grund", "ge\u00b7sun\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Nur Bergesh\u00e4upter stiegen hart empor,", "tokens": ["Nur", "Ber\u00b7ges\u00b7h\u00e4up\u00b7ter", "stie\u00b7gen", "hart", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Ein d\u00fcstrer Richterkreis im D\u00fcster vor.", "tokens": ["Ein", "d\u00fcst\u00b7rer", "Rich\u00b7ter\u00b7kreis", "im", "D\u00fcs\u00b7ter", "vor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Es visperten die Wipfel mir am Fu\u00df,", "tokens": ["Es", "vis\u00b7per\u00b7ten", "die", "Wip\u00b7fel", "mir", "am", "Fu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPER", "APPRART", "NN", "$,"], "meter": "-+---+-+-+", "measure": "dactylic.init"}, "line.2": {"text": "Wie Warnungsfl\u00fcstern oder Todesgru\u00df;", "tokens": ["Wie", "War\u00b7nungs\u00b7fl\u00fcs\u00b7tern", "o\u00b7der", "To\u00b7des\u00b7gru\u00df", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein Summen aus des Seees weitem Thale,", "tokens": ["Ein", "Sum\u00b7men", "aus", "des", "See\u00b7es", "wei\u00b7tem", "Tha\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie Volksgemurmel vor dem Tribunale;", "tokens": ["Wie", "Volks\u00b7ge\u00b7mur\u00b7mel", "vor", "dem", "Tri\u00b7bu\u00b7na\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Mir war, als m\u00fcsse etwas Rechnung geben", "tokens": ["Mir", "war", ",", "als", "m\u00fcs\u00b7se", "et\u00b7was", "Rech\u00b7nung", "ge\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "KOUS", "VMFIN", "PIAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Von todten Pfunden, von vertr\u00e4umtem Leben,", "tokens": ["Von", "tod\u00b7ten", "Pfun\u00b7den", ",", "von", "ver\u00b7tr\u00e4um\u00b7tem", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Als stehe ein verk\u00fcmmert Herz allein,", "tokens": ["Als", "ste\u00b7he", "ein", "ver\u00b7k\u00fcm\u00b7mert", "Herz", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "ART", "VVPP", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Einsam mit seiner Schuld und seiner Pein.", "tokens": ["Ein\u00b7sam", "mit", "sei\u00b7ner", "Schuld", "und", "sei\u00b7ner", "Pein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.5": {"line.1": {"text": "Da auf die Wasser sank ein Silberflor,", "tokens": ["Da", "auf", "die", "Was\u00b7ser", "sank", "ein", "Sil\u00b7ber\u00b7flor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und langsam stieg die Mondesscheib\u2019 empor,", "tokens": ["Und", "lang\u00b7sam", "stieg", "die", "Mon\u00b7des\u00b7scheib'", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Alpen finstre Stirnen strich sie leise,", "tokens": ["Der", "Al\u00b7pen", "finst\u00b7re", "Stir\u00b7nen", "strich", "sie", "lei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und aus den Richtern wurden sanfte Greise;", "tokens": ["Und", "aus", "den", "Rich\u00b7tern", "wur\u00b7den", "sanf\u00b7te", "Grei\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VAFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Der Wellen Zucken ward ein l\u00e4chelnd Winken,", "tokens": ["Der", "Wel\u00b7len", "Zu\u00b7cken", "ward", "ein", "l\u00e4\u00b7chelnd", "Win\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "An jedem Blatte sah ich Tropfen blinken,", "tokens": ["An", "je\u00b7dem", "Blat\u00b7te", "sah", "ich", "Trop\u00b7fen", "blin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und jeder Tropfen schien ein K\u00e4mmerlein,", "tokens": ["Und", "je\u00b7der", "Trop\u00b7fen", "schien", "ein", "K\u00e4m\u00b7mer\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Drin flimmerte der Heimathlampe Schein.", "tokens": ["Drin", "flim\u00b7mer\u00b7te", "der", "Hei\u00b7math\u00b7lam\u00b7pe", "Schein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "O Mond, du bist mir wie ein sp\u00e4ter Freund,", "tokens": ["O", "Mond", ",", "du", "bist", "mir", "wie", "ein", "sp\u00e4\u00b7ter", "Freund", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PPER", "VAFIN", "PPER", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der seine Jugend dem Verarmten eint,", "tokens": ["Der", "sei\u00b7ne", "Ju\u00b7gend", "dem", "Ver\u00b7arm\u00b7ten", "eint", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Um seine sterbenden Erinnerungen", "tokens": ["Um", "sei\u00b7ne", "ster\u00b7ben\u00b7den", "E\u00b7rin\u00b7ne\u00b7run\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUI", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mit zartem Lebenswiderschein geschlungen;", "tokens": ["Mit", "zar\u00b7tem", "Le\u00b7bens\u00b7wi\u00b7der\u00b7schein", "ge\u00b7schlun\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Bist keine Sonne, die ern\u00e4hrt und blendet,", "tokens": ["Bist", "kei\u00b7ne", "Son\u00b7ne", ",", "die", "er\u00b7n\u00e4hrt", "und", "blen\u00b7det", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "$,", "PRELS", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "In Feuerstr\u00f6men lebt, im Blute endet,", "tokens": ["In", "Feu\u00b7er\u00b7str\u00f6\u00b7men", "lebt", ",", "im", "Blu\u00b7te", "en\u00b7det", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$,", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Bist, was dem kranken S\u00e4nger sein Gedicht,", "tokens": ["Bist", ",", "was", "dem", "kran\u00b7ken", "S\u00e4n\u00b7ger", "sein", "Ge\u00b7dicht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "PRELS", "ART", "ADJA", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Ein fremdes, aber, o, ein mildes Licht.", "tokens": ["Ein", "frem\u00b7des", ",", "a\u00b7ber", ",", "o", ",", "ein", "mil\u00b7des", "Licht", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADV", "$,", "FM", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}