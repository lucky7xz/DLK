{"dta.poem.19391": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "124.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "In seiner Klause sa\u00df der Klausner und verga\u00df", "tokens": ["In", "sei\u00b7ner", "Klau\u00b7se", "sa\u00df", "der", "Klaus\u00b7ner", "und", "ver\u00b7ga\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "KON", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das Irdische, dieweil er Himmlisches erma\u00df.", "tokens": ["Das", "Ir\u00b7di\u00b7sche", ",", "die\u00b7weil", "er", "Himm\u00b7li\u00b7sches", "er\u00b7ma\u00df", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Da gieng ein sch\u00f6nes Weib vorm offnen Eingang hin,", "tokens": ["Da", "gieng", "ein", "sch\u00f6\u00b7nes", "Weib", "vorm", "off\u00b7nen", "Ein\u00b7gang", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "APPRART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Aus ihrem Auge schlug ein Blitz in seinen Sinn.", "tokens": ["Aus", "ih\u00b7rem", "Au\u00b7ge", "schlug", "ein", "Blitz", "in", "sei\u00b7nen", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Er f\u00fchlte von dem Schlag des Funken sich durchzuckt,", "tokens": ["Er", "f\u00fchl\u00b7te", "von", "dem", "Schlag", "des", "Fun\u00b7ken", "sich", "durch\u00b7zuckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und schon hat er den Fu\u00df zur Schwell' hinaus geruckt.", "tokens": ["Und", "schon", "hat", "er", "den", "Fu\u00df", "zur", "Schwell'", "hin\u00b7aus", "ge\u00b7ruckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ART", "NN", "APPRART", "NN", "APZR", "VVPP", "$."], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}}, "stanza.4": {"line.1": {"text": "Doch auf dem halben Weg zur Welt ist er zum Gl\u00fcck", "tokens": ["Doch", "auf", "dem", "hal\u00b7ben", "Weg", "zur", "Welt", "ist", "er", "zum", "Gl\u00fcck"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "APPRART", "NN", "VAFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vom Geist zur\u00fcckgemahnt, und zieht den Fu\u00df zur\u00fcck.", "tokens": ["Vom", "Geist", "zu\u00b7r\u00fcck\u00b7ge\u00b7mahnt", ",", "und", "zieht", "den", "Fu\u00df", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$,", "KON", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Er will ihn ziehn, und kann ihn nicht zur\u00fcckziehn wieder,", "tokens": ["Er", "will", "ihn", "ziehn", ",", "und", "kann", "ihn", "nicht", "zu\u00b7r\u00fcck\u00b7ziehn", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVINF", "$,", "KON", "VMFIN", "PPER", "PTKNEG", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und auf der Schwelle selbst l\u00e4\u00dft sich der Klausner nieder.", "tokens": ["Und", "auf", "der", "Schwel\u00b7le", "selbst", "l\u00e4\u00dft", "sich", "der", "Klaus\u00b7ner", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADV", "VVFIN", "PRF", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Es sitzt der Oberleib zur Klaus' hinein gelehnt,", "tokens": ["Es", "sitzt", "der", "O\u00b7berl\u00b7eib", "zur", "Klaus'", "hin\u00b7ein", "ge\u00b7lehnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "APZR", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch auf der Schwelle bleibt der Fu\u00df heraus gedehnt.", "tokens": ["Doch", "auf", "der", "Schwel\u00b7le", "bleibt", "der", "Fu\u00df", "he\u00b7raus", "ge\u00b7dehnt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "APZR", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Seit Jahren mu\u00df der Fu\u00df heraus zur Schwelle hangen,", "tokens": ["Seit", "Jah\u00b7ren", "mu\u00df", "der", "Fu\u00df", "he\u00b7raus", "zur", "Schwel\u00b7le", "han\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "ART", "NN", "PTKVZ", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und alle sahn ihn so, die dort vorbeigegangen. \u2014", "tokens": ["Und", "al\u00b7le", "sahn", "ihn", "so", ",", "die", "dort", "vor\u00b7bei\u00b7ge\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PIS", "VVFIN", "PPER", "ADV", "$,", "PRELS", "ADV", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Halt deinen Fu\u00df zur\u00fcck von Weltlust, la\u00df nicht ihn", "tokens": ["Halt", "dei\u00b7nen", "Fu\u00df", "zu\u00b7r\u00fcck", "von", "Welt\u00b7lust", ",", "la\u00df", "nicht", "ihn"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "PPOSAT", "NN", "PTKVZ", "APPR", "NN", "$,", "VVIMP", "PTKNEG", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Voreilen, weil's so schwer ist ihn zur\u00fcckzuziehn.", "tokens": ["Vor\u00b7ei\u00b7len", ",", "weil's", "so", "schwer", "ist", "ihn", "zu\u00b7r\u00fcck\u00b7zu\u00b7ziehn", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ADV", "ADJD", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}