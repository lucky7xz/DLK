{"textgrid.poem.34145": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Historischer R\u00fcckblick", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nach dem Xenien-Hagel der beiden deutschen Heroen", "tokens": ["Nach", "dem", "Xe\u00b7ni\u00b7en\u00b7Ha\u00b7gel", "der", "bei\u00b7den", "deut\u00b7schen", "He\u00b7roen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "PIAT", "ADJA", "NN"], "meter": "--+--+--+-+--+", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Ward es lebendig im Sumpf, wie man es nie noch geseh'n:", "tokens": ["Ward", "es", "le\u00b7ben\u00b7dig", "im", "Sumpf", ",", "wie", "man", "es", "nie", "noch", "ge\u00b7seh'n", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "APPRART", "NN", "$,", "PWAV", "PIS", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "+-----+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Schiller und Goethe hie\u00dfen die Sudelk\u00f6che in Weimar,", "tokens": ["Schil\u00b7ler", "und", "Goe\u00b7the", "hie\u00b7\u00dfen", "die", "Su\u00b7del\u00b7k\u00f6\u00b7che", "in", "Wei\u00b7mar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NE", "VVFIN", "ART", "NN", "APPR", "NE", "$,"], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Und der erb\u00e4rmlichste Wicht warf sie mit Steinen und Koth.", "tokens": ["Und", "der", "er\u00b7b\u00e4rm\u00b7lichs\u00b7te", "Wicht", "warf", "sie", "mit", "Stei\u00b7nen", "und", "Koth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "PPER", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+--++--+--+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Doch, was bewies der Spectakel? Nichts weiter, als da\u00df das Gelichter", "tokens": ["Doch", ",", "was", "be\u00b7wies", "der", "Spec\u00b7ta\u00b7kel", "?", "Nichts", "wei\u00b7ter", ",", "als", "da\u00df", "das", "Ge\u00b7lich\u00b7ter"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "$,", "PWS", "VVFIN", "ART", "NN", "$.", "PIS", "PTKVZ", "$,", "KOKOM", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.6": {"text": "Noch viel kl\u00e4glicher war, als es die Beiden gemalt!", "tokens": ["Noch", "viel", "kl\u00e4g\u00b7li\u00b7cher", "war", ",", "als", "es", "die", "Bei\u00b7den", "ge\u00b7malt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "VAFIN", "$,", "KOUS", "PPER", "ART", "NN", "VVPP", "$."], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Nach dem Xenien-Hagel der beiden deutschen Heroen", "tokens": ["Nach", "dem", "Xe\u00b7ni\u00b7en\u00b7Ha\u00b7gel", "der", "bei\u00b7den", "deut\u00b7schen", "He\u00b7roen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "PIAT", "ADJA", "NN"], "meter": "--+--+--+-+--+", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Ward es lebendig im Sumpf, wie man es nie noch geseh'n:", "tokens": ["Ward", "es", "le\u00b7ben\u00b7dig", "im", "Sumpf", ",", "wie", "man", "es", "nie", "noch", "ge\u00b7seh'n", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "APPRART", "NN", "$,", "PWAV", "PIS", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "+-----+-+-+--+", "mea