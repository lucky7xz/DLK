{"textgrid.poem.42615": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Die Geisterkelter", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zu Weinsberg, der gepriesnen Stadt,", "tokens": ["Zu", "Weins\u00b7berg", ",", "der", "ge\u00b7pri\u00b7es\u00b7nen", "Stadt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Die von dem Wein den Namen hat,", "tokens": ["Die", "von", "dem", "Wein", "den", "Na\u00b7men", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wo Lieder klingen, sch\u00f6n und neu,", "tokens": ["Wo", "Lie\u00b7der", "klin\u00b7gen", ",", "sch\u00f6n", "und", "neu", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVINF", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und wo die Burg hei\u00dft Weibertreu:", "tokens": ["Und", "wo", "die", "Burg", "hei\u00dft", "Wei\u00b7ber\u00b7treu", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Bei Weib und Wein und bei Gesang", "tokens": ["Bei", "Weib", "und", "Wein", "und", "bei", "Ge\u00b7sang"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "W\u00e4r Luthern dort die Zeit nicht lang,", "tokens": ["W\u00e4r", "Lu\u00b7thern", "dort", "die", "Zeit", "nicht", "lang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ADV", "ART", "NN", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Auch f\u00e4nd er Herberg und Gela\u00df", "tokens": ["Auch", "f\u00e4nd", "er", "Her\u00b7berg", "und", "Ge\u00b7la\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "F\u00fcr Teufel und f\u00fcr Dintenfa\u00df,", "tokens": ["F\u00fcr", "Teu\u00b7fel", "und", "f\u00fcr", "Din\u00b7ten\u00b7fa\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Denn alle Geister wandeln da;", "tokens": ["Denn", "al\u00b7le", "Geis\u00b7ter", "wan\u00b7deln", "da", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "H\u00f6rt! was zu Weinsberg j\u00fcngst geschah.", "tokens": ["H\u00f6rt", "!", "was", "zu", "Weins\u00b7berg", "j\u00fcngst", "ge\u00b7schah", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$.", "PRELS", "APPR", "NE", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der W\u00e4chter, der die Stadt bewacht,", "tokens": ["Der", "W\u00e4ch\u00b7ter", ",", "der", "die", "Stadt", "be\u00b7wacht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "$,", "PRELS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ging seinen Gang in jener Nacht,", "tokens": ["Ging", "sei\u00b7nen", "Gang", "in", "je\u00b7ner", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In der ein Jahr zu Grabe geht", "tokens": ["In", "der", "ein", "Jahr", "zu", "Gra\u00b7be", "geht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ART", "NN", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und gleich ein andres aufersteht.", "tokens": ["Und", "gleich", "ein", "and\u00b7res", "auf\u00b7er\u00b7steht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Schon warnt die Uhr zur Geisterzeit,", "tokens": ["Schon", "warnt", "die", "Uhr", "zur", "Geis\u00b7ter\u00b7zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der W\u00e4chter steht zum Ruf bereit:", "tokens": ["Der", "W\u00e4ch\u00b7ter", "steht", "zum", "Ruf", "be\u00b7reit", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "APPRART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da, zwischen Warnen, zwischen Schlag,", "tokens": ["Da", ",", "zwi\u00b7schen", "War\u00b7nen", ",", "zwi\u00b7schen", "Schlag", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "APPR", "NN", "$,", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Am Scheideweg von Jahr und Tag,", "tokens": ["Am", "Schei\u00b7de\u00b7weg", "von", "Jahr", "und", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "H\u00f6rt er ein Knarren, ein Gebraus,", "tokens": ["H\u00f6rt", "er", "ein", "Knar\u00b7ren", ",", "ein", "Ge\u00b7braus", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Gen\u00fcber \u00f6ffnet sich das Haus,", "tokens": ["Ge\u00b7n\u00fc\u00b7ber", "\u00f6ff\u00b7net", "sich", "das", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Es sinkt die Wand, im hohlen Raum", "tokens": ["Es", "sinkt", "die", "Wand", ",", "im", "hoh\u00b7len", "Raum"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Erhebt sich stolz ein Kelterbaum,", "tokens": ["Er\u00b7hebt", "sich", "stolz", "ein", "Kel\u00b7ter\u00b7baum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Und um ihn dreht in vollem Schwung", "tokens": ["Und", "um", "ihn", "dreht", "in", "vol\u00b7lem", "Schwung"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUI", "PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Sich jauchzend, gl\u00fchend Alt und Jung,", "tokens": ["Sich", "jauch\u00b7zend", ",", "gl\u00fc\u00b7hend", "Alt", "und", "Jung", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVPP", "$,", "ADJD", "ADJD", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Und aus den R\u00f6hren, purpurhell,", "tokens": ["Und", "aus", "den", "R\u00f6h\u00b7ren", ",", "pur\u00b7pur\u00b7hell", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Vollbl\u00fctig, springt des Mostes Quell;", "tokens": ["Voll\u00b7bl\u00fc\u00b7tig", ",", "springt", "des", "Mos\u00b7tes", "Quell", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Ein sausend M\u00fchlrad, tobt der Reihn,", "tokens": ["Ein", "sau\u00b7send", "M\u00fchl\u00b7rad", ",", "tobt", "der", "Reihn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Die Schaufeln treibt der wilde Wein.", "tokens": ["Die", "Schau\u00b7feln", "treibt", "der", "wil\u00b7de", "Wein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Der W\u00e4chter wei\u00df nicht, wie er tu,", "tokens": ["Der", "W\u00e4ch\u00b7ter", "wei\u00df", "nicht", ",", "wie", "er", "tu", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "PTKNEG", "$,", "PWAV", "PPER", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Er kehrt sich ab, den Bergen zu:", "tokens": ["Er", "kehrt", "sich", "ab", ",", "den", "Ber\u00b7gen", "zu", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PTKVZ", "$,", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Doch ob der dunkeln Stadt herein", "tokens": ["Doch", "ob", "der", "dun\u00b7keln", "Stadt", "her\u00b7ein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Ergl\u00e4nzen die in Mittagsschein,", "tokens": ["Er\u00b7gl\u00e4n\u00b7zen", "die", "in", "Mit\u00b7tags\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Des Herbstes goldner Sonnenstaub", "tokens": ["Des", "Herbs\u00b7tes", "gold\u00b7ner", "Son\u00b7nen\u00b7staub"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Umwebt der Reben \u00fcppig Laub,", "tokens": ["Um\u00b7webt", "der", "Re\u00b7ben", "\u00fcp\u00b7pig", "Laub", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Und aus dem Laube blinkt hervor", "tokens": ["Und", "aus", "dem", "Lau\u00b7be", "blinkt", "her\u00b7vor"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Der Winzerinnen bunter Chor;", "tokens": ["Der", "Win\u00b7ze\u00b7rin\u00b7nen", "bun\u00b7ter", "Chor", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Den Tr\u00e4gern in den Furchen all", "tokens": ["Den", "Tr\u00e4\u00b7gern", "in", "den", "Fur\u00b7chen", "all"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN", "PIAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "W\u00e4chst \u00fcbers Haupt der Trauben Schwall,", "tokens": ["W\u00e4chst", "\u00fc\u00b7bers", "Haupt", "der", "Trau\u00b7ben", "Schwall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Die Treterknaben sieht man kaum,", "tokens": ["Die", "Tre\u00b7ter\u00b7kna\u00b7ben", "sieht", "man", "kaum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIS", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "So spritzt um sie der edle Schaum.", "tokens": ["So", "spritzt", "um", "sie", "der", "ed\u00b7le", "Schaum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Gel\u00e4chter und Gesang erschallt,", "tokens": ["Ge\u00b7l\u00e4ch\u00b7ter", "und", "Ge\u00b7sang", "er\u00b7schallt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Die Pritsche klatscht, der Puffer knallt.", "tokens": ["Die", "Prit\u00b7sche", "klatscht", ",", "der", "Puf\u00b7fer", "knallt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Wohl senkt die Sonne jetzt den Lauf,", "tokens": ["Wohl", "senkt", "die", "Son\u00b7ne", "jetzt", "den", "Lauf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Doch rauschen Feuergarben auf", "tokens": ["Doch", "rau\u00b7schen", "Feu\u00b7er\u00b7gar\u00b7ben", "auf"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Und werfen Sterne, gro\u00df und licht,", "tokens": ["Und", "wer\u00b7fen", "Ster\u00b7ne", ",", "gro\u00df", "und", "licht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "$,", "ADJD", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "Dem Abendhimmel ins Gesicht.", "tokens": ["Dem", "A\u00b7bend\u00b7him\u00b7mel", "ins", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.37": {"text": "Da dr\u00f6hnt der Hammer dumpf und schwer", "tokens": ["Da", "dr\u00f6hnt", "der", "Ham\u00b7mer", "dumpf", "und", "schwer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.38": {"text": "Zw\u00f6lfmal vom grauen Kirchturm her.", "tokens": ["Zw\u00f6lf\u00b7mal", "vom", "grau\u00b7en", "Kirch\u00b7turm", "her", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "PTKVZ", "$."], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.39": {"text": "Der Jubel schweigt, der Glanz erlischt,", "tokens": ["Der", "Ju\u00b7bel", "schweigt", ",", "der", "Glanz", "er\u00b7lischt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.40": {"text": "Die Kelter ist hinweggewischt,", "tokens": ["Die", "Kel\u00b7ter", "ist", "hin\u00b7weg\u00b7ge\u00b7wischt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.41": {"text": "Und aus der stillen Kammer nur", "tokens": ["Und", "aus", "der", "stil\u00b7len", "Kam\u00b7mer", "nur"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.42": {"text": "Glimmt eines L\u00e4mpchens letzte Spur.", "tokens": ["Glimmt", "ei\u00b7nes", "L\u00e4mp\u00b7chens", "letz\u00b7te", "Spur", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.43": {"text": "Der W\u00e4chter aber singet schon", "tokens": ["Der", "W\u00e4ch\u00b7ter", "a\u00b7ber", "sin\u00b7get", "schon"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NE", "ADV", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.44": {"text": "Das neue Jahr im alten Ton,", "tokens": ["Das", "neu\u00b7e", "Jahr", "im", "al\u00b7ten", "Ton", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.45": {"text": "Doch flie\u00dfet ihm wie Honigseim", "tokens": ["Doch", "flie\u00b7\u00dfet", "ihm", "wie", "Ho\u00b7ni\u00b7gseim"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "KOKOM", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.46": {"text": "Zum alten Spruch manch neu