{"dta.poem.21050": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Des Siechen Flehgesang.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1798", "urn": "urn:nbn:de:kobv:b4-200905193300", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Auf welchen Fluren wandelt Odalia?               ", "tokens": ["Auf", "wel\u00b7chen", "Flu\u00b7ren", "wan\u00b7delt", "O\u00b7da\u00b7lia", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "VVFIN", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "In welchen L\u00fcften athmet die Herrliche?", "tokens": ["In", "wel\u00b7chen", "L\u00fcf\u00b7ten", "ath\u00b7met", "die", "Herr\u00b7li\u00b7che", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "VVFIN", "ART", "NN", "$."], "meter": "---+-+--+--", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wo trinkt ihr dunkelblaues Auge", "tokens": ["Wo", "trinkt", "ihr", "dun\u00b7kel\u00b7blau\u00b7es", "Au\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Friedlich und liebend das Licht des Himmels?", "tokens": ["Fried\u00b7lich", "und", "lie\u00b7bend", "das", "Licht", "des", "Him\u00b7mels", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "ART", "NN", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "O komm' in deiner r\u00fchrenden M\u00e4dchenhuld!", "tokens": ["O", "komm'", "in", "dei\u00b7ner", "r\u00fch\u00b7ren\u00b7den", "M\u00e4d\u00b7chen\u00b7huld", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "In deiner herzgewinnenden Milde, komm!", "tokens": ["In", "dei\u00b7ner", "herz\u00b7ge\u00b7win\u00b7nen\u00b7den", "Mil\u00b7de", ",", "komm", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "In meinen Adern lodern Flammen;", "tokens": ["In", "mei\u00b7nen", "A\u00b7dern", "lo\u00b7dern", "Flam\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Blitze durchz\u00fccken die siechen Schl\u00e4fe.", "tokens": ["Blit\u00b7ze", "durch\u00b7z\u00fc\u00b7cken", "die", "sie\u00b7chen", "Schl\u00e4\u00b7fe", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "O komm' in deiner freundlichen Innigkeit!", "tokens": ["O", "komm'", "in", "dei\u00b7ner", "freund\u00b7li\u00b7chen", "In\u00b7nig\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "In deiner unentheiligten Reine, komm'!", "tokens": ["In", "dei\u00b7ner", "un\u00b7en\u00b7thei\u00b7lig\u00b7ten", "Rei\u00b7ne", ",", "komm'", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Denn deine Stirn' umleuchtet Ruhe,", "tokens": ["Denn", "dei\u00b7ne", "Stirn'", "um\u00b7leuch\u00b7tet", "Ru\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Heilung enttr\u00e4ufelt den Honiglippen.", "tokens": ["Hei\u00b7lung", "ent\u00b7tr\u00e4u\u00b7felt", "den", "Ho\u00b7ni\u00b7glip\u00b7pen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "Ein Augenblick Hinsinkens an deine Brust,", "tokens": ["Ein", "Au\u00b7gen\u00b7blick", "Hin\u00b7sin\u00b7kens", "an", "dei\u00b7ne", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ein Augenblick Umschlingens von deinem Arm,", "tokens": ["Ein", "Au\u00b7gen\u00b7blick", "Um\u00b7schlin\u00b7gens", "von", "dei\u00b7nem", "Arm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Erlabt mich, wie dein Fl\u00fcgelwehen,", "tokens": ["Er\u00b7labt", "mich", ",", "wie", "dein", "Fl\u00fc\u00b7gel\u00b7we\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Engel des Heiles und des Genesens.", "tokens": ["En\u00b7gel", "des", "Hei\u00b7les", "und", "des", "Ge\u00b7ne\u00b7sens", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "+--+-+-+--", "measure": "iambic.tetra.invert"}}}}}