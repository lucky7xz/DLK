{"dta.poem.7826": {"metadata": {"author": {"name": "Birken, Sigmund von", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1665", "urn": "urn:nbn:de:kobv:b4-200905198447", "language": ["de:0.99"], "booktitle": "Birken, Sigmund von: Pegnesische Gespr\u00e4chspiel-Gesellschaft von Nymfen und Hirten. N\u00fcrnberg, 1665."}, "poem": {"stanza.1": {"line.1": {"text": "Prinz gelehrter Leyer-Lieder/", "tokens": ["Prinz", "ge\u00b7lehr\u00b7ter", "Leyer\u00b7Lie\u00b7der", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$("], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Herzog deiner Sternenbr\u00fcder!", "tokens": ["Her\u00b7zog", "dei\u00b7ner", "Ster\u00b7nen\u00b7br\u00fc\u00b7der", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "la\u00df mir deinen Einflu\u00df fliessen!", "tokens": ["la\u00df", "mir", "dei\u00b7nen", "Ein\u00b7flu\u00df", "flies\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ach! wer itzt in Aganippen", "tokens": ["Ach", "!", "wer", "itzt", "in", "A\u00b7gan\u00b7ip\u00b7pen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "PWS", "ADV", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "m\u00f6chte baden seine Lippen/", "tokens": ["m\u00f6ch\u00b7te", "ba\u00b7den", "sei\u00b7ne", "Lip\u00b7pen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "PPOSAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "an Parnassens nassen F\u00fcssen!", "tokens": ["an", "Par\u00b7nas\u00b7sens", "nas\u00b7sen", "F\u00fcs\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "K\u00e4men doch/ die Pegas-brunnen/", "tokens": ["K\u00e4\u00b7men", "doch", "/", "die", "Pe\u00b7gas\u00b7brun\u00b7nen", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADV", "$(", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "mit der Pegnitz itzt gerunnen/", "tokens": ["mit", "der", "Peg\u00b7nitz", "itzt", "ge\u00b7run\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "die da Geister giessen ein!", "tokens": ["die", "da", "Geis\u00b7ter", "gies\u00b7sen", "ein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "NN", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "ach! da wolt ich trunken werden!", "tokens": ["ach", "!", "da", "wolt", "ich", "trun\u00b7ken", "wer\u00b7den", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ADV", "VMFIN", "PPER", "ADJD", "VAINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "da solt recht besungen seyn", "tokens": ["da", "solt", "recht", "be\u00b7sun\u00b7gen", "seyn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ADJD", "VVPP", "VAINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Dafne/ unsre Zier der Erden.", "tokens": ["Daf\u00b7ne", "/", "uns\u00b7re", "Zier", "der", "Er\u00b7den", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Zwar/ ob F\u00f6bus nit will scheinen:", "tokens": ["Zwar", "/", "ob", "F\u00f6\u00b7bus", "nit", "will", "schei\u00b7nen", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "KOUS", "NE", "PTKNEG", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ich wei\u00df hier der F\u00f6ben einen/", "tokens": ["Ich", "wei\u00df", "hier", "der", "F\u00f6\u00b7ben", "ei\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "ART", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "der auch Geistern gibet Feuer;", "tokens": ["der", "auch", "Geis\u00b7tern", "gi\u00b7bet", "Feu\u00b7er", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "NN", "VVFIN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "dessen Ruhm-Liecht Sonne-strahlet/", "tokens": ["des\u00b7sen", "Ruhm\u00b7Liecht", "Son\u00b7ne\u00b7strah\u00b7let", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PRELAT", "NN", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "und sein Stam\u0304haus Himmel-mahlet;", "tokens": ["und", "sein", "Stam\u0304haus", "Him\u00b7mel\u00b7mah\u00b7let", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "der auch spielt auf F\u00f6bus Leyer.", "tokens": ["der", "auch", "spielt", "auf", "F\u00f6\u00b7bus", "Le\u00b7yer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "APPR", "NE", "NE", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Und es kan/ in Sch\u00e4fer-gr\u00fcnden/", "tokens": ["Und", "es", "kan", "/", "in", "Sch\u00e4\u00b7fer\u00b7gr\u00fcn\u00b7den", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "$(", "APPR", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "seine Gunst/ den Geist entz\u00fcnden/", "tokens": ["sei\u00b7ne", "Gunst", "/", "den", "Geist", "ent\u00b7z\u00fcn\u00b7den", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "ART", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "st\u00e4rker/ als der Helikon.", "tokens": ["st\u00e4r\u00b7ker", "/", "als", "der", "He\u00b7li\u00b7kon", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$(", "KOUS", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Eine Dafne wir auch kennen/", "tokens": ["Ei\u00b7ne", "Daf\u00b7ne", "wir", "auch", "ken\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "PPER", "ADV", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "die/ wie Hyperions Sohn/", "tokens": ["die", "/", "wie", "Hy\u00b7pe\u00b7ri\u00b7ons", "Sohn", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "$(", "KOKOM", "NE", "NN", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.12": {"text": "er anschauet mit Entbrennen.", "tokens": ["er", "an\u00b7schau\u00b7et", "mit", "Ent\u00b7bren\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Spielt mir nun/ ihr Musen-Tr\u00e4ume!", "tokens": ["Spielt", "mir", "nun", "/", "ihr", "Mu\u00b7sen\u00b7Tr\u00e4u\u00b7me", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$(", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "was zeigt/ hier der Prinz der B\u00e4ume?", "tokens": ["was", "zeigt", "/", "hier", "der", "Prinz", "der", "B\u00e4u\u00b7me", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$(", "ADV", "ART", "NN", "ART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "was blickt von der Lorbeer-L\u00e4ube?", "tokens": ["was", "blickt", "von", "der", "Lor\u00b7beer\u00b7L\u00e4u\u00b7be", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ists das Bildnis von Evadne?", "tokens": ["Ists", "das", "Bild\u00b7nis", "von", "E\u00b7vad\u00b7ne", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "APPR", "NE", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Cadmus-Schwester? Ariadne?", "tokens": ["Cad\u00b7mus\u00b7Schwes\u00b7ter", "?", "A\u00b7riad\u00b7ne", "?"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$.", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.6": {"text": "Ists Oenone/ wie ich gl\u00e4ube?", "tokens": ["Ists", "O\u00b7e\u00b7no\u00b7ne", "/", "wie", "ich", "gl\u00e4u\u00b7be", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$(", "PWAV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Eine solche Zier anst\u00fcnde/", "tokens": ["Ei\u00b7ne", "sol\u00b7che", "Zier", "an\u00b7st\u00fcn\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Cepheus/ deinem Wunder-Kinde!", "tokens": ["Ce\u00b7pheus", "/", "dei\u00b7nem", "Wun\u00b7der\u00b7Kin\u00b7de", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$(", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "ob es Tyndaris auch ist?", "tokens": ["ob", "es", "Tyn\u00b7da\u00b7ris", "auch", "ist", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NE", "ADV", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Nein! in diese sch\u00f6nst-geschaffne/", "tokens": ["Nein", "!", "in", "die\u00b7se", "sch\u00f6nst\u00b7ge\u00b7schaff\u00b7ne", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "APPR", "PDAT", "ADJA", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "alle Zier zusammen fliest:", "tokens": ["al\u00b7le", "Zier", "zu\u00b7sam\u00b7men", "fliest", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Es ist/ unsres Dafnis Dafne.", "tokens": ["Es", "ist", "/", "uns\u00b7res", "Daf\u00b7nis", "Daf\u00b7ne", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$(", "PPOSAT", "NE", "NE", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Preist/ ihr S\u00e4nger bey den Heerden!", "tokens": ["Preist", "/", "ihr", "S\u00e4n\u00b7ger", "bey", "den", "Heer\u00b7den", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "PPOSAT", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "diesen Himmel dieser Erden.", "tokens": ["die\u00b7sen", "Him\u00b7mel", "die\u00b7ser", "Er\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "PDAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Seht ein G\u00f6ttlichs Angesichte!", "tokens": ["Seht", "ein", "G\u00f6tt\u00b7lichs", "An\u00b7ge\u00b7sich\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "wo sich Ernst und Huld verm\u00e4hlen/", "tokens": ["wo", "sich", "Ernst", "und", "Huld", "ver\u00b7m\u00e4h\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "NE", "KON", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "und sie sonder Red\u2019 empfehlen.", "tokens": ["und", "sie", "son\u00b7der", "Red'", "emp\u00b7feh\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PIAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Hier ist Inhalt zum Gedichte!", "tokens": ["Hier", "ist", "In\u00b7halt", "zum", "Ge\u00b7dich\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "APPRART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "hier sich Amor selbst ergetzet/", "tokens": ["hier", "sich", "A\u00b7mor", "selbst", "er\u00b7get\u00b7zet", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PRF", "NE", "ADV", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "und ihm auf die Stirne setzet", "tokens": ["und", "ihm", "auf", "die", "Stir\u00b7ne", "set\u00b7zet"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "APPR", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "einen Thron von Helfenbein.", "tokens": 