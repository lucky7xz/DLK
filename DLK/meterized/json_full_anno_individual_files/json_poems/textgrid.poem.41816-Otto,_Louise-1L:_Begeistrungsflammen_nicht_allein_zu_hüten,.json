{"textgrid.poem.41816": {"metadata": {"author": {"name": "Otto, Louise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Begeistrungsflammen nicht allein zu h\u00fcten,", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Begeistrungsflammen nicht allein zu h\u00fcten,", "tokens": ["Be\u00b7geis\u00b7trungs\u00b7flam\u00b7men", "nicht", "al\u00b7lein", "zu", "h\u00fc\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nein, zu entz\u00fcnden sei uns Ziel und Ruhm,", "tokens": ["Nein", ",", "zu", "ent\u00b7z\u00fcn\u00b7den", "sei", "uns", "Ziel", "und", "Ruhm", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PTKZU", "VVINF", "VAFIN", "PPER", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Auch wo des Krieges blut'ge Waffen w\u00fcten", "tokens": ["Auch", "wo", "des", "Krie\u00b7ges", "blut'\u00b7ge", "Waf\u00b7fen", "w\u00fc\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PWAV", "ART", "NN", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Harrt ", "tokens": ["Harrt"], "token_info": ["word"], "pos": ["NE"], "meter": "+", "measure": "single.up"}}, "stanza.2": {"line.1": {"text": "Wir m\u00f6gen schaudern und mit tausend Schmerzen", "tokens": ["Wir", "m\u00f6\u00b7gen", "schau\u00b7dern", "und", "mit", "tau\u00b7send", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "VVINF", "KON", "APPR", "CARD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den holden Frieden pl\u00f