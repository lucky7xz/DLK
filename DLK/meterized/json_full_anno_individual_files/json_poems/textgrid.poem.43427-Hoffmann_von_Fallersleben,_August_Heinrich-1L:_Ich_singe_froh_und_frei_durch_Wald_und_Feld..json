{"textgrid.poem.43427": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich singe froh und frei durch Wald und Feld.", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich singe froh und frei durch Wald und Feld.", "tokens": ["Ich", "sin\u00b7ge", "froh", "und", "frei", "durch", "Wald", "und", "Feld", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "KON", "ADJD", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Soust will ich nichts auf dieser weiten Welt.", "tokens": ["Soust", "will", "ich", "nichts", "auf", "die\u00b7ser", "wei\u00b7ten", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PIS", "APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch diese Freiheit g\u00f6nnet ihr mir nicht,", "tokens": ["Doch", "die\u00b7se", "Frei\u00b7heit", "g\u00f6n\u00b7net", "ihr", "mir", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "VVFIN", "PPER", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und raubt mir Wald und Feld und Luft und Licht.", "tokens": ["Und", "raubt", "mir", "Wald", "und", "Feld", "und", "Luft", "und", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "NN", "KON", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Thierqu\u00e4ler, glaubt ihr etwa mich zu zwingen?", "tokens": ["Thier\u00b7qu\