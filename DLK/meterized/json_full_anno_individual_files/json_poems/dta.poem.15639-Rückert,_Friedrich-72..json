{"dta.poem.15639": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "72.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Welt ist sch\u00f6n, die Welt ist gut, gesehn als Ganzes,", "tokens": ["Die", "Welt", "ist", "sch\u00f6n", ",", "die", "Welt", "ist", "gut", ",", "ge\u00b7sehn", "als", "Gan\u00b7zes", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "ART", "NN", "VAFIN", "ADJD", "$,", "VVPP", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Sch\u00f6pfung Fr\u00fchlingspracht, das Heer des Sternentanzes.", "tokens": ["Der", "Sch\u00f6p\u00b7fung", "Fr\u00fch\u00b7lings\u00b7pracht", ",", "das", "Heer", "des", "Ster\u00b7nen\u00b7tan\u00b7zes", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die Welt ist sch\u00f6n, ist gut, gesehn im einzelst Kleinen;", "tokens": ["Die", "Welt", "ist", "sch\u00f6n", ",", "ist", "gut", ",", "ge\u00b7sehn", "im", "ein\u00b7zelst", "Klei\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "VAFIN", "ADJD", "$,", "VVPP", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein jedes Tr\u00f6pfchen Thau kann Gottes Spiegel scheinen.", "tokens": ["Ein", "je\u00b7des", "Tr\u00f6pf\u00b7chen", "Thau", "kann", "Got\u00b7tes", "Spie\u00b7gel", "schei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "NN", "VMFIN", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Nur wo du Einzelnes auf Einzelnes beziehst,", "tokens": ["Nur", "wo", "du", "Ein\u00b7zel\u00b7nes", "auf", "Ein\u00b7zel\u00b7nes", "be\u00b7ziehst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWAV", "PPER", "PIS", "APPR", "PIS", "VVFIN", "$,"], "meter": "----+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "O wie vor lauter Streit du nicht den Frieden siehst!", "tokens": ["O", "wie", "vor", "lau\u00b7ter", "Streit", "du", "nicht", "den", "Frie\u00b7den", "siehst", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "APPR", "PIAT", "NN", "PPER", "PTKNEG", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Der Frieden ist im Kreis, im Mittelpunkt ist er,", "tokens": ["Der", "Frie\u00b7den", "ist", "im", "Kreis", ",", "im", "Mit\u00b7tel\u00b7punkt", "ist", "er", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPRART", "NN", "$,", "APPRART", "NN", "VAFIN", "PPER", "$,"], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.2": {"text": "Drum ist er \u00fcberall, doch ihn zu sehn ist schwer.", "tokens": ["Drum", "ist", "er", "\u00fc\u00b7be\u00b7rall", ",", "doch", "ihn", "zu", "sehn", "ist", "schwer", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "ADV", "$,", "KON", "PPER", "PTKZU", "VVINF", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Es ist die Eintracht, die sich aus der Zwietracht baut,", "tokens": ["Es", "ist", "die", "Ein\u00b7tracht", ",", "die", "sich", "aus", "der", "Zwiet\u00b7racht", "baut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PRELS", "PRF", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo mancher, vom Ger\u00fcst verwirrt, den Plan nicht schaut.", "tokens": ["Wo", "man\u00b7cher", ",", "vom", "Ge\u00b7r\u00fcst", "ver\u00b7wirrt", ",", "den", "Plan", "nicht", "schaut", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "$,", "APPRART", "NN", "ADJD", "$,", "ART", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Drum denke, was dich st\u00f6rt, da\u00df dich ein Schein beth\u00f6rt,", "tokens": ["Drum", "den\u00b7ke", ",", "was", "dich", "st\u00f6rt", ",", "da\u00df", "dich", "ein", "Schein", "be\u00b7th\u00f6rt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "$,", "PWS", "PPER", "VVFIN", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und was du nicht begreifst, gewi\u00df zum Plan geh\u00f6rt.", "tokens": ["Und", "was", "du", "nicht", "be\u00b7greifst", ",", "ge\u00b7wi\u00df", "zum", "Plan", "ge\u00b7h\u00f6rt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "PTKNEG", "VVFIN", "$,", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}