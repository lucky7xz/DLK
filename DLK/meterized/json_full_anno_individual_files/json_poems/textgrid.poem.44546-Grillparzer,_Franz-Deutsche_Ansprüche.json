{"textgrid.poem.44546": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Deutsche Anspr\u00fcche", "genre": "verse", "period": "N.A.", "pub_year": 1846, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es waren, wie euch wohl bekannt,", "tokens": ["Es", "wa\u00b7ren", ",", "wie", "euch", "wohl", "be\u00b7kannt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "PWAV", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der frommen M\u00e4nner sieben,", "tokens": ["Der", "from\u00b7men", "M\u00e4n\u00b7ner", "sie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die in die W\u00fcste sich verbannt", "tokens": ["Die", "in", "die", "W\u00fcs\u00b7te", "sich", "ver\u00b7bannt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "PRF", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und schlafend dort geblieben.", "tokens": ["Und", "schla\u00b7fend", "dort", "ge\u00b7blie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "So schliefen sie f\u00fcnfhundert Jahr", "tokens": ["So", "schlie\u00b7fen", "sie", "f\u00fcnf\u00b7hun\u00b7dert", "Jahr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "CARD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und tr\u00e4umten dies und jenes:", "tokens": ["Und", "tr\u00e4um\u00b7ten", "dies", "und", "je\u00b7nes", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PDS", "KON", "PDS", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Vom Nichts, von Geist, von Schein und Wahr", "tokens": ["Vom", "Nichts", ",", "von", "Geist", ",", "von", "Schein", "und", "Wahr"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "PIS", "$,", "APPR", "NN", "$,", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Viel Gutes und viel Sch\u00f6nes.", "tokens": ["Viel", "Gu\u00b7tes", "und", "viel", "Sch\u00f6\u00b7nes", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Zuletzt jedoch der Schlaf zerrann,", "tokens": ["Zu\u00b7letzt", "je\u00b7doch", "der", "Schlaf", "zer\u00b7rann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie standen auf den Beinen,", "tokens": ["Sie", "stan\u00b7den", "auf", "den", "Bei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und jedem kam die Sehnsucht an", "tokens": ["Und", "je\u00b7dem", "kam", "die", "Sehn\u00b7sucht", "an"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "VVFIN", "ART", "NN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nach Hause, zu den Seinen.", "tokens": ["Nach", "Hau\u00b7se", ",", "zu", "den", "Sei\u00b7nen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "ART", "PPOSS", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Sie gingen den bekannten Pfad,", "tokens": ["Sie", "gin\u00b7gen", "den", "be\u00b7kann\u00b7ten", "Pfad", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nur schien er sehr ver\u00e4ndert,", "tokens": ["Nur", "schien", "er", "sehr", "ver\u00b7\u00e4n\u00b7dert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er lief wie fr\u00fcher fort gerad,", "tokens": ["Er", "lief", "wie", "fr\u00fc\u00b7her", "fort", "ge\u00b7rad", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "ADJD", "PTKVZ", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch neu war er umr\u00e4ndert.", "tokens": ["Doch", "neu", "war", "er", "um\u00b7r\u00e4n\u00b7dert", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Wo sonst ein Baum, da stand ein Haus,", "tokens": ["Wo", "sonst", "ein", "Baum", ",", "da", "stand", "ein", "Haus", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "$,", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Statt Wiesen waren G\u00e4rten,", "tokens": ["Statt", "Wie\u00b7sen", "wa\u00b7ren", "G\u00e4r\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VAFIN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das schien denn doch ein wenig kraus", "tokens": ["Das", "schien", "denn", "doch", "ein", "we\u00b7nig", "kraus"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ADV", "ADV", "ART", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den wandernden Gef\u00e4hrten.", "tokens": ["Den", "wan\u00b7dern\u00b7den", "Ge\u00b7f\u00e4hr\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Und nun die Menschen vollends gar", "tokens": ["Und", "nun", "die", "Men\u00b7schen", "vol\u00b7lends", "gar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In sonderbaren Trachten,", "tokens": ["In", "son\u00b7der\u00b7ba\u00b7ren", "Trach\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "R\u00fcckgebend jenes: ", "tokens": ["R\u00fcck\u00b7ge\u00b7bend", "je\u00b7nes", ":"], "token_info": ["word", "word", "punct"], "pos": ["NN", "PDAT", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Da sie der Wandrer lachten.", "tokens": ["Da", "sie", "der", "Wand\u00b7rer", "lach\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "So kamen sie zur Stadt zuletzt,", "tokens": ["So", "ka\u00b7men", "sie", "zur", "Stadt", "zu\u00b7letzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zum Haus, das sonst das ihre;", "tokens": ["Zum", "Haus", ",", "das", "sonst", "das", "ih\u00b7re", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "ADV", "ART", "PPOSAT", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Von Fremden fanden sies besetzt,", "tokens": ["Von", "Frem\u00b7den", "fan\u00b7den", "sies", "be\u00b7setzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie weisend von der T\u00fcre.", "tokens": ["Sie", "wei\u00b7send", "von", "der", "T\u00fc\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Da eilen sie zur Obrigkeit", "tokens": ["Da", "ei\u00b7len", "sie", "zur", "Ob\u00b7rig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und klagen, schm\u00e4hen, weinen.", "tokens": ["Und", "kla\u00b7gen", ",", "schm\u00e4\u00b7hen", ",", "wei\u00b7nen", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "VVINF", "$,", "VVFIN", "$,", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Richter, sonst zum Schutz bereit,", "tokens": ["Der", "Rich\u00b7ter", ",", "sonst", "zum", "Schutz", "be\u00b7reit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "APPRART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Versteht kaum, was sie meinen.", "tokens": ["Ver\u00b7steht", "kaum", ",", "was", "sie", "mei\u00b7nen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Allm\u00e4hlich kommt er doch ans Ziel", "tokens": ["All\u00b7m\u00e4h\u00b7lich", "kommt", "er", "doch", "ans", "Ziel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der stammelnden Erkl\u00e4rung,", "tokens": ["Der", "stam\u00b7meln\u00b7den", "Er\u00b7kl\u00e4\u00b7rung", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da spricht er denn vom Rechte viel,", "tokens": ["Da", "spricht", "er", "denn", "vom", "Rech\u00b7te", "viel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPRART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vor allem von Verj\u00e4hrung.", "tokens": ["Vor", "al\u00b7lem", "von", "Ver\u00b7j\u00e4h\u00b7rung", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Er meint: es heilt wohl keine Macht", "tokens": ["Er", "meint", ":", "es", "heilt", "wohl", "kei\u00b7ne", "Macht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VVFIN", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Schl\u00e4ge, die euch trafen,", "tokens": ["Die", "Schl\u00e4\u00b7ge", ",", "die", "euch", "tra\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Denn man verliert, zu sp\u00e4t erwacht,", "t