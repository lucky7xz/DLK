{"textgrid.poem.53118": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wol dem, der sich bey zeiten", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wol dem, der sich bey zeiten", "tokens": ["Wol", "dem", ",", "der", "sich", "bey", "zei\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ART", "$,", "PRELS", "PRF", "APPR", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Auff s\u00fcsse Heyraht lenckt,", "tokens": ["Auff", "s\u00fcs\u00b7se", "Hey\u00b7raht", "lenckt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sich mit geehrten Leuten", "tokens": ["Sich", "mit", "ge\u00b7ehr\u00b7ten", "Leu\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PRF", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Gern zu befreunden denckt,", "tokens": ["Gern", "zu", "be\u00b7freun\u00b7den", "denckt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "---+-+", "measure": "unknown.measure.di"}, "line.5": {"text": "Vnd fast ein Mensch zu Hertzen,", "tokens": ["Vnd", "fast", "ein", "Mensch", "zu", "Hert\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Das wider alle Schmertzen", "tokens": ["Das", "wi\u00b7der", "al\u00b7le", "Schmert\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ihm Ruh vnd Frewde schenckt!", "tokens": ["Ihm", "Ruh", "vnd", "Frew\u00b7de", "schenckt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der selbst mit sich vertragen", "tokens": ["Der", "selbst", "mit", "sich", "ver\u00b7tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "APPR", "PRF", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vnd auch zugleich mit Gott,", "tokens": ["Vnd", "auch", "zu\u00b7gleich", "mit", "Gott", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er trutzt in b\u00f6sen Tagen", "tokens": ["Er", "trutzt", "in", "b\u00f6\u00b7sen", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Hellen gantze Rott'", "tokens": ["Der", "Hel\u00b7len", "gant\u00b7ze", "Rott'"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Vnd h\u00e4lt des Gl\u00fcckes Sachen,", "tokens": ["Vnd", "h\u00e4lt", "des", "Gl\u00fc\u00b7ckes", "Sa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die vns sonst irre machen", "tokens": ["Die", "vns", "sonst", "ir\u00b7re", "ma\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADV", "ADJD", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Getrost f\u00fcr einen Spott.", "tokens": ["Ge\u00b7trost", "f\u00fcr", "ei\u00b7nen", "Spott", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Er schleusset sich den Armen", "tokens": ["Er", "schleus\u00b7set", "sich", "den", "Ar\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Liebsten hertzlich ein", "tokens": ["Der", "Liebs\u00b7ten", "hertz\u00b7lich", "ein"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "ART"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vnd wei\u00df hie zu erwarmen,", "tokens": ["Vnd", "wei\u00df", "hie", "zu", "er\u00b7war\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Auch fr\u00f6r' es Ertz vnd Stein,", "tokens": ["Auch", "fr\u00f6r'", "es", "Ertz", "vnd", "Stein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "L\u00e4sst Lufft vnd Himmel st\u00fcrmen,", "tokens": ["L\u00e4sst", "Lufft", "vnd", "Him\u00b7mel", "st\u00fcr\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Weil er sich kan beschirmen", "tokens": ["Weil", "er", "sich", "kan", "be\u00b7schir\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "VMFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Vnd fern von Kummer seyn.", "tokens": ["Vnd", "fern", "von", "Kum\u00b7mer", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "NN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Gedult lehrt ihn gelohsen", "tokens": ["Ge\u00b7dult", "lehrt", "ihn", "ge\u00b7loh\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Angst, die auff Ihn f\u00e4llt,", "tokens": ["Die", "Angst", ",", "die", "auff", "Ihn", "f\u00e4llt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er wei\u00df, da\u00df nur mit Rosen", "tokens": ["Er", "wei\u00df", ",", "da\u00df", "nur", "mit", "Ro\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ADV", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Kein Stand sich vnterh\u00e4lt;", "tokens": ["Kein", "Stand", "sich", "vn\u00b7ter\u00b7h\u00e4lt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wer alle M\u00fch vnd Leiden", "tokens": ["Wer", "al\u00b7le", "M\u00fch", "vnd", "Lei\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "PIAT", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Au\u00df Z\u00e4rtlicheit wil meiden,", "tokens": ["Au\u00df", "Z\u00e4rt\u00b7li\u00b7cheit", "wil", "mei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Der fliehe diese Welt!", "tokens": ["Der", "flie\u00b7he", "die\u00b7se", "Welt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PDAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Der Sorgen Schaar auff Erden", "tokens": ["Der", "Sor\u00b7gen", "Schaar", "auff", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vmbringt nur Jedermann;", "tokens": ["Vm\u00b7bringt", "nur", "Je\u00b7der\u00b7mann", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PIS", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wil Ihm was schwerer werden,", "tokens": ["Wil", "Ihm", "was", "schwe\u00b7rer", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PIS", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Al\u00df er es tragen kan,", "tokens": ["Al\u00df", "er", "es", "tra\u00b7gen", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Macht jhm den Muth zu tr\u00fcbe:", "tokens": ["Macht", "jhm", "den", "Muth", "zu", "tr\u00fc\u00b7be", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "APPR", "ADJA", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.6": {"text": "Sein Mitt-genoss, die Liebe,", "tokens": ["Sein", "Mit\u00b7tge\u00b7noss", ",", "die", "Lie\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Tritt wacker mit Ihm an.", "tokens": ["Tritt", "wa\u00b7cker", "mit", "Ihm", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "F\u00fcr seiner Liebsten Sinnen", "tokens": ["F\u00fcr", "sei\u00b7ner", "Liebs\u00b7ten", "Sin\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vnd was Ihn sonst ergetzt,", "tokens": ["Vnd", "was", "Ihn", "sonst", "er\u00b7getzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wird Er der Noth kaum innen,", "tokens": ["Wird", "Er", "der", "Noth", "kaum", "in\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "ADV", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie starck sie an Ihn setzt.", "tokens": ["Wie", "starck", "sie", "an", "Ihn", "setzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Er steht in Lust versencket,", "tokens": ["Er", "steht", "in", "Lust", "ver\u00b7sen\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die alles, was Ihn kr\u00e4ncket,", "tokens": ["Die", "al\u00b7les", ",", "was", "Ihn", "kr\u00e4n\u00b7cket", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ihm au\u00df dem Sinne schw\u00e4tzt.", "tokens": ["Ihm", "au\u00df", "dem", "Sin\u00b7ne", "schw\u00e4tzt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.7": {"line.1": {"text": "Er macht mit dem Bescheide", "tokens": ["Er", "macht", "mit", "dem", "Be\u00b7schei\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Heyraht festen Schlu\u00df:", "tokens": ["Der", "Hey\u00b7raht", "fes\u00b7ten", "Schlu\u00df", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df Vnmuth neben Frewde", "tokens": ["Da\u00df", "Vn\u00b7muth", "ne\u00b7ben", "Frew\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "NN", "APPR", "NN"], "meter": "-++--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ihm stets begegnen mu\u00df,", "tokens": ["Ihm", "stets", "be\u00b7geg\u00b7nen", "mu\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Vnd wei\u00df in Zu-vertrawen", "tokens": ["Vnd", "wei\u00df", "in", "Zu\u00b7ver\u00b7tra\u00b7wen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Auff seinen Gott zu bawen,", "tokens": ["Auff", "sei\u00b7nen", "Gott", "zu", "ba\u00b7wen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Der h\u00e4lt auch bey Ihm Fu\u00df.", "tokens": ["Der", "h\u00e4lt", "auch", "bey", "Ihm", "Fu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "APPR", "PPER", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Wer wil, mag einsam bleiben!", "tokens": ["Wer", "wil", ",", "mag", "ein\u00b7sam", "blei\u00b7ben", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "$,", "VMFIN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Des armen Lebens Ziel", "tokens": ["Des", "ar\u00b7men", "Le\u00b7bens", "Ziel"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ist dennoch: sich beweiben.", "tokens": ["Ist", "den\u00b7noch", ":", "sich", "be\u00b7wei\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "$.", "PRF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Man sorg' auch nicht zu viel!", "tokens": ["Man", "sor\u00b7g'", "auch", "nicht", "zu", "viel", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "PTKNEG", "PTKA", "PIS", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Gott wird sein Volck erhalten,", "tokens": ["Gott", "wird", "sein", "Volck", "er\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Den l\u00e4sst ein Weiser walten", "tokens": ["Den", "l\u00e4sst", "ein", "Wei\u00b7ser", "wal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Vnd freyet, wenn er wil.", "tokens": ["Vnd", "fre\u00b7yet", ",", "wenn", "er", "wil", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "