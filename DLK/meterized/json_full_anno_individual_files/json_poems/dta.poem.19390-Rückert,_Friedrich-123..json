{"dta.poem.19390": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "123.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Wandrer im Gebirg verlor die rechten Steige,", "tokens": ["Der", "Wand\u00b7rer", "im", "Ge\u00b7birg", "ver\u00b7lor", "die", "rech\u00b7ten", "Stei\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und blickt umsonst umher, wer ihm dieselben zeige.", "tokens": ["Und", "blickt", "um\u00b7sonst", "um\u00b7her", ",", "wer", "ihm", "die\u00b7sel\u00b7ben", "zei\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PTKVZ", "$,", "PWS", "PPER", "PDS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Doch ein Einsiedler sitzt vertieft dort in Gebeten,", "tokens": ["Doch", "ein", "Ein\u00b7sied\u00b7ler", "sitzt", "ver\u00b7tieft", "dort", "in", "Ge\u00b7be\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "VVFIN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und fragend nach dem Weg, kommt er zu ihm getreten.", "tokens": ["Und", "fra\u00b7gend", "nach", "dem", "Weg", ",", "kommt", "er", "zu", "ihm", "ge\u00b7tre\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ART", "NN", "$,", "VVFIN", "PPER", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Da hebt der fromme Mann, und spricht dazu kein Wort,", "tokens": ["Da", "hebt", "der", "from\u00b7me", "Mann", ",", "und", "spricht", "da\u00b7zu", "kein", "Wort", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,", "KON", "VVFIN", "PAV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den Finger himmelan, und betet schweigend fort.", "tokens": ["Den", "Fin\u00b7ger", "him\u00b7me\u00b7lan", ",", "und", "be\u00b7tet", "schwei\u00b7gend", "fort", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "KON", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Es spricht der Wandersmann: Ich wei\u00df, da\u00df durch Gebet", "tokens": ["Es", "spricht", "der", "Wan\u00b7ders\u00b7mann", ":", "Ich", "wei\u00df", ",", "da\u00df", "durch", "Ge\u00b7bet"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$.", "PPER", "VVFIN", "$,", "KOUS", "APPR", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und Weltentsagung dort der Weg zum Himmel geht.", "tokens": ["Und", "Wel\u00b7ten\u00b7tsa\u00b7gung", "dort", "der", "Weg", "zum", "Him\u00b7mel", "geht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Doch jetzo m\u00f6cht' ich den zum n\u00e4chsten Dorfe wissen;", "tokens": ["Doch", "jet\u00b7zo", "m\u00f6cht'", "ich", "den", "zum", "n\u00e4chs\u00b7ten", "Dor\u00b7fe", "wis\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PPER", "ART", "APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn du die Kunde hast, so la\u00df mich sie nicht missen.", "tokens": ["Wenn", "du", "die", "Kun\u00b7de", "hast", ",", "so", "la\u00df", "mich", "sie", "nicht", "mis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VAFIN", "$,", "ADV", "VVIMP", "PPER", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Da wiegt der fromme Mann, und spricht dazu kein Wort,", "tokens": ["Da", "wiegt", "der", "from\u00b7me", "Mann", ",", "und", "spricht", "da\u00b7zu", "kein", "Wort", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,", "KON", "VVFIN", "PAV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das Haupt verneinend ernst. Der Wanderer geht fort,", "tokens": ["Das", "Haupt", "ver\u00b7nei\u00b7nend", "ernst", ".", "Der", "Wan\u00b7de\u00b7rer", "geht", "fort", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "VVFIN", "$.", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Und denkt: Was k\u00f6nnt' es wol dem frommen Manne schaden,", "tokens": ["Und", "denkt", ":", "Was", "k\u00f6nnt'", "es", "wol", "dem", "from\u00b7men", "Man\u00b7ne", "scha\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "PWS", "VMFIN", "PPER", "ADV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn er bewandert w\u00e4r' auch in der Erde Pfaden?", "tokens": ["Wenn", "er", "be\u00b7wan\u00b7dert", "w\u00e4r'", "auch", "in", "der", "Er\u00b7de", "Pfa\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "VAFIN", "ADV", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Am Himmel w\u00fcrd' es dort ihm keinen Eintrag thun,", "tokens": ["Am", "Him\u00b7mel", "w\u00fcrd'", "es", "dort", "ihm", "kei\u00b7nen", "Ein\u00b7trag", "thun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "ADV", "PPER", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zeigt' er den Weg mir hier; den zeige Gott mir nun!", "tokens": ["Zeigt'", "er", "den", "Weg", "mir", "hier", ";", "den", "zei\u00b7ge", "Gott", "mir", "nun", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PPER", "ADV", "$.", "ART", "ADJA", "NN", "PPER", "ADV", "$."], "meter": "++-+-+-+-+-+", "measure": "unknown.measure.septa"}}}}}