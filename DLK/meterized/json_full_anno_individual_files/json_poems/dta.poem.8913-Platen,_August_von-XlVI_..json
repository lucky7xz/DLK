{"dta.poem.8913": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "XlVI .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Komm fr\u00fcher diesmal, als du pflegst zu kommen!", "tokens": ["Komm", "fr\u00fc\u00b7her", "dies\u00b7mal", ",", "als", "du", "pflegst", "zu", "kom\u00b7men", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ADV", "$,", "KOUS", "PPER", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du bist ein Arzt, wenn unsre Brust beklommen,", "tokens": ["Du", "bist", "ein", "Arzt", ",", "wenn", "uns\u00b7re", "Brust", "be\u00b7klom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "KOUS", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein milder Arzt von immer sanfter Sitte!", "tokens": ["Ein", "mil\u00b7der", "Arzt", "von", "im\u00b7mer", "sanf\u00b7ter", "Sit\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADV", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "O k\u00f6nnt' ich schon in deiner Blumen Mitte,", "tokens": ["O", "k\u00f6nnt'", "ich", "schon", "in", "dei\u00b7ner", "Blu\u00b7men", "Mit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn kaum der Tag am Horizont entglommen,", "tokens": ["Wenn", "kaum", "der", "Tag", "am", "Ho\u00b7ri\u00b7zont", "ent\u00b7glom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Bis er in\u2019s Abendroth zulezt verschwommen,", "tokens": ["Bis", "er", "in's", "A\u00b7ben\u00b7droth", "zu\u00b7lezt", "ver\u00b7schwom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Von Tr\u00e4umen leben, ohne Wunsch und Bitte!", "tokens": ["Von", "Tr\u00e4u\u00b7men", "le\u00b7ben", ",", "oh\u00b7ne", "Wunsch", "und", "Bit\u00b7te", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$,", "KOUI", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wenn deine helle Sonne flammt im Blauen,", "tokens": ["Wenn", "dei\u00b7ne", "hel\u00b7le", "Son\u00b7ne", "flammt", "im", "Blau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "W\u00fcrd\u2019 ich, in's Gras gestreckt, nach oben blicken,", "tokens": ["W\u00fcrd'", "ich", ",", "in's", "Gras", "ge\u00b7streckt", ",", "nach", "o\u00b7ben", "bli\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "APPRART", "NN", "VVPP", "$,", "APPR", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und w\u00fcrde glauben meinen Freund zu schauen!", "tokens": ["Und", "w\u00fcr\u00b7de", "glau\u00b7ben", "mei\u00b7nen", "Freund", "zu", "schau\u00b7en", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "VVFIN", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Geblendet w\u00fcrde dann mein Auge nicken,", "tokens": ["Ge\u00b7blen\u00b7det", "w\u00fcr\u00b7de", "dann", "mein", "Au\u00b7ge", "ni\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ADV", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich w\u00fcrde schlummern bis die Sterne thauen,", "tokens": ["Ich", "w\u00fcr\u00b7de", "schlum\u00b7mern", "bis", "die", "Ster\u00b7ne", "thau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und mich im Schlaf an seinem Bild erquicken!", "tokens": ["Und", "mich", "im", "Schlaf", "an", "sei\u00b7nem", "Bild", "er\u00b7qui\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPRART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}