{"textgrid.poem.33109": {"metadata": {"author": {"name": "B\u00fcchner, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: \u00bbblanka, Blanka von Kastilien,", "genre": "verse", "period": "N.A.", "pub_year": 1849, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbblanka, Blanka von Kastilien,", "tokens": ["\u00bb", "blan\u00b7ka", ",", "Blan\u00b7ka", "von", "Kas\u00b7ti\u00b7li\u00b7en", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "NE", "$,", "NE", "APPR", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Herrscherin ob Frankreichs Lilien,", "tokens": ["Herr\u00b7sche\u00b7rin", "ob", "Fran\u00b7kreichs", "Li\u00b7li\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KOUS", "NE", "NE", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Rein wie sie und kalt wie Schnee!", "tokens": ["Rein", "wie", "sie", "und", "kalt", "wie", "Schnee", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "PPER", "KON", "ADJD", "KOKOM", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Streng und ernst siehst du mein Minnen,", "tokens": ["Streng", "und", "ernst", "siehst", "du", "mein", "Min\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJD", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Keinen Blick selbst kann gewinnen", "tokens": ["Kei\u00b7nen", "Blick", "selbst", "kann", "ge\u00b7win\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "ADV", "VMFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Meiner Liebe tiefes Weh!\u00ab", "tokens": ["Mei\u00b7ner", "Lie\u00b7be", "tie\u00b7fes", "Weh", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Thibaut von Navarra's Klagen", "tokens": ["Thi\u00b7baut", "von", "Na\u00b7var\u00b7ra's", "Kla\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "NE", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Sind's, die so alln\u00e4chtlich tragen", "tokens": ["Sin\u00b7d's", ",", "die", "so", "all\u00b7n\u00e4cht\u00b7lich", "tra\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "PRELS", "ADV", "ADJD", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Stille L\u00fcfte durch die Au.", "tokens": ["Stil\u00b7le", "L\u00fcf\u00b7te", "durch", "die", "Au", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "All' sein Sinnen, all' sein Streben", "tokens": ["All'", "sein", "Sin\u00b7nen", ",", "all'", "sein", "Stre\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIAT", "PPOSAT", "NN", "$,", "PIAT", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Hat er ihr dahin gegeben,", "tokens": ["Hat", "er", "ihr", "da\u00b7hin", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "PAV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Dieser k\u00f6niglichen Frau.", "tokens": ["Die\u00b7ser", "k\u00f6\u00b7nig\u00b7li\u00b7chen", "Frau", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Aber wie der Mond die Bahnen", "tokens": ["A\u00b7ber", "wie", "der", "Mond", "die", "Bah\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ziehet, ohne nur zu ahnen", "tokens": ["Zie\u00b7het", ",", "oh\u00b7ne", "nur", "zu", "ah\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "KOUI", "ADV", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Erdenleid, so geht sie her;", "tokens": ["Er\u00b7den\u00b7leid", ",", "so", "geht", "sie", "her", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Lebt in Ludwig ganz, dem Sohne;", "tokens": ["Lebt", "in", "Lud\u00b7wig", "ganz", ",", "dem", "Soh\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NE", "ADV", "$,", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Den einst schm\u00fcckt des Heilg'en Krone \u2013", "tokens": ["Den", "einst", "schm\u00fcckt", "des", "Heil\u00b7g'\u00b7en", "Kro\u00b7ne", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Und sein Lieben wird stets mehr!", "tokens": ["Und", "sein", "Lie\u00b7ben", "wird", "stets", "mehr", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "VAFIN", "ADV", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Was dem Mann sonst d\u00fcnkt das Beste,", "tokens": ["Was", "dem", "Mann", "sonst", "d\u00fcnkt", "das", "Bes\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kriegsget\u00fcmmel, Schlacht und Feste", "tokens": ["Kriegs\u00b7ge\u00b7t\u00fcm\u00b7mel", ",", "Schlacht", "und", "Fes\u00b7te"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Flieht er wie ein scheues Wild;", "tokens": ["Flieht", "er", "wie", "ein", "scheu\u00b7es", "Wild", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KOKOM", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Keine Wunde kann er schlagen,", "tokens": ["Kei\u00b7ne", "Wun\u00b7de", "kann", "er", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Im Gefecht, beim wildsten Jagen", "tokens": ["Im", "Ge\u00b7fecht", ",", "beim", "wilds\u00b7ten", "Ja\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Steht vor ihm ihr s\u00fc\u00dfes Bild. \u2013", "tokens": ["Steht", "vor", "ihm", "ihr", "s\u00fc\u00b7\u00dfes", "Bild", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "APPR", "PPER", "PPOSAT", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Sprach ein Greis zum Vielgetreuen:", "tokens": ["Sprach", "ein", "Greis", "zum", "Viel\u00b7ge\u00b7treu\u00b7en", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "\u00bbsoll dein Gram nicht sich erneuen,", "tokens": ["\u00bb", "soll", "dein", "Gram", "nicht", "sich", "er\u00b7neu\u00b7en", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VMFIN", "PPOSAT", "NN", "PTKNEG", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Stets, so folge meinem Wort;", "tokens": ["Stets", ",", "so", "fol\u00b7ge", "mei\u00b7nem", "Wort", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Liebe gleicht des S\u00fcdens Bl\u00fcthe,", "tokens": ["Lie\u00b7be", "gleicht", "des", "S\u00fc\u00b7dens", "Bl\u00fc\u00b7the", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Treibt im innersten Gem\u00fcthe", "tokens": ["Treibt", "im", "in\u00b7ners\u00b7ten", "Ge\u00b7m\u00fc\u00b7the"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Unaufhaltsam, ewig fort!", "tokens": ["Un\u00b7auf\u00b7halt\u00b7sam", ",", "e\u00b7wig", "fort", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Kannst sie nicht zum Tod bek\u00e4mpfen,", "tokens": ["Kannst", "sie", "nicht", "zum", "Tod", "be\u00b7k\u00e4mp\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aber ihre Schmerzen d\u00e4mpfen", "tokens": ["A\u00b7ber", "ih\u00b7re", "Schmer\u00b7zen", "d\u00e4mp\u00b7fen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "In des Sch\u00f6nen Zauberreich;", "tokens": ["In", "des", "Sch\u00f6\u00b7nen", "Zau\u00b7ber\u00b7reich", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Nimm den Griffel, nimm die Laute", "tokens": ["Nimm", "den", "Grif\u00b7fel", ",", "nimm", "die", "Lau\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "ART", "NN", "$,", "VVIMP", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und, was dir dein Gram vertraute,", "tokens": ["Und", ",", "was", "dir", "dein", "Gram", "ver\u00b7trau\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWS", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Str\u00f6m' es aus in Liedern reich!", "tokens": ["Str\u00f6m'", "es", "aus", "in", "Lie\u00b7dern", "reich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "APPR", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Nur zum Ruhm der Heldensprossen", "tokens": ["Nur", "zum", "Ruhm", "der", "Hel\u00b7den\u00b7spros\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hat sich Wort und Ton ergossen,", "tokens": ["Hat", "sich", "Wort", "und", "Ton", "er\u00b7gos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PRF", "NN", "KON", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ungef\u00fcgig oft und rauh.", "tokens": ["Un\u00b7ge\u00b7f\u00fc\u00b7gig", "oft", "und", "rauh", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "R\u00fchre du die sanft'ren Saiten,", "tokens": ["R\u00fch\u00b7re", "du", "die", "sanft'\u00b7ren", "Sai\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sing' der Seele Lust und Leiden,", "tokens": ["Sing'", "der", "See\u00b7le", "Lust", "und", "Lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Trag' dein Weh im Preis der Frau!\u00ab", "tokens": ["Trag'", "dein", "Weh", "im", "Preis", "der", "Frau", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPRART", "NN", "ART", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Und Navarra's K\u00f6nig lauschet,", "tokens": ["Und", "Na\u00b7var\u00b7ra's", "K\u00f6\u00b7nig", "lau\u00b7schet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "NN", "VVFIN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Mit dem Schwert die Leier tauschet \u2013", "tokens": ["Mit", "dem", "Schwert", "die", "Lei\u00b7er", "tau\u00b7schet", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ward gesund durch Lied und Ton.", "tokens": ["Ward", "ge\u00b7sund", "durch", "Lied", "und", "Ton", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Doch seitdem Gedicht und Singen,", "tokens": ["Doch", "seit\u00b7dem", "Ge\u00b7dicht", "und", "Sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Liebe wecken, Liebe bringen", "tokens": ["Lie\u00b7be", "we\u00b7cken", ",", "Lie\u00b7be", "brin\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "VVINF", "$,", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und ist Lieb' ihr sch\u00f6nster Lohn! \u2013", "tokens": ["Und", "ist", "Lieb'", "ihr", "sch\u00f6ns\u00b7ter", "Lohn", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VAFIN", "NN", "PPOSAT", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "\u00bbblanka, Blanka von Kastilien,", "tokens": ["\u00bb", "blan\u00b7ka", ",", "Blan\u00b7ka", "von", "Kas\u00b7ti\u00b7li\u00b7en", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "NE", "$,", "NE", "APPR", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Herrscherin ob Frankreichs Lilien,", "tokens": ["Herr\u00b7sche\u00b7rin", "ob", "Fran\u00b7kreichs", "Li\u00b7li\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KOUS", "NE", "NE", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Rein wie sie und kalt wie Schnee!", "tokens": ["Rein", "wie", "sie", "und", "kalt", "wie", "Schnee", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "PPER", "KON", "ADJD", "KOKOM", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Streng und ernst siehst du mein Minnen,", "tokens": ["Streng", "und", "ernst", "siehst", "du", "mein", "Min\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJD", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Keinen Blick selbst kann gewinnen", "tokens": ["Kei\u00b7nen", "Blick", "selbst", "kann", "ge\u00b7win\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "ADV", "VMFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Meiner Liebe tiefes Weh!\u00ab", "tokens": ["Mei\u00b7ner", "Lie\u00b7be", "tie\u00b7fes", "Weh", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Thibaut von Navarra's Klagen", "tokens": ["Thi\u00b7baut", "von", "Na\u00b7var\u00b7ra's", "Kla\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "NE", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Sind's, die so alln\u00e4chtlich tragen", "tokens": ["Sin\u00b7d's", ",", "die", "so", "all\u00b7n\u00e4cht\u00b7lich", "tra\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "PRELS", "ADV", "ADJD", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Stille L\u00fcfte durch die Au.", "tokens": ["Stil\u00b7l