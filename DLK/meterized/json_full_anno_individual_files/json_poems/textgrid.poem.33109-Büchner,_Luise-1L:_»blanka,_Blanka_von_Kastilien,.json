": {"text": "Keine Wunde kann er schlagen,", "tokens": ["Kei\u00b7ne", "Wun\u00b7de", "kann", "er", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Im Gefecht, beim wildsten Jagen", "tokens": ["Im", "Ge\u00b7fecht", ",", "beim", "wilds\u00b7ten", "Ja\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Steht vor ihm ihr s\u00fc\u00dfes Bild. \u2013", "tokens": ["Steht", "vor", "ihm", "ihr", "s\u00fc\u00b7\u00dfes", "Bild", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "APPR", "PPER", "PPOSAT", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Sprach ein Greis zum Vielgetreuen:", "tokens": ["Sprach", "ein", "Greis", "zum", "Viel\u00b7ge\u00b7treu\u00b7en", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "\u00bbsoll dein Gram nicht sich erneuen,", "tokens": ["\u00bb", "soll", "dein", "Gram", "nicht", "sich", "er\u00b7neu\u00b7en", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VMFIN", "PPOSAT", "NN", "PTKNEG", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Stets, so folge meinem Wort;", "tokens": ["Stets", ",", "so", "fol\u00b7ge", "mei\u00b7nem", "Wort", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Liebe gleicht des S\u00