{"dta.poem.6308": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Hitze und von der K\u00e4lte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dieweil das Feur nun keine Gleichheit heget", "tokens": ["Die\u00b7weil", "das", "Feur", "nun", "kei\u00b7ne", "Gleich\u00b7heit", "he\u00b7get"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADV", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit dem, was es in uns erreget;", "tokens": ["Mit", "dem", ",", "was", "es", "in", "uns", "er\u00b7re\u00b7get", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "PWS", "PPER", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So findet unser Geist, wenn man es recht erweget,", "tokens": ["So", "fin\u00b7det", "un\u00b7ser", "Geist", ",", "wenn", "man", "es", "recht", "er\u00b7we\u00b7get", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$,", "KOUS", "PIS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Den wahren Grund von seiner W\u00fcrckung bald,", "tokens": ["Den", "wah\u00b7ren", "Grund", "von", "sei\u00b7ner", "W\u00fcr\u00b7ckung", "bald", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Was seine fressig-streng-und hefftige Gewalt,", "tokens": ["Was", "sei\u00b7ne", "fres\u00b7sig\u00b7streng\u00b7\u00b7und", "heff\u00b7ti\u00b7ge", "Ge\u00b7walt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Was seine hitzige Beschaffenheit,", "tokens": ["Was", "sei\u00b7ne", "hit\u00b7zi\u00b7ge", "Be\u00b7schaf\u00b7fen\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und welches seine Schnelligkeit.", "tokens": ["Und", "wel\u00b7ches", "sei\u00b7ne", "Schnel\u00b7lig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PWAT", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}