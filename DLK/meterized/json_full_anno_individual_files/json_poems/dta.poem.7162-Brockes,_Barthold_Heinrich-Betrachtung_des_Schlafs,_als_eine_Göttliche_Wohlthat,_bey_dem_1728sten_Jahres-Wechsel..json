{"dta.poem.7162": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Betrachtung des Schlafs,  \n als  \n eine G\u00f6ttliche Wohlthat,  \n bey dem 1728sten Jahres-Wechsel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Auch hast Du GOTT und Vater abermahl", "tokens": ["Auch", "hast", "Du", "GoTT", "und", "Va\u00b7ter", "a\u00b7ber\u00b7mahl"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "NE", "KON", "NN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "In diesem Jahr die achte Zahl", "tokens": ["In", "die\u00b7sem", "Jahr", "die", "ach\u00b7te", "Zahl"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von meinen Kindern noch vermehret,", "tokens": ["Von", "mei\u00b7nen", "Kin\u00b7dern", "noch", "ver\u00b7meh\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und mir noch einen Sohn bescheret,", "tokens": ["Und", "mir", "noch", "ei\u00b7nen", "Sohn", "be\u00b7sche\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "F\u00fcr den, so wie auch f\u00fcr die andern alle,", "tokens": ["F\u00fcr", "den", ",", "so", "wie", "auch", "f\u00fcr", "die", "an\u00b7dern", "al\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "ADV", "KOKOM", "ADV", "APPR", "ART", "ADJA", "PIS", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ich Deine Huld noch ferner zu erbitten", "tokens": ["Ich", "Dei\u00b7ne", "Huld", "noch", "fer\u00b7ner", "zu", "er\u00b7bit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "PPOSAT", "NN", "ADV", "ADV", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Mich nicht enthalten kan. ", "tokens": ["Mich", "nicht", "ent\u00b7hal\u00b7ten", "kan", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Das h\u00f6re, liebster GOTT! erbarme doch aus Gnaden", "tokens": ["Das", "h\u00f6\u00b7re", ",", "liebs\u00b7ter", "GoTT", "!", "er\u00b7bar\u00b7me", "doch", "aus", "Gna\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "$,", "ADJA", "NN", "$.", "VVFIN", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Dich ihrer fernerhin, wie Du bisher gethan,", "tokens": ["Dich", "ih\u00b7rer", "fer\u00b7ner\u00b7hin", ",", "wie", "Du", "bis\u00b7her", "ge\u00b7than", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "PTKVZ", "$,", "PWAV", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und nimm Dich V\u00e4terlich der kleinen Heerde an!", "tokens": ["Und", "nimm", "Dich", "V\u00e4\u00b7ter\u00b7lich", "der", "klei\u00b7nen", "Heer\u00b7de", "an", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "PRF", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ach! habe Deine Lust an ihnen,", "tokens": ["Ach", "!", "ha\u00b7be", "Dei\u00b7ne", "Lust", "an", "ih\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VAFIN", "PPOSAT", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Und la\u00df sie Dir getreu aus allen Kr\u00e4fften dienen!", "tokens": ["Und", "la\u00df", "sie", "Dir", "ge\u00b7treu", "aus", "al\u00b7len", "Kr\u00e4ff\u00b7ten", "die\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "PPER", "ADJD", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Gieb ihnen Deine Gr\u00f6\u00df' und Liebe zu erkennen;", "tokens": ["Gieb", "ih\u00b7nen", "Dei\u00b7ne", "Gr\u00f6\u00df'", "und", "Lie\u00b7be", "zu", "er\u00b7ken\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "KON", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "So werden sie sich nicht von Deinen Wegen trennen!", "tokens": ["So", "wer\u00b7den", "sie", "sich", "nicht", "von", "Dei\u00b7nen", "We\u00b7gen", "tren\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "PTKNEG", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}