{"dta.poem.10898": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "No.  2.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Halb in frisch-und k\u00fchlem Schatten, halb in schw\u00fclem Son-\nnenschein,", "tokens": ["Halb", "in", "frisch\u00b7und", "k\u00fch\u00b7lem", "Schat\u00b7ten", ",", "halb", "in", "schw\u00fc\u00b7lem", "Son", "nen\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "ADJA", "NN", "$,", "ADJD", "APPR", "ADJA", "TRUNC", "PTKANT", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Unter Bl\u00e4tter-reichen B\u00e4umen, zwischen Kr\u00e4uter-reichen H\u00fcgeln,", "tokens": ["Un\u00b7ter", "Bl\u00e4t\u00b7ter\u00b7rei\u00b7chen", "B\u00e4u\u00b7men", ",", "zwi\u00b7schen", "Kr\u00e4u\u00b7ter\u00b7rei\u00b7chen", "H\u00fc\u00b7geln", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "$,", "APPR", "NN", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Sieht man einen edlen Hirsch, hier im klaren Bach sich spiegeln,", "tokens": ["Sieht", "man", "ei\u00b7nen", "ed\u00b7len", "Hirsch", ",", "hier", "im", "kla\u00b7ren", "Bach", "sich", "spie\u00b7geln", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ART", "ADJA", "NN", "$,", "ADV", "APPRART", "ADJA", "NN", "PRF", "VVINF", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "So nat\u00fcrlich, da\u00df der Schein selbst ein Urbild scheint zu seyn.", "tokens": ["So", "na\u00b7t\u00fcr\u00b7lich", ",", "da\u00df", "der", "Schein", "selbst", "ein", "Ur\u00b7bild", "scheint", "zu", "seyn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "KOUS", "ART", "NN", "ADV", "ART", "NN", "VVFIN", "PTKZU", "VAINF", "$."], "meter": "--+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Ist gleich seine Stellung still; L\u00e4\u00dft uns doch sein rasches Wesen", "tokens": ["Ist", "gleich", "sei\u00b7ne", "Stel\u00b7lung", "still", ";", "L\u00e4\u00dft", "uns", "doch", "sein", "ra\u00b7sches", "We\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "PTKVZ", "$.", "VVFIN", "PPER", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "-+--+--+--+--+-", "measure": "amphibrach.penta.plus"}, "line.6": {"text": "Seine sch\u00fcchterne Natur, aus fast regen Z\u00fcgen, lesen.", "tokens": ["Sei\u00b7ne", "sch\u00fcch\u00b7ter\u00b7ne", "Na\u00b7tur", ",", "aus", "fast", "re\u00b7gen", "Z\u00fc\u00b7gen", ",", "le\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "APPR", "ADV", "ADJA", "NN", "$,", "VVINF", "$."], "meter": "+-+---+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.7": {"text": "Seht! es r\u00fchren sich die Ohren. Schaut! die Augen sehn", "tokens": ["Seht", "!", "es", "r\u00fch\u00b7ren", "sich", "die", "Oh\u00b7ren", ".", "Schaut", "!", "die", "Au\u00b7gen", "sehn"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$.", "PPER", "VVFIN", "PRF", "ART", "NN", "$.", "VVFIN", "$.", "ART", "NN", "VVINF"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.8": {"text": "H\u00f6rt! ob man nicht eigentlich das Geraschel h\u00f6ren kann,", "tokens": ["H\u00f6rt", "!", "ob", "man", "nicht", "ei\u00b7gent\u00b7lich", "das", "Ge\u00b7ra\u00b7schel", "h\u00f6\u00b7ren", "kann", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$.", "KOUS", "PIS", "PTKNEG", "ADV", "ART", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.9": {"text": "Des von ihm zertretnen Schilfs. Edler Ridinger, dein Geist,", "tokens": ["Des", "von", "ihm", "zer\u00b7tret\u00b7nen", "Schilfs", ".", "Ed\u00b7ler", "Ri\u00b7din\u00b7ger", ",", "dein", "Geist", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "ADJA", "NN", "$.", "ADJA", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.10": {"text": "Welcher uns des Sch\u00f6pfers Macht, in der K\u00f6rper Sch\u00f6nheit,", "tokens": ["Wel\u00b7cher", "uns", "des", "Sch\u00f6p\u00b7fers", "Macht", ",", "in", "der", "K\u00f6r\u00b7per", "Sch\u00f6n\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "PPER", "ART", "NN", "NN", "$,", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.11": {"text": "Zeiget, welche Kraft, zu bilden, Gott den Geistern eingesenket,", "tokens": ["Zei\u00b7get", ",", "wel\u00b7che", "Kraft", ",", "zu", "bil\u00b7den", ",", "Gott", "den", "Geis\u00b7tern", "ein\u00b7ge\u00b7sen\u00b7ket", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAT", "NN", "$,", "PTKZU", "VVINF", "$,", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.12": {"text": "Und zugleich, wie gro\u00df das Maa\u00df, welches dir von Jhm ge-", "tokens": ["Und", "zu\u00b7gleich", ",", "wie", "gro\u00df", "das", "Maa\u00df", ",", "wel\u00b7ches", "dir", "von", "Jhm", "ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "$,", "PWAV", "ADJD", "ART", "NN", "$,", "PRELS", "PPER", "APPR", "PPER", "TRUNC"], "meter": "--+-+-+--+-+-", "measure": "iambic.penta.relaxed"}}}}}