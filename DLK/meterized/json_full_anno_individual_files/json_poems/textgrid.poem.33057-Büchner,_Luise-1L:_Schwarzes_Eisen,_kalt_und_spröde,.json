{"textgrid.poem.33057": {"metadata": {"author": {"name": "B\u00fcchner, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Schwarzes Eisen, kalt und spr\u00f6de,", "genre": "verse", "period": "N.A.", "pub_year": 1849, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schwarzes Eisen, kalt und spr\u00f6de,", "tokens": ["Schwar\u00b7zes", "Ei\u00b7sen", ",", "kalt", "und", "spr\u00f6\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJD", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schelten m\u00f6chte ich dich nicht,", "tokens": ["Schel\u00b7ten", "m\u00f6ch\u00b7te", "ich", "dich", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PRF", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Weil es dir an Lebensw\u00e4rme", "tokens": ["Weil", "es", "dir", "an", "Le\u00b7bens\u00b7w\u00e4r\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und an Biegsamkeit gebricht.", "tokens": ["Und", "an", "Biegs\u00b7am\u00b7keit", "ge\u00b7bricht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Bist du doch in Feuersgluthen", "tokens": ["Bist", "du", "doch", "in", "Feu\u00b7ers\u00b7glu\u00b7then"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zischend einst emporgewallt,", "tokens": ["Zi\u00b7schend", "einst", "em\u00b7por\u00b7ge\u00b7wallt", ","], "token_info": ["word", "word", "word", "punct