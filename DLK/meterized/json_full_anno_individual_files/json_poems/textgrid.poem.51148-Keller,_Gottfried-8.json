{"textgrid.poem.51148": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "8", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So beginnt es rings zu leben,", "tokens": ["So", "be\u00b7ginnt", "es", "rings", "zu", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und die alten Spinnen weben,", "tokens": ["Und", "die", "al\u00b7ten", "Spin\u00b7nen", "we\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die schwarzen M\u00e4use nagen,", "tokens": ["Und", "die", "schwar\u00b7zen", "M\u00e4u\u00b7se", "na\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und ich wollt, es w\u00fcrde tagen!", "tokens": ["Und", "ich", "wollt", ",", "es", "w\u00fcr\u00b7de", "ta\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "$,", "PPER", "VAFIN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Hielt den Teufel f\u00fcr gestorben,", "tokens": ["Hielt", "den", "Teu\u00b7fel", "f\u00fcr", "ge\u00b7stor\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und nun spukt er unverdorben", "tokens": ["Und", "nun", "spukt", "er", "un\u00b7ver\u00b7dor\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Noch in diesen Mauerschl\u00fcften \u2013", "tokens": ["Noch", "in", "die\u00b7sen", "Mau\u00b7er\u00b7schl\u00fcf\u00b7ten", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PDAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bis der Zeugwart kommt, zu l\u00fcften!", "tokens": ["Bis", "der", "Zeug\u00b7wart", "kommt", ",", "zu", "l\u00fcf\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$,", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Zeugwart ist der Herr der St\u00fcrme,", "tokens": ["Zeug\u00b7wart", "ist", "der", "Herr", "der", "St\u00fcr\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "++--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Der die Felsen bricht und T\u00fcrme", "tokens": ["Der", "die", "Fel\u00b7sen", "bricht", "und", "T\u00fcr\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ART", "NN", "VVFIN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der Torheit rohen Willen", "tokens": ["Und", "der", "Tor\u00b7heit", "ro\u00b7hen", "Wil\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wird mit Bitternissen stillen!", "tokens": ["Wird", "mit", "Bit\u00b7ter\u00b7nis\u00b7sen", "stil\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Wehen wird's in Ungewittern,", "tokens": ["We\u00b7hen", "wird's", "in", "Un\u00b7ge\u00b7wit\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df das Haus im Grund mu\u00df zittern", "tokens": ["Da\u00df", "das", "Haus", "im", "Grund", "mu\u00df", "zit\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPRART", "NN", "VMFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die Ziegel auf dem Dach", "tokens": ["Und", "die", "Zie\u00b7gel", "auf", "dem", "Dach"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Klappern uns vor Ungemach!", "tokens": ["Klap\u00b7pern", "uns", "vor", "Un\u00b7ge\u00b7mach", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Wohl uns, kann man alsdann sagen:", "tokens": ["Wohl", "uns", ",", "kann", "man", "als\u00b7dann", "sa\u00b7gen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "$,", "VMFIN", "PIS", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die das Gl\u00fcck nicht mochten tragen,", "tokens": ["Die", "das", "Gl\u00fcck", "nicht", "moch\u00b7ten", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "PTKNEG", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Haben ihres Unsterns Nacht", "tokens": ["Ha\u00b7ben", "ih\u00b7re