{"textgrid.poem.60163": {"metadata": {"author": {"name": "Karsch, Anna Louisa", "birth": "N.A.", "death": "N.A."}, "title": "1L: Seyd mir beklagt, ihr, in das Garn verlockte!", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Seyd mir beklagt, ihr, in das Garn verlockte!", "tokens": ["Seyd", "mir", "be\u00b7klagt", ",", "ihr", ",", "in", "das", "Garn", "ver\u00b7lock\u00b7te", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "PPER", "VVFIN", "$,", "PPER", "$,", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Euch hat aus hoher Luft geh\u00f6rt", "tokens": ["Euch", "hat", "aus", "ho\u00b7her", "Luft", "ge\u00b7h\u00f6rt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der fromme F\u00fchlende; euch h\u00f6rte der Verstockte", "tokens": ["Der", "from\u00b7me", "F\u00fch\u00b7len\u00b7de", ";", "euch", "h\u00f6r\u00b7te", "der", "Ver\u00b7stock\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der keinen Gott erkennt und ehrt.", "tokens": ["Der", "kei\u00b7nen", "Gott", "er\u00b7kennt", "und", "ehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ihr sangt dem Landmann kleine Fr\u00fclings Bothen!", "tokens": ["Ihr", "sangt", "dem", "Land\u00b7mann", "klei\u00b7ne", "Fr\u00fc\u00b7lings", "Bo\u00b7then", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ihr sangt der B\u00e4urin Hoffnung zu;", "tokens": ["Ihr", "sangt", "der", "B\u00e4u\u00b7rin", "Hoff\u00b7nung", "zu", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er grif den Pflug, und sie, versprach bald von der todten", "tokens": ["Er", "grif", "den", "Pflug", ",", "und", "sie", ",", "ver\u00b7sprach", "bald", "von", "der", "tod\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "KON", "PPER", "$,", "VVFIN", "ADV", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Eiskalten Erde Gra\u00df der Kuh!", "tokens": ["Eis\u00b7kal\u00b7ten", "Er\u00b7de", "Gra\u00df", "der", "Kuh", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "ART", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.3": {"line.1": {"text": "Wenn in der Stadt zu satt gewordne Schl\u00e4fer,", "tokens": ["Wenn", "in", "der", "Stadt", "zu", "satt", "ge\u00b7word\u00b7ne", "Schl\u00e4\u00b7fer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "PTKA", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sechs Stunden nach der Sonnen Blick,", "tokens": ["Sechs", "Stun\u00b7den", "nach", "der", "Son\u00b7nen", "Blick", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Noch schliefen; dann vernahm euch lange schon der Sch\u00e4fer", "tokens": ["Noch", "schlie\u00b7fen", ";", "dann", "ver\u00b7nahm", "euch", "lan\u00b7ge", "schon", "der", "Sch\u00e4\u00b7fer"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$.", "ADV", "VVFIN", "PPER", "ADV", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und sang wie ihr von Freud und Gl\u00fcck.", "tokens": ["Und", "sang", "wie", "ihr", "von", "Freud", "und", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KOKOM", "PPER", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Im hohen Grase weideten die Rinder", "tokens": ["Im", "ho\u00b7hen", "Gra\u00b7se", "wei\u00b7de\u00b7ten", "die", "Rin\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Hirte blieb am Eichbaum stehn,", "tokens": ["Der", "Hir\u00b7te", "blieb", "am", "Eich\u00b7baum", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Euch horchend, und das Thal sah eine Welt voll Kinder", "tokens": ["Euch", "hor\u00b7chend", ",", "und", "das", "Thal", "sah", "ei\u00b7ne", "Welt", "voll", "Kin\u00b7der"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVPP", "$,", "KON", "ART", "NN", "VVFIN", "ART", "NN", "ADJD", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nach eurem Liede tanzend gehn.", "tokens": ["Nach", "eu\u00b7rem", "Lie\u00b7de", "tan\u00b7zend", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Mirtill den jungen Sch\u00e4fer nahm Galtere,", "tokens": ["Mir\u00b7till", "den", "jun\u00b7gen", "Sch\u00e4\u00b7fer", "nahm", "Gal\u00b7te\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die sch\u00f6nste, bey der Hand und sprach:", "tokens": ["Die", "sch\u00f6ns\u00b7te", ",", "bey", "der", "Hand", "und", "sprach", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "APPR", "ART", "NN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Lerchen singen s\u00fc\u00df, Geliebter komm und h\u00f6re", "tokens": ["Die", "Ler\u00b7chen", "sin\u00b7gen", "s\u00fc\u00df", ",", "Ge\u00b7lieb\u00b7ter", "komm", "und", "h\u00f6\u00b7re"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$,", "NN", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ihr Lied, und singe lieblich nach!", "tokens": ["Ihr", "Lied", ",", "und", "sin\u00b7ge", "lieb\u00b7lich", "nach", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "KON", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Er, dem des ersten Menschen zweyten Sohnes", "tokens": ["Er", ",", "dem", "des", "ers\u00b7ten", "Men\u00b7schen", "zwey\u00b7ten", "Soh\u00b7nes"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PRELS", "ART", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des Abels fromme Muse ward,", "tokens": ["Des", "A\u00b7bels", "from\u00b7me", "Mu\u00b7se", "ward", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nahm seine Leyer, sang! die H\u00f6he seines Thones", "tokens": ["Nahm", "sei\u00b7ne", "Le\u00b7yer", ",", "sang", "!", "die", "H\u00f6\u00b7he", "sei\u00b7nes", "Tho\u00b7nes"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "NN", "$,", "VVFIN", "$.", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Glich eurer Lobges\u00e4nge Art.", "tokens": ["Glich", "eu\u00b7rer", "Lob\u00b7ge\u00b7s\u00e4n\u00b7ge", "Art", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Dann rollten von Galterens sch\u00f6nen Wange", "tokens": ["Dann", "roll\u00b7ten", "von", "Gal\u00b7te\u00b7rens", "sch\u00f6\u00b7nen", "Wan\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "NE", "ADJA", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Sechs Thr\u00e4nen, blinkend, wie der Thau", "tokens": ["Sechs", "Thr\u00e4\u00b7nen", ",", "blin\u00b7kend", ",", "wie", "der", "Thau"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["CARD", "NN", "$,", "ADJD", "$,", "PWAV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Am Fr\u00fchlings Morgen fiel! indem ihr mit Gesange", "tokens": ["Am", "Fr\u00fch\u00b7lings", "Mor\u00b7gen", "fiel", "!", "in\u00b7dem", "ihr", "mit", "Ge\u00b7san\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "NN", "VVFIN", "$.", "KOUS", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Gegr\u00fc\u00dft die Blumen auf der Au!", "tokens": ["Ge\u00b7gr\u00fc\u00dft", "die", "Blu\u00b7men", "auf", "der", "Au", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Euch h\u00f6rten lachend, Hand an Hand geschlossen", "tokens": ["Euch", "h\u00f6r\u00b7ten", "la\u00b7chend", ",", "Hand", "an", "Hand", "ge\u00b7schlos\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "$,", "NN", "APPR", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Schnitter eilend in das Feld!", "tokens": ["Die", "Schnit\u00b7ter", "ei\u00b7lend", "in", "das", "Feld", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und, im Get\u00fcmmel, ganz mit Krieger Schwei\u00df beflossen", "tokens": ["Und", ",", "im", "Ge\u00b7t\u00fcm\u00b7mel", ",", "ganz", "mit", "Krie\u00b7ger", "Schwei\u00df", "be\u00b7flos\u00b7sen"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "$,", "APPRART", "NN", "$,", "ADV", "APPR", "NN", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Vernahm euch S\u00e4nger noch der Held!", "tokens": ["Ver\u00b7nahm", "euch", "S\u00e4n\u00b7ger", "noch", "der", "Held", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Oft senktet ihr die grauen Fl\u00fcgel nieder,", "tokens": ["Oft", "senk\u00b7tet", "ihr", "die", "grau\u00b7en", "Fl\u00fc\u00b7gel", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Kamt in die Furchen; also trieb", "tokens": ["Kamt", "in", "die", "Fur\u00b7chen", ";", "al\u00b7so", "trieb"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "$.", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mich Nahrungs-Kummer oft, da\u00df ich, zu kleine Lieder", "tokens": ["Mich", "Nah\u00b7rungs\u00b7Kum\u00b7mer", "oft", ",", "da\u00df", "ich", ",", "zu", "klei\u00b7ne", "Lie\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "NN", "ADV", "$,", "KOUS", "PPER", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Matt sang und an Unedle schrieb.", "tokens": ["Matt", "sang", "und", "an", "U\u00b7ned\u00b7le", "schrieb", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "KON", "APPR", "NN", "VVFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.10": {"line.1": {"text": "Ihr sangt nicht mehr, so bald der fette Weitzen", "tokens": ["Ihr", "sangt", "nicht", "mehr", ",", "so", "bald", "der", "fet\u00b7te", "Weit\u00b7zen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "$,", "ADV", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Geerndtet war; ihr S\u00e4nger schwiegt", "tokens": ["Ge\u00b7ernd\u00b7tet", "war", ";", "ihr", "S\u00e4n\u00b7ger", "schwiegt"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "$.", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und m\u00fc\u00dfig liesset ihr euch zu dem Netze reizen", "tokens": ["Und", "m\u00fc\u00b7\u00dfig", "lies\u00b7set", "ihr", "euch", "zu", "dem", "Net\u00b7ze", "rei\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "PRF", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Darin ihr nun gefangen liegt.", "tokens": ["Da\u00b7rin", "ihr", "nun", "ge\u00b7fan\u00b7gen", "liegt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Seyd mir ein Beyspiel! vor dem M\u00fc\u00dfiggange", "tokens": ["Seyd", "mir", "ein", "Bey\u00b7spiel", "!", "vor", "dem", "M\u00fc\u00b7\u00dfig\u00b7gan\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAIMP", "PPER", "ART", "NN", "$.", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Soll sich in mir die Seele scheun,", "tokens": ["Soll", "sich", "in", "mir", "die", "See\u00b7le", "scheun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "APPR", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Kein Tag soll untergehn, da\u00df ich nicht mit Gesange", "tokens": ["Kein", "Tag", "soll", "un\u00b7ter\u00b7gehn", ",", "da\u00df", "ich", "nicht", "mit", "Ge\u00b7san\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VMFIN", "VVINF", "$,", "KOUS", "PPER", "PTKNEG", "APPR", "NN"], "meter": "-+-+-+-+++-+-", "measure": "unknown.measure.septa"}, "line.4": {"text": "Mich meines Sch\u00f6pfers will erfreun!", "tokens": ["Mich", "mei\u00b7nes", "Sch\u00f6p\u00b7fers", "will", "er\u00b7freun", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Mir giebt er von des Landes Mark zu essen;", "tokens": ["Mir", "giebt", "er", "von", "des", "Lan\u00b7des", "Mark", "zu", "es\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mir wird das Leben honigs\u00fc\u00df:", "tokens": ["Mir", "wird", "das", "Le\u00b7ben", "ho\u00b7ni\u00b7gs\u00fc\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sollt aber ich zu satt, den treuen Gott vergessen,", "tokens": ["Sollt", "a\u00b7ber", "ich", "zu", "satt", ",", "den", "treu\u00b7en", "Gott", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PPER", "PTKA", "ADJD", "$,", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der nie verga\u00df und nie verlie\u00df?", "tokens": ["Der", "nie", "ver\u00b7ga\u00df", "und", "nie", "ver\u0