{"textgrid.poem.61935": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "6.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nichts ist so ganz mir verha\u00dft, wie verstimmt hochm\u00fctige Tr\u00e4gheit;", "tokens": ["Nichts", "ist", "so", "ganz", "mir", "ver\u00b7ha\u00dft", ",", "wie", "ver\u00b7stimmt", "hoch\u00b7m\u00fc\u00b7ti\u00b7ge", "Tr\u00e4g\u00b7heit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ADV", "ADV", "PPER", "ADJD", "$,", "PWAV", "VVPP", "ADJA", "NN", "$."], "meter": "-+-+--+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Wenn dir die Krone geb\u00fchrt, geh und erobre sie dir!", "tokens": ["Wenn", "dir", "die", "Kro\u00b7ne", "ge\u00b7b\u00fchrt", ",", "geh", "und", "e\u00b7rob\u00b7re", "sie", "dir", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVPP", "$,", "VVFIN", "KON", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Aber vermagst du es nicht, so la\u00df dein Schmollen und Zaudern,", "tokens": ["A\u00b7ber", "ver\u00b7magst", "du", "es", "nicht", ",", "so", "la\u00df", "dein", "Schmol\u00b7len", "und", "Zau\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "PTKNEG", "$,", "ADV", "VVIMP", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Lern' in bescheidenem Kreis t\u00fcchtig und t\u00e4tig zu sein.", "tokens": ["Lern'", "in", "be\u00b7schei\u00b7de\u00b7nem", "Kreis", "t\u00fcch\u00b7tig", "und", "t\u00e4\u00b7tig", "zu", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "ADJD", "KON", "ADJD", "PTKZU", "VAINF", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Freilich verdammt ihr mit Fug den poetischen Dilettantismus,", "tokens": ["Frei\u00b7lich", "ver\u00b7dammt", "ihr", "mit", "Fug", "den", "po\u00b7e\u00b7ti\u00b7schen", "Di\u00b7let\u00b7tan\u00b7tis\u00b7mus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+--+--+-+-+", "measure": "dactylic.tetra.plus"}, "line.2": {"text": "Doch noch bedenklicher scheint euer politischer mir;", "tokens": ["Doch", "noch", "be\u00b7denk\u00b7li\u00b7cher", "scheint", "eu\u00b7er", "po\u00b7li\u00b7ti\u00b7scher", "mir", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VVFIN", "PPOSAT", "ADJA", "PPER", "$."], "meter": "-+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Denn das Regieren verlangt wie das Dichten den Meister; es wirkt nur", "tokens": ["Denn", "das", "Re\u00b7gie\u00b7ren", "ver\u00b7langt", "wie", "das", "Dich\u00b7ten", "den", "Meis\u00b7ter", ";", "es", "wirkt", "nur"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "KOKOM", "ART", "NN", "ART", "NN", "$.", "PPER", "VVFIN", "ADV"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Weiter ein t\u00f6richt Gesetz als ein verfehltes Gedicht.", "tokens": ["Wei\u00b7ter", "ein", "t\u00f6\u00b7richt", "Ge\u00b7setz", "als", "ein", "ver\u00b7fehl\u00b7tes", "Ge\u00b7dicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NN", "KOKOM", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}}, "stanza.3": {"line.1": {"text": "Ungl\u00fcckselig Geschick, da\u00df sich meist in brennendem Ehrgeiz", "tokens": ["Un\u00b7gl\u00fcck\u00b7se\u00b7lig", "Ge\u00b7schick", ",", "da\u00df", "sich", "meist", "in", "bren\u00b7nen\u00b7dem", "Ehr\u00b7geiz"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "NN", "$,", "KOUS", "PRF", "ADV", "APPR", "ADJA", "NN"], "meter": "+----+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Grade das halbe Talent an das Erhabenste wagt!", "tokens": ["Gra\u00b7de", "das", "hal\u00b7be", "Ta\u00b7lent", "an", "das", "Er\u00b7ha\u00b7bens\u00b7te", "wagt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+--+-+-+-+-+-+", "measure": "iambic.septa.invert"}, "line.3": {"text": "Nach der ambrosischen Frucht wie Tantalus streckt es die Hand aus,", "tokens": ["Nach", "der", "am\u00b7bro\u00b7si\u00b7schen", "Frucht", "wie", "Tan\u00b7ta\u00b7lus", "streckt", "es", "die", "Hand", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "KOKOM", "NE", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Aber der Zweig ist zu hoch, aber der Arm ist zu kurz.", "tokens": ["A\u00b7ber", "der", "Zweig", "ist", "zu", "hoch", ",", "a\u00b7ber", "der", "Arm", "ist", "zu", "kurz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "PTKA", "ADJD", "$,", "ADV", "ART", "NN", "VAFIN", "PTKA", "ADJD", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}}, "stanza.4": {"line.1": {"text": "\u00bbbester, ein Str\u00e4u\u00dfchen f\u00fcr mich!\u00ab Da m\u00e4ht er den Anger und sch\u00fcttet", "tokens": ["\u00bb", "bes\u00b7ter", ",", "ein", "Str\u00e4u\u00df\u00b7chen", "f\u00fcr", "mich", "!", "\u00ab", "Da", "m\u00e4ht", "er", "den", "An\u00b7ger", "und", "sch\u00fct\u00b7tet"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "ADJD", "$,", "ART", "NN", "APPR", "PPER", "$.", "$(", "ADV", "VVFIN", "PPER", "ART", "NN", "KON", "VVFIN"], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Unkraut, Blumen und Gras hoch mir vom Karren vors Haus.", "tokens": ["Un\u00b7kraut", ",", "Blu\u00b7men", "und", "Gras", "hoch", "mir", "vom", "Kar\u00b7ren", "vors", "Haus", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "ADJD", "PPER", "APPRART", "NN", "APPRART", "NN", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Freilich, zum Strau\u00dfe gen\u00fcgt's. Doch w\u00fc\u00dft' ich besseren Dank ihm,", "tokens": ["Frei\u00b7lich", ",", "zum", "Strau\u00b7\u00dfe", "ge\u00b7n\u00fcgt'", "s.", "Doch", "w\u00fc\u00dft'", "ich", "bes\u00b7se\u00b7ren", "Dank", "ihm", ","], "token_info": ["word", "punct", "word", "word", "word", "abbreviation", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPRART", "NN", "VVFIN", "NE", "KON", "VVFIN", "PPER", "VVFIN", "APPR", "PPER", "$,"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.4": {"text": "H\u00e4tt' er sich selber und mir leichter die Freude gemacht.", "tokens": ["H\u00e4tt'", "er", "sich", "sel\u00b7ber", "und", "mir", "leich\u00b7ter", "die", "Freu\u00b7de", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "KON", "PPER", "ADJD", "ART", "NN", "VVPP", "$."], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}}, "stanza.5": {"line.1": {"text": "Nicht zu fr\u00fch mit der Kost buntscheckigen Wissens, ihr Lehrer,", "tokens": ["Nicht", "zu", "fr\u00fch", "mit", "der", "Kost", "bunt\u00b7sche\u00b7cki\u00b7gen", "Wis\u00b7sens", ",", "ihr", "Leh\u00b7rer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "PTKA", "ADJD", "APPR", "ART", "NN", "ADJA", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "--+--++-+-+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "N\u00e4hret den Knaben mir auf; selten gedeiht er davon.", "tokens": ["N\u00e4h\u00b7ret", "den", "Kna\u00b7ben", "mir", "auf", ";", "sel\u00b7ten", "ge\u00b7deiht", "er", "da\u00b7von", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "PTKVZ", "$.", "ADJD", "VVFIN", "PPER", "PAV", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Kr\u00e4ftigt und \u00fcbt ihm den Geist an wenigen w\u00fcrdigen Stoffen;", "tokens": ["Kr\u00e4f\u00b7tigt", "und", "\u00fcbt", "ihm", "den", "Geist", "an", "we\u00b7ni\u00b7gen", "w\u00fcr\u00b7di\u00b7gen", "Stof\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "PPER", "ART", "NN", "APPR", "PIAT", "ADJA", "NN", "$."], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Euer Beruf ist erf\u00fcllt, wenn er zu ", "tokens": ["Eu\u00b7er", "Be\u00b7ruf", "ist", "er\u00b7f\u00fcllt", ",", "wenn", "er", "zu"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$,", "KOUS", "PPER", "APPR"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.6": {"line.1": {"text": "K\u00f6nigin ist die Gestalt; ihr dient anmutig die Farbe,", "tokens": ["K\u00f6\u00b7ni\u00b7gin", "ist", "die", "Ge\u00b7stalt", ";", "ihr", "dient", "an\u00b7mu\u00b7tig", "die", "Far\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$.", "PPER", "VVFIN", "ADJD", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Wie ein k\u00f6stliches Kleid sch\u00f6ner die Sch\u00f6ne dir zeigt.", "tokens": ["Wie", "ein", "k\u00f6st\u00b7li\u00b7ches", "Kleid", "sch\u00f6\u00b7ner", "die", "Sch\u00f6\u00b7ne", "dir", "zeigt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "ADJD", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "--+--++--+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Aber entferne den Schmuck, und sie mag dich noch immer bezaubern,", "tokens": ["A\u00b7ber", "ent\u00b7fer\u00b7ne", "den", "Schmuck", ",", "und", "sie", "mag", "dich", "noch", "im\u00b7mer", "be\u00b7zau\u00b7bern", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "KON", "PPER", "VMFIN", "PRF", "ADV", "ADV", "VVINF", "$,"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.4": {"text": "W\u00e4hrend das leere Gewand jede Bedeutung verliert.", "tokens": ["W\u00e4h\u00b7rend", "das", "lee\u00b7re", "Ge\u00b7wand", "je\u00b7de", "Be\u00b7deu\u00b7tung", "ver\u00b7liert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "PIAT", "NN", "VVFIN", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}}, "stanza.7": {"line.1": {"text": "Heut noch st\u00f6bert der Schnee, wie gestern; aber es weht mir", "tokens": ["Heut", "noch", "st\u00f6\u00b7bert", "der", "Schnee", ",", "wie", "ge\u00b7stern", ";", "a\u00b7ber", "es", "weht", "mir"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "ART", "NN", "$,", "PWAV", "ADJD", "$.", "KON", "PPER", "VVFIN", "PPER"], "meter": "+-+--+--+---+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Still durchs tiefste Gem\u00fct Ahnung des Lenzes dahin.", "tokens": ["Still", "durchs", "tiefs\u00b7te", "Ge\u00b7m\u00fct", "Ah\u00b7nung", "des", "Len\u00b7zes", "da\u00b7hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "ADJA", "NN", "NN", "ART", "NN", "PAV", "$."], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Wem verdank' ich das s\u00fc\u00dfe Gef\u00fchl? Seid ihr's, Hyazinthen,", "tokens": ["Wem", "ver\u00b7dank'", "ich", "das", "s\u00fc\u00b7\u00dfe", "Ge\u00b7f\u00fchl", "?", "Seid", "ih\u00b7r's", ",", "Hy\u00b7a\u00b7zin\u00b7then", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ART", "ADJA", "NN", "$.", "VAIMP", "PIS", "$,", "NN", "$,"], "meter": "--+--+--+-+-+-+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Die ihr am Fenster den Kelch tr\u00e4umerisch duftend erschlie\u00dft,", "tokens": ["Die", "ihr", "am", "Fens\u00b7ter", "den", "Kelch", "tr\u00e4u\u00b7me\u00b7risch", "duf\u00b7tend", "er\u00b7schlie\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "ART", "NN", "ADJD", "VVPP", "VVFIN", "$,"], "meter": "-+-+--++--+--+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Ist's mein T\u00f6chterchen dort im Gemach, das, leise zur Arbeit", "tokens": ["Ist's", "mein", "T\u00f6ch\u00b7ter\u00b7chen", "dort", "im", "Ge\u00b7mach", ",", "das", ",", "lei\u00b7se", "zur", "Ar\u00b7beit"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NE", "PPOSAT", "NN", "ADV", "APPRART", "NN", "$,", "PDS", "$,", "ADJD", "APPRART", "NN"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.6": {"text": "Singend, mich an das Geschwirr steigender Lerchen gemahnt?", "tokens": ["Sin\u00b7gend", ",", "mich", "an", "das", "Ge\u00b7schwirr", "stei\u00b7gen\u00b7der", "Ler\u00b7chen", "ge\u00b7mahnt", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PRF", "APPR", "ART", "NN", "ADJA", "NN", "VVPP", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}}, "stanza.8": {"line.1": {"text": "Nichts ist so ganz mir verha\u00dft, wie verstimmt hochm\u00fctige Tr\u00e4gheit;", "tokens": ["Nichts", "ist", "so", "ganz", "mir", "ver\u00b7ha\u00dft", ",", "wie", "ver\u00b7stimmt", "hoch\u00b7m\u00fc\u00b7ti\u00b7ge", "Tr\u00e4g\u00b7heit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ADV", "ADV", "PPER", "ADJD", "$,", "PWAV", "VVPP", "ADJA", "NN", "$."], "meter": "-+-+--+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Wenn dir die Krone geb\u00fchrt, geh und erobre sie dir!", "tokens": ["Wenn", "dir", "die", "Kro\u00b7ne", "ge\u00b7b\u00fchrt", ",", "geh", "und", "e\u00b7rob\u00b7re", "sie", "dir", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVPP", "$,", "VVFIN", "KON", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Aber vermagst du es nicht, so la\u00df dein Schmollen und Zaudern,", "tokens": ["A\u00b7ber", "ver\u00b7magst", "du", "es", "nicht", ",", "so", "la\u00df", "dein", "Schmol\u00b7len", "und", "Zau\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "PTKNEG", "$,", "ADV", "VVIMP", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Lern' in bescheidenem Kreis t\u00fcchtig und t\u00e4tig zu sein.", "tokens": ["Lern'", "in", "be\u00b7schei\u00b7de\u00b7nem", "Kreis", "t\u00fcch\u00b7tig", "und", "t\u00e4\u00b7tig", "zu", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "ADJD", "KON", "ADJD", "PTKZU", "VAINF", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}}, "stanza.9": {"line.1": {"text": "Freilich verdammt ihr mit Fug den poetischen Dilettantismus,", "tokens": ["Frei\u00b7lich", "ver\u00b7dammt", "ihr", "mit", "Fug", "den", "po\u00b7e\u00b7ti\u00b7schen", "Di\u00b7let\u00b7tan\u00b7tis\u00b7mus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+--+--+-+-+", "measure": "dactylic.tetra.plus"}, "line.2": {"text": "Doch noch bedenklicher scheint euer politischer mir;", "tokens": ["Doch", "noch", "be\u00b7denk\u00b7li\u00b7cher", "scheint", "eu\u00b7er", "po\u00b7li\u00b7ti\u00b7scher", "mir", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VVFIN", "PPOSAT", "ADJA", "PPER", "$."], "meter": "-+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Denn das Regieren verlangt wie das Dichten den Meister; es wirkt nur", "tokens": ["Denn", "das", "Re\u00b7gie\u00b7ren", "ver\u00b7langt", "wie", "das", "Dich\u00b7ten", "den", "Meis\u00b7ter", ";", "es", "wirkt", "nur"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "KOKOM", "ART", "NN", "ART", "NN", "$.", "PPER", "VVFIN", "ADV"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Weiter ein t\u00f6richt Gesetz als ein verfehltes Gedicht.", "tokens": ["Wei\u00b7ter", "ein", "t\u00f6\u00b7richt", "Ge\u00b7setz", "als", "ein", "ver\u00b7fehl\u00b7tes", "Ge\u00b7dicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NN", "KOKOM", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}}, "stanza.10": {"line.1": {"text": "Ungl\u00fcckselig Geschick, da\u00df sich meist in brennendem Ehrgeiz", "tokens": ["Un\u00b7gl\u00fcck\u00b7se\u00b7lig", "Ge\u00b7schick", ",", "da\u00df", "sich", "meist", "in", "bren\u00b7nen\u00b7dem", "Ehr\u00b7geiz"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "NN", "$,", "KOUS", "PRF", "ADV", "APPR", "ADJA", "NN"], "meter": "+----+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Grade das halbe Talent an das Erhabenste wagt!", "tokens": ["Gra\u00b7de", "das", "hal\u00b7be", "Ta\u00b7lent", "an", "das", "Er\u00b7ha\u00b7bens\u00b7te", "wagt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+--+-+-+-+-+-+", "measure": "iambic.septa.invert"}, "line.3": {"text": "Nach der ambrosischen Frucht wie Tantalus streckt es die Hand aus,", "tokens": ["Nach", "der", "am\u00b7bro\u00b7si\u00b7schen", "Frucht", "wie", "Tan\u00b7ta\u00b7lus", "streckt", "es", "die", "Hand", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "KOKOM", "NE", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Aber der Zweig ist zu hoch, aber der Arm ist zu kurz.", "tokens": ["A\u00b7ber", "der", "Zweig", "ist", "zu", "hoch", ",", "a\u00b7ber", "der", "Arm", "ist", "zu", "kurz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "PTKA", "ADJD", "$,", "ADV", "ART", "NN", "VAFIN", "PTKA", "ADJD", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}}, "stanza.11": {"line.1": {"text": "\u00bbbester, ein Str\u00e4u\u00dfchen f\u00fcr mich!\u00ab Da m\u00e4ht er den Anger und sch\u00fcttet", "tokens": ["\u00bb", "bes\u00b7ter", ",", "ein", "Str\u00e4u\u00df\u00b7chen", "f\u00fcr", "mich", "!", "\u00ab", "Da", "m\u00e4ht", "er", "den", "An\u00b7ger", "und", "sch\u00fct\u00b7tet"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "ADJD", "$,", "ART", "NN", "APPR", "PPER", "$.", "$(", "ADV", "VVFIN", "PPER", "ART", "NN", "KON", "VVFIN"], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Unkraut, Blumen und Gras hoch mir vom Karren vors Haus.", "tokens": ["Un\u00b7kraut", ",", "Blu\u00b7men", "und", "Gras", "hoch", "mir", "vom", "Kar\u00b7ren", "vors", "Haus", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "ADJD", "PPER", "APPRART", "NN", "APPRART", "NN", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Freilich, zum Strau\u00dfe gen\u00fcgt's. Doch w\u00fc\u00dft' ich besseren Dank ihm,", "tokens": ["Frei\u00b7lich", ",", "zum", "Strau\u00b7\u