tigt", "und", "\u00fcbt", "ihm", "den", "Geist", "an", "we\u00b7ni\u00b7gen", "w\u00fcr\u00b7di\u00b7gen", "Stof\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "PPER", "ART", "NN", "APPR", "PIAT", "ADJA", "NN", "$."], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Euer Beruf ist erf\u00fcllt, wenn er zu ", "tokens": ["Eu\u00b7er", "Be\u00b7ruf", "ist", "er\u00b7f\u00fcllt", ",", "wenn", "er", "zu"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$,", "KOUS", "PPER", "APPR"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.13": {"line.1": {"text": "K\u00f6nigin ist die Gestalt; ihr dient anmutig die Farbe,", "tokens": ["K\u00f6\u00b7ni\u00b7gin", "ist", "die", "Ge\u00b7stalt", ";", "ihr", "dient", "an\u00b7mu\u00b7tig", "die", "Far\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$.", "PPER", "VVFIN", "ADJD", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Wie ein k\u00f6stliches Kleid sch\u00f6ner die Sch\u00f6ne dir zeigt.", "tokens": ["Wie", "ein", "k\u00f6st\u00b7li\u00b7ches", "Kleid", "sch\u00f6\u00b7ner", "die", "Sch\u00f6\u00b7ne", "dir", "zeigt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "ADJD", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "--+--++--+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Aber entferne den Schmuck, und sie mag dich noch immer bezaubern,", "tokens": ["A\u00b7ber", "ent\u00b7fer\u00b7ne", "den", "Schmuck", ",", "und", "sie", "mag", "dich", "noch", "im\u00b7mer", "be\u00b7zau\u00b7bern", ","], "token_info": ["word", "word", "word", "word", "punct