{"dta.poem.880": {"metadata": {"author": {"name": "Trakl, Georg", "birth": "N.A.", "death": "N.A."}, "title": "RoMANZE ZUR NACHT", "genre": "Lyrik", "period": "N.A.", "pub_year": "1913", "urn": "urn:nbn:de:kobv:b4-30357-9", "language": ["de:0.99"], "booktitle": "Trakl, Georg: Gedichte. Leipzig, 1913."}, "poem": {"stanza.1": {"line.1": {"text": "Einsamer unterm Sternenzelt", "tokens": ["Ein\u00b7sa\u00b7mer", "un\u00b7term", "Ster\u00b7nen\u00b7zelt"], "token_info": ["word", "word", "word"], "pos": ["ADV", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Geht durch die stille Mitternacht.", "tokens": ["Geht", "durch", "die", "stil\u00b7le", "Mit\u00b7ter\u00b7nacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Knab aus Tr\u00e4umen wirr erwacht,", "tokens": ["Der", "Knab", "aus", "Tr\u00e4u\u00b7men", "wirr", "er\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sein Antlitz grau im Mond verf\u00e4llt.", "tokens": ["Sein", "Ant\u00b7litz", "grau", "im", "Mond", "ver\u00b7f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die N\u00e4rrin weint mit offnem Haar", "tokens": ["Die", "N\u00e4r\u00b7rin", "weint", "mit", "off\u00b7nem", "Haar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Am Fenster, das vergittert starrt.", "tokens": ["Am", "Fens\u00b7ter", ",", "das", "ver\u00b7git\u00b7tert", "starrt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Im Teich vorbei auf s\u00fc\u00dfer Fahrt", "tokens": ["Im", "Teich", "vor\u00b7bei", "auf", "s\u00fc\u00b7\u00dfer", "Fahrt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ziehn Liebende sehr wunderbar.", "tokens": ["Ziehn", "Lie\u00b7ben\u00b7de", "sehr", "wun\u00b7der\u00b7bar", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Der M\u00f6rder l\u00e4chelt bleich im Wein,", "tokens": ["Der", "M\u00f6r\u00b7der", "l\u00e4\u00b7chelt", "bleich", "im", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Kranken Todesgrausen packt.", "tokens": ["Die", "Kran\u00b7ken", "To\u00b7des\u00b7grau\u00b7sen", "packt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Nonne betet wund und nackt", "tokens": ["Die", "Non\u00b7ne", "be\u00b7tet", "wund", "und", "nackt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vor des Heilands Kreuzespein.", "tokens": ["Vor", "des", "Hei\u00b7lands", "Kreu\u00b7ze\u00b7spein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Die Mutter leis\u2019 im Schlafe singt.", "tokens": ["Die", "Mut\u00b7ter", "leis'", "im", "Schla\u00b7fe", "singt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sehr friedlich schaut zur Nacht das Kind", "tokens": ["Sehr", "fried\u00b7lich", "schaut", "zur", "Nacht", "das", "Kind"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit Augen, die ganz wahrhaft sind.", "tokens": ["Mit", "Au\u00b7gen", ",", "die", "ganz", "wahr\u00b7haft", "sind", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "ADV", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im Hurenhaus Gel\u00e4chter klingt.", "tokens": ["Im", "Hu\u00b7ren\u00b7haus", "Ge\u00b7l\u00e4ch\u00b7ter", "klingt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Beim Talglicht drunt\u2019 im Kellerloch", "tokens": ["Beim", "Talg\u00b7licht", "drunt'", "im", "Kel\u00b7ler\u00b7loch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Tote malt mit wei\u00dfer Hand", "tokens": ["Der", "To\u00b7te", "malt", "mit", "wei\u00b7\u00dfer", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein grinsend Schweigen an die Wand.", "tokens": ["Ein", "grin\u00b7send", "Schwei\u00b7gen", "an", "die", "Wand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Schl\u00e4fer fl\u00fcstert immer noch.", "tokens": ["Der", "Schl\u00e4\u00b7fer", "fl\u00fcs\u00b7tert", "im\u00b7mer", "noch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}