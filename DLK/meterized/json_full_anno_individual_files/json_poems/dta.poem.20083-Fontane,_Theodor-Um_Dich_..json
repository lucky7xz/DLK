{"dta.poem.20083": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Um Dich .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1851", "urn": "urn:nbn:de:kobv:b4-200905191321", "language": ["de:0.99"], "booktitle": "Fontane, Theodor: Gedichte. Berlin, 1851."}, "poem": {"stanza.1": {"line.1": {"text": "Tritt ein f\u00fcr Deines Herzens Meinung", "tokens": ["Tritt", "ein", "f\u00fcr", "Dei\u00b7nes", "Her\u00b7zens", "Mei\u00b7nung"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und f\u00fcrchte nicht der Feinde Spott,", "tokens": ["Und", "f\u00fcrch\u00b7te", "nicht", "der", "Fein\u00b7de", "Spott", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bek\u00e4mpfe muthig die Verneinung", "tokens": ["Be\u00b7k\u00e4mp\u00b7fe", "mut\u00b7hig", "die", "Ver\u00b7nei\u00b7nung"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADJD", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So Du den Glauben hast an Gott.", "tokens": ["So", "Du", "den", "Glau\u00b7ben", "hast", "an", "Gott", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ART", "NN", "VAFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie Luther einst, in festem Sinnen,", "tokens": ["Wie", "Lu\u00b7ther", "einst", ",", "in", "fes\u00b7tem", "Sin\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "ADV", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So sprich auch Du zu Gottes Ehr\u2019:", "tokens": ["So", "sprich", "auch", "Du", "zu", "Got\u00b7tes", "Ehr'", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "PPER", "APPR", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u201eich geh nach Worms, und ob da drinnen", "tokens": ["\u201e", "ich", "geh", "nach", "Worms", ",", "und", "ob", "da", "drin\u00b7nen"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VVFIN", "APPR", "NN", "$,", "KON", "KOUS", "ADV", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Jedweder Stein ein Teufel w\u00e4r\u2019!\u201c", "tokens": ["Jed\u00b7we\u00b7der", "Stein", "ein", "Teu\u00b7fel", "w\u00e4r'", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PIAT", "NN", "ART", "NN", "VAFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und peitscht Dich dann der Witz mit Ruthen,", "tokens": ["Und", "peitscht", "Dich", "dann", "der", "Witz", "mit", "Ru\u00b7then", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ART", "NN", "APPR", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und hasst man Dich, \u2014 o la\u00df, o la\u00df!", "tokens": ["Und", "hasst", "man", "Dich", ",", "o", "la\u00df", ",", "o", "la\u00df", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "PPER", "$,", "$(", "FM", "FM", "$,", "FM", "FM", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mehr noch als Liebe aller Guten,", "tokens": ["Mehr", "noch", "als", "Lie\u00b7be", "al\u00b7ler", "Gu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KOUS", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gilt aller B\u00f6sen Hohn und Ha\u00df.", "tokens": ["Gilt", "al\u00b7ler", "B\u00f6\u00b7sen", "Hohn", "und", "Ha\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}