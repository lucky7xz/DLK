{"dta.poem.9145": {"metadata": {"author": {"name": "Weise, Christian", "birth": "N.A.", "death": "N.A."}, "title": "X.  \n An die unvergleichliche Margaris.", "genre": "Lyrik; Drama; Prosa", "period": "N.A.", "pub_year": "1701", "urn": "urn:nbn:de:kobv:b4-25043-0", "language": ["de:0.99"], "booktitle": "Weise, Christian: \u00dcberfl\u00fc\u00dfige Gedancken Der gr\u00fcnenden jugend. Leipzig, 1701."}, "poem": {"stanza.1": {"line.1": {"text": "Meines hertzens k\u00f6nigin/", "tokens": ["Mei\u00b7nes", "hert\u00b7zens", "k\u00f6\u00b7ni\u00b7gin", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Angenehmster schatz auf erden/", "tokens": ["An\u00b7ge\u00b7nehms\u00b7ter", "schatz", "auf", "er\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Richte deine lust-geberden", "tokens": ["Rich\u00b7te", "dei\u00b7ne", "lust\u00b7ge\u00b7ber\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "G\u00fcnstig auf den diener hin/", "tokens": ["G\u00fcns\u00b7tig", "auf", "den", "die\u00b7ner", "hin", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "PTKVZ", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Als bey welchem deine gaben", "tokens": ["Als", "bey", "wel\u00b7chem", "dei\u00b7ne", "ga\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PWAT", "PPOSAT", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ruh und trost zu schaffen haben/", "tokens": ["Ruh", "und", "trost", "zu", "schaf\u00b7fen", "ha\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "PTKZU", "VVINF", "VAFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Ich gesteh es zwar betr\u00fcbt/", "tokens": ["Ich", "ge\u00b7steh", "es", "zwar", "be\u00b7tr\u00fcbt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "VVPP", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Sch\u00f6nstes kind ich bin verliebt.", "tokens": ["Sch\u00f6ns\u00b7tes", "kind", "ich", "bin", "ver\u00b7liebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPER", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "2. ", "tokens": [], "token_info": [], "pos": []}, "line.2": {"text": "Ach was soll ich l\u00e4nger schweigen/", "tokens": ["Ach", "was", "soll", "ich", "l\u00e4n\u00b7ger", "schwei\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PWS", "VMFIN", "PPER", "ADJD", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Regen sich doch alle zeugen", "tokens": ["Re\u00b7gen", "sich", "doch", "al\u00b7le", "zeu\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PRF", "ADV", "PIS", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gegen mir und meiner pein/", "tokens": ["Ge\u00b7gen", "mir", "und", "mei\u00b7ner", "pein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "KON", "PPOSAT", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Alle kr\u00e4ffte meiner seelen", "tokens": ["Al\u00b7le", "kr\u00e4ff\u00b7te", "mei\u00b7ner", "see\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Reitzen mich mit sanfftem qv\u00e4len/", "tokens": ["Reit\u00b7zen", "mich", "mit", "sanff\u00b7tem", "qv\u00e4\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPR", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Ich bekenn es offentlich/", "tokens": ["Ich", "be\u00b7kenn", "es", "of\u00b7fent\u00b7lich", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Sch\u00f6nstes kind ich liebe dich.", "tokens": ["Sch\u00f6ns\u00b7tes", "kind", "ich", "lie\u00b7be", "dich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPER", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "3. ", "tokens": [], "token_info": [], "pos": []}, "line.2": {"text": "Alle meine noth zu nichte/", "tokens": ["Al\u00b7le", "mei\u00b7ne", "noth", "zu", "nich\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "APPR", "PIS", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Richte dein geneigt gesichte", "tokens": ["Rich\u00b7te", "dein", "ge\u00b7neigt", "ge\u00b7sich\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "ADJD", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gegen mir nnd meiner brunst/", "tokens": ["Ge\u00b7gen", "mir", "nnd", "mei\u00b7ner", "brunst", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "KON", "PPOSAT", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Alle freude/ lust und schertzen", "tokens": ["Al\u00b7le", "freu\u00b7de", "/", "lust", "und", "schert\u00b7zen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PIAT", "NN", "$(", "VVFIN", "KON", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Rei\u00df ich sonst aus meinem hertzen/", "tokens": ["Rei\u00df", "ich", "sonst", "aus", "mei\u00b7nem", "hert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Ich begehr in dir allein", "tokens": ["Ich", "be\u00b7gehr", "in", "dir", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Selig und vergn\u00fcgt zu seyn.", "tokens": ["Se\u00b7lig", "und", "ver\u00b7gn\u00fcgt", "zu", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "VVPP", "PTKZU", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "4. ", "tokens": [], "token_info": [], "pos": []}, "line.2": {"text": "Alles was ich dir erzehle/", "tokens": ["Al\u00b7les", "was", "ich", "dir", "er\u00b7zeh\u00b7le", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PWS", "PPER", "PPER", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Reimet sich mit meiner seele/", "tokens": ["Rei\u00b7met", "sich", "mit", "mei\u00b7ner", "see\u00b7le", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Glaube/ was die zunge spricht.", "tokens": ["Glau\u00b7be", "/", "was", "die", "zun\u00b7ge", "spricht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "PWS", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Andre m\u00f6gen falsche sachen", "tokens": ["And\u00b7re", "m\u00f6\u00b7gen", "fal\u00b7sche", "sa\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["PIS", "VMFIN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "R\u00fchmen und zur tugend machen/", "tokens": ["R\u00fch\u00b7men", "und", "zur", "tu\u00b7gend", "ma\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "APPRART", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Ich will ohne falschen schein/", "tokens": ["Ich", "will", "oh\u00b7ne", "fal\u00b7schen", "schein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "S\u00fcsses kind/ dein diener seyn.", "tokens": ["S\u00fcs\u00b7ses", "kind", "/", "dein", "die\u00b7ner", "seyn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$(", "PPOSAT", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "5. ", "tokens": [], "token_info": [], "pos": []}, "line.2": {"text": "Alles will ich dir ergeben/", "tokens": ["Al\u00b7les", "will", "ich", "dir", "er\u00b7ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPER", "PPER", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Reitze mein best\u00e4ndigs leben", "tokens": ["Reit\u00b7ze", "mein", "be\u00b7st\u00e4n\u00b7digs", "le\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "ADJA", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gunst-geneigt zu deiner zier:", "tokens": ["Gunst\u00b7ge\u00b7neigt", "zu", "dei\u00b7ner", "zier", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Also werden meine sinnen", "tokens": ["Al\u00b7so", "wer\u00b7den", "mei\u00b7ne", "sin\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ruh und s\u00fcssen trost gewinnen.", "tokens": ["Ruh", "und", "s\u00fcs\u00b7sen", "trost", "ge\u00b7win\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Itzt verbleib ich halb betr\u00fcbt/", "tokens": ["Itzt", "ver\u00b7bleib", "ich", "halb", "be\u00b7tr\u00fcbt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "VVPP", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Sch\u00f6nstes kind in dich verliebt.", "tokens": ["Sch\u00f6ns\u00b7tes", "kind", "in", "dich", "ver\u00b7liebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}