{"dta.poem.17000": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "101.  Das lebendige Todtengrab.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Mensch ist dein Antlitz sch\u00f6n/ und deine Seele bleich/", "tokens": ["Mensch", "ist", "dein", "Ant\u00b7litz", "sch\u00f6n", "/", "und", "dei\u00b7ne", "See\u00b7le", "bleich", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "ADJD", "$(", "KON", "PPOSAT", "NN", "ADJD", "$("], "meter": "++-+-+-+-+-+", "measure": "unknown.measure.septa"}, "line.2": {"text": "So bistu lebendig den Todtengr\u00e4bern gleich.", "tokens": ["So", "bis\u00b7tu", "le\u00b7ben\u00b7dig", "den", "Tod\u00b7ten\u00b7gr\u00e4\u00b7bern", "gleich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "ART", "NN", "ADV", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}}}}}