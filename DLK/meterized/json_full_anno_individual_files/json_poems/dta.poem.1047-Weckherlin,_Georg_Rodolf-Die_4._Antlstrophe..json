{"dta.poem.1047": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Die 4. Antlstrophe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1618", "urn": "urn:nbn:de:kobv:b4-20628-9", "language": ["de:0.85", "nl:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wjewol deine tugent vnd ehren", "tokens": ["Wie\u00b7wol", "dei\u00b7ne", "tu\u00b7gent", "vnd", "eh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "KON", "VVINF"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Vmbschwebend in aller leut mund/", "tokens": ["Vmb\u00b7schwe\u00b7bend", "in", "al\u00b7ler", "leut", "mund", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PIAT", "NN", "NN", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Vns zwunge\u0303 dich von hertzengrund", "tokens": ["Vns", "zwung\u1ebd", "dich", "von", "hert\u00b7zen\u00b7grund"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Vor vnser F\u00fcrstin zubegehren:", "tokens": ["Vor", "vn\u00b7ser", "F\u00fcrs\u00b7tin", "zu\u00b7be\u00b7geh\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wan dir auch vnser F\u00fcrst vnd Herr/", "tokens": ["Wan", "dir", "auch", "vn\u00b7ser", "F\u00fcrst", "vnd", "Herr", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "PPOSAT", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Gl\u00fcckseelig f\u00fcrohin zuleben/", "tokens": ["Gl\u00fcck\u00b7see\u00b7lig", "f\u00fc\u00b7ro\u00b7hin", "zu\u00b7le\u00b7ben", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Sein hertz f\u00fcr ein opfer gegeben/", "tokens": ["Sein", "hertz", "f\u00fcr", "ein", "op\u00b7fer", "ge\u00b7ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Da sein leib noch von dir gar ferr:", "tokens": ["Da", "sein", "leib", "noch", "von", "dir", "gar", "ferr", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "APPR", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "K\u00f6nden Wir doch jetz erst verstehen/", "tokens": ["K\u00f6n\u00b7den", "Wir", "doch", "jetz", "erst", "ver\u00b7ste\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "ADV", "VVINF", "$("], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.10": {"text": "Das was an dir die augen sehen", "tokens": ["Das", "was", "an", "dir", "die", "au\u00b7gen", "se\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "PRELS", "APPR", "PPER", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Ein menschliche zung nimmermehr", "tokens": ["Ein", "menschli\u00b7che", "zung", "nim\u00b7mer\u00b7mehr"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADV"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.12": {"text": "Au\u00dfzusprechen geschickt gnug wer:", "tokens": ["Au\u00df\u00b7zu\u00b7spre\u00b7chen", "ge\u00b7schickt", "gnug", "wer", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "ADV", "PWS", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.13": {"text": "Vnd jetz erst werden wir recht innen/", "tokens": ["Vnd", "jetz", "erst", "wer\u00b7den", "wir", "recht", "in\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "VAFIN", "PPER", "ADV", "ADV", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Wie durch seines hertzens verlust", "tokens": ["Wie", "durch", "sei\u00b7nes", "hert\u00b7zens", "ver\u00b7lust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.15": {"text": "Er vnd Wir vilf\u00e4ltig gewinnen", "tokens": ["Er", "vnd", "Wir", "vil\u00b7f\u00e4l\u00b7tig", "ge\u00b7win\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "KON", "PPER", "ADJD", "VVINF"], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}, "line.16": {"text": "Mit deiner kunft/ gl\u00fcck/ hail vnd lust.", "tokens": ["Mit", "dei\u00b7ner", "kunft", "/", "gl\u00fcck", "/", "hail", "vnd", "lust", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$(", "XY", "$(", "XY", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}