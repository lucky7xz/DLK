{"dta.poem.9644": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Grabschrifft des   Ducs   und Mareschalls von  \n Schomberg/ der/ wie bekandt/ in der letzten Jrr-  \n l\u00e4ndischen schlacht geblieben.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich dient' als General vier grossen Potentaten/", "tokens": ["Ich", "dient'", "als", "Ge\u00b7ne\u00b7ral", "vier", "gros\u00b7sen", "Po\u00b7ten\u00b7ta\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOUS", "NN", "CARD", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In Franckreich/ Engeland/ der Marck und Poringall.", "tokens": ["In", "Fran\u00b7ck\u00b7reich", "/", "En\u00b7ge\u00b7land", "/", "der", "Marck", "und", "Po\u00b7rin\u00b7gall", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$(", "NE", "$(", "ART", "NN", "KON", "NN", "$."], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Der K\u00e4yser suchte mich/ nebst den vereinten Staaten/", "tokens": ["Der", "K\u00e4y\u00b7ser", "such\u00b7te", "mich", "/", "nebst", "den", "ver\u00b7ein\u00b7ten", "Staa\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$(", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Ich ward durch meinen arm Lord/ Grand/ Duc/ Mareschall", "tokens": ["Ich", "ward", "durch", "mei\u00b7nen", "arm", "Lord", "/", "Grand", "/", "Duc", "/", "Ma\u00b7re\u00b7schall"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "ADJA", "NN", "$(", "NN", "$(", "NE", "$(", "NE"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich halff vier ", "tokens": ["Ich", "halff", "vier"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "CARD"], "meter": "--+", "measure": "anapaest.init"}, "line.6": {"text": "Und gab mein gantzes gl\u00fcck f\u00fcr meinen glauben hin.", "tokens": ["Und", "gab", "mein", "gant\u00b7zes", "gl\u00fcck", "f\u00fcr", "mei\u00b7nen", "glau\u00b7ben", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJA", "NN", "APPR", "PPOSAT", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Di\u00df war mehr als genug/ dadurch ber\u00fchmt zu sterben;", "tokens": ["Di\u00df", "war", "mehr", "als", "ge\u00b7nug", "/", "da\u00b7durch", "be\u00b7r\u00fchmt", "zu", "ster\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PIS", "KOKOM", "ADV", "$(", "PAV", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Doch macht mein helden-tod mich erstlich was ich bin.", "tokens": ["Doch", "macht", "mein", "hel\u00b7den\u00b7tod", "mich", "erst\u00b7lich", "was", "ich", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "PPER", "ADV", "PWS", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ich starb in einer schlacht: (di\u00df wird den neid besch\u00e4men!", "tokens": ["Ich", "starb", "in", "ei\u00b7ner", "schlacht", ":(", "di\u00df", "wird", "den", "neid", "be\u00b7sch\u00e4\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "emoticon", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJD", "$(", "PDS", "VAFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ich starb/ als ich gesiegt/ di\u00df ist was mich erhebt.", "tokens": ["Ich", "starb", "/", "als", "ich", "ge\u00b7siegt", "/", "di\u00df", "ist", "was", "mich", "er\u00b7hebt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "KOUS", "PPER", "VVPP", "$(", "PDS", "VAFIN", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wer wird dem grauen haupt ietzt seine lorbeer nehmen/", "tokens": ["Wer", "wird", "dem", "grau\u00b7en", "haupt", "ietzt", "sei\u00b7ne", "lor\u00b7beer", "neh\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "ADJA", "NN", "ADV", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Nun es auff selben stirbt/ und sie mit sich begr\u00e4bt?", "tokens": ["Nun", "es", "auff", "sel\u00b7ben", "stirbt", "/", "und", "sie", "mit", "sich", "be\u00b7gr\u00e4bt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "APPR", "ADV", "VVFIN", "$(", "KON", "PPER", "APPR", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}