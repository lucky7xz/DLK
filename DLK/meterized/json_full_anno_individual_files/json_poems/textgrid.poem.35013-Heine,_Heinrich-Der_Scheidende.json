{"textgrid.poem.35013": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Scheidende", "genre": "verse", "period": "N.A.", "pub_year": 1852, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Erstorben ist in meiner Brust", "tokens": ["E\u00b7rstor\u00b7ben", "ist", "in", "mei\u00b7ner", "Brust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Jedwede weltlich eitle Lust,", "tokens": ["Jed\u00b7we\u00b7de", "welt\u00b7lich", "eit\u00b7le", "Lust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Schier ist mir auch erstorben drin", "tokens": ["Schier", "ist", "mir", "auch", "ers\u00b7tor\u00b7ben", "drin"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "ADV", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Ha\u00df des Schlechten, sogar der Sinn", "tokens": ["Der", "Ha\u00df", "des", "Schlech\u00b7ten", ",", "so\u00b7gar", "der", "Sinn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "$,", "ADV", "ART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "F\u00fcr eigne wie f\u00fcr fremde Not \u2013", "tokens": ["F\u00fcr", "eig\u00b7ne", "wie", "f\u00fcr", "frem\u00b7de", "Not", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "KOKOM", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und in mir lebt nur noch der Tod!", "tokens": ["Und", "in", "mir", "lebt", "nur", "noch", "der", "Tod", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VVFIN", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der Vorhang f\u00e4llt, das St\u00fcck ist aus,", "tokens": ["Der", "Vor\u00b7hang", "f\u00e4llt", ",", "das", "St\u00fcck", "ist", "aus", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VAFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und g\u00e4hnend wandelt jetzt nach Haus", "tokens": ["Und", "g\u00e4h\u00b7nend", "wan\u00b7delt", "jetzt", "nach", "Haus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "ADV", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mein liebes deutsches Publikum,", "tokens": ["Mein", "lie\u00b7bes", "deut\u00b7sches", "Pub\u00b7li\u00b7kum", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die guten Leutchen sind nicht dumm;", "tokens": ["Die", "gu\u00b7ten", "Leut\u00b7chen", "sind", "nicht", "dumm", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das speist jetzt ganz vergn\u00fcgt zu Nacht,", "tokens": ["Das", "speist", "jetzt", "ganz", "ver\u00b7gn\u00fcgt", "zu", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ADV", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und trinkt sein Sch\u00f6ppchen, singt und lacht \u2013", "tokens": ["Und", "trinkt", "sein", "Sch\u00f6ppc\u00b7hen", ",", "singt", "und", "lacht", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$,", "VVFIN", "KON", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Er hatte recht, der edle Heros,", "tokens": ["Er", "hat\u00b7te", "recht", ",", "der", "ed\u00b7le", "He\u00b7ros", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der weiland sprach im Buch Homeros':", "tokens": ["Der", "wei\u00b7land", "sprach", "im", "Buch", "Ho\u00b7me\u00b7ros'", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "APPRART", "NN", "NE", "$."], "meter": "-+-+-++-+", "measure": "unknown.measure.penta"}, "line.9": {"text": "Der kleinste lebendige Philister", "tokens": ["Der", "kleins\u00b7te", "le\u00b7ben\u00b7di\u00b7ge", "Phi\u00b7lis\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Zu Stukkert am Neckar, viel gl\u00fccklicher ist er,", "tokens": ["Zu", "Stuk\u00b7kert", "am", "Ne\u00b7ckar", ",", "viel", "gl\u00fcck\u00b7li\u00b7cher", "ist", "er", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "APPRART", "NN", "$,", "ADV", "ADJD", "VAFIN", "PPER", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.11": {"text": "Als ich, der Pelide, der tote Held,", "tokens": ["Als", "ich", ",", "der", "Pe\u00b7li\u00b7de", ",", "der", "to\u00b7te", "Held", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "ART", "NN", "$,", "ART", "AD