{"textgrid.poem.62749": {"metadata": {"author": {"name": "Z\u00e4unemann, Sidonia Hedwig", "birth": "N.A.", "death": "N.A."}, "title": "Unterth\u00e4niges Sendschreiben an Ihro Hochf\u00fcrstl. Durchl. zu Sachsen-Weimar", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "der ein Herz voller Huld und Gro\u00dfmuth tr\u00e4get,", "tokens": ["der", "ein", "Herz", "vol\u00b7ler", "Huld", "und", "Gro\u00df\u00b7muth", "tr\u00e4\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADJA", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Und darneben jedes Werk mit viel Weisheit \u00fcberleget.", "tokens": ["Und", "dar\u00b7ne\u00b7ben", "je\u00b7des", "Werk", "mit", "viel", "Weis\u00b7heit", "\u00fc\u00b7ber\u00b7le\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "PIAT", "NN", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+--+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Ob er Dirs gleich nicht entdecket, und aus Ehrfurcht oft verhehlt.", "tokens": ["Ob", "er", "Dirs", "gleich", "nicht", "ent\u00b7de\u00b7cket", ",", "und", "aus", "Ehr\u00b7furcht", "oft", "ver\u00b7hehlt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "ADV", "PTKNEG", "VVFIN", "$,", "KON", "APPR", "NN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Du hast mir was ich gew\u00fcnscht, und was meinen Geist erquicket,", "tokens": ["Du", "hast", "mir", "was", "ich", "ge\u00b7w\u00fcnscht", ",", "und", "was", "mei\u00b7nen", "Geist", "er\u00b7quic\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PWS", "PPER", "VVPP", "$,", "KON", "PWS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+--+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Aus besondrer Gnad und Huld zu dem heilgen Christ geschicket.", "tokens": ["Aus", "be\u00b7sond\u00b7rer", "Gnad", "und", "Huld", "zu", "dem", "heil\u00b7gen", "Christ", "ge\u00b7schi\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "APPR", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.6": {"text": "Aber Du was vor die Seele, und zu ihren Zeitvertreib.", "tokens": ["A\u00b7ber", "Du", "was", "vor", "die", "See\u00b7le", ",", "und", "zu", "ih\u00b7ren", "Zeit\u00b7ver\u00b7treib", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PIS", "APPR", "ART", "NN", "$,", "KON", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.7": {"text": "Jens ist n\u00fctzlich; dieses noth; jenes schm\u00fcckt und ziert die Glieder;", "tokens": ["Jens", "ist", "n\u00fctz\u00b7lich", ";", "die\u00b7ses", "noth", ";", "je\u00b7nes", "schm\u00fcckt", "und", "ziert", "die", "Glie\u00b7der", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADJD", "$.", "PDAT", "NN", "$.", "PDS", "VVFIN", "KON", "VVFIN", "ART", "NN", "$."], "meter": "+-+----+-+-+-+-", "measure": "unknown.measure.hexa"}, "line.8": {"text": "Dieses aber giebt dem Geist, und der Seele Kr\u00e4fte wieder.", "tokens": ["Die\u00b7ses", "a\u00b7ber", "giebt", "dem", "Geist", ",", "und", "der", "See\u00b7le", "Kr\u00e4f\u00b7te", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "VVFIN", "ART", "NN", "$,", "KON", "ART", "NN", "NN", "ADV", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.9": {"text": "Diese sch\u00f6n und grossen B\u00fccher fordern meine Geister auf,", "tokens": ["Die\u00b7se", "sch\u00f6n", "und", "gros\u00b7sen", "B\u00fc\u00b7cher", "for\u00b7dern", "mei\u00b7ne", "Geis\u00b7ter", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJD", "KON", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.10": {"text": "Und belustgen meine Sinnen durch den ganzen Lebenslauf.", "tokens": ["Und", "be\u00b7lust\u00b7gen", "mei\u00b7ne", "Sin\u00b7nen", "durch", "den", "gan\u00b7zen", "Le\u00b7bens\u00b7lauf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.11": {"text": "Unser Geist mu\u00df wie der Leib seine Spei\u00df und Nahrung kriegen,", "tokens": ["Un\u00b7ser", "Geist", "mu\u00df", "wie", "der", "Leib", "sei\u00b7ne", "Spei\u00df", "und", "Nah\u00b7rung", "krie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "KOKOM", "ART", "NN", "PPOSAT", "NN", "KON", "NN", "VVINF", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.12": {"text": "Mu\u00df an etwas seine Lust, sein Erg\u00f6tzen und Vergn\u00fcgen", "tokens": ["Mu\u00df", "an", "et\u00b7was", "sei\u00b7ne", "Lust", ",", "sein", "Er\u00b7g\u00f6t\u00b7zen", "und", "Ver\u00b7gn\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VMFIN", "APPR", "PIS", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "KON", "NN"], "meter": "-+--+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.13": {"text": "Durch die Lebens-Zeit empfinden. Dieses schenkt und giebt ein Blat,", "tokens": ["Durch", "die", "Le\u00b7bens\u00b7Zeit", "emp\u00b7fin\u00b7den", ".", "Die\u00b7ses", "schenkt", "und", "giebt", "ein", "Blat", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$.", "PDS", "VVFIN", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.14": {"text": "Und ein Buch, das Geist und Weisheit, und viel Lehren in sich hat.", "tokens": ["Und", "ein", "Buch", ",", "das", "Geist", "und", "Weis\u00b7heit", ",", "und", "viel", "Leh\u00b7ren", "in", "sich", "hat", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "ART", "NN", "KON", "NN", "$,", "KON", "PIAT", "NN", "APPR", "PRF", "VAFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.15": {"text": "Ja, Durchlauchtster! die\u00df Geschenk, diese Welt-ber\u00fchmte B\u00fccher", "tokens": ["Ja", ",", "Durch\u00b7lauchts\u00b7ter", "!", "die\u00df", "Ge\u00b7schenk", ",", "die\u00b7se", "Welt\u00b7be\u00b7r\u00fchm\u00b7te", "B\u00fc\u00b7cher"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$,", "NN", "$.", "PDS", "NN", "$,", "PDAT", "ADJA", "NN"], "meter": "-++-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.16": {"text": "Machen meinen Geist und Sinn immer munterer und sicher,", "tokens": ["Ma\u00b7chen", "mei\u00b7nen", "Geist", "und", "Sinn", "im\u00b7mer", "mun\u00b7te\u00b7rer", "und", "si\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "KON", "NN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.17": {"text": "Auf der Weisheit Bahn zu wandeln und Minerven nachzugehn,", "tokens": ["Auf", "der", "Weis\u00b7heit", "Bahn", "zu", "wan\u00b7deln", "und", "Mi\u00b7ner\u00b7ven", "nach\u00b7zu\u00b7gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "PTKZU", "VVFIN", "KON", "NN", "VVINF", "$,"], "meter": "--+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.18": {"text": "So, da\u00df ich dadurch im Tode dennoch kan das Leben sehn.", "tokens": ["So", ",", "da\u00df", "ich", "da\u00b7durch", "im", "To\u00b7de", "den\u00b7noch", "kan", "das", "Le\u00b7ben", "sehn", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "PAV", "APPRART", "NN", "ADV", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "--+-+-+-+-+-+-+", "measure": "anapaest.init"}, "line.19": {"text": "Deine Dien'rin, ", "tokens": ["Dei\u00b7ne", "Dien'\u00b7rin", ","], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.20": {"text": "Und will Dir im Geist die Hand und den Saum des Purpurs k\u00fcssen,", "tokens": ["Und", "will", "Dir", "im", "Geist", "die", "Hand", "und", "den", "Saum", "des", "Pur\u00b7purs", "k\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "APPRART", "NN", "ART", "NN", "KON", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "--+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.21": {"text": "Vor das gn\u00e4dge Christ-Geschenke, das mir deine Huld beschert.", "tokens": ["Vor", "das", "gn\u00e4d\u00b7ge", "Christ\u00b7Ge\u00b7schen\u00b7ke", ",", "das", "mir", "dei\u00b7ne", "Huld", "be\u00b7schert", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "PRELS", "PPER", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.22": {"text": "Nim hiermit den Dank, ", "tokens": ["Nim", "hier\u00b7mit", "den", "Dank", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PAV", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.23": {"text": "Keinen andern Dank kan ich als ein solches Herze bringen,", "tokens": ["Kei\u00b7nen", "an\u00b7dern", "Dank", "kan", "ich", "als", "ein", "sol\u00b7ches", "Her\u00b7ze", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VMFIN", "PPER", "KOUS", "ART", "PIAT", "NN", "VVINF", "$,"], "meter": "+-+--+-+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.24": {"text": "Das mit Ehrfurcht angef\u00fcllt, und dieselbe durch das Singen", "tokens": ["Das", "mit", "Ehr\u00b7furcht", "an\u00b7ge\u00b7f\u00fcllt", ",", "und", "die\u00b7sel\u00b7be", "durch", "das", "Sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDS", "APPR", "NN", "VVPP", "$,", "KON", "PDAT", "APPR", "ART", "NN"], "meter": "--+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.25": {"text": "Bey bequemer Zeit und Stunden; bey dem Lust- und Freuden-Fest,", "tokens": ["Bey", "be\u00b7que\u00b7mer", "Zeit", "und", "Stun\u00b7den", ";", "bey", "dem", "Lust", "und", "Freu\u00b7den\u00b7Fest", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "$.", "APPR", "ART", "TRUNC", "KON", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.26": {"text": "Voller Demuth, nach der Weise der Poeten sehen l\u00e4st.", "tokens": ["Vol\u00b7ler", "De\u00b7muth", ",", "nach", "der", "Wei\u00b7se", "der", "Po\u00b7et\u00b7en", "se\u00b7hen", "l\u00e4st", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "APPR", "ART", "NN", "ART", "NN", "VVINF", "VVFIN", "$."], "meter": "+--+--+-+-+-+-+", "measure": "dactylic.di.plus"}, "line.27": {"text": "Held! so lang mein Blut sich regt, und mein Mund ein Wort kan lallen,", "tokens": ["Held", "!", "so", "lang", "mein", "Blut", "sich", "regt", ",", "und", "mein", "Mund", "ein", "Wort", "kan", "lal\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ADV", "ADJD", "PPOSAT", "NN", "PRF", "VVFIN", "$,", "KON", "PPOSAT", "NN", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.28": {"text": "Werd ich stets mit Dankgeb\u00fchr vor Dein Antlitz niederfallen.", "tokens": ["Werd", "ich", "stets", "mit", "Dank\u00b7ge\u00b7b\u00fchr", "vor", "Dein", "Ant\u00b7litz", "nie\u00b7der\u00b7fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "APPR", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.29": {"text": "Wenn mein Mund darinne lieset, und mein Geist sich dran erquickt,", "tokens": ["Wenn", "mein", "Mund", "da\u00b7rin\u00b7ne", "lie\u00b7set", ",", "und", "mein", "Geist", "sich", "dran", "er\u00b7quickt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PAV", "VVFIN", "$,", "KON", "PPOSAT", "NN", "PRF", "PAV", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.30": {"text": "Werd ich auch zugleich an Dich und an Deine Gnade denken,", "tokens": ["Werd", "ich", "auch", "zu\u00b7gleich", "an", "Dich", "und", "an", "Dei\u00b7ne", "Gna\u00b7de", "den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "APPR", "PPER", "KON", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.31": {"text": "Und den Geist auf Deine Huld, ", "tokens": ["Und", "den", "Geist", "auf", "Dei\u00b7ne", "Huld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.32": {"text": "Ich w\u00fcnsch Dir, ", "tokens": ["Ich", "w\u00fcnsch", "Dir", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,"], "meter": "+--", "measure": "dactylic.init"}, "line.33": {"text": "Welcher Stadt und Land erfreuet, und vom Fleisch und Blute ist;", "tokens": ["Wel\u00b7cher", "Stadt", "und", "Land", "er\u00b7freu\u00b7et", ",", "und", "vom", "Fleisch", "und", "Blu\u00b7te", "ist", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "KON", "NN", "VVFIN", "$,", "KON", "APPRART", "NN", "KON", "NN", "VAFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.34": {"text": "Welcher aus Sophiens Schoo\u00df und Augustens Lenden stammet;", "tokens": ["Wel\u00b7cher", "aus", "So\u00b7phi\u00b7ens", "Schoo\u00df", "und", "Au\u00b7gus\u00b7tens", "Len\u00b7den", "stam\u00b7met", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "APPR", "NE", "NN", "KON", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+--+-+-", "measure": "trochaic.septa.relaxed"}, "line.35": {"text": "Der mit einem solchen Geist, wie Augustus angeflammet;", "tokens": ["Der", "mit", "ei\u00b7nem", "sol\u00b7chen", "Geist", ",", "wie", "Au\u00b7gus\u00b7tus", "an\u00b7ge\u00b7flam\u00b7met", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "PIAT", "NN", "$,", "PWAV", "NE", "VVPP", "$."], "meter": "+-+-+-+-+--+-+-", "measure": "trochaic.septa.relaxed"}, "line.36": {"text": "Welcher sich in samtnen Windeln auf Charlottens H\u00e4nden regt;", "tokens": ["Wel\u00b7cher", "sich", "in", "samt\u00b7nen", "Win\u00b7deln", "auf", "Char\u00b7lot\u00b7tens", "H\u00e4n\u00b7den", "regt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "PRF", "APPR", "ADJA", "NN", "APPR", "NE", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.37": {"text": "Ihre Sch\u00f6nheit im Gesichte, und Dein Bild im Herzen tr\u00e4gt.", "tokens": ["Ih\u00b7re", "Sch\u00f6n\u00b7heit", "im", "Ge\u00b7sich\u00b7te", ",", "und", "Dein", "Bild", "im", "Her\u00b7zen", "tr\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "$,", "KON", "PPOSAT", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.38": {"text": "Held! in Demuth bitt ich Dich: schenk mir ferner Deine G\u00fcte,", "tokens": ["Held", "!", "in", "De\u00b7muth", "bitt", "ich", "Dich", ":", "schenk", "mir", "fer\u00b7ner", "Dei\u00b7ne", "G\u00fc\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punc