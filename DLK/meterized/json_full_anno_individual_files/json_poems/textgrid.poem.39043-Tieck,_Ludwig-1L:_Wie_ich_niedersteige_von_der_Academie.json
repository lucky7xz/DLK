{"textgrid.poem.39043": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie ich niedersteige von der Academie", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie ich niedersteige von der Academie", "tokens": ["Wie", "ich", "nie\u00b7ders\u00b7tei\u00b7ge", "von", "der", "A\u00b7ca\u00b7de\u00b7mie"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PIS", "APPR", "ART", "NN"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Und \u00fcber den Platz des Pallastes gehe,", "tokens": ["Und", "\u00fc\u00b7ber", "den", "Platz", "des", "Pal\u00b7las\u00b7tes", "ge\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Gewahr ich schon aus der Ferne hoch zu Pferde", "tokens": ["Ge\u00b7wahr", "ich", "schon", "aus", "der", "Fer\u00b7ne", "hoch", "zu", "Pfer\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ADV", "APPR", "ART", "NN", "ADJD", "APPR", "NN"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Einen umstreifenden Doktor und Wunderth\u00e4ter,", "tokens": ["Ei\u00b7nen", "um\u00b7strei\u00b7fen\u00b7den", "Dok\u00b7tor", "und", "Wun\u00b7der\u00b7th\u00e4\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "$,"], "meter": "+--+--+--+-+-", "measure": "dactylic.tri.plus"}, "line.5": {"text": "Der durch das Land, die D\u00f6rfer und St\u00e4dte streift,", "tokens": ["Der", "durch", "das", "Land", ",", "die", "D\u00f6r\u00b7fer", "und", "St\u00e4d\u00b7te", "streift", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "$,", "ART", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Am Sattel h\u00e4ngend neben ihm die Apotheke,", "tokens": ["Am", "Sat\u00b7tel", "h\u00e4n\u00b7gend", "ne\u00b7ben", "ihm", "die", "A\u00b7pot\u00b7he\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "APPR", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+---", "measure": "unknown.measure.penta"}, "line.7": {"text": "Arcana und miracul\u00f6se Mixturen.", "tokens": ["Ar\u00b7ca\u00b7na", "und", "mi\u00b7ra\u00b7cu\u00b7l\u00f6\u00b7se", "Mix\u00b7tu\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJA", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.8": {"text": "Um ihn sind B\u00fcrger und Bauern versammelt,", "tokens": ["Um", "ihn", "sind", "B\u00fcr\u00b7ger", "und", "Bau\u00b7ern", "ver\u00b7sam\u00b7melt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "VAFIN", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Und er preist die hohe Kraft seiner Werke.", "tokens": ["Und", "er", "preist", "die", "ho\u00b7he", "Kraft", "sei\u00b7ner", "Wer\u00b7ke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.10": {"text": "Wie ich langsam und l\u00e4chelnd n\u00e4her schreite,", "tokens": ["Wie", "ich", "lang\u00b7sam", "und", "l\u00e4\u00b7chelnd", "n\u00e4\u00b7her", "schrei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "KON", "ADJD", "ADJD", "VVFIN", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.11": {"text": "Erfreut den Wundermann zu h\u00f6ren und zu sehn,", "tokens": ["Er\u00b7freut", "den", "Wun\u00b7der\u00b7mann", "zu", "h\u00f6\u00b7ren", "und", "zu", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Ruft er pl\u00f6tzlich lauter und feuriger:", "tokens": ["Ruft", "er", "pl\u00f6tz\u00b7lich", "lau\u00b7ter", "und", "feu\u00b7ri\u00b7ger", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.13": {"text": "Und wollt ihr mir nicht glauben, so seht dahin,", "tokens": ["Und", "wollt", "ihr", "mir", "nicht", "glau\u00b7ben", ",", "so", "seht", "da\u00b7hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PPER", "PTKNEG", "VVINF", "$,", "ADV", "VVFIN", "PAV", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "Dort kommt einer meiner Patienten,", "tokens": ["Dort", "kommt", "ei\u00b7ner", "mei\u00b7ner", "Pa\u00b7ti\u00b7en\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.15": {"text": "Noch hinkt er ein weniges, aber von welcher Gicht,", "tokens": ["Noch", "hinkt", "er", "ein", "we\u00b7ni\u00b7ges", ",", "a\u00b7ber", "von", "wel\u00b7cher", "Gicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "PIS", "$,", "ADV", "APPR", "PWAT", "NN", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.16": {"text": "Von welcher L\u00e4hmung ich den edlen Mann geheilt,", "tokens": ["Von", "wel\u00b7cher", "L\u00e4h\u00b7mung", "ich", "den", "ed\u00b7len", "Mann", "ge\u00b7heilt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "PPER", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "L\u00e4\u00dft sich kaum schildern, und nicht genug r\u00fchmen die Kur.", "tokens": ["L\u00e4\u00dft", "sich", "kaum", "schil\u00b7dern", ",", "und", "nicht", "ge\u00b7nug", "r\u00fch\u00b7men", "die", "Kur", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "VVINF", "$,", "KON", "PTKNEG", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "+--+--+-+---+", "measure": "dactylic.di.plus"}, "line.18": {"text": "Alle betrachten mich staunend,", "tokens": ["Al\u00b7le", "be\u00b7trach\u00b7ten", "mich", "stau\u00b7nend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "VVPP", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.19": {"text": "Doch ich, z\u00fcrnend zum Propheten gewandt, erwiedre:", "tokens": ["Doch", "ich", ",", "z\u00fcr\u00b7nend", "zum", "Pro\u00b7phe\u00b7ten", "ge\u00b7wandt", ",", "er\u00b7wied\u00b7re", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PPER", "$,", "VVPP", "APPRART", "NN", "VVPP", "$,", "VVFIN", "$."], "meter": "--+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.20": {"text": "Soll ich H\u00f6rer seyn und Zuschauer eurer Com\u00f6die,", "tokens": ["Soll", "ich", "H\u00f6\u00b7rer", "seyn", "und", "Zu\u00b7schau\u00b7er", "eu\u00b7rer", "Co\u00b7m\u00f6\u00b7die", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "NN", "VAINF", "KON", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.21": {"text": "M\u00fc\u00dft ihr mich nicht selbst als Person auftreten lassen.", "tokens": ["M\u00fc\u00dft", "ihr", "mich", "nicht", "selbst", "als", "Per\u00b7son", "auf\u00b7tre\u00b7ten", "las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "PTKNEG", "ADV", "KOUS", "NN", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Er, ohne gest\u00f6rt zu seyn, f\u00e4hrt fort,", "tokens": ["Er", ",", "oh\u00b7ne", "ge\u00b7st\u00f6rt", "zu", "seyn", ",", "f\u00e4hrt", "fort", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "KOUI", "VVPP", "PTKZU", "VAINF", "$,", "VVFIN", "PTKVZ", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.23": {"text": "Und noch aus der Ferne vernehm ich:", "tokens": ["Und", "noch", "aus", "der", "Fer\u00b7ne", "ver\u00b7nehm", "ich", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "VVFIN", "PPER", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.24": {"text": "Seht ein Beispiel von der Menschen Undankbarkeit,", "tokens": ["Seht", "ein", "Bei\u00b7spiel", "von", "der", "Men\u00b7schen", "Un\u00b7dank\u00b7bar\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+--+", "measure": "iambic.hexa.chol"}, "line.25": {"text": "Nicht Wort will er es nun haben, was ich an ihm gethan,", "tokens": ["Nicht", "Wort", "will", "er", "es", "nun", "ha\u00b7ben", ",", "was", "ich", "an", "ihm", "ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "VMFIN", "PPER", "PPER", "ADV", "VAFIN", "$,", "PWS", "PPER", "APPR", "PPER", "VVPP", "$,"], "meter": "-+--+-+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.26": {"text": "Aber er komme mir nur zum zweitenmal,", "tokens": ["A\u00b7ber", "er", "kom\u00b7me", "mir", "nur", "zum", "zwei\u00b7ten\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADV", "APPRART", "ADV", "$,"], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.27": {"text": "Da wird kein Mittel f\u00fcr ihn in allen meinen Schachteln seyn:", "tokens": ["Da", "wird", "kein", "Mit\u00b7tel", "f\u00fcr", "ihn", "in", "al\u00b7len", "mei\u00b7nen", "Schach\u00b7teln", "seyn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIAT", "NN", "APPR", "PPER", "APPR", "PIAT", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+--+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.28": {"text": "Drum kauft, ihr Landesgenossen, kauft f\u00fcr Weniges", "tokens": ["Drum", "kauft", ",", "ihr", "Lan\u00b7des\u00b7ge\u00b7nos\u00b7sen", ",", "kauft", "f\u00fcr", "We\u00b7ni\u00b7ges"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PAV", "VVFIN", "$,", "PPOSAT", "NN", "$,", "VVFIN", "APPR", "NN"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.29": {"text": "Heil, Gesundheit, Schmerzlosigkeit, heitern Geist,", "tokens": ["Heil", ",", "Ge\u00b7sund\u00b7heit", ",", "Schmerz\u00b7lo\u00b7sig\u00b7keit", ",", "hei\u00b7tern", "Geist", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.30": {"text": "So lang' es euch von mir noch so gut geboten wird.", "tokens": ["So", "lang'", "es", "euch", "von", "mir", "noch", "so", "gut", "ge\u00b7bo\u00b7ten", "wird", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "PRF", "APPR", "PPER", "ADV", "ADV", "ADJD", "VVPP", "VAFIN", "$."], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Wie ich niedersteige von der Academie", "tokens": ["Wie", "ich", "nie\u00b7ders\u00b7tei\u00b7ge", "von", "der", "A\u00b7ca\u00b7de\u00b7mie"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PIS", "APPR", "ART", "NN"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Und \u00fcber den Platz des Pallastes gehe,", "tokens": ["Und", "\u00fc\u00b7ber", "den", "Platz", "des", "Pal\u00b7las\u00b7tes", "ge\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Gewahr ich schon aus der Ferne hoch zu Pferde", "tokens": ["Ge\u00b7wahr", "ich", "schon", "aus", "der", "Fer\u00b7ne", "hoch", "zu", "Pfer\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ADV", "APPR", "ART", "NN", "ADJD", "APPR", "NN"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Einen umstreifenden Doktor und Wunderth\u00e4ter,", "tokens": ["Ei\u00b7nen", "um\u00b7strei\u00b7fen\u00b7den", "Dok\u00b7tor", "und", "Wun\u00b7der\u00b7th\u00e4\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word"