{"dta.poem.23592": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Landwirthschaftliches.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1840", "urn": "urn:nbn:de:kobv:b4-200905192626", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mit Vortheil l\u00e4sst sich bauen", "tokens": ["Mit", "Vor\u00b7theil", "l\u00e4sst", "sich", "bau\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "PRF", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein neues Futterkraut,", "tokens": ["Ein", "neu\u00b7es", "Fut\u00b7ter\u00b7kraut", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das man in allen Gauen", "tokens": ["Das", "man", "in", "al\u00b7len", "Gau\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "PIS", "APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sonst wenig hat gebaut.", "tokens": ["Sonst", "we\u00b7nig", "hat", "ge\u00b7baut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VAFIN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Damit kann man beleben", "tokens": ["Da\u00b7mit", "kann", "man", "be\u00b7le\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "VMFIN", "PIS", "VVINF"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Die Viehzucht \u00fcberall,", "tokens": ["Die", "Vieh\u00b7zucht", "\u00fc\u00b7be\u00b7rall", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Man kann es t\u00e4glich geben", "tokens": ["Man", "kann", "es", "t\u00e4g\u00b7lich", "ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VMFIN", "PPER", "ADJD", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dem Vieh in H\u00fcrd' und Stall.", "tokens": ["Dem", "Vieh", "in", "H\u00fcrd'", "und", "Stall", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und ist gar wohl bekannt,", "tokens": ["Und", "ist", "gar", "wohl", "be\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Die Frucht hat andern Namen,", "tokens": ["Die", "Frucht", "hat", "an\u00b7dern", "Na\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wird ", "tokens": ["Wird"], "token_info": ["word"], "pos": ["VAFIN"], "meter": "+", "measure": "single.up"}}, "stanza.4": {"line.1": {"text": "Wenn's Vieh daran nur lecket,", "tokens": ["Wenn's", "Vieh", "da\u00b7ran", "nur", "le\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PAV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So wird es wohlgemuth,", "tokens": ["So", "wird", "es", "wohl\u00b7ge\u00b7muth", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df es, was man bezwecket,", "tokens": ["Da\u00df", "es", ",", "was", "man", "be\u00b7zwe\u00b7cket", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "PRELS", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Recht gern und willig thut.", "tokens": ["Recht", "gern", "und", "wil\u00b7lig", "thut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "KON", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}