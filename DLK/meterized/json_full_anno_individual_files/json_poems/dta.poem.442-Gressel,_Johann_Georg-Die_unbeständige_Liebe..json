{"dta.poem.442": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Die unbest\u00e4ndige Liebe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Die Lieb ist anfangs s\u00fc\u00df/ das Ende aber bitter/", "tokens": ["Die", "Lieb", "ist", "an\u00b7fangs", "s\u00fc\u00df", "/", "das", "En\u00b7de", "a\u00b7ber", "bit\u00b7ter", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "$(", "ART", "NN", "ADV", "ADJD", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Jhr Kommen bringet Lust/ ihr Abzug Ungewitter/", "tokens": ["Ihr", "Kom\u00b7men", "brin\u00b7get", "Lust", "/", "ihr", "Ab\u00b7zug", "Un\u00b7ge\u00b7wit\u00b7ter", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "NN", "$(", "PPOSAT", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sie gleicht dem s\u00fcssen Flu\u00df/ der sich ins Meer ergie\u00dft/", "tokens": ["Sie", "gleicht", "dem", "s\u00fcs\u00b7sen", "Flu\u00df", "/", "der", "sich", "ins", "Meer", "er\u00b7gie\u00dft", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$(", "PRELS", "PRF", "APPRART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und drauf als selbiges mit bitterm Saltze flie\u00dft.", "tokens": ["Und", "drauf", "als", "sel\u00b7bi\u00b7ges", "mit", "bit\u00b7term", "Salt\u00b7ze", "flie\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "KOUS", "ADJA", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}