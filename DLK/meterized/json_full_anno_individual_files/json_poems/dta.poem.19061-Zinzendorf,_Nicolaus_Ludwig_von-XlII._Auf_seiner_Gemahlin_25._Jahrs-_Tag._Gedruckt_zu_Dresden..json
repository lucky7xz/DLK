{"dta.poem.19061": {"metadata": {"author": {"name": "Zinzendorf, Nicolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "XlII.  Auf seiner Gemahlin 25. Jahrs-  \n Tag.   Gedruckt zu Dresden.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20688-6", "language": ["de:0.99"], "booktitle": "Zinzendorf, Nicolaus Ludwig von: Teutscher Gedichte Erster Theil. Herrnhuth, 1735."}, "poem": {"stanza.1": {"line.1": {"text": "Geht! werft euch vor die Majest\u00e4t", "tokens": ["Geht", "!", "werft", "euch", "vor", "die", "Ma\u00b7jes\u00b7t\u00e4t"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$.", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des K\u00f6niges der Liebe,", "tokens": ["Des", "K\u00f6\u00b7ni\u00b7ges", "der", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die euch bereits entgegen geht,", "tokens": ["Die", "euch", "be\u00b7reits", "ent\u00b7ge\u00b7gen", "geht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "PTKVZ", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Jhr, meiner Seelen Triebe;", "tokens": ["Ihr", ",", "mei\u00b7ner", "See\u00b7len", "Trie\u00b7be", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Blickt mit des Glaubens Augen an!", "tokens": ["Blickt", "mit", "des", "Glau\u00b7bens", "Au\u00b7gen", "an", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Du bist, o Seelen-Br\u00e4utigam!", "tokens": ["Du", "bist", ",", "o", "See\u00b7len\u00b7Br\u00e4u\u00b7ti\u00b7gam", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "FM", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein allgemeines Wesen:", "tokens": ["Ein", "all\u00b7ge\u00b7mei\u00b7nes", "We\u00b7sen", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wer kranck auf Erden zu dir kam,", "tokens": ["Wer", "kranck", "auf", "Er\u00b7den", "zu", "dir", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "APPR", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den liessest du genesen.", "tokens": ["Den", "lies\u00b7sest", "du", "ge\u00b7ne\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und deine Wunder-Kraft gesp\u00fcrt.", "tokens": ["Und", "dei\u00b7ne", "Wun\u00b7der\u00b7Kraft", "ge\u00b7sp\u00fcrt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ich bins versichert, da\u00df du mich", "tokens": ["Ich", "bins", "ver\u00b7si\u00b7chert", ",", "da\u00df", "du", "mich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "KOUS", "PPER", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu deinem Volck gezehlet;", "tokens": ["Zu", "dei\u00b7nem", "Volck", "ge\u00b7zeh\u00b7let", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mit deinem Hertzen ewiglich", "tokens": ["Mit", "dei\u00b7nem", "Hert\u00b7zen", "e\u00b7wig\u00b7lich"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Verbunden und verm\u00e4hlet;", "tokens": ["Ver\u00b7bun\u00b7den", "und", "ver\u00b7m\u00e4h\u00b7let", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Auch mit vor meine Seele flehst.", "tokens": ["Auch", "mit", "vor", "mei\u00b7ne", "See\u00b7le", "flehst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ich mache mich im Geist bereit,", "tokens": ["Ich", "ma\u00b7che", "mich", "im", "Geist", "be\u00b7reit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Beym Thone stiller Lieder,", "tokens": ["Beym", "Tho\u00b7ne", "stil\u00b7ler", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und werffe deiner W\u00fcrdigkeit,", "tokens": ["Und", "werf\u00b7fe", "dei\u00b7ner", "W\u00fcr\u00b7dig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mich vor die F\u00fcsse nieder.", "tokens": ["Mich", "vor", "die", "F\u00fcs\u00b7se", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und bete meiner Seele vor.", "tokens": ["Und", "be\u00b7te", "mei\u00b7ner", "See\u00b7le", "vor", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Gib, da\u00df ich spreche, was dein Mund", "tokens": ["Gib", ",", "da\u00df", "ich", "spre\u00b7che", ",", "was", "dein", "Mund"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "$,", "KOUS", "PPER", "VVFIN", "$,", "PRELS", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem Vater sagen wolte,", "tokens": ["Dem", "Va\u00b7ter", "sa\u00b7gen", "wol\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wann er ihn an den Liebes-Bund", "tokens": ["Wann", "er", "ihn", "an", "den", "Lie\u00b7bes\u00b7Bund"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit mir erinnern solte.", "tokens": ["Mit", "mir", "e\u00b7rin\u00b7nern", "sol\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVINF", "VMFIN", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "So lall ihm auch das andre nach.", "tokens": ["So", "lall", "ihm", "auch", "das", "and\u00b7re", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "PIS", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Mein Abba! deine Gnaden-Wahl", "tokens": ["Mein", "Ab\u00b7ba", "!", "dei\u00b7ne", "Gna\u00b7den\u00b7Wahl"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$.", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "An mir zu offenbahren,", "tokens": ["An", "mir", "zu", "of\u00b7fen\u00b7bah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Hast du mich in das Jammer-Thal,", "tokens": ["Hast", "du", "mich", "in", "das", "Jam\u00b7mer\u00b7Thal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vor f\u00fcnf und zwantzig Jahren,", "tokens": ["Vor", "f\u00fcnf", "und", "zwant\u00b7zig", "Jah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "KON", "CARD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Durch eine, dir gem\u00e4sse, Macht,", "tokens": ["Durch", "ei\u00b7ne", ",", "dir", "ge\u00b7m\u00e4s\u00b7se", ",", "Macht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "$,", "PPER", "ADJA", "$,", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Aus Mutter Leib hervor gebracht.", "tokens": ["Aus", "Mut\u00b7ter", "Leib", "her\u00b7vor", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "PTKVZ", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Die Welt bekam ich ins Gesicht,", "tokens": ["Die", "Welt", "be\u00b7kam", "ich", "ins", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie hat mir wohl gefallen;", "tokens": ["Sie", "hat", "mir", "wohl", "ge\u00b7fal\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Bey nahe h\u00e4tt ich Lust gekriegt,", "tokens": ["Bey", "na\u00b7he", "h\u00e4tt", "ich", "Lust", "ge\u00b7kriegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "VAFIN", "PPER", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit ihr dahin zu wallen.", "tokens": ["Mit", "ihr", "da\u00b7hin", "zu", "wal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PAV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Du weist es Hertzens-K\u00fcndiger.", "tokens": ["Du", "weist", "es", "Hert\u00b7zens\u00b7K\u00fcn\u00b7di\u00b7ger", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Die gr\u00f6ste Ungelegenheit", "tokens": ["Die", "gr\u00f6s\u00b7te", "Un\u00b7ge\u00b7le\u00b7gen\u00b7heit"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Nachfolg unsers Lammes,", "tokens": ["Der", "Nach\u00b7folg", "un\u00b7sers", "Lam\u00b7mes", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die angebohrne Herrlichkeit,", "tokens": ["Die", "an\u00b7ge\u00b7bohr\u00b7ne", "Herr\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des weltlich hohen Stammes,", "tokens": ["Des", "welt\u00b7lich", "ho\u00b7hen", "Stam\u00b7mes", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die noch so manches Hertz verstockt,", "tokens": ["Die", "noch", "so", "man\u00b7ches", "Hertz", "ver\u00b7stockt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die hatte mich auch angelockt.", "tokens": ["Die", "hat\u00b7te", "mich", "auch", "an\u00b7ge\u00b7lockt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Bey dieser steten Demmerung,", "tokens": ["Bey", "die\u00b7ser", "ste\u00b7ten", "Dem\u00b7me\u00b7rung", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.2": {"text": "Wo Tag und Nacht vorhanden,", "tokens": ["Wo", "Tag", "und", "Nacht", "vor\u00b7han\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und weder Finsterni\u00df genung,", "tokens": ["Und", "we\u00b7der", "Fins\u00b7ter\u00b7ni\u00df", "ge\u00b7nung", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "KON", "NN", "ADV", "$,"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.4": {"text": "Nach wahres Licht entstanden,", "tokens": ["Nach", "wah\u00b7res", "Licht", "ent\u00b7stan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Des Weges zu der Seligkeit.", "tokens": ["Des", "We\u00b7ges", "zu", "der", "Se\u00b7lig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Da wird man ehrbar und gerecht;", "tokens": ["Da", "wird", "man", "ehr\u00b7bar", "und", "ge\u00b7recht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da f\u00fcrchtet man die H\u00f6lle,", "tokens": ["Da", "f\u00fcrch\u00b7tet", "man", "die", "H\u00f6l\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und ist sein Lebenlang ein Knecht,", "tokens": ["Und", "ist", "sein", "Le\u00b7ben\u00b7lang", "ein", "Knecht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Un kommt nicht von der Stelle.", "tokens": ["Un", "kommt", "nicht", "von", "der", "Stel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKNEG", "APPR", "ART", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "Macht, da\u00df die S\u00fcnde sch\u00e4ndlich ist.", "tokens": ["Macht", ",", "da\u00df", "die", "S\u00fcn\u00b7de", "sch\u00e4nd\u00b7lich", "ist", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "ART", "NN", "ADJD", "VAFIN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.11": {"line.1": {"text": "Das Hertz nimmt Christi Sinn nicht an,", "tokens": ["Das", "Hertz", "nimmt", "Chris\u00b7ti", "Sinn", "nicht", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NE", "NN", "PTKNEG", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Leidenschaften leben,", "tokens": ["Die", "Lei\u00b7den\u00b7schaf\u00b7ten", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und m\u00fcssen sich nur dann und wann", "tokens": ["Und", "m\u00fcs\u00b7sen", "sich", "nur", "dann", "und", "wann"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PRF", "ADV", "ADV", "KON", "PWAV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In den Gehorsam geben;", "tokens": ["In", "den", "Ge\u00b7hor\u00b7sam", "ge\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ein Satanas dem andern weicht.", "tokens": ["Ein", "Sa\u00b7ta\u00b7nas", "dem", "an\u00b7dern", "weicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Von einer solchen Sclaverey,", "tokens": ["Von", "ei\u00b7ner", "sol\u00b7chen", "Scla\u00b7ve\u00b7rey", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ward ich in diesen Stunden,", "tokens": ["Ward", "ich", "in", "die\u00b7sen", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "w