,", "tokens": ["Man", "hats", "kaum", "h\u00f6\u00b7ren", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ADV", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wohin noch keine Sinnen gehn,", "tokens": ["Wo\u00b7hin", "noch", "kei\u00b7ne", "Sin\u00b7nen", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da kan das Hertz nach brennen.", "tokens": ["Da", "kan", "das", "Hertz", "nach", "bren\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "APPR", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Hat meine Seele JEsum lieb.", "tokens": ["Hat", "mei\u00b7ne", "See\u00b7le", "Je\u00b7sum", "lieb", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "NE", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.17": {"line.1": {"text": "Mein Salomo! verm\u00e4hle dich", "tokens": ["Mein", "Sa\u00b7lo\u00b7mo", "!", "ver\u00b7m\u00e4h\u00b7le", "dich"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NE", "$.", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit meinen innern Sinnen:", "tokens": ["Mit", "mei\u00b7nen", "in\u00b7nern", "Sin\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Beherrsche mehr, als K\u00f6niglich,", "tokens": ["Be\u00b7herr\u00b7sche", "mehr", ",", "als", "K\u00f6\u00b7nig\u00b7lich", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "KOUS", "NN", "$,"], "meter": "-+-