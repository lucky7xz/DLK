{"dta.poem.1929": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Ubersezte Spruch-Reimen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Knabe/ der nun redt und sicher lauffen kan/", "tokens": ["Ein", "Kna\u00b7be", "/", "der", "nun", "redt", "und", "si\u00b7cher", "lauf\u00b7fen", "kan", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "ADV", "VVFIN", "KON", "ADJD", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Liebt stilles Sitzen nicht/ spielt gern mit seines gleichen/", "tokens": ["Liebt", "stil\u00b7les", "Sit\u00b7zen", "nicht", "/", "spielt", "gern", "mit", "sei\u00b7nes", "glei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "PTKNEG", "$(", "VVFIN", "ADV", "APPR", "PPOSAT", "ADJA", "$("], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Erz\u00fcrnt sich bald/ und l\u00e4st den Zorn auch bald verstreichen/", "tokens": ["Er\u00b7z\u00fcrnt", "sich", "bald", "/", "und", "l\u00e4st", "den", "Zorn", "auch", "bald", "ver\u00b7strei\u00b7chen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "$(", "KON", "VVFIN", "ART", "NN", "ADV", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "F\u00e4ngt iede Viertelstund ein neues Wesen an.", "tokens": ["F\u00e4ngt", "ie\u00b7de", "Vier\u00b7tel\u00b7stund", "ein", "neu\u00b7es", "We\u00b7sen", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der J\u00fcngling ohne Bart/ der strengen Zucht entgangen/", "tokens": ["Der", "J\u00fcng\u00b7ling", "oh\u00b7ne", "Bart", "/", "der", "stren\u00b7gen", "Zucht", "ent\u00b7gan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$(", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Hat Pferd und Hunde lieb/ sucht ihm das gr\u00fcne Feld/", "tokens": ["Hat", "Pferd", "und", "Hun\u00b7de", "lieb", "/", "sucht", "ihm", "das", "gr\u00fc\u00b7ne", "Feld", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "NN", "ADJD", "$(", "VVFIN", "PPER", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wehlt langsam was ihm gutt/ h\u00e4lt selten lange Geld/", "tokens": ["Wehlt", "lang\u00b7sam", "was", "ihm", "gutt", "/", "h\u00e4lt", "sel\u00b7ten", "lan\u00b7ge", "Geld", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PWS", "PPER", "ADJD", "$(", "VVFIN", "ADJD", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "H\u00f6rt kein Erinnern an/ hat B\u00f6ses bald gefangen/", "tokens": ["H\u00f6rt", "kein", "E\u00b7rin\u00b7nern", "an", "/", "hat", "B\u00f6\u00b7ses", "bald", "ge\u00b7fan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "PTKVZ", "$(", "VAFIN", "NE", "ADV", "ADJD", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Hochtrabend/ voll Begier/ geschwinde Brunst zu fassen/", "tokens": ["Hoch\u00b7tra\u00b7bend", "/", "voll", "Be\u00b7gier", "/", "ge\u00b7schwin\u00b7de", "Brunst", "zu", "fas\u00b7sen", "/"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$(", "ADJD", "NN", "$(", "ADJA", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und was er werth geacht hinwieder zu verlassen.", "tokens": ["Und", "was", "er", "werth", "ge\u00b7acht", "hin\u00b7wie\u00b7der", "zu", "ver\u00b7las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADJD", "VVPP", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Bey reiffer Mannbarkeit ist alles umgekehrt/", "tokens": ["Bey", "reif\u00b7fer", "Mann\u00b7bar\u00b7keit", "ist", "al\u00b7les", "um\u00b7ge\u00b7kehrt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "PIS", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Man nimmt des Beutels wahr/ damit man solchen spicke/", "tokens": ["Man", "nimmt", "des", "Beu\u00b7tels", "wahr", "/", "da\u00b7mit", "man", "sol\u00b7chen", "spi\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "ADJD", "$(", "KOUS", "PIS", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Sucht Freundschafft in der Welt/ m\u00fcht sich um Ehr und", "tokens": ["Sucht", "Freund\u00b7schafft", "in", "der", "Welt", "/", "m\u00fcht", "sich", "um", "Ehr", "und"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "NN", "APPR", "ART", "NN", "$(", "VVFIN", "PRF", "APPR", "NN", "KON"], "meter": "-+-+-++--+-", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "Gl\u00fccke/", "tokens": ["Gl\u00fc\u00b7cke", "/"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.15": {"text": "Und h\u00fctet sich zu seyn mit Reu und Schimpff beschwert.", "tokens": ["Und", "h\u00fc\u00b7tet", "sich", "zu", "seyn", "mit", "Reu", "und", "Schimpff", "be\u00b7schwert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "PTKZU", "VAINF", "APPR", "NE", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Viel Ungelegenheit umgiebt den gutten Alten/", "tokens": ["Viel", "Un\u00b7ge\u00b7le\u00b7gen\u00b7heit", "um\u00b7giebt", "den", "gut\u00b7ten", "Al\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Er sucht/ und was er findt de\u00df mu\u00df er sich enthalten", "tokens": ["Er", "sucht", "/", "und", "was", "er", "findt", "de\u00df", "mu\u00df", "er", "sich", "ent\u00b7hal\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$(", "KON", "PWS", "PPER", "VVFIN", "PDS", "VMFIN", "PPER", "PRF", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "us Kargheit/ Furcht und Geitz/ greifft alles laulicht an/", "tokens": ["us", "Karg\u00b7heit", "/", "Furcht", "und", "Geitz", "/", "greifft", "al\u00b7les", "lau\u00b7licht", "an", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$(", "NN", "KON", "NN", "$(", "VVFIN", "PIS", "ADJD", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Schiebt auff/ und hofft noch viel/ lobt was man hat gethan", "tokens": ["Schiebt", "auff", "/", "und", "hofft", "noch", "viel", "/", "lobt", "was", "man", "hat", "ge\u00b7than"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "$(", "KON", "VVFIN", "ADV", "ADV", "$(", "VVFIN", "PWS", "PIS", "VAFIN", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "ey seiner jungen Zeit/ klagt \u00fcber Zeit und Sitten/", "tokens": ["ey", "sei\u00b7ner", "jun\u00b7gen", "Zeit", "/", "klagt", "\u00fc\u00b7ber", "Zeit", "und", "Sit\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$(", "VVFIN", "APPR", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "nd l\u00e4st der Jugend Thun nicht leichtlich unbestritten.", "tokens": ["nd", "l\u00e4st", "der", "Ju\u00b7gend", "Thun", "nicht", "leicht\u00b7lich", "un\u00b7be\u00b7strit\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "VVFIN", "PTKNEG", "ADJD", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}