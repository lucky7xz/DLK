{"dta.poem.6196": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Jahrs-Zeiten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn endlich vollends gar so Barchus, als Pomone", "tokens": ["Wenn", "end\u00b7lich", "vol\u00b7lends", "gar", "so", "Bar\u00b7chus", ",", "als", "Po\u00b7mo\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "ADV", "ADV", "ADV", "ADV", "NE", "$,", "KOUS", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sich eine Crone", "tokens": ["Sich", "ei\u00b7ne", "Cro\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["PRF", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Von Trauben und von Fr\u00fcchten winden,", "tokens": ["Von", "Trau\u00b7ben", "und", "von", "Fr\u00fcch\u00b7ten", "win\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und man derselben Ruhm durch lautes Jauchzen mehrt,", "tokens": ["Und", "man", "der\u00b7sel\u00b7ben", "Ruhm", "durch", "lau\u00b7tes", "Jauch\u00b7zen", "mehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "PDAT", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Weil, durch des ", "tokens": ["Weil", ",", "durch", "des"], "token_info": ["word", "punct", "word", "word"], "pos": ["KOUS", "$,", "APPR", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Erf\u00fcllet ist und eingetroffen;", "tokens": ["Er\u00b7f\u00fcl\u00b7let", "ist", "und", "ein\u00b7ge\u00b7trof\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "KON", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "So wird durch minder Feur der Creis der Welt verkl\u00e4rt.", "tokens": ["So", "wird", "durch", "min\u00b7der", "Feur", "der", "Creis", "der", "Welt", "ver\u00b7kl\u00e4rt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "ADJA", "NN", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die Sonne, welche scheint am Firmamente", "tokens": ["Die", "Son\u00b7ne", ",", "wel\u00b7che", "scheint", "am", "Fir\u00b7ma\u00b7men\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Als wenn sie r\u00fcckwerts rennte,", "tokens": ["Als", "wenn", "sie", "r\u00fcck\u00b7werts", "renn\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Wirfft ein gemildert Feur, bey etwas k\u00fcrtzern Tagen,", "tokens": ["Wirfft", "ein", "ge\u00b7mil\u00b7dert", "Feur", ",", "bey", "et\u00b7was", "k\u00fcrt\u00b7zern", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "VVPP", "NN", "$,", "APPR", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und kocht im Reben-Laub den edlen Safft,", "tokens": ["Und", "kocht", "im", "Re\u00b7ben\u00b7Laub", "den", "ed\u00b7len", "Safft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Der bey so manchen Fest uns manche Freude schafft.", "tokens": ["Der", "bey", "so", "man\u00b7chen", "Fest", "uns", "man\u00b7che", "Freu\u00b7de", "schafft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADV", "PIAT", "NN", "PPER", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "In B\u00e4umen, welche Fr\u00fcchte tragen,", "tokens": ["In", "B\u00e4u\u00b7men", ",", "wel\u00b7che", "Fr\u00fcch\u00b7te", "tra\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PWAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Erh\u00e4lt sie ihren Safft, verd\u00fcnnt und macht ihn fliessen,", "tokens": ["Er\u00b7h\u00e4lt", "sie", "ih\u00b7ren", "Safft", ",", "ver\u00b7d\u00fcnnt", "und", "macht", "ihn", "flies\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$,", "VVFIN", "KON", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "So, da\u00df er auswerts sich im Dunst nicht kan ergiessen.", "tokens": ["So", ",", "da\u00df", "er", "aus\u00b7werts", "sich", "im", "Dunst", "nicht", "kan", "er\u00b7gies\u00b7sen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "ADV", "PRF", "APPRART", "NN", "PTKNEG", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Durch ein gem\u00e4chliches Bewegen", "tokens": ["Durch", "ein", "ge\u00b7m\u00e4ch\u00b7li\u00b7ches", "Be\u00b7we\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Treibt sie ihn in die hole R\u00f6ren", "tokens": ["Treibt", "sie", "ihn", "in", "die", "ho\u00b7le", "R\u00f6\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Bis da\u00df die Fr\u00fccht\u2019entstehn, die so viel Anmuht hegen,", "tokens": ["Bis", "da\u00df", "die", "Fr\u00fccht'\u00b7ent\u00b7stehn", ",", "die", "so", "viel", "An\u00b7muht", "he\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "ART", "NN", "$,", "PRELS", "ADV", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Die mit so holden Ruch und Farben, Nas\u2019 und Augen", "tokens": ["Die", "mit", "so", "hol\u00b7den", "Ruch", "und", "Far\u00b7ben", ",", "Nas'", "und", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "APPR", "ADV", "ADJA", "NN", "KON", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Sehr zu vergn\u00fcgen taugen,", "tokens": ["Sehr", "zu", "ver\u00b7gn\u00fc\u00b7gen", "tau\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.21": {"text": "Und dem Geschmack zugleich so manche Lust gew\u00e4hren.", "tokens": ["Und", "dem", "Ge\u00b7schmack", "zu\u00b7gleich", "so", "man\u00b7che", "Lust", "ge\u00b7w\u00e4h\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "ADV", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}