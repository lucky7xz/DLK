{"textgrid.poem.30885": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "100.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Alles, was heilsam, was l\u00f6blich sich nennet,", "tokens": ["Al\u00b7les", ",", "was", "heil\u00b7sam", ",", "was", "l\u00f6b\u00b7lich", "sich", "nen\u00b7net", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PRELS", "ADJD", "$,", "PRELS", "ADJD", "PRF", "VVFIN", "$,"], "meter": "+--+--+-+--", "measure": "dactylic.di.plus"}, "line.2": {"text": "Was sich selbst herrlich und witzig bekennet,", "tokens": ["Was", "sich", "selbst", "herr\u00b7lich", "und", "wit\u00b7zig", "be\u00b7ken\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "ADV", "ADJD", "KON", "ADJD", "VVFIN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Kumme mit Eile, den Fehler zu b\u00fcssen,", "tokens": ["Kum\u00b7me", "mit", "Ei\u00b7le", ",", "den", "Feh\u00b7ler", "zu", "b\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "Lege der F\u00fcrstin sich nieder zun F\u00fcssen.", "tokens": ["Le\u00b7ge", "der", "F\u00fcrs\u00b7tin", "sich", "nie\u00b7der", "zun", "F\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PRF", "PTKVZ", "APPRART", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.5": {"text": "Alles, was gl\u00e4ntzet, was funckelt, was strahlet,", "tokens": ["Al\u00b7les", ",", "was", "gl\u00e4nt\u00b7zet", ",", "was", "fun\u00b7ckelt", ",", "was", "strah\u00b7let", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "$,", "PWS", "VVFIN", "$,", "PWS", "VVFIN", "$,", "PWS", "VVFIN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.6": {"text": "Alles, was schm\u00fccket, was zieret, was mahlet,", "tokens": ["Al\u00b7les", ",", "was", "schm\u00fc\u00b7cket", ",", "was", "zie\u00b7ret", ",", "was", "mah\u00b7let", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "$,", "PWS", "VVFIN", "$,", "PWS", "VVFIN", "$,", "PWS", "VVFIN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.7": {"text": "Kumme mit Eile, Genade zu flehen,", "tokens": ["Kum\u00b7me", "mit", "Ei\u00b7le", ",", "Ge\u00b7na\u00b7de", "zu", "fle\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,", "NN", "PTKZU", "VVINF", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.8": {"text": "Lasse dem\u00fctig und dienstbar sich sehen.", "tokens": ["Las\u00b7se", "de\u00b7m\u00fc\u00b7tig", "und", "dienst\u00b7bar", "sich", "se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "KON", "ADJD", "PRF", "VVINF", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.9": {"text": "Sch\u00e4met euch, da\u00df ihr euch dessen ger\u00fchmet,", "tokens": ["Sch\u00e4\u00b7met", "euch", ",", "da\u00df", "ihr", "euch", "des\u00b7sen", "ge\u00b7r\u00fch\u00b7met", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "PPER", "PPER", "PDS", "VVFIN", "$,"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.10": {"text": "Was euch nicht eignet, und was sich nicht ziemet!", "tokens": ["Was", "euch", "nicht", "eig\u00b7net", ",", "und", "was", "sich", "nicht", "zie\u00b7met", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PTKNEG", "VVFIN", "$,", "KON", "PWS", "PRF", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Ey, wie so habt ihr euch sch\u00e4ndlich vergessen,", "tokens": ["Ey", ",", "wie", "so", "habt", "ihr", "euch", "sch\u00e4nd\u00b7lich", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ADV", "VAFIN", "PPER", "PPER", "ADJD", "VVPP", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.12": {"text": "Was ihr nicht waret, euch doch zu vermessen!", "tokens": ["Was", "ihr", "nicht", "wa\u00b7ret", ",", "euch", "doch", "zu", "ver\u00b7mes\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PTKNEG", "VVFIN", "$,", "PPER", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Alles, was heilsam, was l\u00f6blich zu nennen,", "tokens": ["Al\u00b7les", ",", "was", "heil\u00b7sam", ",", "was", "l\u00f6b\u00b7lich", "zu", "nen\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PRELS", "ADJD", "$,", "PRELS", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.14": {"text": "Alles, was herrlich, was witzig zu kennen,", "tokens": ["Al\u00b7les", ",", "was", "herr\u00b7lich", ",", "was", "wit\u00b7zig", "zu", "ken\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PRELS", "ADJD", "$,", "PRELS", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.15": {"text": "Hat sich an unsere Heldin verbunden;", "tokens": ["Hat", "sich", "an", "un\u00b7se\u00b7re", "Hel\u00b7din", "ver\u00b7bun\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PRF", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.16": {"text": "Anderswo wird es so tauglich nicht funden.", "tokens": ["An\u00b7ders\u00b7wo", "wird", "es", "so", "taug\u00b7lich", "nicht", "fun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADJD", "PTKNEG", "VVFIN", "$."], "meter": "---+--+--+-", "measure": "iambic.tri.relaxed"}, "line.17": {"text": "Alles, was gl\u00e4ntzet, was funckelt, was strahlet,", "tokens": ["Al\u00b7les", ",", "was", "gl\u00e4nt\u00b7zet", ",", "was", "fun\u00b7ckelt", ",", "was", "strah\u00b7let", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "$,", "PWS", "VVFIN", "$,", "PWS", "VVFIN", "$,", "PWS", "VVFIN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.18": {"text": "Alles, was schm\u00fccket, was zieret, was mahlet,", "tokens": ["Al\u00b7les", ",", "was", "schm\u00fc\u00b7cket", ",", "was", "zie\u00b7ret", ",", "was", "mah\u00b7let", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "$,", "PWS", "VVFIN", "$,", "PWS", "VVFIN", "$,", "PWS", "VVFIN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.19": {"text": "Hat sich an unsere G\u00f6ttin ergeben,", "tokens": ["Hat", "sich", "an", "un\u00b7se\u00b7re", "G\u00f6t\u00b7tin", "er\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PRF", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+--+