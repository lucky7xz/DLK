{"dta.poem.16005": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "59.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Erd' im Schwesterchor kann wohl mit ihrem Lo\u00dfe", "tokens": ["Die", "Erd'", "im", "Schwes\u00b7ter\u00b7chor", "kann", "wohl", "mit", "ih\u00b7rem", "Lo\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN", "VMFIN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zufrieden seyn, und du sei's auch in ihrem Scho\u00dfe!", "tokens": ["Zu\u00b7frie\u00b7den", "seyn", ",", "und", "du", "sei's", "auch", "in", "ih\u00b7rem", "Scho\u00b7\u00dfe", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAINF", "$,", "KON", "PPER", "VAFIN", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Gemessen ist ihr Theil nach gutem Mittelma\u00dfe,", "tokens": ["Ge\u00b7mes\u00b7sen", "ist", "ihr", "Theil", "nach", "gu\u00b7tem", "Mit\u00b7tel\u00b7ma\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie wandelt, sich zum Heil, die goldne Mittelstra\u00dfe;", "tokens": ["Sie", "wan\u00b7delt", ",", "sich", "zum", "Heil", ",", "die", "gold\u00b7ne", "Mit\u00b7tel\u00b7stra\u00b7\u00dfe", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PRF", "APPRART", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Der Sonne nicht so fern, um wie Saturn zu frieren,", "tokens": ["Der", "Son\u00b7ne", "nicht", "so", "fern", ",", "um", "wie", "Sa\u00b7turn", "zu", "frie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "ADV", "ADJD", "$,", "KOUI", "KOKOM", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Noch wie Merkur so nah, um drin sich zu verlieren.", "tokens": ["Noch", "wie", "Mer\u00b7kur", "so", "nah", ",", "um", "drin", "sich", "zu", "ver\u00b7lie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "NE", "ADV", "ADJD", "$,", "KOUI", "ADV", "PRF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Du siehst am besten auch des K\u00f6nigs Angesicht,", "tokens": ["Du", "siehst", "am", "bes\u00b7ten", "auch", "des", "K\u00f6\u00b7nigs", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKA", "ADJD", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nicht ganz und gar entfernt, doch alzu nahe nicht.", "tokens": ["Nicht", "ganz", "und", "gar", "ent\u00b7fernt", ",", "doch", "al\u00b7zu", "na\u00b7he", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "KON", "ADV", "ADJD", "$,", "ADV", "PTKA", "ADJD", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Und wenn sie mit Gefolg wie Jupiter nicht schreitet,", "tokens": ["Und", "wenn", "sie", "mit", "Ge\u00b7folg", "wie", "Ju\u00b7pi\u00b7ter", "nicht", "schrei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "NN", "KOKOM", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So geht sie auch wie Mars nicht v\u00f6llig unbegleitet.", "tokens": ["So", "geht", "sie", "auch", "wie", "Mars", "nicht", "v\u00f6l\u00b7lig", "un\u00b7be\u00b7glei\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "KOKOM", "NN", "PTKNEG", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Ihr einer treuer Mond gen\u00fcgt ihr zum Begleiter,", "tokens": ["Ihr", "ei\u00b7ner", "treu\u00b7er", "Mond", "ge\u00b7n\u00fcgt", "ihr", "zum", "Be\u00b7glei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und dir gen\u00fcgt ein Freund, du brauchest keinen weiter.", "tokens": ["Und", "dir", "ge\u00b7n\u00fcgt", "ein", "Freund", ",", "du", "brau\u00b7chest", "kei\u00b7nen", "wei\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "PIAT", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}