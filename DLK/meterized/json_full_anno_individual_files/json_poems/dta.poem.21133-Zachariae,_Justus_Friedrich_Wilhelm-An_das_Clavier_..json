{"dta.poem.21133": {"metadata": {"author": {"name": "Zachariae, Justus Friedrich Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "An das Clavier .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-20676-2", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du Zeitvertreib so mancher jungen Sch\u00f6ne,", "tokens": ["Du", "Zeit\u00b7ver\u00b7treib", "so", "man\u00b7cher", "jun\u00b7gen", "Sch\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ADV", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und manches jungen Herrn, der dir sein Opfer bringt,", "tokens": ["Und", "man\u00b7ches", "jun\u00b7gen", "Herrn", ",", "der", "dir", "sein", "Op\u00b7fer", "bringt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJA", "NN", "$,", "PRELS", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wenn er, entz\u00fcckt in ungef\u00fchlte T\u00f6ne,", "tokens": ["Wenn", "er", ",", "ent\u00b7z\u00fcckt", "in", "un\u00b7ge\u00b7f\u00fchl\u00b7te", "T\u00f6\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein welsches Ach in zwanzig Tacten singt.", "tokens": ["Ein", "wel\u00b7sches", "Ach", "in", "zwan\u00b7zig", "Tac\u00b7ten", "singt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "CARD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Auf dir war nie ein welsches Lied erklungen,", "tokens": ["Auf", "dir", "war", "nie", "ein", "wel\u00b7sches", "Lied", "er\u00b7klun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "ADV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du warst noch von dem Tand der ewgen Triller frey.", "tokens": ["Du", "warst", "noch", "von", "dem", "Tand", "der", "ew\u00b7gen", "Tril\u00b7ler", "frey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "ART", "NN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das, was ich sang, ward immer deutsch gesungen;", "tokens": ["Das", ",", "was", "ich", "sang", ",", "ward", "im\u00b7mer", "deutsch", "ge\u00b7sun\u00b7gen", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PWS", "PPER", "VVFIN", "$,", "VAFIN", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Doch mein Geschmack bleibt dir nicht mehr so treu.", "tokens": ["Doch", "mein", "Ge\u00b7schmack", "bleibt", "dir", "nicht", "mehr", "so", "treu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PPER", "PTKNEG", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Dir hat der Schwung der Oper schon gefallen.", "tokens": ["Dir", "hat", "der", "Schwung", "der", "O\u00b7per", "schon", "ge\u00b7fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es fesselt dich nicht mehr der deutschen Tonkunst Zwang.", "tokens": ["Es", "fes\u00b7selt", "dich", "nicht", "mehr", "der", "deut\u00b7schen", "Ton\u00b7kunst", "Zwang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "ADV", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Du f\u00e4ngst schon an, die Triller nachzulallen,", "tokens": ["Du", "f\u00e4ngst", "schon", "an", ",", "die", "Tril\u00b7ler", "nach\u00b7zu\u00b7lal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKVZ", "$,", "ART", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die bis ins Herz die Pompeati sang.", "tokens": ["Die", "bis", "ins", "Herz", "die", "Pom\u00b7pe\u00b7a\u00b7ti", "sang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPRART", "NN", "ART", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Wie voll bist du von neuen Z\u00e4rtlichkeiten,", "tokens": ["Wie", "voll", "bist", "du", "von", "neu\u00b7en", "Z\u00e4rt\u00b7lich\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "O siegendes Clavier, da dich die Oper hebt.", "tokens": ["O", "sie\u00b7gen\u00b7des", "Cla\u00b7vier", ",", "da", "dich", "die", "O\u00b7per", "hebt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Symphonie rauscht schon durch deine Saiten;", "tokens": ["Die", "Sym\u00b7pho\u00b7nie", "rauscht", "schon", "durch", "dei\u00b7ne", "Sai\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Unmuth flieht, und alles ist belebt.", "tokens": ["Der", "Un\u00b7muth", "flieht", ",", "und", "al\u00b7les", "ist", "be\u00b7lebt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "KON", "PIS", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}