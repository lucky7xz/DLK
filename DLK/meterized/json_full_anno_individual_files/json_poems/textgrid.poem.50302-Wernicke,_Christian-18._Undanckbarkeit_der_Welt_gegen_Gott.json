{"textgrid.poem.50302": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "18. Undanckbarkeit der Welt gegen Gott", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es ist das Ungemach, der Kummer, und die Pein,", "tokens": ["Es", "ist", "das", "Un\u00b7ge\u00b7mach", ",", "der", "Kum\u00b7mer", ",", "und", "die", "Pein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "ART", "NN", "$,", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die uns der Himmel hier abmisst, ", "tokens": ["Die", "uns", "der", "Him\u00b7mel", "hier", "ab\u00b7misst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die Freud' hergegen ist, die uns ins Gl\u00fcckes Schoss", "tokens": ["Die", "Freud'", "her\u00b7ge\u00b7gen", "ist", ",", "die", "uns", "ins", "Gl\u00fc\u00b7ckes", "Schoss"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVPP", "VAFIN", "$,", "PRELS", "PPER", "APPRART", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Des h\u00f6chsten milde Hand austheilt, ", "tokens": ["Des", "h\u00f6chs\u00b7ten", "mil\u00b7de", "Hand", "aust\u00b7heilt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wir aber stimmen schlecht mit Gottes Gutthat ein,", "tokens": ["Wir", "a\u00b7ber", "stim\u00b7men", "schlecht", "mit", "Got\