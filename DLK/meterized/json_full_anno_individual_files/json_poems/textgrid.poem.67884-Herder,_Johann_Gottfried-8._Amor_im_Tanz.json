{"textgrid.poem.67884": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "8. Amor im Tanz", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Junges Volk, man rufet euch", "tokens": ["Jun\u00b7ges", "Volk", ",", "man", "ru\u00b7fet", "euch"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PIS", "VVFIN", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Zu dem Tanz hervor.", "tokens": ["Zu", "dem", "Tanz", "her\u00b7vor", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Auf! es spielet schon zugleich", "tokens": ["Auf", "!", "es", "spie\u00b7let", "schon", "zu\u00b7gleich"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "$.", "PPER", "VVFIN", "ADV", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Unser ganzes Chor.", "tokens": ["Un\u00b7ser", "gan\u00b7zes", "Chor", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Wer nun Lust zu tanzen hat", "tokens": ["Wer", "nun", "Lust", "zu", "tan\u00b7zen", "hat"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "NN", "PTKZU", "VVINF", "VAFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Stelle hier sich ein,", "tokens": ["Stel\u00b7le", "hier", "sich", "ein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PRF", "PTKVZ", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Tanze, bis er Tanzes satt,", "tokens": ["Tan\u00b7ze", ",", "bis", "er", "Tan\u00b7zes", "satt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Und begn\u00fcgt mag seyn.", "tokens": ["Und", "be\u00b7gn\u00fcgt", "mag", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "VMFIN", "VAINF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Wisset aber, da\u00df sich hab'", "tokens": ["Wis\u00b7set", "a\u00b7ber", ",", "da\u00df", "sich", "hab'"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ADV", "$,", "KOUS", "PRF", "VAFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Hier auch eingestellt", "tokens": ["Hier", "auch", "ein\u00b7ge\u00b7stellt"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADV", "VVPP"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Amor, der ber\u00fchmte Knab'", "tokens": ["A\u00b7mor", ",", "der", "be\u00b7r\u00fchm\u00b7te", "Knab'"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf der weiten Welt:", "tokens": ["Auf", "der", "wei\u00b7ten", "Welt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Amor, der viel Possen macht,", "tokens": ["A\u00b7mor", ",", "der", "viel", "Pos\u00b7sen", "macht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Und sich nur ergetzt,", "tokens": ["Und", "sich", "nur", "er\u00b7getzt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "ADV", "VVPP", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Wenn er euch in Leid gebracht", "tokens": ["Wenn", "er", "euch", "in", "Leid", "ge\u00b7bracht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "APPR", "NN", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Und in Noth gesetzt.", "tokens": ["Und", "in", "Noth", "ge\u00b7setzt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Er wird wanken hin und her,", "tokens": ["Er", "wird", "wan\u00b7ken", "hin", "und", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVFIN", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-++-+-+", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Nehmet seiner wahr!", "tokens": ["Neh\u00b7met", "sei\u00b7ner", "wahr", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "In den Augen ohngef\u00e4hr", "tokens": ["In", "den", "Au\u00b7gen", "ohn\u00b7ge\u00b7f\u00e4hr"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wird er offenbar,", "tokens": ["Wird", "er", "of\u00b7fen\u00b7bar", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Drinnen der geschwinde Sch\u00fctz", "tokens": ["Drin\u00b7nen", "der", "ge\u00b7schwin\u00b7de", "Sch\u00fctz"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Seinen Bogen spannt", "tokens": ["Sei\u00b7nen", "Bo\u00b7gen", "spannt"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Und euch, wie der schnelle Blitz", "tokens": ["Und", "euch", ",", "wie", "der", "schnel\u00b7le", "Blitz"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "$,", "PWAV", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Trifft gar unbekannt.", "tokens": ["Trifft", "gar", "un\u00b7be\u00b7kannt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Auf den Lippen wird er oft", "tokens": ["Auf", "den", "Lip\u00b7pen", "wird", "er", "oft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VAFIN", "PPER", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Auch zu finden seyn,", "tokens": ["Auch", "zu", "fin\u00b7den", "seyn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKZU", "VVINF", "VAINF", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Und sich bei euch unverhofft", "tokens": ["Und", "sich", "bei", "euch", "un\u00b7ver\u00b7hofft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PRF", "APPR", "PPER", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Heimlich schleichen ein.", "tokens": ["Heim\u00b7lich", "schlei\u00b7chen", "ein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Durch der Worte S\u00fc\u00dfigkeit", "tokens": ["Durch", "der", "Wor\u00b7te", "S\u00fc\u00b7\u00dfig\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Hat er seine Lust,", "tokens": ["Hat", "er", "sei\u00b7ne", "Lust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Euch zu st\u00fcrzen nur in Leid", "tokens": ["Euch", "zu", "st\u00fcr\u00b7zen", "nur", "in", "Leid"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "PTKZU", "VVINF", "ADV", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Schlau und unbewust.", "tokens": ["Schlau", "und", "un\u00b7be\u00b7wust", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "H\u00e4ndedr\u00fccken keiner trau!", "tokens": ["H\u00e4n\u00b7de\u00b7dr\u00fc\u00b7cken", "kei\u00b7ner", "trau", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Er ists, der es thut:", "tokens": ["Er", "ists", ",", "der", "es", "thut", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Er verbirgt sich so genau,", "tokens": ["Er", "ver\u00b7birgt", "sich", "so", "ge\u00b7nau", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Qu\u00e4let manches Blut,", "tokens": ["Qu\u00e4\u00b7let", "man\u00b7ches", "Blut", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Da\u00df in Hoffnung wird gef\u00fchrt", "tokens": ["Da\u00df", "in", "Hoff\u00b7nung", "wird", "ge\u00b7f\u00fchrt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "NN", "VAFIN", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Einer Sch\u00f6nen Gunst,", "tokens": ["Ei\u00b7ner", "Sch\u00f6\u00b7nen", "Gunst", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Die doch nicht die Hand ger\u00fchrt \u2013", "tokens": ["Die", "doch", "nicht", "die", "Hand", "ge\u00b7r\u00fchrt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PTKNEG", "ART", "NN", "VVPP", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Es war Amors Kunst.", "tokens": ["Es", "war", "A\u00b7mors", "Kunst", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NE", "NN", "$."], "meter": "--+-+", "measure": "anapaest.init"}}, "stanza.6": {"line.1": {"text": "So er nun durch seine Pfeil'", "tokens": ["So", "er", "nun", "durch", "sei\u00b7ne", "Pfeil'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Euch verliebt gemacht,", "tokens": ["Euch", "ver\u00b7liebt", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVPP", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Wird er lachen und in Eil", "tokens": ["Wird", "er", "la\u00b7chen", "und", "in", "Eil"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "VVINF", "KON", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Geben gute Nacht;", "tokens": ["Ge\u00b7ben", "gu\u00b7te", "Nacht", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Sehet zu wie? wo? und wann", "tokens": ["Se\u00b7het", "zu", "wie", "?", "wo", "?", "und", "wann"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["VVFIN", "APPR", "PWAV", "$.", "PWAV", "$.", "KON", "PWAV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ihr dann H\u00fclfe kriegt?", "tokens": ["Ihr", "dann", "H\u00fcl\u00b7fe", "kriegt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Der wird \u00fcbel seyn daran,", "tokens": ["Der", "wird", "\u00fc\u00b7bel", "seyn", "da\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADJD", "VAINF", "PAV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Der verwundet liegt.", "tokens": ["Der", "ver\u00b7wun\u00b7det", "liegt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "VVPP", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "Junges Volk, man rufet euch", "tokens": ["Jun\u00b7ges", "Volk", ",", "man", "ru\u00b7fet", "euch"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PIS", "VVFIN", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Zu dem Tanz hervor.", "tokens": ["Zu", "dem", "Tanz", "her\u00b7vor", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Auf! es spielet schon zugleich", "tokens": ["Auf", "!", "es", "spie\u00b7let", "schon", "zu\u00b7gleich"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "$.", "PPER", "VVFIN", "ADV", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line