sure": "trochaic.tri"}, "line.3": {"text": "In den Augen ohngef\u00e4hr", "tokens": ["In", "den", "Au\u00b7gen", "ohn\u00b7ge\u00b7f\u00e4hr"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wird er offenbar,", "tokens": ["Wird", "er", "of\u00b7fen\u00b7bar", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Drinnen der geschwinde Sch\u00fctz", "tokens": ["Drin\u00b7nen", "der", "ge\u00b7schwin\u00b7de", "Sch\u00fctz"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Seinen Bogen spannt", "tokens": ["Sei\u00b7nen", "Bo\u00b7gen", "spannt"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Und euch, wie der schnelle Blitz", "tokens": ["Und", "euch", ",", "wie", "der", "schnel\u00b7le", "Blitz"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "$,", "PWAV", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Trifft gar unbekannt.", "tokens": ["Trifft", "gar", "un\u00b7be\u00b7kannt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.10": {"line.1": {"text": "Auf den Lippen wird er oft", "tokens": ["Auf", "den", "Lip\u00b7pen", "wird", "er", "oft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VAFIN", "PPER", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Auch zu finden seyn,", "tokens": ["Auch", "zu", "fin\u00b7den", "seyn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKZU", "VVINF", "VAINF", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Und sich bei euch unverhofft", "tokens": ["Und", "sich", "bei", "euch", "un\u00b7ver\u00b7hofft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PRF", "APPR", "PPER", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Heimlich schleichen ein.", "tokens": ["Heim\u00b7lich", "schlei\u00b7chen", "ein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Durch der Worte S\u00fc\u00dfigkeit", "tokens": ["Durch", "der", "Wor\u00b7te", "S\u00fc\u00b7\u00dfig\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Hat er seine Lust,", "tokens": ["Hat", "er", "sei\u00b7ne", "Lust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Euch zu st\u00fcrzen nur in Leid", "tokens": ["Euch", "zu", "st\u00fcr\u00b7zen", "nur", "in", "Leid"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "PTKZU", "VVINF", "ADV", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Schlau und unbewust.", "tokens": ["Schlau", "und", "un\u00b7be\u00b7wust", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.11": {"line.1": {"text": "H\u00e4ndedr\u00fccken keiner trau!", "tokens": ["H\u00e4n\u00b7de\u00b7dr\u00fc\u00b7cken", "kei\u00b7ner", "trau", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Er ists, der es thut:", "tokens": ["Er", "ists", ",", "der", "es", "thut", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Er verbirgt sich so genau,", "tokens": ["Er", "ver\u00b7birgt", "sich", "so", "ge\u00b7nau", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Qu\u00e4let manches Blut,", "tokens": ["Qu\u00e4\u00b7let", "man\u00b7ches", "Blut", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Da\u00df in Hoffnung wird gef\u00fchrt", "tokens": ["Da\u00df", "in", "Hoff\u00b7nung", "wird", "ge\u00b7f\u00fchrt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "NN", "VAFIN", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Einer Sch\u00f6nen Gunst,", "tokens": ["Ei\u00b7ner", "Sch\u00f6\u00b7nen", "Gunst", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Die doch nicht die Hand ger\u00fchrt \u2013", "tokens": ["Die", "doch", "nicht", "die", "Hand", "ge\u00b7r\u00fchrt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PTKNEG", "ART", "NN", "VVPP", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Es war Amors Kunst.", "tokens": ["Es", "war", "A\u00b7mors", "Kunst", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NE", "NN", "$."], "meter": "--+-+", "measure": "anapaest.init"}}, "stanza.12": {"line.1": {"text": "So er nun durch seine Pfeil'", "tokens": ["So", "er", "nun", "durch", "sei\u00b7ne", "Pfeil'"], "toke