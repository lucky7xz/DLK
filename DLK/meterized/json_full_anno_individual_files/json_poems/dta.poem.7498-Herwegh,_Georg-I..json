{"dta.poem.7498": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "I.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192430", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So hat ein Purpur wieder fallen m\u00fcssen!", "tokens": ["So", "hat", "ein", "Pur\u00b7pur", "wie\u00b7der", "fal\u00b7len", "m\u00fcs\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hast eine Krone wiederum geraubt!", "tokens": ["Hast", "ei\u00b7ne", "Kro\u00b7ne", "wie\u00b7de\u00b7rum", "ge\u00b7raubt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Du schonst die Schlangen zwischen Deinen F\u00fc\u00dfen", "tokens": ["Du", "schonst", "die", "Schlan\u00b7gen", "zwi\u00b7schen", "Dei\u00b7nen", "F\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und trittst den jungen Adlern auf das Haubt!", "tokens": ["Und", "trittst", "den", "jun\u00b7gen", "Ad\u00b7lern", "auf", "das", "Haubt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Du l\u00e4\u00dft die Sterne von dem Himmel sinken", "tokens": ["Du", "l\u00e4\u00dft", "die", "Ster\u00b7ne", "von", "dem", "Him\u00b7mel", "sin\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und Flittergold an Deinem Mantel blinken!", "tokens": ["Und", "Flit\u00b7ter\u00b7gold", "an", "Dei\u00b7nem", "Man\u00b7tel", "blin\u00b7ken", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Sprich, Schicksal, sprich, was hast Du diesen Tempel", "tokens": ["Sprich", ",", "Schick\u00b7sal", ",", "sprich", ",", "was", "hast", "Du", "die\u00b7sen", "Tem\u00b7pel"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "$,", "NN", "$,", "ADJD", "$,", "PWS", "VAFIN", "PPER", "PDAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "So fr\u00fch in Schutt und Asche hingelegt?", "tokens": ["So", "fr\u00fch", "in", "Schutt", "und", "A\u00b7sche", "hin\u00b7ge\u00b7legt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "So rein und frisch war dieser M\u00fcnze Stempel \u2014", "tokens": ["So", "rein", "und", "frisch", "war", "die\u00b7ser", "M\u00fcn\u00b7ze", "Stem\u00b7pel"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "VAFIN", "PDAT", "NN", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Was hast Du heute schon sie umgepr\u00e4gt?", "tokens": ["Was", "hast", "Du", "heu\u00b7te", "schon", "sie", "um\u00b7ge\u00b7pr\u00e4gt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "ADV", "ADV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "O theurer, als im goldenen Pokale", "tokens": ["O", "theu\u00b7rer", ",", "als", "im", "gol\u00b7de\u00b7nen", "Po\u00b7ka\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "ADJD", "$,", "KOUS", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Einst jene Perle der Kleopatra,", "tokens": ["Einst", "je\u00b7ne", "Per\u00b7le", "der", "Kleo\u00b7pat\u00b7ra", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Lag eine Perle in dem Haubte da;", "tokens": ["Lag", "ei\u00b7ne", "Per\u00b7le", "in", "dem", "Haub\u00b7te", "da", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Der M\u00f6rder Tod schlich n\u00e4chtlich sich in's Haus,", "tokens": ["Der", "M\u00f6r\u00b7der", "Tod", "schlich", "n\u00e4cht\u00b7lich", "sich", "in's", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADJD", "ADJD", "PRF", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Der rohe Knecht zerbrach die zarte Schale", "tokens": ["Der", "ro\u00b7he", "Knecht", "zer\u00b7brach", "die", "zar\u00b7te", "Scha\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Und go\u00df den hellen Geist als Opfer aus. \u2014", "tokens": ["Und", "go\u00df", "den", "hel\u00b7len", "Geist", "als", "Op\u00b7fer", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "KOUS", "NN", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Mein B\u00fcchner tot! Ihr habt mein Herz begraben!", "tokens": ["Mein", "B\u00fcch\u00b7ner", "tot", "!", "Ihr", "habt", "mein", "Herz", "be\u00b7gra\u00b7ben", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "$.", "PPER", "VAFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mein B\u00fcchner tot, als seine Hand schon offen,", "tokens": ["Mein", "B\u00fcch\u00b7ner", "tot", ",", "als", "sei\u00b7ne", "Hand", "schon", "of\u00b7fen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "$,", "KOUS", "PPOSAT", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und als ein Volk schon harrete der Gaben,", "tokens": ["Und", "als", "ein", "Volk", "schon", "har\u00b7re\u00b7te", "der", "Ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da wird der F\u00fcrst von j\u00e4hem Schlag getroffen;", "tokens": ["Da", "wird", "der", "F\u00fcrst", "von", "j\u00e4\u00b7hem", "Schlag", "ge\u00b7trof\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Der Jugend fehlt ein F\u00fchrer in die Schlacht,", "tokens": ["Der", "Ju\u00b7gend", "fehlt", "ein", "F\u00fch\u00b7rer", "in", "die", "Schlacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Um einen Fr\u00fchling ist die Welt gebracht;", "tokens": ["Um", "ei\u00b7nen", "Fr\u00fch\u00b7ling", "ist", "die", "Welt", "ge\u00b7bracht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Die Glocke, die im Sturm so rein geklungen,", "tokens": ["Die", "Glo\u00b7cke", ",", "die", "im", "Sturm", "so", "rein", "ge\u00b7klun\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPRART", "NN", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Ist, da sie Frieden lauten wollt', zersprungen.", "tokens": ["Ist", ",", "da", "sie", "Frie\u00b7den", "lau\u00b7ten", "wollt'", ",", "zer\u00b7sprun\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "$,", "KOUS", "PPER", "NN", "VVINF", "VMFIN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wer weint mit mir? Nein, \u2014 Ihr begreift es nicht,", "tokens": ["Wer", "weint", "mit", "mir", "?", "Nein", ",", "Ihr", "be\u00b7greift", "es", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPR", "PPER", "$.", "PTKANT", "$,", "$(", "PPER", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wie zehnfach stets das Herz des Dichters bricht,", "tokens": ["Wie", "zehn\u00b7fach", "stets", "das", "Herz", "des", "Dich\u00b7ters", "bricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ADV", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wie blutend, gleich der Sonne, nur sich rei\u00dft", "tokens": ["Wie", "blu\u00b7tend", ",", "gleich", "der", "Son\u00b7ne", ",", "nur", "sich", "rei\u00dft"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "VVPP", "$,", "ADV", "ART", "NN", "$,", "ADV", "PRF", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Von dieser Erde \u2014 stets ein Dichtergeist,", "tokens": ["Von", "die\u00b7ser", "Er\u00b7de", "stets", "ein", "Dich\u00b7ter\u00b7geist", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$(", "ADV", "ART", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Wie immer, wo er von dem Leib sich l\u00f6ste;", "tokens": ["Wie", "im\u00b7mer", ",", "wo", "er", "von", "dem", "Leib", "sich", "l\u00f6s\u00b7te", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "$,", "PWAV", "PPER", "APPR", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Sein eigner Schmerz beim Scheiden war der gr\u00f6\u00dfte.", "tokens": ["Sein", "eig\u00b7ner", "Schmerz", "beim", "Schei\u00b7den", "war", "der", "gr\u00f6\u00df\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPRART", "NN", "VAFIN", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Ein Scepter kann man ruhig fallen sehn,", "tokens": ["Ein", "Scep\u00b7ter", "kann", "man", "ru\u00b7hig", "fal\u00b7len", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PIS", "ADJD", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Wenn einmal nur mit ihm die Hand gespielt,", "tokens": ["Wenn", "ein\u00b7mal", "nur", "mit", "ihm", "die", "Hand", "ge\u00b7spielt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADV", "APPR", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Von einem Weibe kann man l\u00e4chelnd gehn,", "tokens": ["Von", "ei\u00b7nem", "Wei\u00b7be", "kann", "man", "l\u00e4\u00b7chelnd", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VMFIN", "PIS", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Wenn man's nur einmal in den Armen hielt;", "tokens": ["Wenn", "man's", "nur", "ein\u00b7mal", "in", "den", "Ar\u00b7men", "hielt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Der Todesstunde Qual sind jene Schemen,", "tokens": ["Der", "To\u00b7dess\u00b7tun\u00b7de", "Qual", "sind", "je\u00b7ne", "Sche\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Die wir mit uns in unsre Grube nehmen,", "tokens": ["Die", "wir", "mit", "uns", "in", "uns\u00b7re", "Gru\u00b7be", "neh\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PPER", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Die Geister, die am Sterbebette stehn,", "tokens": ["Die", "Geis\u00b7ter", ",", "die", "am", "Ster\u00b7be\u00b7bet\u00b7te", "stehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Und uns um Leben und Gestaltung flehn,", "tokens": ["Und", "uns", "um", "Le\u00b7ben", "und", "Ge\u00b7stal\u00b7tung", "flehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Die schon die junge Morgenr\u00f6te wittern", "tokens": ["Die", "schon", "die", "jun\u00b7ge", "Mor\u00b7gen\u00b7r\u00f6\u00b7te", "wit\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Und ihrem Werden bang entgegen zittern,", "tokens": ["Und", "ih\u00b7rem", "Wer\u00b7den", "bang", "ent\u00b7ge\u00b7gen", "zit\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADJD", "PTKVZ", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Des Dichters Qual die ungeborne Welt,", "tokens": ["Des", "Dich\u00b7ters", "Qual", "die", "un\u00b7ge\u00b7bor\u00b7ne", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Der Keim, der mit der reifen Garbe f\u00e4llt.", "tokens": ["Der", "Keim", ",", "der", "mit", "der", "rei\u00b7fen", "Gar\u00b7be", "f\u00e4llt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ich will Euch an ein Dichterlager bringen.", "tokens": ["Ich", "will", "Euch", "an", "ein", "Dich\u00b7ter\u00b7la\u00b7ger", "brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Seht mit dem Tod ihn um die Zukunft ringen,", "tokens": ["Seht", "mit", "dem", "Tod", "ihn", "um", "die", "Zu\u00b7kunft", "rin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Seht seines Auges letzten Fieberstrahl,", "tokens": ["Seht", "sei\u00b7nes", "Au\u00b7ges", "letz\u00b7ten", "Fie\u00b7ber\u00b7strahl", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wie es so trunken in die Leere schaut", "tokens": ["Wie", "es", "so", "trun\u00b7ken", "in", "die", "Lee\u00b7re", "schaut"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "ADJD", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und drein noch sterbend Paradiese baut!", "tokens": ["Und", "drein", "noch", "ster\u00b7bend", "Pa\u00b7ra\u00b7die\u00b7se", "baut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Die Hand zuckt nach der Stirne noch einmal,", "tokens": ["Die", "Hand", "zuckt", "nach", "der", "Stir\u00b7ne", "noch", "ein\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Das Herz pocht wilder an die schwachen Rippen,", "tokens": ["Das", "Herz", "pocht", "wil\u00b7der", "an", "die", "schwa\u00b7chen", "Rip\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJA", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Das Zauberwort schwebt auf den blassen Lippen \u2014", "tokens": ["Das", "Zau\u00b7ber\u00b7wort", "schwebt", "auf", "den", "blas\u00b7sen", "Lip\u00b7pen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Noch Ein Geheimni\u00df m\u00f6cht' er uns entdecken,", "tokens": ["Noch", "Ein", "Ge\u00b7heim\u00b7ni\u00df", "m\u00f6cht'", "er", "uns", "ent\u00b7de\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Den letzten, gr\u00f6\u00dften Traum in's Dasein wecken. \u2014", "tokens": ["Den", "letz\u00b7ten", ",", "gr\u00f6\u00df\u00b7ten", "Traum", "in's", "Da\u00b7sein", "we\u00b7cken", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "APPRART", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "O Herr des Himmels, sei ihm jetzt nicht taub!", "tokens": ["O", "Herr", "des", "Him\u00b7mels", ",", "sei", "ihm", "jetzt", "nicht", "taub", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ART", "NN", "$,", "VAFIN", "PPER", "ADV", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Noch eine Stunde g\u00f6nn' ihm, o Geschick!", "tokens": ["Noch", "ei\u00b7ne", "Stun\u00b7de", "g\u00f6nn'", "ihm", ",", "o", "Ge\u00b7schick", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "PPER", "$,", "FM", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Verl\u00f6sche uns nicht des Profeten Blick!", "tokens": ["Ver\u00b7l\u00f6\u00b7sche", "uns", "nicht", "des", "Pro\u00b7fe\u00b7ten", "Blick", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.14": {"text": "Umsonst \u2014 es bricht die m\u00fcde Brust in Staub,", "tokens": ["Um\u00b7sonst", "es", "bricht", "die", "m\u00fc\u00b7de", "Brust", "in", "Staub", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "PPER", "VVFIN", "ART", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.15": {"text": "Und mit ihr wieder eine Freiheitsst\u00fctze,", "tokens": ["Und", "mit", "ihr", "wie\u00b7der", "ei\u00b7ne", "Frei\u00b7heits\u00b7st\u00fct\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Auf's st