{"textgrid.poem.41041": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Gefangenschaft", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Endlos blauer Himmel, senktest du in mich nur einen Strahl!", "tokens": ["End\u00b7los", "blau\u00b7er", "Him\u00b7mel", ",", "senk\u00b7test", "du", "in", "mich", "nur", "ei\u00b7nen", "Strahl", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,", "VVFIN", "PPER", "APPR", "PPER", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Doch es bleibt mein Herz verschlossen, ach! von Schl\u00f6ssern ohne Zahl!", "tokens": ["Doch", "es", "bleibt", "mein", "Herz", "ver\u00b7schlos\u00b7sen", ",", "ach", "!", "von", "Schl\u00f6s\u00b7sern", "oh\u00b7ne", "Zahl", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "NN", "VVPP", "$,", "ITJ", "$.", "APPR", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Und in dieses Herz versenket bin ich wie in einen Schacht,", "tokens": ["Und", "in", "die\u00b7ses", "Herz", "ver\u00b7sen\u00b7ket", "bin", "ich", "wie", "in", "ei\u00b7nen", "Schacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PDAT", "NN", "VVPP", "VAFIN", "PPER", "KOKOM", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "F\u00fchle nur ein schmerzlich Pochen, h\u00f6r' es in der stillen Nacht,", "tokens": ["F\u00fch\u00b7le", "nur", "ein", "schmerz\u00b7lich", "Po\u00b7chen", ",", "h\u00f6r'", "es", "in", "der", "stil\u00b7len", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "ADJD", "NN", "$,", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.5": {"text": "Immer st\u00e4rker t\u00f6nt der Hammer, bis die Wand des Schachtes bricht,", "tokens": ["Im\u00b7mer", "st\u00e4r\u00b7ker", "t\u00f6nt", "der", "Ham\u00b7mer", ",", "bis", "die", "Wand", "des", "Schach\u00b7tes", "bricht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "ART", "NN", "$,", "KOUS", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.6": {"text": "Dann willkommen, blauer Himmel! der mir ward hier innen nicht!", "tokens": ["Dann", "will\u00b7kom\u00b7men", ",", "blau\u00b7er", "Him\u00b7mel", "!", "der", "mir", "ward", "hier", "in\u00b7nen", "nicht", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADJA", "NN", "$.", "ART", "PPER", "VAFIN", "ADV", "ADV", "PTKNEG", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.2": {"line.1": {"text": "Endlos blauer Himmel, senktest du in mich nur einen Strahl!", "tokens": ["End\u00b7los", "blau\u00b7er", "Him\u00b7mel", ",", "senk\u00b7test", "du", "in", "mich", "nur", "ei\u00b7nen", "Strahl", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,", "VVFIN", "PPER", "APPR", "PPER", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Doch es bleibt mein Herz verschlossen, ach! von Schl\u00f