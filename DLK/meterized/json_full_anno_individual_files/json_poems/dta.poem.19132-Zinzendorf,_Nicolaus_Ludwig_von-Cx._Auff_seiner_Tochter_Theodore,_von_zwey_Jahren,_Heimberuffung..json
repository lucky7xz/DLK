{"dta.poem.19132": {"metadata": {"author": {"name": "Zinzendorf, Nicolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "Cx.  Auff seiner Tochter Theodore, von zwey  \n Jahren, Heimberuffung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20688-6", "language": ["de:0.99"], "booktitle": "Zinzendorf, Nicolaus Ludwig von: Teutscher Gedichte Erster Theil. Herrnhuth, 1735."}, "poem": {"stanza.1": {"line.1": {"text": "Hertz der g\u00f6ttlichen Natur,", "tokens": ["Hertz", "der", "g\u00f6tt\u00b7li\u00b7chen", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Hertz der offenbarten Liebe,", "tokens": ["Hertz", "der", "of\u00b7fen\u00b7bar\u00b7ten", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Hertz der Triebe,", "tokens": ["Hertz", "der", "Trie\u00b7be", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Meine Seele opffert dir", "tokens": ["Mei\u00b7ne", "See\u00b7le", "opf\u00b7fert", "dir"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Diese hier,", "tokens": ["Die\u00b7se", "hier", ","], "token_info": ["word", "word", "punct"], "pos": ["PDS", "ADV", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Und im brennenden Verlangen", "tokens": ["Und", "im", "bren\u00b7nen\u00b7den", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Deine Salbung zu empfangen,", "tokens": ["Dei\u00b7ne", "Sal\u00b7bung", "zu", "emp\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Oeffnet sich des Geistes Th\u00fcr.", "tokens": ["Oeff\u00b7net", "sich", "des", "Geis\u00b7tes", "Th\u00fcr."], "token_info": ["word", "word", "word", "word", "abbreviation"], "pos": ["VVFIN", "PRF", "ART", "NN", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Dieses war des Glaubens-Wort,", "tokens": ["Die\u00b7ses", "war", "des", "Glau\u00b7bens\u00b7Wort", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Welches meiner Tochter Seele", "tokens": ["Wel\u00b7ches", "mei\u00b7ner", "Toch\u00b7ter", "See\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Aus der H\u00f6hle,", "tokens": ["Aus", "der", "H\u00f6h\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "--+-", "measure": "anapaest.init"}, "line.4": {"text": "Und aus alle ihrem Drang,", "tokens": ["Und", "aus", "al\u00b7le", "ih\u00b7rem", "Drang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIAT", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Auffw\u00e4rts schwang;", "tokens": ["Auf\u00b7fw\u00e4rts", "schwang", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Dieser Stimme stilles Th\u00f6nen,", "tokens": ["Die\u00b7ser", "Stim\u00b7me", "stil\u00b7les", "Th\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und der Theodore Kr\u00f6nen,", "tokens": ["Und", "der", "Theo\u00b7do\u00b7re", "Kr\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Waren ein Zusammenhang.", "tokens": ["Wa\u00b7ren", "ein", "Zu\u00b7sam\u00b7men\u00b7hang", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.3": {"line.1": {"text": "Theurer Heyland! sage mir:", "tokens": ["Theu\u00b7rer", "Hey\u00b7land", "!", "sa\u00b7ge", "mir", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "NN", "$.", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie gerath ich arme Made", "tokens": ["Wie", "ge\u00b7rath", "ich", "ar\u00b7me", "Ma\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "PPER", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Zu der Gnade,", "tokens": ["Zu", "der", "Gna\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Da\u00df du meiner Kinder Last", "tokens": ["Da\u00df", "du", "mei\u00b7ner", "Kin\u00b7der", "Last"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Selbst erfa\u00dft?", "tokens": ["Selbst", "er\u00b7fa\u00dft", "?"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVPP", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Denn sie kan so bald nicht dr\u00fccken,", "tokens": ["Denn", "sie", "kan", "so", "bald", "nicht", "dr\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "ADV", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "So befreyst du meinen R\u00fccken,", "tokens": ["So", "be\u00b7freyst", "du", "mei\u00b7nen", "R\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Den du sonst beladen hast.", "tokens": ["Den", "du", "sonst", "be\u00b7la\u00b7den", "hast", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Theodora Charitas", "tokens": ["Theo\u00b7do\u00b7ra", "Cha\u00b7ri\u00b7tas"], "token_info": ["word", "word"], "pos": ["NE", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "War zwar eins der ungemeinen", "tokens": ["War", "zwar", "eins", "der", "un\u00b7ge\u00b7mei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "PIS", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Edlen Kleinen,", "tokens": ["Ed\u00b7len", "Klei\u00b7nen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Jhrer H\u00fctte engen Raum", "tokens": ["Ih\u00b7rer", "H\u00fct\u00b7te", "en\u00b7gen", "Raum"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Merckt man kaum,", "tokens": ["Merckt", "man", "kaum", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Und ihr Kinder-Sinn und Wille", "tokens": ["Und", "ihr", "Kin\u00b7der\u00b7Sinn", "und", "Wil\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Reget sich in solcher Stille,", "tokens": ["Re\u00b7get", "sich", "in", "sol\u00b7cher", "Stil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Da\u00df man denckt, es sey ein Traum.", "tokens": ["Da\u00df", "man", "denckt", ",", "es", "sey", "ein", "Traum", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "$,", "PPER", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Eben drum, du theures Hertz!", "tokens": ["E\u00b7ben", "drum", ",", "du", "theu\u00b7res", "Hertz", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PAV", "$,", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Spricht der Hirt der kleinen Schafe:", "tokens": ["Spricht", "der", "Hirt", "der", "klei\u00b7nen", "Scha\u00b7fe", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dorel schlafe;", "tokens": ["Do\u00b7rel", "schla\u00b7fe", ";"], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Weil es ewig Schade w\u00e4r,", "tokens": ["Weil", "es", "e\u00b7wig", "Scha\u00b7de", "w\u00e4r", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "ADJD", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wenn die Ehr", "tokens": ["Wenn", "die", "Ehr"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "ART", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Einer unbefleckten Seele,", "tokens": ["Ei\u00b7ner", "un\u00b7be\u00b7fleck\u00b7ten", "See\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Uber der Gefahr der H\u00f6le,", "tokens": ["U\u00b7ber", "der", "Ge\u00b7fahr", "der", "H\u00f6\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Sich ein einigs mal verl\u00f6hr.", "tokens": ["Sich", "ein", "ei\u00b7nigs", "mal", "ver\u00b7l\u00f6hr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "PIS", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Falle hurtig, vierdtes Loo\u00df,", "tokens": ["Fal\u00b7le", "hur\u00b7tig", ",", "vierd\u00b7tes", "Loo\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADJD", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "So mir lieblich ausgefallen;", "tokens": ["So", "mir", "lieb\u00b7lich", "aus\u00b7ge\u00b7fal\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Unter allen", "tokens": ["Un\u00b7ter", "al\u00b7len"], "token_info": ["word", "word"], "pos": ["APPR", "PIAT"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Findest du das sch\u00f6nste Theil.", "tokens": ["Fin\u00b7dest", "du", "das", "sch\u00f6ns\u00b7te", "Theil", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Fahr in Eil,", "tokens": ["Fahr", "in", "Eil", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Und bleib im zerspaltnen Hertzen", "tokens": ["Und", "bleib", "im", "zer\u00b7spalt\u00b7nen", "Hert\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Des verkl\u00e4rten Manns der Schmertzen,", "tokens": ["Des", "ver\u00b7kl\u00e4r\u00b7ten", "Manns", "der", "Schmert\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Stecken als ein reiner Pfeil!", "tokens": ["Ste\u00b7cken", "als", "ein", "rei\u00b7ner", "Pfeil", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "H\u00f6rst du deines Vaters Rath;", "tokens": ["H\u00f6rst", "du", "dei\u00b7nes", "Va\u00b7ters", "Rath", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Oder singst du deine Lieder", "tokens": ["O\u00b7der", "singst", "du", "dei\u00b7ne", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Etwa wieder?", "tokens": ["Et\u00b7wa", "wie\u00b7der", "?"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADV", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Da\u00df dir ja der Worte Sinn", "tokens": ["Da\u00df", "dir", "ja", "der", "Wor\u00b7te", "Sinn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "NN"], "meter": "----+-+", "measure": "unknown.measure.di"}, "line.5": {"text": "Nicht entrinn!", "tokens": ["Nicht", "ent\u00b7rinn", "!"], "token_info": ["word", "word", "punct"], "pos": ["PTKNEG", "PTKVZ", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "La\u00df dich deinen K\u00f6nig k\u00fcssen;", "tokens": ["La\u00df", "dich", "dei\u00b7nen", "K\u00f6\u00b7nig", "k\u00fcs\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Will er aber sonst was wissen,", "tokens": ["Will", "er", "a\u00b7ber", "sonst", "was", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Statt der Antwort, sincke hin.", "tokens": ["Statt", "der", "Ant\u00b7wort", ",", "sin\u00b7cke", "hin", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Meine Sorg ist aus vor dich.", "tokens": ["Mei\u00b7ne", "Sorg", "ist", "aus", "vor", "dich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "APPR", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Drum, du F\u00fcrst der Seelen-Pflege,", "tokens": ["Drum", ",", "du", "F\u00fcrst", "der", "See\u00b7len\u00b7Pfle\u00b7ge", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "PPER", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Theurer Hege!", "tokens": ["Theu\u00b7rer", "He\u00b7ge", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Es erstreckt sich ja die Macht", "tokens": ["Es", "er\u00b7streckt", "sich", "ja", "die", "Macht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Meiner Wacht", "tokens": ["Mei\u00b7ner", "Wacht"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Nur auf die in H\u00fctten wohnen;", "tokens": ["Nur", "auf", "die", "in", "H\u00fct\u00b7ten", "woh\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "APPR", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Du bist H\u00fcter bey den Thronen;", "tokens": ["Du", "bist", "H\u00fc\u00b7ter", "bey", "den", "Thro\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Nimm die Dorel gut in acht!", "tokens": ["Nimm", "die", "Do\u00b7rel", "gut", "in", "acht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "ADJD", "APPR", "CARD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}