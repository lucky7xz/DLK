{"dta.poem.9888": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mein hertze brennt in heisser glut", "tokens": ["Mein", "hert\u00b7ze", "brennt", "in", "heis\u00b7ser", "glut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und wirfft die flammen dennoch nicht empor/", "tokens": ["Und", "wirfft", "die", "flam\u00b7men", "den\u00b7noch", "nicht", "em\u00b7por", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "VVFIN", "ADV", "PTKNEG", "PTKVZ", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich wei\u00df nicht/ wie mir ist zu muth/", "tokens": ["Ich", "wei\u00df", "nicht", "/", "wie", "mir", "ist", "zu", "muth", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$(", "PWAV", "PPER", "VAFIN", "APPR", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mein seuffzen bring ich nur mit schmertzen vor;", "tokens": ["Mein", "seuff\u00b7zen", "bring", "ich", "nur", "mit", "schmert\u00b7zen", "vor", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADV", "APPR", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Der augen na\u00df/ so h\u00e4uffig kommt gerannt/", "tokens": ["Der", "au\u00b7gen", "na\u00df", "/", "so", "h\u00e4uf\u00b7fig", "kommt", "ge\u00b7rannt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$(", "ADV", "ADJD", "VVFIN", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Entz\u00fcndet mehr/ als l\u00f6schet/ meinen brandt.", "tokens": ["Ent\u00b7z\u00fcn\u00b7det", "mehr", "/", "als", "l\u00f6\u00b7schet", "/", "mei\u00b7nen", "brandt", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$(", "KOKOM", "VVFIN", "$(", "PPOSAT", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Den krancken ist zu helffen leicht/", "tokens": ["Den", "kran\u00b7cken", "ist", "zu", "helf\u00b7fen", "leicht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKZU", "VVINF", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn er die schmertzen nur dem artzt bekennt/", "tokens": ["Wenn", "er", "die", "schmert\u00b7zen", "nur", "dem", "artzt", "be\u00b7kennt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "VVFIN", "ADV", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Eh ihm das gifft zum hertzen steigt/", "tokens": ["Eh", "ihm", "das", "gifft", "zum", "hert\u00b7zen", "steigt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPRART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und eh die seele sich vom leibe trennt;", "tokens": ["Und", "eh", "die", "see\u00b7le", "sich", "vom", "lei\u00b7be", "trennt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "VVFIN", "PRF", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ich aber sterbe lieber tausend mahl/", "tokens": ["Ich", "a\u00b7ber", "ster\u00b7be", "lie\u00b7ber", "tau\u00b7send", "mahl", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ADV", "CARD", "ADV", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Eh da\u00df ich solt er\u00f6ffnen meine qual.", "tokens": ["Eh", "da\u00df", "ich", "solt", "er\u00b7\u00f6ff\u00b7nen", "mei\u00b7ne", "qual", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOUS", "PPER", "VMFIN", "VVINF", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ich liebe gleichwohl meinen tod", "tokens": ["Ich", "lie\u00b7be", "gleich\u00b7wohl", "mei\u00b7nen", "tod"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und halte viel vom ursprung meiner pein;", "tokens": ["Und", "hal\u00b7te", "viel", "vom", "ur\u00b7sprung", "mei\u00b7ner", "pein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPRART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die/ so mich hat gebracht in noth/", "tokens": ["Die", "/", "so", "mich", "hat", "ge\u00b7bracht", "in", "noth", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$(", "ADV", "PPER", "VAFIN", "VVPP", "APPR", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die la\u00df ich mir doch nicht zu wider seyn.", "tokens": ["Die", "la\u00df", "ich", "mir", "doch", "nicht", "zu", "wi\u00b7der", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVIMP", "PPER", "PPER", "ADV", "PTKNEG", "PTKZU", "APPR", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Lebt in der welt ein unvergn\u00fcgter sinn/", "tokens": ["Lebt", "in", "der", "welt", "ein", "un\u00b7ver\u00b7gn\u00fcg\u00b7ter", "sinn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "F\u00fcrwahr so wei\u00df ich/ da\u00df ich solcher bin.", "tokens": ["F\u00fcr\u00b7wahr", "so", "wei\u00df", "ich", "/", "da\u00df", "ich", "sol\u00b7cher", "bin", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "$(", "KOUS", "PPER", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Es saget zwar der weisen zunfft:", "tokens": ["Es", "sa\u00b7get", "zwar", "der", "wei\u00b7sen", "zunfft", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Man soll die lust was enger spannen ein/", "tokens": ["Man", "soll", "die", "lust", "was", "en\u00b7ger", "span\u00b7nen", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "ART", "NN", "PWS", "ADJD", "VVFIN", "ART", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Damit die herrschafft der vornunfft", "tokens": ["Da\u00b7mit", "die", "herr\u00b7schafft", "der", "vor\u00b7nunfft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dem hertzen m\u00f6ge vorbehalten seyn;", "tokens": ["Dem", "hert\u00b7zen", "m\u00f6\u00b7ge", "vor\u00b7be\u00b7hal\u00b7ten", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ich f\u00fchle doch/ so fern es einer f\u00fchlt/", "tokens": ["Ich", "f\u00fch\u00b7le", "doch", "/", "so", "fern", "es", "ei\u00b7ner", "f\u00fchlt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$(", "ADV", "ADJD", "PPER", "PIS", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Da\u00df liebe nur tyrannisch meister spielt.", "tokens": ["Da\u00df", "lie\u00b7be", "nur", "ty\u00b7ran\u00b7nisch", "meis\u00b7ter", "spielt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "ADV", "ADJD", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Jhr freunde habet gute nacht/", "tokens": ["Ihr", "freun\u00b7de", "ha\u00b7bet", "gu\u00b7te", "nacht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hinf\u00fcro lieb ich nur die einsamkeit/", "tokens": ["Hin\u00b7f\u00fc\u00b7ro", "lieb", "ich", "nur", "die", "ein\u00b7sam\u00b7keit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "ADV", "ART", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ach! ach! wer h\u00e4tte di\u00df gedacht/", "tokens": ["Ach", "!", "ach", "!", "wer", "h\u00e4t\u00b7te", "di\u00df", "ge\u00b7dacht", "/"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "XY", "$.", "PWS", "VAFIN", "PDS", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df aus der lust entspringe hertzenleid", "tokens": ["Da\u00df", "aus", "der", "lust", "ent\u00b7sprin\u00b7ge", "hert\u00b7zen\u00b7leid"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ich liebe zwar/ so fern es lieben heist/", "tokens": ["Ich", "lie\u00b7be", "zwar", "/", "so", "fern", "es", "lie\u00b7ben", "heist", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$(", "ADV", "ADJD", "PPER", "VVINF", "VAFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Wenn einer irrt ohn hertze/ seel und geist.", "tokens": ["Wenn", "ei\u00b7ner", "irrt", "ohn", "hert\u00b7ze", "/", "seel", "und", "geist", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "APPR", "NN", "$(", "XY", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}