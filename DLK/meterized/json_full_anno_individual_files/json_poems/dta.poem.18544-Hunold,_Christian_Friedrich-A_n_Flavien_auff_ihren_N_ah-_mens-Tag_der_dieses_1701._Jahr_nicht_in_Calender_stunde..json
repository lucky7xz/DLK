{"dta.poem.18544": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "A n   Flavien   auff ihren   N ah-  \n mens-Tag/ der dieses 1701. Jahr  \n nicht in Calender stunde.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1702", "urn": "urn:nbn:de:kobv:b4-200905197766", "language": ["de:0.99"], "booktitle": "Hunold, Christian Friedrich: Die Edle Bem\u00fchung m\u00fcssiger Stunden. Hamburg, 1702."}, "poem": {"stanza.1": {"line.1": {"text": "Auf/ sch\u00f6nste Flavia/ ermuntre deine Sinnen/", "tokens": ["Auf", "/", "sch\u00f6ns\u00b7te", "Fla\u00b7via", "/", "er\u00b7munt\u00b7re", "dei\u00b7ne", "Sin\u00b7nen", "/"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "$(", "ADJA", "NN", "$(", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Da heute mein Gel\u00fcck und dein Ergetzen lacht.", "tokens": ["Da", "heu\u00b7te", "mein", "Ge\u00b7l\u00fcck", "und", "dein", "Er\u00b7get\u00b7zen", "lacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-++--+", "measure": "iambic.hexa.chol"}, "line.3": {"text": "La\u00df uns den frohen Tag in sch\u00f6nster Lust beginnen/", "tokens": ["La\u00df", "uns", "den", "fro\u00b7hen", "Tag", "in", "sch\u00f6ns\u00b7ter", "Lust", "be\u00b7gin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "An welchen die Natur ein Meister-St\u00fcck gemacht.", "tokens": ["An", "wel\u00b7chen", "die", "Na\u00b7tur", "ein", "Meis\u00b7ter\u00b7St\u00fcck", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der angenehme Tag soll einen Nahmen f\u00fchren/", "tokens": ["Der", "an\u00b7ge\u00b7neh\u00b7me", "Tag", "soll", "ei\u00b7nen", "Nah\u00b7men", "f\u00fch\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der dir mein Engel ist zum ersten beygelegt.", "tokens": ["Der", "dir", "mein", "En\u00b7gel", "ist", "zum", "ers\u00b7ten", "bey\u00b7ge\u00b7legt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPOSAT", "NN", "VAFIN", "APPRART", "ADJA", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und mu\u00df mein Wesen gantz von deinen Wesen r\u00fchren/", "tokens": ["Und", "mu\u00df", "mein", "We\u00b7sen", "gantz", "von", "dei\u00b7nen", "We\u00b7sen", "r\u00fch\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPOSAT", "NN", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So hat dein Nahmens-Tag auch meine Lust erregt.", "tokens": ["So", "hat", "dein", "Nah\u00b7mens\u00b7Tag", "auch", "mei\u00b7ne", "Lust", "er\u00b7regt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Zwar kan man ihn dis Jahr nicht in Calender sehen/", "tokens": ["Zwar", "kan", "man", "ihn", "dis", "Jahr", "nicht", "in", "Ca\u00b7len\u00b7der", "se\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "PPER", "PDS", "NN", "PTKNEG", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "So sieht dein sch\u00f6nes Aug' ihn doch in meiner Brust.", "tokens": ["So", "sieht", "dein", "sch\u00f6\u00b7nes", "Aug'", "ihn", "doch", "in", "mei\u00b7ner", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ach glaube nur es ist nicht ohne Grund geschehen/", "tokens": ["Ach", "glau\u00b7be", "nur", "es", "ist", "nicht", "oh\u00b7ne", "Grund", "ge\u00b7sche\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "ADV", "PPER", "VAFIN", "PTKNEG", "APPR", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Was hier das Schicksall will/ ist mir nicht unbewust.", "tokens": ["Was", "hier", "das", "Schick\u00b7sall", "will", "/", "ist", "mir", "nicht", "un\u00b7be\u00b7wust", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "NN", "VMFIN", "$(", "VAFIN", "PPER", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Der Himmel will nicht mehr dich in Calender wissen/", "tokens": ["Der", "Him\u00b7mel", "will", "nicht", "mehr", "dich", "in", "Ca\u00b7len\u00b7der", "wis\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PTKNEG", "ADV", "PPER", "APPR", "NN", "VVINF", "$("], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "Denn was ihm selber gleicht/ soll unverg\u00e4nglich seyn.", "tokens": ["Denn", "was", "ihm", "sel\u00b7ber", "gleicht", "/", "soll", "un\u00b7ver\u00b7g\u00e4ng\u00b7lich", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADV", "VVFIN", "$(", "VMFIN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Die Dinte lescht die Zeit/ Papier wird leicht zerrissen/", "tokens": ["Die", "Din\u00b7te", "lescht", "die", "Zeit", "/", "Pa\u00b7pier", "wird", "leicht", "zer\u00b7ris\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$(", "NN", "VAFIN", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Drum schreibt dich seine Hand in meinen Hertzen ein.", "tokens": ["Drum", "schreibt", "dich", "sei\u00b7ne", "Hand", "in", "mei\u00b7nen", "Hert\u00b7zen", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Er wei\u00df wol/ das mein Hertz dich ewig denckt zu lieben/", "tokens": ["Er", "wei\u00df", "wol", "/", "das", "mein", "Hertz", "dich", "e\u00b7wig", "denckt", "zu", "lie\u00b7ben", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$(", "ART", "PPOSAT", "NN", "PPER", "ADJD", "VVFIN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Weil du mit Diamant da eingezeichnet bist.", "tokens": ["Weil", "du", "mit", "Di\u00b7a\u00b7mant", "da", "ein\u00b7ge\u00b7zeich\u00b7net", "bist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Deswegen ist sein Schlu\u00df wo die Person geblieben/", "tokens": ["Des\u00b7we\u00b7gen", "ist", "sein", "Schlu\u00df", "wo", "die", "Per\u00b7son", "ge\u00b7blie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPOSAT", "NN", "PWAV", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+++-+-", "measure": "unknown.measure.septa"}, "line.20": {"text": "Da werde k\u00fcnfftig auch der Nahme nicht vermi\u00dft.", "tokens": ["Da", "wer\u00b7de", "k\u00fcnff\u00b7tig", "auch", "der", "Nah\u00b7me", "nicht", "ver\u00b7mi\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADJD", "ADV", "ART", "NN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Mit deinen Nahmen denckt sich auch dein Stand zu \u00e4ndern;", "tokens": ["Mit", "dei\u00b7nen", "Nah\u00b7men", "denckt", "sich", "auch", "dein", "Stand", "zu", "\u00e4n\u00b7dern", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PRF", "ADV", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Ich k\u00fcsse dieses Jahr dich ja als meine Braut?", "tokens": ["Ich", "k\u00fcs\u00b7se", "die\u00b7ses", "Jahr", "dich", "ja", "als", "mei\u00b7ne", "Braut", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDAT", "NN", "PPER", "ADV", "KOUS", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Jedoch jetzt f\u00e4llt mir ein/ es wird ja fremden L\u00e4ndern", "tokens": ["Je\u00b7doch", "jetzt", "f\u00e4llt", "mir", "ein", "/", "es", "wird", "ja", "frem\u00b7den", "L\u00e4n\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ART", "$(", "PPER", "VAFIN", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Zu mehrer Fruchtbarkeit ein sch\u00f6ner Zweig vertraut.", "tokens": ["Zu", "meh\u00b7rer", "Frucht\u00b7bar\u00b7keit", "ein", "sch\u00f6\u00b7ner", "Zweig", "ver\u00b7traut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ART", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Der Himmel der dich pflantzt/ will auch die Fr\u00fcchte sehen/", "tokens": ["Der", "Him\u00b7mel", "der", "dich", "pflantzt", "/", "will", "auch", "die", "Fr\u00fcch\u00b7te", "se\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "PPER", "VVFIN", "$(", "VMFIN", "ADV", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Und setzt dich in ein Land/ das dem Gelobten gleicht.", "tokens": ["Und", "setzt", "dich", "in", "ein", "Land", "/", "das", "dem", "Ge\u00b7lob\u00b7ten", "gleicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ART", "NN", "$(", "PDS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Der Liebe West wird da zu deinen Wachsthum wehen/", "tokens": ["Der", "Lie\u00b7be", "West", "wird", "da", "zu", "dei\u00b7nen", "Wach\u00b7sthum", "we\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VAFIN", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Wenn noch der Silber Mond zweymahl zur\u00fccke weicht.", "tokens": ["Wenn", "noch", "der", "Sil\u00b7ber", "Mond", "zwey\u00b7mahl", "zu\u00b7r\u00fc\u00b7cke", "weicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "NN", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Ach das doch diese Zeit nicht soll gefl\u00fcgelt werden!", "tokens": ["Ach", "das", "doch", "die\u00b7se", "Zeit", "nicht", "soll", "ge\u00b7fl\u00fc\u00b7gelt", "wer\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PDS", "ADV", "PDAT", "NN", "PTKNEG", "VMFIN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Und das du s\u00fcsser Tag nicht angebrochen bist!", "tokens": ["Und", "das", "du", "s\u00fcs\u00b7ser", "Tag", "nicht", "an\u00b7ge\u00b7bro\u00b7chen", "bist", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PPER", "ADJA", "NN", "PTKNEG", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Ich wei\u00df/ ich hab' alsdenn mein Paradie\u00df auff Erden/", "tokens": ["Ich", "wei\u00df", "/", "ich", "hab'", "als\u00b7denn", "mein", "Pa\u00b7ra\u00b7die\u00df", "auff", "Er\u00b7den", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "PPER", "VAFIN", "ADV", "PPOSAT", "NN", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Wenn meine Flavia Madame worden ist.", "tokens": ["Wenn", "mei\u00b7ne", "Fla\u00b7via", "Ma\u00b7da\u00b7me", "wor\u00b7den", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "FM", "FM", "VAPP", "VAFIN", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.33": {"text": "Indessen m\u00fcsse dich das Gl\u00fcck so vielmahls k\u00fcssen/", "tokens": ["In\u00b7des\u00b7sen", "m\u00fcs\u00b7se", "dich", "das", "Gl\u00fcck", "so", "viel\u00b7mahls", "k\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PRF", "ART", "NN", "ADV", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Als Lieb' und Treu vor dir in meinen Hertzen gr\u00fcnt;", "tokens": ["Als", "Lieb'", "und", "Treu", "vor", "dir", "in", "mei\u00b7nen", "Hert\u00b7zen", "gr\u00fcnt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "APPR", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Die Freude sey um dich/ wie ich mich m\u00f6chte wissen/", "tokens": ["Die", "Freu\u00b7de", "sey", "um", "dich", "/", "wie", "ich", "mich", "m\u00f6ch\u00b7te", "wis\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "PPER", "$(", "PWAV", "PPER", "PPER", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "So wei\u00df ich, da\u00df sie dir zu Tisch und Bette dient.", "tokens": ["So", "wei\u00df", "ich", ",", "da\u00df", "sie", "dir", "zu", "Tisch", "und", "Bet\u00b7te", "dient", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "PPER", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}