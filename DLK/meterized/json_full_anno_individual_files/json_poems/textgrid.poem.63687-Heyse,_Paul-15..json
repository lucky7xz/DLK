{"textgrid.poem.63687": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "15.", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer eines Regentags einsame Stunden", "tokens": ["Wer", "ei\u00b7nes", "Re\u00b7gen\u00b7tags", "ein\u00b7sa\u00b7me", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Uns traulich k\u00fcrzt durch freundlichen Besuch,", "tokens": ["Uns", "trau\u00b7lich", "k\u00fcrzt", "durch", "freund\u00b7li\u00b7chen", "Be\u00b7such", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sei's in Person, sei's durch ein sch\u00f6nes Buch,", "tokens": ["Sei's", "in", "Per\u00b7son", ",", "sei's", "durch", "ein", "sch\u00f6\u00b7nes", "Buch", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NE", "$,", "VAFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Der hat wohl immer dankbar uns gefunden.", "tokens": ["Der", "hat", "wohl", "im\u00b7mer", "dank\u00b7bar", "uns", "ge\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADV", "ADJD", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Du hast zu gr\u00f6\u00dferm Danke mich verbunden,", "tokens": ["Du", "hast", "zu", "gr\u00f6\u00b7\u00dferm", "Dan\u00b7ke", "mich", "ver\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ADJA", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da du mir zeigtest, teurer Wildenbruch,", "tokens": ["Da", "du", "mir", "zeig\u00b7test", ",", "teu\u00b7rer", "Wil\u00b7den\u00b7bruch", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Noch sei, trotz unsrer Heutigen Widerspruch,", "tokens": ["Noch", "sei", ",", "trotz", "uns\u00b7rer", "Heu\u00b7ti\u00b7gen", "Wi\u00b7der\u00b7spruch", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "$,", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Der B\u00fchne gro\u00dfe Kunst nicht ganz verschwunden.", "tokens": ["Der", "B\u00fch\u00b7ne", "gro\u00b7\u00dfe", "Kunst", "nicht", "ganz", "ver\u00b7schwun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "PTKNEG", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Dein edles Kind, die \u00bbNabensteinerin\u00ab,", "tokens": ["Dein", "ed\u00b7les", "Kind", ",", "die", "\u00bb", "Na\u00b7ben\u00b7stei\u00b7ne\u00b7rin", "\u00ab", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PRELS", "$(", "NN", "$(", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "So herb und hold, so r\u00fchrenden Geschickes,", "tokens": ["So", "herb", "und", "hold", ",", "so", "r\u00fch\u00b7ren\u00b7den", "Ge\u00b7schi\u00b7ckes", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "$,", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie nahm im Fluge sie so ganz mich hin", "tokens": ["Wie", "nahm", "im", "Flu\u00b7ge", "sie", "so", "ganz", "mich", "hin"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "APPRART", "NN", "PPER", "ADV", "ADV", "PPER", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Mit allem Zauber ihres keuschen Blickes!", "tokens": ["Mit", "al\u00b7lem", "Zau\u00b7ber", "ih\u00b7res", "keu\u00b7schen", "Bli\u00b7ckes", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So ward der graue Tag mir zum Gewinn.", "tokens": ["So", "ward", "der", "grau\u00b7e", "Tag", "mir", "zum", "Ge\u00b7winn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Gru\u00df dir und Dank, und freu dich deines Gl\u00fcckes!", "tokens": ["Gru\u00df", "dir", "und", "Dank", ",", "und", "freu", "dich", "dei\u00b7nes", "Gl\u00fc\u00b7ckes", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "KON", "NN", "$,", "KON", "ADJD", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Wer eines Regentags einsame Stunden", "tokens": ["Wer", "ei\u00b7nes", "Re\u00b7gen\u00b7tags", "ein\u00b7sa\u00b7me", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Uns trauli