{"textgrid.poem.40250": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich war der Herr der Welt vor dir,", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich war der Herr der Welt vor dir,", "tokens": ["Ich", "war", "der", "Herr", "der", "Welt", "vor", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "im Traum;", "tokens": ["im", "Traum", ";"], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "wie eine Sonne warst du mir,", "tokens": ["wie", "ei\u00b7ne", "Son\u00b7ne", "warst", "du", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VAFIN", "PPER", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "im Traum.", "tokens": ["im", "Traum", "."], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "Ich schm\u00fcckte dich mit allen guten", "tokens": ["Ich", "schm\u00fcck\u00b7te", "dich", "mit", "al\u00b7len", "gu\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PIAT", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Gl\u00fccksehnsuchtsgluten", "tokens": ["Gl\u00fcck\u00b7sehn\u00b7suchts\u00b7glu\u00b7ten"], "token_info": ["word"], "pos": ["NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "in diesem Traum,", "tokens": ["in", "die\u00b7sem", "Traum", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "und hie\u00df dich leuchten, lie\u00df dich schweben.", "tokens": ["und", "hie\u00df", "dich", "leuch\u00b7ten", ",", "lie\u00df", "dich", "schwe\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Und habe mich in den Staub gebogen", "tokens": ["Und", "ha\u00b7be", "mich", "in", "den", "Staub", "ge\u00b7bo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "APPR", "ART", "NN", "VVPP"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "vor dir, im Traum,", "tokens": ["vor", "dir", ",", "im", "Traum", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "APPRART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "und dich belogen und betrogen", "tokens": ["und", "dich", "be\u00b7lo\u00b7gen", "und", "be\u00b7tro\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVPP", "KON", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "im Staub, im Traum \u2013", "tokens": ["im", "Staub", ",", "im", "Traum", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPRART", "NN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.13": {"text": "komm, la\u00df uns ", "tokens": ["komm", ",", "la\u00df", "uns"], "token_info": ["word", "punct", "word", "word"], "pos": ["VVFIN", "$,", "VVIMP", "PPER"], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Ich war der Herr der Welt vor dir,", "tokens": ["Ich", "war", "der", "Herr", "der", "Welt", "vor", "dir", ","], "token_i