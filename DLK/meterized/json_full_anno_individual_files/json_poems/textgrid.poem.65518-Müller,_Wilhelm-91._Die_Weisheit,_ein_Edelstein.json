{"textgrid.poem.65518": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "91. Die Weisheit, ein Edelstein", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kannst die Weisheit du vergleichen einem reinen Edelstein,", "tokens": ["Kannst", "die", "Weis\u00b7heit", "du", "ver\u00b7glei\u00b7chen", "ei\u00b7nem", "rei\u00b7nen", "E\u00b7del\u00b7stein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "PPER", "VVINF", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "So begreifst du auch, weswegen Wenige sie kaufen ein.", "tokens": ["So", "be\u00b7greifst", "du", "auch", ",", "wes\u00b7we\u00b7gen", "We\u00b7ni\u00b7ge", "sie", "kau\u00b7fen", "ein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "PWAV", "VVFIN", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Dieser wei\u00df sie nicht zu sch\u00e4tzen, mi\u00dft sie mit des Kiesels Ma\u00df,", "tokens": ["Die\u00b7ser", "wei\u00df", "sie", "nicht", "zu", "sch\u00e4t\u00b7zen", ",", "mi\u00dft", "sie", "mit", "des", "Kie\u00b7sels", "Ma\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PTKNEG", "PTKZU", "VVINF", "$,", "VVFIN", "PPER", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Und, weil scheckiger es schimmert, w\u00e4hlt er buntes Flitterglas.", "tokens": ["Und", ",", "weil", "sche\u00b7cki\u00b7ger", "es", "schim\u00b7mert", ",", "w\u00e4hlt", "er", "bun\u00b7tes", "Flit\u00b7ter\u00b7glas", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "ADJD", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+--+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.5": {"text": "Jener m\u00f6chte sie wohl haben, doch sein Beutel ist nicht schwer,", "tokens": ["Je\u00b7ner", "m\u00f6ch\u00b7te", "sie", "wohl", "ha\u00b7ben", ",", "doch", "sein", "Beu\u00b7tel", "ist", "nicht", "schwer", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "VMFIN", "PPER", "ADV", "VAFIN", "$,", "ADV", "PPOSAT", "NN", "VAFIN", "PTKNEG", "ADJD", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.6": {"text": "Und ein blindes Huhn nur findet sie in Spreu von ungef\u00e4hr.", "tokens": ["Und", "ein", "blin\u00b7des", "Huhn", "nur", "fin\u00b7det", "sie", "in", "Spreu", "von", "un\u00b7ge\u00b7f\u00e4hr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "ADV", "VVFIN", "PPER", "APPR", "NN", "APPR", "ADJD", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.2": {"line.1": {"text": "Kannst die Weisheit du vergleichen einem reinen Edelstein,", "tokens": ["Kannst", "die", "Weis\u00b7heit", "du", "ver\u00b7glei\u00b7chen", "ei\u00b7nem", "rei\u00b7nen", "E\u00b7del\u00b7stein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "PPER", "VVINF", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "So begreifst du auch, weswegen Wenige sie kaufen ein.", "tokens": ["So", "be\u00b7greifst", "du", "auch", ",", "wes\u00b7we\u00b7gen", "We\u00b7ni\u00b7ge"