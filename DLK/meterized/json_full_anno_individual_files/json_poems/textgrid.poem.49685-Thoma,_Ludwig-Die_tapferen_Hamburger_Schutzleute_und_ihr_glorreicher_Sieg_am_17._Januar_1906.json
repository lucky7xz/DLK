ord", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.30": {"line.1": {"text": "Tischler Wittmann fand als Bayer", "tokens": ["Tischler", "Witt\u00b7mann", "fand", "als", "Ba\u00b7yer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "NE", "VVFIN", "KOKOM", "NE"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Hierorts seine Leichenfeier,", "tokens": ["Hier\u00b7orts", "sei\u00b7ne", "Lei\u00b7chen\u00b7fei\u00b7er", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Denn ein Hieb, den man ihm gab,", "tokens": ["Denn", "ein", "Hieb", ",", "den", "man", "ihm", "gab", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "PIS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Rief ihn von der Erde ab.", "tokens": ["Rief", "ihn", "von", "der", "Er\u00b7de", "ab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.31": {"line.1": {"text": "Als er sich am Boden w\u00e4lzte,", "tokens": ["Als", "er", "sich", "am", "Bo\u00b7den", "w\u00e4lz\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kam ein Schutzmann noch und pelzte", "tokens": ["Kam", "ein", "Schutz\u00b7mann", "noch", "und", "pelz\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "ART", "NN", "ADV", "KON", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3