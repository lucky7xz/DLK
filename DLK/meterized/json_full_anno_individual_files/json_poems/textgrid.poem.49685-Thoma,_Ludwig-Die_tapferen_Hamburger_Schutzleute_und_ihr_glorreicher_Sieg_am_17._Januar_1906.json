{"textgrid.poem.49685": {"metadata": {"author": {"name": "Thoma, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Die tapferen Hamburger Schutzleute und ihr glorreicher Sieg am 17. Januar 1906", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ja, ihr tapfern Hanseaten,", "tokens": ["Ja", ",", "ihr", "tap\u00b7fern", "Han\u00b7se\u00b7a\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ihr vollbrachtet Heldentaten,", "tokens": ["Ihr", "voll\u00b7brach\u00b7tet", "Hel\u00b7den\u00b7ta\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ihr wart so f\u00fcrchterlich", "tokens": ["Und", "ihr", "wart", "so", "f\u00fcrch\u00b7ter\u00b7lich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADV", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Als wie Anno siebenzich.", "tokens": ["Als", "wie", "An\u00b7no", "sie\u00b7ben\u00b7zich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Und ihr lie\u00dft die Fahne flattern,", "tokens": ["Und", "ihr", "lie\u00dft", "die", "Fah\u00b7ne", "flat\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schluget drauf, bis Wasserblattern", "tokens": ["Schlu\u00b7get", "drauf", ",", "bis", "Was\u00b7ser\u00b7blat\u00b7tern"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PTKVZ", "$,", "KOUS", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Jeder hatte an der Hand,", "tokens": ["Je\u00b7der", "hat\u00b7te", "an", "der", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Die er um den S\u00e4bel wand.", "tokens": ["Die", "er", "um", "den", "S\u00e4\u00b7bel", "wand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Alte Weiber, Kinder, Greise", "tokens": ["Al\u00b7te", "Wei\u00b7ber", ",", "Kin\u00b7der", ",", "Grei\u00b7se"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["ADJA", "NN", "$,", "NN", "$,", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Stacht ihr auf Soldatenweise", "tokens": ["Stacht", "ihr", "auf", "Sol\u00b7da\u00b7ten\u00b7wei\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und zwar meistenteils von hint',", "tokens": ["Und", "zwar", "meis\u00b7ten\u00b7teils", "von", "hint'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "APPR", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo sie stark verletzlich sind.", "tokens": ["Wo", "sie", "stark", "ver\u00b7letz\u00b7lich", "sind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Eure wackern Reiter fingen", "tokens": ["Eu\u00b7re", "wa\u00b7ckern", "Rei\u00b7ter", "fin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kr\u00fcppel, die nur m\u00fchsam gingen.", "tokens": ["Kr\u00fcp\u00b7pel", ",", "die", "nur", "m\u00fch\u00b7sam", "gin\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ADV", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der Pallasch saust herab,", "tokens": ["Und", "der", "Pal\u00b7lasch", "saust", "her\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADV", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Da\u00df es gleich ein Seufzen gab.", "tokens": ["Da\u00df", "es", "gleich", "ein", "Seuf\u00b7zen", "gab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Ja, in Hamburg ist's gewesen,", "tokens": ["Ja", ",", "in", "Ham\u00b7burg", "ist's", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "APPR", "NE", "NE", "VAPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo wir jetzt mit Ehrfurcht lesen,", "tokens": ["Wo", "wir", "jetzt", "mit", "Ehr\u00b7furcht", "le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df mit aller Manneskraft", "tokens": ["Da\u00df", "mit", "al\u00b7ler", "Man\u00b7nes\u00b7kraft"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sich gezeigt die Schutzmannschaft.", "tokens": ["Sich", "ge\u00b7zeigt", "die", "Schutz\u00b7mann\u00b7schaft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "VVPP", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Keiner ist davongelaufen", "tokens": ["Kei\u00b7ner", "ist", "da\u00b7von\u00b7ge\u00b7lau\u00b7fen"], "token_info": ["word", "word", "word"], "pos": ["PIS", "VAFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Vor den Weib- und Kinderhaufen,", "tokens": ["Vor", "den", "Weib", "und", "Kin\u00b7der\u00b7hau\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "TRUNC", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Jeder zeigt mit Stich und Hieb", "tokens": ["Je\u00b7der", "zeigt", "mit", "Stich", "und", "Hieb"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Seinen Staatserhaltungstrieb.", "tokens": ["Sei\u00b7nen", "Staa\u00b7tser\u00b7hal\u00b7tungs\u00b7trieb", "."], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Ohne Furcht und Allerbarmen", "tokens": ["Oh\u00b7ne", "Furcht", "und", "Al\u00b7ler\u00b7bar\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Haben ihrer acht Schandarmen", "tokens": ["Ha\u00b7ben", "ih\u00b7rer", "acht", "Schan\u00b7dar\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "CARD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Eine alte Frau besiegt,", "tokens": ["Ei\u00b7ne", "al\u00b7te", "Frau", "be\u00b7siegt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df sie schwach am Boden liegt.", "tokens": ["Da\u00df", "sie", "schwach", "am", "Bo\u00b7den", "liegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Und ein Kind mit dreizehn Jahren", "tokens": ["Und", "ein", "Kind", "mit", "drei\u00b7zehn", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "CARD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hat es alsobald erfahren,", "tokens": ["Hat", "es", "al\u00b7so\u00b7bald", "er\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df der tapfre Hanseat", "tokens": ["Da\u00df", "der", "tapf\u00b7re", "Han\u00b7se\u00b7at"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Einen scharfen S\u00e4bel hat.", "tokens": ["Ei\u00b7nen", "schar\u00b7fen", "S\u00e4\u00b7bel", "hat", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Ihrer vier Schandarmen haben", "tokens": ["Ih\u00b7rer", "vier", "Schan\u00b7dar\u00b7men", "ha\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "CARD", "NN", "VAFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Einen kleinen Waisenknaben", "tokens": ["Ei\u00b7nen", "klei\u00b7nen", "Wai\u00b7sen\u00b7kna\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Treu gemeinsam so l\u00e4diert,", "tokens": ["Treu", "ge\u00b7mein\u00b7sam", "so", "l\u00e4\u00b7diert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ADV", "VVFIN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Da\u00df er nie mehr revoltiert.", "tokens": ["Da\u00df", "er", "nie", "mehr", "re\u00b7vol\u00b7tiert", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "H\u00f6niger hie\u00df ein Privater,", "tokens": ["H\u00f6\u00b7ni\u00b7ger", "hie\u00df", "ein", "Pri\u00b7va\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der auch als Familienvater", "tokens": ["Der", "auch", "als", "Fa\u00b7mi\u00b7li\u00b7en\u00b7va\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADV", "KOUS", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Damals auf die Stra\u00dfe ging", "tokens": ["Da\u00b7mals", "auf", "die", "Stra\u00b7\u00dfe", "ging"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und dabei f\u00fcnf Hiebe fing.", "tokens": ["Und", "da\u00b7bei", "f\u00fcnf", "Hie\u00b7be", "fing", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "CARD", "NN", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.11": {"line.1": {"text": "Reindel hie\u00df ein Frauenswesen,", "tokens": ["Rein\u00b7del", "hie\u00df", "ein", "Frau\u00b7ens\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Das man ohne Federlesen", "tokens": ["Das", "man", "oh\u00b7ne", "Fe\u00b7der\u00b7le\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "PIS", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Durch den Hut und falschen Zopf", "tokens": ["Durch", "den", "Hut", "und", "fal\u00b7schen", "Zopf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "KON", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Haute auf den Hinterkopf.", "tokens": ["Hau\u00b7te", "auf", "den", "Hin\u00b7ter\u00b7kopf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Den Verleger Martoschecken", "tokens": ["Den", "Ver\u00b7le\u00b7ger", "Mar\u00b7to\u00b7sche\u00b7cken"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wollte einer niederstrecken,", "tokens": ["Woll\u00b7te", "ei\u00b7ner", "nie\u00b7der\u00b7stre\u00b7cken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch er traf ihn diesmal nicht,", "tokens": ["Doch", "er", "traf", "ihn", "dies\u00b7mal", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADV", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Weil er hinters Gitter fl\u00fccht'.", "tokens": ["Weil", "er", "hin\u00b7ters", "Git\u00b7ter", "fl\u00fccht'", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Tischler Wittmann fand als Bayer", "tokens": ["Tischler", "Witt\u00b7mann", "fand", "als", "Ba\u00b7yer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "NE", "VVFIN", "KOKOM", "NE"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Hierorts seine Leichenfeier,", "tokens": ["Hier\u00b7orts", "sei\u00b7ne", "Lei\u00b7chen\u00b7fei\u00b7er", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Denn ein Hieb, den man ihm gab,", "tokens": ["Denn", "ein", "Hieb", ",", "den", "man", "ihm", "gab", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "PIS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Rief ihn von der Erde ab.", "tokens": ["Rief", "ihn", "von", "der", "Er\u00b7de", "ab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Als er sich am Boden w\u00e4lzte,", "tokens": ["Als", "er", "sich", "am", "Bo\u00b7den", "w\u00e4lz\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kam ein Schutzmann noch und pelzte", "tokens": ["Kam", "ein", "Schutz\u00b7mann", "noch", "und", "pelz\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "ART", "NN", "ADV", "KON", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Mit den Stiefeln ihn und sprach:", "tokens": ["Mit", "den", "Stie\u00b7feln", "ihn", "und", "sprach", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "KON", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "\u00bbkn\u00f6delbayer, gibst du nach?\u00ab", "tokens": ["\u00bb", "kn\u00f6\u00b7del\u00b7bay\u00b7er", ",", "gibst", "du", "nach", "?", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADJD", "$,", "VVFIN", "PPER", "PTKVZ", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "Frauen wurden weggetragen,", "tokens": ["Frau\u00b7en", "wur\u00b7den", "weg\u00b7ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Finger wurden abgeschlagen,", "tokens": ["Fin\u00b7ger", "wur\u00b7den", "ab\u00b7ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Kinder schrieen blutbespritzt,", "tokens": ["Kin\u00b7der", "schri\u00b7een", "blut\u00b7be\u00b7spritzt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ob ein Gott im Himmel sitzt.", "tokens": ["Ob", "ein", "Gott", "im", "Him\u00b7mel", "sitzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.16": {"line.1": {"text": "In der Berg- und Schmiedegassen", "tokens": ["In", "der", "Ber\u00b7g", "und", "Schmie\u00b7de\u00b7gas\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "TRUNC", "KON", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "War ein starkes Aderlassen,", "tokens": ["War", "ein", "star\u00b7kes", "A\u00b7der\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und das stolze Heldenschwert", "tokens": ["Und", "das", "stol\u00b7ze", "Hel\u00b7den\u00b7schwert"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Traf nur den, der sich nicht wehrt.", "tokens": ["Traf", "nur", "den", ",", "der", "sich", "nicht", "wehrt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "$,", "PRELS", "PRF", "PTKNEG", "VVFIN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.17": {"line.1": {"text": "Dieses sind die Heldentaten", "tokens": ["Die\u00b7ses", "sind", "die", "Hel\u00b7den\u00b7ta\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "NN"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Von den tapfren Hanseaten,", "tokens": ["Von", "den", "tapf\u00b7ren", "Han\u00b7se\u00b7a\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die so kampfbegierig sind", "tokens": ["Die", "so", "kampf\u00b7be\u00b7gie\u00b7rig", "sind"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADV", "ADJD", "VAFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Gegen Greis und Weib und Kind.", "tokens": ["Ge\u00b7gen", "Greis", "und", "Weib", "und", "Kind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.18": {"line.1": {"text": "Ja, ihr tapfern Hanseaten,", "tokens": ["Ja", ",", "ihr", "tap\u00b7fern", "Han\u00b7se\u00b7a\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ihr vollbrachtet Heldentaten,", "tokens": ["Ihr", "voll\u00b7brach\u00b7tet", "Hel\u00b7den\u00b7ta\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ihr wart so f\u00fcrchterlich", "tokens": ["Und", "ihr", "wart", "so", "f\u00fcrch\u00b7ter\u00b7lich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADV", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Als wie Anno siebenzich.", "tokens": ["Als", "wie", "An\u00b7no", "sie\u00b7ben\u00b7zich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.19": {"line.1": {"text": "Und ihr lie\u00dft die Fahne flattern,", "tokens": ["Und", "ihr", "lie\u00dft", "die", "Fah\u00b7ne", "flat\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schluget drauf, bis Wasserblattern", "tokens": ["Schlu\u00b7get", "drauf", ",", "bis", "Was\u00b7ser\u00b7blat\u00b7tern"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PTKVZ", "$,", "KOUS", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Jeder hatte an der Hand,", "tokens": ["Je\u00b7der", "hat\u00b7te", "an", "der", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Die er um den S\u00e4bel wand.", "tokens": ["Die", "er", "um", "den", "S\u00e4\u00b7bel", "wand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.20": {"line.1": {"text": "Alte Weiber, Kinder, Greise", "tokens": ["Al\u00b7te", "Wei\u00b7ber", ",", "Kin\u00b7der", ",", "Grei\u00b7se"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["ADJA", "NN", "$,", "NN", "$,", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Stacht ihr auf Soldatenweise", "tokens": ["Stacht", "ihr", "auf", "Sol\u00b7da\u00b7ten\u00b7wei\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und zwar meistenteils von hint',", "tokens": ["Und", "zwar", "meis\u00b7ten\u00b7teils", "von", "hint'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "APPR", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo sie stark verletzlich sind.", "tokens": ["Wo", "sie", "stark", "ver\u00b7letz\u00b7lich", "sind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.21": {"line.1": {"text": "Eure wackern Reiter fingen", "tokens": ["Eu\u00b7re", "wa\u00b7ckern", "Rei\u00b7ter", "fin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kr\u00fcppel, die nur m\u00fchsam gingen.", "tokens": ["Kr\u00fcp\u00b7pel", ",", "die", "nur", "m\u00fch\u00b7sam", "gin\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ADV", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der Pallasch saust herab,", "tokens": ["Und", "der", "Pal\u00b7lasch", "saust", "her\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADV", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Da\u00df es gleich ein Seufzen gab.", "tokens": ["Da\u00df", "es", "gleich", "ein", "Seuf\u00b7zen", "gab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.22": {"line.1": {"text": "Ja, in Hamburg ist's gewesen,", "tokens": ["Ja", ",", "in", "Ham\u00b7burg", "ist's", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "APPR", "NE", "NE", "VAPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo wir jetzt mit Ehrfurcht lesen,", "tokens": ["Wo", "wir", "jetzt", "mit", "Ehr\u00b7furcht", "le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df mit aller Manneskraft", "tokens": ["Da\u00df", "mit", "al\u00b7ler", "Man\u00b7nes\u00b7kraft"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sich gezeigt die Schutzmannschaft.", "tokens": ["Sich", "ge\u00b7zeigt", "die", "Schutz\u00b7mann\u00b7schaft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "VVPP", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.23": {"line.1": {"text": "Keiner ist davongelaufen", "tokens": ["Kei\u00b7ner", "ist", "da\u00b7von\u00b7ge\u00b7lau\u00b7fen"], "token_info": ["word", "word", "word"], "pos": ["PIS", "VAFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Vor den Weib- und Kinderhaufen,", "tokens": ["Vor", "den", "Weib", "und", "Kin\u00b7der\u00b7hau\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "TRUNC", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Jeder zeigt mit Stich und Hieb", "tokens": ["Je\u00b7der", "zeigt", "mit", "Stich", "und", "Hieb"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Seinen Staatserhaltungstrieb.", "tokens": ["Sei\u00b7nen", "Staa\u00b7tser\u00b7hal\u00b7tungs\u00b7trieb", "."], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.24": {"line.1": {"text": "Ohne Furcht und Allerbarmen", "tokens": ["Oh\u00b7ne", "Furcht", "und", "Al\u00b7ler\u00b7bar\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Haben ihrer acht Schandarmen", "tokens": ["Ha\u00b7ben", "ih\u00b7rer", "acht", "Schan\u00b7dar\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "CARD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Eine alte Frau besiegt,", "tokens": ["Ei\u00b7ne", "al\u00b7te", "Frau", "be\u00b7siegt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df sie schwach am Boden liegt.", "tokens": ["Da\u00df", "sie", "schwach", "am", "Bo\u00b7den", "liegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.25": {"line.1": {"text": "Und ein Kind mit dreizehn Jahren", "tokens": ["Und", "ein", "Kind", "mit", "drei\u00b7zehn", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "CARD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hat es alsobald erfahren,", "tokens": ["Hat", "es", "al\u00b7so\u00b7bald", "er\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df der tapfre Hanseat", "tokens": ["Da\u00df", "der", "tapf\u00b7re", "Han\u00b7se\u00b7at"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Einen scharfen S\u00e4bel hat.", "tokens": ["Ei\u00b7nen", "schar\u00b7fen", "S\u00e4\u00b7bel", "hat", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.26": {"line.1": {"text": "Ihrer vier Schandarmen haben", "tokens": ["Ih\u00b7rer", "vier", "Schan\u00b7dar\u00b7men", "ha\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "CARD", "NN", "VAFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Einen kleinen Waisenknaben", "tokens": ["Ei\u00b7nen", "klei\u00b7nen", "Wai\u00b7sen\u00b7kna\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Treu gemeinsam so l\u00e4diert,", "tokens": ["Treu", "ge\u00b7mein\u00b7sam", "so", "l\u00e4\u00b7diert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ADV", "VVFIN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Da\u00df er nie mehr revoltiert.", "tokens": ["Da\u00df", "er", "nie", "mehr", "re\u00b7vol\u00b7tiert", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.27": {"line.1": {"text": "H\u00f6niger hie\u00df ein Privater,", "tokens": ["H\u00f6\u00b7ni\u00b7ger", "hie\u00df", "ein", "Pri\u00b7va\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der auch als Familienvater", "tokens": ["Der", "auch", "als", "Fa\u00b7mi\u00b7li\u00b7en\u00b7va\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADV", "KOUS", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Damals auf die Stra\u00dfe ging", "tokens": ["Da\u00b7mals", "auf", "die", "Stra\u00b7\u00dfe", "ging"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und dabei f\u00fcnf Hiebe fing.", "tokens": ["Und", "da\u00b7bei", "f\u00fcnf", "Hie\u00b7be", "fing", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "CARD", "NN", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.28": {"line.1": {"text": "Reindel hie\u00df ein Frauenswesen,", "tokens": ["Rein\u00b7del", "hie\u00df", "ein", "Frau\u00b7ens\u