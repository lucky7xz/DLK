{"dta.poem.10223": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "Die Kapelle zum finstern Stern.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1883", "urn": "urn:nbn:de:kobv:b4-200905197184", "language": ["de:0.85", "af:0.14"], "booktitle": "Liliencron, Detlev von: Adjutantenritte und andere Gedichte. Leipzig, [1883]."}, "poem": {"stanza.1": {"line.1": {"text": "\u201evom Trinken ist dir die Stirne hei\u00df,", "tokens": ["\u201e", "vom", "Trin\u00b7ken", "ist", "dir", "die", "Stir\u00b7ne", "hei\u00df", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPRART", "NN", "VAFIN", "PPER", "ART", "NN", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mein Segel wiegt unten, scharlach und wei\u00df,", "tokens": ["Mein", "Se\u00b7gel", "wiegt", "un\u00b7ten", ",", "schar\u00b7lach", "und", "wei\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "$,", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Schlo\u00dfknechte spannen den Baldachin,", "tokens": ["Schlo\u00df\u00b7knech\u00b7te", "span\u00b7nen", "den", "Bal\u00b7da\u00b7ch\u00b7in", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Der K\u00f6nig schl\u00e4ft auf dem Hermelin,", "tokens": ["Der", "K\u00f6\u00b7nig", "schl\u00e4ft", "auf", "dem", "Her\u00b7me\u00b7lin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Verworren Get\u00f6n vom Prunkgelag,", "tokens": ["Ver\u00b7wor\u00b7ren", "Ge\u00b7t\u00f6n", "vom", "Prunk\u00b7ge\u00b7lag", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da schie\u00dft mit gleichem Einfallschlag", "tokens": ["Da", "schie\u00dft", "mit", "glei\u00b7chem", "Ein\u00b7fall\u00b7schlag"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "\u201ehalt, halt, K\u00f6nig Erich!\u201c \u2026 Fackeln im Wind", "tokens": ["\u201e", "halt", ",", "halt", ",", "K\u00f6\u00b7nig", "E\u00b7rich", "!", "\u201c", "\u2026", "Fa\u00b7ckeln", "im", "Wind"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "punct", "punct", "punct", "word", "word", "word"], "pos": ["$(", "VVFIN", "$,", "VVFIN", "$,", "NN", "NE", "$.", "$(", "$(", "NN", "APPRART", "NN"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "\u201ewo blieb Wieb Sture, gieb Antwort, geschwind,", "tokens": ["\u201e", "wo", "blieb", "Wieb", "Stu\u00b7re", ",", "gieb", "Ant\u00b7wort", ",", "ge\u00b7schwind", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["$(", "PWAV", "VVFIN", "NN", "NN", "$,", "VVIMP", "NN", "$,", "ADJD", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "\u201ebei Sanct J\u00fcrgen, ich ri\u00df sie dir Hund vom Leib,\u201c", "tokens": ["\u201e", "bei", "Sanct", "J\u00fcr\u00b7gen", ",", "ich", "ri\u00df", "sie", "dir", "Hund", "vom", "Leib", ",", "\u201c"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "APPR", "NE", "NE", "$,", "PPER", "VVFIN", "PPER", "PPER", "NN", "APPRART", "NN", "$,", "$("], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.2": {"text": "\u201ebei Sanct J\u00fcrgen, sie war mir Zeitvertreib", "tokens": ["\u201e", "bei", "Sanct", "J\u00fcr\u00b7gen", ",", "sie", "war", "mir", "Zeit\u00b7ver\u00b7treib"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "APPR", "NE", "NE", "$,", "PPER", "VAFIN", "PPER", "NN"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}}, "stanza.6": {"line.1": {"text": "Der Ritter ringt ihm den Dolch vom Gehenk,", "tokens": ["Der", "Rit\u00b7ter", "ringt", "ihm", "den", "Dolch", "vom", "Ge\u00b7henk", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Das rote Blut tropft ins w\u00fcste Gemeng,", "tokens": ["Das", "ro\u00b7te", "Blut", "tropft", "ins", "w\u00fcs\u00b7te", "Ge\u00b7meng", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "Wo Lauge durchstach den erlauchten Herrn,", "tokens": ["Wo", "Lau\u00b7ge", "durch\u00b7stach", "den", "er\u00b7lauch\u00b7ten", "Herrn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Da steht die Kapelle zum finstern Stern,", "tokens": ["Da", "steht", "die", "Ka\u00b7pel\u00b7le", "zum", "fins\u00b7tern", "Stern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}}}}