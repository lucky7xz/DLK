{"dta.poem.21326": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Caput VII.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-30602-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Und als ich einschlief, da tr\u00e4umte mir,", "tokens": ["Und", "als", "ich", "ein\u00b7schlief", ",", "da", "tr\u00e4um\u00b7te", "mir", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ich schlenderte wieder im hellen", "tokens": ["Ich", "schlen\u00b7der\u00b7te", "wie\u00b7der", "im", "hel\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "ADJA"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Mondschein die hallenden Stra\u00dfen entlang,", "tokens": ["Mond\u00b7schein", "die", "hal\u00b7len\u00b7den", "Stra\u00b7\u00dfen", "ent\u00b7lang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "In dem alterth\u00fcmlichen C\u00f6llen.", "tokens": ["In", "dem", "al\u00b7tert\u00b7h\u00fcm\u00b7li\u00b7chen", "C\u00f6l\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Und hinter mir ging wieder einher", "tokens": ["Und", "hin\u00b7ter", "mir", "ging", "wie\u00b7der", "ein\u00b7her"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "VVFIN", "ADV", "PTKVZ"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Mein schwarzer, vermummter Begleiter.", "tokens": ["Mein", "schwar\u00b7zer", ",", "ver\u00b7mumm\u00b7ter", "Be\u00b7glei\u00b7ter", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Ich war so m\u00fcde, mir brachen die Knie,", "tokens": ["Ich", "war", "so", "m\u00fc\u00b7de", ",", "mir", "bra\u00b7chen", "die", "Knie", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Doch immer gingen wir weiter.", "tokens": ["Doch", "im\u00b7mer", "gin\u00b7gen", "wir", "wei\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}}}}