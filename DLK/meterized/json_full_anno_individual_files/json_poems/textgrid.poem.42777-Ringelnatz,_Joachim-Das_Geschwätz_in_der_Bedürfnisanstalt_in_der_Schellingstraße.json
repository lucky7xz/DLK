{"textgrid.poem.42777": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "Das Geschw\u00e4tz in der Bed\u00fcrfnisanstalt in der Schellingstra\u00dfe", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Heute wurde Geld eingesammelt,", "tokens": ["Heu\u00b7te", "wur\u00b7de", "Geld", "ein\u00b7ge\u00b7sam\u00b7melt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "VVPP", "$,"], "meter": "+-+-++-+-", "measure": "unknown.measure.penta"}, "line.2": {"text": "Wo ich angestellt bin, in dem B\u00fcro,", "tokens": ["Wo", "ich", "an\u00b7ge\u00b7stellt", "bin", ",", "in", "dem", "B\u00fc\u00b7ro", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVPP", "VAFIN", "$,", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-++-+", "measure": "unknown.measure.hexa"}, "line.3": {"text": "F\u00fcr die Frau von jemand, der sich erh\u00e4ngte.", "tokens": ["F\u00fcr", "die", "Frau", "von", "je\u00b7mand", ",", "der", "sich", "er\u00b7h\u00e4ng\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "PIS", "$,", "PRELS", "PRF", "VVFIN", "$."], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Eine B\u00fcchse ging rum. Und jeder schenkte.", "tokens": ["Ei\u00b7ne", "B\u00fcch\u00b7se", "ging", "rum", ".", "Und", "je\u00b7der", "schenk\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$.", "KON", "PIS", "VVFIN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Drei Mark; das ist bei uns immer so.", "tokens": ["Drei", "Mark", ";", "das", "ist", "bei", "uns", "im\u00b7mer", "so", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "$.", "PDS", "VAFIN", "APPR", "PPER", "ADV", "ADV", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Es braucht niemand zu wissen, wodran ich bin.", "tokens": ["Es", "braucht", "nie\u00b7mand", "zu", "wis\u00b7sen", ",", "wod\u00b7ran", "ich", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "PTKZU", "VVINF", "$,", "PWAV", "PPER", "VAFIN", "$."], "meter": "-+---+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ich habe das Geld meiner Mutter gestohlen.", "tokens": ["Ich", "ha\u00b7be", "das", "Geld", "mei\u00b7ner", "Mut\u00b7ter", "ge\u00b7stoh\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}, "stanza.3": {"line.1": {"text": "Ich habe noch gestern acht Mark f\u00fcr Kohlen", "tokens": ["Ich", "ha\u00b7be", "noch", "ge\u00b7stern", "acht", "Mark", "f\u00fcr", "Koh\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "CARD", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bezahlt. Und die Alte stumpft doch blo\u00df so hin.", "tokens": ["Be\u00b7zahlt", ".", "Und", "die", "Al\u00b7te", "stumpft", "doch", "blo\u00df", "so", "hin", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$.", "KON", "ART", "NN", "VVFIN", "ADV", "ADV", "ADV", "PTKVZ", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Und bei ihrer Schwindsucht und sowieso", "tokens": 