{"dta.poem.2633": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Trost-Schreiben  \n An Hn. M. F.   J. P.   \u00fcber dem Absterben seiner  \n Ehellebsten M. M. g. R. den 30.  \n Novemb. 1671.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ach hoch betr\u00fcbter Freund was soll ich dir doch schreib&#275;", "tokens": ["Ach", "hoch", "be\u00b7tr\u00fcb\u00b7ter", "Freund", "was", "soll", "ich", "dir", "doch", "schreib", "&#275;"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "XML_entity"], "pos": ["ITJ", "ADJD", "ADJA", "NN", "PWS", "VMFIN", "PPER", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Jetzt da dein treues Hertz in heissem Blute schwimmt!", "tokens": ["Jetzt", "da", "dein", "treu\u00b7es", "Hertz", "in", "heis\u00b7sem", "Blu\u00b7te", "schwimmt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPOSAT", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Darff mein elender Reim den Seelen-Ri\u00df auffreiben?", "tokens": ["Darff", "mein", "e\u00b7len\u00b7der", "Reim", "den", "See\u00b7len\u00b7Ri\u00df", "auf\u00b7frei\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "ADJA", "NN", "ART", "NN", "VVINF", "$."], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Den stets ein neues Weh/ und herbes Ach! ergrimmt.", "tokens": ["Den", "stets", "ein", "neu\u00b7es", "Weh", "/", "und", "her\u00b7bes", "Ach", "!", "er\u00b7grimmt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADV", "ART", "ADJA", "NN", "$(", "KON", "ADJA", "NN", "$.", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Denn solche Schmertzen sind zu grossen Potentaten/", "tokens": ["Denn", "sol\u00b7che", "Schmert\u00b7zen", "sind", "zu", "gros\u00b7sen", "Po\u00b7ten\u00b7ta\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der beste Redner wird hier sprachlo\u00df und verstummt/", "tokens": ["Der", "bes\u00b7te", "Red\u00b7ner", "wird", "hier", "sprac\u00b7hlo\u00df", "und", "ver\u00b7stummt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "VVFIN", "KON", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Dein bleiches Angesicht/ die Augen wie Granaten/", "tokens": ["Dein", "blei\u00b7ches", "An\u00b7ge\u00b7sicht", "/", "die", "Au\u00b7gen", "wie", "Gra\u00b7na\u00b7ten", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$(", "ART", "NN", "KOKOM", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und der erschrockne Leib in schwartzen Flor vermummt/", "tokens": ["Und", "der", "er\u00b7schrock\u00b7ne", "Leib", "in", "schwart\u00b7zen", "Flor", "ver\u00b7mummt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Zu dem der Mutter Leid und \u00e4ngstlich H\u00e4nde ringen/", "tokens": ["Zu", "dem", "der", "Mut\u00b7ter", "Leid", "und", "\u00e4ngst\u00b7lich", "H\u00e4n\u00b7de", "rin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "NN", "KON", "ADJD", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Der Schwester Jam\u0303er-Gall/ der Freundschafft Thr\u00e4n\u0113-See/", "tokens": ["Der", "Schwes\u00b7ter", "Jam\u0303er\u00b7Gall", "/", "der", "Freund\u00b7schafft", "Thr\u00e4n\u0113\u00b7See", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$(", "ART", "NN", "NN", "$("], "meter": "-+-++-+--+", "measure": "iambic.penta.chol"}, "line.11": {"text": "Die lassen mich kein Lied bey solchem Zustand singen/", "tokens": ["Die", "las\u00b7sen", "mich", "kein", "Lied", "bey", "sol\u00b7chem", "Zu\u00b7stand", "sin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PIAT", "NN", "APPR", "PIAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Indem ich selbst nicht wei\u00df ob ich bey Todten steh.", "tokens": ["In\u00b7dem", "ich", "selbst", "nicht", "wei\u00df", "ob", "ich", "bey", "Tod\u00b7ten", "steh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PTKNEG", "VVFIN", "KOUS", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ich seh dein Hochzeit-Bett in einen Sarch verkehret/", "tokens": ["Ich", "seh", "dein", "Hoch\u00b7zeit\u00b7Bett", "in", "ei\u00b7nen", "Sarch", "ver\u00b7keh\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Eh noch die Cynthia uns neunmal angelacht/", "tokens": ["Eh", "noch", "die", "Cyn\u00b7thia", "uns", "neun\u00b7mal", "an\u00b7ge\u00b7lacht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "PPER", "ADV", "VVPP", "$("], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.15": {"text": "So wird dein Rebenstock sambt Bl\u00fcth und Frucht verheeret/", "tokens": ["So", "wird", "dein", "Re\u00b7ben\u00b7stock", "sambt", "Bl\u00fcth", "und", "Frucht", "ver\u00b7hee\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Mu\u00df dessen seyn ein Grab/ dem er das Leben bracht.", "tokens": ["Mu\u00df", "des\u00b7sen", "seyn", "ein", "Grab", "/", "dem", "er", "das", "Le\u00b7ben", "bracht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PDS", "VAFIN", "ART", "NN", "$(", "PRELS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Ach h\u00e4ngt Geburt und Tod in so genauen Ketten/", "tokens": ["Ach", "h\u00e4ngt", "Ge\u00b7burt", "und", "Tod", "in", "so", "ge\u00b7nau\u00b7en", "Ket\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "NN", "KON", "NN", "APPR", "ADV", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Und sol das Leben denn des Sterbens Nachbar seyn!", "tokens": ["Und", "sol", "das", "Le\u00b7ben", "denn", "des", "Ster\u00b7bens", "Nach\u00b7bar", "seyn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "NN", "KON", "ART", "NN", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Die Stunde so uns heist das grosse Rund betreten", "tokens": ["Die", "Stun\u00b7de", "so", "uns", "heist", "das", "gros\u00b7se", "Rund", "be\u00b7tre\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "PPER", "VVFIN", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "F\u00fchrt uns offt wieder ab/ und in das Grab hinein/", "tokens": ["F\u00fchrt", "uns", "offt", "wie\u00b7der", "ab", "/", "und", "in", "das", "Grab", "hin\u00b7ein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "PTKVZ", "$(", "KON", "APPR", "ART", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Schien nicht des Himmels Gunst vollkommen dir geneiget/", "tokens": ["Schien", "nicht", "des", "Him\u00b7mels", "Gunst", "voll\u00b7kom\u00b7men", "dir", "ge\u00b7nei\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ART", "NN", "NN", "ADJD", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Als in dem ersten Lentz die Blume deiner Eh", "tokens": ["Als", "in", "dem", "ers\u00b7ten", "Lentz", "die", "Blu\u00b7me", "dei\u00b7ner", "Eh"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Was nur ergetzen kan/ anmuthig hat gezeiget?", "tokens": ["Was", "nur", "er\u00b7get\u00b7zen", "kan", "/", "an\u00b7mut\u00b7hig", "hat", "ge\u00b7zei\u00b7get", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVINF", "VMFIN", "$(", "ADJD", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Flo\u00df dir nicht Segen zu von der gestirnten H\u00f6h?", "tokens": ["Flo\u00df", "dir", "nicht", "Se\u00b7gen", "zu", "von", "der", "ge\u00b7stirn\u00b7ten", "H\u00f6h", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "NN", "PTKZU", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Es brandte deine Lieb in unzertrennten Flammen/", "tokens": ["Es", "brand\u00b7te", "dei\u00b7ne", "Lieb", "in", "un\u00b7zer\u00b7trenn\u00b7ten", "Flam\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Sie war dein Augen-Trost/ du ihrer W\u00fcnsche Ziel/", "tokens": ["Sie", "war", "dein", "Au\u00b7gen\u00b7Trost", "/", "du", "ih\u00b7rer", "W\u00fcn\u00b7sche", "Ziel", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "$(", "PPER", "PPOSAT", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Und g\u00f6ldner Friede band euch dergestalt zusammen/", "tokens": ["Und", "g\u00f6ld\u00b7ner", "Frie\u00b7de", "band", "euch", "der\u00b7ge\u00b7stalt", "zu\u00b7sam\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "PPER", "ADV", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Da\u00df nichts als Freud und Lust in beyder Armen fiel.", "tokens": ["Da\u00df", "nichts", "als", "Freud", "und", "Lust", "in", "bey\u00b7der", "Ar\u00b7men", "fiel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "KOKOM", "NN", "KON", "NN", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Wer h\u00e4tte da vermeynt (und war es zu ergr\u00fcnden?)", "tokens": ["Wer", "h\u00e4t\u00b7te", "da", "ver\u00b7meynt", "(", "und", "war", "es", "zu", "er\u00b7gr\u00fcn\u00b7den", "?", ")"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "VAFIN", "ADV", "VVPP", "$(", "KON", "VAFIN", "PPER", "PTKZU", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Da\u00df nach so kurtzer Zeit du deine Seelen-Lust", "tokens": ["Da\u00df", "nach", "so", "kurt\u00b7zer", "Zeit", "du", "dei\u00b7ne", "See\u00b7len\u00b7Lust"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ADV", "ADJA", "NN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Ins Leich-Tuch eingeh\u00fcllt/ im Sarche soltest finden?", "tokens": ["Ins", "Leich\u00b7Tuch", "ein\u00b7ge\u00b7h\u00fcllt", "/", "im", "Sar\u00b7che", "sol\u00b7test", "fin\u00b7den", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$(", "APPRART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Ach unverhoffter Fall/ und schmertzlicher Verlust!", "tokens": ["Ach", "un\u00b7ver\u00b7hoff\u00b7ter", "Fall", "/", "und", "schmertz\u00b7li\u00b7cher", "Ver\u00b7lust", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "ADJA", "NN", "$(", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Ein eintzig Augenblick beraubt dich aller Freuden/", "tokens": ["Ein", "eint\u00b7zig", "Au\u00b7gen\u00b7blick", "be\u00b7raubt", "dich", "al\u00b7ler", "Freu\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VVFIN", "PPER", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Und l\u00e4st dich nicht die Frucht von eurem Segen sehn/", "tokens": ["Und", "l\u00e4st", "dich", "nicht", "die", "Frucht", "von", "eu\u00b7rem", "Se\u00b7gen", "sehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Zwey Hertzen m\u00fcssen ietzt von deinem Hertzen scheiden/", "tokens": ["Zwey", "Hert\u00b7zen", "m\u00fcs\u00b7sen", "ietzt", "von", "dei\u00b7nem", "Hert\u00b7zen", "schei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VMFIN", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Welch Mensch verarget dir dein Winseln und dein Flehn?", "tokens": ["Welch", "Mensch", "ver\u00b7ar\u00b7get", "dir", "dein", "Win\u00b7seln", "und", "dein", "Flehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Des Creutzes Aloe ist hier nur allzubitter/", "tokens": ["Des", "Creut\u00b7zes", "A\u00b7loe", "ist", "hier", "nur", "all\u00b7zu\u00b7bit\u00b7ter", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VAFIN", "ADV", "ADV", "ADJD", "$("], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.38": {"text": "Sie nimmt die Zuckerung des Trostes gar nicht an/", "tokens": ["Sie", "nimmt", "die", "Zu\u00b7cke\u00b7rung", "des", "Tros\u00b7tes", "gar", "nicht", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "ADV", "PTKNEG", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Ein solcher Fall best\u00fcrtzt die trefflichsten Gem\u00fcther/", "tokens": ["Ein", "sol\u00b7cher", "Fall", "be\u00b7st\u00fcrtzt", "die", "treff\u00b7lichs\u00b7ten", "Ge\u00b7m\u00fc\u00b7ther", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.40": {"text": "Da\u00df auch ein Stoicus sich hier nicht halten kan.", "tokens": ["Da\u00df", "auch", "ein", "Stoi\u00b7cus", "sich", "hier", "nicht", "hal\u00b7ten", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "PRF", "ADV", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.41": {"text": "Es mag die Vorder-Welt des Orpheus Treu beschreiben/", "tokens": ["Es", "mag", "die", "Vor\u00b7der\u00b7Welt", "des", "Or\u00b7pheus", "Treu", "be\u00b7schrei\u00b7ben", "/"], "token_info": ["word", "word"