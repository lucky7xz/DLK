{"dta.poem.20710": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "An  \n Ihro K\u00f6nigliche Hoheit  \n  die  \n  Prinzessinn Louise,  \n Tochter des Prinzen Ferdinand  \n von Preu\u00dfen  K. H.  \n Als diese h\u00f6chsten Herrschaften Bellev\u00fce beziehen wollten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Prinze\u00dfin! die so lieblich bl\u00fchet,                 ", "tokens": ["Prin\u00b7ze\u00b7\u00dfin", "!", "die", "so", "lieb\u00b7lich", "bl\u00fc\u00b7het", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ART", "ADV", "ADJD", "VVFIN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "So freundlich ist wie sch\u00f6ner May \u2014", "tokens": ["So", "freund\u00b7lich", "ist", "wie", "sch\u00f6\u00b7ner", "May"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "KOKOM", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Des n\u00e4chsten Jahres Fr\u00fchling siehet", "tokens": ["Des", "n\u00e4chs\u00b7ten", "Jah\u00b7res", "Fr\u00fch\u00b7ling", "sie\u00b7het"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NE", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dich wandeln, wenn der Morgen neu", "tokens": ["Dich", "wan\u00b7deln", ",", "wenn", "der", "Mor\u00b7gen", "neu"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ART", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Des Tannenhaynes S\u00e4nger weckte,", "tokens": ["Des", "Tan\u00b7nen\u00b7hay\u00b7nes", "S\u00e4n\u00b7ger", "weck\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und wie mit Diamanten-Glanz", "tokens": ["Und", "wie", "mit", "Di\u00b7a\u00b7man\u00b7ten\u00b7Glanz"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PWAV", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der Tau das Ufergras bedeckte,", "tokens": ["Der", "Tau", "das", "U\u00b7fer\u00b7gras", "be\u00b7deck\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Da wandelst Du im Blumenkranz;", "tokens": ["Da", "wan\u00b7delst", "Du", "im", "Blu\u00b7men\u00b7kranz", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und jede Dryas wird sich neigen,", "tokens": ["Und", "je\u00b7de", "Dryas", "wird", "sich", "nei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "PRF", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "Und jedes Wiesenbl\u00fcmchen Dir", "tokens": ["Und", "je\u00b7des", "Wie\u00b7sen\u00b7bl\u00fcm\u00b7chen", "Dir"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Dem\u00fcthig sich zu F\u00fc\u00dfen beugen \u2014", "tokens": ["De\u00b7m\u00fct\u00b7hig", "sich", "zu", "F\u00fc\u00b7\u00dfen", "beu\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PRF", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Und: \u201eFlora, Flora wandelt hier!\u201c", "tokens": ["Und", ":", "\u201e", "Flo\u00b7ra", ",", "Flo\u00b7ra", "wan\u00b7delt", "hier", "!", "\u201c"], "token_info": ["word", "punct", "punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["KON", "$.", "$(", "NE", "$,", "NN", "VVFIN", "ADV", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Wird Nachtigall und Lerche singen.", "tokens": ["Wird", "Nach\u00b7ti\u00b7gall", "und", "Ler\u00b7che", "sin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}