{"textgrid.poem.67812": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "8. Zaid und Zaida", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Durch die Strasse seiner Dame", "tokens": ["Durch", "die", "Stras\u00b7se", "sei\u00b7ner", "Da\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wandelt Zaid auf und nieder,", "tokens": ["Wan\u00b7delt", "Zaid", "auf", "und", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Harrend, da\u00df die Stunde komme,", "tokens": ["Har\u00b7rend", ",", "da\u00df", "die", "Stun\u00b7de", "kom\u00b7me", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Endlich komme, sie zu sprechen.", "tokens": ["End\u00b7lich", "kom\u00b7me", ",", "sie", "zu", "spre\u00b7chen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PPER", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Und schon geht der Mohr verzweifelnd,", "tokens": ["Und", "schon", "geht", "der", "Mohr", "ver\u00b7zwei\u00b7felnd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da es sich so lange z\u00f6gert,", "tokens": ["Da", "es", "sich", "so", "lan\u00b7ge", "z\u00f6\u00b7gert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Denket: nur von ihr Ein Anblick", "tokens": ["Den\u00b7ket", ":", "nur", "von", "ihr", "Ein", "An\u00b7blick"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$.", "ADV", "APPR", "PPER", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wird all meine Flammen k\u00fchlen. \u2013", "tokens": ["Wird", "all", "mei\u00b7ne", "Flam\u00b7men", "k\u00fch\u00b7len", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "PIAT", "PPOSAT", "NN", "VVINF", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und da sieht er sie! Am Fenster", "tokens": ["Und", "da", "sieht", "er", "sie", "!", "Am", "Fens\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PPER", "$.", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tritt hervor sie, wie die Sonne", "tokens": ["Tritt", "her\u00b7vor", "sie", ",", "wie", "die", "Son\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PTKVZ", "PPER", "$,", "PWAV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Aufgeht in dem Ungewitter,", "tokens": ["Auf\u00b7geht", "in", "dem", "Un\u00b7ge\u00b7wit\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Wie der Mond im Dunkel aufgeht.", "tokens": ["Wie", "der", "Mond", "im", "Dun\u00b7kel", "auf\u00b7geht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}}, "stanza.4": {"line.1": {"text": "Leise tritt ihr Zaid n\u00e4her:", "tokens": ["Lei\u00b7se", "tritt", "ihr", "Zaid", "n\u00e4\u00b7her", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Alla mit dir, sch\u00f6ne Mohrin!", "tokens": ["Al\u00b7la", "mit", "dir", ",", "sch\u00f6\u00b7ne", "Moh\u00b7rin", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "APPR", "PPER", "$,", "ADJA", "NN", "$."], "meter": "-+--+---", "measure": "iambic.di.relaxed"}, "line.3": {"text": "Ist es wahr, was meine Pagen,", "tokens": ["Ist", "es", "wahr", ",", "was", "mei\u00b7ne", "Pa\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "$,", "PRELS", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Deine Dienerinnen sagen?", "tokens": ["Dei\u00b7ne", "Die\u00b7ne\u00b7rin\u00b7nen", "sa\u00b7gen", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Sagen: Du willt mich verlassen,", "tokens": ["Sa\u00b7gen", ":", "Du", "willt", "mich", "ver\u00b7las\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "PPER", "VMFIN", "PRF", "VVINF", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Wollest einem schn\u00f6den Mohren,", "tokens": ["Wol\u00b7lest", "ei\u00b7nem", "schn\u00f6\u00b7den", "Moh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Der von deines Vaters G\u00fctern", "tokens": ["Der", "von", "dei\u00b7nes", "Va\u00b7ters", "G\u00fc\u00b7tern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kaum noch ankam, dich verm\u00e4hlen?", "tokens": ["Kaum", "noch", "an\u00b7kam", ",", "dich", "ver\u00b7m\u00e4h\u00b7len", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "$,", "PRF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Ist es wahr, o sch\u00f6nste Zaida?", "tokens": ["Ist", "es", "wahr", ",", "o", "sch\u00f6ns\u00b7te", "Zai\u00b7da", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "$,", "FM", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sage mir es, t\u00e4usche mich nicht,", "tokens": ["Sa\u00b7ge", "mir", "es", ",", "t\u00e4u\u00b7sche", "mich", "nicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPER", "$,", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Wolle mir es nicht verhelen,", "tokens": ["Wol\u00b7le", "mir", "es", "nicht", "ver\u00b7he\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Was so laut ja alle wissen!", "tokens": ["Was", "so", "laut", "ja", "al\u00b7le", "wis\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADJD", "ADV", "PIS", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Tiefgebeugt erwiedert Zaida:", "tokens": ["Tief\u00b7ge\u00b7beugt", "er\u00b7wie\u00b7dert", "Zai\u00b7da", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "NE", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ja, mein Guter, es ist Zeit nun,", "tokens": ["Ja", ",", "mein", "Gu\u00b7ter", ",", "es", "ist", "Zeit", "nun", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPOSAT", "NN", "$,", "PPER", "VAFIN", "NN", "ADV", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Da\u00df sich dein' und meine Freundschaft", "tokens": ["Da\u00df", "sich", "dein'", "und", "mei\u00b7ne", "Freund\u00b7schaft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "PPOSAT", "KON", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Trenne, weil es alle wissen.", "tokens": ["Tren\u00b7ne", ",", "weil", "es", "al\u00b7le", "wis\u00b7sen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "PIS", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Um und an bin ich verlohren,", "tokens": ["Um", "und", "an", "bin", "ich", "ver\u00b7loh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "KON", "APPR", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn die Sache weiter fortgeht,", "tokens": ["Wenn", "die", "Sa\u00b7che", "wei\u00b7ter", "fort\u00b7geht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Alla wei\u00df, wie es mich schmerzet,", "tokens": ["Al\u00b7la", "wei\u00df", ",", "wie", "es", "mich", "schmer\u00b7zet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "PWAV", "PPER", "PRF", "VVFIN", "$,"], "meter": "+-++--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Wies mich dr\u00fccket, dich zu lassen.", "tokens": ["Wies", "mich", "dr\u00fc\u00b7cket", ",", "dich", "zu", "las\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "PPER", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Du weist wohl, wie ich dich liebte,", "tokens": ["Du", "weist", "wohl", ",", "wie", "ich", "dich", "lieb\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PWAV", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Troz des Widerspruchs der Meinen,", "tokens": ["Troz", "des", "Wi\u00b7der\u00b7spruchs", "der", "Mei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Weist, was ich mit meiner Mutter", "tokens": ["Weist", ",", "was", "ich", "mit", "mei\u00b7ner", "Mut\u00b7ter"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "$,", "PWS", "PPER", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "F\u00fcr Verdru\u00df und Kummer hatte,", "tokens": ["F\u00fcr", "Ver\u00b7dru\u00df", "und", "Kum\u00b7mer", "hat\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Wenn ich dich zu Nacht erharrte,", "tokens": ["Wenn", "ich", "dich", "zu", "Nacht", "er\u00b7harr\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Harrte, dich noch sp\u00e4t zu sehen;", "tokens": ["Harr\u00b7te", ",", "dich", "noch", "sp\u00e4t", "zu", "se\u00b7hen", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPER", "ADV", "ADJD", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dies auf Einmal mir zu enden,", "tokens": ["Dies", "auf", "Ein\u00b7mal", "mir", "zu", "en\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ADV", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wollen sie jezt \u2013 mich verm\u00e4hlen.", "tokens": ["Wol\u00b7len", "sie", "jezt", "\u2013", "mich", "ver\u00b7m\u00e4h\u00b7len", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$(", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Bald wird eine andre Dame", "tokens": ["Bald", "wird", "ei\u00b7ne", "and\u00b7re", "Da\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sch\u00f6n und artig dein seyn, Zaid,", "tokens": ["Sch\u00f6n", "und", "ar\u00b7tig", "dein", "seyn", ",", "Zaid", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "KON", "ADJD", "PPOSAT", "VAINF", "$,", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die dich liebet, die du liebest,", "tokens": ["Die", "dich", "lie\u00b7bet", ",", "die", "du", "lie\u00b7best", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Weil du es verdienst, o Zaid.", "tokens": ["Weil", "du", "es", "ver\u00b7dienst", ",", "o", "Zaid", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "$,", "FM", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Tiefgebeugt der Mohr erwiedert,", "tokens": ["Tief\u00b7ge\u00b7beugt", "der", "Mohr", "er\u00b7wie\u00b7dert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hingedr\u00fcckt von tausend Kummer:", "tokens": ["Hin\u00b7ge\u00b7dr\u00fcckt", "von", "tau\u00b7send", "Kum\u00b7mer", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "CARD", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00bbnicht versteh' ichs, sch\u00f6ne Zaida,", "tokens": ["\u00bb", "nicht", "ver\u00b7steh'", "ichs", ",", "sch\u00f6\u00b7ne", "Zai\u00b7da", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PTKNEG", "VVFIN", "PIS", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie du mit mir also handelst?", "tokens": ["Wie", "du", "mit", "mir", "al\u00b7so", "han\u00b7delst", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "PPER", "ADV", "VAFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Nicht versteh' ichs, wie du also", "tokens": ["Nicht", "ver\u00b7steh'", "ichs", ",", "wie", "du", "al\u00b7so"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKNEG", "VVFIN", "PIS", "$,", "PWAV", "PPER", "ADV"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Wechselst meine treue Liebe?", "tokens": ["Wech\u00b7selst", "mei\u00b7ne", "treu\u00b7e", "Lie\u00b7be", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Einem h\u00e4\u00dflich schlechten Mohren,", "tokens": ["Ei\u00b7nem", "h\u00e4\u00df\u00b7lich", "schlech\u00b7ten", "Moh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der so grossen Guts nicht werth ist.", "tokens": ["Der", "so", "gros\u00b7sen", "Guts", "nicht", "werth", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "PTKNEG", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Warst du's, die auf dieser Stelle", "tokens": ["Warst", "du's", ",", "die", "auf", "die\u00b7ser", "Stel\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "PIS", "$,", "PRELS", "APPR", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zu mir sprach, noch jenen Abend?", "tokens": ["Zu", "mir", "sprach", ",", "noch", "je\u00b7nen", "A\u00b7bend", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "$,", "ADV", "PDAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dein bin ich, dein bin ich ewig!", "tokens": ["Dein", "bin", "ich", ",", "dein", "bin", "ich", "e\u00b7wig", "!"], "token_info": ["word", "word", "word", "punct", "word", "word