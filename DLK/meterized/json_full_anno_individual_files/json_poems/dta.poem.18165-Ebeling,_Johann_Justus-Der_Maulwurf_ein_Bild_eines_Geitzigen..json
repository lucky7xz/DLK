{"dta.poem.18165": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Der Maulwurf  \n ein Bild eines Geitzigen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198805", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Maulwurf hatte sich im Garten einst", "tokens": ["Ein", "Maul\u00b7wurf", "hat\u00b7te", "sich", "im", "Gar\u00b7ten", "einst"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PRF", "APPRART", "NN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "verkrochen,", "tokens": ["ver\u00b7kro\u00b7chen", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Und durch sein blind Gew\u00fchl viel H\u00fcgel", "tokens": ["Und", "durch", "sein", "blind", "Ge\u00b7w\u00fchl", "viel", "H\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "ADJD", "NN", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "aufgeh\u00e4uft,", "tokens": ["auf\u00b7ge\u00b7h\u00e4uft", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Als ihn des G\u00e4rtners Schwur den j\u00e4hen Tod ge-", "tokens": ["Als", "ihn", "des", "G\u00e4rt\u00b7ners", "Schwur", "den", "j\u00e4\u00b7hen", "Tod", "ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "sprochen,", "tokens": ["spro\u00b7chen", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.7": {"text": "Dem er die Frucht verdarb eh sie zum Nutz gereift,", "tokens": ["Dem", "er", "die", "Frucht", "ver\u00b7darb", "eh", "sie", "zum", "Nutz", "ge\u00b7reift", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVFIN", "KOUS", "PPER", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Er lief bald hie bald da in dem verborgnen G\u00e4ngen,", "tokens": ["Er", "lief", "bald", "hie", "bald", "da", "in", "dem", "ver\u00b7borg\u00b7nen", "G\u00e4n\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ADV", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Um sich in lukrer Erd noch weiter durch zu drengen.", "tokens": ["Um", "sich", "in", "luk\u00b7rer", "Erd", "noch", "wei\u00b7ter", "durch", "zu", "dren\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PRF", "APPR", "ADJA", "NN", "ADV", "ADV", "APPR", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Doch da er sich mit M\u00fch stets immer weiter", "tokens": ["Doch", "da", "er", "sich", "mit", "M\u00fch", "stets", "im\u00b7mer", "wei\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "PRF", "APPR", "NN", "ADV", "ADV", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "brachte,", "tokens": ["brach\u00b7te", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.12": {"text": "Mit fressender Begier nach denen W\u00fcrmen nagt,", "tokens": ["Mit", "fres\u00b7sen\u00b7der", "Be\u00b7gier", "nach", "de\u00b7nen", "W\u00fcr\u00b7men", "nagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "PRELS", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Und wieder abermahl geworfne H\u00fcgel machte,", "tokens": ["Und", "wie\u00b7der", "a\u00b7ber\u00b7mahl", "ge\u00b7worf\u00b7ne", "H\u00fc\u00b7gel", "mach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Ward seine R\u00e4uberei dem G\u00e4rtner angesagt:", "tokens": ["Ward", "sei\u00b7ne", "R\u00e4u\u00b7be\u00b7rei", "dem", "G\u00e4rt\u00b7ner", "an\u00b7ge\u00b7sagt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Er kam und stach ihn aus, wo er sich aufgeschmissen,", "tokens": ["Er", "kam", "und", "stach", "ihn", "aus", ",", "wo", "er", "sich", "auf\u00b7ge\u00b7schmis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PPER", "PTKVZ", "$,", "PWAV", "PPER", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und muste auf dem Klump zugleich sein Leben schliessen.", "tokens": ["Und", "mus\u00b7te", "auf", "dem", "Klump", "zu\u00b7gleich", "sein", "Le\u00b7ben", "schlies\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "APPR", "ART", "NN", "ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Das ist ein klares Bild von Menschen die stets", "tokens": ["Das", "ist", "ein", "kla\u00b7res", "Bild", "von", "Men\u00b7schen", "die", "stets"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "APPR", "NN", "ART", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.18": {"text": "w\u00fchlen,", "tokens": ["w\u00fch\u00b7len", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.19": {"text": "Nach eitlen Jrdischen; sie h\u00e4uffen Geld auf Geld:", "tokens": ["Nach", "eit\u00b7len", "Jr\u00b7di\u00b7schen", ";", "sie", "h\u00e4uf\u00b7fen", "Geld", "auf", "Geld", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$.", "PPER", "ADJD", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Und da die Geitzigen nach diesen Kothe zielen,", "tokens": ["Und", "da", "die", "Geit\u00b7zi\u00b7gen", "nach", "die\u00b7sen", "Ko\u00b7the", "zie\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "APPR", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Verdirbt ihr blinder Trieb die Wollfart dieser Welt,", "tokens": ["Ver\u00b7dirbt", "ihr", "blin\u00b7der", "Trieb", "die", "Woll\u00b7fart", "die\u00b7ser", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "ART", "NN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Der HErr, der oben wohnt, der sieht auf ihr Verderben,", "tokens": ["Der", "Herr", ",", "der", "o\u00b7ben", "wohnt", ",", "der", "sieht", "auf", "ihr", "Ver\u00b7der\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "VVFIN", "$,", "PRELS", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Und l\u00e4st den Geitzigen als einen Maulwurf sterben.", "tokens": ["Und", "l\u00e4st", "den", "Geit\u00b7zi\u00b7gen", "als", "ei\u00b7nen", "Maul\u00b7wurf", "ster\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "KOKOM", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Er pflegt die Sterblichen, die alles zu sich raffen,", "tokens": ["Er", "pflegt", "die", "Sterb\u00b7li\u00b7chen", ",", "die", "al\u00b7les", "zu", "sich", "raf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PRELS", "PIS", "APPR", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Und denen gelber Koth des Lebens Element,", "tokens": ["Und", "de\u00b7nen", "gel\u00b7ber", "Koth", "des", "Le\u00b7bens", "E\u00b7le\u00b7ment", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "ADJA", "NN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Wenn sie genug geh\u00e4ufft, damit auch zu bestraffen,", "tokens": ["Wenn", "sie", "ge\u00b7nug", "ge\u00b7h\u00e4ufft", ",", "da\u00b7mit", "auch", "zu", "be\u00b7straf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVPP", "$,", "KOUS", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Da\u00df sich ihr Lebens Ziel bei ihren Klumpen endt:", "tokens": ["Da\u00df", "sich", "ihr", "Le\u00b7bens", "Ziel", "bei", "ih\u00b7ren", "Klum\u00b7pen", "endt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "PPOSAT", "NN", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Denn sie darum gescharrt in ihren ganzen Leben,", "tokens": ["Denn", "sie", "da\u00b7rum", "ge\u00b7scharrt", "in", "ih\u00b7ren", "gan\u00b7zen", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PAV", "VVPP", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Damit sie drauf mit Qual zuletzt den Geist aufgeben.", "tokens": ["Da\u00b7mit", "sie", "drauf", "mit", "Qual", "zu\u00b7letzt", "den", "Geist", "auf\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PAV", "APPR", "NN", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}