{"dta.poem.18703": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "H\u00f6chste Gunst .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Ungez\u00e4hmt so wie ich war", "tokens": ["Un\u00b7ge\u00b7z\u00e4hmt", "so", "wie", "ich", "war"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "ADV", "KOKOM", "PPER", "VAFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Hab\u2019 ich einen Herrn gefunden,", "tokens": ["Hab'", "ich", "ei\u00b7nen", "Herrn", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und gez\u00e4hmt nach manchem Jahr", "tokens": ["Und", "ge\u00b7z\u00e4hmt", "nach", "man\u00b7chem", "Jahr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Eine Herrin auch gefunden.", "tokens": ["Ei\u00b7ne", "Her\u00b7rin", "auch", "ge\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Da sie Pr\u00fcfung nicht gespart", "tokens": ["Da", "sie", "Pr\u00fc\u00b7fung", "nicht", "ge\u00b7spart"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "NN", "PTKNEG", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Haben sie mich treu gefunden,", "tokens": ["Ha\u00b7ben", "sie", "mich", "treu", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PRF", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und mit Sorgfalt mich bewahrt", "tokens": ["Und", "mit", "Sorg\u00b7falt", "mich", "be\u00b7wahrt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "PPER", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Als den Schatz den sie gefunden.", "tokens": ["Als", "den", "Schatz", "den", "sie", "ge\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Niemand diente zweyen Herrn", "tokens": ["Nie\u00b7mand", "dien\u00b7te", "zwe\u00b7yen", "Herrn"], "token_info": ["word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "VVFIN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Der dabey sein Gl\u00fcck gefunden;", "tokens": ["Der", "da\u00b7bey", "sein", "Gl\u00fcck", "ge\u00b7fun\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PAV", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Herr und Herrin sehn es gern", "tokens": ["Herr", "und", "Her\u00b7rin", "sehn", "es", "gern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "VVFIN", "PPER", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Dass sie beyde mich gefunden,", "tokens": ["Dass", "sie", "bey\u00b7de", "mich", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "PRF", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Und mir leuchtet Gl\u00fcck und Stern", "tokens": ["Und", "mir", "leuch\u00b7tet", "Gl\u00fcck", "und", "Stern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "NN", "KON", "NN"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.14": {"text": "Da ich beyde Sie gefunden.", "tokens": ["Da", "ich", "bey\u00b7de", "Sie", "ge\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}