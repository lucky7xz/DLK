{"dta.poem.8158": {"metadata": {"author": {"name": "Storm, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Abends.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1852", "urn": "urn:nbn:de:kobv:b4-200905196098", "language": ["de:0.99"], "booktitle": "Storm, Theodor: Gedichte. Kiel, 1852."}, "poem": {"stanza.1": {"line.1": {"text": "Nacht?", "tokens": ["Nacht", "?"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Warum brennen deine Lippen so viel r\u00f6ther bei", "tokens": ["Wa\u00b7rum", "bren\u00b7nen", "dei\u00b7ne", "Lip\u00b7pen", "so", "viel", "r\u00f6\u00b7ther", "bei"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPOSAT", "NN", "ADV", "ADV", "ADJD", "APPR"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.3": {"text": "der Nacht?", "tokens": ["der", "Nacht", "?"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "Warum ist in meinem Herzen so die Sehnsucht", "tokens": ["Wa\u00b7rum", "ist", "in", "mei\u00b7nem", "Her\u00b7zen", "so", "die", "Sehn\u00b7sucht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "APPR", "PPOSAT", "NN", "ADV", "ART", "NN"], "meter": "+-+-+-+--++-", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "auferwacht,", "tokens": ["au\u00b7fer\u00b7wacht", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Diese brennend rothen Lippen dir zu k\u00fcssen bei", "tokens": ["Die\u00b7se", "bren\u00b7nend", "ro\u00b7then", "Lip\u00b7pen", "dir", "zu", "k\u00fcs\u00b7sen", "bei"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDAT", "ADJD", "ADJA", "NN", "PPER", "PTKZU", "VVINF", "APPR"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.s