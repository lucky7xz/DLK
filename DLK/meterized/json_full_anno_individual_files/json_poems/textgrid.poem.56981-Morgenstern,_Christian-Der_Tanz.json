{"textgrid.poem.56981": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Der Tanz", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Vierviertelschwein und eine Auftakteule", "tokens": ["Ein", "Vier\u00b7vier\u00b7tel\u00b7schwein", "und", "ei\u00b7ne", "Auf\u00b7tak\u00b7teu\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "trafen sich im Schatten einer S\u00e4ule,", "tokens": ["tra\u00b7fen", "sich", "im", "Schat\u00b7ten", "ei\u00b7ner", "S\u00e4u\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "die im Geiste ihres Sch\u00f6pfers stand.", "tokens": ["die", "im", "Geis\u00b7te", "ih\u00b7res", "Sch\u00f6p\u00b7fers", "stand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Und zum Spiel der Fiedelbogenpflanze", "tokens": ["Und", "zum", "Spiel", "der", "Fie\u00b7del\u00b7bo\u00b7gen\u00b7pflan\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "reichten sich die zwei zum Tanze", "tokens": ["reich\u00b7ten", "sich", "die", "zwei", "zum", "Tan\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "ART", "CARD", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Fu\u00df und Hand.", "tokens": ["Fu\u00df", "und", "Hand", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Und auf seinen dreien rosa Beinen", "tokens": ["Und", "auf", "sei\u00b7nen", "drei\u00b7en", "ro\u00b7sa", "Bei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "CARD", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "h\u00fcpfte das Vierviertelschwein grazi\u00f6s,", "tokens": ["h\u00fcpf\u00b7te", "das", "Vier\u00b7vier\u00b7tel\u00b7schwein", "gra\u00b7zi\u00b7\u00f6s", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "+--+---+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "und die Auftakteul auf ihrem einen", "tokens": ["und", "die", "Auf\u00b7tak\u00b7teul", "auf", "ih\u00b7rem", "ei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "PPOSAT", "ART"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "wiegte rhythmisch ihr Gekr\u00f6s.", "tokens": ["wieg\u00b7te", "rhyth\u00b7misch", "ihr", "Ge\u00b7kr\u00f6s", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und der Schatten fiel,", "tokens": ["Und", "der", "Schat\u00b7ten", "fiel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$,"], "meter": "--+-+", "measure": "anapaest.init"}, "line.6": {"text": "und der Pflanze Spiel", "tokens": ["und", "der", "Pflan\u00b7ze", "Spiel"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "klang verwirrend melodi\u00f6s.", "tokens": ["klang", "ver\u00b7wir\u00b7rend", "m