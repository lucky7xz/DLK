{"dta.poem.13514": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Heimweh .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.85", "af:0.14"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Wer in die Fremde will wandern,", "tokens": ["Wer", "in", "die", "Frem\u00b7de", "will", "wan\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "VMFIN", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Der mu\u00df mit der Liebsten gehn,", "tokens": ["Der", "mu\u00df", "mit", "der", "Liebs\u00b7ten", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Es jubeln und lassen die Andern", "tokens": ["Es", "ju\u00b7beln", "und", "las\u00b7sen", "die", "An\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVINF", "KON", "VVFIN", "ART", "ADJA"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Den Fremden alleine stehn.", "tokens": ["Den", "Frem\u00b7den", "al\u00b7lei\u00b7ne", "stehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVINF", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Was wisset Ihr, dunkele Wipfeln,", "tokens": ["Was", "wis\u00b7set", "Ihr", ",", "dun\u00b7ke\u00b7le", "Wip\u00b7feln", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Von der alten sch\u00f6nen Zeit?", "tokens": ["Von", "der", "al\u00b7ten", "sch\u00f6\u00b7nen", "Zeit", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach, die Heimath hinter den Gipfeln,", "tokens": ["Ach", ",", "die", "Hei\u00b7math", "hin\u00b7ter", "den", "Gip\u00b7feln", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Wie liegt sie von hier so weit.", "tokens": ["Wie", "liegt", "sie", "von", "hier", "so", "weit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "APPR", "ADV", "ADV", "ADJD", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Am liebsten betracht' ich die Sterne,", "tokens": ["Am", "liebs\u00b7ten", "be\u00b7tracht'", "ich", "die", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Die schienen, wenn ich ging zu ihr,", "tokens": ["Die", "schie\u00b7nen", ",", "wenn", "ich", "ging", "zu", "ihr", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "KOUS", "PPER", "VVFIN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Nachtigall h\u00f6r' ich so gerne,", "tokens": ["Die", "Nach\u00b7ti\u00b7gall", "h\u00f6r'", "ich", "so", "ger\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sie sang vor der Liebsten Th\u00fcr.", "tokens": ["Sie", "sang", "vor", "der", "Liebs\u00b7ten", "Th\u00fcr."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.4": {"line.1": {"text": "Der Morgen, das ist meine Freude!", "tokens": ["Der", "Mor\u00b7gen", ",", "das", "ist", "mei\u00b7ne", "Freu\u00b7de", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PDS", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da steig' ich in stiller Stund'", "tokens": ["Da", "steig'", "ich", "in", "stil\u00b7ler", "Stund'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Auf den h\u00f6chsten Berg in die Weite,", "tokens": ["Auf", "den", "h\u00f6chs\u00b7ten", "Berg", "in", "die", "Wei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Gr\u00fc\u00df Dich Deutschland aus Herzensgrund!", "tokens": ["Gr\u00fc\u00df", "Dich", "Deutschland", "aus", "Her\u00b7zens\u00b7grund", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "NE", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}