{"dta.poem.9904": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "13.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ob man mich schon nicht gesehen/ so wei\u00df ich doch gar gewi\u00df/", "tokens": ["Ob", "man", "mich", "schon", "nicht", "ge\u00b7se\u00b7hen", "/", "so", "wei\u00df", "ich", "doch", "gar", "ge\u00b7wi\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PRF", "ADV", "PTKNEG", "VVPP", "$(", "ADV", "VVFIN", "PPER", "ADV", "ADV", "ADV", "$("], "meter": "+-+-+-+--+--+-+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Da\u00df ein jeder mich sehr liebet/ und verlanget eben di\u00df.", "tokens": ["Da\u00df", "ein", "je\u00b7der", "mich", "sehr", "lie\u00b7bet", "/", "und", "ver\u00b7lan\u00b7get", "e\u00b7ben", "di\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "PIS", "PPER", "ADV", "VVFIN", "$(", "KON", "VVFIN", "ADV", "PDS", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Man kan ohne mich nicht seyn/ ohne mich kan niemand leben/", "tokens": ["Man", "kan", "oh\u00b7ne", "mich", "nicht", "seyn", "/", "oh\u00b7ne", "mich", "kan", "nie\u00b7mand", "le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "APPR", "PPER", "PTKNEG", "VAINF", "$(", "APPR", "PPER", "VMFIN", "PIS", "VVINF", "$("], "meter": "+-+--+-+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Eher pfleg ich mich nicht weg/ bi\u00df ich recht verliebt/ zu geben.", "tokens": ["E\u00b7her", "pfleg", "ich", "mich", "nicht", "weg", "/", "bi\u00df", "ich", "recht", "ver\u00b7liebt", "/", "zu", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "PTKNEG", "PTKVZ", "$(", "KOUS", "PPER", "ADJD", "VVPP", "$(", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}}}}