{"dta.poem.10401": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "XxVII.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Du bliebest mir treu am l\u00e4ngsten,", "tokens": ["Du", "blie\u00b7best", "mir", "treu", "am", "l\u00e4ngs\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "APPRART", "ADJA", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und hast dich f\u00fcr mich verwendet,", "tokens": ["Und", "hast", "dich", "f\u00fcr", "mich", "ver\u00b7wen\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "APPR", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und hast mir Trost gespendet", "tokens": ["Und", "hast", "mir", "Trost", "ge\u00b7spen\u00b7det"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In meinen N\u00f6then und Aengsten.", "tokens": ["In", "mei\u00b7nen", "N\u00f6\u00b7then", "und", "A\u00b7engs\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Du gabest mir Trank und Speise,", "tokens": ["Du", "ga\u00b7best", "mir", "Trank", "und", "Spei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "KON", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und hast mir Geld geborget,", "tokens": ["Und", "hast", "mir", "Geld", "ge\u00b7bor\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und hast mich mit W\u00e4sche versorget,", "tokens": ["Und", "hast", "mich", "mit", "W\u00e4\u00b7sche", "ver\u00b7sor\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Und mit dem Pa\u00df f\u00fcr die Reise.", "tokens": ["Und", "mit", "dem", "Pa\u00df", "f\u00fcr", "die", "Rei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.3": {"line.1": {"text": "Mein Liebchen! da\u00df Gott dich beh\u00fcte,", "tokens": ["Mein", "Lieb\u00b7chen", "!", "da\u00df", "Gott", "dich", "be\u00b7h\u00fc\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "KOUS", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Noch lange, vor Hitz' und vor K\u00e4lte,", "tokens": ["Noch", "lan\u00b7ge", ",", "vor", "Hitz'", "und", "vor", "K\u00e4l\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Und da\u00df er dir nimmer vergelte", "tokens": ["Und", "da\u00df", "er", "dir", "nim\u00b7mer", "ver\u00b7gel\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "PPER", "ADV", "VVFIN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Die mir erwiesene G\u00fcte.", "tokens": ["Die", "mir", "er\u00b7wie\u00b7se\u00b7ne", "G\u00fc\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}}}}