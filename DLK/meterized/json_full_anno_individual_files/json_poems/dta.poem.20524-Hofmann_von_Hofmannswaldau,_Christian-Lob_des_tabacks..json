{"dta.poem.20524": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Lob des tabacks.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Rosen und violen", "tokens": ["Ro\u00b7sen", "und", "vi\u00b7o\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "M\u00f6gen kinder hohlen/", "tokens": ["M\u00f6\u00b7gen", "kin\u00b7der", "hoh\u00b7len", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Kinder dieser zeit.", "tokens": ["Kin\u00b7der", "die\u00b7ser", "zeit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PDAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Di\u00df/ was meinen sinn erfreut/", "tokens": ["Di\u00df", "/", "was", "mei\u00b7nen", "sinn", "er\u00b7freut", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$(", "PWS", "PPOSAT", "NN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und in meinen h\u00e4nden brennet/", "tokens": ["Und", "in", "mei\u00b7nen", "h\u00e4n\u00b7den", "bren\u00b7net", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "ADJA", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wird Taback genennet.", "tokens": ["Wird", "Ta\u00b7back", "ge\u00b7nen\u00b7net", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "VVPP", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Bisem mu\u00df dir weichen/", "tokens": ["Bi\u00b7sem", "mu\u00df", "dir", "wei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPER", "VVINF", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Dir ist nicht zu gleichen", "tokens": ["Dir", "ist", "nicht", "zu", "glei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PTKNEG", "PTKZU", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Ambra und zibeth;", "tokens": ["A\u00b7mbra", "und", "zi\u00b7beth", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "KON", "VVFIN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Wenn dein sch\u00f6ner rauch auffgeht/", "tokens": ["Wenn", "dein", "sch\u00f6\u00b7ner", "rauch", "auff\u00b7geht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und dein edles feuer blicket/", "tokens": ["Und", "dein", "ed\u00b7les", "feu\u00b7er", "bli\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "So bin ich erqvicket.", "tokens": ["So", "bin", "ich", "er\u00b7qvic\u00b7ket", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVFIN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Haben b\u00f6se zungen", "tokens": ["Ha\u00b7ben", "b\u00f6\u00b7se", "zun\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Viel von dir gesungen/", "tokens": ["Viel", "von", "dir", "ge\u00b7sun\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "VVPP", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Und dir beygewollt;", "tokens": ["Und", "dir", "bey\u00b7ge\u00b7wollt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Dieses ist nicht deine schuld.", "tokens": ["Die\u00b7ses", "ist", "nicht", "dei\u00b7ne", "schuld", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PTKNEG", "PPOSAT", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Bleibet doch/ der alles f\u00fchret/", "tokens": ["Blei\u00b7bet", "doch", "/", "der", "al\u00b7les", "f\u00fch\u00b7ret", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$(", "ART", "PIS", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Niemahls unber\u00fchret.", "tokens": ["Nie\u00b7mahls", "un\u00b7be\u00b7r\u00fch\u00b7ret", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADJD", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Di\u00df soll mich nicht treiben/", "tokens": ["Di\u00df", "soll", "mich", "nicht", "trei\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "PTKNEG", "VVINF", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Von dir weg zu bleiben/", "tokens": ["Von", "dir", "weg", "zu", "blei\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "PTKZU", "VVINF", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Wenn der p\u00f6bel sagt/", "tokens": ["Wenn", "der", "p\u00f6\u00b7bel", "sagt", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Da\u00df mir rauch und dampff behagt.", "tokens": ["Da\u00df", "mir", "rauch", "und", "dampff", "be\u00b7hagt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "KON", "PAV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Er/ sein geld und seine taschen", "tokens": ["Er", "/", "sein", "geld", "und", "sei\u00b7ne", "ta\u00b7schen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "$(", "PPOSAT", "NN", "KON", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wird zu rauch und ", "tokens": ["Wird", "zu", "rauch", "und"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PTKA", "ADJD", "KON"], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.5": {"line.1": {"text": "Nun so will ich trincken/", "tokens": ["Nun", "so", "will", "ich", "trin\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VMFIN", "PPER", "VVINF", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Weil die sternen blincken/", "tokens": ["Weil", "die", "ster\u00b7nen", "blin\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Und das grosse licht", "tokens": ["Und", "das", "gros\u00b7se", "licht"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Durch die d\u00fcstren wolcken bricht;", "tokens": ["Durch", "die", "d\u00fcst\u00b7ren", "wol\u00b7cken", "bricht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ja/ des Ph\u00f6bus g\u00f6ldner wagen", "tokens": ["Ja", "/", "des", "Ph\u00f6\u00b7bus", "g\u00f6ld\u00b7ner", "wa\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$(", "ART", "NN", "ADJA", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Soll mein rauchwerck tragen.", "tokens": ["Soll", "mein", "rauch\u00b7werck", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Venus wird nicht z\u00fcrnen/", "tokens": ["Ve\u00b7nus", "wird", "nicht", "z\u00fcr\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PTKNEG", "VVINF", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Wenn auff ihre stirnen", "tokens": ["Wenn", "auff", "ih\u00b7re", "stir\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PPOSAT", "ADJA"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Sich tabacks-rauch legt.", "tokens": ["Sich", "ta\u00b7backs\u00b7rauch", "legt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PRF", "NN", "VVFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Ward sie doch auch nicht bewegt/", "tokens": ["Ward", "sie", "doch", "auch", "nicht", "be\u00b7wegt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "PTKNEG", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wenn Vulcan/ das ungeheuer/", "tokens": ["Wenn", "Vul\u00b7can", "/", "das", "un\u00b7ge\u00b7heu\u00b7er", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "NE", "$(", "ART", "ADJD", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Machte rauch und feuer.", "tokens": ["Mach\u00b7te", "rauch", "und", "feu\u00b7er", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "KON", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "Und vor andern allen", "tokens": ["Und", "vor", "an\u00b7dern", "al\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PIS", "PIAT"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Wird der rauch gefallen", "tokens": ["Wird", "der", "rauch", "ge\u00b7fal\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ART", "ADJD", "VVPP"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Dir/ o krieges-gott.", "tokens": ["Dir", "/", "o", "krie\u00b7ge\u00b7sgott", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["PPER", "$(", "FM", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Drum hat es auch keine noth/", "tokens": ["Drum", "hat", "es", "auch", "kei\u00b7ne", "noth", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "ADV", "PIAT", "NN", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Wenn die sachen/ so wir \u00fcben/", "tokens": ["Wenn", "die", "sa\u00b7chen", "/", "so", "wir", "\u00fc\u00b7ben", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "$(", "ADV", "PPER", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Nur die g\u00f6tter lieben.", "tokens": ["Nur", "die", "g\u00f6t\u00b7ter", "lie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.8": {"line.1": {"text": "Nun/ ihr lieben br\u00fcder/", "tokens": ["Nun", "/", "ihr", "lie\u00b7ben", "br\u00fc\u00b7der", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "PPOSAT", "ADJA", "NN", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Thut/ was wein und lieder", "tokens": ["Thut", "/", "was", "wein", "und", "lie\u00b7der"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$(", "PWS", "PTKVZ", "KON", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Itzt hat angestimmt.", "tokens": ["Itzt", "hat", "an\u00b7ge\u00b7stimmt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Schaut! wie meine pfeiffe glimmt/", "tokens": ["Schaut", "!", "wie", "mei\u00b7ne", "pfeif\u00b7fe", "glimmt", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "PWAV", "PPOSAT", "NN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Da doch meiner liebsten sinnen", "tokens": ["Da", "doch", "mei\u00b7ner", "liebs\u00b7ten", "sin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Nicht so brennen k\u00f6nnen.", "tokens": ["Nicht", "so", "bren\u00b7nen", "k\u00f6n\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "VVINF", "VMINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}