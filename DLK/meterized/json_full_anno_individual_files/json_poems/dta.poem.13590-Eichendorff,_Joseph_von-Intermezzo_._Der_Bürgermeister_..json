{"dta.poem.13590": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Intermezzo .  \n  Der B\u00fcrgermeister .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Hochweiser Rath, geehrte Kollegen!", "tokens": ["Hoch\u00b7wei\u00b7ser", "Rath", ",", "ge\u00b7ehr\u00b7te", "Kol\u00b7le\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Bevor wir uns heut aufs Rathen legen", "tokens": ["Be\u00b7vor", "wir", "uns", "heut", "aufs", "Ra\u00b7then", "le\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "ADV", "APPRART", "NN", "VVINF"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Bitt' ich erst reiflich zu erw\u00e4gen:", "tokens": ["Bitt'", "ich", "erst", "reif\u00b7lich", "zu", "er\u00b7w\u00e4\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PPER", "ADV", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ob wir vielleicht, um Zeit zu gewinnen,", "tokens": ["Ob", "wir", "viel\u00b7leicht", ",", "um", "Zeit", "zu", "ge\u00b7win\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "$,", "KOUI", "NN", "PTKZU", "VVINF", "$,"], "meter": "---+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Heut sogleich mit dem Rathen beginnen,", "tokens": ["Heut", "sog\u00b7leich", "mit", "dem", "Ra\u00b7then", "be\u00b7gin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Oder ob wir erst proponiren m\u00fcssen,", "tokens": ["O\u00b7der", "ob", "wir", "erst", "pro\u00b7po\u00b7ni\u00b7ren", "m\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Was uns versammelt und was wir alle wissen? \u2014", "tokens": ["Was", "uns", "ver\u00b7sam\u00b7melt", "und", "was", "wir", "al\u00b7le", "wis\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "PPER", "VVPP", "KON", "PWS", "PPER", "PIS", "VVINF", "$.", "$("], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Ich mu\u00df pfiichtm\u00e4\u00dfig voranschicken hierbei,", "tokens": ["Ich", "mu\u00df", "pfiicht\u00b7m\u00e4\u00b7\u00dfig", "vor\u00b7an\u00b7schi\u00b7cken", "hier\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADJD", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.9": {"text": "Da\u00df die Art der Gesch\u00e4fte zweierlei sey:", "tokens": ["Da\u00df", "die", "Art", "der", "Ge\u00b7sch\u00e4f\u00b7te", "zwei\u00b7er\u00b7lei", "sey", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "ADV", "VAFIN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.10": {"text": "Die einen sind die eiligen,", "tokens": ["Die", "ei\u00b7nen", "sind", "die", "ei\u00b7li\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VAFIN", "ART", "ADJA", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Die andern die langweiligen.", "tokens": ["Die", "an\u00b7dern", "die", "lang\u00b7wei\u00b7li\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ART", "ADJA", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Auf jene pfleg' ich ", "tokens": ["Auf", "je\u00b7ne", "pfleg'", "ich"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "PPER"], "meter": "-+-+-", "measure": "iambic.di"}, "line.13": {"text": "Die andern k\u00f6nnen liegen bleiben.", "tokens": ["Die", "an\u00b7dern", "k\u00f6n\u00b7nen", "lie\u00b7gen", "blei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VMFIN", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Die Liegenden aber, geehrte Br\u00fcder,", "tokens": ["Die", "Lie\u00b7gen\u00b7den", "a\u00b7ber", ",", "ge\u00b7ehr\u00b7te", "Br\u00fc\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "ADJA", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.15": {"text": "Zerfallen in wicht'ge und in h\u00f6chstwicht'ge wieder.", "tokens": ["Zer\u00b7fal\u00b7len", "in", "wicht'\u00b7ge", "und", "in", "h\u00f6chst\u00b7wicht'\u00b7ge", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "KON", "APPR", "ADJA", "ADV", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.16": {"text": "Bei jenen \u2014 nun \u2014 man wird verwegen,", "tokens": ["Bei", "je\u00b7nen", "nun", "man", "wird", "ver\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "$(", "ADV", "$(", "PIS", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Man schreibt nach amtlichem Ueberlegen", "tokens": ["Man", "schreibt", "nach", "amt\u00b7li\u00b7chem", "Ue\u00b7ber\u00b7le\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.18": {"text": "More solito hier, und dort ad acta,", "tokens": ["Mo\u00b7re", "so\u00b7li\u00b7to", "hier", ",", "und", "dort", "ad", "ac\u00b7ta", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADV", "$,", "KON", "ADV", "FM", "FM", "$,"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.19": {"text": "Die Diener rennen, man flucht, verpakt da,", "tokens": ["Die", "Die\u00b7ner", "ren\u00b7nen", ",", "man", "flucht", ",", "ver\u00b7pakt", "da", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PIS", "VVFIN", "$,", "VVFIN", "ADV", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.20": {"text": "Der Staat florirt und bleibt im Takt da.", "tokens": ["Der", "Staat", "flo\u00b7rirt", "und", "bleibt", "im", "Takt", "da", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Doch werden die Zeiten so ungeschliffen,", "tokens": ["Doch", "wer\u00b7den", "die", "Zei\u00b7ten", "so", "un\u00b7ge\u00b7schlif\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.22": {"text": "Wild umzuspringen mit den Begriffen,", "tokens": ["Wild", "um\u00b7zu\u00b7sprin\u00b7gen", "mit", "den", "Be\u00b7grif\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.23": {"text": "Kommt gar, wie heute, ein Fall, der eilig", "tokens": ["Kommt", "gar", ",", "wie", "heu\u00b7te", ",", "ein", "Fall", ",", "der", "ei\u00b7lig"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "ADV", "$,", "PWAV", "ADV", "$,", "ART", "NN", "$,", "PRELS", "ADJD"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.24": {"text": "Und doch h\u00f6chstwichtig zugleich \u2014 dann freilich", "tokens": ["Und", "doch", "h\u00f6chst\u00b7wich\u00b7tig", "zu\u00b7gleich", "dann", "frei\u00b7lich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADV", "ADJD", "ADV", "$(", "ADV", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Mu\u00df man von neuem unterscheiden:", "tokens": ["Mu\u00df", "man", "von", "neu\u00b7em", "un\u00b7ter\u00b7schei\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "APPR", "ADJA", "VVINF", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.26": {"text": "Ob er mehr eilig oder mehr wichtig. \u2014", "tokens": ["Ob", "er", "mehr", "ei\u00b7lig", "o\u00b7der", "mehr", "wich\u00b7tig", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "KON", "ADV", "ADJD", "$.", "$("], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.27": {"text": "Ich bitte, meine Herrn, verstehn sie mich richtig!", "tokens": ["Ich", "bit\u00b7te", ",", "mei\u00b7ne", "Herrn", ",", "ver\u00b7stehn", "sie", "mich", "rich\u00b7tig", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPOSAT", "NN", "$,", "VVFIN", "PPER", "PRF", "ADJD", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Der Punkt ist von Einflu\u00df. Denn wir vermeiden", "tokens": ["Der", "Punkt", "ist", "von", "Ein\u00b7flu\u00df", ".", "Denn", "wir", "ver\u00b7mei\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "APPR", "NN", "$.", "KON", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Find't sich der Fall mehr eilig als liegend.", "tokens": ["Find't", "sich", "der", "Fall", "mehr", "ei\u00b7lig", "als", "lie\u00b7gend", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "ADV", "ADJD", "KOKOM", "VVPP", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Ist aber das Wichtige \u00fcberwiegend,", "tokens": ["Ist", "a\u00b7ber", "das", "Wich\u00b7ti\u00b7ge", "\u00fc\u00b7berw\u00b7ie\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "W\u00e4re die Eile am unrechten Ort.", "tokens": ["W\u00e4\u00b7re", "die", "Ei\u00b7le", "am", "un\u00b7rech\u00b7ten", "Ort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.6": {"text": "Meine Herren, sie haben nun die Pr\u00e4missen,", "tokens": ["Mei\u00b7ne", "Her\u00b7ren", ",", "sie", "ha\u00b7ben", "nun", "die", "Pr\u00e4\u00b7mis\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPER", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+--+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.7": {"text": "Sie werden den Beschlu\u00df zu finden wissen.", "tokens": ["Sie", "wer\u00b7den", "den", "Be\u00b7schlu\u00df", "zu", "fin\u00b7den", "wis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PTKZU", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}