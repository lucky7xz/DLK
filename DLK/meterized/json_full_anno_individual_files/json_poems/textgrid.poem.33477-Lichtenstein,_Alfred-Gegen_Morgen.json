{"textgrid.poem.33477": {"metadata": {"author": {"name": "Lichtenstein, Alfred", "birth": "N.A.", "death": "N.A."}, "title": "Gegen Morgen", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was k\u00fcmmern mich die flinken Zeitungsjungen.", "tokens": ["Was", "k\u00fcm\u00b7mern", "mich", "die", "flin\u00b7ken", "Zei\u00b7tungs\u00b7jun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mich \u00e4ngstet nicht das Nahen versp\u00e4teter Autotiere.", "tokens": ["Mich", "\u00e4ngs\u00b7tet", "nicht", "das", "Na\u00b7hen", "ver\u00b7sp\u00e4\u00b7te\u00b7ter", "Au\u00b7to\u00b7tie\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+--+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Ich ruhe auf meinen schreitenden Beinen.", "tokens": ["Ich", "ru\u00b7he", "auf", "mei\u00b7nen", "schrei\u00b7ten\u00b7den", "Bei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Verregnet ist mein Gesicht.", "tokens": ["Ver\u00b7reg\u00b7net", "ist", "mein", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Gr\u00fcnliche Reste der Nacht", "tokens": ["Gr\u00fcn\u00b7li\u00b7che", "Res\u00b7te", "der", "Nacht"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "ART", "NN"], "meter": "+--+--+", 