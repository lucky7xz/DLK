{"dta.poem.18132": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Elisabethe geborene Kunradinne.  \n Du bist Helena, gar eben eine  \n Krone.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-200905198432", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wer will bekennen nicht, Jungfrau, da\u00df in dir wohne", "tokens": ["Wer", "will", "be\u00b7ken\u00b7nen", "nicht", ",", "Jung\u00b7frau", ",", "da\u00df", "in", "dir", "woh\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "VVINF", "PTKNEG", "$,", "NN", "$,", "KOUS", "APPR", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Tugend Ebenbild, der Spiegel aller Zier,", "tokens": ["Der", "Tu\u00b7gend", "E\u00b7ben\u00b7bild", ",", "der", "Spie\u00b7gel", "al\u00b7ler", "Zier", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die G\u00f6ttin Venus selbst an Sitten weichet dir,", "tokens": ["Die", "G\u00f6t\u00b7tin", "Ve\u00b7nus", "selbst", "an", "Sit\u00b7ten", "wei\u00b7chet", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADV", "APPR", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ja du bist Helena, gar eben eine Krone.", "tokens": ["Ja", "du", "bist", "He\u00b7le\u00b7na", ",", "gar", "e\u00b7ben", "ei\u00b7ne", "Kro\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PPER", "VAFIN", "NE", "$,", "ADV", "ADV",