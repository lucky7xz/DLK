{"textgrid.poem.40783": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: \u00bbdichter, bleib' bei deinen Blumen! Nicht an Thronen frech gemeistert! \u2013", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbdichter, bleib' bei deinen Blumen! Nicht an Thronen frech gemeistert! \u2013", "tokens": ["\u00bb", "dich\u00b7ter", ",", "bleib'", "bei", "dei\u00b7nen", "Blu\u00b7men", "!", "Nicht", "an", "Thro\u00b7nen", "frech", "ge\u00b7meis\u00b7tert", "!", "\u2013"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADJD", "$,", "VVFIN", "APPR", "PPOSAT", "NN", "$.", "PTKNEG", "APPR", "NN", "ADJD", "VVPP", "$.", "$("], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Wenn dich mehr als Blumenkronen eines F\u00fcrsten Kron' begeistert,", "tokens": ["Wenn", "dich", "mehr", "als", "Blu\u00b7men\u00b7kro\u00b7nen", "ei\u00b7nes", "F\u00fcrs\u00b7ten", "Kron'", "be\u00b7geis\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "KOUS", "NN", "ART", "NN", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Feire, wie's so manch' bescheidner, vaterl\u00e4nd'scher S\u00e4nger thut,", "tokens": ["Fei\u00b7re", ",", "wie's", "so", "man\u00b7ch'", "be\u00b7scheid\u00b7ner", ",", "va\u00b7ter\u00b7l\u00e4n\u00b7d'\u00b7scher", "S\u00e4n\u00b7ger", "thut", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "ADV", "PIAT", "ADJA", "$,", "ADJA", "NN", "VVFIN", "$,"], "meter": "+--+-+-+-+-++-+-+", "measure": "iambic.octa.plus.invert"}, "line.4": {"text": "Hohe Fest- und Namenstage, huldigend mit Sangesgluth!\u00ab", "tokens": ["Ho\u00b7he", "Fest", "und", "Na\u00b7mens\u00b7ta\u00b7ge", ",", "hul\u00b7di\u00b7gend", "mit", "San\u00b7ges\u00b7gluth", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ADJA", "TRUNC", "KON", "NN", "$,", "ADJD", "APPR", "NN", "$.", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.2": {"line.1": {"text": "Hohn bed\u00fcnkt es mich, den F\u00fcrsten sonst zum Ruhme nichts zu singen,", "tokens": ["Hohn", "be\u00b7d\u00fcnkt", "es", "mich", ",", "den", "F\u00fcrs\u00b7ten", "sonst", "zum", "Ruh\u00b7me", "nichts", "zu", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PRF", "$,", "ART", "NN", "ADV", "APPRART", "NN", "PIS", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Als da\u00df sie geboren wurden, und auch Namen gar empfingen!", "tokens": ["Als", "da\u00df", "sie", "ge\u00b7bo\u00b7ren", "wur\u00b7den", ",", "und", "auch", "Na\u00b7men", "gar", "emp\u00b7fin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "VVPP", "VAFIN", "$,", "KON", "ADV", "NN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Buben m\u00f6gen solches r\u00fchmen! Aber schweigen la\u00dft mein Lied,", "tokens": ["Bu\u00b7ben", "m\u00f6\u00b7gen", "sol\u00b7ches", "r\u00fch\u00b7men", "!", "A\u00b7ber", "schwei\u00b7gen", "la\u00dft", "mein", "Lied", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PIS", "VVINF", "$.", "KON", "VVINF", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Bis es gro\u00dfe Thaten ragen, Licht und Freiheit strahlen sieht!", "tokens": ["Bis", "es", "gro\u00b7\u00dfe", "Tha\u00b7ten", "ra\u00b7gen", ",", "Licht", "und", "Frei\u00b7heit", "strah\u00b7len", "sieht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJA", "NN", "VVFIN", "$,", "NN", "KON", "NN", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.3": {"line.1": {"text": "\u00bbwie du doch so unertr\u00e4glich! Freiheit stets, und Freiheit wieder!", "tokens": ["\u00bb", "wie", "du", "doch", "so", "un\u00b7er\u00b7tr\u00e4g\u00b7lich", "!", "Frei\u00b7heit", "stets", ",", "und", "Frei\u00b7heit", "wie\u00b7der", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "PPER", "ADV", "ADV", "ADJD", "$.", "NN", "ADV", "$,", "KON", "NN", "ADV", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Stets dasselbe Liedlein leiernd! Kennst du sonst denn keine Lieder?", "tokens": ["Stets", "das\u00b7sel\u00b7be", "Lied\u00b7lein", "lei\u00b7ernd", "!", "Kennst", "du", "sonst", "denn", "kei\u00b7ne", "Lie\u00b7der", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDAT", "NN", "ADJD", "$.", "VVFIN", "PPER", "ADV", "ADV", "PIAT", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Willst du winseln nur und klagen, nimm dir doch ein andres Ziel!", "tokens": ["Willst", "du", "win\u00b7seln", "nur", "und", "kla\u00b7gen", ",", "nimm", "dir", "doch", "ein", "and\u00b7res", "Ziel", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVFIN", "ADV", "KON", "VVINF", "$,", "VVIMP", "PPER", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Suche andre Stoff' und Weisen, in der Welt ist Jammers viel!\u00ab", "tokens": ["Su\u00b7che", "and\u00b7re", "Stoff'", "und", "Wei\u00b7sen", ",", "in", "der", "Welt", "ist", "Jam\u00b7mers", "viel", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJA", "ADJA", "NN", "KON", "NN", "$,", "APPR", "ART", "NN", "VAFIN", "NN", "ADV", "$.", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.4": {"line.1": {"text": "Soll ich unser Land wohl schm\u00e4hen? O kein sch\u00f6n'res find' ich wieder!", "tokens": ["Soll", "ich", "un\u00b7ser", "Land", "wohl", "schm\u00e4\u00b7hen", "?", "O", "kein", "sch\u00f6n'\u00b7res", "find'", "ich", "wie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "ADV", "VVINF", "$.", "NE", "PIAT", "ADJA", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Soll ich unser Volk verl\u00e4stern? Das ist treu und gut und bieder!", "tokens": ["Soll", "ich", "un\u00b7ser", "Volk", "ver\u00b7l\u00e4s\u00b7tern", "?", "Das", "ist", "treu", "und", "gut", "und", "bie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$.", "PDS", "VAFIN", "ADJD", "KON", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Einen Fehl nur haben beide: da\u00df die Freiheit ihnen fehlt,", "tokens": ["Ei\u00b7nen", "Fehl", "nur", "ha\u00b7ben", "bei\u00b7de", ":", "da\u00df", "die", "Frei\u00b7heit", "ih\u00b7nen", "fehlt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "PIS", "$.", "KOUS", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Drob das Herz nur ", "tokens": ["Drob", "das", "Herz", "nur"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "ADV"], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.5": {"line.1": {"text": "\u00bbei, dein Schmerz sei dir gelassen! Doch was st\u00f6rest du die Andern,", "tokens": ["\u00bb", "ei", ",", "dein", "Schmerz", "sei", "dir", "ge\u00b7las\u00b7sen", "!", "Doch", "was", "st\u00f6\u00b7rest", "du", "die", "An\u00b7dern", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ITJ", "$,", "PPOSAT", "NN", "VAFIN", "PPER", "VVPP", "$.", "KON", "PWS", "VVFIN", "PPER", "ART", "ADJA", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Die zu euren sch\u00f6nen Bergen, duft'gen W\u00e4ldern fr\u00f6hlich wandern,", "tokens": ["Die", "zu", "eu\u00b7ren", "sch\u00f6\u00b7nen", "Ber\u00b7gen", ",", "duft'\u00b7gen", "W\u00e4l\u00b7dern", "fr\u00f6h\u00b7lich", "wan\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "ADJA", "NN", "$,", "ADJA", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "An der reifen Saat sich freuend, labend sich am goldnen Wein?", "tokens": ["An", "der", "rei\u00b7fen", "Saat", "sich", "freu\u00b7end", ",", "la\u00b7bend", "sich", "am", "gold\u00b7nen", "Wein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PRF", "VVPP", "$,", "ADJD", "PRF", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Was in ihren Jubel rasselst du mit unsern Ketten drein?\u00ab", "tokens": ["Was", "in", "ih\u00b7ren", "Ju\u00b7bel", "ras\u00b7selst", "du", "mit", "un\u00b7sern", "Ket\u00b7ten", "drein", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "APPR", "PPOSAT", "NN", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "PTKVZ", "$.", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.6": {"line.1": {"text": "Eben weil in solchem Jubel, zwischen solchem Bl\u00fcthenleben,", "tokens": ["E\u00b7ben", "weil", "in", "sol\u00b7chem", "Ju\u00b7bel", ",", "zwi\u00b7schen", "sol\u00b7chem", "Bl\u00fc\u00b7then\u00b7le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "APPR", "PIAT", "NN", "$,", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Zwischen goldner Saaten S\u00e4useln, zwischen Kr\u00e4nzen duft'ger Reben,", "tokens": ["Zwi\u00b7schen", "gold\u00b7ner", "Saa\u00b7ten", "S\u00e4u\u00b7seln", ",", "zwi\u00b7schen", "Kr\u00e4n\u00b7zen", "duft'\u00b7ger", "Re\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$,", "APPR", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Unter B\u00e4umen gr\u00fcn und laubig, unter Lerchen leichtbeschwingt,", "tokens": ["Un\u00b7ter", "B\u00e4u\u00b7men", "gr\u00fcn", "und", "lau\u00b7big", ",", "un\u00b7ter", "Ler\u00b7chen", "leicht\u00b7be\u00b7schwingt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "KON", "ADJD", "$,", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Das Gerassel arger Ketten gar so wunderschaurig klingt!", "tokens": ["Das", "Ge\u00b7ras\u00b7sel", "ar\u00b7ger", "Ket\u00b7ten", "gar", "so", "wun\u00b7der\u00b7schau\u00b7rig", "klingt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "ADV", "ADV", "ADJD", "VVFIN", "$."], "meter": "--+-+-+-+-+-+-+", "measure": "anapaest.init"}}, "stanza.7": {"line.1": {"text": "\u00bbdichter, bleib' bei deinen Blumen! Nicht an Thronen frech gemeistert! \u2013", "tokens": ["\u00bb", "dich\u00b7ter", ",", "bleib'", "bei", "dei\u00b7nen", "Blu\u00b7men", "!", "Nicht", "an", "Thro\u00b7nen", "frech", "ge\u00b7meis\u00b7tert", "!", "\u2013"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADJD", "$,", "VVFIN", "APPR", "PPOSAT", "NN", "$.", "PTKNEG", "APPR", "NN", "ADJD", "VVPP", "$.", "$("], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Wenn dich mehr als Blumenkronen eines F\u00fcrsten Kron' begeistert,", "tokens": ["Wenn", "dich", "mehr", "als", "Blu\u00b7men\u00b7kro\u00b7nen", "ei\u00b7nes", "F\u00fcrs\u00b7ten", "Kron'", "be\u00b7geis\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "KOUS", "NN", "ART", "NN", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Feire, wie's so manch' bescheidner, vaterl\u00e4nd'scher S\u00e4nger thut,", "tokens": ["Fei\u00b7re", ",", "wie's", "so", "man\u00b7ch'", "be\u00b7scheid\u00b7ner", ",", "va\u00b7ter\