{"textgrid.poem.34546": {"metadata": {"author": {"name": "Hartleben, Otto Erich", "birth": "N.A.", "death": "N.A."}, "title": "1L: So tauchte die Fackel empor aus tiefer, tiefer Nacht,", "genre": "verse", "period": "N.A.", "pub_year": 1897, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So tauchte die Fackel empor aus tiefer, tiefer Nacht,", "tokens": ["So", "tauch\u00b7te", "die", "Fa\u00b7ckel", "em\u00b7por", "aus", "tie\u00b7fer", ",", "tie\u00b7fer", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKVZ", "APPR", "ADJD", "$,", "ADJA", "NN", "$,"], "meter": "-+--+--+-+-+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "und meine Hand, die Linke, hielt den Fackelschaft.", "tokens": ["und", "mei\u00b7ne", "Hand", ",", "die", "Lin\u00b7ke", ",", "hielt", "den", "Fa\u00b7ckel\u00b7schaft", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "ART", "NN", "$,", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sie trug und hielt das Feuer hoch und liess den Sturm", "tokens": ["Sie", "trug", "und", "hielt", "das", "Feu\u00b7er", "hoch", "und", "liess", "den", "Sturm"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ART", "NN", "ADJD", "KON", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "getrost zerfetzen diese Flamme, die doch nimmer starb.", "tokens": ["ge\u00b7trost", "zer\u00b7fet\u00b7zen", "die\u00b7se", "Flam\u00b7me", ",", "die", "doch", "nim\u00b7mer", "starb", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PDAT", "NN", "$,", "PRELS", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.2": {"line.1": {"text": "Im Thale lagerte die kalte Finsternis.", "tokens": ["Im", "Tha\u00b7le", "la\u00b7ger\u00b7te", "die", "kal\u00b7te", "Fins\u00b7ter\u00b7nis", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Viel scheue Augen wachten da und sahen her", "tokens": ["Viel", "scheu\u00b7e", "Au\u00b7gen", "wach\u00b7ten", "da", "und", "sa\u00b7hen", "her"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "ADV", "KON", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "zu mir, weil ich die Leuchte in der Linken trug.", "tokens": ["zu", "mir", ",", "weil", "ich", "die", "Leuch\u00b7te", "in", "der", "Lin\u00b7ken", "trug", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "KOUS", "PPER", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Doch selber sah ich nur die M\u00fccken in dem roten Glanz,", "tokens": ["Doch", "sel\u00b7ber", "sah", "ich", "nur", "die", "M\u00fc\u00b7cken", "in", "dem", "ro\u00b7ten", "Glanz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.5": {"text": "die Eintagsfliegen, die sich flatternd in den Schein gedr\u00e4ngt \u2013", "tokens": ["die", "Ein\u00b7tags\u00b7flie\u00b7gen", ",", "die", "sich", "flat\u00b7ternd", "in", "den", "Schein", "ge\u00b7dr\u00e4ngt", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PRF", "VVPP", "APPR", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.6": {"text": "und ahnte nicht, wie meine Fackel drunten wohl", "tokens": ["und", "ahn\u00b7te", "nicht", ",", "wie", "mei\u00b7ne", "Fa\u00b7ckel", "drun\u00b7ten", "wohl"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PTKNEG", "$,", "PWAV", "PPOSAT", "NN", "ADV", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "weithin verg\u00f6ttert wurde wie ein Sternenlicht.", "tokens": ["weit\u00b7hin", "ver\u00b7g\u00f6t\u00b7tert", "wur\u00b7de", "wie", "ein", "Ster\u00b7nen\u00b7licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "VAFIN", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "So tauchte die Fackel empor aus tiefer, tiefer Nacht,", "tokens": ["So", "tauch\u00b7te", "die", "Fa\u00b7ckel", "em\u00b7por", "aus", "tie\u00b7fer", ",", "tie\u00b7fer", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKVZ", "APPR", "ADJD", "$,", "ADJA", "NN", "$,"], "meter": "-+--+--+-+-+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "und meine Hand, die Linke, hielt den Fackelschaft.", "tokens": ["und", "mei\u00b7ne", "Hand", ",", "die", "Lin\u00b7ke", ",", "hielt", "den", "Fa\u00b7ckel\u00b7schaft", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "ART", "NN", "$,", "VVFIN", "ART", "NN", "