{"dta.poem.15930": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "135.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Am Neujahrsmorgen merkt man wol auf Schicksalszeichen;", "tokens": ["Am", "Neu\u00b7jahrs\u00b7mor\u00b7gen", "merkt", "man", "wol", "auf", "Schick\u00b7sals\u00b7zei\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PIS", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Glaubt' ich den meinigen, so m\u00fc\u00dft' ich schon erbleichen.", "tokens": ["Glaubt'", "ich", "den", "mei\u00b7ni\u00b7gen", ",", "so", "m\u00fc\u00dft'", "ich", "schon", "er\u00b7blei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "PPOSS", "$,", "ADV", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ich schl\u00fcpft', als ich aufstand, verkehrt in mein Gewand;", "tokens": ["Ich", "schl\u00fcpft'", ",", "als", "ich", "auf\u00b7stand", ",", "ver\u00b7kehrt", "in", "mein", "Ge\u00b7wand", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "VVFIN", "$,", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als ich die Uhr nahm, fand ich da\u00df sie stille stand.", "tokens": ["Als", "ich", "die", "Uhr", "nahm", ",", "fand", "ich", "da\u00df", "sie", "stil\u00b7le", "stand", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,", "VVFIN", "PPER", "KOUS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "M\u00f6g' alles, was verkehrt ich dieses Jahr soll thun,", "tokens": ["M\u00f6g'", "al\u00b7les", ",", "was", "ver\u00b7kehrt", "ich", "die\u00b7ses", "Jahr", "soll", "thun", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "$,", "PWS", "VVFIN", "PPER", "PDAT", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So leicht wie dies Gewand seyn umzuwenden nun!", "tokens": ["So", "leicht", "wie", "dies", "Ge\u00b7wand", "seyn", "um\u00b7zu\u00b7wen\u00b7den", "nun", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "PDS", "NN", "VAINF", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Und wenn mir soll die Uhr des Lebens stille stehn,", "tokens": ["Und", "wenn", "mir", "soll", "die", "Uhr", "des", "Le\u00b7bens", "stil\u00b7le", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VMFIN", "ART", "NN", "ART", "NN", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "M\u00f6g' es so unvermerkt und sanft im Schlaf geschehn!", "tokens": ["M\u00f6g'", "es", "so", "un\u00b7ver\u00b7merkt", "und", "sanft", "im", "Schlaf", "ge\u00b7schehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADJD", "KON", "ADJD", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}