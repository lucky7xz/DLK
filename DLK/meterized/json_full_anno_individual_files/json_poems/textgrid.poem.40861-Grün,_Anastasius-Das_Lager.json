}, "line.2": {"text": "Kein m\u00fcrrisch, unwirsch Antlitz, rings freundlich jedes Gesicht;", "tokens": ["Kein", "m\u00fcr\u00b7risch", ",", "un\u00b7wirsch", "Ant\u00b7litz", ",", "rings", "freund\u00b7lich", "je\u00b7des", "Ge\u00b7sicht", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJD", "$,", "ADJD", "NN", "$,", "ADV", "ADJD", "PIAT", "NN", "$."], "meter": "-+-+-+--+-+--+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Ist's Wunderkraft des Bodens, dem Frieden sonst geweiht?", "tokens": ["Ist's", "Wun\u00b7der\u00b7kraft", "des", "Bo\u00b7dens", ",", "dem", "Frie\u00b7den", "sonst", "ge\u00b7weiht", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ART", "NN", "$,", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Kann sich das Herz nicht entw\u00f6hnen verfloss'ner sch\u00f6ner Zeit? \u2013", "tokens": ["Kann", "sich", "das", "Herz", "nicht", "ent\u00b7w\u00f6h\u00b7nen", "ver\u00b7floss'\u00b7ner", "sch\u00f6\u00b7ner", "Zeit", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "PRF", "ART", "NN", "PTKNEG", "ADJA", "ADJA", "ADJA", "NN", "$.", "$("], "meter": "+--+--+--+-+-+", "measure": "dactylic.tri.plus"}}, "stanza.18": {"line.1": {"text": "Wenn die Drommete rufet, klingt's fast wie Tanzmelodei;", "tokens": ["Wenn", "die", "Drom\u00b7me\u00b7te", "ru\u00b7fet", ",", "klingt's", "fast", "wie", "Tanz\u00b7me\u00b7lo\u00b7dei", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$,", "VVFIN", "ADV", "KOKOM", "NN", "$."], "meter": "--+--+-+--+--+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Und manchen Ritters Auge, de\u00df Herz sonst froh und frei,", "tokens": ["Und", "man\u00b7chen", "Rit\u00b7ters", "Au\u00b7ge", ",", "de\u00df", "Herz", "sonst", "froh", "und", "frei", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "NN", "$,", "ART", "NN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Beinah' wird's feucht, erblickt er am Helm den welken Strau\u00df,", "tokens": ["Bei\u00b7nah'", "wird's", "feucht", ",", "er\u00b7blickt", "er", "am", "Helm", "den", "wel\u00b7ken", "Strau\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADJD", "$,", "VVFIN", "PPER", "APPRART", "NN", "ART", "NN", "NE", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und will er ein Kriegslied brummen, flugs wird ein Brautlied draus!", "tokens": ["Und", "will", "er", "ein", "Kriegs\u00b7lied", "brum\u00b7men", ",", "flugs", "wird", "ein", "Braut\u00b7lied", "draus", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ART", "NN", "VVINF", "$,", "ADV", "VAFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.19": {"line.1": {"text": "Max selber, wenn er sinnend durchs Lager einsam wallt,", "tokens": ["Max", "sel\u00b7ber", ",", "wenn", "er", "sin\u00b7nend", "durchs", "La\u00b7ger", "ein\u00b7sam", "wallt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "$,", "KOUS", "PPER", "ADJD", "APPRART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Blickt seitw\u00e4rts oft, als z\u00f6ge mit ihm noch eine Gestalt;", "tokens": ["Blickt", "seit\u00b7w\u00e4rts", "oft", ",", "als", "z\u00f6\u00b7ge", "mit", "ihm", "noch", "ei\u00b7ne", "Ge\u00b7stalt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "$,", "KOUS", "NN", "APPR", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Oft schwebt' ihm Red' im Munde, wenn er allein sich fand,", "tokens": ["Oft", "schwebt'", "ihm", "Red'", "im", "Mun\u00b7de", ",", "wenn", "er", "al\u00b7lein", "sich", "fand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "APPRART", "NN", "$,", "KOUS", "PPER", "ADV", "PRF", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und einmal rief er: Geliebte! als der Narr daneben stand.", "tokens": ["Und", "ein\u00b7mal", "rief", "er", ":", "Ge\u00b7lieb\u00b7te", "!", "als", "der", "Narr", "da\u00b7ne\u00b7ben", "stand", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "$.", "NN", "$.", "KOUS", "ART", "NN", "PAV", "VVFIN", "$."], "meter": "-+-+--+-+-+-+-+", "measure": "iambic.septa.relaxed"}}, "stanza.20": {"line.1": {"text": "Des Nachts, wenn er gewappnet im stillen Zelte ruht,", "tokens": ["Des", "Nachts", ",", "wenn", "er", "ge\u00b7wapp\u00b7net", "im", "stil\u00b7len", "Zel\u00b7te", "ruht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "$,", "KOUS", "PPER", "VVFIN", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und meint den Traum zu tr\u00e4umen von Schlachten, Brand und Blut,", "tokens": ["Und", "meint", "den", "Traum", "zu", "tr\u00e4u\u00b7men", "von", "Schlach\u00b7ten", ",", "Brand", "und", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKZU", "VVINF", "APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Naht ein verkl\u00e4rtes Wesen \u2013 l\u00e4ngst d\u00e4ucht es ihm bekannt \u2013", "tokens": ["Naht", "ein", "ver\u00b7kl\u00e4r\u00b7tes", "We\u00b7sen", "\u2013", "l\u00e4ngst", "d\u00e4ucht", "es", "ihm", "be\u00b7kannt", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$(", "ADV", "VVFIN", "PPER", "PPER", "ADJD", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und neigt des Friedens Palme auf ihn mit wei\u00dfer Hand.", "tokens": ["Und", "neigt", "des", "Frie\u00b7dens", "Pal\u00b7me", "auf", "ihn", "mit", "wei\u00b7\u00dfer", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "NN", "APPR", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}}}}