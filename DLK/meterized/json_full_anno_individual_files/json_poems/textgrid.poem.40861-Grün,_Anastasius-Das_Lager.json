{"textgrid.poem.40861": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "Das Lager", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "F\u00fcrwahr, ein friedlich St\u00e4dtchen das sch\u00f6ne Saint Omar!", "tokens": ["F\u00fcr\u00b7wahr", ",", "ein", "fried\u00b7lich", "St\u00e4dt\u00b7chen", "das", "sch\u00f6\u00b7ne", "Saint", "O\u00b7mar", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "ADJD", "NN", "ART", "ADJA", "NE", "NE", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Hier junges Gr\u00fcn der Wiesen, dort Fl\u00fcsse silberklar,", "tokens": ["Hier", "jun\u00b7ges", "Gr\u00fcn", "der", "Wie\u00b7sen", ",", "dort", "Fl\u00fcs\u00b7se", "sil\u00b7ber\u00b7klar", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "ART", "NN", "$,", "ADV", "NN", "ADJD", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Ein Spiegelsee nicht ferne, und schwimmende Inseln drin,", "tokens": ["Ein", "Spie\u00b7gel\u00b7see", "nicht", "fer\u00b7ne", ",", "und", "schwim\u00b7men\u00b7de", "In\u00b7seln", "drin", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "ADV", "$,", "KON", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Drauf schiffen l\u00e4utende Heerden sanft mit den Fluthen hin.", "tokens": ["Drauf", "schif\u00b7fen", "l\u00e4u\u00b7ten\u00b7de", "Heer\u00b7den", "sanft", "mit", "den", "Flut\u00b7hen", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ADJA", "NN", "ADJD", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+--+-+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Sankt Audomar's Abteie in blankem Marmorgewand", "tokens": ["Sankt", "Au\u00b7do\u00b7ma\u00b7r's", "Ab\u00b7tei\u00b7e", "in", "blan\u00b7kem", "Mar\u00b7mor\u00b7ge\u00b7wand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "NE", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.2": {"text": "Sieht wie des Friedens Schutzgeist aufs segenreiche Land.", "tokens": ["Sieht", "wie", "des", "Frie\u00b7dens", "Schutz\u00b7geist", "aufs", "se\u00b7gen\u00b7rei\u00b7che", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "ART", "NN", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Das W\u00f6rtlein Krieg war wenig bekannt auf Omars Flur,", "tokens": ["Das", "W\u00f6rt\u00b7lein", "Krieg", "war", "we\u00b7nig", "be\u00b7kannt", "auf", "O\u00b7mars", "Flur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "ADV", "ADJD", "APPR", "NE", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und in des Klosters Chronik stand's halb verwittert nur.", "tokens": ["Und", "in", "des", "Klos\u00b7ters", "Chro\u00b7nik", "stan\u00b7d's", "halb", "ver\u00b7wit\u00b7tert", "nur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "VVFIN", "ADJD", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.3": {"line.1": {"text": "Zufriedenheit und Friede schien hier zu ruhn seit lang',", "tokens": ["Zu\u00b7frie\u00b7den\u00b7heit", "und", "Frie\u00b7de", "schien", "hier", "zu", "ruhn", "seit", "lang'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "ADV", "PTKZU", "VVINF", "APPR", "NE", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und h\u00f6rte Erz man t\u00f6nen, war's nur der Glocken Klang,", "tokens": ["Und", "h\u00f6r\u00b7te", "Erz", "man", "t\u00f6\u00b7nen", ",", "wa\u00b7r's", "nur", "der", "Glo\u00b7cken", "Klang", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "PIS", "VVINF", "$,", "VAFIN", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "Und rief um H\u00fclfe Jemand, war's h\u00f6chstens ein irres Sch\u00e4flein,", "tokens": ["Und", "rief", "um", "H\u00fcl\u00b7fe", "Je\u00b7mand", ",", "wa\u00b7r's", "h\u00f6chs\u00b7tens", "ein", "ir\u00b7res", "Sch\u00e4f\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "PIS", "$,", "VAFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+--+-+-", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "Und \u00e4rgerte sich Einer, war's auf der Kanzel das Pf\u00e4fflein.", "tokens": ["Und", "\u00e4r\u00b7ger\u00b7te", "sich", "Ei\u00b7ner", ",", "wa\u00b7r's", "auf", "der", "Kan\u00b7zel", "das", "Pf\u00e4f\u00b7flein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "PIS", "$,", "VAFIN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+---+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "Doch jetzt! Ein weites Lager vom See bis zur Abtei,", "tokens": ["Doch", "jetzt", "!", "Ein", "wei\u00b7tes", "La\u00b7ger", "vom", "See", "bis", "zur", "Ab\u00b7tei", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$.", "ART", "ADJA", "NN", "APPRART", "NN", "APPR", "APPRART", "NN", "$,"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die Glocken \u00fcbert\u00e4ubet der Krieger Feldgeschrei,", "tokens": ["Die", "Glo\u00b7cken", "\u00fc\u00b7bert\u00b7\u00e4u\u00b7bet", "der", "Krie\u00b7ger", "Feld\u00b7ge\u00b7schrei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Die Fluthen \u00fcbergl\u00e4nzet der Zelte wei\u00dfes Linnen,", "tokens": ["Die", "Flut\u00b7hen", "\u00fc\u00b7ber\u00b7gl\u00e4n\u00b7zet", "der", "Zel\u00b7te", "wei\u00b7\u00dfes", "Lin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Ein Geist der Rache blicket der Dom mit seinen Zinnen!", "tokens": ["Ein", "Geist", "der", "Ra\u00b7che", "bli\u00b7cket", "der", "Dom", "mit", "sei\u00b7nen", "Zin\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.5": {"line.1": {"text": "Da seht ihr Maxens Lager, dicht Zelt am Zelte stehn,", "tokens": ["Da", "seht", "ihr", "Ma\u00b7xens", "La\u00b7ger", ",", "dicht", "Zelt", "am", "Zel\u00b7te", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "NN", "$,", "ADJD", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und dr\u00fcber in den L\u00fcften die bunten Banner wehn,", "tokens": ["Und", "dr\u00fc\u00b7ber", "in", "den", "L\u00fcf\u00b7ten", "die", "bun\u00b7ten", "Ban\u00b7ner", "wehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "APPR", "ART", "NN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Hoch \u00fcber allen flattert der deutsche Kaiseraar", "tokens": ["Hoch", "\u00fc\u00b7ber", "al\u00b7len", "flat\u00b7tert", "der", "deut\u00b7sche", "Kai\u00b7se\u00b7raar"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "PIS", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und sammelt unter die Fl\u00fcgel der Kriegsgenossen Schaar.", "tokens": ["Und", "sam\u00b7melt", "un\u00b7ter", "die", "Fl\u00fc\u00b7gel", "der", "Kriegs\u00b7ge\u00b7nos\u00b7sen", "Schaar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "NN", "$."], "meter": "-+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.6": {"line.1": {"text": "Geschwader aus allen Landen, so weit man fl\u00e4misch spricht,", "tokens": ["Ge\u00b7schwa\u00b7der", "aus", "al\u00b7len", "Lan\u00b7den", ",", "so", "weit", "man", "fl\u00e4\u00b7misch", "spricht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PIAT", "NN", "$,", "ADV", "ADJD", "PIS", "ADJD", "VVFIN", "$,"], "meter": "-+--+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Auch Albions tapfre Streiter", "tokens": ["Auch", "Al\u00b7bions", "tapf\u00b7re", "Strei\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "NE", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und Mancher, um den am Ister ein deutsches M\u00e4dchen weint;", "tokens": ["Und", "Man\u00b7cher", ",", "um", "den", "am", "Is\u00b7ter", "ein", "deut\u00b7sches", "M\u00e4d\u00b7chen", "weint", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "KOUI", "ART", "APPRART", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Verschiedne Banner und Zungen \u2013 ", "tokens": ["Ver\u00b7schied\u00b7ne", "Ban\u00b7ner", "und", "Zun\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "Doch, traun, ein seltsam Lager! der Schlachten Wiege nicht!", "tokens": ["Doch", ",", "traun", ",", "ein", "selt\u00b7sam", "La\u00b7ger", "!", "der", "Schlach\u00b7ten", "Wie\u00b7ge", "nicht", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VVINF", "$,", "ART", "ADJD", "NN", "$.", "ART", "NN", "NN", "PTKNEG", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Kein m\u00fcrrisch, unwirsch Antlitz, rings freundlich jedes Gesicht;", "tokens": ["Kein", "m\u00fcr\u00b7risch", ",", "un\u00b7wirsch", "Ant\u00b7litz", ",", "rings", "freund\u00b7lich", "je\u00b7des", "Ge\u00b7sicht", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJD", "$,", "ADJD", "NN", "$,", "ADV", "ADJD", "PIAT", "NN", "$."], "meter": "-+-+-+--+-+--+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Ist's Wunderkraft des Bodens, dem Frieden sonst geweiht?", "tokens": ["Ist's", "Wun\u00b7der\u00b7kraft", "des", "Bo\u00b7dens", ",", "dem", "Frie\u00b7den", "sonst", "ge\u00b7weiht", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ART", "NN", "$,", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Kann sich das Herz nicht entw\u00f6hnen verfloss'ner sch\u00f6ner Zeit? \u2013", "tokens": ["Kann", "sich", "das", "Herz", "nicht", "ent\u00b7w\u00f6h\u00b7nen", "ver\u00b7floss'\u00b7ner", "sch\u00f6\u00b7ner", "Zeit", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "PRF", "ART", "NN", "PTKNEG", "ADJA", "ADJA", "ADJA", "NN", "$.", "$("], "meter": "+--+--+--+-+-+", "measure": "dactylic.tri.plus"}}, "stanza.8": {"line.1": {"text": "Wenn die Drommete rufet, klingt's fast wie Tanzmelodei;", "tokens": ["Wenn", "die", "Drom\u00b7me\u00b7te", "ru\u00b7fet", ",", "klingt's", "fast", "wie", "Tanz\u00b7me\u00b7lo\u00b7dei", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$,", "VVFIN", "ADV", "KOKOM", "NN", "$."], "meter": "--+--+-+--+--+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Und manchen Ritters Auge, de\u00df Herz sonst froh und frei,", "tokens": ["Und", "man\u00b7chen", "Rit\u00b7ters", "Au\u00b7ge", ",", "de\u00df", "Herz", "sonst", "froh", "und", "frei", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "NN", "$,", "ART", "NN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Beinah' wird's feucht, erblickt er am Helm den welken Strau\u00df,", "tokens": ["Bei\u00b7nah'", "wird's", "feucht", ",", "er\u00b7blickt", "er", "am", "Helm", "den", "wel\u00b7ken", "Strau\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADJD", "$,", "VVFIN", "PPER", "APPRART", "NN", "ART", "NN", "NE", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und will er ein Kriegslied brummen, flugs wird ein Brautlied draus!", "tokens": ["Und", "will", "er", "ein", "Kriegs\u00b7lied", "brum\u00b7men", ",", "flugs", "wird", "ein", "Braut\u00b7lied", "draus", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ART", "NN", "VVINF", "$,", "ADV", "VAFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.9": {"line.1": {"text": "Max selber, wenn er sinnend durchs Lager einsam wallt,", "tokens": ["Max", "sel\u00b7ber", ",", "wenn", "er", "sin\u00b7nend", "durchs", "La\u00b7ger", "ein\u00b7sam", "wallt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "$,", "KOUS", "PPER", "ADJD", "APPRART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Blickt seitw\u00e4rts oft, als z\u00f6ge mit ihm noch eine Gestalt;", "tokens": ["Blickt", "seit\u00b7w\u00e4rts", "oft", ",", "als", "z\u00f6\u00b7ge", "mit", "ihm", "noch", "ei\u00b7ne", "Ge\u00b7stalt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "$,", "KOUS", "NN", "APPR", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Oft schwebt' ihm Red' im Munde, wenn er allein sich fand,", "tokens": ["Oft", "schwebt'", "ihm", "Red'", "im", "Mun\u00b7de", ",", "wenn", "er", "al\u00b7lein", "sich", "fand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "APPRART", "NN", "$,", "KOUS", "PPER", "ADV", "PRF", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und einmal rief er: Geliebte! als der Narr daneben stand.", "tokens": ["Und", "ein\u00b7mal", "rief", "er", ":", "Ge\u00b7lieb\u00b7te", "!", "als", "der", "Narr", "da\u00b7ne\u00b7ben", "stand", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "$.", "NN", "$.", "KOUS", "ART", "NN", "PAV", "VVFIN", "$."], "meter": "-+-+--+-+-+-+-+", "measure": "iambic.septa.relaxed"}}, "stanza.10": {"line.1": {"text": "Des Nachts, wenn er gewappnet im stillen Zelte ruht,", "tokens": ["Des", "Nachts", ",", "wenn", "er", "ge\u00b7wapp\u00b7net", "im", "stil\u00b7len", "Zel\u00b7te", "ruht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "$,", "KOUS", "PPER", "VVFIN", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und meint den Traum zu tr\u00e4umen von Schlachten, Brand und Blut,", "tokens": ["Und", "meint", "den", "Traum", "zu", "tr\u00e4u\u00b7men", "von", "Schlach\u00b7ten", ",", "Brand", "und", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKZU", "VVINF", "APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Naht ein verkl\u00e4rtes Wesen \u2013 l\u00e4ngst d\u00e4ucht es ihm bekannt \u2013", "tokens": ["Naht", "ein", "ver\u00b7kl\u00e4r\u00b7tes", "We\u00b7sen", "\u2013", "l\u00e4ngst", "d\u00e4ucht", "es", "ihm", "be\u00b7kannt", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$(", "ADV", "VVFIN", "PPER", "PPER", "ADJD", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und neigt des Friedens Palme auf ihn mit wei\u00dfer Hand.", "tokens": ["Und", "neigt", "des", "Frie\u00b7dens", "Pal\u00b7me", "auf", "ihn", "mit", "wei\u00b7\u00dfer", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "NN", "APPR", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.11": {"line.1": {"text": "F\u00fcrwahr, ein friedlich St\u00e4dtchen das sch\u00f6ne Saint Omar!", "tokens": ["F\u00fcr\u00b7wahr", ",", "ein", "fried\u00b7lich", "St\u00e4dt\u00b7chen", "das", "sch\u00f6\u00b7ne", "Saint", "O\u00b7mar", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "ADJD", "NN", "ART", "ADJA", "NE", "NE", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Hier junges Gr\u00fcn der Wiesen, dort Fl\u00fcsse silberklar,", "tokens": ["Hier", "jun\u00b7ges", "Gr\u00fcn", "der", "Wie\u00b7sen", ",", "dort", "Fl\u00fcs\u00b7se", "sil\u00b7ber\u00b7klar", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "ART", "NN", "$,", "ADV", "NN", "ADJD", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Ein Spiegelsee nicht ferne, und schwimmende Inseln drin,", "tokens": ["Ein", "Spie\u00b7gel\u00b7see", "nicht", "fer\u00b7ne", ",", "und", "schwim\u00b7men\u00b7de", "In\u00b7seln", "drin", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "ADV", "$,", "KON", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Drauf schiffen l\u00e4utende Heerden sanft mit den Fluthen hin.", "tokens": ["Drauf", "schif\u00b7fen", "l\u00e4u\u00b7ten\u00b7de", "Heer\u00b7den", "sanft", "mit", "den", "Flut\u00b7hen", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ADJA", "NN", "ADJD", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+--+-+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.12": {"line.1": {"text": "Sankt Audomar's Abteie in blankem Marmorgewand", "tokens": ["Sankt", "Au\u00b7do\u00b7ma\u00b7r's", "Ab\u00b7tei\u00b7e", "in", "blan\u00b7kem", "Mar\u00b7mor\u00b7ge\u00b7wand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "NE", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.2": {"text": "Sieht wie des Friedens Schutzgeist aufs segenreiche Land.", "tokens": ["Sieht", "wie", "des", "Frie\u00b7dens", "Schutz\u00b7geist", "aufs", "se\u00b7gen\u00b7rei\u00b7che", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "ART", "NN", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Das W\u00f6rtlein Krieg war wenig bekannt auf Omars Flur,", "tokens": ["Das", "W\u00f6rt\u00b7lein", "Krieg", "war", "we\u00b7nig", "be\u00b7kannt", "auf", "O\u00b7mars", "Flur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "ADV", "ADJD", "APPR", "NE", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und in des Klosters Chronik stand'