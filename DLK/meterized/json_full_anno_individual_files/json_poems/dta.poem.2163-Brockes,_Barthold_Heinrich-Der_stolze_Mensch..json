{"dta.poem.2163": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der stolze Mensch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du solltest dich wahrhaftig sch\u00e4men,", "tokens": ["Du", "soll\u00b7test", "dich", "wahr\u00b7haf\u00b7tig", "sch\u00e4\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den Mittelplatz vom Vieh und Engeln einzu-", "tokens": ["Den", "Mit\u00b7tel\u00b7platz", "vom", "Vieh", "und", "En\u00b7geln", "ein\u00b7zu"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN", "KON", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "nehmen:", "tokens": ["neh\u00b7men", ":"], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Dein aufgeblasner Stolz, womit du ganz erf\u00fcllt,", "tokens": ["Dein", "auf\u00b7ge\u00b7blas\u00b7ner", "Stolz", ",", "wo\u00b7mit", "du", "ganz", "er\u00b7f\u00fcllt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PWAV", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Stellt dich dir selbst hier vor als deines Sch\u00f6pfers Bild.", "tokens": ["Stellt", "dich", "dir", "selbst", "hier", "vor", "als", "dei\u00b7nes", "Sch\u00f6p\u00b7fers", "Bild", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "ADV", "PTKVZ", "KOKOM", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ver\u00e4chtlich stolzer Staub, der zanket, zweifelt, gl\u00e4ubet,", "tokens": ["Ver\u00b7\u00e4cht\u00b7lich", "stol\u00b7zer", "Staub", ",", "der", "zan\u00b7ket", ",", "zwei\u00b7felt", ",", "gl\u00e4u\u00b7bet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$,", "PRELS", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der kriecht, der sich erhebt, der f\u00e4llt und doch verneint,", "tokens": ["Der", "kriecht", ",", "der", "sich", "er\u00b7hebt", ",", "der", "f\u00e4llt", "und", "doch", "ver\u00b7neint", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PRF", "VVFIN", "$,", "PRELS", "VVFIN", "KON", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Da\u00df er gefallen sey;", "tokens": ["Da\u00df", "er", "ge\u00b7fal\u00b7len", "sey", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Der seine Ketten zeigt, und doch spricht, ich bin frey;", "tokens": ["Der", "sei\u00b7ne", "Ket\u00b7ten", "zeigt", ",", "und", "doch", "spricht", ",", "ich", "bin", "frey", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "$,", "KON", "ADV", "VVFIN", "$,", "PPER", "VAFIN", "ADJD", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "De\u00df dunkles tr\u00fcbes Aug, was man auf Erden treibet,", "tokens": ["De\u00df", "dunk\u00b7les", "tr\u00fc\u00b7bes", "Aug", ",", "was", "man", "auf", "Er\u00b7den", "trei\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,", "PRELS", "PIS", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ja alles, was darauf, wohl einzusehn vermeynt.", "tokens": ["Ja", "al\u00b7les", ",", "was", "da\u00b7rauf", ",", "wohl", "ein\u00b7zu\u00b7sehn", "ver\u00b7meynt", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "PIS", "$,", "PRELS", "PAV", "$,", "ADV", "VVINF", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}