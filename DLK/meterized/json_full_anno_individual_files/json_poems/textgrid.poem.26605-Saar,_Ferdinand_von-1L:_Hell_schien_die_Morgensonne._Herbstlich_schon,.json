{"textgrid.poem.26605": {"metadata": {"author": {"name": "Saar, Ferdinand von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Hell schien die Morgensonne. Herbstlich schon,", "genre": "verse", "period": "N.A.", "pub_year": 1869, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hell schien die Morgensonne. Herbstlich schon,", "tokens": ["Hell", "schien", "die", "Mor\u00b7gen\u00b7son\u00b7ne", ".", "Herbst\u00b7lich", "schon", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "$.", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Erquickend strich die Luft durch's weite Thal,", "tokens": ["Er\u00b7qui\u00b7ckend", "strich", "die", "Luft", "durch's", "wei\u00b7te", "Thal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Inde\u00df ich, eines Fl\u00fc\u00dfchens Lauf verfolgend,", "tokens": ["In\u00b7de\u00df", "ich", ",", "ei\u00b7nes", "Fl\u00fc\u00df\u00b7chens", "Lauf", "ver\u00b7fol\u00b7gend", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Auf schmalem Fu\u00dfpfad schritt und frohgemuth", "tokens": ["Auf", "schma\u00b7lem", "Fu\u00df\u00b7pfad", "schritt", "und", "froh\u00b7ge\u00b7muth"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die Blicke schweifen lie\u00df in's Himmelblau", "tokens": ["Die", "Bli\u00b7cke", "schwei\u00b7fen", "lie\u00df", "in's", "Him\u00b7mel\u00b7blau"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVINF", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und \u00fcber R\u00fcben- und Kartoffelfelder,", "tokens": ["Und", "\u00fc\u00b7ber", "R\u00fc\u00b7ben", "und", "Kar\u00b7tof\u00b7fel\u00b7fel\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "TRUNC", "KON", "NN", "$,"], "meter": "-+-+--++-+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Die weit sich dehnten bis zum dunklen Rande", "tokens": ["Die", "weit", "sich", "dehn\u00b7ten", "bis", "zum", "dunk\u00b7len", "Ran\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "PRF", "VVFIN", "APPR", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Der fernen Tannenw\u00e4lder. Links von mir,", "tokens": ["Der", "fer\u00b7nen", "Tan\u00b7nen\u00b7w\u00e4l\u00b7der", ".", "Links", "von", "mir", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "ADV", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Auf hohem Damme, liefen an den Schienen", "tokens": ["Auf", "ho\u00b7hem", "Dam\u00b7me", ",", "lie\u00b7fen", "an", "den", "Schie\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Die Stangen hin mit ihren Wunderdr\u00e4hten,", "tokens": ["Die", "Stan\u00b7gen", "hin", "mit", "ih\u00b7ren", "Wun\u00b7der\u00b7dr\u00e4h\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Drauf zwitschernd noch die letzten Schwalben sa\u00dfen;", "tokens": ["Drauf", "zwit\u00b7schernd", "noch", "die", "letz\u00b7ten", "Schwal\u00b7ben", "sa\u00b7\u00dfen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADJD", "ADV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Ein W\u00e4chterhaus, umbl\u00fcht von Sonnenblumen,", "tokens": ["Ein", "W\u00e4ch\u00b7ter\u00b7haus", ",", "um\u00b7bl\u00fcht", "von", "Son\u00b7nen\u00b7blu\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Sah nahebei aus dichten Weidenb\u00fcschen", "tokens": ["Sah", "na\u00b7he\u00b7bei", "aus", "dich\u00b7ten", "Wei\u00b7den\u00b7b\u00fc\u00b7schen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Gleich einem lieblichen Idyll hervor.", "tokens": ["Gleich", "ei\u00b7nem", "lieb\u00b7li\u00b7chen", "I\u00b7dyll", "her\u00b7vor", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Da h\u00f6rt' ich lauten Aufschrei \u2013 und gewahrte,", "tokens": ["Da", "h\u00f6rt'", "ich", "lau\u00b7ten", "Auf\u00b7schrei", "\u2013", "und", "ge\u00b7wahr\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJA", "NN", "$(", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Wie eine Kuh aus nied'rem Koben brach \u2013", "tokens": ["Wie", "ei\u00b7ne", "Kuh", "aus", "nie\u00b7d'\u00b7rem", "Ko\u00b7ben", "brach", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.17": {"text": "Und hinterdrein, halbw\u00fcchsig kaum, ein M\u00e4dchen,", "tokens": ["Und", "hin\u00b7ter\u00b7drein", ",", "halb\u00b7w\u00fcch\u00b7sig", "kaum", ",", "ein", "M\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "ADJD", "ADV", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Das wie in Todesangst den Schweif des Thiers", "tokens": ["Das", "wie", "in", "To\u00b7de\u00b7sangst", "den", "Schweif", "des", "Thiers"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "KOKOM", "APPR", "NN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Umklammert hielt, um es am Flieh'n zu hindern.", "tokens": ["Um\u00b7klam\u00b7mert", "hielt", ",", "um", "es", "am", "Flieh'n", "zu", "hin\u00b7dern", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "$,", "KOUI", "PPER", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Zu schwach doch war der \u00c4rmsten Kraft; geschleift", "tokens": ["Zu", "schwach", "doch", "war", "der", "\u00c4rms\u00b7ten", "Kraft", ";", "ge\u00b7schleift"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PTKA", "ADJD", "ADV", "VAFIN", "ART", "ADJA", "NN", "$.", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Auf steilem Abhang, lie\u00df sie endlich los,", "tokens": ["Auf", "stei\u00b7lem", "Ab\u00b7hang", ",", "lie\u00df", "sie", "end\u00b7lich", "los", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "VVFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Dieweil Frau Bl\u00e4sse rasch mit tollen S\u00e4tzen", "tokens": ["Die\u00b7weil", "Frau", "Bl\u00e4s\u00b7se", "rasch", "mit", "tol\u00b7len", "S\u00e4t\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "NN", "ADJD", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Feldeinw\u00e4rts sprang. Dies Alles war so rasch", "tokens": ["Feld\u00b7e\u00b7in\u00b7w\u00e4rts", "sprang", ".", "Dies", "Al\u00b7les", "war", "so", "rasch"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$.", "PDS", "PIS", "VAFIN", "ADV", "ADJD"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.24": {"text": "Gescheh'n, da\u00df ich nicht Hilfe bringen konnte \u2013", "tokens": ["Ge\u00b7scheh'n", ",", "da\u00df", "ich", "nicht", "Hil\u00b7fe", "brin\u00b7gen", "konn\u00b7te", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "PTKNEG", "NN", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "Und nun trat auch der W\u00e4chter selbst hervor,", "tokens": ["Und", "nun", "trat", "auch", "der", "W\u00e4ch\u00b7ter", "selbst", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ADV", "ART", "NN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "Ein alter, hag'rer Mann, der sich verzweifelt", "tokens": ["Ein", "al\u00b7ter", ",", "hag'\u00b7rer", "Mann", ",", "der", "sich", "ver\u00b7zwei\u00b7felt"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$,", "PRELS", "PRF", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "In's graue Haar fuhr, als er schon fernab", "tokens": ["In's", "grau\u00b7e", "Haar", "fuhr", ",", "als", "er", "schon", "fern\u00b7ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "$,", "KOUS", "PPER", "ADV", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "Die Kuh erblickte durch die R\u00fcben traben.", "tokens": ["Die", "Kuh", "er\u00b7blick\u00b7te", "durch", "die", "R\u00fc\u00b7ben", "tra\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "\u00bbo du!\u00ab rief er in heft'gem Zorn hinab", "tokens": ["\u00bb", "o", "du", "!", "\u00ab", "rief", "er", "in", "heft'\u00b7gem", "Zorn", "hin\u00b7ab"], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "FM", "PPER", "$.", "$(", "VVFIN", "PPER", "APPR", "ADJA", "NN", "PTKVZ"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.30": {"text": "Zur zitternden Gestalt, die sich, vor ihm", "tokens": ["Zur", "zit\u00b7tern\u00b7den", "Ge\u00b7stalt", ",", "die", "sich", ",", "vor", "ihm"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "$,", "PRELS", "PRF", "$,", "APPR", "PPER"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.31": {"text": "Am Boden, wimmernd an's zerschund'ne Knie griff:", "tokens": ["Am", "Bo\u00b7den", ",", "wim\u00b7mernd", "an's", "zer\u00b7schun\u00b7d'\u00b7ne", "Knie", "griff", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "ADJD", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.32": {"text": "\u00bbo du Verfluchte! Dreimal Gottverfluchte!", "tokens": ["\u00bb", "o", "du", "Ver\u00b7fluch\u00b7te", "!", "Drei\u00b7mal", "Gott\u00b7ver\u00b7fluch\u00b7te", "!"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "FM", "PPER", "NN", "$.", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.33": {"text": "Sch\u00e4rft' ich nicht ein dir, sie mir wohl zu h\u00fcten?", "tokens": ["Sch\u00e4rft'", "ich", "nicht", "ein", "dir", ",", "sie", "mir", "wohl", "zu", "h\u00fc\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ART", "PPER", "$,", "PPER", "PPER", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.34": {"text": "Ein Bettler bin ich, \u00e4rmer als ein Bettler,", "tokens": ["Ein", "Bett\u00b7ler", "bin", "ich", ",", "\u00e4r\u00b7mer", "als", "ein", "Bett\u00b7ler", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "$,", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.35": {"text": "Wenn sie entl\u00e4uft! Und kehrte sie auch wieder \u2013", "tokens": ["Wenn", "sie", "ent\u00b7l\u00e4uft", "!", "Und", "kehr\u00b7te", "sie", "auch", "wie\u00b7der", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$.", "KON", "VVFIN", "PPER", "ADV", "ADV", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.36": {"text": "Wer zahlt den b\u00f6sen Schaden auf den Feldern?", "tokens": ["Wer", "zahlt", "den", "b\u00f6\u00b7sen", "Scha\u00b7den", "auf", "den", "Fel\u00b7dern", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.37": {"text": "Und kommt zum Walde sie, trifft sie der Heger \u2013", "tokens": ["Und", "kommt", "zum", "Wal\u00b7de", "sie", ",", "trifft", "sie", "der", "He\u00b7ger", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "PPER", "$,", "VVFIN", "PPER", "ART", "NN", "$("], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.38": {"text": "Der ist mir gram und schie\u00dft die Bestie nieder!", "tokens": ["Der", "ist", "mir", "gram", "und", "schie\u00dft", "die", "Be\u00b7stie", "nie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADJD", "KON", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.39": {"text": "Nun auf! Nun lauf und bring' sie mir zur\u00fcck \u2013", "tokens": ["Nun", "auf", "!", "Nun", "lauf", "und", "bring'", "sie", "mir", "zu\u00b7r\u00fcck", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$.", "ADV", "PTKVZ", "KON", "VVFIN", "PPER", "PPER", "PTKVZ", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.40": {"text": "Wenn dir dein Leben lieb ist, Gottverfluchte!\u00ab", "tokens": ["Wenn", "dir", "dein", "Le\u00b7ben", "lieb", "ist", ",", "Gott\u00b7ver\u00b7fluch\u00b7te", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "ADJD", "VAFIN", "$,", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.41": {"text": "So schrie er wild und mit geballten F\u00e4usten,", "tokens": ["So", "schrie", "er", "wild", "und", "mit", "ge\u00b7ball\u00b7ten", "F\u00e4us\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "KON", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.42": {"text": "Nach Odem ringend, in ohnm\u00e4cht'ger Wuth.", "tokens": ["Nach", "O\u00b7dem", "rin\u00b7gend", ",", "in", "ohn\u00b7m\u00e4cht'\u00b7ger", "Wuth", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVPP", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.43": {"text": "Ich selber \u2013 rathlos stand ich; wu\u00dfte nicht,", "tokens": ["Ich", "sel\u00b7ber", "\u2013", "rath\u00b7los", "stand", "ich", ";", "wu\u00df\u00b7te", "nicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADV", "$(", "ADJD", "VVFIN", "PPER", "$.", "VVFIN", "PTKNEG", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.44": {"text": "Sollt' ich den Mann beg\u00fct'gen \u2013 sollt' ich rasch", "tokens": ["Sollt'", "ich", "den", "Mann", "be\u00b7g\u00fct'\u00b7gen", "\u2013", "sollt'", "ich", "rasch"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ART", "NN", "VVINF", "$(", "VMFIN", "PPER", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.45": {"text": "Statt jener armen Kleinen nach der Kuh", "tokens": ["Statt", "je\u00b7ner", "ar\u00b7men", "Klei\u00b7nen", "nach", "der", "Kuh"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PDAT", "ADJA", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.46": {"text": "In Lau