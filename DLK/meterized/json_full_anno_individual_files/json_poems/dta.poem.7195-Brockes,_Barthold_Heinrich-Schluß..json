{"dta.poem.7195": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Schlu\u00df.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sieht man eine sch\u00f6ne Bluhme,", "tokens": ["Sieht", "man", "ei\u00b7ne", "sch\u00f6\u00b7ne", "Bluh\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sieht man gr\u00fcnes Laub und Gras;", "tokens": ["Sieht", "man", "gr\u00fc\u00b7nes", "Laub", "und", "Gras", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADJA", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach! so denckt nur, GOTT zum Ruhme:", "tokens": ["Ach", "!", "so", "denckt", "nur", ",", "GoTT", "zum", "Ruh\u00b7me", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ADV", "VVFIN", "ADV", "$,", "NE", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ey! GOTT Lob! wie sch\u00f6n ist das!", "tokens": ["Ey", "!", "GoTT", "Lob", "!", "wie", "sch\u00f6n", "ist", "das", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "NE", "NN", "$.", "PWAV", "ADJD", "VAFIN", "PDS", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "In des Sch\u00f6pffers Wunder-Wercken", "tokens": ["In", "des", "Sch\u00f6pf\u00b7fers", "Wun\u00b7der\u00b7\u00b7Wer\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Seine Macht und Weisheit mercken,", "tokens": ["Sei\u00b7ne", "Macht", "und", "Weis\u00b7heit", "mer\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und, an der Gesch\u00f6pffe Sch\u00e4tzen,", "tokens": ["Und", ",", "an", "der", "Ge\u00b7sch\u00f6pf\u00b7fe", "Sch\u00e4t\u00b7zen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "GoTT zu Ehren, sich erg\u00f6tzen;", "tokens": ["GoTT", "zu", "Eh\u00b7ren", ",", "sich", "er\u00b7g\u00f6t\u00b7zen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "$,", "PRF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Dient nicht nur zur Lust f\u00fcr euch;", "tokens": ["Dient", "nicht", "nur", "zur", "Lust", "f\u00fcr", "euch", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ADV", "APPRART", "NN", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Ist ein GOTTES-Dienst zugleich.", "tokens": ["Ist", "ein", "GoT\u00b7TES\u00b7Dienst", "zu\u00b7gleich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}