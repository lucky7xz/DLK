{"textgrid.poem.32674": {"metadata": {"author": {"name": "Miller, Johann Martin", "birth": "N.A.", "death": "N.A."}, "title": "An ein Paar Ringelt\u00e4ubchen", "genre": "verse", "period": "N.A.", "pub_year": 1772, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Flattert n\u00e4her, bunte Ringelt\u00e4ubchen!", "tokens": ["Flat\u00b7tert", "n\u00e4\u00b7her", ",", "bun\u00b7te", "Rin\u00b7gel\u00b7t\u00e4ub\u00b7chen", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADJD", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Komm, mit deinem lieben, trauten Weibchen,", "tokens": ["Komm", ",", "mit", "dei\u00b7nem", "lie\u00b7ben", ",", "trau\u00b7ten", "Weib\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "APPR", "PPOSAT", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Frommer Tauber, tiefer in den Hain!", "tokens": ["From\u00b7mer", "Tau\u00b7ber", ",", "tie\u00b7fer", "in", "den", "Hain", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJD", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Unter diesen gr\u00fcnen Finsternissen", "tokens": ["Un\u00b7ter", "die\u00b7sen", "gr\u00fc\u00b7nen", "Fins\u00b7ter\u00b7nis\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "K\u00f6nnt ihr ungest\u00f6rt einander k\u00fcssen,", "tokens": ["K\u00f6nnt", "ihr", "un\u00b7ge\u00b7st\u00f6rt", "ein\u00b7an\u00b7der", "k\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJD", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Und euch ganz der s\u00fc\u00dfen Liebe weihn.", "tokens": ["Und", "euch", "ganz", "der", "s\u00fc\u00b7\u00dfen", "Lie\u00b7be", "weihn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Friedlich soll die Pappel euch bewirten;", "tokens": ["Fried\u00b7lich", "soll", "die", "Pap\u00b7pel", "euch", "be\u00b7wir\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "F\u00fcrchtet nicht den frommen L\u00e4mmerhirten,", "tokens": ["F\u00fcrch\u00b7tet", "nicht", "den", "from\u00b7men", "L\u00e4m\u00b7mer\u00b7hir\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Den, wie euch, ein sanfter Trieb erf\u00fcllt!", "tokens": ["Den", ",", "wie", "euch", ",", "ein", "sanf\u00b7ter", "Trieb", "er\u00b7f\u00fcllt", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "PPER", "$,", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "K\u00f6nnt' ich Lieb' und Z\u00e4rtlichkeit verletzen?", "tokens": ["K\u00f6nnt'", "ich", "Lieb'", "und", "Z\u00e4rt\u00b7lich\u00b7keit", "ver\u00b7let\u00b7zen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "NN", "KON", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "O, ich folge Cypriens Gesetzen,", "tokens": ["O", ",", "ich", "fol\u00b7ge", "Cyp\u00b7riens", "Ge\u00b7set\u00b7zen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VVFIN", "NE", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Und wer ihnen folgt, ist sanft und mild.", "tokens": ["Und", "wer", "ih\u00b7nen", "folgt", ",", "ist", "sanft", "und", "mild", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVFIN", "$,", "VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "--+-+-+-+", "measure": "anapaest.init"}}, "stanza.3": {"line.1": {"text": "Sammelt kleine Myrtenreiser, bauet", "tokens": ["Sam\u00b7melt", "klei\u00b7ne", "Myr\u00b7ten\u00b7rei\u00b7ser", ",", "bau\u00b7et"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["NE", "ADJA", "NN", "$,", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Euer Nest im Pappelstamm, vertrauet", "tokens": ["Eu\u00b7er", "Nest", "im", "Pap\u00b7pel\u00b7stamm", ",", "ver\u00b7trau\u00b7et"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "$,", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Eure wei\u00dfen Eierchen ihm an!", "tokens": ["Eu\u00b7re", "wei\u00b7\u00dfen", "Ei\u00b7er\u00b7chen", "ihm", "an", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "An der Unschuld sichern Ruhepl\u00e4tzen", "tokens": ["An", "der", "Un\u00b7schuld", "si\u00b7chern", "Ru\u00b7he\u00b7pl\u00e4t\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Soll kein wilder Habicht euch verletzen,", "tokens": ["Soll", "kein", "wil\u00b7der", "Ha\u00b7bicht", "euch", "ver\u00b7let\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIAT", "ADJA", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Euren Jungen sich kein Sperber nahn.", "tokens": ["Eu\u00b7ren", "Jun\u00b7gen", "sich", "kein", "Sper\u00b7ber", "nahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PRF", "PIAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Still ist diese Gegend; Ruhe wohnet", "tokens": ["Still", "ist", "die\u00b7se", "Ge\u00b7gend", ";", "Ru\u00b7he", "woh\u00b7net"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "VAFIN", "PDAT", "NN", "$.", "NN", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Rings umher; die sanfte Unschuld thronet", "tokens": ["Rings", "um\u00b7her", ";", "die", "sanf\u00b7te", "Un\u00b7schuld", "thro\u00b7net"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PTKVZ", "$.", "ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Hier am liebsten. Eine Sch\u00e4ferin,", "tokens": ["Hier", "am", "liebs\u00b7ten", ".", "Ei\u00b7ne", "Sch\u00e4\u00b7fe\u00b7rin", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PTKA", "ADJD", "$.", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Die an Liebreiz eurer G\u00f6ttin gleichet,", "tokens": ["Die", "an", "Lieb\u00b7reiz", "eu\u00b7rer", "G\u00f6t\u00b7tin", "glei\u00b7chet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Daphne, mit den blauen Augen, schleichet", "tokens": ["Daph\u00b7ne", ",", "mit", "den", "blau\u00b7en", "Au\u00b7gen", ",", "schlei\u00b7chet"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["NE", "$,", "APPR", "ART", "ADJA", "NN", "$,", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Oft in diese stillen Schatten hin.", "tokens": ["Oft", "in", "die\u00b7se", "stil\u00b7len", "Schat\u00b7ten", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PDAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Und der Friede wallt auf allen Wegen", "tokens": ["Und", "der", "Frie\u00b7de", "wallt", "auf", "al\u00b7len", "We\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Der geliebten Sch\u00e4ferin entgegen,", "tokens": ["Der", "ge\u00b7lieb\u00b7ten", "Sch\u00e4\u00b7fe\u00b7rin", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Unschuld folget ihren Schritten nach;", "tokens": ["Un\u00b7schuld", "fol\u00b7get", "ih\u00b7ren", "Schrit\u00b7ten", "nach", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Zephyr weht durchs junge Laub gelinder,", "tokens": ["Ze\u00b7phyr", "weht", "durchs", "jun\u00b7ge", "Laub", "ge\u00b7lin\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "ADJA", "NN", "ADJA", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Laute Wasserf\u00e4lle brausen minder,", "tokens": ["Lau\u00b7te", "Was\u00b7ser\u00b7f\u00e4l\u00b7le", "brau\u00b7sen", "min\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Und das Lied der Nachtigall wird wach.", "tokens": ["Und", "das", "Lied", "der", "Nach\u00b7ti\u00b7gall", "wird", "wach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "VAFIN", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Wenn ihr euch auf schlanken \u00c4sten wieget,", "tokens": ["Wenn", "ihr", "euch", "auf", "schlan\u00b7ken", "\u00c4s\u00b7ten", "wie\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Und das M\u00e4dchen hier auf Blumen lieget;", "tokens": ["Und", "das", "M\u00e4d\u00b7chen", "hier", "auf", "Blu\u00b7men", "lie\u00b7get", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Dann beginnt der K\u00fcsse s\u00fc\u00dfes Spiel!", "tokens": ["Dann", "be\u00b7ginnt", "der", "K\u00fcs\u00b7se", "s\u00fc\u00b7\u00dfes", "Spiel", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Weckt in ihrem jugendlichen Herzen", "tokens": ["Weckt", "in", "ih\u00b7rem", "ju\u00b7gend\u00b7li\u00b7chen", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Unbekannte Seufzer, stille Schmerzen,", "tokens": ["Un\u00b7be\u00b7kann\u00b7te", "Seuf\u00b7zer", ",", "stil\u00b7le", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Und der Sehnsucht z\u00e4rtliches Gef\u00fchl!", "tokens": ["Und", "der", "Sehn\u00b7sucht", "z\u00e4rt\u00b7li\u00b7ches", "Ge\u00b7f\u00fchl", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Seufzer werden ihren Busen heben,", "tokens": ["Seuf\u00b7zer", "wer\u00b7den", "ih\u00b7ren", "Bu\u00b7sen", "he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Thr\u00e4nen \u00fcber ihre Wangen beben,", "tokens": ["Thr\u00e4\u00b7nen", "\u00fc\u00b7ber", "ih\u00b7re", "Wan\u00b7gen", "be\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Liebe wird im blauen Auge gl\u00fchn;", "tokens": ["Lie\u00b7be", "wird", "im", "blau\u00b7en", "Au\u00b7ge", "gl\u00fchn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Dann will ich mich sittsam zu ihr stehlen,", "tokens": ["Dann", "will", "ich", "mich", "sitt\u00b7sam", "zu", "ihr", "steh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "ADJD", "APPR", "PPER", "VVINF", "$,"], "meter": "+---+-+-+-", "measure": "dactylic.init"}, "line.5": {"text": "Zum Geliebten wird sie mich erw\u00e4hlen,", "tokens": ["Zum", "Ge\u00b7lieb\u00b7ten", "wird", "sie", "mich", "er\u00b7w\u00e4h\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Und mit mir in Eine H\u00fctte ziehn.", "tokens": ["Und", "mit", "mir", "in", "Ei\u00b7ne", "H\u00fct\u00b7te", "ziehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.8": {"line.1": {"text": "Kommt dann, T\u00e4ubchen, wenn der Herbst entfliehet,", "tokens": ["Kommt", "dann", ",", "T\u00e4ub\u00b7chen", ",", "wenn", "der", "Herbst", "ent\u00b7flie\u00b7het", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "NN", "$,", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Und der Winter unsre Flur beziehet,", "tokens": ["Und", "der", "Win\u00b7ter", "uns\u00b7re", "Flur", "be\u00b7zie\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Vor die H\u00fctte! Bis er wieder flieht,", "tokens": ["Vor", "die", "H\u00fct\u00b7te", "!", "Bis", "er", "wie\u00b7der", "flieht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$.", "KOUS", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Will ich euch die besten K\u00f6rner streuen;", "tokens": ["Will", "ich", "euch", "die", "bes\u00b7ten", "K\u00f6r\u00b7ner", "streu\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "O, wie wird sich meine Daphne freuen,", "tokens": ["O", ",", "wie", "wird", "sich", "mei\u00b7ne", "Daph\u00b7ne", "freu\u00b7en", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "VAFIN", "PRF", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Wenn sie ihre T\u00e4ubchen wieder sieht!", "tokens": ["Wenn", "sie", "ih\u00b7re", "T\u00e4ub\u00b7chen", "wie\u00b7der", "sieht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.9": {"line.1": {"text": "Hat euch meine Bitte schon ger\u00fchret?", "tokens": ["Hat", "euch", "mei\u00b7ne", "Bit\u00b7te", "schon", "ge\u00b7r\u00fch\u00b7ret", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPOSAT", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Seht! von Lieb' und Mitleid hergef\u00fchret,", "tokens": ["Seht", "!", "von", "Lieb'", "und", "Mit\u00b7leid", "her\u00b7ge\u00b7f\u00fch\u00b7ret", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Flattert ihr der k\u00fchlen Pappel zu.", "tokens": ["Flat\u00b7tert", "ihr", "der", "k\u00fch\u00b7len", "Pap\u00b7pel", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "O wie pocht mein Herz in st\u00e4rkern Schl\u00e4gen!", "tokens": ["O", "wie", "pocht", "mein", "Herz", "in", "st\u00e4r\u00b7kern", "Schl\u00e4\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "VVFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Tausend Freuden lachen mir entgegen;", "tokens": ["Tau\u00b7send", "Freu\u00b7den", "la\u00b7chen", "mir", "ent\u00b7ge\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Hoffnung, Hoffnung! o wie s\u00fc\u00df bist du!", "tokens": ["Hoff\u00b7nung", ",", "Hoff\u00b7nung", "!", "o", "wie", "s\u00fc\u00df", "bist", "du", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$.", "FM", "KOKOM", "ADJD", "VAFIN", "PPER", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.10": {"line.1": {"text": "Flattert n\u00e4her, bunte Ringelt\u00e4ubchen!", "tokens": ["Flat\u00b7tert", "n\u00e4\u00b7her", ",", "bun\u00b7te", "Rin\u00b7gel\u00b7t\u00e4ub\u00b7chen", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADJD", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Komm, mit deinem lieben, trauten Weibchen,", "tokens": ["Komm", ",", "mit", "dei\u00b7nem", "lie\u00b7ben", ",", "trau\u00b7ten", "Weib\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "APPR", "PPOSAT", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Frommer Tauber, tiefer in den Hain!", "tokens": ["From\u00b7mer", "Tau\u00b7ber", ",", "tie\u00b7fer", "in", "den", "Hain", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJD", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Unter diesen gr\u00fcnen Finsternissen", "tokens": ["Un\u00b7ter", "die\u00b7sen", "gr\u00fc\u00b7nen", "Fins\u00b7ter\u00b7nis\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "K\u00f6nnt ihr ungest\u00f6rt einander k\u00fcssen,", "tokens": ["K\u00f6nnt", "ihr", "un\u00b7ge\u00b7st\u00f6rt", "ein\u00b7an\u00b7der", "k\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJD", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Und euch ganz der s\u00fc\u00dfen Liebe weihn.", "tokens": ["Und", "euch", "ganz", "der", "s\u00fc\u00b7\u00dfen", "Lie\u00b7be", "weihn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.11": {"line.1": {"text": "Friedlich soll die Pappel euch bewirten;", "tokens": ["Fried\u00b7lich", "soll", "die", "Pap\u00b7pel", "euch", "be\u00b7wir\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "F\u00fcrchtet nicht den frommen L\u00e4mmerhirten,", "tokens": ["F\u00fcrch\u00b7tet", "ni