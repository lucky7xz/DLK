{"textgrid.poem.61154": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "1L: O zieht nur auf mit flatternden Standarten!", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O zieht nur auf mit flatternden Standarten!", "tokens": ["O", "zieht", "nur", "auf", "mit", "flat\u00b7tern\u00b7den", "Stand\u00b7ar\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "APPR", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ruft euren \u00dcbermut von allen Zinnen!", "tokens": ["Ruft", "eu\u00b7ren", "\u00dc\u00b7ber\u00b7mut", "von", "al\u00b7len", "Zin\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Haut, wie Sir John, mit prahlendem Beginnen", "tokens": ["Haut", ",", "wie", "Sir", "John", ",", "mit", "prah\u00b7len\u00b7dem", "Be\u00b7gin\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PWAV", "NN", "NE", "$,", "APPR", "ADJA", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Die Klinge, die zum Spiel ihr f\u00fchrt, voll Scharten!", "tokens": ["Die", "Klin\u00b7ge", ",", "die", "zum", "Spiel", "ihr", "f\u00fchrt", ",", "voll", "Schar\u00b7ten", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPRART", "NN", "PPER", "VVFIN", "$,", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Kampflieder auch stimmt an von allen Arten,", "tokens": ["K