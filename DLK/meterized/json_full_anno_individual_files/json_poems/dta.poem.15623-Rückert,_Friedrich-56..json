{"dta.poem.15623": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "56.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du kannst dir deinen Leib, dein Schicksal auch, nicht machen,", "tokens": ["Du", "kannst", "dir", "dei\u00b7nen", "Leib", ",", "dein", "Schick\u00b7sal", "auch", ",", "nicht", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "ADV", "$,", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch \u00fcberwalten kannst du sie und \u00fcberwachen.", "tokens": ["Doch", "\u00fc\u00b7ber\u00b7wal\u00b7ten", "kannst", "du", "sie", "und", "\u00fc\u00b7ber\u00b7wa\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VMFIN", "PPER", "PPER", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die Grundlag' hat gelegt Nothwendigkeit, Natur;", "tokens": ["Die", "Grund\u00b7lag'", "hat", "ge\u00b7legt", "Noth\u00b7wen\u00b7dig\u00b7keit", ",", "Na\u00b7tur", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Baumeisterin des Bau's ist deine Freiheit nur.", "tokens": ["Bau\u00b7meis\u00b7te\u00b7rin", "des", "Bau's", "ist", "dei\u00b7ne", "Frei\u00b7heit", "nur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VAFIN", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "La\u00df nur das Untere zum Obern niemals werden,", "tokens": ["La\u00df", "nur", "das", "Un\u00b7te\u00b7re", "zum", "O\u00b7bern", "nie\u00b7mals", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ART", "NN", "APPRART", "NN", "ADV", "VAINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und sei getrost, es ruht der Himmel auf der Erden.", "tokens": ["Und", "sei", "ge\u00b7trost", ",", "es", "ruht", "der", "Him\u00b7mel", "auf", "der", "Er\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "VVPP", "$,", "PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}