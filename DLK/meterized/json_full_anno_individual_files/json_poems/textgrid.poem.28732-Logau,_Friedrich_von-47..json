{"textgrid.poem.28732": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "47.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sein Eigenschafft und Art bekam ein iedes Thier,", "tokens": ["Sein", "Ei\u00b7gen\u00b7schafft", "und", "Art", "be\u00b7kam", "ein", "ie\u00b7des", "Thier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "VVFIN", "ART", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und wie sie einmal war, so bleibt sie f\u00fcr und f\u00fcr:", "tokens": ["Und", "wie", "sie", "ein\u00b7mal", "war", ",", "so", "bleibt", "sie", "f\u00fcr", "und", "f\u00fcr", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADV", "VAFIN", "$,", "ADV", "VVFIN", "PPER", "APPR", "KON", "APPR", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der L\u00f6w, der bleibt behertzt; der Hase, der bleibt scheu;", "tokens": ["Der", "L\u00f6w", ",", "der", "bleibt", "be\u00b7hertzt", ";", "der", "Ha\u00b7se", ",", "der", "bleibt", "scheu", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "ADJD", "$.", "ART", "NN", "$,", "PRELS", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Fuchs, der bleibet schlau; der Hund, der bleibet treu;", "tokens": ["Der", "Fuchs", ",", "der", "blei\u00b7bet", "schlau", ";", "der", "Hund", ",", "der", "blei\u00b7bet", "treu", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NE", "$,", "PRELS", "VVFIN", "ADJD", "$.", "ART", "NN", "$,", "PRELS", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der Mensch nur wandelt sich, vermummt sich immerdar,", "tokens": ["Der", "Mensch", "nur", "wan\u00b7delt", "sich", ",", "ver\u00b7mummt", "sich", "im\u00b7mer\u00b7dar", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "PRF", "$,", "VVFIN", "PRF", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ist diese Stunde nicht der, der er jene war.", "tokens": ["Ist", "die\u00b7se", "Stun\u00b7de", "nicht", "der", ",", "der", "er", "je\u00b7ne", "war", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDAT", "NN", "PTKNEG", "ART", "$,", "PRELS", "PPER", "PDS", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Was dient ihm dann Vernunfft? Sie hilfft dahin ihm ein,", "tokens": ["Was", "dient", "ihm", "dann", "Ver\u00b7nunfft", "?", "Sie", "hilfft", "da\u00b7hin", "ihm", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "NN", "$.", "PPER", "VVFIN", "PAV", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Da\u00df er kan mit Vernunfft recht unvern\u00fcnfftig seyn.", "tokens": ["Da\u00df", "er", "kan", "mit", "Ver\u00b7nunfft", "recht", "un\u00b7ver\u00b7n\u00fcnff\u00b7tig", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VMFIN", "APPR", "NN", "ADV", "ADJD", "VAINF", "$."], "meter": "--+--+-+-+-+", "measure": "ana