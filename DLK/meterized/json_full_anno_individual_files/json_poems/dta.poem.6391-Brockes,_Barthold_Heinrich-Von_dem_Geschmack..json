{"dta.poem.6391": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Geschmack.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Man kan durch den Geschmack, den diese S\u00e4ffte", "tokens": ["Man", "kan", "durch", "den", "Ge\u00b7schmack", ",", "den", "die\u00b7se", "S\u00e4ff\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIS", "VMFIN", "APPR", "ART", "NN", "$,", "PRELS", "PDAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auf so viel Art in uns erwecken,", "tokens": ["Auf", "so", "viel", "Art", "in", "uns", "er\u00b7we\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "PIAT", "NN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Durch ihres Eindrucks viel und mannigfache Kr\u00e4ffte,", "tokens": ["Durch", "ih\u00b7res", "Ein\u00b7drucks", "viel", "und", "man\u00b7nig\u00b7fa\u00b7che", "Kr\u00e4ff\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da sie bald s\u00fc\u00df, bald starck, bald scharff sind, das entdecken:", "tokens": ["Da", "sie", "bald", "s\u00fc\u00df", ",", "bald", "starck", ",", "bald", "scharff", "sind", ",", "das", "ent\u00b7de\u00b7cken", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,", "ADV", "ADJD", "VAFIN", "$,", "PDS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Da\u00df sie in uns allein.", "tokens": ["Da\u00df", "sie", "in", "uns", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Doch was die C\u00f6rperchen, die sie erzeugen, seyn;", "tokens": ["Doch", "was", "die", "C\u00f6r\u00b7per\u00b7chen", ",", "die", "sie", "er\u00b7zeu\u00b7gen", ",", "seyn", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "$,", "PRELS", "PPER", "VVINF", "$,", "VAINF", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Macht der drauf sinnende Verstand", "tokens": ["Macht", "der", "drauf", "sin\u00b7nen\u00b7de", "Ver\u00b7stand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ART", "PAV", "ADJA", "NN"], "meter": "+--+---+", "measure": "dactylic.di.plus"}, "line.8": {"text": "Uns nur allein bekannt.", "tokens": ["Uns", "nur", "al\u00b7lein", "be\u00b7kannt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}