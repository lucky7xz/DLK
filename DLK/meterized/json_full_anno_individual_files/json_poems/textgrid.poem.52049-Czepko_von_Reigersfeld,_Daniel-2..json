{"textgrid.poem.52049": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vespillo wird ber\u00fchmt vor andern Aertzten werden,", "tokens": ["Ves\u00b7pil\u00b7lo", "wird", "be\u00b7r\u00fchmt", "vor", "an\u00b7dern", "A\u00b7ertz\u00b7ten", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADJD", "APPR", "ADJA", "NN", "VAINF", "$,"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "In dem er seine Kunst auff manchen Schlag versucht,", "tokens": ["In", "dem", "er", "sei\u00b7ne", "Kunst", "auff", "man\u00b7chen", "Schlag", "ver\u00b7sucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PPOSAT", "NN", "APPR", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Itzt Pulver st\u00f6st und siebt, itzt Tr\u00e4ncke seudt und kucht:", "tokens": ["Itzt", "Pul\u00b7ver", "st\u00f6st", "und", "siebt", ",", "itzt", "Tr\u00e4n\u00b7cke", "seudt", "und", "kucht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "KON", "VVFIN", "$,", "ADV", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und seine Krancken schickt fein ordentlich zur Erden.", "tokens": ["Und", "sei\u00b7ne", "Kran\u00b7cken", "schickt", "fein", "or\u00b7dent\u00b7lich", "zur", "Er\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ADJD", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Man mu\u00df nur seiner Art und Wi\u00dfenschafft gewohnen,", "tokens": ["Man", "mu\u00df", "nur", "sei\u00b7ner", "Art", "und", "Wi\u00b7\u00dfen\u00b7schafft", "ge\u00b7woh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "ADV", "PPOSAT", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und wer wil einen Artzt vor die Gerichte ziehn:", "tokens": ["Und", "wer", "wil", "ei\u00b7nen", "Artzt", "vor", "die", "Ge\u00b7rich\u00b7te", "ziehn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VMFIN", "ART", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Er t\u00f6dtet ohn Entgelt, man wei\u00df es zuvorhin,", "tokens": ["Er", "t\u00f6d\u00b7tet", "ohn", "Ent\u00b7gelt", ",", "man", "wei\u00df", "es", "zu\u00b7vor\u00b7hin", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,", "PIS", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dem er sein Recht gethan, der mu\u00df ihn noch belohnen.", "tokens": ["Dem", "er", "sein", "Recht", "ge\u00b7than", ",", "der", "mu\u00df", "ihn", "noch", "be\u00b7loh\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPOSAT", "NN", "VVPP", "$,", "ART", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Vespillo wird ber\u00fchmt vor andern Aertzten werden,", "tokens": ["Ves\u00b7pil