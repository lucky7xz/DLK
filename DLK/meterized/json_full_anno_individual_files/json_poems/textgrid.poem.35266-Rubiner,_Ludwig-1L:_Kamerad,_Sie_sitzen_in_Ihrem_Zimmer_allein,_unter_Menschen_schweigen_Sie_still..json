{"textgrid.poem.35266": {"metadata": {"author": {"name": "Rubiner, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Kamerad, Sie sitzen in Ihrem Zimmer allein, unter Menschen schweigen Sie still.", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kamerad, Sie sitzen in Ihrem Zimmer allein, unter Menschen schweigen Sie still.", "tokens": ["Ka\u00b7me\u00b7rad", ",", "Sie", "sit\u00b7zen", "in", "Ih\u00b7rem", "Zim\u00b7mer", "al\u00b7lein", ",", "un\u00b7ter", "Men\u00b7schen", "schwei\u00b7gen", "Sie", "still", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "ADV", "$,", "APPR", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+--+-+-+-+-+-+--+", "measure": "trochaic.octa.plus.relaxed"}, "line.2": {"text": "Aber ich wei\u00df meine stummen Kameraden hunderttausend auf der Welt, zu denen ich reden will.", "tokens": ["A\u00b7ber", "ich", "wei\u00df", "mei\u00b7ne", "stum\u00b7men", "Ka\u00b7me\u00b7ra\u00b7den", "hun\u00b7dert\u00b7tau\u00b7send", "auf", "der", "Welt", ",", "zu", "de\u00b7nen", "ich", "re\u00b7den", "will", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "CARD", "APPR", "ART", "NN", "$,", "APPR", "PRELS", "PPER", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+-+-+-+-+-+-+-+--+-+", "measure": "trochaic.octa.plus"}}, "stanza.2": {"line.1": {"text": "Wir waren noch klein, da erhob zu uns die Erde ihr bergiges Schmerzensgesicht,", "tokens": ["Wir", "wa\u00b7ren", "noch", "klein", ",", "da", "er\u00b7hob", "zu", "uns", "die", "Er\u00b7de", "ihr", "ber\u00b7gi\u00b7ges", "Schmer\u00b7zens\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "ADV", "VVFIN", "APPR", "PPER", "ART", "NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+--+-+-+-+-+-+--+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "In unsre Zehen bebte fernes Gel\u00e4nd, von Sturz und Strudel ums Licht.", "tokens": ["In", "uns\u00b7re", "Ze\u00b7hen", "beb\u00b7te", "fer\u00b7nes", "Ge\u00b7l\u00e4nd", ",", "von", "Sturz", "und", "Stru\u00b7del", "ums", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ADJA", "NN", "$,", "APPR", "NN", "KON", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+--+-+-+--+", "measure": "iambic.octa.plus.relaxed"}}, "stanza.3": {"line.1": {"text": "Die Menschen in schlaffer Geilheit und tr\u00e4g liebten die Erde nicht mehr,", "tokens": ["Die", "Men\u00b7schen", "in", "schlaf\u00b7fer", "Geil\u00b7heit", "und", "tr\u00e4g", "lieb\u00b7ten", "die", "Er\u00b7de", "nicht", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "KON", "ADJD", "VVFIN", "ART", "NN", "PTKNEG", "ADV", "$,"], "meter": "-+--+-+--++--+--+", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Aber die Erde schrie, wir h\u00f6rten sie nicht, und sie donnerte Zeichen her.", "tokens": ["A\u00b7ber", "die", "Er\u00b7de", "schrie", ",", "wir", "h\u00f6r\u00b7ten", "sie", "nicht", ",", "und", "sie", "don\u00b7ner\u00b7te", "Zei\u00b7chen", "her", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "PTKNEG", "$,", "KON", "PPER",