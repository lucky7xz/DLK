o": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADJD", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Da stand er in der Werkstatt bald", "tokens": ["Da", "stand", "er", "in", "der", "Werk\u00b7statt", "bald"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Schurzfell und mit Hammer.", "tokens": ["Mit", "Schurz\u00b7fell", "und", "mit", "Ham\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Da rannt er den \u00d6fen in den Bauch", "tokens": ["Da", "rannt", "er", "den", "\u00d6\u00b7fen", "in", "den", "Bauch"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mit schweren Eisenstangen,", "tokens": ["Mit", "schwe\u00b7ren", "Ei\u00b7sen\u00b7stan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df hell aus Schlacken und aus Rauch", "tokens": ["Da\u00df", "hell", "aus", "Schla\u00b7cken", "und", "aus", "Rauch"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Metallne B\u00e4che sprangen!", "tokens": ["Me\u00b7tall\u00b7ne", "B\u00e4\u00b7che", "spran\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Kanonen go\u00df er \u2013 manches St\u00fcck!", "tokens": ["Ka\u00b7no\u00b7nen", "go\u00df", "er", "\u2013", "man\u00b7ches", "St\u00fcck", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$(", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die br\u00fcllten auf allen Meeren;", "tokens": ["Die", "br\u00fcll\u00b7ten", "auf", "al\u00b7len", "Mee\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die brachten die Franzen ins Ungel\u00fcck", "tokens": ["Die", "brach\u00b7ten", "die", "Fran\u00b7zen", "ins", "Un\u00b7ge\u00b7l\u00fcck"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ART", "NN", "APPRART", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Und mu\u00dften Indien verheeren.", "tokens": ["Und", "mu\u00df\u00b7ten", "In\u00b7di\u00b7en", "ver\u00b7hee\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "NE", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.5": {"line.1": {"text": "Die warfen Kugeln, leidlich schwer,", "tokens": ["Die", "war\u00b7fen", "Ku\u00b7geln", ",", "leid\u00b7lich", "schwer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Chinesen in die Rippen;", "tokens": ["Den", "Chi\u00b7ne\u00b7sen", "in", "die", "Rip\u00b7pen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die jauchzten Britanniens Ruhm daher", "tokens": ["D