nct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Da gab es kein Erbarmen:", "tokens": ["Da", "gab", "es", "kein", "Er\u00b7bar\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da ward er vor die T\u00fcr gesetzt", "tokens": ["Da", "ward", "er", "vor", "die", "T\u00fcr", "ge\u00b7setzt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "ART", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wohl unter die Kr\u00fcppel und Armen.", "tokens": ["Wohl", "un\u00b7ter", "die", "Kr\u00fcp\u00b7pel", "und", "Ar\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "KON", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.8": {"line.1": {"text": "Er ging \u2013 die Brust so zornig weh,", "tokens": ["Er", "ging", "\u2013", "die", "Brust", "so", "zor\u00b7nig", "weh", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "ART", "NN", "ADV", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als ob sie der Donner duchgrollte", "tokens": ["Als", "ob", "sie", "der", "Don\u00b7ner", "duch\u00b7groll\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "PPER", "ART", "NN", "VVFIN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Von allen M\u00f6rsern, die er je", "tokens": ["Von", "al\u00b7len", "M\u00f6r\u00b7sern", ",", "die", "er", "je"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "$,", "PRELS", "