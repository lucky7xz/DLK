{"dta.poem.9541": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Sonnet.  \n Cupido an die sch\u00f6ne Barnien.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Hjer la\u00df ich pfeil und glut/ die rosen mit den myrten/", "tokens": ["Hjer", "la\u00df", "ich", "pfeil", "und", "glut", "/", "die", "ro\u00b7sen", "mit", "den", "myr\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVIMP", "PPER", "NN", "KON", "NN", "$(", "ART", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Durch die ich vor der zeit in himmel konte gehn.", "tokens": ["Durch", "die", "ich", "vor", "der", "zeit", "in", "him\u00b7mel", "kon\u00b7te", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "APPR", "ART", "NN", "APPR", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Hier la\u00df ich krantz und band und alle waffen stehn/", "tokens": ["Hier", "la\u00df", "ich", "krantz", "und", "band", "und", "al\u00b7le", "waf\u00b7fen", "stehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "NN", "KON", "VVFIN", "KON", "PIAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ich flieh/ und fliehe fort/ und la\u00df es euch ihr hirten.", "tokens": ["Ich", "flieh", "/", "und", "flie\u00b7he", "fort", "/", "und", "la\u00df", "es", "euch", "ihr", "hir\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "KON", "VVFIN", "PTKVZ", "$(", "KON", "VVIMP", "PPER", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Kein spiel/ kein fr\u00f6lich seyn/ kein s\u00fcsses hals-umg\u00fcrten/", "tokens": ["Kein", "spiel", "/", "kein", "fr\u00f6\u00b7lich", "seyn", "/", "kein", "s\u00fcs\u00b7ses", "hals\u00b7um\u00b7g\u00fcr\u00b7ten", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$(", "PIAT", "ADJD", "VAINF", "$(", "PIAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und keine Nymphe soll mit schallen und geth\u00f6n", "tokens": ["Und", "kei\u00b7ne", "Nym\u00b7phe", "soll", "mit", "schal\u00b7len", "und", "ge\u00b7th\u00f6n"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "VMFIN", "APPR", "VVINF", "KON", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mich f\u00fcrderhin durch thal/ durch pusch und auen sehn/", "tokens": ["Mich", "f\u00fcr\u00b7der\u00b7hin", "durch", "thal", "/", "durch", "pusch", "und", "au\u00b7en", "sehn", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "NE", "$(", "APPR", "ADJD", "KON", "VVINF", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Kein west-wind soll mich mehr mit s\u00fcssem thau bewirthen/", "tokens": ["Kein", "west\u00b7wind", "soll", "mich", "mehr", "mit", "s\u00fcs\u00b7sem", "thau", "be\u00b7wirt\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Weil Barnia mit glantz glut/ feuer/ licht und gifft/", "tokens": ["Weil", "Bar\u00b7nia", "mit", "glantz", "glut", "/", "feu\u00b7er", "/", "licht", "und", "gifft", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "APPR", "NN", "NN", "$(", "NN", "$(", "NN", "KON", "NN", "$("], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Pfeil/ k\u00f6cher/ krantz und speil und alles \u00fcbertrifft.", "tokens": ["Pfeil", "/", "k\u00f6\u00b7cher", "/", "krantz", "und", "speil", "und", "al\u00b7les", "\u00fc\u00b7bert\u00b7rifft", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "ADJA", "$(", "NN", "KON", "ADJD", "KON", "PIS", "VVFIN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Ade/ ich gehe fort zu den entfernten w\u00fcsten/", "tokens": ["A\u00b7de", "/", "ich", "ge\u00b7he", "fort", "zu", "den", "ent\u00b7fern\u00b7ten", "w\u00fcs\u00b7ten", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "PPER", "VVFIN", "PTKVZ", "APPR", "ART", "ADJA", "VVFIN", "$("], "meter": "+--+-----+-+-", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "Doch nehmt hier warnung an/ und h\u00f6ret noch ein wort:", "tokens": ["Doch", "nehmt", "hier", "war\u00b7nung", "an", "/", "und", "h\u00f6\u00b7ret", "noch", "ein", "wort", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "NN", "PTKVZ", "$(", "KON", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Jhr sterblichen/ wenn ich itzund von euch bin fort/", "tokens": ["Ihr", "sterb\u00b7li\u00b7chen", "/", "wenn", "ich", "it\u00b7zund", "von", "euch", "bin", "fort", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "$(", "KOUS", "PPER", "ADV", "APPR", "PPER", "VAFIN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Last euch nach Barnien/ sie t\u00f6dtet/ nicht gel\u00fcsten.", "tokens": ["Last", "euch", "nach", "Bar\u00b7ni\u00b7en", "/", "sie", "t\u00f6d\u00b7tet", "/", "nicht", "ge\u00b7l\u00fcs\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NE", "$(", "PPER", "VVFIN", "$(", "PTKNEG", "VVPP", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}}}}}