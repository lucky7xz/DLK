{"dta.poem.14": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Die blaue Korn-Blume.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198797", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Auf, mein Herz der Felder Seegen,", "tokens": ["Auf", ",", "mein", "Herz", "der", "Fel\u00b7der", "See\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "PPOSAT", "NN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der in seinen Wachsthum bl\u00fcht,", "tokens": ["Der", "in", "sei\u00b7nen", "Wach\u00b7sthum", "bl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Als die Sprossen ewger G\u00fct,", "tokens": ["Als", "die", "Spros\u00b7sen", "ew\u00b7ger", "G\u00fct", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Zu betrachten, zu erwegen!", "tokens": ["Zu", "be\u00b7trach\u00b7ten", ",", "zu", "er\u00b7we\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Hier hat unsre Spei\u00df und Freude", "tokens": ["Hier", "hat", "uns\u00b7re", "Spei\u00df", "und", "Freu\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "KON", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Uns der H\u00f6chste vorgelegt:", "tokens": ["Uns", "der", "H\u00f6chs\u00b7te", "vor\u00b7ge\u00b7legt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Da der Akker alle beide,", "tokens": ["Da", "der", "Ak\u00b7ker", "al\u00b7le", "bei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PIAT", "PIS", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Lust und Nuzzen reichlich tr\u00e4gt,", "tokens": ["Lust", "und", "Nuz\u00b7zen", "reich\u00b7lich", "tr\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Wo sich als auf Speise-Tischen,", "tokens": ["Wo", "sich", "als", "auf", "Spei\u00b7se\u00b7Ti\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "KOUS", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Frucht und Blumen lieblich mischen.", "tokens": ["Frucht", "und", "Blu\u00b7men", "lieb\u00b7lich", "mi\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Da wo fette Aehren wallen,", "tokens": ["Da", "wo", "fet\u00b7te", "A\u00b7eh\u00b7ren", "wal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWAV", "ADJA", "NN", "VVINF", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Mit dem s\u00fcssen Korngericht,", "tokens": ["Mit", "dem", "s\u00fcs\u00b7sen", "Korn\u00b7ge\u00b7richt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "M\u00fcssen uns auch ins Gesicht,", "tokens": ["M\u00fcs\u00b7sen", "uns", "auch", "ins", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Manche sch\u00f6ne Blumen fallen.", "tokens": ["Man\u00b7che", "sch\u00f6\u00b7ne", "Blu\u00b7men", "fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Dieses dr\u00fckt uns deine G\u00fcte,", "tokens": ["Die\u00b7ses", "dr\u00fckt", "uns", "dei\u00b7ne", "G\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Weiser Vater! die uns n\u00e4hrt,", "tokens": ["Wei\u00b7ser", "Va\u00b7ter", "!", "die", "uns", "n\u00e4hrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Durch die Augen ins Gem\u00fcte,", "tokens": ["Durch", "die", "Au\u00b7gen", "ins", "Ge\u00b7m\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Die uns \u00fcberzeugend lehrt,", "tokens": ["Die", "uns", "\u00fc\u00b7berz\u00b7eu\u00b7gend", "lehrt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Wie du auch uns mit Erg\u00f6zzen,", "tokens": ["Wie", "du", "auch", "uns", "mit", "Er\u00b7g\u00f6z\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "PPER", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Wilt das Nahrungs Brodt aufsezzen.", "tokens": ["Wilt", "das", "Nah\u00b7rungs", "Brodt", "auf\u00b7sez\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "NN", "VVIZU", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Eine jede Akker-Blume,", "tokens": ["Ei\u00b7ne", "je\u00b7de", "Ak\u00b7ker\u00b7Blu\u00b7me", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist auf dem besaamten Feld,", "tokens": ["Ist", "auf", "dem", "be\u00b7saam\u00b7ten", "Feld", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Als ein Herold aufgestellt,", "tokens": ["Als", "ein", "He\u00b7rold", "auf\u00b7ge\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und prangt zu des H\u00f6chsten Ruhme;", "tokens": ["Und", "prangt", "zu", "des", "H\u00f6chs\u00b7ten", "Ruh\u00b7me", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Aber keine von den allen,", "tokens": ["A\u00b7ber", "kei\u00b7ne", "von", "den", "al\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "APPR", "ART", "PIAT", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Die an Farben mannigfalt,", "tokens": ["Die", "an", "Far\u00b7ben", "man\u00b7nig\u00b7falt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Will mir lieblicher gefallen,", "tokens": ["Will", "mir", "lieb\u00b7li\u00b7cher", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Von Geruch, und an Gestalt;", "tokens": ["Von", "Ge\u00b7ruch", ",", "und", "an", "Ge\u00b7stalt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "KON", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Als die Korn-Blum, die im Glanze,", "tokens": ["Als", "die", "Korn\u00b7Blum", ",", "die", "im", "Glan\u00b7ze", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NE", "$,", "PRELS", "APPRART", "NN", "$,"], "meter": "+-++--+-", "measure": "trochaic.tetra.relaxed"}, "line.10": {"text": "Gleichet einem blauen Kranze.", "tokens": ["Glei\u00b7chet", "ei\u00b7nem", "blau\u00b7en", "Kran\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Sieht man sie etwan von Ferne,", "tokens": ["Sieht", "man", "sie", "et\u00b7wan", "von", "Fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PPER", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zwischen einer gr\u00fcnen Saat,", "tokens": ["Zwi\u00b7schen", "ei\u00b7ner", "gr\u00fc\u00b7nen", "Saat", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die noch keine Aehren hat,", "tokens": ["Die", "noch", "kei\u00b7ne", "A\u00b7eh\u00b7ren", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PIAT", "NN", "VAFIN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Scheinen sie als blaue Sterne,", "tokens": ["Schei\u00b7nen", "sie", "als", "blau\u00b7e", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KOUS", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Die des Himmels Farben tragen,", "tokens": ["Die", "des", "Him\u00b7mels", "Far\u00b7ben", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und der Felder Schmuk erh\u00f6hn;", "tokens": ["Und", "der", "Fel\u00b7der", "Schmuk", "er\u00b7h\u00f6hn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "VVINF", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.7": {"text": "Wenn wir nach der Deutung fragen,", "tokens": ["Wenn", "wir", "nach", "der", "Deu\u00b7tung", "fra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Warum sie in Felde stehn:", "tokens": ["Wa\u00b7rum", "sie", "in", "Fel\u00b7de", "stehn", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "So deucht mir, da\u00df sie uns weisen,", "tokens": ["So", "deucht", "mir", ",", "da\u00df", "sie", "uns", "wei\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "PRF", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "Welchen wir als ", "tokens": ["Wel\u00b7chen", "wir", "als"], "token_info": ["word", "word", "word"], "pos": ["PWS", "PPER", "KOUS"], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.5": {"line.1": {"text": "Der im Himmel herrlich thronet,", "tokens": ["Der", "im", "Him\u00b7mel", "herr\u00b7lich", "thro\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Stellt uns die gestirnte Zier,", "tokens": ["Stellt", "uns", "die", "ge\u00b7stirn\u00b7te", "Zier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "An der blauen Kornblum f\u00fcr;", "tokens": ["An", "der", "blau\u00b7en", "Korn\u00b7blum", "f\u00fcr", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "APPR", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Der da \u00fcber Sternen wohnet,", "tokens": ["Der", "da", "\u00fc\u00b7ber", "Ster\u00b7nen", "woh\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Will uns an derselben lehren,", "tokens": ["Will", "uns", "an", "der\u00b7sel\u00b7ben", "leh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "PDAT", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Da\u00df das gr\u00fcn besaamte Land", "tokens": ["Da\u00df", "das", "gr\u00fcn", "be\u00b7saam\u00b7te", "Land"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJD", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Uns auch zeige den zu ehren,", "tokens": ["Uns", "auch", "zei\u00b7ge", "den", "zu", "eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ART", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Der im Himmel nur bekandt:", "tokens": ["Der", "im", "Him\u00b7mel", "nur", "be\u00b7kandt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Da\u00df wir schuldig in dem Gr\u00fcnen,", "tokens": ["Da\u00df", "wir", "schul\u00b7dig", "in", "dem", "Gr\u00fc\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Die in blinder Einfalt stekken,", "tokens": ["Die", "in", "blin\u00b7der", "Ein\u00b7falt", "stek\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Denken da\u00df ein Akkerman", "tokens": ["Den\u00b7ken", "da\u00df", "ein", "Ak\u00b7ker\u00b7man"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KOUS", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sich die Fr\u00fcchte geben kan,", "tokens": ["Sich", "die", "Fr\u00fcch\u00b7te", "ge\u00b7ben", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Die der Sch\u00f6pfer mu\u00df erwekken.", "tokens": ["Die", "der", "Sch\u00f6p\u00b7fer", "mu\u00df", "er\u00b7wek\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Denn sie glauben durch das S\u00e4en,", "tokens": ["Denn", "sie", "glau\u00b7ben", "durch", "das", "S\u00e4\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wenn der Akker umgew\u00fchlt,", "tokens": ["Wenn", "der", "Ak\u00b7ker", "um\u00b7ge\u00b7w\u00fchlt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "W\u00fcrde ein gesegnet M\u00e4hen,", "tokens": ["W\u00fcr\u00b7de", "ein", "ge\u00b7seg\u00b7net", "M\u00e4\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "VVPP", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Als von selbst darauf erzielt:", "tokens": ["Als", "von", "selbst", "da\u00b7rauf", "er\u00b7zielt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ADV", "PAV", "VVPP", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.9": {"text": "M\u00f6chten diese andre Lehren", "tokens": ["M\u00f6ch\u00b7ten", "die\u00b7se", "and\u00b7re", "Leh\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "PDAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Von der blauen Kornblum h\u00f6ren:", "tokens": ["Von", "der", "blau\u00b7en", "Korn\u00b7blum", "h\u00f6\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "