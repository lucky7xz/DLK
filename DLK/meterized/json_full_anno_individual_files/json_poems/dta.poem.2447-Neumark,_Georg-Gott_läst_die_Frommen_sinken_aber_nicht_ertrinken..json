{"dta.poem.2447": {"metadata": {"author": {"name": "Neumark, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Gott l\u00e4st die Frommen sinken/ aber  \n nicht ertrinken.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1652", "urn": "urn:nbn:de:kobv:b4-20428-0", "language": ["de:0.99"], "booktitle": "Neumark, Georg: Poetisch- und Musikalisches Lustw\u00e4ldchen. Hamburg, 1652."}, "poem": {"stanza.1": {"line.1": {"text": "Gott lesst die Frommen zwar ein wenig untersinken/", "tokens": ["Gott", "lesst", "die", "From\u00b7men", "zwar", "ein", "we\u00b7nig", "un\u00b7ter\u00b7sin\u00b7ken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "ADV", "ART", "PIS", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Jm See der Traurigkeit doch aber nicht ertrinken.", "tokens": ["Jm", "See", "der", "Trau\u00b7rig\u00b7keit", "doch", "a\u00b7ber", "nicht", "er\u00b7trin\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "ADV", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}