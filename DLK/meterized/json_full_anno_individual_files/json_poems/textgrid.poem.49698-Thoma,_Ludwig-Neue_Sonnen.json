{"textgrid.poem.49698": {"metadata": {"author": {"name": "Thoma, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Neue Sonnen", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es ist nicht allzu schwer, ber\u00fchmt zu werden,", "tokens": ["Es", "ist", "nicht", "all\u00b7zu", "schwer", ",", "be\u00b7r\u00fchmt", "zu", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "PTKA", "ADJD", "$,", "ADJD", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bei uns in Deutschland; sonst wo kenn' ich's nicht.", "tokens": ["Bei", "uns", "in", "Deutschland", ";", "sonst", "wo", "kenn'", "ich's", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "NE", "$.", "ADV", "PWAV", "VVFIN", "PIS", "PTKNEG", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Hier ist jetzt alles Phrase, Ton, Geb\u00e4rden,", "tokens": ["Hier", "ist", "jetzt", "al\u00b7les", "Phra\u00b7se", ",", "Ton", ",", "Ge\u00b7b\u00e4r\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PIAT", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der ist am besten, der am sch\u00f6nsten spricht;", "tokens": ["Der", "ist", "am", "bes\u00b7ten", ",", "der", "am", "sch\u00f6ns\u00b7ten", "spricht", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PTKA", "ADJD", "$,", "PRELS", "APPRART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Es sagt der deutsche Vater zu dem Sohne:", "tokens": ["Es", "sagt", "der", "deut\u00b7sche", "Va\u00b7ter", "zu", "dem", "Soh\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Des gro\u00dfen Kanzlers gro\u00dfer Epigone", "tokens": ["Des", "gro\u00b7\u00dfen", "Kanz\u00b7lers", "gro\u00b7\u00dfer", "E\u00b7pi\u00b7go\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Ist auch nicht schlecht.", "tokens": ["Ist", "auch", "nicht", "schlecht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PTKNEG", "ADJD", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Na, mir ist's recht.", "tokens": ["Na", ",", "mir", "ist's", "recht", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VAFIN", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Man machte fr\u00fcher nicht so viel aus Taten", "tokens": ["Man", "mach\u00b7te", "fr\u00fc\u00b7her", "nicht", "so", "viel", "aus", "Ta\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "ADJD", "PTKNEG", "ADV", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als heut' aus einem sch\u00f6nen Adjektiv;", "tokens": ["Als", "heut'", "aus", "ei\u00b7nem", "sch\u00f6\u00b7nen", "Ad\u00b7jek\u00b7tiv", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ist ihm ein Kraftwort rund und nett geraten,", "tokens": ["Ist", "ihm", "ein", "Kraft\u00b7wort", "rund", "und", "nett", "ge\u00b7ra\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "ADJD", "KON", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Verbeugt sich alles vor dem Helden tief.", "tokens": ["Ver\u00b7beugt", "sich", "al\u00b7les", "vor", "dem", "Hel\u00b7den", "tief", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PIS", "APPR", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Es sagt der Biedre zu dem deutschen Bruder:", "tokens": ["Es", "sagt", "der", "Bied\u00b7re", "zu", "dem", "deut\u00b7schen", "Bru\u00b7der", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ein rechter Mann ist Gott sei Dank am Ruder,", "tokens": ["Ein", "rech\u00b7ter", "Mann", "ist", "Gott", "sei", "Dank", "am", "Ru\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "NN", "VAFIN", "APPR", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Es geht bergauf.", "tokens": ["Es", "geht", "ber\u00b7gauf", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Ich pfeif' darauf.", "tokens": ["Ich", "pfeif'", "da\u00b7rauf", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Er st\u00f6\u00dft die Volksvertretung mit den Sohlen", "tokens": ["Er", "st\u00