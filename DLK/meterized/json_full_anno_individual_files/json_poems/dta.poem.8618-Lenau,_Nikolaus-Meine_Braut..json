{"dta.poem.8618": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Meine Braut.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1832", "urn": "urn:nbn:de:kobv:b4-200905193572", "language": ["de:0.99"], "booktitle": "Lenau, Nikolaus: Gedichte. Stuttgart, 1832."}, "poem": {"stanza.1": {"line.1": {"text": "An der duftverlornen Gr\u00e4nze", "tokens": ["An", "der", "duft\u00b7ver\u00b7lor\u00b7nen", "Gr\u00e4n\u00b7ze"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jener Berge tanzen hold", "tokens": ["Je\u00b7ner", "Ber\u00b7ge", "tan\u00b7zen", "hold"], "token_info": ["word", "word", "word", "word"], "pos": ["PDAT", "NN", "VVFIN", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Abendwolken ihre T\u00e4nze,", "tokens": ["Ab\u00b7end\u00b7wol\u00b7ken", "ih\u00b7re", "T\u00e4n\u00b7ze", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Leichtgesch\u00fcrzt im Strahlengold.", "tokens": ["Leicht\u00b7ge\u00b7sch\u00fcrzt", "im", "Strah\u00b7len\u00b7gold", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wenn ich nach den lichten R\u00e4umen", "tokens": ["Wenn", "ich", "nach", "den", "lich\u00b7ten", "R\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jener Berg' hin\u00fcberseh',", "tokens": ["Je\u00b7ner", "Ber\u00b7g'", "hin\u00b7\u00fcber\u00b7seh'", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ueberschleicht mich's wie ein Tr\u00e4umen,", "tokens": ["Ue\u00b7ber\u00b7schleicht", "mich's", "wie", "ein", "Tr\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "KOKOM", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Fa\u00dft mein Herz ein dunkles Weh.", "tokens": ["Fa\u00dft", "mein", "Herz", "ein", "dunk\u00b7les", "Weh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und mir ist, als wohne dr\u00fcben", "tokens": ["Und", "mir", "ist", ",", "als", "woh\u00b7ne", "dr\u00fc\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "$,", "KOUS", "VVFIN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Meine Braut und harre bang,", "tokens": ["Mei\u00b7ne", "Braut", "und", "har\u00b7re", "bang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df ich komme, sie zu lieben,", "tokens": ["Da\u00df", "ich", "kom\u00b7me", ",", "sie", "zu", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Eh' verbl\u00fcht ist Herz und Wang'.", "tokens": ["Eh'", "ver\u00b7bl\u00fcht", "ist", "Herz", "und", "Wang'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "VAFIN", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Pl\u00f6tzlich treibt ein wildes Sehnen", "tokens": ["Pl\u00f6tz\u00b7lich", "treibt", "ein", "wil\u00b7des", "Seh\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nach den Bergen mich, zu ihr,", "tokens": ["Nach", "den", "Ber\u00b7gen", "mich", ",", "zu", "ihr", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "$,", "APPR", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Fluchtverstreute Wonnethr\u00e4nen", "tokens": ["Flucht\u00b7ver\u00b7streu\u00b7te", "Won\u00b7net\u00b7hr\u00e4\u00b7nen"], "token_info": ["word", "word"], "pos": ["NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "St\u00fcrzen aus dem Auge mir.", "tokens": ["St\u00fcr\u00b7zen", "aus", "dem", "Au\u00b7ge", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Doch die Berge sich verdunkeln,", "tokens": ["Doch", "die", "Ber\u00b7ge", "sich", "ver\u00b7dun\u00b7keln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und die Wolken werden Nacht;", "tokens": ["Und", "die", "Wol\u00b7ken", "wer\u00b7den", "Nacht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nicht ein Sternlein seh' ich funkeln,", "tokens": ["Nicht", "ein", "Stern\u00b7lein", "seh'", "ich", "fun\u00b7keln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und der Sturm ist aufgewacht;", "tokens": ["Und", "der", "Sturm", "ist", "auf\u00b7ge\u00b7wacht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.6": {"line.1": {"text": "Scheltend ruft er mir entgegen:", "tokens": ["Schel\u00b7tend", "ruft", "er", "mir", "ent\u00b7ge\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hei\u00dfer Narr, wohin? verzeuch!", "tokens": ["Hei\u00b7\u00dfer", "Narr", ",", "wo\u00b7hin", "?", "ver\u00b7zeuch", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PWAV", "$.", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Deine Braut hei\u00dft Qual, \u2014 den Segen", "tokens": ["Dei\u00b7ne", "Braut", "hei\u00dft", "Qual", ",", "den", "Se\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "NN", "$,", "$(", "ART", "NN"], "meter": "+-+-++-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Spricht das Ungl\u00fcck \u00fcber euch!", "tokens": ["Spricht", "das", "Un\u00b7gl\u00fcck", "\u00fc\u00b7ber", "euch", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}