{"dta.poem.16246": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "300.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wer sich in sich vertieft, kann nicht die Welt regieren;", "tokens": ["Wer", "sich", "in", "sich", "ver\u00b7tieft", ",", "kann", "nicht", "die", "Welt", "re\u00b7gie\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "APPR", "PRF", "VVPP", "$,", "VMFIN", "PTKNEG", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und wer sich hin ihr gibt, der wird sich selbst verlieren.", "tokens": ["Und", "wer", "sich", "hin", "ihr", "gibt", ",", "der", "wird", "sich", "selbst", "ver\u00b7lie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PRF", "ADV", "PPER", "VVFIN", "$,", "PRELS", "VAFIN", "PRF", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Dich hinzugeben ihr, und wieder dich zur\u00fcck", "tokens": ["Dich", "hin\u00b7zu\u00b7ge\u00b7ben", "ihr", ",", "und", "wie\u00b7der", "dich", "zu\u00b7r\u00fcck"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KON", "ADV", "PPER", "PTKVZ"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von ihr zu nehmen, das allein ist Lust und Gl\u00fcck.", "tokens": ["Von", "ihr", "zu", "neh\u00b7men", ",", "das", "al\u00b7lein", "ist", "Lust", "und", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PTKZU", "VVINF", "$,", "PRELS", "ADV", "VAFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Des Geistes Athem soll wie der des Mundes seyn:", "tokens": ["Des", "Geis\u00b7tes", "A\u00b7them", "soll", "wie", "der", "des", "Mun\u00b7des", "seyn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VMFIN", "KOKOM", "ART", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du sendest warm ihn aus, und ziehest frisch ihn ein.", "tokens": ["Du", "sen\u00b7dest", "warm", "ihn", "aus", ",", "und", "zie\u00b7hest", "frisch", "ihn", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PPER", "PTKVZ", "$,", "KON", "VVFIN", "ADJD", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}