{"textgrid.poem.49365": {"metadata": {"author": {"name": "Ball, Hugo", "birth": "N.A.", "death": "N.A."}, "title": "Die Sonne", "genre": "verse", "period": "N.A.", "pub_year": 1906, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zwischen meinen Augenlidern f\u00e4hrt ein Kinderwagen.", "tokens": ["Zwi\u00b7schen", "mei\u00b7nen", "Au\u00b7gen\u00b7li\u00b7dern", "f\u00e4hrt", "ein", "Kin\u00b7der\u00b7wa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.2": {"text": "Zwischen meinen Augenlidern geht ein Mann mit einem Pudel.", "tokens": ["Zwi\u00b7schen", "mei\u00b7nen", "Au\u00b7gen\u00b7li\u00b7dern", "geht", "ein", "Mann", "mit", "ei\u00b7nem", "Pu\u00b7del", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Eine Baumgruppe wird zum Schlangenb\u00fcndel und zischt in den Himmel.", "tokens": ["Ei\u00b7ne", "Baum\u00b7grup\u00b7pe", "wird", "zum", "Schlan\u00b7gen\u00b7b\u00fcn\u00b7del", "und", "zischt", "in", "den", "Him\u00b7mel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPRART", "NN", "KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+--+-+-+--+--+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Ein Stein h\u00e4lt eine Rede. B\u00e4ume in Gr\u00fcnbrand. Fliehende Inseln.", "tokens": ["Ein", "Stein", "h\u00e4lt", "ei\u00b7ne", "Re\u00b7de", ".", "B\u00e4u\u00b7me", "in", "Gr\u00fcn\u00b7brand", ".", "Flie\u00b7hen\u00b7de", "In\u00b7seln", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$.", "NN", "APPR", "NN", "$.", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-+--+-", "measure": "iambic.septa.relaxed"}, "line.5": {"text": "Schwanken und Muschelgeklingel und Fischkopf wie auf dem Meeresboden.", "tokens": ["Schwan\u00b7ken", "und", "Mu\u00b7schel\u00b7ge\u00b7klin\u00b7gel", "und", "Fischkopf", "wie", "auf", "dem", "Mee\u00b7res\u00b7bo\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "KON", "NN", "KOKOM", "APPR", "ART", "NN", "$."], "meter": "+--+--+--+-+-+-+-", "measure": "dactylic.tri.plus"}}, "stanza.2": {"line.1": {"text": "Meine Beine strecken sich aus bis zum Horizont. Eine Hofkutsche knackt", "tokens": ["Mei\u00b7ne", "Bei\u00b7ne", "stre\u00b7cken", "sich", "aus", "bis", "zum", "Ho\u00b7ri\u00b7zont", ".", "Ei\u00b7ne", "Hof\u00b7kutsc\u00b7he", "knackt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "APPR", "ADV", "APPRART", "NN", "$.", "ART", "NN", "VVFIN"], "meter": "+-+-+--+--+--+--+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.2": {"text": "Dr\u00fcber weg. Meine Stiefel ragen am Horizont empor wie die T\u00fcrme einer", "tokens": ["Dr\u00fc\u00b7ber", "weg", ".", "Mei\u00b7ne", "Stie\u00b7fel", "ra\u00b7gen", "am", "Ho\u00b7ri\u00b7zont", "em\u00b7por", "wie", "die", "T\u00fcr\u00b7me", "ei\u00b7ner"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "PTKVZ", "$.", "PPOSAT", "NN", "VVFIN", "APPRART", "NN", "PTKVZ", "KOKOM", "ART", "NN", "ART"], "meter": "+-+--+-+--+-+-+--+-+-", "measure": "trochaic.octa.plus.relaxed"}, "line.3": {"text": "Versinkenden Stadt. Ich bin der Riese Goliath. Ich verdaue Ziegenk\u00e4se.", "tokens": ["Ver\u00b7sin\u00b7ken\u00b7den", "Stadt", ".", "Ich", "bin", "der", "Rie\u00b7se", "Go\u00b7li\u00b7ath", ".", "Ich", "ver\u00b7dau\u00b7e", "Zie\u00b7gen\u00b7k\u00e4\u00b7se", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "PPER", "VAFIN", "ART", "NN", "NE", "$.", "PPER", "ADJA", "NN", "$."], "meter": "-+--+-+-+-+--+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}, "line.4": {"text": "Ich bin ein Mammuthk\u00e4lbchen. Gr\u00fcne Grasigel schn\u00fcffeln an mir.", "tokens": ["Ich", "bin", "ein", "Mam\u00b7muth\u00b7k\u00e4lb\u00b7chen", ".", "Gr\u00fc\u00b7ne", "Gra\u00b7si\u00b7gel", "schn\u00fcf\u00b7feln", "an", "mir", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$.", "ADJA", "NN", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+--+--+", "measure": "iambic.septa.relaxed"}, "line.5": {"text": "Gras spannt gr\u00fcne S\u00e4bel und Br\u00fccken und Regenb\u00f6gen \u00fcber meinen Bauch.", "tokens": ["Gras", "spannt", "gr\u00fc\u00b7ne", "S\u00e4\u00b7bel", "und", "Br\u00fc\u00b7cken", "und", "Re\u00b7gen\u00b7b\u00f6\u00b7gen", "\u00fc\u00b7ber", "mei\u00b7nen", "Bauch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJA", "NN", "KON", "NN", "KON", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+--+--+-+-+-+-+", "measure": "trochaic.octa.plus.relaxed"}}, "stanza.3": {"line.1": {"text": "Meine Ohren sind rosa Riesenmuscheln, ganz offen. Mein K\u00f6rper schwillt an", "tokens": ["Mei\u00b7ne", "Oh\u00b7ren", "sind", "ro\u00b7sa", "Rie\u00b7sen\u00b7mu\u00b7scheln", ",", "ganz", "of\u00b7fen", ".", "Mein", "K\u00f6r\u00b7per", "schwillt", "an"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "NN", "$,", "ADV", "ADJD", "$.", "PPOSAT", "NN", "VVFIN", "PTKVZ"], "meter": "+-+--+-+-+--+--+--+", "measure": "trochaic.octa.plus.relaxed"}, "line.2": {"text": "Von Ger\u00e4uschen, die sich gefangen haben darin.", "tokens": ["Von", "Ge\u00b7r\u00e4u\u00b7schen", ",", "die", "sich", "ge\u00b7fan\u00b7gen", "ha\u00b7ben", "da\u00b7rin", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "PRF", "VVPP", "VAFIN", "PAV", "$."], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Ich h\u00f6re das Meckern", "tokens": ["Ich", "h\u00f6\u00b7re", "das", "Me\u00b7ckern"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Des gro\u00dfen Pan. Ich h\u00f6re die zinnoberrote Musik der Sonne. Sie steht", "tokens": ["Des", "gro\u00b7\u00dfen", "Pan", ".", "Ich", "h\u00f6\u00b7re", "die", "zin\u00b7no\u00b7ber\u00b7ro\u00b7te", "Mu\u00b7sik", "der", "Son\u00b7ne", ".", "Sie", "steht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "PPER", "VVFIN", "ART", "ADJA", "NN", "ART", "NN", "$.", "PPER", "VVFIN"], "meter": "-+-+-+--+--+-+--+--+", "measure": "iambic.octa.plus.relaxed"}, "line.5": {"text": "Links oben. Zinnoberrot spr\u00fchen die Fetzen hinaus in die Weltnacht.", "tokens": ["Links", "o\u00b7ben", ".", "Zin\u00b7no\u00b7ber\u00b7rot", "spr\u00fc\u00b7hen", "die", "Fet\u00b7zen", "hin\u00b7aus", "in", "die", "Welt\u00b7nacht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$.", "NN", "VVFIN", "ART", "NN", "APZR", "APPR", "ART", "NN", "$."], "meter": "-+-+--++--+--+--+-", "measure": "iambic.septa.relaxed"}, "line.6": {"text": "Wenn sie herunterf\u00e4llt, zerquetscht sie die Stadt und die Kircht\u00fcrme", "tokens": ["Wenn", "sie", "her\u00b7un\u00b7ter\u00b7f\u00e4llt", ",", "zer\u00b7quetscht", "sie", "die", "Stadt", "und", "die", "Kirch\u00b7t\u00fcr\u00b7me"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVPP", "$,", "VVFIN", "PPER", "ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.7": {"text": "Und alle Vorg\u00e4rten voll Krokus und Hyazinthen, und wird einen Schall geben", "tokens": ["Und", "al\u00b7le", "Vor\u00b7g\u00e4r\u00b7ten", "voll", "Kro\u00b7kus", "und", "Hy\u00b7a\u00b7zin\u00b7then", ",", "und", "wird", "ei\u00b7nen", "Schall", "ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "ADJD", "NN", "KON", "NN", "$,", "KON", "VAFIN", "ART", "NN", "VVINF"], "meter": "-+--+--++-+-+-+-+-+--", "measure": "iambic.octa.plus.relaxed"}, "line.8": {"text": "Wie Blech von Kindertrompeten.", "tokens": ["Wie", "Blech", "von", "Kin\u00b7der\u00b7trom\u00b7pe\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Aber es ist in der Luft ein Gegeneinanderwehen von Purpur und Eigelb", "tokens": ["A\u00b7ber", "es", "ist", "in", "der", "Luft", "ein", "Ge\u00b7gen\u00b7ein\u00b7an\u00b7der\u00b7we\u00b7hen", "von", "Pur\u00b7pur", "und", "Ei\u00b7gelb"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "APPR", "ART", "NN", "ART", "NN", "APPR", "NN", "KON", "NN"], "meter": "+--+--+-+--+-+--+--+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Und Flaschengr\u00fcn: Schaukeln, die eine orangene Faust festh\u00e4lt an langen F\u00e4den,", "tokens": ["Und", "Fla\u00b7schen\u00b7gr\u00fcn", ":", "Schau\u00b7keln", ",", "die", "ei\u00b7ne", "o\u00b7ran\u00b7ge\u00b7ne", "Faust", "fest\u00b7h\u00e4lt", "an", "lan\u00b7gen", "F\u00e4\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$.", "NN", "$,", "PRELS", "ART", "ADJA", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+--+--+-+-+-+-", "measure": "amphibrach.penta.plus"}, "line.3": {"text": "Und ist ein Singen von Vogelh\u00e4lsen, die \u00fcber die Zweige h\u00fcpfen.", "tokens": ["Und", "ist", "ein", "Sin\u00b7gen", "von", "Vo\u00b7gel\u00b7h\u00e4l\u00b7sen", ",", "die", "\u00fc\u00b7ber", "die", "Zwei\u00b7ge", "h\u00fcp\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "APPR", "NE", "$,", "PRELS", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+--+-+--+--+-+-", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "Ein sehr zartes Gest\u00e4nge von Kinderfahnen.", "tokens": ["Ein", "sehr", "zar\u00b7tes", "Ge\u00b7st\u00e4n\u00b7ge", "von", "Kin\u00b7der\u00b7fah\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "APPR", "NN", "$."], "meter": "+-+--+--+-+-", "measure": "trochaic.penta.relaxed"}}, "stanza.5": {"line.1": {"text": "Morgen wird man die Sonne auf einen gro\u00dfr\u00e4drigen Wagen laden", "tokens": ["Mor\u00b7gen", "wird", "man", "die", "Son\u00b7ne", "auf", "ei\u00b7nen", "gro\u00df\u00b7r\u00e4d\u00b7ri\u00b7gen", "Wa\u00b7gen", "la\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PIS", "ART", "NN", "APPR", "ART", "ADJA", "NN", "VVINF"], "meter": "+-+--+--+-+-+-+-+-", "measure": "trochaic.octa.plus.relaxed"}, "line.2": {"text": "Und in die Kunsthandlung Caspari fahren. Ein viehk\u00f6pfiger Neger", "tokens": ["Und", "in", "die", "Kunst\u00b7hand\u00b7lung", "Cas\u00b7pa\u00b7ri", "fah\u00b7ren", ".", "Ein", "vieh\u00b7k\u00f6p\u00b7fi\u00b7ger", "Ne\u00b7ger"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "NE", "VVINF", "$.", "ART", "ADJA", "NN"], "meter": "-+-+---+-+-+-+--+-", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "Mit wulstigein Nacken, Bl\u00e4hnase und breitem Schritt wird f\u00fcnfzig wei\u00df-", "tokens": ["Mit", "wuls\u00b7ti\u00b7gein", "Na\u00b7cken", ",", "Bl\u00e4h\u00b7na\u00b7se", "und", "brei\u00b7tem", "Schritt", "wird", "f\u00fcnf\u00b7zig", "wei\u00df"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "NN", "KON", "ADJA", "NN", "VAFIN", "CARD", "TRUNC"], "meter": "-+--+--+--+-+-+-+", "measure": "amphibrach.tetra.plus"}, "line.4": {"text": "Juckende Esel halten, die vor den Wagen gespannt sind beim Pyramidenbau.", "tokens": ["Ju\u00b7cken\u00b7de", "E\u00b7sel", "hal\u00b7ten", ",", "die", "vor", "den", "Wa\u00b7gen", "ge\u00b7spannt", "sind", "beim", "Py\u00b7ra\u00b7mi\u00b7den\u00b7bau", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$,", "PRELS", "APPR", "ART", "NN", "VVPP", "VAFIN", "APPRART", "NN", "$."], "meter": "+--+-+-+--+--+--+-+-+", "measure": "iambic.octa.plus.invert"}}, "stanza.6": {"line.1": {"text": "Eine Menge blutbunten Volks wird sich stauen:", "tokens": ["Ei\u00b7ne", "Men\u00b7ge", "blut\u00b7bun\u00b7ten", "Volks", "wird", "sich", "stau\u00b7en", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VAFIN", "PRF", "VVINF", "$."], "meter": "+-+-+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Kindsbetterinnen und Ammen,", "tokens": ["Kinds\u00b7bet\u00b7te\u00b7rin\u00b7nen", "und", "Am\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Kranke im Fahrstuhl, ein stelzender Kranich, zwei Veitst\u00e4nzerinnen.", "tokens": ["Kran\u00b7ke", "im", "Fahr\u00b7stuhl", ",", "ein", "stel\u00b7zen\u00b7der", "Kra\u00b7nich", ",", "zwei", "Veit\u00b7st\u00e4n\u00b7ze\u00b7rin\u00b7nen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,", "ART", "ADJA", "NN", "$,", "CARD", "NN", "$."], "meter": "+--+--+--+-+-+-+-", "measure": "dactylic.tri.plus"}, "line.4": {"text": "Ein Herr mit einer Ripsschleifenkrawatte und ein rotduftender Schutzmann.", "tokens": ["Ein", "Herr", "mit", "ei\u00b7ner", "Rips\u00b7schlei\u00b7fen\u00b7kra\u00b7wat\u00b7te", "und", "ein", "rot\u00b7duf\u00b7ten\u00b7der", "Schutz\u00b7mann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "KON", "ART", "ADJA", "NN", "$."], "meter": "-+-+-++--+--+-+--+-", "measure": "iambic.octa.plus.relaxed"}}, "stanza.7": {"line.1": {"text": "Ich kann mich nicht halten: Ich bin voller Seligkeit. Die Fensterkreuze", "tokens": ["Ich", "kann", "mich", "nicht", "hal\u00b7ten", ":", "Ich", "bin", "vol\u00b7ler", "Se\u00b7lig\u00b7keit", ".", "Die", "Fens\u00b7ter\u00b7kreu\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$.", "PPER", "VAFIN", "ADJA", "NN", "$.", "ART", "NN"], "meter": "-+--+-+-+-+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Zerplat