{"dta.poem.9622": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Sie hat sich mit einem messer versehret.  \n C. E.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mich jam\u0303ert deiner hand/ mich schmertzen deine wunden/", "tokens": ["Mich", "jam\u0303ert", "dei\u00b7ner", "hand", "/", "mich", "schmert\u00b7zen", "dei\u00b7ne", "wun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$(", "PPER", "VVFIN", "PPOSAT", "ADJA", "$("], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Die du/ Climene/ j\u00fcngst durch einen schnitt empfunden;", "tokens": ["Die", "du", "/", "Cli\u00b7me\u00b7ne", "/", "j\u00fcngst", "durch", "ei\u00b7nen", "schnitt", "emp\u00b7fun\u00b7den", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "$(", "NE", "$(", "ADV", "APPR", "ART", "ADJD", "VVPP", "$."], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Das messer/ welches dich bi\u00df auff das blut ger\u00fchrt/", "tokens": ["Das", "mes\u00b7ser", "/", "wel\u00b7ches", "dich", "bi\u00df", "auff", "das", "blut", "ge\u00b7r\u00fchrt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PWS", "PPER", "ADV", "APPR", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Hat mich und meinen geist in gleiche noth gerissen/", "tokens": ["Hat", "mich", "und", "mei\u00b7nen", "geist", "in", "glei\u00b7che", "noth", "ge\u00b7ris\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "KON", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich mu\u00df so wohl/ wie du/ den k\u00fchnen frevel b\u00fcssen/", "tokens": ["Ich", "mu\u00df", "so", "wohl", "/", "wie", "du", "/", "den", "k\u00fch\u00b7nen", "fre\u00b7vel", "b\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "$(", "PWAV", "PPER", "$(", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Den es nur wider dich allein hat ausgef\u00fchrt.", "tokens": ["Den", "es", "nur", "wi\u00b7der", "dich", "al\u00b7lein", "hat", "aus\u00b7ge\u00b7f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "APPR", "PPER", "ADV", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Di\u00df leid ich neben dir; nur du f\u00fchlst nicht die schmertzen/", "tokens": ["Di\u00df", "leid", "ich", "ne\u00b7ben", "dir", ";", "nur", "du", "f\u00fchlst", "nicht", "die", "schmert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "APPR", "PPER", "$.", "ADV", "PPER", "VVFIN", "PTKNEG", "ART", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die dein erhitztes aug\u2019 in meiner brust erregt;", "tokens": ["Die", "dein", "er\u00b7hitz\u00b7tes", "aug'", "in", "mei\u00b7ner", "brust", "er\u00b7regt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der schaden ist zu gro\u00df/ den mir die liebe schl\u00e4gt/", "tokens": ["Der", "scha\u00b7den", "ist", "zu", "gro\u00df", "/", "den", "mir", "die", "lie\u00b7be", "schl\u00e4gt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "PTKA", "ADJD", "$(", "ART", "PPER", "ART", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Climene/ lache nicht/ hier gilt f\u00fcrwahr kein schertzen:", "tokens": ["Cli\u00b7me\u00b7ne", "/", "la\u00b7che", "nicht", "/", "hier", "gilt", "f\u00fcr\u00b7wahr", "kein", "schert\u00b7zen", ":"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "VVFIN", "PTKNEG", "$(", "ADV", "VVFIN", "ADV", "PIAT", "VVINF", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.5": {"text": "Denn eine wunde/ die das hertze selber trifft/", "tokens": ["Denn", "ei\u00b7ne", "wun\u00b7de", "/", "die", "das", "hert\u00b7ze", "sel\u00b7ber", "trifft", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "$(", "PRELS", "ART", "NN", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Hat selten etwas guts/ offt gar den todt gestifft.", "tokens": ["Hat", "sel\u00b7ten", "et\u00b7was", "guts", "/", "offt", "gar", "den", "todt", "ge\u00b7stifft", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PIAT", "NN", "$(", "ADV", "ADV", "ART", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}