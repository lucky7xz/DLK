{"dta.poem.7848": {"metadata": {"author": {"name": "Birken, Sigmund von", "birth": "N.A.", "death": "N.A."}, "title": "Pegnesische  \n Fr\u00fclings-Freude.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1665", "urn": "urn:nbn:de:kobv:b4-200905198447", "language": ["de:0.85", "sv:0.14"], "booktitle": "Birken, Sigmund von: Pegnesische Gespr\u00e4chspiel-Gesellschaft von Nymfen und Hirten. N\u00fcrnberg, 1665."}, "poem": {"stanza.1": {"line.1": {"text": "Edles Brautbett Hochverm\u00e4hlter theurer", "tokens": ["Ed\u00b7les", "Braut\u00b7bett", "Hoch\u00b7ver\u00b7m\u00e4hl\u00b7ter", "theu\u00b7rer"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "NN", "ADJD"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Du solst heisen Immer-gr\u00fcn.", "tokens": ["Du", "solst", "hei\u00b7sen", "Im\u00b7mer\u00b7gr\u00fcn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Gib/ der Nachwelt/ deiner Fr\u00fcchte viel zu-", "tokens": ["Gib", "/", "der", "Nach\u00b7welt", "/", "dei\u00b7ner", "Fr\u00fcch\u00b7te", "viel", "zu"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "$(", "ART", "NN", "$(", "PPOSAT", "NN", "ADV", "TRUNC"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "sey ein ewiges Festin!", "tokens": ["sey", "ein", "e\u00b7wi\u00b7ges", "Fes\u00b7tin", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}}}}}