{"textgrid.poem.50749": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "22", "genre": "verse", "period": "N.A.", "pub_year": 1846, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer ohne Schmerz, der ist auch ohne Liebe,", "tokens": ["Wer", "oh\u00b7ne", "Schmerz", ",", "der", "ist", "auch", "oh\u00b7ne", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "NN", "$,", "PRELS", "VAFIN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wer ohne Leid, der ist auch ohne Treu,", "tokens": ["Wer", "oh\u00b7ne", "Leid", ",", "der", "ist", "auch", "oh\u00b7ne", "Treu", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "NN", "$,", "PRELS", "VAFIN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und dem nur wird die Sonne wolkenfrei,", "tokens": ["Und", "dem", "nur", "wird", "die", "Son\u00b7ne", "wol\u00b7ken\u00b7frei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADV", "VAFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der aus dem Dunkel ringt mit hei\u00dfem Triebe.", "tokens": ["Der", "aus", "dem", "Dun\u00b7kel", "ringt", "mit", "hei\u00b7\u00dfem", "Trie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Bei ", "tokens": ["Bei"], "token_info": ["word"], "pos": ["APPR"], "meter": "-", "measure": "single.down"}, "line.2": {"text": "In wildem Tummel trollt ihr euch herbei", "tokens": ["In", "wil\u00b7dem", "Tum\u00b7mel", "trollt", "ihr", "euch", "her\u00b7bei"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "PPER", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und me\u00dft das Erdreich ohne heil'ge Scheu,", "tokens": ["Und", "me\u00dft", "das", "Er\u00b7dreich", "oh\u00b7ne", "heil'\u00b7ge", "Scheu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Als ob zu hoffen kein Kolumb mehr bliebe!", "tokens": ["Als", "ob", "zu", "hof\u00b7fen", "kein", "Ko\u00b7lumb", "mehr", "blie\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PTKZU", "VVFIN", "PIAT", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Euch ist der eigne Leichnam noch nicht klar,", "tokens": ["Euch", "ist", "der", "eig\u00b7ne", "Leich\u00b7nam", "noch", "nicht", "klar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "ADV", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ihr kennet kaum den Wurm zu euren F\u00fc\u00dfen,", "tokens": ["Ihr", "ken\u00b7net", "kaum", "den", "Wurm", "zu", "eu\u00b7ren", "F\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Blume nicht, die spro\u00dft aus eurem Grab.", "tokens": ["Die", "Blu\u00b7me", "nicht", ",", "die", "spro\u00dft", "aus", "eu\u00b7rem", "Grab", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "$,", "PRELS", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Doch h\u00fcpfet ihr und kr\u00f6nt mit Stroh das Haar,", "tokens": ["Doch", "h\u00fcp\u00b7fet", "ihr", "und", "kr\u00f6nt", "mit", "Stroh", "das", "Haar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "KON", "VVFIN", "APPR", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Gedankenlos als G\u00f6tter euch zu gr\u00fc\u00dfen;", "tokens": ["Ge\u00b7dan\u00b7ken\u00b7los", "als", "G\u00f6t\u00b7ter", "euch", "zu", "gr\u00fc\u00b7\u00dfen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Zweifel fehlt \u2013 und das bricht euch den Stab!", "tokens": ["Der", "Zwei\u00b7fel", "fehlt", "\u2013", "und", "das", "bricht", "euch", "den", "Stab", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$(", "KON", "PDS", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Wer ohne Schmerz, der ist auch ohne Liebe,", "tokens": ["Wer", "oh\u00b7ne", "Schmerz", ",", "der", "ist", "auch", "oh\u00b7ne", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "NN", "$,", "PRELS", "VAFIN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wer ohne Leid, der ist auch ohne Treu,", "tokens": ["Wer", "oh\u