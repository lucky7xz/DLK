{"textgrid.poem.43036": {"metadata": {"author": {"name": "Aston, Louise", "birth": "N.A.", "death": "N.A."}, "title": "Hinaus!", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Seh' ich die V\u00f6gel fliehn in die Nester,", "tokens": ["Seh'", "ich", "die", "V\u00f6\u00b7gel", "fliehn", "in", "die", "Nes\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Ist eine Thr\u00e4ne dem Auge erlaubt;", "tokens": ["Ist", "ei\u00b7ne", "Thr\u00e4\u00b7ne", "dem", "Au\u00b7ge", "er\u00b7laubt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Denn nur die Sorge ist meine Schwester,", "tokens": ["Denn", "nur", "die", "Sor\u00b7ge", "ist", "mei\u00b7ne", "Schwes\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Drum denn hinaus in's Freie! in's Weite!", "tokens": ["Drum", "denn", "hin\u00b7aus", "in's", "Frei\u00b7e", "!", "in's", "Wei\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "ADV", "APZR", "APPRART", "NN", "$.", "APPRART", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Jubelnd begr\u00fc\u00dfen mich, die Befreite,", "tokens": ["Ju\u00b7belnd", "be\u00b7gr\u00fc\u00b7\u00dfen", "mich", ",", "die", "Be\u00b7frei\u00b7te", ","], "token_info": ["word", "word", "wor