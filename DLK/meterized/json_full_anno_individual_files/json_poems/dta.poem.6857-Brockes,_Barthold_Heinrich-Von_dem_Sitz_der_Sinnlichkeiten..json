{"dta.poem.6857": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Sitz der Sinnlichkeiten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Glandul sondert hier von Dingen ohne Zahl,", "tokens": ["Die", "Glan\u00b7dul", "son\u00b7dert", "hier", "von", "Din\u00b7gen", "oh\u00b7ne", "Zahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Woraus die Dr\u00fcck\u2019 entstehn, den Eindruck auf einmal", "tokens": ["Wo\u00b7raus", "die", "Dr\u00fcck", "ent\u00b7stehn", ",", "den", "Ein\u00b7druck", "auf", "ein\u00b7mal"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "VVINF", "$,", "ART", "NN", "APPR", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Schnell von einander ab.", "tokens": ["Schnell", "von", "ein\u00b7an\u00b7der", "ab", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PRF", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Durch Striche des Gesichts,", "tokens": ["Durch", "Stri\u00b7che", "des", "Ge\u00b7sichts", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die ins Gehirn gedr\u00fcckt,", "tokens": ["Die", "ins", "Ge\u00b7hirn", "ge\u00b7dr\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Sind die von dem Geh\u00f6r auch im geringsten nichts", "tokens": ["Sind", "die", "von", "dem", "Ge\u00b7h\u00f6r", "auch", "im", "ge\u00b7rings\u00b7ten", "nichts"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "APPR", "ART", "NN", "ADV", "APPRART", "ADJA", "PIS"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Verhindert noch verr\u00fcckt.", "tokens": ["Ver\u00b7hin\u00b7dert", "noch", "ver\u00b7r\u00fcckt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Zween Sinnen sind durch gleiche Krafft ger\u00fchret,", "tokens": ["Zween", "Sin\u00b7nen", "sind", "durch", "glei\u00b7che", "Krafft", "ge\u00b7r\u00fch\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VAFIN", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Ohn da\u00df man einen Raum der Zeit darin versp\u00fcret:", "tokens": ["Ohn", "da\u00df", "man", "ei\u00b7nen", "Raum", "der", "Zeit", "da\u00b7rin", "ver\u00b7sp\u00fc\u00b7ret", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PIS", "ART", "NN", "ART", "NN", "PAV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Durch einen s\u00fcssen Dunst wird unsre Nas\u2019 erg\u00f6tzt,", "tokens": ["Durch", "ei\u00b7nen", "s\u00fcs\u00b7sen", "Dunst", "wird", "uns\u00b7re", "Nas'", "er\u00b7g\u00f6tzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VAFIN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Indem das Podagra den Fu\u00df in Schmertzen setzt.", "tokens": ["In\u00b7dem", "das", "Po\u00b7da\u00b7gra", "den", "Fu\u00df", "in", "Schmert\u00b7zen", "setzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}