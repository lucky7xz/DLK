{"dta.poem.11558": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "I.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.99"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "Weil schon vor so viel hundert Jahren,", "tokens": ["Weil", "schon", "vor", "so", "viel", "hun\u00b7dert", "Jah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPR", "ADV", "ADV", "CARD", "NN", "$,"], "meter": "-++-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da unsre V\u00e4ter noch Heiden waren,", "tokens": ["Da", "uns\u00b7re", "V\u00e4\u00b7ter", "noch", "Hei\u00b7den", "wa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "NN", "VAFIN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Unser geliebtes Schwabenland", "tokens": ["Un\u00b7ser", "ge\u00b7lieb\u00b7tes", "Schwa\u00b7ben\u00b7land"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "So lustig wie ein Garten stand,", "tokens": ["So", "lus\u00b7tig", "wie", "ein", "Gar\u00b7ten", "stand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So sah der Teufel auch einmal", "tokens": ["So", "sah", "der", "Teu\u00b7fel", "auch", "ein\u00b7mal"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Vom Michelsberg in's Maienthal", "tokens": ["Vom", "Mi\u00b7chels\u00b7berg", "in's", "Mai\u00b7ent\u00b7hal"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPRART", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.7": {"text": "Und auf das weit bebaute Feld.", "tokens": ["Und", "auf", "das", "weit", "be\u00b7bau\u00b7te", "Feld", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Er sprach: das ist ja wohlbestellt;", "tokens": ["Er", "sprach", ":", "das", "ist", "ja", "wohl\u00b7be\u00b7stellt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PDS", "VAFIN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Hier bl\u00fcht, wie einst im Paradies,", "tokens": ["Hier", "bl\u00fcht", ",", "wie", "einst", "im", "Pa\u00b7ra\u00b7dies", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PWAV", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Der Apfelbaum und schmeckt so s\u00fc\u00df!", "tokens": ["Der", "Ap\u00b7fel\u00b7baum", "und", "schmeckt", "so", "s\u00fc\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "VVFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Wir wollen dieses Gartens pflegen,", "tokens": ["Wir", "wol\u00b7len", "die\u00b7ses", "Gar\u00b7tens", "pfle\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PDAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Und soll sich erst kein Pfaff drein legen!", "tokens": ["Und", "soll", "sich", "erst", "kein", "Pfaff", "drein", "le\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PRF", "ADV", "PIAT", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "\u2014 Solch Frevelwort des Satans h\u00f6rt", "tokens": ["Solch", "Fre\u00b7vel\u00b7wort", "des", "Sa\u00b7tans", "h\u00f6rt"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "PIAT", "NN", "ART", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "Der Herr im Himmel ungest\u00f6rt,", "tokens": ["Der", "Herr", "im", "Him\u00b7mel", "un\u00b7ge\u00b7st\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "War aber gar nicht sehr ergezt,", "tokens": ["War", "a\u00b7ber", "gar", "nicht", "sehr", "er\u00b7gezt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "PTKNEG", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Da\u00df sich der Bock zum G\u00e4rtner sezt.", "tokens": ["Da\u00df", "sich", "der", "Bock", "zum", "G\u00e4rt\u00b7ner", "sezt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NE", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Er sandte Bonifazium", "tokens": ["Er", "sand\u00b7te", "Bo\u00b7ni\u00b7fa\u00b7zi\u00b7um"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Damals im deutschen Reich herum,", "tokens": ["Da\u00b7mals", "im", "deut\u00b7schen", "Reich", "he\u00b7rum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Da\u00df er, des heiligen Geistes voll,", "tokens": ["Da\u00df", "er", ",", "des", "hei\u00b7li\u00b7gen", "Geis\u00b7tes", "voll", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "ART", "ADJA", "NN", "ADJD", "$,"], "meter": "---+--+-+", "measure": "iambic.tri.relaxed"}, "line.20": {"text": "Den himmlischen Weinstock pflanzen soll;", "tokens": ["Den", "himm\u00b7li\u00b7schen", "Wein\u00b7stock", "pflan\u00b7zen", "soll", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "VMFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.21": {"text": "So r\u00fcckt er nun auch zum Michelsberg.", "tokens": ["So", "r\u00fcckt", "er", "nun", "auch", "zum", "Mi\u00b7chels\u00b7berg", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "APPRART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.22": {"text": "Das kam dem Satan \u00fcberzwerch,", "tokens": ["Das", "kam", "dem", "Sa\u00b7tan", "\u00fc\u00b7berz\u00b7werch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Th\u00e4t ihm sogleich den Weg verrennen,", "tokens": ["Th\u00e4t", "ihm", "sog\u00b7leich", "den", "Weg", "ver\u00b7ren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Lie\u00df den Boden wie Schwefel brennen,", "tokens": ["Lie\u00df", "den", "Bo\u00b7den", "wie", "Schwe\u00b7fel", "bren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "KOKOM", "NN", "VVINF", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.25": {"text": "H\u00fcllet mit Dampf und Wetterschein", "tokens": ["H\u00fcl\u00b7let", "mit", "Dampf", "und", "Wet\u00b7ter\u00b7schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "KON", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.26": {"text": "Das ganze Revier h\u00f6chst grausam ein,", "tokens": ["Das", "gan\u00b7ze", "Re\u00b7vier", "h\u00f6chst", "grau\u00b7sam", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "ADJD", "PTKVZ", "$,"], "meter": "-+----+-+", "measure": "dactylic.init"}, "line.27": {"text": "Geht selber auf den Heiligen los,", "tokens": ["Geht", "sel\u00b7ber", "auf", "den", "Hei\u00b7li\u00b7gen", "los", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.28": {"text": "Der stand aller irdischen Waffen blo\u00df,", "tokens": ["Der", "stand", "al\u00b7ler", "ir\u00b7di\u00b7schen", "Waf\u00b7fen", "blo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PIAT", "ADJA", "NN", "ADV", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.29": {"text": "Die H\u00e4nde sein zum Himmel kehrt,", "tokens": ["Die", "H\u00e4n\u00b7de", "sein", "zum", "Him\u00b7mel", "kehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Rief: Starker Gott! leih mir ein Schwert!", "tokens": ["Rief", ":", "Star\u00b7ker", "Gott", "!", "leih", "mir", "ein", "Schwert", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ADJA", "NN", "$.", "VVIMP", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Da z\u00fcckt herab, wie ein Donnerstreich,", "tokens": ["Da", "z\u00fcckt", "her\u00b7ab", ",", "wie", "ein", "Don\u00b7ner\u00b7streich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "$,", "PWAV", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.32": {"text": "Erzengel Michael sogleich.", "tokens": ["Er\u00b7zen\u00b7gel", "Mic\u00b7hael", "sog\u00b7leich", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NE", "ADV", "$."], "meter": "+--++-+", "measure": "dactylic.init"}, "line.33": {"text": "Sein Fl\u00fcgel und sein Fu\u00dftritt d\u00e4mpft", "tokens": ["Sein", "Fl\u00fc\u00b7gel", "und", "sein", "Fu\u00df\u00b7tritt", "d\u00e4mpft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Das Feuer schnell, er ficht und k\u00e4mpft", "tokens": ["Das", "Feu\u00b7er", "schnell", ",", "er", "ficht", "und", "k\u00e4mpft"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "$,", "PPER", "ADJD", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Und w\u00fcrgt den Schwarzen blau und gr\u00fcn,", "tokens": ["Und", "w\u00fcrgt", "den", "Schwar\u00b7zen", "blau", "und", "gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "Der h\u00e4tte schier nach Gott geschrien;", "tokens": ["Der", "h\u00e4t\u00b7te", "schier", "nach", "Gott", "ge\u00b7schri\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADJD", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.37": {"text": "Schmei\u00dft ihn der Engel auch alsbald", "tokens": ["Schmei\u00dft", "ihn", "der", "En\u00b7gel", "auch", "als\u00b7bald"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.38": {"text": "Kopfunter in den H\u00f6llenspalt,", "tokens": ["Ko\u00b7pfun\u00b7ter", "in", "den", "H\u00f6l\u00b7len\u00b7spalt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "Schlie\u00dft sich der Boden eilend zu,", "tokens": ["Schlie\u00dft", "sich", "der", "Bo\u00b7den", "ei\u00b7lend", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.40": {"text": "Da war's auf Erden wieder Ruh,", "tokens": ["Da", "wa\u00b7r's", "auf", "Er\u00b7den", "wie\u00b7der", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "NN", "ADV", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.41": {"text": "Die L\u00fcfte flossen leicht und rein,", "tokens": ["Die", "L\u00fcf\u00b7te", "flos\u00b7sen", "leicht", "und", "rein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.42": {"text": "Der Engel sah wie Sonnenschein.", "tokens": ["Der", "En\u00b7gel", "sah", "wie", "Son\u00b7nen\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KOKOM", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.43": {"text": "Unser heiliger bedankt sich sehr,", "tokens": ["Un\u00b7ser", "hei\u00b7li\u00b7ger", "be\u00b7dankt", "sich", "sehr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "VVFIN", "PRF", "ADV", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.44": {"text": "M\u00f6cht' aber noch ein W\u00f6rtlein mehr", "tokens": ["M\u00f6cht'", "a\u00b7ber", "noch", "ein", "W\u00f6rt\u00b7lein", "mehr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "ADV", "ART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.45": {"text": "Mit dem Patronen gern verkehren,", "tokens": ["Mit", "dem", "Pat\u00b7ro\u00b7nen", "gern", "ver\u00b7keh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.46": {"text": "De\u00df wollte Jener sich erwehren,", "tokens": ["De\u00df", "woll\u00b7te", "Je\u00b7ner", "sich", "er\u00b7weh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PDAT", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.47": {"text": "Sprach: Jetzo hab' ich keine Zeit.", "tokens": ["Sprach", ":", "Jet\u00b7zo", "hab'", "ich", "kei\u00b7ne", "Zeit", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ADV", "VAFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.48": {"text": "Da ging Herr Bonifaz so weit,", "tokens": ["Da", "ging", "Herr", "Bo\u00b7ni\u00b7faz", "so", "weit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.49": {"text": "Da\u00df er ihn fa\u00dft an seiner Schwingen,", "tokens": ["Da\u00df", "er", "ihn", "fa\u00dft", "an", "sei\u00b7ner", "Schwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.50": {"text": "Der Engel lie\u00df sich doch nicht zwingen,", "tokens": ["Der", "En\u00b7gel", "lie\u00df", "sich", "doch", "nicht", "zwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.51": {"text": "War wie ein Morgenrauch entschl\u00fcpft.", "tokens": ["War", "wie", "ein", "Mor\u00b7gen\u00b7rauch", "ent\u00b7schl\u00fcpft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KOKOM", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.52": {"text": "Der Mann Gottes stund sehr verbl\u00fcfft.", "tokens": ["Der", "Mann", "Got\u00b7tes", "stund", "sehr", "ver\u00b7bl\u00fcfft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "ADV", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.53": {"text": "Ihm war, wie er mit dem Erzengel rang,", "tokens": ["Ihm", "war", ",", "wie", "er", "mit", "dem", "Er\u00b7zen\u00b7gel", "rang", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "PWAV", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.54": {"text": "Eine Feder, g\u00fclden, sch\u00f6n und lang", "tokens": ["Ei\u00b7ne", "Fe\u00b7der", ",", "g\u00fcl\u00b7den", ",", "sch\u00f6n", "und", "lang"], "token_info": ["word", "word",