{"textgrid.poem.30250": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "64.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Phyllis solte pfeiffen lernen,", "tokens": ["Phyl\u00b7lis", "sol\u00b7te", "pfeif\u00b7fen", "ler\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "VVINF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wolte sich davon entfernen,", "tokens": ["Wol\u00b7te", "sich", "da\u00b7von", "ent\u00b7fer\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "PAV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ward beredet doch zum greiffen,", "tokens": ["Ward", "be\u00b7re\u00b7det", "doch", "zum", "greif\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "ADV", "APPRART", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "So der Grund ist zu dem pfeiffen.", "tokens": ["So", "der", "Grund", "ist", "zu", "dem", "pfeif\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "APPR", "ART", "NN", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.5": {"text": "Als sie dieses nun verstunde,", "tokens": ["Als", "sie", "die\u00b7ses", "nun", "ver\u00b7stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDS", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Lied sie auch die Pfeiff im Munde,", "tokens": ["Lied", "sie", "auch", "die", "Pfeiff", "im", "Mun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wolte sie, war so beflissen,", "tokens": ["Wol\u00b7te", "sie", ",", "war", "so", "be\u00b7flis\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "VAFIN", "ADV", "VVPP", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.8": {"text": "Nimmer ausser Mundes wissen,", "tokens": ["Nim\u00b7mer", "aus\u00b7ser", "Mun\u00b7des", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Liebte sonderlich die Lieder,", "tokens": ["Lieb\u00b7te", "son\u00b7der\u00b7lich", "die", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Die da gingen hoch, nicht nieder:", "tokens": ["Die", "da", "gin\u00b7gen", "hoch", ",", "nicht", "nie\u00b7der", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "ADJD", "$,", "PTKNEG", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Also wil in allen Sachen", "tokens": ["Al\u00b7so", "wil", "in", "al\u00b7len", "Sa\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Nur der Anfang schwer sich machen.", "tokens": ["Nur", "der", "An\u00b7fang", "schwer", "sich", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADJD", "PRF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Phy