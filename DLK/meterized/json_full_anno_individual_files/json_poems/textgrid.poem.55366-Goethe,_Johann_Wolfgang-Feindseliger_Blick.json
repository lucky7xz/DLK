{"textgrid.poem.55366": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Feindseliger Blick", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbdu kommst doch \u00fcber so viele hinaus,", "tokens": ["\u00bb", "du", "kommst", "doch", "\u00fc\u00b7ber", "so", "vie\u00b7le", "hin\u00b7aus", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "ADV", "APPR", "ADV", "PIS", "PTKVZ", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Warum bist du gleich au\u00dferm Haus,", "tokens": ["Wa\u00b7rum", "bist", "du", "gleich", "au\u00b7\u00dferm", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Warum gleich aus dem H\u00e4uschen,", "tokens": ["Wa\u00b7rum", "gleich", "aus", "dem", "H\u00e4usc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "APPR", "ART", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Wenn einer dir mit Brillen spricht?", "tokens": ["Wenn", "ei\u00b7ner", "dir", "mit", "Bril\u00b7len", "spricht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du machst ein ganz verflucht Gesicht", "tokens": ["Du", "machst", "ein", "ganz", "ver\u00b7flucht", "Ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADV", "VVPP", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und bist so