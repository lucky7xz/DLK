{"textgrid.poem.33805": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "Kolibri", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Alles wird dir gern verziehen,", "tokens": ["Al\u00b7les", "wird", "dir", "gern", "ver\u00b7zie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sch\u00f6ner Vogel, selbst dein Neid,", "tokens": ["Sch\u00f6\u00b7ner", "Vo\u00b7gel", ",", "selbst", "dein", "Neid", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "ADV", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Weil dir Sch\u00f6nheit ward verliehen,", "tokens": ["Weil", "dir", "Sch\u00f6n\u00b7heit", "ward", "ver\u00b7lie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Fl\u00fcgel, funkelnd wie Geschmeid,", "tokens": ["Fl\u00fc\u00b7gel", ",", "fun\u00b7kelnd", "wie", "Ge\u00b7schmeid", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVPP", "KOKOM", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Gold, Rubin, Topas, Smaragd, Saphir", "tokens": ["Gold", ",", "Ru\u00b7bin", ",", "To\u00b7pas", ",", "Sma\u00b7ragd", ",", "Sa\u00b7phir"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["NN", "$,", "NE", "$,", "NE", "$,", "NN", "$,", "NE"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Halten sich versteckt aus Furcht vor dir.", "tokens": ["Hal\u00b7ten", "sich", "ver\u00b7steckt", "aus", "Furcht", "vor", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "VVPP", "APPR", "NN", "APPR", "PPER", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Flatternd um die Bl\u00fctenkronen", "tokens": ["Flat\u00b7ternd", "um", "die", "Bl\u00fc\u00b7ten\u00b7kro\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und die Kelche, kleines Ding,", "tokens": ["Und", "die", "Kel\u00b7che", ",", "klei\u00b7nes", "Ding", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "K\u00e4mpfst du w\u00fctend, ohne Schonen,", "tokens": ["K\u00e4mpfst", "du", "w\u00fc\u00b7tend", ",", "oh\u00b7ne", "Scho\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "$,", "KOUI", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit dem Abendschmetterling,", "tokens": ["Mit", "dem", "A\u00b7bendsc\u00b7hmet\u00b7ter\u00b7ling", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Der allein an Glanz nicht deinem weicht,", "tokens": ["Der", "al\u00b7lein", "an", "Glanz", "nicht", "dei\u00b7nem", "weicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "NN", "PTKNEG", "PPOSAT", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Der allein an Sch\u00f6nheit dich erreicht!", "tokens": ["Der", "al\u00b7lein", "an", "Sch\u00f6n\u00b7heit", "dich", "er\u00b7reicht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "NN", "PPER", "VVPP", "$."], "meter": "----+-+-+", "measure": "unknown.measure.tri"}}, "stanza.3": {"line.1": {"text": "Weil in deiner Brust es rascher", "tokens": ["Weil", "in", "dei\u00b7ner", "Brust", "es", "ra\u00b7scher"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PPOSAT", "NN", "PPER", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Als in der des Schw\u00e4rmers pocht,", "tokens": ["Als", "in", "der", "des", "Schw\u00e4r\u00b7mers", "pocht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hat darum der trunkne Nascher", "tokens": ["Hat", "da\u00b7rum", "der", "trunk\u00b7ne", "Na\u00b7scher"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PAV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dich zu solchem Zorn vermocht?", "tokens": ["Dich", "zu", "sol\u00b7chem", "Zorn", "ver\u00b7mocht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PIAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "F\u00fcr den Tag zu sp\u00e4t, der Nacht zu fr\u00fch,", "tokens": ["F\u00fcr", "den", "Tag", "zu", "sp\u00e4t", ",", "der", "Nacht", "zu", "fr\u00fch", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKA", "ADJD", "$,", "ART", "NN", "PTKA", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Scheint er nur ein Bild der Phantasie?", "tokens": ["Scheint", "er", "nur", "ein", "Bild", "der", "Phan\u00b7ta\u00b7sie", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Deinen hei\u00dfen Mut zu z\u00fcgeln,", "tokens": ["Dei\u00b7nen", "hei\u00b7\u00dfen", "Mut", "zu", "z\u00fc\u00b7geln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Glimmt sein feurig Augenpaar,", "tokens": ["Glimmt", "sein", "feu\u00b7rig", "Au\u00b7gen\u00b7paar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJD", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch mit ausgespannten Fl\u00fcgeln", "tokens": ["Doch", "mit", "aus\u00b7ge\u00b7spann\u00b7ten", "Fl\u00fc\u00b7geln"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "St\u00fcrzest du dich, wie der Aar,", "tokens": ["St\u00fcr\u00b7zest", "du", "dich", ",", "wie", "der", "Aar", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "$,", "PWAV", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Auf dein Opfer und beginnst den Streit", "tokens": ["Auf", "dein", "Op\u00b7fer", "und", "be\u00b7ginnst", "den", "Streit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "KON", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Mit dem tr\u00e4gen Sohn der Dunkelheit.", "tokens": ["Mit", "dem", "tr\u00e4\u00b7gen", "Sohn", "der", "Dun\u00b7kel\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Welch ein wunderbares Streiten!", "tokens": ["Welch", "ein", "wun\u00b7der\u00b7ba\u00b7res", "Strei\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schlachtfeld ist die blaue Luft,", "tokens": ["Schlacht\u00b7feld", "ist", "die", "blau\u00b7e", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Kampfpreis sind die S\u00fc\u00dfigkeiten,", "tokens": ["Kampf\u00b7preis", "sind", "die", "S\u00fc\u00b7\u00dfig\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Honigtropfen, Nelkenduft.", "tokens": ["Ho\u00b7nig\u00b7trop\u00b7fen", ",", "Nel\u00b7ken\u00b7duft", "."], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "K\u00e4mpften Ritter je mit stolzrer Zier,", "tokens": ["K\u00e4mpf\u00b7ten", "Rit\u00b7ter", "je", "mit", "stolz\u00b7rer", "Zier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Strahlend in den Waffen beim Turnier?", "tokens": ["Strah\u00b7lend", "in", "den", "Waf\u00b7fen", "beim", "Tur\u00b7nier", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Ward je gr\u00f6\u00dfrer Mut entfaltet,", "tokens": ["Ward", "je", "gr\u00f6\u00df\u00b7rer", "Mut", "ent\u00b7fal\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Gr\u00f6\u00dfere Behendigkeit?", "tokens": ["Gr\u00f6\u00b7\u00dfe\u00b7re", "Be\u00b7hen\u00b7dig\u00b7keit", "?"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--++-+", "measure": "dactylic.init"}, "line.3": {"text": "Ha! wie sich so bunt gestaltet", "tokens": ["Ha", "!", "wie", "sich", "so", "bunt", "ge\u00b7stal\u00b7tet"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "PWAV", "PRF", "ADV", "ADJD", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Jede Wendung in dem Streit!", "tokens": ["Je\u00b7de", "Wen\u00b7dung", "in", "dem", "Streit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie der goldne Schmuck der Federn blinkt", "tokens": ["Wie", "der", "gold\u00b7ne", "Schmuck", "der", "Fe\u00b7dern", "blinkt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Und dann in den Staub zerknittert sinkt!", "tokens": ["Und", "dann", "in", "den", "Staub", "zer\u00b7knit\u00b7tert", "sinkt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "VVPP", "VVFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "Mit dem Schnabel, deiner Lanze,", "tokens": ["Mit", "dem", "Schna\u00b7bel", ",", "dei\u00b7ner", "Lan\u00b7ze", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wirfst du auf den Gegner dich,", "tokens": ["Wirfst", "du", "auf", "den", "Geg\u00b7ner", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ART", "NN", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bringst dem zarten Schuppenglanze", "tokens": ["Bringst", "dem", "zar\u00b7ten", "Schup\u00b7pen\u00b7glan\u00b7ze"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wunden bei mit jedem Stich.", "tokens": ["Wun\u00b7den", "bei", "mit", "je\u00b7dem", "Stich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wohl ihm, wenn er in geschwinder Flucht", "tokens": ["Wohl", "ihm", ",", "wenn", "er", "in", "ge\u00b7schwin\u00b7der", "Flucht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "$,", "KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Vor dem Sieger sich zu retten sucht.", "tokens": ["Vor", "dem", "Sie\u00b7ger", "sich", "zu", "ret\u00b7ten", "sucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PRF", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.8": {"line.1": {"text": "Doch es mahnt das nahe Dunkeln,", "tokens": ["Doch", "es", "mahnt", "das", "na\u00b7he", "Dun\u00b7keln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kolibri, dich an dein Nest.", "tokens": ["Ko\u00b7lib\u00b7ri", ",", "dich", "an", "dein", "Nest", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "K\u00e4fer, mit dir fliegend, funkeln,", "tokens": ["K\u00e4\u00b7fer", ",", "mit", "dir", "flie\u00b7gend", ",", "fun\u00b7keln", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "APPR", "PPER", "VVPP", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "F\u00fchren dich zum Siegesfest.", "tokens": ["F\u00fch\u00b7ren", "dich", "zum", "Sie\u00b7ges\u00b7fest", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ja, dir ward das h\u00f6chste Gl\u00fcck gew\u00e4hrt,", "tokens": ["Ja", ",", "dir", "ward", "das", "h\u00f6chs\u00b7te", "Gl\u00fcck", "ge\u00b7w\u00e4hrt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VAFIN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Fliegend Kleinod, das der Nektar n\u00e4hrt!", "tokens": ["Flie\u00b7gend", "Klei\u00b7nod", ",", "das", "der", "Nek\u00b7tar", "n\u00e4hrt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.9": {"line.1": {"text": "Alles wird dir gern verziehen,", "tokens": ["Al\u00b7les", "wird", "dir", "gern", "ver\u00b7zie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS"