{"dta.poem.2759": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Ein anders.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Zeugt Sternen/ zeugt von meinen lichten Flammen/", "tokens": ["Zeugt", "Ster\u00b7nen", "/", "zeugt", "von", "mei\u00b7nen", "lich\u00b7ten", "Flam\u00b7men", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$(", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich pflag zuvor die Liebe zu verdammen/", "tokens": ["Ich", "pflag", "zu\u00b7vor", "die", "Lie\u00b7be", "zu", "ver\u00b7dam\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das Wort/ dazu mein Mund schien unge\u00fcbt/", "tokens": ["Das", "Wort", "/", "da\u00b7zu", "mein", "Mund", "schien", "un\u00b7ge\u00b7\u00fcbt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PAV", "PPOSAT", "NN", "VVFIN", "ADJD", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Bricht itzt heraus: Ich bin/ ich bin verliebt.", "tokens": ["Bricht", "itzt", "he\u00b7raus", ":", "Ich", "bin", "/", "ich", "bin", "ver\u00b7liebt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PTKVZ", "$.", "PPER", "VAFIN", "$(", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die hohe Macht/ so \u00fcber mich geschlossen/", "tokens": ["Die", "ho\u00b7he", "Macht", "/", "so", "\u00fc\u00b7ber", "mich", "ge\u00b7schlos\u00b7sen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ADV", "APPR", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und meinem Blut die Regung eingegossen/", "tokens": ["Und", "mei\u00b7nem", "Blut", "die", "Re\u00b7gung", "ein\u00b7ge\u00b7gos\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Kan/ wie ein Feur nicht l\u00e4nger heimlich seyn/", "tokens": ["Kan", "/", "wie", "ein", "Feur", "nicht", "l\u00e4n\u00b7ger", "heim\u00b7lich", "seyn", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "$(", "KOKOM", "ART", "NN", "PTKNEG", "ADJD", "ADJD", "VAINF", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sie weist der Welt den reinen Glantz und Schein.", "tokens": ["Sie", "weist", "der", "Welt", "den", "rei\u00b7nen", "Glantz", "und", "Schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Zwar was ich mir zum Grundstein auserwehlet/", "tokens": ["Zwar", "was", "ich", "mir", "zum", "Grund\u00b7stein", "au\u00b7ser\u00b7weh\u00b7let", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "PPER", "PPER", "APPRART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Soll Tugend seyn/ mit der ich mich verm\u00e4hlet:", "tokens": ["Soll", "Tu\u00b7gend", "seyn", "/", "mit", "der", "ich", "mich", "ver\u00b7m\u00e4h\u00b7let", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "VAINF", "$(", "APPR", "PRELS", "PPER", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein edler Geist/ ein Treu-verbundner Muth/", "tokens": ["Ein", "ed\u00b7ler", "Geist", "/", "ein", "Treu\u00b7ver\u00b7bund\u00b7ner", "Muth", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Best\u00e4ndigkeit mein Schmuck und h\u00f6chstes Gut.", "tokens": ["Be\u00b7st\u00e4n\u00b7dig\u00b7keit", "mein", "Schmuck", "und", "h\u00f6chs\u00b7tes", "Gut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "In diesem Brand wil ich unendlich leuchten/", "tokens": ["In", "die\u00b7sem", "Brand", "wil", "ich", "un\u00b7end\u00b7lich", "leuch\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VMFIN", "PPER", "ADJD", "VVINF", "$("], "meter": "-+-+--++-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Aurora mag das frische Gra\u00df befeuchten/", "tokens": ["Au\u00b7ro\u00b7ra", "mag", "das", "fri\u00b7sche", "Gra\u00df", "be\u00b7feuch\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die braune Nacht das matte Feld bethaun/", "tokens": ["Die", "brau\u00b7ne", "Nacht", "das", "mat\u00b7te", "Feld", "be\u00b7thaun", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Man soll mich doch stets unver\u00e4ndert schaun.", "tokens": ["Man", "soll", "mich", "doch", "stets", "un\u00b7ver\u00b7\u00e4n\u00b7dert", "schaun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPER", "ADV", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Und f\u00fchl ich gleich Cupido deine Stricke/", "tokens": ["Und", "f\u00fchl", "ich", "gleich", "Cu\u00b7pi\u00b7do", "dei\u00b7ne", "Stri\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "NE", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mein Fu\u00df und Hertz weicht nimmermehr zur\u00fccke.", "tokens": ["Mein", "Fu\u00df", "und", "Hertz", "weicht", "nim\u00b7mer\u00b7mehr", "zu\u00b7r\u00fc\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich wei\u00df ja wol/ was GOtt und Gl\u00fcck verbindt/", "tokens": ["Ich", "wei\u00df", "ja", "wol", "/", "was", "Gott", "und", "Gl\u00fcck", "ver\u00b7bindt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "$(", "PWS", "NN", "KON", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df da der Mensch Lust und Vergn\u00fcgung findt.", "tokens": ["Da\u00df", "da", "der", "Mensch", "Lust", "und", "Ver\u00b7gn\u00fc\u00b7gung", "findt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.6": {"line.1": {"text": "Komm/ Liebster/ komm; Mein Hertze steht dir offen/", "tokens": ["Komm", "/", "Liebs\u00b7ter", "/", "komm", ";", "Mein", "Hert\u00b7ze", "steht", "dir", "of\u00b7fen", "/"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "NN", "$(", "VVFIN", "$.", "PPOSAT", "NN", "VVFIN", "PPER", "ADJD", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Was kan ich mehr auf dieser Erden hoffen/", "tokens": ["Was", "kan", "ich", "mehr", "auf", "die\u00b7ser", "Er\u00b7den", "hof\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "APPR", "PDAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Als wenn dem Schlu\u00df der Himmel unterschreibt/", "tokens": ["Als", "wenn", "dem", "Schlu\u00df", "der", "Him\u00b7mel", "un\u00b7ter\u00b7schreibt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "ART", "NN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df mir dein Hertz zum Unterpfande bleibt.", "tokens": ["Da\u00df", "mir", "dein", "Hertz", "zum", "Un\u00b7ter\u00b7pfan\u00b7de", "bleibt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Und solt ich nicht dir meine Gluth bekennen?", "tokens": ["Und", "solt", "ich", "nicht", "dir", "mei\u00b7ne", "Gluth", "be\u00b7ken\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PTKNEG", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du weist das Feur/ in dem wir m\u00fcssen brennen/", "tokens": ["Du", "weist", "das", "Feur", "/", "in", "dem", "wir", "m\u00fcs\u00b7sen", "bren\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$(", "APPR", "PRELS", "PPER", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Kein irdisch Dampff/ kein Nebel h\u00e4lt es ein/", "tokens": ["Kein", "ir\u00b7disch", "Dampff", "/", "kein", "Ne\u00b7bel", "h\u00e4lt", "es", "ein", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$(", "PIAT", "NN", "VVFIN", "PPER", "ART", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Es sucht die H\u00f6h\u2019 den Sternen gleich zu seyn.", "tokens": ["Es", "sucht", "die", "H\u00f6h'", "den", "Ster\u00b7nen", "gleich", "zu", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "ADV", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Mehr: will uns doch die Ariadne weichen/", "tokens": ["Mehr", ":", "will", "uns", "doch", "die", "A\u00b7riad\u00b7ne", "wei\u00b7chen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "VMFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$("], "meter": "+---+-+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Der meine Treu in allem sich soll gleichen:", "tokens": ["Der", "mei\u00b7ne", "Treu", "in", "al\u00b7lem", "sich", "soll", "glei\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "APPR", "PIS", "PRF", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und legt uns doch der F\u00fcrst der g\u00fcldnen Rey/", "tokens": ["Und", "legt", "uns", "doch", "der", "F\u00fcrst", "der", "g\u00fcld\u00b7nen", "Rey", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ART", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die Strahlen-Kron der Berenice bey.", "tokens": ["Die", "Strah\u00b7len\u00b7Kron", "der", "Be\u00b7re\u00b7ni\u00b7ce", "bey", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Brennt Sterne/ brennt/ Dolmetscher unser Sinnen/", "tokens": ["Brennt", "Ster\u00b7ne", "/", "brennt", "/", "Dol\u00b7met\u00b7scher", "un\u00b7ser", "Sin\u00b7nen", "/"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$(", "VVFIN", "$(", "NE", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Last eure Glut nicht diese Nacht zerrinnen/", "tokens": ["Last", "eu\u00b7re", "Glut", "nicht", "die\u00b7se", "Nacht", "zer\u00b7rin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "PTKNEG", "PDAT", "NN", "VVINF", "$("], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Das stete Licht/ der unentsunckne Schein/", "tokens": ["Das", "ste\u00b7te", "Licht", "/", "der", "un\u00b7ent\u00b7sunck\u00b7ne", "Schein", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mu\u00df itzt ein Bild von unsrer Liebe seyn.", "tokens": ["Mu\u00df", "itzt", "ein", "Bild", "von", "uns\u00b7rer", "Lie\u00b7be", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}