ne.7": {"text": "Sie necken, schrecken, jagen sich,", "tokens": ["Sie", "ne\u00b7cken", ",", "schre\u00b7cken", ",", "ja\u00b7gen", "sich", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVINF", "$,", "VVFIN", "$,", "VVFIN", "PRF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Sie schlagen und vertragen sich", "tokens": ["Sie", "schla\u00b7gen", "und", "ver\u00b7tra\u00b7gen", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und gr\u00fc\u00dfen keck mit Hohngel\u00e4chter", "tokens": ["Und", "gr\u00fc\u00b7\u00dfen", "keck", "mit", "Hohn\u00b7ge\u00b7l\u00e4ch\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Des Turmes hochgestellten W\u00e4chter. \u2013", "tokens": ["Des", "Tur\u00b7mes", "hoch\u00b7ge\u00b7stell\u00b7ten", "W\u00e4ch\u00b7ter", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "ADJA", "NE", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Der Sommer ging, die Schwalben setzen", "tokens": ["Der", "Som\u00b7mer", "ging", ",", "die", "Schwal\u00b7ben", "set\u00b7zen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sich auf das Kirchendach und schw\u00e4tzen.", "tokens": ["Sich", "auf", "das", "Kir\u00b7chen\u00b7dach", "und", "schw\u00e4t\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "KON", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": 