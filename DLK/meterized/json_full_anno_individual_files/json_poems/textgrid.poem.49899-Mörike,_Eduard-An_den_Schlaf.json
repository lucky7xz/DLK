{"textgrid.poem.49899": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "An den Schlaf", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schlaf! s\u00fc\u00dfer Schlaf! obwohl dem Tod wie du nichts gleicht,", "tokens": ["Schlaf", "!", "s\u00fc\u00b7\u00dfer", "Schlaf", "!", "ob\u00b7wohl", "dem", "Tod", "wie", "du", "nichts", "gleicht", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ADJA", "NN", "$.", "KOUS", "ART", "NN", "KOKOM", "PPER", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Auf diesem Lager doch willkommen hei\u00df ich dich!", "tokens": ["Auf", "die\u00b7sem", "La\u00b7ger", "doch", "will\u00b7kom\u00b7men", "hei\u00df", "ich", "dich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ADV", "ADJD", "VVFIN", "PPER", "PRF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Denn ohne Leben so, wie lieblich lebt es sich!", "tokens": ["Denn", "oh\u00b7ne", "Le\u00b7ben", "so", ",", "wie", "lieb\u00b7lich", "lebt", "es", "sich", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "ADV", "$,", "PWAV", "ADJD", "VVFIN", "PPER", "PRF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So weit vom Sterben, ach, wie stirbt es sich so leicht!", "tokens": ["So", "weit", "vom", "Ster\u00b7ben", ",", "ach", ",", "wie", "stirbt", "es", "sich", "so", "leicht", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "p