{"dta.poem.9513": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Galante gedichte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Hochwerthes jungfern-volck/ ihr holden anmuths-son\u0303en/", "tokens": ["Hoch\u00b7wert\u00b7hes", "jung\u00b7fern\u00b7volck", "/", "ihr", "hol\u00b7den", "an\u00b7muths\u00b7so\u00f1en", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$(", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Jhr auserwehlter schmuck/ der haus und gassen ziert.", "tokens": ["Ihr", "au\u00b7ser\u00b7wehl\u00b7ter", "schmuck", "/", "der", "haus", "und", "gas\u00b7sen", "ziert", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$(", "ART", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wer ist so steinern/ der euch nicht hat lieb gewonnen?", "tokens": ["Wer", "ist", "so", "stei\u00b7nern", "/", "der", "euch", "nicht", "hat", "lieb", "ge\u00b7won\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "VVINF", "$(", "PRELS", "PPER", "PTKNEG", "VAFIN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und welchen habt ihr nicht mit fesseln heimgef\u00fchrt?", "tokens": ["Und", "wel\u00b7chen", "habt", "ihr", "nicht", "mit", "fes\u00b7seln", "heim\u00b7ge\u00b7f\u00fchrt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAT", "VAFIN", "PPER", "PTKNEG", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wer ist so k\u00fchn/ der darff f\u00fcr eure augen treten/", "tokens": ["Wer", "ist", "so", "k\u00fchn", "/", "der", "darff", "f\u00fcr", "eu\u00b7re", "au\u00b7gen", "tre\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "ADJD", "$(", "ART", "VMFIN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wenn ihr die waaren habt der sch\u00f6nheit ausgelegt?", "tokens": ["Wenn", "ihr", "die", "waa\u00b7ren", "habt", "der", "sch\u00f6n\u00b7heit", "aus\u00b7ge\u00b7legt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "VVINF", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wer will euch/ liebste/ nicht als einen Gott anbeten/", "tokens": ["Wer", "will", "euch", "/", "liebs\u00b7te", "/", "nicht", "als", "ei\u00b7nen", "Gott", "an\u00b7be\u00b7ten", "/"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "$(", "VVFIN", "$(", "PTKNEG", "KOUS", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Weil ihr das bildnis seyd/ das Venus selbst gepr\u00e4gt.", "tokens": ["Weil", "ihr", "das", "bild\u00b7nis", "seyd", "/", "das", "Ve\u00b7nus", "selbst", "ge\u00b7pr\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VAFIN", "$(", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Jedoch ich wil nur blo\u00df ein theil von dem ber\u00fchren/", "tokens": ["Je\u00b7doch", "ich", "wil", "nur", "blo\u00df", "ein", "theil", "von", "dem", "be\u00b7r\u00fch\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VMFIN", "ADV", "ADV", "ART", "NN", "APPR", "ART", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Mit welchem die natur euch herrlich hat versehn.", "tokens": ["Mit", "wel\u00b7chem", "die", "na\u00b7tur", "euch", "herr\u00b7lich", "hat", "ver\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ART", "NN", "PPER", "ADJD", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Der sinnen schiff soll mich in solche l\u00e4nder f\u00fchren/", "tokens": ["Der", "sin\u00b7nen", "schiff", "soll", "mich", "in", "sol\u00b7che", "l\u00e4n\u00b7der", "f\u00fch\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VMFIN", "PRF", "APPR", "PIAT", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Wo auff der see voll milch nur liebes-winde wehn.", "tokens": ["Wo", "auff", "der", "see", "voll", "milch", "nur", "lie\u00b7bes\u00b7win\u00b7de", "wehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "ADJD", "ADJD", "ADV", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Die br\u00fcste sind mein zweck/ die sch\u00f6nen marmol-ballen/", "tokens": ["Die", "br\u00fcs\u00b7te", "sind", "mein", "zweck", "/", "die", "sch\u00f6\u00b7nen", "mar\u00b7mol\u00b7bal\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "PPOSAT", "NN", "$(", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Auf welchen Amor ihm ein lust schlo\u00df hat gebaut;", "tokens": ["Auf", "wel\u00b7chen", "A\u00b7mor", "ihm", "ein", "lust", "schlo\u00df", "hat", "ge\u00b7baut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NE", "PPER", "ART", "NN", "VVFIN", "VAFIN", "VVPP", "$."], "meter": "-+-+--+-+--+", "measure": "iambic.penta.relaxed"}, "line.15": {"text": "Die durch das athem-spiel sich heben und auch fallen/", "tokens": ["Die", "durch", "das", "a\u00b7them\u00b7spiel", "sich", "he\u00b7ben", "und", "auch", "fal\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "PRF", "VVINF", "KON", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Auf die der sonne gold wolriechend ambra thaut.", "tokens": ["Auf", "die", "der", "son\u00b7ne", "gold", "wol\u00b7ri\u00b7e\u00b7chend", "am\u00b7bra", "thaut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ART", "ADJA", "NN", "ADJD", "NE", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.17": {"text": "Sie sind ein paradie\u00df/ in welchem \u00e4pffel reiffen/", "tokens": ["Sie", "sind", "ein", "pa\u00b7ra\u00b7die\u00df", "/", "in", "wel\u00b7chem", "\u00e4pf\u00b7fel", "reif\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$(", "APPR", "PWAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Nach derer s\u00fcssen kost iedweder Adam lechst/", "tokens": ["Nach", "de\u00b7rer", "s\u00fcs\u00b7sen", "kost", "ied\u00b7we\u00b7der", "A\u00b7dam", "lechst", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDS", "VVINF", "VMFIN", "PIAT", "NN", "VVFIN", "$("], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.19": {"text": "Zwey felsen/ um die stets des Zephirs winde pfeiffen.", "tokens": ["Zwey", "fel\u00b7sen", "/", "um", "die", "stets", "des", "Ze\u00b7phirs", "win\u00b7de", "pfeif\u00b7fen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "$(", "APPR", "PRELS", "ADV", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Ein garten sch\u00f6ner tracht/ wo die vergn\u00fcgung w\u00e4chst.", "tokens": ["Ein", "gar\u00b7ten", "sch\u00f6\u00b7ner", "tracht", "/", "wo", "die", "ver\u00b7gn\u00fc\u00b7gung", "w\u00e4chst", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$(", "PWAV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Ein \u00fcber-irrdisch bild/ dem alle opffern m\u00fcssen.", "tokens": ["Ein", "\u00fc\u00b7ber\u00b7irr\u00b7disch", "bild", "/", "dem", "al\u00b7le", "opf\u00b7fern", "m\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$(", "ART", "PIAT", "NN", "VMFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Ein ausgeputzt altar/ f\u00fcr dem die welt sich beugt.", "tokens": ["Ein", "aus\u00b7ge\u00b7putzt", "al\u00b7tar", "/", "f\u00fcr", "dem", "die", "welt", "sich", "beugt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$(", "APPR", "PRELS", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Ein crystallinen quell/ aus welchem str\u00f6me fl\u00fcssen/", "tokens": ["Ein", "crys\u00b7tal\u00b7li\u00b7nen", "quell", "/", "aus", "wel\u00b7chem", "str\u00f6\u00b7me", "fl\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "APPR", "PWAT", "ADJA", "VVINF", "$("], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.24": {"text": "Davon die s\u00fc\u00dfigkeit den Nectar \u00fcbersteigt.", "tokens": ["Da\u00b7von", "die", "s\u00fc\u00b7\u00dfig\u00b7keit", "den", "Nec\u00b7tar", "\u00fc\u00b7bers\u00b7teigt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Sie sind zwey schwestern/ die in einem bette schlaffen/", "tokens": ["Sie", "sind", "zwey", "schwes\u00b7tern", "/", "die", "in", "ei\u00b7nem", "bet\u00b7te", "schlaf\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "CARD", "VVINF", "$(", "ART", "APPR", "ART", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Davon die eine doch die andre keinmahl dr\u00fcckt.", "tokens": ["Da\u00b7von", "die", "ei\u00b7ne", "doch", "die", "and\u00b7re", "kein\u00b7mahl", "dr\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "ART", "ADV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Zwey kammern welche voll von blancken liebes-waffen/", "tokens": ["Zwey", "kam\u00b7mern", "wel\u00b7che", "voll", "von", "blan\u00b7cken", "lie\u00b7bes\u00b7waf\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "VVFIN", "PIS", "ADJD", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Aus denen Cypripor die g\u00f6ldnen pfetle schickt.", "tokens": ["Aus", "de\u00b7nen", "Cyp\u00b7ri\u00b7por", "die", "g\u00f6ld\u00b7nen", "pfet\u00b7le", "schickt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "NE", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Sie sind ein zeher leim/ woran die sinnen kleben;", "tokens": ["Sie", "sind", "ein", "ze\u00b7her", "leim", "/", "wo\u00b7ran", "die", "sin\u00b7nen", "kle\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$(", "PWAV", "ART", "ADJA", "VVINF", "$."], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.30": {"text": "Ein feuer/ welches macht die k\u00e4ltste hertzen warm;", "tokens": ["Ein", "feu\u00b7er", "/", "wel\u00b7ches", "macht", "die", "k\u00e4lts\u00b7te", "hert\u00b7zen", "warm", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PWS", "VVFIN", "ART", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Ein bezoar/ der auch entseelten giebt das leben;", "tokens": ["Ein", "be\u00b7zoar", "/", "der", "auch", "ent\u00b7seel\u00b7ten", "giebt", "das", "le\u00b7ben", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "$(", "ART", "ADV", "VVINF", "VVFIN", "PDS", "VVINF", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.32": {"text": "Ein solcher schatz/ f\u00fcr dem das reichthum selbst ist arm.", "tokens": ["Ein", "sol\u00b7cher", "schatz", "/", "f\u00fcr", "dem", "das", "reicht\u00b7hum", "selbst", "ist", "arm", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "$(", "APPR", "ART", "ART", "NN", "ADV", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Ein kr\u00e4fftig himmel-brod/ das die verliebten schmecken;", "tokens": ["Ein", "kr\u00e4ff\u00b7tig", "him\u00b7mel\u00b7brod", "/", "das", "die", "ver\u00b7lieb\u00b7ten", "schme\u00b7cken", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$(", "PRELS", "ART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Ein alabaster hau\u00df/ so mit rubinen prahlt;", "tokens": ["Ein", "a\u00b7la\u00b7bas\u00b7ter", "hau\u00df", "/", "so", "mit", "ru\u00b7bi\u00b7nen", "prahlt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ADV", "APPR", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Ein s\u00fcsser honigseim/ den matte seelen lecken;", "tokens": ["Ein", "s\u00fcs\u00b7ser", "ho\u00b7ni\u00b7gseim", "/", "den", "mat\u00b7te", "see\u00b7len", "le\u00b7cken", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "ADJA", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Ein himmel/ wo das heer der liebes-sterne strahlt.", "tokens": ["Ein", "him\u00b7mel", "/", "wo", "das", "heer", "der", "lie\u00b7bes\u00b7ster\u00b7ne", "strahlt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PWAV", "ART", "NN", "ART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Ein scharff geschliffen schwerd/ das tieffe wunden hauet/", "tokens": ["Ein", "scharff", "ge\u00b7schlif\u00b7fen", "schwerd", "/", "das", "tief\u00b7fe", "wun\u00b7den", "hau\u00b7et", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVPP", "VAFIN", "$(", "ART", "ADJA", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Ein rofen-strauch/ der auch im winter rosen bringt.", "tokens": ["Ein", "ro\u00b7fen\u00b7strauch", "/", "der", "auch", "im", "win\u00b7ter", "ro\u00b7sen", "bringt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "ADV", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Ein meer/ worauff man der Syrenen kr\u00e4ffte schauet/", "tokens": ["Ein", "meer", "/", "wo\u00b7rauff", "man", "der", "Sy\u00b7re\u00b7nen", "kr\u00e4ff\u00b7te", "schau\u00b7et", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PWAV", "PIS", "ART", "NN", "VVFIN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Von denen das ges\u00e4ng bi\u00df in die seele dringt.", "tokens": ["Von", "de\u00b7nen", "das", "ge\u00b7s\u00e4ng", "bi\u00df", "in", "die", "see\u00b7le", "dringt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ART", "NN", "APPR", "APPR", "ART", "