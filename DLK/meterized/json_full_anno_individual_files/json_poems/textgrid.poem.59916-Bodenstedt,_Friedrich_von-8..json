{"textgrid.poem.59916": {"metadata": {"author": {"name": "Bodenstedt, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "8.", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn alle Gl\u00e4ubigen die rechten Pfade gehn,", "tokens": ["Wenn", "al\u00b7le", "Gl\u00e4u\u00b7bi\u00b7gen", "die", "rech\u00b7ten", "Pfa\u00b7de", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So bleibt mir nichts, als ihnen nachzusehn \u2013", "tokens": ["So", "bleibt", "mir", "nichts", ",", "als", "ih\u00b7nen", "nach\u00b7zu\u00b7sehn", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIS", "$,", "KOUS", "PPER", "VVIZU", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wie aber k\u00f6nnte ich dabei bestehn!", "tokens": ["Wie", "a\u00b7ber", "k\u00f6nn\u00b7te", "ich", "da\u00b7bei", "be\u00b7stehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VMFIN", "PPER", "PAV", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wenn jeder Durstige selbst sucht den Weg zum Quell,", "tokens": ["Wenn", "je\u00b7der", "Durs\u00b7ti\u00b7ge", "selbst", "sucht", "den", "Weg", "zum", "Quell", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "ADV", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der ihm entgegenrieselt klar und hell,", "tokens": ["Der", "ihm", "ent\u00b7ge\u00b7gen\u00b7rie\u00b7selt", "klar", "und", "hell", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Bin ich ein \u00fcberfl\u00fcssiger Gesell.", "tokens": ["Bin", "ich", "ein", "\u00fc\u00b7berf\u00b7l\u00fcs\u00b7si\u00b7ger", "Ge\u00b7sell", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Doch lieber tr\u00fcbe ich die Quellen allesamt,", "tokens": ["Doch", "lie\u00b7ber", "tr\u00fc\u00b7be", "ich", "die", "Quel\u00b7len", "al\u00b7le\u00b7samt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als da\u00df ich wank' und weich' aus meinem Amt \u2013", "tokens": ["Als", "da\u00df", "ich", "wank'", "und", "weich'", "aus", "mei\u00b7nem", "Amt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "VVFIN", "KON", "VVFIN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wer mir nicht folgen will: der sei verdammt!", "tokens": ["Wer", "mir", "nicht", "fol\u00b7gen", "will", ":", "der", "sei", "ver\u00b7dammt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PTKNEG", "VVINF", "VMFIN", "$.", "ART", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Wenn alle Gl\u00e4ubigen die rechten Pfade gehn,", "tokens": ["Wenn", "al\u00b7le", "Gl\u00e4u\u00b7bi\u00b7gen", "die", "rech\u00b7ten", "Pfa\u00b7de", "gehn", ","], "toke