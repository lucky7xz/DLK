Cha\u00b7tre", "fiel", "ihr", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Sie rang voll Gluth die sch\u00f6nen H\u00e4nde,", "tokens": ["Sie", "rang", "voll", "Gluth", "die", "sch\u00f6\u00b7nen", "H\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Und rief im sch\u00f6nsten letzten Act:", "tokens": ["Und", "rief", "im", "sch\u00f6ns\u00b7ten", "letz\u00b7ten", "Act", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Ach der Contract, ach der Contract!", "tokens": ["Ach", "der", "Con\u00b7tract", ",", "ach", "der", "Con\u00b7tract", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "ART", "NN", "$,", "ADV", "ART", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.27": {"text": "Und damit ging das St\u00fcck zu Ende.", "tokens": ["Und", "da\u00b7mit", "ging", "das", "St\u00fcck", "zu", "En\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Nun trug man in dem Publicum", "tokens": ["Nun", "trug", "man", "in", "dem", "Pub\u00b7li\u00b7cum"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Den kl\u00e4glichen Contract herum,", "tokens": ["Den", "kl\u00e4g\u00b7li\u00b7chen", "Con\u00b7tract", "he\u00b7rum", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Und lachte selbst an Ludwigs Hofe", "tokens": ["Und", "lach\u00b7te", "selbst", "an", "Lud\u00b7wigs", "Ho\u00b7fe"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "APPR", "NE", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.31": {"text": "Von der Prinzessinn bis zur Zofe,", "tokens": ["Von", "der", "Prin\u00b7zes\u00b7sinn", "bis", "zur", "Zo\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "APPRART", "NN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.32": {"text": "Und sprach und spielte manchen Act", "tokens": ["Und", "sprach", "und", "spiel\u00b7te", "man\u00b7chen", "Act"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Von dem Contract.", "tokens": ["Von", "dem", "Con\u00b7tract", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}