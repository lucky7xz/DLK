{"textgrid.poem.52980": {"metadata": {"author": {"name": "Seume, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der zauberischen Buhlerinn,", "genre": "verse", "period": "N.A.", "pub_year": 1786, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der zauberischen Buhlerinn,", "tokens": ["Der", "zau\u00b7be\u00b7ri\u00b7schen", "Buh\u00b7le\u00b7rinn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der sch\u00f6nen ", "tokens": ["Der", "sch\u00f6\u00b7nen"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Die ihr vermuthlich alle kennt;", "tokens": ["Die", "ihr", "ver\u00b7muth\u00b7lich", "al\u00b7le", "kennt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Schnell mu\u00df er fort zum Regiment,", "tokens": ["Schnell", "mu\u00df", "er", "fort", "zum", "Re\u00b7gi\u00b7ment", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VMFIN", "PPER", "PTKVZ", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und fordert nun mit hei\u00dfem Beben", "tokens": ["Und", "for\u00b7dert", "nun", "mit", "hei\u00b7\u00dfem", "Be\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Contract der Treu aufs ganze Leben.", "tokens": ["Con\u00b7tract", "der", "Treu", "aufs", "gan\u00b7ze", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Mi\u00df Ninon l\u00e4chelte und schrieb,", "tokens": ["Mi\u00df", "Ni\u00b7non", "l\u00e4\u00b7chel\u00b7te", "und", "schrieb", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Da ihr nichts weiter \u00fcbrig blieb,", "tokens": ["Da", "ihr", "nichts", "wei\u00b7ter", "\u00fcb\u00b7rig", "blieb", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Hei\u00df, wie die h\u00f6chste Flamme brennt,", "tokens": ["Hei\u00df", ",", "wie", "die", "h\u00f6chs\u00b7te", "Flam\u00b7me", "brennt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Der treusten Liebe Testament.", "tokens": ["Der", "treus\u00b7ten", "Lie\u00b7be", "Tes\u00b7ta\u00b7ment", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Nun lie\u00df der gute Mann sich tr\u00f6sten,", "tokens": ["Nun", "lie\u00df", "der", "gu\u00b7te", "Mann", "sich", "tr\u00f6s\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Besah das Blatt, wie einen Zauberring,", "tokens": ["Be\u00b7sah", "das", "Blatt", ",", "wie", "ei\u00b7nen", "Zau\u00b7ber\u00b7ring", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "PWAV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Und k\u00fc\u00dfte sie und ihre Schrift, und ging.", "tokens": ["Und", "k\u00fc\u00df\u00b7te", "sie", "und", "ih\u00b7re", "Schrift", ",", "und", "ging", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "KON", "PPOSAT", "NN", "$,", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Doch kaum war er bey der Armee, so l\u00f6sten", "tokens": ["Doch", "kaum", "war", "er", "bey", "der", "Ar\u00b7mee", ",", "so", "l\u00f6s\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "PPER", "APPR", "ART", "NN", "$,", "ADV", "VVFIN"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.15": {"text": "Gem\u00e4chlich alle Schw\u00fcre sich", "tokens": ["Ge\u00b7m\u00e4ch\u00b7lich", "al\u00b7le", "Schw\u00fc\u00b7re", "sich"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "PIAT", "NN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Bey Ninon auf, und kurze Zeit verstrich,", "tokens": ["Bey", "Ni\u00b7non", "auf", ",", "und", "kur\u00b7ze", "Zeit", "ver\u00b7strich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "PTKVZ", "$,", "KON", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "So spielte sie die feuervolle,", "tokens": ["So", "spiel\u00b7te", "sie", "die", "feu\u00b7er\u00b7vol\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Nat\u00fcrliche und allerliebste Rolle", "tokens": ["Na\u00b7t\u00fcr\u00b7li\u00b7che", "und", "al\u00b7ler\u00b7