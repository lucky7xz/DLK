{"textgrid.poem.42403": {"metadata": {"author": {"name": "Lachmann, Hedwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Vielleicht dass einst, wenn wie in Luft zerronnen", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vielleicht dass einst, wenn wie in Luft zerronnen", "tokens": ["Viel\u00b7leicht", "dass", "einst", ",", "wenn", "wie", "in", "Luft", "zer\u00b7ron\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "ADV", "$,", "KOUS", "KOKOM", "APPR", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sich dein Gelebtes deinem Blick entwand,", "tokens": ["Sich", "dein", "Ge\u00b7leb\u00b7tes", "dei\u00b7nem", "Blick", "ent\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PPOSAT", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dir m\u00fcd im Schosse liegt die welke Hand,", "tokens": ["Dir", "m\u00fcd", "im", "Schos\u00b7se", "liegt", "die", "wel\u00b7ke", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPRART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Du wieder m\u00fcnden wirst, wo du begonnen.", "tokens": ["Du", "wie\u00b7der", "m\u00fcn\u00b7den", "wirst", ",", "wo", "du", "be\u00b7gon\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJD", "VAFIN", "$,", "PWAV", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Vielleicht dass da aus heimlichen Verstecken", "tokens": ["Viel\u00b7leicht", "dass", "da", "aus", "heim\u00