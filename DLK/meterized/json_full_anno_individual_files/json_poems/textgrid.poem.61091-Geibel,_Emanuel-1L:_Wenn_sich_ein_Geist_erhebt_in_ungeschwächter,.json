{"textgrid.poem.61091": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wenn sich ein Geist erhebt in ungeschw\u00e4chter,", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn sich ein Geist erhebt in ungeschw\u00e4chter,", "tokens": ["Wenn", "sich", "ein", "Geist", "er\u00b7hebt", "in", "un\u00b7ge\u00b7schw\u00e4ch\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "VVFIN", "APPR", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Erhabner W\u00fcrde mit gewalt'gem Schritte,", "tokens": ["Er\u00b7hab\u00b7ner", "W\u00fcr\u00b7de", "mit", "ge\u00b7walt'\u00b7gem", "Schrit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zu stolz, da\u00df er des Haufens Gunst erbitte,", "tokens": ["Zu", "stolz", ",", "da\u00df", "er", "des", "Hau\u00b7fens", "Gunst", "er\u00b7bit\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "$,", "KOUS", "PPER", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So wird er oft dem niedern zum Gel\u00e4chter.", "tokens": ["So", "wird", "er", "oft", "dem", "nie\u00b7dern", "zum", "Ge\u00b7l\u00e4ch\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ART", "ADJA", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "So gingest du, der treue Kronenw\u00e4chter", "tokens": ["So", "gin\u00b7gest", "du", ",", "der",