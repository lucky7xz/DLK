a"}}, "stanza.10": {"line.1": {"text": "Wir sitzen gleich beym Abend-Essen,", "tokens": ["Wir", "sit\u00b7zen", "gleich", "beym", "A\u00b7ben\u00b7dEs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So reicht man uns dein Schreiben ein:", "tokens": ["So", "reicht", "man", "uns", "dein", "Schrei\u00b7ben", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PPER", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Louyse ist uns unvergessen,", "tokens": ["Lou\u00b7yse", "ist", "uns", "un\u00b7ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Was kan uns angenehmer seyn?", "tokens": ["Was", "kan", "uns", "an\u00b7ge\u00b7neh\u00b7mer", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Kommt zu uns, schreibst du, lieben Kinder!", "tokens": ["Kommt", "zu", "uns", ",", "schreibst", "du", ",", "lie\u00b7ben", "Kin\u00b7der", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "$,", "VVFIN", "PPER", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Kan ich nicht kommen, so kommt ihr;", "tokens": ["Kan", "ich", "nicht", "kom\u00b7men", ",", "so", "kommt", "ihr", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "VVINF", "$,", "ADV", "VVFIN", "PPER", "$."], "meter": "-+-+--+-", "measu