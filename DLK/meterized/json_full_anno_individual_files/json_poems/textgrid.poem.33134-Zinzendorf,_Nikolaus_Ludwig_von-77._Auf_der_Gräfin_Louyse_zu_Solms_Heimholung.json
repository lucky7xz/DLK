{"textgrid.poem.33134": {"metadata": {"author": {"name": "Zinzendorf, Nikolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "77. Auf der Gr\u00e4fin Louyse zu Solms Heimholung", "genre": "verse", "period": "N.A.", "pub_year": 1730, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die kleine M\u00fch, das kurze Streiten", "tokens": ["Die", "klei\u00b7ne", "M\u00fch", ",", "das", "kur\u00b7ze", "Strei\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Bringt unaussprechlich s\u00fcsse Ruh,", "tokens": ["Bringt", "un\u00b7aus\u00b7sprech\u00b7lich", "s\u00fcs\u00b7se", "Ruh", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die tiefsten Gottes-Heimlichkeiten", "tokens": ["Die", "tiefs\u00b7ten", "Got\u00b7tes\u00b7Heim\u00b7lich\u00b7kei\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Aus Zion fliessen denen zu,", "tokens": ["Aus", "Zi\u00b7on", "flies\u00b7sen", "de\u00b7nen", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "PDS", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die aller Dinge sich enthalten,", "tokens": ["Die", "al\u00b7ler", "Din\u00b7ge", "sich", "ent\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Auch nicht das Kleinste r\u00fchren an:", "tokens": ["Auch", "nicht", "das", "Kleins\u00b7te", "r\u00fch\u00b7ren", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "L\u00e4\u00dft man den Br\u00e4utigam nur walten,", "tokens": ["L\u00e4\u00dft", "man", "den", "Br\u00e4u\u00b7ti\u00b7gam", "nur", "wal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ART", "NE", "ADV", "VVINF", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.8": {"text": "So sieht man, was die Liebe kan.", "tokens": ["So", "sieht", "man", ",", "was", "die", "Lie\u00b7be", "kan", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "$,", "PRELS", "ART", "NN", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die Liebe kr\u00f6nt des Lamms Jungfrauen,", "tokens": ["Die", "Lie\u00b7be", "kr\u00f6nt", "des", "Lamms", "Jung\u00b7frau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und f\u00fchrt sie vor des Vaters Thron,", "tokens": ["Und", "f\u00fchrt", "sie", "vor", "des", "Va\u00b7ters", "Thron", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den nur ein reines Herz darf schauen;", "tokens": ["Den", "nur", "ein", "rei\u00b7nes", "Herz", "darf", "schau\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "ADJA", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Liebe wird der Keuschheit Lohn.", "tokens": ["Die", "Lie\u00b7be", "wird", "der", "Keuschheit", "Lohn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Mir ists so neulich als von gestern.", "tokens": ["Mir", "ists", "so", "neu\u00b7lich", "als", "von", "ge\u00b7stern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "KOKOM", "APPR", "ADV", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "J\u00fcngst tratest du in unser Chor,", "tokens": ["J\u00fcngst", "tra\u00b7test", "du", "in", "un\u00b7ser", "Chor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Louyse, theureste der Schwestern;", "tokens": ["Lou\u00b7yse", ",", "theu\u00b7res\u00b7te", "der", "Schwes\u00b7tern", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "ART", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.8": {"text": "So sang man dir die Worte vor.", "tokens": ["So", "sang", "man", "dir", "die", "Wor\u00b7te", "vor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Kaum, da\u00df uns dein Gesicht erschienen,", "tokens": ["Kaum", ",", "da\u00df", "uns", "dein", "Ge\u00b7sicht", "er\u00b7schie\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Kaum da\u00df dein Glaub uns aufgewekt,", "tokens": ["Kaum", "da\u00df", "dein", "Glaub", "uns", "auf\u00b7ge\u00b7wekt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPOSAT", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die wir des K\u00f6nigs Willen dienen,", "tokens": ["Die", "wir", "des", "K\u00f6\u00b7nigs", "Wil\u00b7len", "die\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "(doch leider! noch nicht unbeflekt,)", "tokens": ["(", "doch", "lei\u00b7der", "!", "noch", "nicht", "un\u00b7be\u00b7flekt", ",", ")"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "ADV", "$.", "ADV", "PTKNEG", "ADJD", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So ward es wieder weggenommen.", "tokens": ["So", "ward", "es", "wie\u00b7der", "weg\u00b7ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Louyse solte Bielitz sehn;", "tokens": ["Lou\u00b7yse", "sol\u00b7te", "Bie\u00b7litz", "sehn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "In Bielitz auch zur Ruhe kommen.", "tokens": ["In", "Bie\u00b7litz", "auch", "zur", "Ru\u00b7he", "kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "So wolte Gott! so ists geschehn.", "tokens": ["So", "wol\u00b7te", "Gott", "!", "so", "ists", "ge\u00b7schehn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "NN", "$.", "ADV", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wir sitzen gleich beym Abend-Essen,", "tokens": ["Wir", "sit\u00b7zen", "gleich", "beym", "A\u00b7ben\u00b7dEs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So reicht man uns dein Schreiben ein:", "tokens": ["So", "reicht", "man", "uns", "dein", "Schrei\u00b7ben", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PPER", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Louyse ist uns unvergessen,", "tokens": ["Lou\u00b7yse", "ist", "uns", "un\u00b7ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Was kan uns angenehmer seyn?", "tokens": ["Was", "kan", "uns", "an\u00b7ge\u00b7neh\u00b7mer", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Kommt zu uns, schreibst du, lieben Kinder!", "tokens": ["Kommt", "zu", "uns", ",", "schreibst", "du", ",", "lie\u00b7ben", "Kin\u00b7der", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "$,", "VVFIN", "PPER", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Kan ich nicht kommen, so kommt ihr;", "tokens": ["Kan", "ich", "nicht", "kom\u00b7men", ",", "so", "kommt", "ihr", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "VVINF", "$,", "ADV", "VVFIN", "PPER", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Der Liebe, meinem Ueberwinder,", "tokens": ["Der", "Lie\u00b7be", ",", "mei\u00b7nem", "Ue\u00b7ber\u00b7win\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ist auch das Bielitzer Revier.", "tokens": ["Ist", "auch", "das", "Bie\u00b7lit\u00b7zer", "Re\u00b7vier", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+--++", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Ich kan nicht mehr, so gern ich schriebe;", "tokens": ["Ich", "kan", "nicht", "mehr", ",", "so", "gern", "ich", "schrie\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "ADV", "$,", "ADV", "ADV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So schlossest du das theure Blat,", "tokens": ["So", "schlos\u00b7sest", "du", "das", "theu\u00b7re", "Blat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gnug, da\u00df mein Herz euch in die Liebe,", "tokens": ["Gnug", ",", "da\u00df", "mein", "Herz", "euch", "in", "die", "Lie\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPOSAT", "NN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "++-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die mich besitzt, ergeben hat.", "tokens": ["Die", "mich", "be\u00b7sitzt", ",", "er\u00b7ge\u00b7ben", "hat", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$,", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich lieb euch aller End und Orten", "tokens": ["Ich", "lieb", "euch", "al\u00b7ler", "End", "und", "Or\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PIAT", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mit Schwesterlicher Z\u00e4rtlichkeit,", "tokens": ["Mit", "Schwes\u00b7ter\u00b7li\u00b7cher", "Z\u00e4rt\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Dein Vater endigt mit den Worten:", "tokens": ["Dein", "Va\u00b7ter", "en\u00b7digt", "mit", "den", "Wor\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Mein Kind ist ausser Ort und Zeit.", "tokens": ["Mein", "Kind", "ist", "aus\u00b7ser", "Ort", "und", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "So ists! wir spielen mit dem Sterben.", "tokens": ["So", "ists", "!", "wir", "spie\u00b7len", "mit", "dem", "Ster\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "$.", "PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die H\u00fctte ist bald abgelegt,", "tokens": ["Die", "H\u00fct\u00b7te", "ist", "bald", "ab\u00b7ge\u00b7legt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn nur das s\u00fcndliche Verderben", "tokens": ["Wenn", "nur", "das", "s\u00fcnd\u00b7li\u00b7che", "Ver\u00b7der\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Seele nicht mehr niederschl\u00e4gt.", "tokens": ["Die", "See\u00b7le", "nicht", "mehr", "nie\u00b7der\u00b7schl\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Bey t\u00e4glich ausgestandnem Tode,", "tokens": ["Bey", "t\u00e4g\u00b7lich", "aus\u00b7ge\u00b7stand\u00b7nem", "To\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Hats mit dem Tode keine Noth;", "tokens": ["Hats", "mit", "dem", "To\u00b7de", "kei\u00b7ne", "Noth", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "PIAT", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Nach Josua und Calebs Mode", "tokens": ["Nach", "Jo\u00b7sua", "und", "Ca\u00b7lebs", "Mo\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "KON", "NE", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Fri\u00dft ihn ein Gottes-Mensch wie Brod.", "tokens": ["Fri\u00dft", "ihn", "ein", "Got\u00b7tes\u00b7Mensch", "wie", "Brod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "KOKOM", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Die kleine M\u00fch, das kurze Streiten", "tokens": ["Die", "klei\u00b7ne", "M\u00fch", ",", "das", "kur\u00b7ze", "Strei\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Bringt unaussprechlich s\u00fcsse Ruh,", "tokens": ["Bringt", "un\u00b7aus\u00b7sprech\u00b7lich", "s\u00fcs\u00b7se", "Ruh", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die tiefsten Gottes-Heimlichkeiten", "tokens": ["Die", "tiefs\u00b7ten", "Got\u00b7tes\u00b7Heim\u00b7lich\u00b7kei\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Aus Zion fliessen denen zu,", "tokens": ["Aus", "Zi\u00b7on", "flies\u00b7sen", "de\u00b7nen", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "PDS", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die aller Dinge sich enthalten,", "tokens": ["Die", "al\u00b7ler", "Din\u00b7ge", "sich", "ent\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Auch nicht das Kleinste r\u00fchren an:", "tokens": ["Auch", "nicht", "das", "Kleins\u00b7te", "r\u00fch\u00b7ren", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "L\u00e4\u00dft man den