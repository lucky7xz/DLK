tra"}, "line.19": {"text": "Und die ich, r\u00fcckt der Feind heran,", "tokens": ["Und", "die", "ich", ",", "r\u00fcckt", "der", "Feind", "he\u00b7ran", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRELS", "PPER", "$,", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Schnell, wie mich selbst, verbergen kann.", "tokens": ["Schnell", ",", "wie", "mich", "selbst", ",", "ver\u00b7ber\u00b7gen", "kann", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWAV", "PPER", "ADV", "$,", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "So sprach die Schnecke. Junker Schr\u00f6ter", "tokens": ["So", "sprach", "die", "Schne\u00b7cke", ".", "Jun\u00b7ker", "Schr\u00f6\u00b7ter"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "NE", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Bestieg noch einmal den Katheder;", "tokens": ["Be\u00b7stieg", "noch", "ein\u00b7mal", "den", "Ka\u00b7the\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.23": {"text": "Allein das Lied des Schaalthiers blieb", "tokens": ["Al\u00b7lein", "das", "Lied", "des", "Scha\u00b7al\u00b7thiers", "blieb"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+---", "measure": "unknown.measure.tri"}, "line.24": {"text": "Noch immer auf der alten Weise.", "tokens": ["Noch", "im\u00b7mer", "auf", "der", "al\u00b7ten", "Wei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "me