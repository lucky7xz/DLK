{"textgrid.poem.63012": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein Schr\u00f6ter, der mit einer Schnecke", "genre": "verse", "period": "N.A.", "pub_year": 1786, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Schr\u00f6ter, der mit einer Schnecke", "tokens": ["Ein", "Schr\u00f6\u00b7ter", ",", "der", "mit", "ei\u00b7ner", "Schne\u00b7cke"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Im Schatten einer Wei\u00dfdornhecke", "tokens": ["Im", "Schat\u00b7ten", "ei\u00b7ner", "Wei\u00df\u00b7dorn\u00b7he\u00b7cke"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Spatzieren kroch, gerieth mit ihr", "tokens": ["Spat\u00b7zie\u00b7ren", "kroch", ",", "ge\u00b7rieth", "mit", "ihr"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "$,", "VVFIN", "APPR", "PPOSAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In Streit, und zwar der H\u00f6rner wegen.", "tokens": ["In", "Streit", ",", "und", "zwar", "der", "H\u00f6r\u00b7ner", "we\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "KON", "ADV", "ART", "NN", "APPR", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Kaum tr\u00e4gt ein junger Offizier", "tokens": ["Kaum", "tr\u00e4gt", "ein", "jun\u00b7ger", "Of\u00b7fi\u00b7zier"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So stolz den neuen Troddeldegen", "tokens": ["So", "stolz", "den", "neu\u00b7en", "Trod\u00b7del\u00b7de\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Als Junker Schr\u00f6ter sein Geweih.", "tokens": ["Als", "Jun\u00b7ker", "Schr\u00f6\u00b7ter", "sein", "Ge\u00b7weih", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der Hirsch, dem wir am meisten gleichen,", "tokens": ["Der", "Hirsch", ",", "dem", "wir", "am", "meis\u00b7ten", "glei\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "PTKA", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Sprach er, mu\u00df ohne Prahlerey,", "tokens": ["Sprach", "er", ",", "mu\u00df", "oh\u00b7ne", "Prah\u00b7le\u00b7rey", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "VMFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Mit seinem Kopfputz meinem weichen:", "tokens": ["Mit", "sei\u00b7nem", "Kopf\u00b7putz", "mei\u00b7nem", "wei\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPOSAT", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Er dienet mir, du weist es schon,", "tokens": ["Er", "die\u00b7net", "mir", ",", "du", "weist", "es", "schon", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PPER", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Zur Hand und wie dem Krebs zur Scheere,", "tokens": ["Zur", "Hand", "und", "wie", "dem", "Krebs", "zur", "Schee\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "PWAV", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Im Krieg zum Schutz und Trutzgewehre,", "tokens": ["Im", "Krieg", "zum", "Schutz", "und", "Trutz\u00b7ge\u00b7weh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPRART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Und.... Alles gut, mein lieber Sohn,", "tokens": ["Und", "....", "Al\u00b7les", "gut", ",", "mein", "lie\u00b7ber", "Sohn", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$(", "PIS", "ADJD", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Und doch m\u00f6cht ich mit dir nicht tauschen;", "tokens": ["Und", "doch", "m\u00f6cht", "ich", "mit", "dir", "nicht", "tau\u00b7schen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PPER", "APPR", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Auf meinen H\u00f6rnern hat die Macht", "tokens": ["Auf", "mei\u00b7nen", "H\u00f6r\u00b7nern", "hat", "die", "Macht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Des Zevs zwey Augen angebracht,", "tokens": ["Des", "Zevs", "zwey", "Au\u00b7gen", "an\u00b7ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "CARD", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Wodurch ich die Gefahr belauschen,", "tokens": ["Wo\u00b7durch", "ich", "die", "Ge\u00b7fahr", "be\u00b7lau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Und die ich, r\u00fcckt der Feind heran,", "tokens": ["Und", "die", "ich", ",", "r\u00fcckt", "der", "Feind", "he\u00b7ran", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRELS", "PPER", "$,", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Schnell, wie mich selbst, verbergen kann.", "tokens": ["Schnell", ",", "wie", "mich", "selbst", ",", "ver\u00b7ber\u00b7gen", "kann", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWAV", "PPER", "ADV", "$,", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "So sprach die Schnecke. Junker Schr\u00f6ter", "tokens": ["So", "sprach", "die", "Schne\u00b7cke", ".", "Jun\u00b7ker", "Schr\u00f6\u00b7ter"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "NE", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Bestieg noch einmal den Katheder;", "tokens": ["Be\u00b7stieg", "noch", "ein\u00b7mal", "den", "Ka\u00b7the\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.23": {"text": "Allein das Lied des Schaalthiers blieb", "tokens": ["Al\u00b7lein", "das", "Lied", "des", "Scha\u00b7al\u00b7thiers", "blieb"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+---", "measure": "unknown.measure.tri"}, "line.24": {"text": "Noch immer auf der alten Weise.", "tokens": ["Noch", "im\u00b7mer", "auf", "der", "al\u00b7ten", "Wei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Ein Amor, der auf einer Reise", "tokens": ["Ein", "A\u00b7mor", ",", "der", "auf", "ei\u00b7ner", "Rei\u00b7se"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NE", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Als Schmetterling sein Wesen trieb,", "tokens": ["Als", "Schmet\u00b7ter\u00b7ling", "sein", "We\u00b7sen", "trieb", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Und sich, um auszuruhn, ins Gr\u00fcne", "tokens": ["Und", "sich", ",", "um", "aus\u00b7zu\u00b7ruhn", ",", "ins", "Gr\u00fc\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KON", "PRF", "$,", "KOUI", "VVIZU", "$,", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Herablie\u00df, mu\u00dfte Schiedsmann seyn.", "tokens": ["He\u00b7rab\u00b7lie\u00df", ",", "mu\u00df\u00b7te", "Schieds\u00b7mann", "seyn", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VMFIN", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Ich, sprach er mit gelehrter Miene,", "tokens": ["Ich", ",", "sprach", "er", "mit", "ge\u00b7lehr\u00b7ter", "Mie\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "Bin f\u00fcr die H\u00f6rner, die man sein", "tokens": ["Bin", "f\u00fcr", "die", "H\u00f6r\u00b7ner", ",", "die", "man", "sein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "APPR", "ART", "NN", "$,", "PRELS", "PIS", "VAINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Verbergen kann; doch d\u00e4cht ich w\u00e4ren", "tokens": ["Ver\u00b7ber\u00b7gen", "kann", ";", "doch", "d\u00e4cht", "ich", "w\u00e4\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VMFIN", "$.", "ADV", "VVFIN", "PPER", "VAFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.32": {"text": "Die Augen f\u00fcglich zu entbehren.", "tokens": ["Die", "Au\u00b7gen", "f\u00fcg\u00b7lich", "zu", "ent\u00b7beh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.33": {"text": "Ey, rief die Schnecke, Freund, wie so?", "tokens": ["Ey", ",", "rief", "die", "Schne\u00b7cke", ",", "Freund", ",", "wie", "so", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ART", "NN", "$,", "NN", "$,", "PWAV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Allein der kleine Schelm entfloh,", "tokens": ["Al\u00b7lein", "der", "klei\u00b7ne", "Schelm", "ent\u00b7floh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Anstatt das R\u00e4thsel aufzukl\u00e4ren.", "tokens": ["An\u00b7statt", "das", "R\u00e4th\u00b7sel", "auf\u00b7zu\u00b7kl\u00e4\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVIZU", "$."], "meter"