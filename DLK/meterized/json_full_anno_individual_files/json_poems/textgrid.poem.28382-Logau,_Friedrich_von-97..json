{"textgrid.poem.28382": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "97.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als wie in dieser Stund ein Freund zum Freunde k\u00fcmmt", "tokens": ["Als", "wie", "in", "die\u00b7ser", "Stund", "ein", "Freund", "zum", "Freun\u00b7de", "k\u00fcmmt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "KOKOM", "APPR", "PDAT", "NN", "ART", "NN", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und dann in jener Stund auch wieder Abschied nimmt,", "tokens": ["Und", "dann", "in", "je\u00b7ner", "Stund", "auch", "wie\u00b7der", "Ab\u00b7schied", "nimmt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PDAT", "NN", "ADV", "ADV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So habt ihr mich, ich euch, O Vater! nur begr\u00fcst,", "tokens": ["So", "habt", "ihr", "mich", ",", "ich", "euch", ",", "O", "Va\u00b7ter", "!", "nur", "be\u00b7gr\u00fcst", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "$,", "PPER", "PPER", "$,", "NE", "NN", "$.", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So habt ihr mich, ich euch gehabt und auch vermist", "tokens": ["So", "habt", "ihr", "mich", ",", "ich", "euch", "ge\u00b7habt", "und", "auch", "ver\u00b7mist"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "$,", "PPER", "PPER", "VAPP", "KON", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Gar inner kurtzen Zeit, da Titans g\u00f6ldnes Rund", "tokens": ["Gar", "in\u00b7ner", "kurt\u00b7zen", "Zeit", ",", "da", "Ti\u00b7tans", "g\u00f6ld\u00b7nes", "Rund"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "ADJA", "NN", "$,", "KOUS", "NE", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Noch nicht zu meinem Jahr auff halbem Wege stund.", "tokens": ["Noch", "nicht", "zu", "mei\u00b7nem", "Jahr", "auff", "hal\u00b7bem", "We\u00b7ge", "stund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "APPR", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wie kummts? Ein zartes Kind hat keinen sichren Raum,", "tokens": ["Wie", "kummts", "?", "Ein", "zar\u00b7tes", "Kind", "hat", "kei\u00b7nen", "sich\u00b7ren", "Raum", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "$.", "ART", "ADJA", "NN", "VAFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wo da ein br\u00fcnstig Hengst laufft frey von Stang und Zaum.", "tokens": ["Wo", "da", "ein", "br\u00fcns\u00b7tig", "Hengst", "laufft", "frey", "von", "Stang", "und", "Zaum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "ADJD", "NN", "VVFIN", "ADJD", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Die Welt rast, tobt, schaumt, strampfft; der Laster Sprung und Streich", "tokens": ["Die", "Welt", "rast", ",", "tobt", ",", "schaumt", ",", "strampfft", ";", "der", "Las\u00b7ter", "Sprung", "und", "Streich"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$.", "ART", "NN", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ist nicht ein Ding f\u00fcr mich, die Engel sind mir gleich;", "tokens": ["Ist", "nicht", "ein", "Ding", "f\u00fcr", "mich", ",", "die", "En\u00b7gel", "sind", "mir", "gleich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ART", "NN", "APPR", "PPER", "$,", "ART", "NN", "VAFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Der Himmel ist ein Land f\u00fcr mich und meinen Geist,", "tokens": ["Der", "Him\u00b7mel", "ist", "ein", "Land", "f\u00fcr", "mich", "und", "mei\u00b7nen", "Geist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "APPR", "PPER", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Der mich dem frechen Volck der S\u00fcnd entweichen heist,", "tokens": ["Der", "mich", "dem", "fre\u00b7chen", "Volck", "der", "S\u00fcnd", "ent\u00b7wei\u00b7chen", "heist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ART", "ADJA", "NN", "ART", "NN", "VVINF", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Eh als den stillen Sinn das \u00fcbergoldte Gifft", "tokens": ["Eh", "als", "den", "stil\u00b7len", "Sinn", "das", "\u00fc\u00b7ber\u00b7gold\u00b7te", "Gifft"], "toke