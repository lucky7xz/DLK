{"dta.poem.10798": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Die  \n Geburt des Menschen ein Wun-  \n der der Gottheit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198774", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Als GOtt die grosse Welt in ihren\nStand gebracht,", "tokens": ["Als", "Gott", "die", "gros\u00b7se", "Welt", "in", "ih\u00b7ren", "Stand", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hat er die kleine Welt, den Menschen", "tokens": ["Hat", "er", "die", "klei\u00b7ne", "Welt", ",", "den", "Men\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sein kr\u00e4fftig Allmachts-Wort, sein unumschr\u00e4nk-", "tokens": ["Sein", "kr\u00e4ff\u00b7tig", "All\u00b7machts\u00b7Wort", ",", "sein", "un\u00b7um\u00b7schr\u00e4nk"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "ADJD", "NN", "$,", "PPOSAT", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Zog seinen Leib hervor aus einem Klumpen Er-", "tokens": ["Zog", "sei\u00b7nen", "Leib", "her\u00b7vor", "aus", "ei\u00b7nem", "Klum\u00b7pen", "Er"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKVZ", "APPR", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der Leib der Seelen-Haus, der Glieder ihr Ge-", "tokens": ["Der", "Leib", "der", "See\u00b7len\u00b7Haus", ",", "der", "Glie\u00b7der", "ihr", "Ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "$,", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ist seiner weisen Macht vollkomnes Meisterst\u00fck;", "tokens": ["Ist", "sei\u00b7ner", "wei\u00b7sen", "Macht", "voll\u00b7kom\u00b7nes", "Meis\u00b7ter\u00b7st\u00fck", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Es ist ein Wunderwerk daran recht zuerkennen,", "tokens": ["Es", "ist", "ein", "Wun\u00b7der\u00b7werk", "da\u00b7ran", "recht", "zu\u00b7er\u00b7ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PAV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wie ", "tokens": ["Wie"], "token_info": ["word"], "pos": ["PWAV"], "meter": "+", "measure": "single.up"}, "line.9": {"text": "Des ersten Menschen Leib, der wie ein Thon ge-", "tokens": ["Des", "ers\u00b7ten", "Men\u00b7schen", "Leib", ",", "der", "wie", "ein", "Thon", "ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN", "$,", "PRELS", "KOKOM", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "War ein recht sichtbar Bild von seiner Majest\u00e4t,", "tokens": ["War", "ein", "recht", "sicht\u00b7bar", "Bild", "von", "sei\u00b7ner", "Ma\u00b7jes\u00b7t\u00e4t", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJD", "ADJD", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Es war der Stamm gesezt zum menschlichen Ge-", "tokens": ["Es", "war", "der", "Stamm", "ge\u00b7sezt", "zum", "menschli\u00b7chen", "Ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVPP", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.12": {"text": "Der durch die Wunderkrafft nun ferner Fr\u00fcchte", "tokens": ["Der", "durch", "die", "Wun\u00b7der\u00b7krafft", "nun", "fer\u00b7ner", "Fr\u00fcch\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "ADV", "ADV", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Der Weisheit Rathschlus war, es solt des Man-", "tokens": ["Der", "Weis\u00b7heit", "Rath\u00b7schlus", "war", ",", "es", "solt", "des", "Man"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "NE", "VAFIN", "$,", "PPER", "VMFIN", "ART", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Die Rippe selbsten leihn, zu einem ersten Weib,", "tokens": ["Die", "Rip\u00b7pe", "selbs\u00b7ten", "leihn", ",", "zu", "ei\u00b7nem", "ers\u00b7ten", "Weib", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVINF", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Und nachmahls solte denn aus keuschen Liebes-", "tokens": ["Und", "nach\u00b7mahls", "sol\u00b7te", "denn", "aus", "keu\u00b7schen", "Lie\u00b7bes"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VMFIN", "ADV", "APPR", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Das Wachsthum des Geschlechts von Mann und", "tokens": ["Das", "Wach\u00b7sthum", "des", "Ge\u00b7schlechts", "von", "Mann", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "APPR", "NN", "KON"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Wie wunderbar ist es da\u00df durch die Liebesglut,", "tokens": ["Wie", "wun\u00b7der\u00b7bar", "ist", "es", "da\u00df", "durch", "die", "Lie\u00b7bes\u00b7glut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPER", "KOUS", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Die Welt bev\u00f6lkert wird, da\u00df von dem Fleisch und", "tokens": ["Die", "Welt", "be\u00b7v\u00f6l\u00b7kert", "wird", ",", "da\u00df", "von", "dem", "Fleisch", "und"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVPP", "VAFIN", "$,", "KOUS", "APPR", "ART", "NN", "KON"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Der ersten Menschen sind drauf nach und nach ent-", "tokens": ["Der", "ers\u00b7ten", "Men\u00b7schen", "sind", "drauf", "nach", "und", "nach", "ent"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PAV", "APPR", "KON", "APPR", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Die vor dem sind gewest, und noch iezund vorhan-", "tokens": ["Die", "vor", "dem", "sind", "ge\u00b7west", ",", "und", "noch", "ie\u00b7zund", "vor\u00b7han"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "VAFIN", "VVPP", "$,", "KON", "ADV", "ADV", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Wer dieses nur bedenkt, dem f\u00e4lt gleich dabei ein,", "tokens": ["Wer", "die\u00b7ses", "nur", "be\u00b7denkt", ",", "dem", "f\u00e4lt", "gleich", "da\u00b7bei", "ein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PDAT", "ADV", "VVFIN", "$,", "PRELS", "VVFIN", "ADV", "PAV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Es mu\u00df ein ewger ", "tokens": ["Es", "mu\u00df", "ein", "ew\u00b7ger"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "ADJA"], "meter": "-+-+-", "measure": "iambic.di"}, "line.23": {"text": "Es kan ja nimmermehr, von ohngefehr geschehen,", "tokens": ["Es", "kan", "ja", "nim\u00b7mer\u00b7mehr", ",", "von", "ohn\u00b7ge\u00b7fehr", "ge\u00b7sche\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "$,", "APPR", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Da\u00df Menschen wiederum von Menschen so ent-", "tokens": ["Da\u00df", "Men\u00b7schen", "wie\u00b7de\u00b7rum", "von", "Men\u00b7schen", "so", "ent"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "ADV", "APPR", "NN", "ADV", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "Als die Erfahrung lehrt; Es bringt die Fortpflan-", "tokens": ["Als", "die", "Er\u00b7fah\u00b7rung", "lehrt", ";", "Es", "bringt", "die", "Fort\u00b7pflan"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$.", "PPER", "VVFIN", "ART", "TRUNC"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.26": {"text": "Des menschlichen Geschlechts uns zur Verwunde-", "tokens": ["Des", "menschli\u00b7chen", "Ge\u00b7schlechts", "uns", "zur", "Ver\u00b7wun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PPER", "APPRART", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.27": {"text": "Und w\u00e4r kein ewger GOtt, bemerkt ihr blinden Toh-", "tokens": ["Und", "w\u00e4r", "kein", "ew\u00b7ger", "Gott", ",", "be\u00b7merkt", "ihr", "blin\u00b7den", "Toh"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PIAT", "ADJA", "NN", "$,", "VVFIN", "PPOSAT", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Wie w\u00e4ret ihr denn wol, so wunderbar gebohren?", "tokens": ["Wie", "w\u00e4\u00b7ret", "ihr", "denn", "wol", ",", "so", "wun\u00b7der\u00b7bar", "ge\u00b7boh\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "ADV", "$,", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Jhr Gottsverl\u00e4ugner ihr, die ihr so sp\u00f6ttisch lacht,", "tokens": ["Ihr", "Gotts\u00b7ver\u00b7l\u00e4ug\u00b7ner", "ihr", ",", "die", "ihr", "so", "sp\u00f6t\u00b7tisch", "lacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "$,", "PRELS", "PPER", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Wenn man euch lehren wil, da\u00df euch ein ", "tokens": ["Wenn", "man", "euch", "leh\u00b7ren", "wil", ",", "da\u00df", "euch", "ein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PIS", "PPER", "VVINF", "VMFIN", "$,", "KOUS", "PPER", "ART"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.31": {"text": "Wie ist nicht die Geburt ein Wunderwerk zu nen-", "tokens": ["Wie", "ist", "nicht", "die", "Ge\u00b7burt", "ein", "Wun\u00b7der\u00b7werk", "zu", "nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PTKNEG", "ART", "NN", "ART", "NN", "APPR", "TRUNC"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.32": {"text": "Woran der Gottheit Gr\u00f6s recht herrlich zu erken-", "tokens": ["Wo\u00b7ran", "der", "Got\u00b7theit", "Gr\u00f6s", "recht", "herr\u00b7lich", "zu", "er\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "NN", "ADV", "ADJD", "APPR", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "O! Menschen geht zur\u00fck; und denkt den Ursprung", "tokens": ["O", "!", "Men\u00b7schen", "geht", "zu\u00b7r\u00fck", ";", "und", "denkt", "den", "Ur\u00b7sprung"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$.", "NN", "VVFIN", "PTKVZ", "$.", "KON", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.34": {"text": "Was war eur erster Stoff woraus sich allgemach", "tokens": ["Was", "war", "eur", "ers\u00b7ter", "Stoff", "wo\u00b7raus", "sich", "all\u00b7ge\u00b7mach"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "PPOSAT", "ADJA", "NN", "PWAV", "PRF", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Des K\u00f6rpers netter Bau gestrekt, geformt, gef\u00fc-", "tokens": ["Des", "K\u00f6r\u00b7pers", "net\u00b7ter", "Bau", "ge\u00b7strekt", ",", "ge\u00b7formt", ",", "ge\u00b7f\u00fc"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["ART", "NN", "ADJA", "NN", "VVPP", "$,", "VVPP", "$,", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Denkt wie entsteht der Mensch, der in der Mutter", 