{"dta.poem.12693": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf einen andern.  \n  D.  V. A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der schmied nimmt eisen an, und was noch h\u00e4rter ist,", "tokens": ["Der", "schmied", "nimmt", "ei\u00b7sen", "an", ",", "und", "was", "noch", "h\u00e4r\u00b7ter", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "PTKVZ", "$,", "KON", "PWS", "ADV", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "K\u00f6mmt seinem hammer zu; Was ", "tokens": ["K\u00f6mmt", "sei\u00b7nem", "ham\u00b7mer", "zu", ";", "Was"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKVZ", "$.", "PWS"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Hat uns, und heute zwar, sein meister-st\u00fcck gelehret;", "tokens": ["Hat", "uns", ",", "und", "heu\u00b7te", "zwar", ",", "sein", "meis\u00b7ter\u00b7st\u00fcck", "ge\u00b7leh\u00b7ret", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "KON", "ADV", "ADV", "$,", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wann jener pinckt und schl\u00e4gt, und der in b\u00fcchern list.", "tokens": ["Wann", "je\u00b7ner", "pinckt", "und", "schl\u00e4gt", ",", "und", "der", "in", "b\u00fc\u00b7chern", "list", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDS", "VVFIN", "KON", "VVFIN", "$,", "KON", "ART", "APPR", "ADJA"