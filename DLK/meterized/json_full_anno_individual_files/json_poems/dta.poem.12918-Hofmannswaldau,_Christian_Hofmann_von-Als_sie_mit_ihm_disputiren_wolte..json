{"dta.poem.12918": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Als sie mit ihm disputiren wolte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ljebwerthe Magdalis! ich mag nicht disputiren,", "tokens": ["Ljeb\u00b7wert\u00b7he", "Mag\u00b7da\u00b7lis", "!", "ich", "mag", "nicht", "dis\u00b7pu\u00b7ti\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$.", "PPER", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wer unter uns das recht auf seiner seiten hat.", "tokens": ["Wer", "un\u00b7ter", "uns", "das", "recht", "auf", "sei\u00b7ner", "sei\u00b7ten", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "PPER", "ART", "ADJD", "APPR", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Es w\u00fcrde der proce\u00df mir allzuschwer zu f\u00fchren.", "tokens": ["Es", "w\u00fcr\u00b7de", "der", "pro\u00b7ce\u00df", "mir", "all\u00b7zu\u00b7schwer", "zu", "f\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PPER", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Liebe giebt vielleicht den allerbesten rath.", "tokens": ["Die", "Lie\u00b7be", "giebt", "viel\u00b7leicht", "den", "al\u00b7ler\u00b7bes\u00b7ten", "rath", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Sie spricht: Ey gieb ihr recht, was auch ihr mund beschl\u00fc\u00dft.", "tokens": ["Sie", "spricht", ":", "Ey", "gieb", "ihr", "recht", ",", "was", "auch", "ihr", "mund", "be\u00b7schl\u00fc\u00dft", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "NN", "VVIMP", "PPER", "ADJD", "$,", "PRELS", "ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und dieses thu ich auch, dieweil du sch\u00f6ne bist.", "tokens": ["Und", "die\u00b7ses", "thu", "ich", "auch", ",", "die\u00b7weil", "du", "sch\u00f6\u00b7ne", "bist", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "PPER", "ADV", "$,", "KOUS", "PPER", "ADJA", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}