{"textgrid.poem.60538": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "1L: Fahr wohl, fahr hin, o Jahr! nimm fort mit dir im Scheiden", "genre": "verse", "period": "N.A.", "pub_year": 1839, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fahr wohl, fahr hin, o Jahr! nimm fort mit dir im Scheiden", "tokens": ["Fahr", "wohl", ",", "fahr", "hin", ",", "o", "Jahr", "!", "nimm", "fort", "mit", "dir", "im", "Schei\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "$,", "VVFIN", "PTKVZ", "$,", "FM", "NN", "$.", "VVIMP", "PTKVZ", "APPR", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "All deine Lust, nur la\u00df nicht liegen mir die Leiden!", "tokens": ["All", "dei\u00b7ne", "Lust", ",", "nur", "la\u00df", "nicht", "lie\u00b7gen", "mir", "die", "Lei\u00b7den", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "$,", "ADV", "VVIMP", "PTKNEG", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "O k\u00f6nnt ich hinter dir die Pforte schlie\u00dfen, \u2013 h\u00f6ren,", "tokens": ["O", "k\u00f6nnt", "ich", "hin\u00b7ter", "dir", "die", "Pfor\u00b7te", "schlie\u00b7\u00dfen", ",", "\u2013", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "APPR", "PPER", "ART", "NN", "VVINF", "$,", "$(", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie deine Tritte sich in stiller Nacht verl\u00f6ren!", "tokens": ["Wie", "dei\u00b7ne", "Trit\u00b7te", "sich", "in", "stil\u00b7ler", "Nacht", "ver\u00b7l\u00f6\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "PRF", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Jetzt nah und schon so fern, wie auf der Flucht ein Reiter,", "tokens": ["Jetzt", "nah", "und", "schon", "so", "fern", ",", "wie", "auf", "der", "Flucht", "ein", "Rei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADV", "ADV", "ADJD", "$,", "PWAV", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df mein Ged\u00e4chtnis, m\u00fcd, nicht folgen k\u00f6nnte weiter,", "tokens": ["Da\u00df", "mein", "Ge\u00b7d\u00e4cht\u00b7nis", ",", "m\u00fcd", ",", "nicht", "fol\u00b7gen", "k\u00f6nn\u00b7te", "wei\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "$,", "ADJD", "$,", "PTKNEG", "VVINF", "VMFIN", "ADV", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Wie einem Reitersmann des Weges noch ein St\u00fccke", "tokens": ["Wie", "ei\u00b7nem", "Rei\u00b7ters\u00b7mann", "des", "We\u00b7ges", "noch", "ein", "St\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nachbellend folgt der Hund, und still dann kehrt zur\u00fccke!", "tokens": ["Nach\u00b7bel\u00b7lend", "folgt", "der", "Hund", ",", "und", "still", "dann", "kehrt", "zu\u00b7r\u00fc\u00b7cke", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "ART", "NN", "$,", "KON", "ADJD", "ADV", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Doch ist dies eitler Wahn, des Weges nimmer m\u00fcde,", "tokens": ["Doch", "ist", "dies", "eit\u00b7ler", "Wahn", ",", "des", "We\u00b7ges", "nim\u00b7mer", "m\u00fc\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PDS", "ADJA", "NN", "$,", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Folgt deinen Spuren nach, wohl bis er stirbt, mein R\u00fcde.", "tokens": ["Folgt", "dei\u00b7nen", "Spu\u00b7ren", "nach", ",", "wohl", "bis", "er", "stirbt", ",", "mein", "R\u00fc\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKVZ", "$,", "ADV", "KOUS", "PPER", "VVFIN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Fahr hin, unholdes Jahr! mir warst du von den schlimmen;", "tokens": ["Fahr", "hin", ",", "un\u00b7hol\u00b7des", "Jahr", "!", "mir", "warst", "du", "von", "den", "schlim\u00b7men", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "ADJA", "NN", "$.", "PPER", "VAFIN", "PPER", "APPR", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es m\u00f6gen andre dir ein Liedlein Dankes stimmen.", "tokens": ["Es", "m\u00f6\u00b7gen", "and\u00b7re", "dir", "ein", "Lied\u00b7lein", "Dan\u00b7kes", "stim\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIS", "PPER", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Die andern?! \u2013 strafend will die Scham mich \u00fcberkommen,", "tokens": ["Die", "an\u00b7dern", "?!", "\u2013", "stra\u00b7fend", "will", "die", "Scham", "mich", "\u00fc\u00b7ber\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$.", "$(", "ADJD", "VMFIN", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df ich, was andern frommt, nicht mir auch lie\u00dfe frommen.", "tokens": ["Da\u00df", "ich", ",", "was", "an\u00b7dern", "frommt", ",", "nicht", "mir", "auch", "lie\u00b7\u00dfe", "from\u00b7men", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "PRELS", "PIS", "VVFIN", "$,", "PTKNEG", "PPER", "ADV", "VVFIN", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Was gilt mein K\u00f6rnlein Schmerz, was gilt mein L\u00fcftchen Klage,", "tokens": ["Was", "gilt", "mein", "K\u00f6rn\u00b7lein", "Schmerz", ",", "was", "gilt", "mein", "L\u00fcft\u00b7chen", "Kla\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPOSAT", "NN", "NN", "$,", "PWS", "VVFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "O scheidend Jahr, wenn ich den letzten Gru\u00df dir sage?", "tokens": ["O", "schei\u00b7dend", "Jahr", ",", "wenn", "ich", "den", "letz\u00b7ten", "Gru\u00df", "dir", "sa\u00b7ge", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "NN", "$,", "KOUS", "PPER", "ART", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Doch l\u00e4\u00dft mein Herz auch nur vom Weltgeschick sich f\u00fchren,", "tokens": ["Doch", "l\u00e4\u00dft", "mein", "Herz", "auch", "nur", "vom", "Welt\u00b7ge\u00b7schick", "sich", "f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "ADV", "ADV", "APPRART", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Kann mich dein Scheiden nicht zu Dankestr\u00e4nen r\u00fchren.", "tokens": ["Kann", "mich", "dein", "Schei\u00b7den", "nicht", "zu", "Dan\u00b7kes\u00b7tr\u00e4\u00b7nen", "r\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "PTKNEG", "PTKZU", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Zwar hie\u00df dein wahres Wort manch L\u00fcgenbild erblassen,", "tokens": ["Zwar", "hie\u00df", "dein", "wah\u00b7res", "Wort", "manch", "L\u00fc\u00b7gen\u00b7bild", "er\u00b7blas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch war dein Lieben matt, doch war zu k\u00fchl dein Hassen.", "tokens": ["Doch", "war", "dein", "Lie\u00b7ben", "matt", ",", "doch", "war", "zu", "k\u00fchl", "dein", "Has\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPOSAT", "NN", "PTKVZ", "$,", "ADV", "VAFIN", "PTKA", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.11": {"line.1": {"text": "Zwar hast du unserm Heil den Weg gebahnt von Eisen;", "tokens": ["Zwar", "hast", "du", "un\u00b7serm", "Heil", "den", "Weg", "ge\u00b7bahnt", "von", "Ei\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "NN", "ART", "NN", "VVPP", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch eisern mochte nicht dein Wille sich erweisen.", "tokens": ["Doch", "ei\u00b7sern", "moch\u00b7te", "nicht", "dein", "Wil\u00b7le", "sich", "er\u00b7wei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PTKNEG", "PPOSAT", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.12": {"line.1": {"text": "Noch f\u00e4hrt der Nachtgeist fort zu siegen und zu schrecken,", "tokens": ["Noch", "f\u00e4hrt", "der", "Nacht\u00b7geist", "fort", "zu", "sie\u00b7gen", "und", "zu", "schre\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKVZ", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Auf neuen Feldern stets sein Lager abzustecken.", "tokens": ["Auf", "neu\u00b7en", "Fel\u00b7dern", "stets", "sein", "La\u00b7ger", "ab\u00b7zu\u00b7ste\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "PPOSAT", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.13": {"line.1": {"text": "Eins sei gebeten, Jahr: was du getan, gesonnen,", "tokens": ["Eins", "sei", "ge\u00b7be\u00b7ten", ",", "Jahr", ":", "was", "du", "ge\u00b7tan", ",", "ge\u00b7son\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "VAFIN", "VVPP", "$,", "NN", "$.", "PWS", "PPER", "VVPP", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Verlaufe nicht im Sand wie Wein zerschlagner Tonnen.", "tokens": ["Ver\u00b7lau\u00b7fe", "nicht", "im", "Sand", "wie", "Wein", "zer\u00b7schlag\u00b7ner", "Ton\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "APPRART", "NN", "KOKOM", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.14": {"line.1": {"text": "Wenn die Abl\u00f6se kommt, das neue Jahr von Osten,", "tokens": ["Wenn", "die", "Ab\u00b7l\u00f6\u00b7se", "kommt", ",", "das", "neu\u00b7e", "Jahr", "von", "Os\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$,", "ART", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Und nimmt an deiner Statt den Erdenwacheposten,", "tokens": ["Und", "nimmt", "an", "dei\u00b7ner", "Statt", "den", "Er\u00b7den\u00b7wa\u00b7che\u00b7pos\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.15": {"line.1": {"text": "So murmle nicht zu dumpf die geltende Parole", "tokens": ["So", "murm\u00b7le", "nicht", "zu", "dumpf", "die", "gel\u00b7ten\u00b7de", "Pa\u00b7ro\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PTKNEG", "PTKA", "ADJD", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In den bereiften Bart, da\u00df sie der Wind nicht hole;", "tokens": ["In", "den", "be\u00b7reif\u00b7ten", "Bart", ",", "da\u00df", "sie", "der", "Wind", "nicht", "ho\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "KOUS", "PPER", "ART", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.16": {"line.1": {"text": "Damit dein Nachmann fein einhellig sich gebare,", "tokens": ["Da\u00b7mit", "dein", "Nach\u00b7mann", "fein", "ein\u00b7hel\u00b7lig", "sich", "ge\u00b7ba\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPOSAT", "NN", "ADJD", "ADJD", "PRF", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In deinem Segensspruch nicht fluchend weiter fahre,", "tokens": ["In", "dei\u00b7nem", "Se\u00b7gens\u00b7spruch", "nicht", "flu\u00b7chend", "wei\u00b7ter", "fah\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKNEG", "ADJD", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.17": {"line.1": {"text": "Und nicht, wo du geflucht, ins Knie anbetend sinke,", "tokens": ["Und", "nicht", ",", "wo", "du", "ge\u00b7flucht", ",", "ins", "Knie", "an\u00b7be\u00b7tend", "sin\u00b7ke", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "$,", "PWAV", "PPER", "VVPP", "$,", "APPRART", "NN", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und nicht, was du verscheucht, zur\u00fcck liebkosend winke;", "tokens": ["Und", "nicht", ",", "was", "du", "ver\u00b7scheucht", ",", "zu\u00b7r\u00fcck", "lieb\u00b7ko\u00b7send", "win\u00b7ke", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "$,", "PWS", "PPER", "VVFIN", "$,", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.18": {"line.1": {"text": "Und wo du Funken warfst, die gl\u00fccklich schon gez\u00fcndet,", "tokens": ["Und", "wo", "du", "Fun\u00b7ken", "warfst", ",", "die", "gl\u00fcck\u00b7lich", "schon", "ge\u00b7z\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "NN", "VAFIN", "$,", "PRELS", "ADJD", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo schon der Rauch f\u00fcr bald den Flammenschlag verk\u00fcndet,", "tokens": ["Wo", "schon", "der", "Rauch", "f\u00fcr", "bald", "den", "Flam\u00b7men\u00b7schlag", "ver\u00b7k\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "APPR", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.19": {"line.1": {"text": "Da soll das neue Jahr nicht schrecken vor dem Rauche,", "tokens": ["Da", "soll", "das", "neu\u00b7e", "Jahr", "nicht", "schre\u00b7cken", "vor", "dem", "Rau\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "ADJA", "NN", "PTKNEG", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nicht l\u00f6schen feig stupid mit seinem Wasserschlauche!", "tokens": ["Nicht", "l\u00f6\u00b7schen", "feig", "stu\u00b7pid", "mit", "sei\u00b7nem", "Was\u00b7ser\u00b7schlau\u00b7che", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "ADJD", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.20": {"line.1": {"text": "Fahr wohl, fahr hin, o Jahr! nimm fort mit dir im Scheiden", "tokens": ["Fahr", "wohl", ",", "fahr", "hin", ",", "o", "Jahr", "!", "nimm", "fort", "mit", "dir", "im", "Schei\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "$,", "VVFIN", "PTKVZ", "$,", "FM", "NN", "$.", "VVIMP", "PTKVZ", "APPR", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "All deine Lust, nur la\u00df nicht liegen mir die Leiden!", "tokens": ["All", "dei\u00b7ne", "Lust", ",", "nur", "la\u00df", "nicht", "lie\u00b7gen", "mir", "die", "Lei\u00b7den", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "$,", "ADV", "VVIMP", "PTKNEG", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.21": {"line.1": {"text": "O k\u00f6nnt ich hinter dir die Pforte schlie\u00dfen, \u2013 h\u00f6ren,", "tokens": ["O", "k\u00f6nnt", "ich", "hin\u00b7ter", "dir", "die", "Pfor\u00b7te", "schlie\u00b7\u00dfen", ",", "\u2013", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "APPR", "PPER", "ART", "NN", "VVINF", "$,", "$(", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie deine Tritte sich in stiller Nacht verl\u00f6ren!", "tokens": ["Wie", "dei\u00b7ne", "Trit\u00b7te", "sich", "in", "stil\u00b7ler", "Nacht", "ver\u00b7l\u00f6\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "PRF", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.22": {"line.1": {"text": "Jetzt nah und schon so fern, wie auf der Flucht ein Reiter,", "tokens": ["Jetzt", "nah", "und", "schon", "so", "fern", ",", "wie", "auf", "der", "Flucht", "ein", "Rei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADV", "ADV", "ADJD", "$,", "PWAV", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df mein Ged\u00e4chtnis, m\u00fcd, nicht folgen k\u00f6nnte weiter,", "tokens": ["Da\u00df", "mein", "Ge\u00b7d\u00e4cht\u00b7nis", ",", "m\u00fcd", ",", "nicht", "fol\u00b7gen", "k\u00f6nn\u00b7te", "wei\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "$,", "ADJD", "$,", "PTKNEG", "VVINF", "VMFIN", "ADV", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.23": {"line.1": {"text": "Wie einem Reitersmann des Weges noch ein St\u00fccke", "tokens": ["Wie", "ei\u00b7nem", "Rei\u00b7ters\u00b7mann", "des", "We\u00b7ges", "noch", "ein", "St\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nachbellend folgt der Hund, und still dann kehrt zur\u00fccke!", "tokens": ["Nach\u00b7bel\u00b7lend", "folgt", "der", "Hund", ",", "und", "still", "dann", "kehrt", "zu\u00b7r\u00fc\u00b7cke", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "ART", "NN", "$,", "KON", "ADJD", "ADV", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.24": {"line.1": {"text": "Doch ist dies eitler Wahn, des Weges nimmer m\u00fcde,", "tokens": ["Doch", "ist", "dies", "eit\u00b7ler", "Wahn", ",", "des", "We\u00b7ges", "nim\u00b7mer", "m\u00fc\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PDS", "ADJA", "NN", "$,", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Folgt deinen Spuren nach, wohl bis er stirbt, mein R\u00fcde.", "tokens": ["Folgt", "dei\u00b7nen", "Spu\u00b7ren", "nach", ",", "wohl", "bis", "er", "stirbt", ",", "mein", "R\u00fc\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKVZ", "$,", "ADV", "KOUS", "PPER", "VVFIN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.25": {"line.1": {"text": "Fahr hin, unholdes Jahr! mir warst du von den schlimmen;", "tokens": ["Fahr", "hin", ",", "un\u00b7hol\u00b7des", "Jahr", "!", "mir", "warst", "du", "von", "den", "schlim\u00b7men", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "ADJA", "NN", "$.", "PPER", "VAFIN", "PPER", "APPR", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es m\u00f6gen andre dir ein Liedlein Dankes stimmen.", "tokens": ["Es", "m\u00f6\u00b7gen", "and\u00b7re", "dir", "ein", "Lied\u00b7lein", "Dan\u00b7kes", "stim\u00b7men", "."], "token_info": ["word", "word", 