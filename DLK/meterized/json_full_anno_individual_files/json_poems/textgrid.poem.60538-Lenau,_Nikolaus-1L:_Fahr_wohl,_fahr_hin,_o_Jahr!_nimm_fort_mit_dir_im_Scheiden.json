 "punct"], "pos": ["KON", "VAFIN", "PPOSAT", "NN", "PTKVZ", "$,", "ADV", "VAFIN", "PTKA", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.30": {"line.1": {"text": "Zwar hast du unserm Heil den Weg gebahnt von Eisen;", "tokens": ["Zwar", "hast", "du", "un\u00b7serm", "Heil", "den", "Weg", "ge\u00b7bahnt", "von", "Ei\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "NN", "ART", "NN", "VVPP", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch eisern mochte nicht dein Wille sich erweisen.", "tokens": ["Doch", "ei\u00b7sern", "moch\u00b7te", "nicht", "dein", "Wil\u00b7le", "sich", "er\u00b7wei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PTKNEG", "PPOSAT", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.31": {"line.1": {"text": "Noch f\u00e4hrt der Nachtgeist fort zu siegen und zu schrecken,", "tokens": ["Noch", "f\u00e4hrt", "der", "Nacht\u00b7geist", "fort", "zu", "sie\u00b7gen", "und", "zu", "schre\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKVZ", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Auf neuen Feldern stets sein Lager abzustecken.", "tokens": ["Auf", "neu\u00b7en", "Fel\u00b7dern", "stets", "sein", "La\u00b7ger", "ab\u00b7zu\u00b7ste\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "PPOSAT", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.32": {"line.1": {"text": "Eins sei gebeten, Jahr: was du getan, gesonnen,", "tokens": ["Eins", "sei", "ge\u00b7be\u00b7ten", ",", "Jahr", ":", "was", "du", "ge\u00b7tan", ",", "ge\u00b7son\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "VAFIN", "VVPP", "$,", "NN", "$.", "PWS", "PPER", "VVPP", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Verlaufe nicht im Sand wie Wein zerschlagner Tonnen.", "tokens": ["Ver\u00b7lau\u00b7fe", "nicht", "im", "Sand", "wie", "Wein", "zer\u00b7schlag\u00b7ner", "Ton\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "APPRART", "NN", "KOKOM", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.33": {"line.1": {"text": "Wenn die Abl\u00f6se kommt, das neue Jahr von Osten,", "tokens": ["Wenn", "die", "Ab\u00b7l\u00f6\u00b7se", "kommt", ",", "das", "neu\u00b7e", "Jahr", "von", "Os\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$,", "ART", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Und nimmt an deiner Statt den Erdenwacheposten,", "tokens": ["Und", "nimmt", "an", "dei\u00b7ner", "Statt", "den", "Er\u00b7den\u00b7wa\u00b7che\u00b7pos\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.34": {"line.1": {"text": "So murmle nicht zu dumpf die geltende Parole", "tokens": ["So", "murm\u00b7le", "nicht", "zu", "dumpf", "die", "gel\u00b7ten\u00b7de", "Pa\u00b7ro\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PTKNEG", "PTKA", "ADJD", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In den bereiften Bart, da\u00df sie der Wind nicht hole;", "tokens": ["In", "den", "be\u00b7reif\u00b7ten", "Bart", ",", "da\u00df", "sie", "der", "Wind", "nicht", "ho\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "KOUS", "PPER", "ART", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.35": {"line.1": {"text": "Damit dein Nachmann fein einhellig sich gebare,", "tokens": ["Da\u00b7mit", "dein", "Nach\u00b7mann", "fein", "ein\u00b7hel\u00b7lig", "sich", "ge\u00b7ba\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPOSAT", "NN", "ADJD", "ADJD", "PRF", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In deinem Segensspruch nicht fluchend weiter fahre,", "tokens": ["In", "dei\u00b7nem", "Se\u00b7gens\u00b7spruch", "nicht", "flu\u00b7chend", "wei\u00b7ter", "fah\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKNEG", "ADJD", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.36": {"line.1": {"text": "Und nicht, wo du geflucht, ins Knie anbetend sinke,", "tokens": ["Und", "nicht", ",", "wo", "du", "ge\u00b7flucht", ",", "ins", "Knie", "an\u00b7be\u00b7tend", "sin\u00b7ke", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "$,", "PWAV", "PPER", "VVPP", "$,", "APPRART", "NN", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und nicht, was du verscheucht, zur\u00fcck liebkosend winke;", "tokens": ["Und", "nicht", ",", "was", "du", "ver\u00b7scheucht", ",", "zu\u00b7r\u00fcck", "lieb\u00b7ko\u00b7send", "win\u00b7ke", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "$,", "PWS", "PPER", "VVFIN", "$,", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.37": {"line.1": {"text": "Und wo du Funken warfst, die gl\u00fccklich schon gez\u00fcndet,", "tokens": ["Und", "wo", "du", "Fun\u00b7ken", "warfst", ",", "die", "gl\u00fcck\u00b7lich", "schon", "ge\u00b7z\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "NN", "VAFIN", "$,", "PRELS", "ADJD", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo schon der Rauch f\u00fcr bald den Flammenschlag verk\u00fcndet,", "tokens": ["Wo", "schon", "der", "Rauch", "f\u00fcr", "bald", "den", "Flam\u00b7men\u00b7schlag", "ver\u00b7k\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "APPR", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.38": {"line.1": {"text": "Da soll das neue Jahr nicht schrecken vor dem Rauche,", "tokens": ["Da", "soll", "das", "neu\u00b7e", "Jahr", "nicht", "schre\u00b7cken", "vor", "dem", "Rau\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "ADJA", "NN", "PTKNEG", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nicht l\u00f6schen feig stupid mit seinem Wasserschlauche!", "t