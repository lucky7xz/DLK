{"dta.poem.13211": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Z ehndes  B uch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "O V\u00e4terlicher Trieb! o nicht erh\u00f6rte Lust!", "tokens": ["O", "V\u00e4\u00b7ter\u00b7li\u00b7cher", "Trieb", "!", "o", "nicht", "er\u00b7h\u00f6r\u00b7te", "Lust", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$.", "FM", "PTKNEG", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Alte scheint f\u00fcr Trost sich selber nicht bewu\u00dft;", "tokens": ["Der", "Al\u00b7te", "scheint", "f\u00fcr", "Trost", "sich", "sel\u00b7ber", "nicht", "be\u00b7wu\u00dft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "PRF", "ADV", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Er k\u00fc\u00dft\u2019 ihn auf die Stirn; er flie\u00dft in Freuden-Thr\u00e4nen;", "tokens": ["Er", "k\u00fc\u00dft'", "ihn", "auf", "die", "Stirn", ";", "er", "flie\u00dft", "in", "Freu\u00b7den\u00b7Thr\u00e4\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "$.", "PPER", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "340Er spricht: \u201dO k\u00f6nnt ich ihn auch bald zum K\u00f6nig kr\u00f6nen!", "tokens": ["spricht", ":", "\"", "O", "k\u00f6nnt", "ich", "ihn", "auch", "bald", "zum", "K\u00f6\u00b7nig", "kr\u00f6\u00b7nen", "!"], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "$(", "NE", "VVFIN", "PPER", "PPER", "ADV", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.5": {"text": "Er nimmt ihn auf den Arm; sie l\u00e4cheln beyderseits;", "tokens": ["Er", "nimmt", "ihn", "auf", "den", "Arm", ";", "sie", "l\u00e4\u00b7cheln", "bey\u00b7der\u00b7seits", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "$.", "PPER", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Man sieht an ihnen nichts als Zeichen eines Streits;", "tokens": ["Man", "sieht", "an", "ih\u00b7nen", "nichts", "als", "Zei\u00b7chen", "ei\u00b7nes", "Streits", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "PPER", "PIS", "KOKOM", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Er hebt den Prinzen auf und wendt ihn hin und wieder;", "tokens": ["Er", "hebt", "den", "Prin\u00b7zen", "auf", "und", "wendt", "ihn", "hin", "und", "wie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "KON", "VVFIN", "PPER", "PTKVZ", "KON", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Bald l\u00e4\u00dft er ihn besorgt bi\u00df zu der Erden nieder;", "tokens": ["Bald", "l\u00e4\u00dft", "er", "ihn", "be\u00b7sorgt", "bi\u00df", "zu", "der", "Er\u00b7den", "nie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "345Die Kette, die den Hals des guten Raths umf\u00e4ngt,", "tokens": ["Ket\u00b7te", ",", "die", "den", "Hals", "des", "gu\u00b7ten", "Raths", "um\u00b7f\u00e4ngt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.10": {"text": "Ist das, auf was der Prinz mit beyden H\u00e4nden dr\u00e4ngt.", "tokens": ["Ist", "das", ",", "auf", "was", "der", "Prinz", "mit", "bey\u00b7den", "H\u00e4n\u00b7den", "dr\u00e4ngt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "$,", "APPR", "PRELS", "ART", "NN", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Des Prinzens Munterkeit und angenehmes Spielen", "tokens": ["Des", "Prin\u00b7zens", "Mun\u00b7ter\u00b7keit", "und", "an\u00b7ge\u00b7neh\u00b7mes", "Spie\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "L\u00e4\u00dft unsern Alten nichts als Freud und Anmuth f\u00fchlen.", "tokens": ["L\u00e4\u00dft", "un\u00b7sern", "Al\u00b7ten", "nichts", "als", "Freud", "und", "An\u00b7muth", "f\u00fch\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PIS", "KOKOM", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Hier greifft er nach dem Bart; dort nach dem goldnen Herz;", "tokens": ["Hier", "greifft", "er", "nach", "dem", "Bart", ";", "dort", "nach", "dem", "gold\u00b7nen", "Herz", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$.", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "350Mund, Auge, Nas und Ohr wird seiner Finger Scherz;", "tokens": [",", "Au\u00b7ge", ",", "Nas", "und", "Ohr", "wird", "sei\u00b7ner", "Fin\u00b7ger", "Scherz", ";"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$,", "NN", "$,", "NN", "KON", "NN", "VAFIN", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.15": {"text": "Der Grei\u00df liebkoset ihm; er schmeichelt seinen Wangen;", "tokens": ["Der", "Grei\u00df", "lieb\u00b7ko\u00b7set", "ihm", ";", "er", "schmei\u00b7chelt", "sei\u00b7nen", "Wan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$.", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Jezt wissen beyde nicht, was Aug\u2019 und Sinn verlangen;", "tokens": ["Jezt", "wis\u00b7sen", "bey\u00b7de", "nicht", ",", "was", "Aug'", "und", "Sinn", "ver\u00b7lan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PTKNEG", "$,", "PWS", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Der wendet seinen Blick, die Mutter anzusehn,", "tokens": ["Der", "wen\u00b7det", "sei\u00b7nen", "Blick", ",", "die", "Mut\u00b7ter", "an\u00b7zu\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "$,", "ART", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Da sich der Rath vergi\u00dft, sich auch dahin zu drehn;", "tokens": ["Da", "sich", "der", "Rath", "ver\u00b7gi\u00dft", ",", "sich", "auch", "da\u00b7hin", "zu", "drehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "VVFIN", "$,", "PRF", "ADV", "PAV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "355Jezt biegt und neigt er sich; sein ganzes Thun ist Jrren;", "tokens": ["biegt", "und", "neigt", "er", "sich", ";", "sein", "gan\u00b7zes", "Thun", "ist", "Jr\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "PPER", "PRF", "$.", "PPOSAT", "ADJA", "NN", "VAFIN", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.20": {"text": "Wie wann Vergn\u00fcgen, Lieb und Freud ein Herz verwirren.", "tokens": ["Wie", "wann", "Ver\u00b7gn\u00fc\u00b7gen", ",", "Lieb", "und", "Freud", "ein", "Herz", "ver\u00b7wir\u00b7ren", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PWAV", "NN", "$,", "NN", "KON", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}