{"dta.poem.16181": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "235.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df gar kein Wissbares, da\u00df nichts unwissbar sei,", "tokens": ["Da\u00df", "gar", "kein", "Wiss\u00b7ba\u00b7res", ",", "da\u00df", "nichts", "un\u00b7wiss\u00b7bar", "sei", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PIAT", "NN", "$,", "KOUS", "PIS", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ist einerlei im Sinn, im Ausdruck zweierlei.", "tokens": ["Ist", "ei\u00b7ner\u00b7lei", "im", "Sinn", ",", "im", "Aus\u00b7druck", "zwei\u00b7er\u00b7lei", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPRART", "NN", "$,", "APPRART", "NN", "CARD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Im Ausdruck theilten sich, im Endzweck einverstanden,", "tokens": ["Im", "Aus\u00b7druck", "theil\u00b7ten", "sich", ",", "im", "End\u00b7zweck", "ein\u00b7ver\u00b7stan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PRF", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Scheinweise, die im Kampf mit wahren Weisen standen.", "tokens": ["Schein\u00b7wei\u00b7se", ",", "die", "im", "Kampf", "mit", "wah\u00b7ren", "Wei\u00b7sen", "stan\u00b7den", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "APPRART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Scheinweise wissen, auf in gleichen Schein zu l\u00f6sen", "tokens": ["Schein\u00b7wei\u00b7se", "wis\u00b7sen", ",", "auf", "in", "glei\u00b7chen", "Schein", "zu", "l\u00f6\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVINF", "$,", "APPR", "APPR", "ADJA", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wahrheit und Unterschied des Guten und des B\u00f6sen.", "tokens": ["Wahr\u00b7heit", "und", "Un\u00b7ter\u00b7schied", "des", "Gu\u00b7ten", "und", "des", "B\u00f6\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}}, "stanza.4": {"line.1": {"text": "Doch Weise wissen fest den Unterschied zu halten,", "tokens": ["Doch", "Wei\u00b7se", "wis\u00b7sen", "fest", "den", "Un\u00b7ter\u00b7schied", "zu", "hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVINF", "VVFIN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Wahrheit im Geweb des Irrthums zu entfalten:", "tokens": ["Die", "Wahr\u00b7heit", "im", "Ge\u00b7web", "des", "Irr\u00b7thums", "zu", "ent\u00b7fal\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Da\u00df etwas nicht gewu\u00dft, etwas gewu\u00dft kann werden,", "tokens": ["Da\u00df", "et\u00b7was", "nicht", "ge\u00b7wu\u00dft", ",", "et\u00b7was", "ge\u00b7wu\u00dft", "kann", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PTKNEG", "VVPP", "$,", "ADV", "ADJD", "VMFIN", "VAINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und dis ist noth uns just, und jenes nicht, auf Erden.", "tokens": ["Und", "dis", "ist", "noth", "uns", "just", ",", "und", "je\u00b7nes", "nicht", ",", "auf", "Er\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "NN", "PPER", "VVFIN", "$,", "KON", "PDS", "PTKNEG", "$,", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}