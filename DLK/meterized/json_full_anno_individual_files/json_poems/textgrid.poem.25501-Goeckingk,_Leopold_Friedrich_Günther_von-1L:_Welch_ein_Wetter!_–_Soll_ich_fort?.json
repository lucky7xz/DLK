: "trochaic.tri"}, "line.28": {"text": "Sollt ich auch an starrer Hand", "tokens": ["Sollt", "ich", "auch", "an", "star\u00b7rer", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.29": {"text": "Meinen Renner leiten,", "tokens": ["Mei\u00b7nen", "Ren\u00b7ner", "lei\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.30": {"text": "Und zuerst vom Felsenrand'", "tokens": ["Und", "zu\u00b7erst", "vom", "Fel\u00b7sen\u00b7rand'"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.31": {"text": "In die Tiefe gleiten.", "tokens": ["In", "die", "Tie\u00b7fe", "glei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.32": {"text": "Mag ich ganze Meilen mich", "tokens": ["Mag", "ich", "gan\u00b7ze", "Mei\u00b7len", "mich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADJA", "NN", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.33": {"text": "In dem Forst' verirren,", "tokens": ["In", "dem", "For\u00b7st'", "ver\u00b7ir\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.34": {"text": "Mag der Schuhuh f\u00fcrchterlich", "tokens": ["Mag", "der", "Schu\u00b7huh", "f\u00fcrch\u00b7ter\u00b7lich"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "ART", "NN", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.35": {"text": "Ueberm Kopf' mir schwirren,", "tokens": ["Ue\u00b7berm", "Kopf'", "mir", "schwir\u00b7ren", ","], "token_inf