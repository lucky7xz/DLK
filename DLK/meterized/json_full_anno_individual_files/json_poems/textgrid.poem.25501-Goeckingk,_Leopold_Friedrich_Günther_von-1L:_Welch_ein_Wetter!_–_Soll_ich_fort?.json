{"text": "Dicke Schlo\u00dfen prallen,", "tokens": ["Di\u00b7cke", "Schlo\u00b7\u00dfen", "pral\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.15": {"text": "Rasselnd von dem Dach' herab", "tokens": ["Ras\u00b7selnd", "von", "dem", "Da\u00b7ch'", "her\u00b7ab"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN", "ADV"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.16": {"text": "Morsche Ziegel fallen,", "tokens": ["Mor\u00b7sche", "Zie\u00b7gel", "fal\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.17": {"text": "Und noch lauter als das Horn,", "tokens": ["Und", "noch", "lau\u00b7ter", "als", "das", "Horn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PIAT", "KOKOM", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.18": {"text": "Das den Schlaf zerstreuet,", "tokens": ["Das", "den", "Schlaf", "zer\u00b7streu\u00b7et", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.19": {"text": "Kunz, der Heuchler, schreiet!", "tokens": ["Kunz", ",", "der", "Heuch\u00b7ler", ",", "schrei\u00b7et", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "$,", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.20": {"text": "Sieh! wie selbst die Rosse dort", "tokens": ["Sieh", "!", "wie", "selbst", "die", "Ros\u00b7se", "dort"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$.", "PWAV", "ADV", "ART", "NN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.21": {"text": "Fortzugehn sich str\u00e4uben!"