{"dta.poem.23656": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Schiller in Lauchst\u00e4dt 1804.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1840", "urn": "urn:nbn:de:kobv:b4-200905192626", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df man zu Lauchst\u00e4dt sonst zur Sommerzeit", "tokens": ["Da\u00df", "man", "zu", "Lauch\u00b7st\u00e4dt", "sonst", "zur", "Som\u00b7mer\u00b7zeit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "APPR", "NE", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Kom\u00f6die spielte, wei\u00df man weit und breit;", "tokens": ["Ko\u00b7m\u00f6\u00b7die", "spiel\u00b7te", ",", "wei\u00df", "man", "weit", "und", "breit", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "VVFIN", "PIS", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Auch da\u00df zuweilen dann zugegen war", "tokens": ["Auch", "da\u00df", "zu\u00b7wei\u00b7len", "dann", "zu\u00b7ge\u00b7gen", "war"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "ADV", "ADV", "ADJD", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Von Weimar aus das gro\u00dfe Dichterpaar,", "tokens": ["Von", "Wei\u00b7mar", "aus", "das", "gro\u00b7\u00dfe", "Dich\u00b7ter\u00b7paar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und wie der Musensohn vom Saalathen", "tokens": ["Und", "wie", "der", "Mu\u00b7sen\u00b7sohn", "vom", "Saa\u00b7la\u00b7then"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Nach Lauchst\u00e4dt pflegte grade dann zu gehn.", "tokens": ["Nach", "Lauch\u00b7st\u00e4dt", "pfleg\u00b7te", "gra\u00b7de", "dann", "zu", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "ADV", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Doch wei\u00df man nicht, was eines Tags geschah.", "tokens": ["Doch", "wei\u00df", "man", "nicht", ",", "was", "ei\u00b7nes", "Tags", "ge\u00b7schah", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "PTKNEG", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Man spielt die R\u00e4uber; Schiller selbst ist da.", "tokens": ["Man", "spielt", "die", "R\u00e4u\u00b7ber", ";", "Schil\u00b7ler", "selbst", "ist", "da", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "$.", "NE", "ADV", "VAFIN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Vom Dichter ist das ganze Haus begl\u00fcckt,", "tokens": ["Vom", "Dich\u00b7ter", "ist", "das", "gan\u00b7ze", "Haus", "be\u00b7gl\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Der Dichter selber ist vom Spiel entz\u00fcckt.", "tokens": ["Der", "Dich\u00b7ter", "sel\u00b7ber", "ist", "vom", "Spiel", "ent\u00b7z\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Doch ach! der Vorhang f\u00e4llt, das St\u00fcck ist aus;", "tokens": ["Doch", "ach", "!", "der", "Vor\u00b7hang", "f\u00e4llt", ",", "das", "St\u00fcck", "ist", "aus", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$.", "ART", "NN", "VVFIN", "$,", "ART", "NN", "VAFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Zufrieden geht das Publicum nach Haus.", "tokens": ["Zu\u00b7frie\u00b7den", "geht", "das", "Pub\u00b7li\u00b7cum", "nach", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Nur Bruder Studio ist so erfreut,", "tokens": ["Nur", "Bru\u00b7der", "Stu\u00b7dio", "ist", "so", "er\u00b7freut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "NE", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.14": {"text": "Da\u00df er gar manche R\u00e4uberseen' erneut.", "tokens": ["Da\u00df", "er", "gar", "man\u00b7che", "R\u00e4u\u00b7ber\u00b7seen'", "er\u00b7neut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PIAT", "NN", "VVFIN", "$."], "meter": "-++--+-+-+", "measure": "iambic.penta.relaxed"}, "line.15": {"text": "Frisch! ", "tokens": ["Frisch", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+", "measure": "single.up"}, "line.16": {"text": "Und dort: ", "tokens": ["Und", "dort", ":"], "token_info": ["word", "word", "punct"], "pos": ["KON", "ADV", "$."], "meter": "-+", "measure": "iambic.single"}, "line.17": {"text": "Man l\u00e4rmet, jubelt, schw\u00e4rmet, trinkt und singt,", "tokens": ["Man", "l\u00e4r\u00b7met", ",", "ju\u00b7belt", ",", "schw\u00e4r\u00b7met", ",", "trinkt", "und", "singt", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Der Dichter sitzt von froher Schaar umringt,", "tokens": ["Der", "Dich\u00b7ter", "sitzt", "von", "fro\u00b7her", "Schaar", "um\u00b7ringt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Er sitzt so heiter und so wohlgemuth.", "tokens": ["Er", "sitzt", "so", "hei\u00b7ter", "und", "so", "wohl\u00b7ge\u00b7muth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "KON", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Er trinkt als tr\u00e4nk' er neue Jugendglut.", "tokens": ["Er", "trinkt", "als", "tr\u00e4nk'", "er", "neu\u00b7e", "Ju\u00b7gend\u00b7glut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Doch als es endlich nun am Wein gebricht,", "tokens": ["Doch", "als", "es", "end\u00b7lich", "nun", "am", "Wein", "ge\u00b7bricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Da ruft er: \u201eNein! wir trennen uns noch nicht,", "tokens": ["Da", "ruft", "er", ":", "\u201e", "Nein", "!", "wir", "tren\u00b7nen", "uns", "noch", "nicht", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "$(", "PTKANT", "$.", "PPER", "VVFIN", "PPER", "ADV", "PTKNEG", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Noch nicht! dem Gl\u00fccklichen schl\u00e4gt keine Uhr.", "tokens": ["Noch", "nicht", "!", "dem", "Gl\u00fcck\u00b7li\u00b7chen", "schl\u00e4gt", "kei\u00b7ne", "Uhr", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "$.", "ART", "NN", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+--++-+", "measure": "iambic.penta.relaxed"}, "line.24": {"text": "Hinaus mit mir, hinaus in die Natur!\u201c", "tokens": ["Hin\u00b7aus", "mit", "mir", ",", "hin\u00b7aus", "in", "die", "Na\u00b7tur", "!", "\u201c"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "APPR", "PPER", "$,", "ADV", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Sch\u00f6n war die Nacht, kein L\u00fcftchen regte sich,", "tokens": ["Sch\u00f6n", "war", "die", "Nacht", ",", "kein", "L\u00fcft\u00b7chen", "reg\u00b7te", "sich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "$,", "PIAT", "NN", "VVFIN", "PRF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "Hell schien der Mond, das letzte W\u00f6lkchen wich.", "tokens": ["Hell", "schien", "der", "Mond", ",", "das", "letz\u00b7te", "W\u00f6lk\u00b7chen", "wich", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "$,", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "Da rief der Dichter zu den Seinen: \u201eTraun!", "tokens": ["Da", "rief", "der", "Dich\u00b7ter", "zu", "den", "Sei\u00b7nen", ":", "\u201e", "Traun", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ART", "PPOSS", "$.", "$(", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "Hier ist gut sein, hier lasst uns H\u00fctten baun!\u201c", "tokens": ["Hier", "ist", "gut", "sein", ",", "hier", "lasst", "uns", "H\u00fct\u00b7ten", "baun", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VAFIN", "ADJD", "VAINF", "$,", "ADV", "VVFIN", "PPER", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.29": {"text": "Und unter B\u00e4umen in der Mondscheinnacht", "tokens": ["Und", "un\u00b7ter", "B\u00e4u\u00b7men", "in", "der", "Mond\u00b7schein\u00b7nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.30": {"text": "Wird schnell ein Lauberh\u00fcttenfest gemacht.", "tokens": ["Wird", "schnell", "ein", "Lau\u00b7ber\u00b7h\u00fct\u00b7ten\u00b7fest", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.31": {"text": "Und wie man hat gebracht die B\u00e4nk' herbei,", "tokens": ["Und", "wie", "man", "hat", "ge\u00b7bracht", "die", "B\u00e4nk'", "her\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PIS", "VAFIN", "VVPP", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.32": {"text": "Und sitzt und singt, da kommt die Polizei.", "tokens": ["Und", "sitzt", "und", "singt", ",", "da", "kommt", "die", "Po\u00b7li\u00b7zei", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "$,", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.33": {"text": "\u201ewas will der Sklav bei freien M\u00e4nnern hier?\u201c", "tokens": ["\u201e", "was", "will", "der", "Sklav", "bei", "frei\u00b7en", "M\u00e4n\u00b7nern", "hier", "?", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PWS", "VMFIN", "ART", "NN", "APPR", "ADJA", "NN", "ADV", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.34": {"text": "\u201eich will, spricht der Soldat, ich will da\u00df ihr", "tokens": ["\u201e", "ich", "will", ",", "spricht", "der", "Sol\u00b7dat", ",", "ich", "will", "da\u00df", "ihr"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VMFIN", "$,", "VVFIN", "ART", "NN", "$,", "PPER", "VMFIN", "KOUS", "PPER"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.35": {"text": "Nicht weiter singt und in so sp\u00e4ter Zeit", "tokens": ["Nicht", "wei\u00b7ter", "singt", "und", "in", "so", "sp\u00e4\u00b7ter", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "VVFIN", "KON", "APPR", "ADV", "ADJD", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.36": {"text": "Die Badeg\u00e4st' aus ihrem Schlafe schreit\u201c", "tokens": ["Die", "Ba\u00b7de\u00b7g\u00e4st'", "aus", "ih\u00b7rem", "Schla\u00b7fe", "schreit", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.37": {"text": "Da wird nur heftiger die Sangeslust", "tokens": ["Da", "wird", "nur", "hef\u00b7ti\u00b7ger", "die", "San\u00b7ges\u00b7lust"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "ADJD", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.38": {"text": "Und alles schreit vereint aus voller Brust:", "tokens": ["Und", "al\u00b7les", "schreit", "ver\u00b7eint", "aus", "vol\u00b7ler", "Brust", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "ADJD", "VVPP", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.39": {"text": "\u201ewas will der Sklav bei freien M\u00e4nnern hier?\u201c", "tokens": ["\u201e", "was", "will", "der", "Sklav", "bei", "frei\u00b7en", "M\u00e4n\u00b7nern", "hier", "?", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PWS", "VMFIN", "ART", "NN", "APPR", "ADJA", "NN", "ADV", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.40": {"text": "Und singt: ", "tokens": ["Und", "singt", ":"], "token_info": ["word", "word", "punct"], "pos": ["KON", "VVFIN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.41": {"text": "Doch jener rief: \u201eheraus! Soldaten, raus!\u201c", "tokens": ["Doch", "je\u00b7ner", "rief", ":", "\u201e", "he\u00b7raus", "!", "Sol\u00b7da\u00b7ten", ",", "raus", "!", "\u201c"], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "word", "punct", "word", "punct", "punct"], "pos": ["KON", "PDS", "VVFIN", "$.", "$(", "PTKVZ", "$.", "NN", "$,", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.42": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.43": {"text": "Der Sangesf\u00fcrst mit seinem Hof entwich,", "tokens": ["Der", "San\u00b7ges\u00b7f\u00fcrst", "mit", "sei\u00b7nem", "Hof", "ent\u00b7wich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.44": {"text": "Er lie\u00df sein gro\u00dfes sch\u00f6nes Reich im Stich,", "tokens": ["Er", "lie\u00df", "sein", "gro\u00b7\u00dfes", "sch\u00f6\u00b7nes", "Reich", "im", "Stich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.45": {"text": "Den heitern Himmel mit der Sternenpracht,", "tokens": ["Den", "hei\u00b7tern", "Him\u00b7mel", "mit", "der", "Ster\u00b7nen\u00b7pracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.46": {"text": "Die wonnigmilde lichte Mondscheinnacht:", "tokens": ["Die", "won\u00b7nig\u00b7mil\u00b7de", "lich\u00b7te", "Mond\u00b7schein\u00b7nacht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.47": {"text": "\u201ein des Herzens heilig stille R\u00e4ume", "tokens": ["\u201e", "in", "des", "Her\u00b7zens", "hei\u00b7lig", "stil\u00b7le", "R\u00e4u\u00b7me"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "APPR", "ART", "NN", "ADJD", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.48": {"text": "Musst du fliehen aus des Lebens Drang!", "tokens": ["Musst", "du", "flie\u00b7hen", "aus", "des", "Le\u00b7bens", "Drang", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.49": {"text": "Freiheit ist nur in dem Reich der Tr\u00e4ume,", "tokens": ["Frei\u00b7heit", "ist", "nur", "in", "dem", "Reich", "der", "Tr\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-++--+-+-", "measure": "trochaic.penta.relaxed"}, "line.50": {"text": "Und das Sch\u00f6ne bl\u00fcht nur im Gesang.\u201c", "tokens": ["Und", "das", "Sch\u00f6\u00b7ne", "bl\u00fcht", "nur", "im", "Ge\u00b7sang", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADV", "APPRART", "NN", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.51": {"text": "Trug nun auch damals mancher Musensohn", "tokens": ["Trug", "nun", "auch", "da\u00b7mals", "man\u00b7cher", "Mu\u00b7sen\u00b7sohn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "ADV", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.52": {"text": "Gar manchen Schlag und Puff und Knuff davon,", "tokens": ["Gar", "man\u00b7chen", "Schlag", "und", "Puff", "und", "Knuff", "da\u00b7von", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "KON", "NN", "KON", "NN", "PAV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.53": {"text": "So denkt doch freudig unter uns daran", "tokens": ["So", "denkt", "doch", "freu\u00b7dig", "un\u00b7ter", "uns", "da\u00b7ran"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ADJD", "APPR", "PPER", "PAV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.54": {"text": "Noch Einer, der es nie vergessen kann,", "tokens": ["Noch", "Ei\u00b7ner", ",", "der", "es", "nie", "ver\u00b7ges\u00b7sen", "kann", ","], "token_info": ["word", "word", "punct", "wo