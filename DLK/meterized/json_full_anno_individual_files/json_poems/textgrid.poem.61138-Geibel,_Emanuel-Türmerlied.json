{"textgrid.poem.61138": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "T\u00fcrmerlied", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wachet auf! ruft euch die Stimme", "tokens": ["Wa\u00b7chet", "auf", "!", "ruft", "euch", "die", "Stim\u00b7me"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKVZ", "$.", "VVFIN", "PPER", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Des W\u00e4chters von der hohen Zinne,", "tokens": ["Des", "W\u00e4ch\u00b7ters", "von", "der", "ho\u00b7hen", "Zin\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wach' auf, du weites deutsches Land!", "tokens": ["Wach'", "auf", ",", "du", "wei\u00b7tes", "deut\u00b7sches", "Land", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$,", "PPER", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die ihr an der Donau hauset,", "tokens": ["Die", "ihr", "an", "der", "Do\u00b7nau", "hau\u00b7set", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ART", "NE", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und wo der Rhein durch Felsen brauset,", "tokens": ["Und", "wo", "der", "Rhein", "durch", "Fel\u00b7sen", "brau\u00b7set", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NE", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und wo sich t\u00fcrmt der D\u00fcne Sand!", "tokens": ["Und", "wo", "sich", "t\u00fcrmt", "der", "D\u00fc\u00b7ne", "Sand", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PRF", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Habt Wacht am Heimatsherd,", "tokens": ["Habt", "Wacht", "am", "Hei\u00b7mats\u00b7herd", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "In treuer Hand das Schwert,", "tokens": ["In", "treu\u00b7er", "Hand", "das", "Schwert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Jede Stunde!", "tokens": ["Je\u00b7de", "Stun\u00b7de", "!"], "token_info": ["word", "word", "punct"], "pos": ["PIAT", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.10": {"text": "Zu scharfem Streit", "tokens": ["Zu", "schar\u00b7fem", "Streit"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "Macht euch bereit!", "tokens": ["Macht", "euch", "be\u00b7reit", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.12": {"text": "Der Tag des Kampfes ist nicht weit.", "tokens": ["Der", "Tag", "des", "Kamp\u00b7fes", "ist", "nicht", "weit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "H\u00f6rt ihr's dumpf im Osten klingen?", "tokens": ["H\u00f6rt", "ih\u00b7r's", "dumpf", "im", "Os\u00b7ten", "klin\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJD", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Er m\u00f6cht' euch gar zu gern verschlingen,", "tokens": ["Er", "m\u00f6cht'", "euch", "gar", "zu", "gern", "ver\u00b7schlin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "PTKA", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der Geier, der nach Beute kreist.", "tokens": ["Der", "Gei\u00b7er", ",", "der", "nach", "Beu\u00b7te", "kreist", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "H\u00f6rt im Westen ihr die Schlange?", "tokens": ["H\u00f6rt", "im", "Wes\u00b7ten", "ihr", "die", "Schlan\u00b7ge", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sie m\u00f6chte mit Sirenensange", "tokens": ["Sie", "m\u00f6ch\u00b7te", "mit", "Si\u00b7re\u00b7nen\u00b7san\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Vergiften euch den frommen Geist.", "tokens": ["Ver\u00b7gif\u00b7ten", "euch", "den", "from\u00b7men", "Geist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Schon naht des Geiers Flug,", "tokens": ["Schon", "naht", "des", "Gei\u00b7ers", "Flug", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Schon birgt die Schlange klug", "tokens": ["Schon", "birgt", "die", "Schlan\u00b7ge", "klug"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Sich zum Sprunge;", "tokens": ["Sich", "zum", "Sprun\u00b7ge", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PRF", "APPRART", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.10": {"text": "Drum haltet Wacht", "tokens": ["Drum", "hal\u00b7tet", "Wacht"], "token_info": ["word", "word", "word"], "pos": ["PAV", "VVFIN", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "Um Mitternacht", "tokens": ["Um", "Mit\u00b7ter\u00b7nacht"], "token_info": ["word", "word"], "pos": ["KOUI", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.12": {"text": "Und wetzt die Schwerter f\u00fcr die Schlacht!", "tokens": ["Und", "wetzt", "die", "Schwer\u00b7ter", "f\u00fcr", "die", "Schlacht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Reiniget euch in Gebeten,", "tokens": ["Rei\u00b7ni\u00b7get", "euch", "in", "Ge\u00b7be\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf da\u00df ihr vor den Herrn k\u00f6nnt treten,", "tokens": ["Auf", "da\u00df", "ihr", "vor", "den", "Herrn", "k\u00f6nnt", "tre\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn er um euer Werk euch fr\u00e4gt;", "tokens": ["Wenn", "er", "um", "eu\u00b7er", "Werk", "euch", "fr\u00e4gt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Keusch im Lieben, fest im Glauben,", "tokens": ["Keusch", "im", "Lie\u00b7ben", ",", "fest", "im", "Glau\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "ADJA", "$,", "ADJD", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "La\u00dft euch den treuen Mut nicht rauben,", "tokens": ["La\u00dft", "euch", "den", "treu\u00b7en", "Mut", "nicht", "rau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "ADJA", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Seid einig, da die Stunde schl\u00e4gt!", "tokens": ["Seid", "ei\u00b7nig", ",", "da", "die", "Stun\u00b7de", "schl\u00e4gt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "$,", "KOUS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Das Kreuz sei eure Zier,", "tokens": ["Das", "Kreuz", "sei", "eu\u00b7re", "Zier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Eur Helmbusch und Panier", "tokens": ["Eur", "Helm\u00b7busch", "und", "Pa\u00b7nier"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "In den Schlachten.", "tokens": ["In", "den", "Schlach\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "--+-", "measure": "anapaest.init"}, "line.10": {"text": "Wer in dem Feld", "tokens": ["Wer", "in", "dem", "Feld"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "APPR", "ART", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.11": {"text": "Zu Gott sich h\u00e4lt,", "tokens": ["Zu", "Gott", "sich", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.12": {"text": "Der hat allein sich wohl gestellt.", "tokens": ["Der", "hat", "al\u00b7lein", "sich", "wohl", "ge\u00b7stellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "PRF", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Sieh herab vom Himmel droben,", "tokens": ["Sieh", "her\u00b7ab", "vom", "Him\u00b7mel", "dro\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPRART", "NN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Herr, den der Engel Zungen loben,", "tokens": ["Herr", ",", "den", "der", "En\u00b7gel", "Zun\u00b7gen", "lo\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ART", "NN", "NN", "VVINF", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Sei gn\u00e4dig diesem deutschen Land!", "tokens": ["Sei", "gn\u00e4\u00b7dig", "die\u00b7sem", "deut\u00b7schen", "Land", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Donnernd aus der Feuerwolke", "tokens": ["Don\u00b7nernd", "aus", "der", "Feu\u00b7er\u00b7wol\u00b7ke"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sprich zu den F\u00fcrsten, sprich zum Volke", "tokens": ["Sprich", "zu", "den", "F\u00fcrs\u00b7ten", ",", "sprich", "zum", "Vol\u00b7ke"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "APPR", "ART", "NN", "$,", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und lehr' uns stark sein Hand in Hand!", "tokens": ["Und", "lehr'", "uns", "stark", "sein", "Hand", "in", "Hand", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "ADJD", "PPOSAT", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Sei du uns Fels und Burg,", "tokens": ["Sei", "du", "uns", "Fels", "und", "Burg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PRF", "NE", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Du f\u00fchrst uns wohl hindurch.", "tokens": ["Du", "f\u00fchrst", "uns", "wohl", "hin\u00b7durch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Halleluja!", "tokens": ["Hal\u00b7le\u00b7lu\u00b7ja", "!"], "token_info": ["word", "punct"], "pos": ["ITJ", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.10": {"text": "Denn dein ist heut", "tokens": ["Denn", "dein", "ist", "heut"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "VAFIN", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "Und alle Zeit", "tokens": ["Und", "al\u00b7le", "Zeit"], "token_info": ["word", "word", "word"], "pos": ["KON", "PIAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.12": {"text": "Das 