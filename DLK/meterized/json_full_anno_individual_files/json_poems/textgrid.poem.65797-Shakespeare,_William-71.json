{"textgrid.poem.65797": {"metadata": {"author": {"name": "Shakespeare, William", "birth": "N.A.", "death": "N.A."}, "title": "71", "genre": "verse", "period": "N.A.", "pub_year": 1590, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn ich gestorben, traure l\u00e4nger nicht", "tokens": ["Wenn", "ich", "ge\u00b7stor\u00b7ben", ",", "trau\u00b7re", "l\u00e4n\u00b7ger", "nicht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVPP", "$,", "VVFIN", "ADJD", "PTKNEG"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Als dumpfer Grabeglocken Trauerton", "tokens": ["Als", "dum\u00b7pfer", "Gra\u00b7be\u00b7glo\u00b7cken", "Trau\u00b7er\u00b7ton"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ADJA", "NN", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Welt von meinem Scheiden gibt Bericht,", "tokens": ["Der", "Welt", "von", "mei\u00b7nem", "Schei\u00b7den", "gibt", "Be\u00b7richt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und da\u00df zu armen W\u00fcrmern ich entflohn.", "tokens": ["Und", "da\u00df", "zu", "ar\u00b7men", "W\u00fcr\u00b7mern", "ich", "ent\u00b7flohn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "APPR", "ADJA", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ja, liesest du dies Wort, vergi\u00df die Hand,", "tokens": ["Ja", ",", "lie\u00b7sest", "du", "dies", "Wort", ",", "ver\u00b7gi\u00df", "die", "Hand", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VVFIN", "PPER", "PDS"