{"textgrid.poem.54049": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Kino privat", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In vielen Prokuristen steckt ein Perser-Schah,", "tokens": ["In", "vie\u00b7len", "Pro\u00b7ku\u00b7ris\u00b7ten", "steckt", "ein", "Per\u00b7ser\u00b7Sch\u00b7ah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.2": {"text": "der ruht, verzaubert. Aber manchmal, im B\u00fcro,", "tokens": ["der", "ruht", ",", "ver\u00b7zau\u00b7bert", ".", "A\u00b7ber", "manch\u00b7mal", ",", "im", "B\u00fc\u00b7ro", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "VVFIN", "$,", "VVFIN", "$.", "KON", "ADV", "$,", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "wenn schl\u00e4frig nebenan die Schreibmaschinen schnattern,", "tokens": ["wenn", "schl\u00e4f\u00b7rig", "ne\u00b7be\u00b7nan", "die", "Schreib\u00b7ma\u00b7schi\u00b7nen", "schnat\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "so kurz nach Tisch . . . schl\u00e4gt er im Traum die Augen auf", "tokens": ["so", "kurz", "nach", "Tisch", ".", ".", ".", "schl\u00e4gt", "er", "im", "Traum", "die", "Au\u00b7gen", "auf"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "APPR", "NN", "$.", "$.", "$.", "VVFIN", "PPER", "APPRART", "NN", "ART", "NN", "APPR"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "und atmet.", "tokens": ["und", "at\u00b7met", "."], "token_info": ["word", "word", "punct"], "pos": ["KON", "VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Dreimal klatscht er leise", "tokens": ["Drei\u00b7mal", "klatscht", "er", "lei\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "in die H\u00e4nde. Ibrahim erscheint", "tokens": ["in", "die", "H\u00e4n\u00b7de", ".", "Ib\u00b7ra\u00b7him", "er\u00b7scheint"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "$.", "NE", "VVFIN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "und kreuzt die Arme, neigt sich, schweigt.", "tokens": ["und", "kreuzt", "die", "Ar\u00b7me", ",", "neigt", "sich", ",", "schweigt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "VVFIN", "PRF", "$,", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "\u00bbdie M\u00e4dchen!\u00ab sagt der Prok . . . der Schah.", "tokens": ["\u00bb", "die", "M\u00e4d\u00b7chen", "!", "\u00ab", "sagt", "der", "Prok", ".", ".", ".", "der", "Schah", "."], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct", "punct", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "$.", "$(", "VVFIN", "ART", "NN", "$.", "$.", "$.", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Und sieben M\u00e4dchen trippeln", "tokens": ["Und", "sie\u00b7ben", "M\u00e4d\u00b7chen", "trip\u00b7peln"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "CARD", "NN", "VVINF"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.11": {"text": "um ihn herum, jung, schlank, mit \u00d6l gesalbt,", "tokens": ["um", "ihn", "he\u00b7rum", ",", "jung", ",", "schlank", ",", "mit", "\u00d6l", "ge\u00b7salbt", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "PTKVZ", "$,", "ADJD", "$,", "VVFIN", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "und eine ist dabei, feist wie ein praller Sack.", "tokens": ["und", "ei\u00b7ne", "ist", "da\u00b7bei", ",", "feist", "wie", "ein", "pral\u00b7ler", "Sack", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "PAV", "$,", "ADJD", "KOKOM", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Der Schah versinkt in Weiberfleisch, in Br\u00fcste, die ihn streicheln,", "tokens": ["Der", "Schah", "ver\u00b7sinkt", "in", "Wei\u00b7ber\u00b7fleisch", ",", "in", "Br\u00fcs\u00b7te", ",", "die", "ihn", "strei\u00b7cheln", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$,", "APPR", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.14": {"text": "er wei\u00df nichts mehr, sieht rot, ist sieben Male Mann . . .", "tokens": ["er", "wei\u00df", "nichts", "mehr", ",", "sieht", "rot", ",", "ist", "sie\u00b7ben", "Ma\u00b7le", "Mann", ".", ".", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PPER", "VVFIN", "PIS", "ADV", "$,", "VVFIN", "ADJD", "$,", "VAFIN", "CARD", "NN", "NN", "$.", "$.", "$."], "meter": "-+--++-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.15": {"text": "Wach auf, Gehirn! Das Hirn erwacht,", "tokens": ["Wach", "auf", ",", "Ge\u00b7hirn", "!", "Das", "Hirn", "er\u00b7wacht", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$,", "NN", "$.", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "und aller Unflat, den er je gelesen", "tokens": ["und", "al\u00b7ler", "Un\u00b7flat", ",", "den", "er", "je", "ge\u00b7le\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "$,", "PRELS", "PPER", "ADV", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "und je ertr\u00e4umt, bricht aus dem Prokuristen-Schah.", "tokens": ["und", "je", "er\u00b7tr\u00e4umt", ",", "bricht", "aus", "dem", "Pro\u00b7ku\u00b7ris\u00b7ten\u00b7Sch\u00b7ah", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVPP", "$,", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.18": {"text": "Er schaut, er schmatzt, er schmeckt, er wittert . . .", "tokens": ["Er", "schaut", ",", "er", "schmatzt", ",", "er", "schmeckt", ",", "er", "wit\u00b7tert", ".", ".", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "punct", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "$.", "$.", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "\u00bbfatme! Suleima! Ah, du bist . . . \u00ab", "tokens": ["\u00bb", "fat\u00b7me", "!", "Su\u00b7lei\u00b7ma", "!", "Ah", ",", "du", "bist", ".", ".", ".", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct", "punct", "punct", "punct"], "pos": ["$(", "VVFIN", "$.", "NE", "$.", "NN", "$,", "PPER", "VAFIN", "$.", "$.", "$.", "$("], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.20": {"text": "Entzwei", "tokens": ["Ent\u00b7zwei"], "token_info": ["word"], "pos": ["XY"], "meter": "-+", "measure": "iambic.single"}, "line.21": {"text": "rei\u00dft ihn ein Klingellaut, der hart verzittert.", "tokens": ["rei\u00dft", "ihn", "ein", "Klin\u00b7gel\u00b7laut", ",", "der", "hart", "ver\u00b7zit\u00b7tert", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "PRELS", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Schah ab. Der Prokurist:", "tokens": ["Schah", "ab", ".", "Der", "Pro\u00b7ku\u00b7rist", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$.", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.23": {"text": "\u00bbhier L\u00fctzow siebenundsiebenzignulldrei!", "tokens": ["\u00bb", "hier", "L\u00fct\u00b7zow", "sie\u00b7be\u00b7nund\u00b7sie\u00b7ben\u00b7zi\u00b7gnull\u00b7drei", "!"], "token_info": ["punct", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "NE", "PTKVZ", "$."], "meter": "-+-+---+--+", "measure": "iambic.tetra.chol"}, "line.24": {"text": "Am Apparat. Der Skonto? Wie gew\u00f6hnlich!", "tokens": ["Am", "Ap\u00b7pa\u00b7rat", ".", "Der", "Skon\u00b7to", "?", "Wie", "ge\u00b7w\u00f6hn\u00b7lich", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$.", "ART", "NN", "$.", "PWAV", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "Na, unser Doktor Freutel hat pers\u00f6nlich . . .\u00ab", "tokens": ["Na", ",", "un\u00b7ser", "Dok\u00b7tor", "Freu\u00b7tel", "hat", "per\u00b7s\u00f6n\u00b7lich", ".", ".", ".", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "punct", "punct", "punct"], "pos": ["ITJ", "$,", "PPOSAT", "NN", "NN", "VAFIN", "ADJD", "$.", "$.", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "In vielen Angestellten wohnt ein Dschingis Khan,", "tokens": ["In", "vie\u00b7len", "An\u00b7ge\u00b7stell\u00b7ten", "wohnt", "ein", "Dschin\u00b7gis", "Khan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "der schl\u00e4ft, verzaubert.", "tokens": ["der", "schl\u00e4ft", ",", "ver\u00b7zau\u00b7bert", "."], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ART", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Aber manchmal, wenn", "tokens": ["A\u00b7ber", "manch\u00b7mal", ",", "wenn"], "token_info": ["word", "word", "punct", "word"], "pos": ["KON", "ADV", "$,", "KOUS"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "der launenhafte Chef den Angestellten piesackt,", "tokens": ["der", "lau\u00b7nen\u00b7haf\u00b7te", "Chef", "den", "An\u00b7ge\u00b7stell\u00b7ten", "pie\u00b7sackt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.5": {"text": "bis dem die Galle hochsteigt, bis er kocht", "tokens": ["bis", "dem", "die", "Gal\u00b7le", "hoch\u00b7steigt", ",", "bis", "er", "kocht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PRELS", "ART", "NN", "VVPP", "$,", "KOUS", "PPER", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "und bis er platzt \u2013: dann steht der Kriegsmongole", "tokens": ["und", "bis", "er", "platzt", "\u2013", ":", "dann", "steht", "der", "Kriegs\u00b7mon\u00b7go\u00b7le"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "$(", "$.", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "wild in ihm auf. Er st\u00fcrzt sich auf den Chef,", "tokens": ["wild", "in", "ihm", "auf", ".", "Er", "st\u00fcrzt", "sich", "auf", "den", "Chef", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPER", "PTKVZ", "$.", "PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "pfeift seinen Leuten, und die packen", "tokens": ["pfeift", "sei\u00b7nen", "Leu\u00b7ten", ",", "und", "die", "pa\u00b7cken"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "KON", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "den Herrn Direktor, binden ihn mit Lassos", "tokens": ["den", "Herrn", "Di\u00b7rek\u00b7tor", ",", "bin\u00b7den", "ihn", "mit", "Las\u00b7sos"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$,", "VAFIN", "PPER", "APPR", "NE"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "und werfen ihn auf ihre Pferde,", "tokens": ["und", "wer\u00b7fen", "ihn", "auf", "ih\u00b7re", "Pfer\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "nein: er wird am Sattel festgebunden", "tokens": ["nein", ":", "er", "wird", "am", "Sat\u00b7tel", "fest\u00b7ge\u00b7bun\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$.", "PPER", "VAFIN", "APPRART", "NN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.12": {"text": "und mu\u00df nun laufen. Laufe! Willst du laufen!", "tokens": ["und", "mu\u00df", "nun", "lau\u00b7fen", ".", "Lau\u00b7fe", "!", "Willst", "du", "lau\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "VVINF", "$.", "NN", "$.", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Du Hund! Die Peitsche saust. Es st\u00f6hnt der Chef!", "tokens": ["Du", "Hund", "!", "Die", "Peit\u00b7sche", "saust", ".", "Es", "st\u00f6hnt", "der", "Chef", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$.", "ART", "NN", "VVFIN", "$.", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Dann wirbeln ihn die Reiter auf die Erde", "tokens": ["Dann", "wir\u00b7beln", "ihn", "die", "Rei\u00b7ter", "auf", "die", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "und schneiden ihm . . . nein: nadeln ihn . . .", "tokens": ["und", "schnei\u00b7den", "ihm", ".", ".", ".", "nein", ":", "na\u00b7deln", "ihn", ".", ".", "."], "token_info": ["word", "word", "word", "punct", "punct", "punct", "word", "punct", "word", "word", "punct", "punct", "punct"], "pos": ["KON", "VVFIN", "PPER", "$.", "$.", "$.", "PTKANT", "$.", "VVFIN", "PPER", "$.", "$.", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "nein: braten ihn in Kohlenfeuer", "tokens": ["nein", ":", "bra\u00b7ten", "ihn", "in", "Koh\u00b7len\u00b7feu\u00b7er"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$.", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "und streuen Salz und Pfeffer in die Wunden.", "tokens": ["und", "streu\u00b7en", "Salz", "und", "Pfef\u00b7fer", "in", "die", "Wun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "KON", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Und Mostrich.", "tokens": ["Und", "Most\u00b7rich", "."], "token_info": ["word", "word", "punct"], "pos": ["KON", "NE", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.19": {"text": "Und der Dschingis Khan", "tokens": ["Und", "der", "Dschin\u00b7gis", "Khan"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "NE"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.20": {"text": "streicht seinen Seidenbart und l\u00e4chelt: \u00bbNa, Herr Zaschke . . .?\u00ab", "tokens": ["streicht", "sei\u00b7nen", "Sei\u00b7den\u00b7bart", "und", "l\u00e4\u00b7chelt", ":", "\u00bb", "Na", ",", "Herr", "Zaschke", ".", ".", ".", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "punct", "punct", "punct", "punct", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "KON", "VVFIN", "$.", "$(", "ITJ", "$,", "NN", "NE", "$.", "$.", "$.", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Und w\u00e4hrend der Gefangene sich am Boden ringelt,", "tokens": ["Und", "w\u00e4h\u00b7rend", "der", "Ge\u00b7fan\u00b7ge\u00b7ne", "sich", "am", "Bo\u00b7den", "rin\u00b7gelt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "PRF", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.22": {"text": "ergreift der Dschingis Khan den vollen Silberhumpen,", "tokens": ["er\u00b7