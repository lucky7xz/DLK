le"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "$(", "$.", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "wild in ihm auf. Er st\u00fcrzt sich auf den Chef,", "tokens": ["wild", "in", "ihm", "auf", ".", "Er", "st\u00fcrzt", "sich", "auf", "den", "Chef", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPER", "PTKVZ", "$.", "PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "pfeift seinen Leuten, und die packen", "tokens": ["pfeift", "sei\u00b7nen", "Leu\u00b7ten", ",", "und", "die", "pa\u00b7cken"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "KON", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "den Herrn Direktor, binden ihn mit Lassos", "tokens": ["den", "Herrn", "Di\u00b7rek\u00b7tor", ",", "bin\u00b7den", "ihn", "mit", "Las\u00b7sos"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$,", "VAFIN", "PPER", "APPR", "NE"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "und werfen ihn auf ihre Pferde,", "tokens": ["und", "wer\u00b7fen", "ihn", "auf", "ih\u00b7re", "Pfer\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "nein: er wird am Sattel festgebunden", "tokens": ["nein", ":", "er", "wird", "am", "Sat\u00b7tel", "fest\u00b7ge\u00b7bun\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$.", "PPER", "VAFIN", "APPRART", "NN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.12": {"text": "und mu\u00df nun laufen. Laufe! Willst du laufen!", "tokens": ["und", "mu\u00df", "nun", "lau\u00b7fen", ".", "Lau\u00b7fe", "!", "Willst", "du", "lau\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "VVINF", "$.", "NN", "$.", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Du Hund! Die Peitsche saust. Es st\u00f6hnt der Chef!", "tokens": ["Du", "Hund", "!", "Die", "Peit\u00b7sche", "saust", ".", "Es", "st\u00f6hnt", "der", "Chef", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$.", "ART", "NN", "VVFIN", "$.", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Dann wirbeln ihn die Reiter auf die Erde", "tokens": ["Dann", "wir\u00b7beln", "ihn", "die", "Rei\u00b7ter", "auf", "die", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "und schneiden ihm . . . nein: nadeln ihn . . .", "tokens": ["und", "schnei\u00b7den", "ihm", ".", ".", ".", "nein", ":", "na\u00b7deln", "ihn", ".", ".", "."], "token_info": ["word", "word", "word", "punct", "punct", "punct", "word", "punct", "word", "word", "punct", "punct", "punct"], "pos": ["KON", "VVFIN", "PPER", "$.", "$.", "$.", "PTKANT", "$.", "VVFIN", "PPER", "$.", "$.", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "nein: braten ihn in Kohlenfeuer", "tokens": ["nein", ":", "bra\u00b7ten", "ihn", "in", "Koh\u00b7len\u00b7feu\u00b7er"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$.", "VVFIN", "PP