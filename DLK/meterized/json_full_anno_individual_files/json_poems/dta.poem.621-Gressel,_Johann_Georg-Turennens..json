{"dta.poem.621": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Turennens.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Turennens Leichnam ruht/ wo Franckreichs Herscher schlaffen", "tokens": ["Tu\u00b7ren\u00b7nens", "Leich\u00b7nam", "ruht", "/", "wo", "Fran\u00b7ck\u00b7reichs", "Her\u00b7scher", "schlaf\u00b7fen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NN", "VVFIN", "$(", "PWAV", "NE", "NN", "VVINF"], "meter": "+--+-+-+--+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Die Ehre hat sein Muth durch seine tapffere Waffen", "tokens": ["Die", "Eh\u00b7re", "hat", "sein", "Muth", "durch", "sei\u00b7ne", "tapf\u00b7fe\u00b7re", "Waf\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Mit grossen Ruhm erlangt; Denn wer die Kronen sch\u00fctzt", "tokens": ["Mit", "gros\u00b7sen", "Ruhm", "er\u00b7langt", ";", "Denn", "wer", "die", "Kro\u00b7nen", "sch\u00fctzt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$.", "KON", "PWS", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wird ja so werth geacht/ als der den Thron besitzt.", "tokens": ["Wird", "ja", "so", "werth", "ge\u00b7acht", "/", "als", "der", "den", "Thron", "be\u00b7sitzt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ADJD", "VVPP", "$(", "KOUS", "ART", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}