word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "PTKNEG", "VAFIN", "$,", "PRF", "ADV", "VVIZU"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "In ihrem Streit,", "tokens": ["In", "ih\u00b7rem", "Streit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.13": {"text": "Und \u00fcber eine Kleinigkeit", "tokens": ["Und", "\u00fc\u00b7ber", "ei\u00b7ne", "Klei\u00b7nig\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Sich mit einander zu vergleichen,", "tokens": ["Sich", "mit", "ein\u00b7an\u00b7der", "zu", "ver\u00b7glei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "PRF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Beschlo\u00df mit Schl\u00e4gerei! Der Deutsche bi\u00df", "tokens": ["Be\u00b7schlo\u00df", "mit", "Schl\u00e4\u00b7ge\u00b7rei", "!", "Der", "Deut\u00b7sche", "bi\u00df"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "APPR", "NN", "$.", "ART", "NN", "APPR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Den Britten tapfer, schmi\u00df", "tokens": ["Den", "Brit\u00b7ten", "tap\u00b7fer", ",", "schmi\u00df"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "ADJD", "$,", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.17": {"text": "Ihn nieder, ri\u00df", "tokens": ["Ihn", "nie\u00b7der", ",", "ri\u00df"], "token_info": ["word", "word", "punct", "word"], "pos": ["PPER", "PTKVZ", "$,", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.18": {"text": "Ein Aug' ihm aus, rieb ihm die Ohren,", "tokens": ["Ein", "Aug'", "ihm", "aus", ",", "rieb", "ihm", "die", "Oh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "PTKVZ", "$,", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.19": {"text": "Die abgestutzten, scharf, macht' ihn schachmatt,", "tokens": ["Die", "ab\u00b7ge\u00b7stutz\u00b7ten", ",", "scharf", ",", "macht'", "ihn", "schach\u00b7matt", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "VVFIN", "$,", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Und kurz: der Britte hat", "tokens": ["Und", "kurz", ":", "der", "Brit\u00b7te", "hat"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADJD", "$.", "ART", "NN", "VAFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.21": {"text": "Die Schlacht verloren!", "tokens": ["Die", "Schlacht", "ver\u00b7lo\u00b7ren", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.8": {"line.1": {"text": "Der gute Britte war dem Deutschen viel zu schwach.", "tokens": ["Der", "gu\u00b7te", "Brit\u00b7te", "war", "dem", "Deut\u00b7schen", "viel", "zu", "schwach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ART", "NN", "ADV", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df doch dem armen Schelm die nicht zu Hilfe kamen,", "tokens": ["Da\u00df", "doch", "dem", "ar\u00b7men", "Schelm", "die", "nicht", "zu", "Hil\u00b7fe", "ka\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "ART", "PTKNEG", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die's sahn, wie schwach er war!", "tokens": ["Die's", "sahn", ",", "wie", "schwach", "er", "war", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "PWAV", "VVFIN", "PPER", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die Fabel lehrt: Wir ahmen", "tokens": ["Die", "Fa\u00b7bel", "lehrt", ":", "Wir", "ah\u00b7men"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Nicht eben alle Moden nach!", "tokens": ["Nicht", "e\u00b7ben", "al\u00b7le", "Mo\u00b7den", "nach", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}