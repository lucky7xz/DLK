{"textgrid.poem.36519": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "18. Die zwei Esel", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zwei Esel zankten sich, ein Deutscher und ein Britte;", "tokens": ["Zwei", "E\u00b7sel", "zank\u00b7ten", "sich", ",", "ein", "Deut\u00b7scher", "und", "ein", "Brit\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "PRF", "$,", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nein, sprach der Deutsche, nein!", "tokens": ["Nein", ",", "sprach", "der", "Deut\u00b7sche", ",", "nein", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PTKANT", "$,", "VVFIN", "ART", "NN", "$,", "PTKANT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich w\u00e4r ein Schaf, ich Esel! wenn ich's litte,", "tokens": ["Ich", "w\u00e4r", "ein", "Schaf", ",", "ich", "E\u00b7sel", "!", "wenn", "ich's", "lit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PPER", "NN", "$.", "KOUS", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df man die Ohren mir beschnitte,", "tokens": ["Da\u00df", "man", "die", "Oh\u00b7ren", "mir", "be\u00b7schnit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die lang genug nicht k\u00f6nnen sein!", "tokens": ["Die", "lang", "ge\u00b7nug", "nicht", "k\u00f6n\u