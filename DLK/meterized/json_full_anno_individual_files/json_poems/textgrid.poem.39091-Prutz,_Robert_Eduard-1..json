{"textgrid.poem.39091": {"metadata": {"author": {"name": "Prutz, Robert Eduard", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Um die Kirche zu Sankt Paul", "tokens": ["Um", "die", "Kir\u00b7che", "zu", "Sankt", "Paul"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "ART", "NN", "PTKZU", "VVFIN", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "einen Schatten sieht man reiten,", "tokens": ["ei\u00b7nen", "Schat\u00b7ten", "sieht", "man", "rei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "mitternachts auf hohem Gaul,", "tokens": ["mit\u00b7ter\u00b7nachts", "auf", "ho\u00b7hem", "Gaul", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "auswattiert die magren Seiten.", "tokens": ["aus\u00b7wat\u00b7tiert", "die", "mag\u00b7ren", "Sei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Pfeile tr\u00e4gt er nicht noch K\u00f6cher,", "tokens": ["Pfei\u00b7le", "tr\u00e4gt", "er", "nicht", "noch", "K\u00f6\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PTKNEG", "ADV", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "f\u00fchrt kein Schlachtschwert um die Lenden:", "tokens": ["f\u00fchrt", "kein", "Schlacht\u00b7schwert", "um", "die", "Len\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber einen Tintenstecher", "tokens": ["A\u00b7ber", "ei\u00b7nen", "Tin\u00b7ten\u00b7ste\u00b7cher"], "token_info": ["word", "word", "word"], "pos": ["KON", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "sorgsam tr\u00e4gt er in den H\u00e4nden.", "tokens": ["sorg\u00b7sam", "tr\u00e4gt", "er", "in", "den", "H\u00e4n\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Statt des F\u00e4hnleins, statt der Lanze", "tokens": ["Statt", "des", "F\u00e4hn\u00b7leins", ",", "statt", "der", "Lan\u00b7ze"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ART", "NN", "$,", "KOUI", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "ragt ihm eine ries'ge Feder", "tokens": ["ragt", "ihm", "ei\u00b7ne", "ries'\u00b7ge", "Fe\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "hinterm Ohr im stolzen Glanze:", "tokens": ["hin\u00b7term", "Ohr", "im", "stol\u00b7zen", "Glan\u00b7ze", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die Ohren sind von Leder.", "tokens": ["Und", "die", "Oh\u00b7ren", "sind", "von", "Le\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Hinter ihm auf einem Grauen \u2013", "tokens": ["Hin\u00b7ter", "ihm", "auf", "ei\u00b7nem", "Grau\u00b7en", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "oder sag' ich Esel f\u00fcglich? \u2013", "tokens": ["o\u00b7der", "sag'", "ich", "E\u00b7sel", "f\u00fcg\u00b7lich", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "PPER", "NN", "ADJD", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "l\u00e4\u00dft sein Knappe sich erschauen,", "tokens": ["l\u00e4\u00dft", "sein", "Knap\u00b7pe", "sich", "er\u00b7schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "wohlbeleibt und h\u00f6chst vergn\u00fcglich.", "tokens": ["wohl\u00b7be\u00b7leibt", "und", "h\u00f6chst", "ver\u00b7gn\u00fcg\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "ADV", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Nicht zum K\u00e4mpfen, nicht zum Fechten,", "tokens": ["Nicht", "zum", "K\u00e4mp\u00b7fen", ",", "nicht", "zum", "Fech\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPRART", "NN", "$,", "PTKNEG", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "besser wohl ist der beim Trinken;", "tokens": ["bes\u00b7ser", "wohl", "ist", "der", "beim", "Trin\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "VAFIN", "ART", "APPRART", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "eine Wurst h\u00e4ngt ihm zur Rechten,", "tokens": ["ei\u00b7ne", "Wurst", "h\u00e4ngt", "ihm", "zur", "Rech\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "+-+---+-", "measure": "unknown.measure.tri"}, "line.4": {"text": "auf der andern ihm ein Schinken.", "tokens": ["auf", "der", "an\u00b7dern", "ihm", "ein", "Schin\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Eine purpurne Melone", "tokens": ["Ei\u00b7ne", "pur\u00b7pur\u00b7ne", "Me\u00b7lo\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-++--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "sieht man auf der Faust ihm prahlen,", "tokens": ["sieht", "man", "auf", "der", "Faust", "ihm", "prah\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "um die Scheitel eine Krone,", "tokens": ["um", "die", "Schei\u00b7tel", "ei\u00b7ne", "Kro\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "aber nur von Eierschalen. \u2013", "tokens": ["a\u00b7ber", "nur", "von", "Ei\u00b7er\u00b7scha\u00b7len", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADV", "APPR", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "So in mittern\u00e4cht'ger Stunde,", "tokens": ["So", "in", "mit\u00b7ter\u00b7n\u00e4cht'\u00b7ger", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "auf dem Esel, auf dem Gaul,", "tokens": ["auf", "dem", "E\u00b7sel", ",", "auf", "dem", "Gaul", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "reiten sie gespenst'ge Runde", "tokens": ["rei\u00b7ten", "sie", "ge\u00b7spenst'\u00b7ge", "Run\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "um die Kirche zu Sankt Paul;", "tokens": ["um", "die", "Kir\u00b7che", "zu", "Sankt", "Paul", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKZU", "VVFIN", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "klopfen dreimal an die Pforte,", "tokens": ["klop\u00b7fen", "drei\u00b7mal", "an", "die", "Pfor\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "rufen dreimal, \u00e4u\u00dferst kl\u00e4glich,", "tokens": ["ru\u00b7fen", "drei\u00b7mal", ",", "\u00e4u\u00b7\u00dferst", "kl\u00e4g\u00b7lich", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "unvernommne, dumpfe Worte \u2013", "tokens": ["un\u00b7ver\u00b7nomm\u00b7ne", ",", "dump\u00b7fe", "Wor\u00b7te", "\u2013"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADJA", "$,", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "doch die T\u00fcr bleibt unbeweglich!", "tokens": ["doch", "die", "T\u00fcr", "bleibt", "un\u00b7be\u00b7weg\u00b7lich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Unbeweglich, wie sie warten,", "tokens": ["Un\u00b7be\u00b7weg\u00b7lich", ",", "wie", "sie", "war\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWAV", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "wie sie rufen, wie sie klopfen;", "tokens": ["wie", "sie", "ru\u00b7fen", ",", "wie", "sie", "klop\u00b7fen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVINF", "$,", "PWAV", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "ja dem \u00d6le selbst, dem zarten,", "tokens": ["ja", "dem", "\u00d6\u00b7le", "selbst", ",", "dem", "zar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADV", "$,", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "das sie auf die Angeln tropfen:", "tokens": ["das", "sie", "auf", "die", "An\u00b7geln", "trop\u00b7fen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Bis beim ersten Hahnenschreie,", "tokens": ["Bis", "beim", "ers\u00b7ten", "Hah\u00b7nen\u00b7schrei\u00b7e", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "bei des Morgenlieds Gedudel,", "tokens": ["bei", "des", "Mor\u00b7gen\u00b7lieds", "Ge\u00b7du\u00b7del", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "heimw\u00e4rts reiten diese zweie,", "tokens": ["heim\u00b7w\u00e4rts", "rei\u00b7ten", "die\u00b7se", "zwei\u00b7e", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PDS", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "kl\u00e4glich, wie bego\u00dfne Pudel. \u2013", "tokens": ["kl\u00e4g\u00b7lich", ",", "wie", "be\u00b7go\u00df\u00b7ne", "Pu\u00b7del", ".", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "$,", "PWAV", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Diesen Ritter, diesen Knappen,", "tokens": ["Die\u00b7sen", "Rit\u00b7ter", ",", "die\u00b7sen", "Knap\u00b7pen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "wei\u00df ihn keiner mir zu nennen?", "tokens": ["wei\u00df", "ihn", "kei\u00b7ner", "mir", "zu", "nen\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIS", "PPER", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ihre Namen, ihre Wappen,", "tokens": ["Ih\u00b7re", "Na\u00b7men", ",", "ih\u00b7re", "Wap\u00b7pen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "lehrt die Seltsamen mich kennen!", "tokens": ["lehrt", "die", "Selt\u00b7sa\u00b7men", "mich", "ken\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "W\u00e4r's vielleicht der gro\u00dfe Tote,", "tokens": ["W\u00e4r's", "viel\u00b7leicht", "der", "gro\u00b7\u00dfe", "To\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "der ber\u00fchmte, vielgenannte,", "tokens": ["der", "be\u00b7r\u00fchm\u00b7te", ",", "viel\u00b7ge\u00b7nann\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "w\u00e4r's la Manchas Don Quichote,", "tokens": ["w\u00e4r's", "la", "Man\u00b7chas", "Don", "Qui\u00b7cho\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "FM", "FM", "FM", "FM", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "w\u00e4r es Sancho, der bekannte?", "tokens": ["w\u00e4r", "es", "San\u00b7cho", ",", "der", "be\u00b7kann\u00b7te", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NE", "$,", "PRELS", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Oder aber (sprechet leiser,", "tokens": ["O\u00b7der", "a\u00b7ber", "(", "spre\u00b7chet", "lei\u00b7ser", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "$(", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "denn es graut mich der Gemeinheit) \u2013", "tokens": ["denn", "es", "graut", "mich", "der", "Ge\u00b7mein\u00b7heit", ")", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ART", "NN", "$(", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "w\u00e4r's vielleicht der k\u00fcnft'ge Kaiser,", "tokens": ["w\u00e4r's", "viel\u00b7leicht", "der", "k\u00fcnft'\u00b7ge", "Kai\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "ihm voran die deutsche Einheit?", "tokens": ["ihm", "vo\u00b7ran", "die", "deut\u00b7sche", "Ein\u00b7heit", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Um die Kirche zu Sankt Paul", "tokens": ["Um", "die", "Kir\u00b7che", "zu", "Sankt", "Paul"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "ART", "NN", "PTKZU", "VVFIN", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "einen Schatten sieht man reiten,", "tokens": ["ei\u00b7nen", "Schat\u00b7ten", "sieht", "man", "rei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "mitternachts auf hohem Gaul,", "tokens": ["mit\u00b7ter\u00b7nachts", "auf", "ho\u00b7hem", "Gaul", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "auswattiert die magren Seiten.", "tokens": ["aus\u00b7wat\u00b7tiert", "die", "mag\u00b7ren", "Sei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "Pfeile tr\u00e4gt er nicht noch K\u00f6cher,", "tokens": ["Pfei\u00b7le", "tr\u00e4gt", "er", "nicht", "noch