{"dta.poem.11554": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Mausfallen-Spr\u00fcchlein.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.99"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "Muse-M\u00e4uschen, stell dich ein", "tokens": ["Mu\u00b7se\u00b7M\u00e4us\u00b7chen", ",", "stell", "dich", "ein"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "ADJD", "PPER", "ART"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Heut Nacht bei Mondenschein!", "tokens": ["Heut", "Nacht", "bei", "Mon\u00b7den\u00b7schein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mach' aber die Th\u00fcr fein hinter dir zu,", "tokens": ["Mach'", "a\u00b7ber", "die", "Th\u00fcr", "fein", "hin\u00b7ter", "dir", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "ADJD", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "H\u00f6rst du?", "tokens": ["H\u00f6rst", "du", "?"], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PPER", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Dabei h\u00fcte dein Schw\u00e4nzchen!", "tokens": ["Da\u00b7bei", "h\u00fc\u00b7te", "dein", "Schw\u00e4nz\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Nach Tische singen wir,", "tokens": ["Nach", "Ti\u00b7sche", "sin\u00b7gen", "wir", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Nach Tische springen wir", "tokens": ["Nach", "Ti\u00b7sche", "sprin\u00b7gen", "wir"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Und machen ein T\u00e4nzchen:", "tokens": ["Und", "ma\u00b7chen", "ein", "T\u00e4nz\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Witt witt!", "tokens": ["Witt", "witt", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.10": {"text": "Meine alte Katze tanzt wahrscheinlich mit.", "tokens": ["Mei\u00b7ne", "al\u00b7te", "Kat\u00b7ze", "tanzt", "wahr\u00b7schein\u00b7lich", "mit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}}}}