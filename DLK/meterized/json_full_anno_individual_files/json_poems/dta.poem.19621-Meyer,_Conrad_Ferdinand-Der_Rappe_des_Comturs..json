{"dta.poem.19621": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Der Rappe des Comturs.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "Herr Konrad Schmid legt' um die Wehr,", "tokens": ["Herr", "Kon\u00b7rad", "Schmid", "legt'", "um", "die", "Wehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Man f\u00fchrt' ihm seinen Rappen her:", "tokens": ["Man", "f\u00fchrt'", "ihm", "sei\u00b7nen", "Rap\u00b7pen", "her", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u201eden Zwingli la\u00df ich nicht im Stich,", "tokens": ["\u201e", "den", "Zwing\u00b7li", "la\u00df", "ich", "nicht", "im", "Stich", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VVIMP", "PPER", "PTKNEG", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und kommt ihr mit, so freut es mich.\u201c", "tokens": ["Und", "kommt", "ihr", "mit", ",", "so", "freut", "es", "mich", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "$,", "ADV", "VVFIN", "PPER", "PRF", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da griffen mit dem Herren wert", "tokens": ["Da", "grif\u00b7fen", "mit", "dem", "Her\u00b7ren", "wert"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Von K\u00fcsnach drei\u00dfig frisch zum Schwert:", "tokens": ["Von", "K\u00fcs\u00b7nach", "drei\u00b7\u00dfig", "frisch", "zum", "Schwert", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "CARD", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Mit Mann und Ro\u00df im Morgenrot", "tokens": ["Mit", "Mann", "und", "Ro\u00df", "im", "Mor\u00b7gen\u00b7rot"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Stie\u00df ab das kriegbeladne Boot.", "tokens": ["Stie\u00df", "ab", "das", "krieg\u00b7be\u00b7lad\u00b7ne", "Boot", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Tr\u00e4g schlich der Tag; dann durch die Nacht", "tokens": ["Tr\u00e4g", "schlich", "der", "Tag", ";", "dann", "durch", "die", "Nacht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "ADJD", "ART", "NN", "$.", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Flog Kunde von verlorner Schlacht.", "tokens": ["Flog", "Kun\u00b7de", "von", "ver\u00b7lor\u00b7ner", "Schlacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Von dr\u00fcben rief der Horgnerthurm,", "tokens": ["Von", "dr\u00fc\u00b7ben", "rief", "der", "Horg\u00b7ner\u00b7thurm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Bald st\u00f6hnten alle Glocken Sturm,", "tokens": ["Bald", "st\u00f6hn\u00b7ten", "al\u00b7le", "Glo\u00b7cken", "Sturm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Und was geblieben war zu Haus,", "tokens": ["Und", "was", "ge\u00b7blie\u00b7ben", "war", "zu", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVPP", "VAFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Das stand am See, lugt' angstvoll aus.", "tokens": ["Das", "stand", "am", "See", ",", "lugt'", "angst\u00b7voll", "aus", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPRART", "NN", "$,", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Am Himmel k\u00e4mpfte lichter Schein", "tokens": ["Am", "Him\u00b7mel", "k\u00e4mpf\u00b7te", "lich\u00b7ter", "Schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Mit schwarz geballten Wolkenreihn.", "tokens": ["Mit", "schwarz", "ge\u00b7ball\u00b7ten", "Wol\u00b7ken\u00b7reihn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "\u201ehilf Gott, ein Nachtgespenst!\u201c Sie sahn", "tokens": ["\u201e", "hilf", "Gott", ",", "ein", "Nacht\u00b7ge\u00b7spenst", "!", "\u201c", "Sie", "sahn"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "punct", "word", "word"], "pos": ["$(", "VVIMP", "NN", "$,", "ART", "NN", "$.", "$(", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Es drohend durch die Fluten nahn.", "tokens": ["Es", "dro\u00b7hend", "durch", "die", "Flu\u00b7ten", "nahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Wo breit des Mondes Silber flo\u00df,", "tokens": ["Wo", "breit", "des", "Mon\u00b7des", "Sil\u00b7ber", "flo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Da rang und rauscht' ein m\u00e4chtig Ro\u00df", "tokens": ["Da", "rang", "und", "rauscht'", "ein", "m\u00e4ch\u00b7tig", "Ro\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "ART", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Und wilder schnaubt's und n\u00e4her fuhr's ...", "tokens": ["Und", "wil\u00b7der", "schnaubt's", "und", "n\u00e4\u00b7her", "fuhr's", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "ADJA", "KON", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "\u201ehilf Gott, der Rappe des Comturs!\u201c", "tokens": ["\u201e", "hilf", "Gott", ",", "der", "Rap\u00b7pe", "des", "Com\u00b7turs", "!", "\u201c"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVIMP", "NN", "$,", "ART", "NN", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Nun trat das Schlachtro\u00df festen Grund,", "tokens": ["Nun", "trat", "das", "Schlach\u00b7tro\u00df", "fes\u00b7ten", "Grund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Die bleiche Menge stand im Rund.", "tokens": ["Die", "blei\u00b7che", "Men\u00b7ge", "stand", "im", "Rund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Zur Erde starrt' sein Augenstern,", "tokens": ["Zur", "Er\u00b7de", "starrt'", "sein", "Au\u00b7gens\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.26": {"text": "Als sucht' es dort den todten Herrn ...", "tokens": ["Als", "sucht'", "es", "dort", "den", "tod\u00b7ten", "Herrn", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Ein Knabe hub dem edeln Thier", "tokens": ["Ein", "Kna\u00b7be", "hub", "dem", "e\u00b7deln", "Thier"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Die M\u00e4hne lind: \u201eDu blutest hier!\u201c", "tokens": ["Die", "M\u00e4h\u00b7ne", "lind", ":", "\u201e", "Du", "blu\u00b7test", "hier", "!", "\u201c"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "ADJD", "$.", "$(", "PPER", "VVFIN", "ADV", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Die Wunde badete die Flut,", "tokens": ["Die", "Wun\u00b7de", "ba\u00b7de\u00b7te", "die", "Flut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-++--+", "measure": "iambic.tetra.chol"}, "line.30": {"text": "Jetzt \u00fcberquillt sie neu von Blut,", "tokens": ["Jetzt", "\u00fc\u00b7berq\u00b7uillt", "sie", "neu", "von", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Und jeder Tropfen schwer und rot", "tokens": ["Und", "je\u00b7der", "Trop\u00b7fen", "schwer", "und", "rot"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Verk\u00fcndet eines Mannes Tod.", "tokens": ["Ver\u00b7k\u00fcn\u00b7det", "ei\u00b7nes", "Man\u00b7nes", "Tod", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Die Comturei mit Thurm und Thor", "tokens": ["Die", "Com\u00b7tu\u00b7rei", "mit", "Thurm", "und", "Thor"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Ragt wei\u00df im Mondenglanz empor.", "tokens": ["Ragt", "wei\u00df", "im", "Mon\u00b7den\u00b7glanz", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Heim schritt der Rapp das Dorf entlang,", "tokens": ["Heim", "schritt", "der", "Rapp", "das", "Dorf", "ent\u00b7lang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "ART", "NN", "APPO", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.36": {"text": "Sein Huf wie \u00fcber Gr\u00fcften klang,", "tokens": ["Sein", "Huf", "wie", "\u00fc\u00b7ber", "Gr\u00fcf\u00b7ten", "klang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KOKOM", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.37": {"text": "Und Alter, Wittwe, Kind und Maid", "tokens": ["Und", "Al\u00b7ter", ",", "Witt\u00b7we", ",", "Kind", "und", "Maid"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KON", "NN", "$,", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.38": {"text": "Zog schluchzend nach wie Grabgeleit.", "tokens": ["Zog", "schluch\u00b7zend", "nach", "wie", "Grab\u00b7ge\u00b7leit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "KOKOM", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}