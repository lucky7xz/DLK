{"dta.poem.10170": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der gestirnte Baum.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20086-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Zweige, welche sonst durch gr\u00fcnes Laub verdecket,", "tokens": ["Die", "Zwei\u00b7ge", ",", "wel\u00b7che", "sonst", "durch", "gr\u00fc\u00b7nes", "Laub", "ver\u00b7de\u00b7cket", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sind auch anietzt aufs nen verstecket.", "tokens": ["Sind", "auch", "an\u00b7ietzt", "aufs", "nen", "ver\u00b7ste\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "APPRART", "ADJA", "VVFIN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Ein rauher Reiff, der alles ietzt erf\u00fcllet,", "tokens": ["Ein", "rau\u00b7her", "Reiff", ",", "der", "al\u00b7les", "ietzt", "er\u00b7f\u00fcl\u00b7let", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PIS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Hat auch die kleinsten Zweig\u2019 \u00fcmgeben und verh\u00fcllet;", "tokens": ["Hat", "auch", "die", "kleins\u00b7ten", "Zweig'", "\u00fcm\u00b7ge\u00b7ben", "und", "ver\u00b7h\u00fcl\u00b7let", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "VVINF", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So, da\u00df der B\u00e4ume Wipfel sich", "tokens": ["So", ",", "da\u00df", "der", "B\u00e4u\u00b7me", "Wip\u00b7fel", "sich"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "ART", "NN", "NN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In ihren gross- und kleinen Zweigen,", "tokens": ["In", "ih\u00b7ren", "gross", "und", "klei\u00b7nen", "Zwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "TRUNC", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Absonderlich von weiten, eigentlich,", "tokens": ["Ab\u00b7son\u00b7der\u00b7lich", "von", "wei\u00b7ten", ",", "ei\u00b7gent\u00b7lich", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "$,", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Als w\u00e4ren sie aufs neu belaubet, zeigen:", "tokens": ["Als", "w\u00e4\u00b7ren", "sie", "aufs", "neu", "be\u00b7lau\u00b7bet", ",", "zei\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "APPRART", "ADJD", "VVFIN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Zumahl wenn sich der Reiff mit Sternen-f\u00f6rm\u2019gen", "tokens": ["Zu\u00b7mahl", "wenn", "sich", "der", "Reiff", "mit", "Ster\u00b7nen\u00b7f\u00f6r\u00b7m'\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "PRF", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Der mit den Spitzen sich an seine Theilchen h\u00e4nget,", "tokens": ["Der", "mit", "den", "Spit\u00b7zen", "sich", "an", "sei\u00b7ne", "Theil\u00b7chen", "h\u00e4n\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "PRF", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und ihn dadurch noch luckrer macht, vermenget.", "tokens": ["Und", "ihn", "da\u00b7durch", "noch", "luck\u00b7rer", "macht", ",", "ver\u00b7men\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PPER", "PAV", "ADV", "ADJD", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ich habe solchen Baum einst Wunder-Wunder-sch\u00f6n,", "tokens": ["Ich", "ha\u00b7be", "sol\u00b7chen", "Baum", "einst", "Wun\u00b7der\u00b7Wun\u00b7der\u00b7sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In einer Winter-Nacht, gesehn:", "tokens": ["In", "ei\u00b7ner", "Win\u00b7ter\u00b7Nacht", ",", "ge\u00b7sehn", ":"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als der entw\u00f6lckte Mond auf die gefrornen Spitzen,", "tokens": ["Als", "der", "ent\u00b7w\u00f6lck\u00b7te", "Mond", "auf", "die", "ge\u00b7fror\u00b7nen", "Spit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Indem es eben starck gereifft,", "tokens": ["In\u00b7dem", "es", "e\u00b7ben", "starck", "ge\u00b7reifft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und der gefrorne Schnee sich \u00fcberall geh\u00e4ufft,", "tokens": ["Und", "der", "ge\u00b7fror\u00b7ne", "Schnee", "sich", "\u00fc\u00b7be\u00b7rall", "ge\u00b7h\u00e4ufft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "PRF", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Mit hellem Schimmer fiel. Man sah\u2019 ein helles blitzen", "tokens": ["Mit", "hel\u00b7lem", "Schim\u00b7mer", "fiel", ".", "Man", "sah'", "ein", "hel\u00b7les", "blit\u00b7zen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$.", "PIS", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "So kr\u00e4fftig, starck und hell, da\u00df sie nicht anders schienen,", "tokens": ["So", "kr\u00e4ff\u00b7tig", ",", "starck", "und", "hell", ",", "da\u00df", "sie", "nicht", "an\u00b7ders", "schie\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADJD", "KON", "ADJD", "$,", "KOUS", "PPER", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Als Sterne erster Gr\u00f6ss\u2019 an den Sapphirnen B\u00fchnen.", "tokens": ["Als", "Ster\u00b7ne", "ers\u00b7ter", "Gr\u00f6ss'", "an", "den", "Sap\u00b7phir\u00b7nen", "B\u00fch\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Ich ward recht in der That dadurch betrogen.", "tokens": ["Ich", "ward", "recht", "in", "der", "That", "da\u00b7durch", "be\u00b7tro\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "ART", "NN", "PAV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Denn, wie ich mein Gesicht von unten aufw\u00e4rts wandt\u2019,", "tokens": ["Denn", ",", "wie", "ich", "mein", "Ge\u00b7sicht", "von", "un\u00b7ten", "auf\u00b7w\u00e4rts", "wandt'", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWAV", "PPER", "PPOSAT", "NN", "APPR", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Um, durch den Baum, des Himmels blauen Bogen", "tokens": ["Um", ",", "durch", "den", "Baum", ",", "des", "Him\u00b7mels", "blau\u00b7en", "Bo\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUI", "$,", "APPR", "ART", "NN", "$,", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Bewundernd anzusehn, und ihn voll Sterne fand,", "tokens": ["Be\u00b7wun\u00b7dernd", "an\u00b7zu\u00b7sehn", ",", "und", "ihn", "voll", "Ster\u00b7ne", "fand", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVIZU", "$,", "KON", "PPER", "ADJD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die ich sonst nie gesehn; erstaunt ich, bis ich klar", "tokens": ["Die", "ich", "sonst", "nie", "ge\u00b7sehn", ";", "er\u00b7staunt", "ich", ",", "bis", "ich", "klar"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "PPER", "ADV", "ADV", "VVPP", "$.", "VVFIN", "PPER", "$,", "KOUS", "PPER", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Erblickte, wie die glatten Spitzen", "tokens": ["Er\u00b7blick\u00b7te", ",", "wie", "die", "glat\u00b7ten", "Spit\u00b7zen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "PWAV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Vom hart gefrornen Schnee, mit einem hellen blitzen,", "tokens": ["Vom", "hart", "ge\u00b7fror\u00b7nen", "Schnee", ",", "mit", "ei\u00b7nem", "hel\u00b7len", "blit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJD", "ADJA", "NN", "$,", "APPR", "ART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Der neuen Sternen Ursprung war.", "tokens": ["Der", "neu\u00b7en", "Ster\u00b7nen", "Ur\u00b7sprung", "war", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Zwar wird mein Auge fast, in diesem hellen Schein,", "tokens": ["Zwar", "wird", "mein", "Au\u00b7ge", "fast", ",", "in", "die\u00b7sem", "hel\u00b7len", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "ADV", "$,", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Geblendet und verwirrt; allein", "tokens": ["Ge\u00b7blen\u00b7det", "und", "ver\u00b7wirrt", ";", "al\u00b7lein"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["VVPP", "KON", "ADJD", "$.", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "O grosses All! Ach! la\u00df die Creatur", "tokens": ["O", "gros\u00b7ses", "All", "!", "Ach", "!", "la\u00df", "die", "Crea\u00b7tur"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NE", "ADJA", "NN", "$.", "ITJ", "$.", "VVIMP", "ART", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Uns offt, wann wir mit Lust derselben Schmuck versp\u00fchren,", "tokens": ["Uns", "offt", ",", "wann", "wir", "mit", "Lust", "der\u00b7sel\u00b7ben", "Schmuck", "ver\u00b7sp\u00fch\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "PWAV", "PPER", "APPR", "NN", "PDAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Auf solche Art verwirrt, auf die gew\u00fcnschte Spur", "tokens": ["Auf", "sol\u00b7che", "Art", "ver\u00b7wirrt", ",", "auf", "die", "ge\u00b7w\u00fcnschte", "Spur"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "ADJD", "$,", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.6": {"text": "Von Deiner Wunder-Gr\u00f6sse, f\u00fchren!", "tokens": ["Von", "Dei\u00b7ner", "Wun\u00b7der\u00b7Gr\u00f6s\u00b7se", ",", "f\u00fch\u00b7ren", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}