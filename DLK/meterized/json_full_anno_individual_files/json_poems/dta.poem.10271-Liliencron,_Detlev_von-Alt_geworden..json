{"dta.poem.10271": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "Alt geworden.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1883", "urn": "urn:nbn:de:kobv:b4-200905197184", "language": ["de:0.99"], "booktitle": "Liliencron, Detlev von: Adjutantenritte und andere Gedichte. Leipzig, [1883]."}, "poem": {"stanza.1": {"line.1": {"text": "Unvergessen bleibt der Garten,             ", "tokens": ["Un\u00b7ver\u00b7ges\u00b7sen", "bleibt", "der", "Gar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der des Kindes Welt enthielt.", "tokens": ["Der", "des", "Kin\u00b7des", "Welt", "ent\u00b7hielt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ob in seinen engen Wegen", "tokens": ["Ob", "in", "sei\u00b7nen", "en\u00b7gen", "We\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Noch ein Kindesh\u00e4ndchen spielt?", "tokens": ["Noch", "ein", "Kin\u00b7des\u00b7h\u00e4nd\u00b7chen", "spielt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Und wie tief die Waldesschatten,", "tokens": ["Und", "wie", "tief", "die", "Wal\u00b7des\u00b7schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ADJD", "ART", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Junger Liebe erstes Jahr.", "tokens": ["Jun\u00b7ger", "Lie\u00b7be", "ers\u00b7tes", "Jahr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ob die B\u00e4ume wohl noch leben,", "tokens": ["Ob", "die", "B\u00e4u\u00b7me", "wohl", "noch", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ob sie scheitelt noch ihr Haar?", "tokens": ["Ob", "sie", "schei\u00b7telt", "noch", "ihr", "Haar", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Regen rauschte viel hernieder,", "tokens": ["Re\u00b7gen", "rauschte", "viel", "her\u00b7nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "PTKVZ", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Viele Jahre rauschten hin.", "tokens": ["Vie\u00b7le", "Jah\u00b7re", "rauschten", "hin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PTKVZ", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Waldesschatten, kleiner Garten \u2014", "tokens": ["Wal\u00b7des\u00b7schat\u00b7ten", ",", "klei\u00b7ner", "Gar\u00b7ten"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Grauer Bart umw\u00e4chst das Kinn.", "tokens": ["Grau\u00b7er", "Bart", "um\u00b7w\u00e4chst", "das", "Kinn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}