{"dta.poem.5759": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Eigenschafften der  \n Materie.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.85", "af:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Auf diese Weise wird das Wesen", "tokens": ["Auf", "die\u00b7se", "Wei\u00b7se", "wird", "das", "We\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von der ", "tokens": ["Von", "der"], "token_info": ["word", "word"], "pos": ["APPR", "ART"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Der Grund allein giebt ihren Stand,", "tokens": ["Der", "Grund", "al\u00b7lein", "giebt", "ih\u00b7ren", "Stand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und ihre Eigenschafft uns klar zu lesen.", "tokens": ["Und", "ih\u00b7re", "Ei\u00b7gen\u00b7schafft", "uns", "klar", "zu", "le\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PPER", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Sie fliessen all\u2019 daher, blos, da\u00df sie ausgespannt.", "tokens": ["Sie", "flies\u00b7sen", "all'", "da\u00b7her", ",", "blos", ",", "da\u00df", "sie", "aus\u00b7ge\u00b7spannt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "PAV", "$,", "ADV", "$,", "KOUS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}