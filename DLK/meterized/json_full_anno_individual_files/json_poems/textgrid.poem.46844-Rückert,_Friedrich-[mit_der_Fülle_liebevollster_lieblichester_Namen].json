{"textgrid.poem.46844": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[mit der F\u00fclle liebevollster lieblichester Namen]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit der F\u00fclle liebevollster lieblichester Namen,", "tokens": ["Mit", "der", "F\u00fcl\u00b7le", "lie\u00b7be\u00b7volls\u00b7ter", "lieb\u00b7li\u00b7ches\u00b7ter", "Na\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.2": {"text": "Herzchen, Liebchen, Kind! mit jedem besten besten Namen", "tokens": ["Herz\u00b7chen", ",", "Lieb\u00b7chen", ",", "Kind", "!", "mit", "je\u00b7dem", "bes\u00b7ten", "bes\u00b7ten", "Na\u00b7men"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "$.", "APPR", "PIAT", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.3": {"text": "Nannt' ich gern im Leben dich; in solchem Liebesschwanken,", "tokens": ["Nannt'", "ich", "gern", "im", "Le\u00b7ben", "dich", ";", "in", "sol\u00b7chem", "Lie\u00b7bes\u00b7schwan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPRART", "NN", "PPER", "$.", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.4": {"text": "Tochter, blieb bei vielen Namen dir kein fester Namen.", "tokens": ["Toch\u00b7ter", ",", "blieb", "bei", "vie\u00b7len", "Na\u00b7men", "dir", "kein", "fes\u00b7ter", "Na\u00b7men", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["N