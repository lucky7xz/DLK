{"textgrid.poem.39388": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: In Hosen steckst du deine T\u00e4nzerinnen?", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In Hosen steckst du deine T\u00e4nzerinnen?", "tokens": ["In", "Ho\u00b7sen", "steckst", "du", "dei\u00b7ne", "T\u00e4n\u00b7ze\u00b7rin\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "O! welch ein puritanisches Beginnen!", "tokens": ["O", "!", "welch", "ein", "pu\u00b7ri\u00b7ta\u00b7ni\u00b7sches", "Be\u00b7gin\u00b7nen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PWAT", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Cythere zeigt sich nackt, warm athmend noch im Stein,", "tokens": ["Cy\u00b7the\u00b7re", "zeigt", "sich", "nackt", ",", "warm", "ath\u00b7mend", "noch", "im", "Stein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PRF", "ADJD", "$,", "ADJD", "VVPP", "ADV", "APPRART", "NN", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.4": {"text": "Und weckt Begierden nur in p\u00f6belhaften Sinnen.", "tokens": ["Und", "weckt", "Be\u00b7gier\u00b7den", "nur", "in", "p\u00f6\u00b7bel\u00b7haf\u00b7ten", "Sin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die Kunst und die Decenz, sie haben nichts gemein.", "tokens": ["Die", "Kunst", "und", "die", "De\u00b7cenz", ",", "sie", "ha\u00b7ben", "nichts", "ge\u00b7mein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,", "PPER", "VAFIN", "PIS", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Den \u00fcpp'gen Gliederbau, die biegsamen Gestalten", "tokens": ["Den", "\u00fcpp'\u00b7gen", "Glie\u00b7der\u00b7bau", ",", "die", "bieg\u00b7sa\u00b7men", "Ge\u00b7stal\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "In jeder Wendung darf Terpsichore entfalten,", "tokens": ["In", "je\u00b7der", "Wen\u00b7dung", "darf", "Ter\u00b7psi\u00b7cho\u00b7re", "ent\u00b7fal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VMFIN", "NE", "VVFIN", "$,"], "meter": "-+-+-+++-+-+-", "measure": "unknown.measure.septa"}, "line.8": {"text": "Und im Tricot zugleich nackt und bekleidet sein.", "tokens": ["Und", "im", "Tri\u00b7cot", "zu\u00b7gleich", "nackt", "und", "be\u00b7klei\u00b7det", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "ADV", "ADJD", "KON", "VVFIN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die Hosen sollen wohl die M\u00e4dchen sittsam machen;", "tokens": ["Die", "Ho\u00b7sen", "sol\u00b7len", "wohl", "die", "M\u00e4d\u00b7chen", "sitt\u00b7sam", "ma\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2