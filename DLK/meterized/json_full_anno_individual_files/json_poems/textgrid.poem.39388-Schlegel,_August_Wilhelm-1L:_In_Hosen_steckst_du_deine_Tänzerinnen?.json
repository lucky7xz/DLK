a"}, "line.7": {"text": "Das andre Bein, gestreckt, nach allen Winden dreh'n.", "tokens": ["Das", "and\u00b7re", "Bein", ",", "ge\u00b7streckt", ",", "nach", "al\u00b7len", "Win\u00b7den", "dreh'", "n."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "abbreviation"], "pos": ["ART", "ADJA", "NN", "$,", "VVPP", "$,", "APPR", "PIAT", "NN", "CARD", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die S\u00fcnden des Geschmacks mi\u00dfbilligt auch die Tugend", "tokens": ["Die", "S\u00fcn\u00b7den", "des", "Ge\u00b7schmacks", "mi\u00df\u00b7bil\u00b7ligt", "auch", "die", "Tu\u00b7gend"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Sonst ist sie tolerant f\u00fcr Sch\u00f6nheit, Reiz und Jugend.", "tokens": ["Sonst", "ist", "sie", "to\u00b7le\u00b7rant", "f\u00fcr", "Sch\u00f6n\u00b7heit", ",", "Reiz", "und", "Ju\u00b7gend", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "APPR", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "In Hosen steckst du deine T\u00e4nzerinnen?", "tokens": ["In", "Ho\u00b7sen", "steckst", "du", "dei\u00b7ne", "T\u00e4n\u00b7ze\u00b7rin\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "O! welch ein puritanisches Beginnen!", "tokens": ["O", "!", "welch", "ein", "pu\u00b7ri\u00b7ta\u00b7ni\u00b7sches", "Be\u00b7gin\u00b7nen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE"