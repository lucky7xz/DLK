{"textgrid.poem.24793": {"metadata": {"author": {"name": "Hofmannsthal, Hugo von", "birth": "N.A.", "death": "N.A."}, "title": "Der Kaiser von China spricht:", "genre": "verse", "period": "N.A.", "pub_year": 1901, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In der Mitte aller Dinge", "tokens": ["In", "der", "Mit\u00b7te", "al\u00b7ler", "Din\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wohne Ich, der Sohn des Himmels.", "tokens": ["Woh\u00b7ne", "Ich", ",", "der", "Sohn", "des", "Him\u00b7mels", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Meine Frauen, meine B\u00e4ume,", "tokens": ["Mei\u00b7ne", "Frau\u00b7en", ",", "mei\u00b7ne", "B\u00e4u\u00b7me", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Meine Tiere, meine Teiche", "tokens": ["Mei\u00b7ne", "Tie\u00b7re", ",", "mei\u00b7ne", "Tei\u00b7che"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Schlie\u00dft die erste Mauer ein.", "tokens": ["Schlie\u00dft", "die", "ers\u00b7te", "Mau\u00b7er", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Drunten liegen meine Ahnen:", "tokens": ["Drun\u00b7ten", "lie\u00b7gen", "mei\u00b7ne", "Ah\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Aufgebahrt mit ihren Waffen,", "tokens": ["Auf\u00b7ge\u00b7bahrt", "mit", "ih\u00b7ren", "Waf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ihre Kronen auf den H\u00e4uptern,", "tokens": ["Ih\u00b7re", "Kro\u00b7nen", "auf", "den", "H\u00e4up\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Wie es einem jeden ziemt,", "tokens": ["Wie", "es", "ei\u00b7nem", "je\u00b7den", "ziemt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "PIAT", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Wohnen sie in den Gew\u00f6lben.", "tokens": ["Woh\u00b7nen", "sie", "in", "den", "Ge\u00b7w\u00f6l\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "ART", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.11": {"text": "Bis ins Herz der Welt hinunter", "tokens": ["Bis", "ins", "Herz", "der", "Welt", "hin\u00b7un\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "APPRART", "NN", "ART", "NN", "APPO"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Dr\u00f6hnt das Schreiten meiner Hoheit.", "tokens": ["Dr\u00f6hnt", "das", "Schrei\u00b7ten", "mei\u00b7ner", "Ho\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Stumm von meinen Rasenb\u00e4nken,", "tokens": ["Stumm", "von", "mei\u00b7nen", "Ra\u00b7sen\u00b7b\u00e4n\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Gr\u00fcnen Schemeln meiner F\u00fc\u00dfe,", "tokens": ["Gr\u00fc\u00b7nen", "Sche\u00b7meln", "mei\u00b7ner", "F\u00fc\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Gehen gleichgeteilte Str\u00f6me", "tokens": ["Ge\u00b7hen", "gleich\u00b7ge\u00b7teil\u00b7te", "Str\u00f6\u00b7me"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Osten-, west- und s\u00fcd- und nordw\u00e4rts,", "tokens": ["Os\u00b7ten", ",", "west", "und", "s\u00fcd", "und", "nord\u00b7w\u00e4rts", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["TRUNC", "$,", "TRUNC", "KON", "TRUNC", "KON", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Meinen Garten zu bew\u00e4ssern,", "tokens": ["Mei\u00b7nen", "Gar\u00b7ten", "zu", "be\u00b7w\u00e4s\u00b7sern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Der die weite Erde ist.", "tokens": ["Der", "die", "wei\u00b7te", "Er\u00b7de", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.19": {"text": "Spiegeln hier die dunkeln Augen,", "tokens": ["Spie\u00b7geln", "hier", "die", "dun\u00b7keln", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Bunten Schwingen meiner Tiere,", "tokens": ["Bun\u00b7ten", "Schwin\u00b7gen", "mei\u00b7ner", "Tie\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Spiegeln drau\u00dfen bunte St\u00e4dte,", "tokens": ["Spie\u00b7geln", "drau\u00b7\u00dfen", "bun\u00b7te", "St\u00e4d\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Dunkle Mauern, dichte W\u00e4lder", "tokens": ["Dunk\u00b7le", "Mau\u00b7ern", ",", "dich\u00b7te", "W\u00e4l\u00b7der"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "Und Gesichter vieler V\u00f6lker.", "tokens": ["Und", "Ge\u00b7sich\u00b7ter", "vie\u00b7ler", "V\u00f6l\u00b7ker", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Meine Edlen, wie die Sterne,", "tokens": ["Mei\u00b7ne", "Ed\u00b7len", ",", "wie", "die", "Ster\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PWAV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "Wohnen rings um mich, sie haben", "tokens": ["Woh\u00b7nen", "rings", "um", "mich", ",", "sie", "ha\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "ADV", "APPR", "PPER", "$,", "PPER", "VAFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "Namen, die ich ihnen gab,", "tokens": ["Na\u00b7men", ",", "die", "ich", "ih\u00b7nen", "gab", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.27": {"text": "Namen nach der einen Stunde,", "tokens": ["Na\u00b7men", "nach", "der", "ei\u00b7nen", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Da mir einer n\u00e4her kam,", "tokens": ["Da", "mir", "ei\u00b7ner", "n\u00e4\u00b7her", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.29": {"text": "Frauen, die ich ihnen schenkte,", "tokens": ["Frau\u00b7en", ",", "die", "ich", "ih\u00b7nen", "schenk\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.30": {"text": "Und die Scharen ihrer Kinder;", "tokens": ["Und", "die", "Scha\u00b7ren", "ih\u00b7rer", "Kin\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.31": {"text": "Allen Edlen dieser Erde", "tokens": ["Al\u00b7len", "Ed\u00b7len", "die\u00b7ser", "Er\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "NN", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.32": {"text": "Schuf ich Augen, Wuchs und Lippen,", "tokens": ["Schuf", "ich", "Au\u00b7gen", ",", "Wuchs", "und", "Lip\u00b7pen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.33": {"text": "Wie der G\u00e4rtner an den Blumen.", "tokens": ["Wie", "der", "G\u00e4rt\u00b7ner", "an", "den", "Blu\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.34": {"text": "Aber zwischen \u00e4u\u00dfern Mauern", "tokens": ["A\u00b7ber", "zwi\u00b7schen", "\u00e4u\u00b7\u00dfern", "Mau\u00b7ern"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.35": {"text": "Wohnen V\u00f6lker, meine Krieger,", "tokens": ["Woh\u00b7nen", "V\u00f6l\u00b7ker", ",", "mei\u00b7ne", "Krie\u00b7ger", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.36": {"text": "V\u00f6lker, meine Ackerbauer.", "tokens": ["V\u00f6l\u00b7ker", ",", "mei\u00b7ne", "A\u00b7cker\u00b7bau\u00b7er", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.37": {"text": "Neue Mauern und dann wieder", "tokens": ["Neu\u00b7e", "Mau\u00b7ern", "und", "dann", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "ADV", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.38": {"text": "Jene unterworfnen V\u00f6lker,", "tokens": ["Je\u00b7ne", "un\u00b7ter\u00b7worf\u00b7nen", "V\u00f6l\u00b7ker", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.39": {"text": "V\u00f6lker immer dumpfern Blutes,", "tokens": ["V\u00f6l\u00b7ker", "im\u00b7mer", "dum\u00b7pfern", "Blu\u00b7tes", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.40": {"text": "Bis ans Meer, die letzte Mauer,", "tokens": ["Bis", "ans", "Meer", ",", "die", "letz\u00b7te", "Mau\u00b7er", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "APPRART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.41": {"text": "Die mein Reich und mich umgibt.", "tokens": ["Die", "mein", "Reich", "und", "mich", "um\u00b7gibt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "KON", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "In der Mitte aller Dinge", "tokens": ["In", "der", "Mit\u00b7te", "al\u00b7ler", "Din\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wohne Ich, der Sohn des Himmels.", "tokens": ["Woh\u00b7ne", "Ich", ",", "der", "Sohn", "des", "Him\u00b7mels", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Meine Frauen, meine B\u00e4ume,", "tokens": ["Mei\u00b7ne", "Frau\u00b7en", ",", "mei\u00b7ne", "B\u00e4u\u00b7me", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Meine Tiere, meine Teiche", "tokens": ["Mei\u00b7ne", "Tie\u00b7re", ",", "mei\u00b7ne", "Tei\u00b7che"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Schlie\u00dft die erste Mauer ein.", "tokens": ["Schlie\u00dft", "die", "ers\u00b7te", "Mau\u