{"dta.poem.10236": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "Die Schlacht bei Bornh\u00f6ved.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1883", "urn": "urn:nbn:de:kobv:b4-200905197184", "language": ["de:0.99"], "booktitle": "Liliencron, Detlev von: Adjutantenritte und andere Gedichte. Leipzig, [1883]."}, "poem": {"stanza.1": {"line.1": {"text": "Der K\u00f6nig, der in Banden war             ", "tokens": ["Der", "K\u00f6\u00b7nig", ",", "der", "in", "Ban\u00b7den", "war"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Grafen von Schwerin,", "tokens": ["Des", "Gra\u00b7fen", "von", "Schwe\u00b7rin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Das war der K\u00f6nig Waldemar,", "tokens": ["Das", "war", "der", "K\u00f6\u00b7nig", "Wal\u00b7de\u00b7mar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Verst\u00e4ubter Hermelin.", "tokens": ["Ver\u00b7st\u00e4ub\u00b7ter", "Her\u00b7me\u00b7lin", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Bis er versprach, das ganze Land,", "tokens": ["Bis", "er", "ver\u00b7sprach", ",", "das", "gan\u00b7ze", "Land", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wo deutscher Stamm und Kern,", "tokens": ["Wo", "deut\u00b7scher", "Stamm", "und", "Kern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zur\u00fcckzugeben in die Hand", "tokens": ["Zu\u00b7r\u00fcck\u00b7zu\u00b7ge\u00b7ben", "in", "die", "Hand"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der anerkannten Herrn.", "tokens": ["Der", "an\u00b7er\u00b7kann\u00b7ten", "Herrn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Auf M\u00e4rschen lang und M\u00e4rschen hei\u00df", "tokens": ["Auf", "M\u00e4r\u00b7schen", "lang", "und", "M\u00e4r\u00b7schen", "hei\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "ADJD", "KON", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des K\u00f6nigs Helmbusch vorn,", "tokens": ["Des", "K\u00f6\u00b7nigs", "Helm\u00b7busch", "vorn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der nickt und winkt scharlach und wei\u00df", "tokens": ["Der", "nickt", "und", "winkt", "schar\u00b7lach", "und", "wei\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "KON", "VVFIN", "ADJD", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und gr\u00fc\u00dft den G\u00fcldensporn.", "tokens": ["Und", "gr\u00fc\u00dft", "den", "G\u00fcl\u00b7den\u00b7sporn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Gen\u00fcber schnitzt sein Widerpart", "tokens": ["Ge\u00b7n\u00fc\u00b7ber", "schnitzt", "sein", "Wi\u00b7der\u00b7part"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Pfeil sich und den Bolz,", "tokens": ["Den", "Pfeil", "sich", "und", "den", "Bolz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "KON", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Von Bremen Bischof Gerihardt,", "tokens": ["Von", "Bre\u00b7men", "Bi\u00b7schof", "Ge\u00b7ri\u00b7hardt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NE", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Graf Adolf, Holstenstolz.", "tokens": ["Graf", "A\u00b7dolf", ",", "Hols\u00b7ten\u00b7stolz", "."], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NE", "NE", "$,", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Maria Magdalenentag,", "tokens": ["Ma\u00b7ria", "Mag\u00b7da\u00b7le\u00b7nen\u00b7tag", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Mittsommersonnenschein,", "tokens": ["Mitt\u00b7som\u00b7mer\u00b7son\u00b7nen\u00b7schein", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gel\u00e4rm auf Schild und Eisendach,", "tokens": ["Ge\u00b7l\u00e4rm", "auf", "Schild", "und", "Ei\u00b7sen\u00b7dach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Lanzen rasseln drein.", "tokens": ["Die", "Lan\u00b7zen", "ras\u00b7seln", "drein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Wie Blatt und Zweig im Bachgesp\u00fcl,", "tokens": ["Wie", "Blatt", "und", "Zweig", "im", "Bach\u00b7ge\u00b7sp\u00fcl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So treibt manch blond Gesell.", "tokens": ["So", "treibt", "manch", "blond", "Ge\u00b7sell", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Graf Adolf nur im Kampfgew\u00fchl,", "tokens": ["Graf", "A\u00b7dolf", "nur", "im", "Kampf\u00b7ge\u00b7w\u00fchl", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er treibt nicht von der Stell\u2019.", "tokens": ["Er", "treibt", "nicht", "von", "der", "Stell'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "\u201emaria Magdalena, hilf,", "tokens": ["\u201e", "ma\u00b7ria", "Mag\u00b7da\u00b7le\u00b7na", ",", "hilf", ","], "token_info": ["punct", "word", "word", "punct", "word", "punct"], "pos": ["$(", "FM", "FM", "$,", "VVIMP", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.2": {"text": "D\u00e4mm\u2019 ab die D\u00e4nenflut,", "tokens": ["D\u00e4mm'", "ab", "die", "D\u00e4\u00b7nen\u00b7flut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du hebst zerknicktes Rohr und Schilf,", "tokens": ["Du", "hebst", "zer\u00b7knick\u00b7tes", "Rohr", "und", "Schilf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gieb uns den alten Mut,", "tokens": ["Gieb", "uns", "den", "al\u00b7ten", "Mut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Der Graf packt fest in Zeug und Riem,", "tokens": ["Der", "Graf", "packt", "fest", "in", "Zeug", "und", "Riem", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sieg oder untergehn.", "tokens": ["Sieg", "o\u00b7der", "un\u00b7ter\u00b7gehn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "VVINF", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Da sieh! am Himmel zeigt sich ihm", "tokens": ["Da", "sieh", "!", "am", "Him\u00b7mel", "zeigt", "sich", "ihm"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "PTKVZ", "$.", "APPRART", "NN", "VVFIN", "PRF", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Maria Magdalen,", "tokens": ["Ma\u00b7ria", "Mag\u00b7da\u00b7len", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.9": {"line.1": {"text": "Hei! flog der Graf ins Schlachtgedr\u00e4ng,", "tokens": ["Hei", "!", "flog", "der", "Graf", "ins", "Schlacht\u00b7ge\u00b7dr\u00e4ng", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Die Axt durchbricht den Wald,", "tokens": ["Die", "Axt", "durch\u00b7bricht", "den", "Wald", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Um seinen Harnisch im Gemeng", "tokens": ["Um", "sei\u00b7nen", "Har\u00b7nisch", "im", "Ge\u00b7meng"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUI", "PPOSAT", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Holstentatze krallt.", "tokens": ["Die", "Hols\u00b7ten\u00b7tat\u00b7ze", "krallt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Herr Waldemar, der D\u00e4nen Schild,", "tokens": ["Herr", "Wal\u00b7de\u00b7mar", ",", "der", "D\u00e4\u00b7nen", "Schild", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie hei\u00dfes Eisen gl\u00fcht.", "tokens": ["Wie", "hei\u00b7\u00dfes", "Ei\u00b7sen", "gl\u00fcht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In seinen Augen roth und wild", "tokens": ["In", "sei\u00b7nen", "Au\u00b7gen", "roth", "und", "wild"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Zornesblume bl\u00fcht.", "tokens": ["Die", "Zor\u00b7nes\u00b7blu\u00b7me", "bl\u00fcht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}