{"textgrid.poem.31301": {"metadata": {"author": {"name": "Kleist, Heinrich von", "birth": "N.A.", "death": "N.A."}, "title": "Der Engel am Grabe des Herrn", "genre": "verse", "period": "N.A.", "pub_year": 1794, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als still und kalt, mit sieben Todeswunden,", "tokens": ["Als", "still", "und", "kalt", ",", "mit", "sie\u00b7ben", "To\u00b7des\u00b7wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "KON", "ADJD", "$,", "APPR", "CARD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Herr in seinem Grabe lag; das Grab,", "tokens": ["Der", "Herr", "in", "sei\u00b7nem", "Gra\u00b7be", "lag", ";", "das", "Grab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$.", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Als sollt es zehn lebendge Riesen fesseln,", "tokens": ["Als", "sollt", "es", "zehn", "le\u00b7bend\u00b7ge", "Rie\u00b7sen", "fes\u00b7seln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VMFIN", "PPER", "CARD", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In eine Felskluft schmetternd eingehauen;", "tokens": ["In", "ei\u00b7ne", "Fels\u00b7kluft", "schmet\u00b7ternd", "ein\u00b7ge\u00b7hau\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Gew\u00e4lzet, mit der M\u00e4nner Kraft, verschlo\u00df", "tokens": ["Ge\u00b7w\u00e4l\u00b7zet", ",", "mit", "der", "M\u00e4n\u00b7ner", "Kraft", ",", "ver\u00b7schlo\u00df"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["VVPP", "$,", "APPR", "ART", "NN", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ein Sandstein, der Bestechung taub, die T\u00fcre;", "tokens": ["Ein", "Sand\u00b7stein", ",", "der", "Be\u00b7ste\u00b7chung", "taub", ",", "die", "T\u00fc\u00b7re", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "ADJD", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Rings war des Landvogts Siegel aufgedr\u00fcckt:", "tokens": ["Rings", "war", "des", "Land\u00b7vogts", "Sie\u00b7gel", "auf\u00b7ge\u00b7dr\u00fcckt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Es h\u00e4tte der Gedanke selber nicht", "tokens": ["Es", "h\u00e4t\u00b7te", "der", "Ge\u00b7dan\u00b7ke", "sel\u00b7ber", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "PTKNEG"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Der H\u00f6hle unbemerkt entschl\u00fcpfen k\u00f6nnen;", "tokens": ["Der", "H\u00f6h\u00b7le", "un\u00b7be\u00b7merkt", "ent\u00b7schl\u00fcp\u00b7fen", "k\u00f6n\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Und gleichwohl noch, als ob zu f\u00fcrchten sei,", "tokens": ["Und", "gleich\u00b7wohl", "noch", ",", "als", "ob", "zu", "f\u00fcrch\u00b7ten", "sei", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "$,", "KOKOM", "KOUS", "PTKZU", "VVINF", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Es k\u00f6nn auch der Granitblock sich bekehren,", "tokens": ["Es", "k\u00f6nn", "auch", "der", "Gra\u00b7nit\u00b7block", "sich", "be\u00b7keh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ART", "NN", "PRF", "VVINF", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Ging eine Schar von H\u00fctern auf und ab,", "tokens": ["Ging", "ei\u00b7ne", "Schar", "von", "H\u00fc\u00b7tern", "auf", "und", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "NN", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Und starrte nach des Siegels Bildern hin:", "tokens": ["Und", "starr\u00b7te", "nach", "des", "Sie\u00b7gels", "Bil\u00b7dern", "hin", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Da kamen, bei des Morgens Strahl,", "tokens": ["Da", "ka\u00b7men", ",", "bei", "des", "Mor\u00b7gens", "Strahl", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "APPR", "ART", "ADV", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Des ewgen Glaubens voll, die drei Marien her,", "tokens": ["Des", "ew\u00b7gen", "Glau\u00b7bens", "voll", ",", "die", "drei", "Ma\u00b7ri\u00b7en", "her", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "$,", "PRELS", "CARD", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Zu sehn, ob Jesus noch darinnen sei:", "tokens": ["Zu", "sehn", ",", "ob", "Je\u00b7sus", "noch", "da\u00b7rin\u00b7nen", "sei", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "KOUS", "NE", "ADV", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Denn Er, versprochen hatt er ihnen,", "tokens": ["Denn", "Er", ",", "ver\u00b7spro\u00b7chen", "hatt", "er", "ih\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "VVPP", "VAFIN", "PPER", "PPER", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Er werd am dritten Tage auferstehn.", "tokens": ["Er", "werd", "am", "drit\u00b7ten", "Ta\u00b7ge", "auf\u00b7er\u00b7stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Da nun die Fraun, die gl\u00e4ubigen, sich nahten", "tokens": ["Da", "nun", "die", "Fraun", ",", "die", "gl\u00e4u\u00b7bi\u00b7gen", ",", "sich", "nah\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ADV", "ART", "NN", "$,", "ART", "ADJA", "$,", "PRF", "VVFIN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.20": {"text": "Der Grabesh\u00f6hle: was erblickten sie?", "tokens": ["Der", "Gra\u00b7bes\u00b7h\u00f6h\u00b7le", ":", "was", "er\u00b7blick\u00b7ten", "sie", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "PWS", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Die H\u00fcter, die das Grab bewachen sollten,", "tokens": ["Die", "H\u00fc\u00b7ter", ",", "die", "das", "Grab", "be\u00b7wa\u00b7chen", "soll\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Gest\u00fcrzt, das Angesicht in Staub,", "tokens": ["Ge\u00b7st\u00fcrzt", ",", "das", "An\u00b7ge\u00b7sicht", "in", "Staub", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Wie Tote, um den Felsen lagen sie;", "tokens": ["Wie", "To\u00b7te", ",", "um", "den", "Fel\u00b7sen", "la\u00b7gen", "sie", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "$,", "KOUI", "ART", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Der Stein war weit hinweggew\u00e4lzt vom Eingang;", "tokens": ["Der", "Stein", "war", "weit", "hin\u00b7weg\u00b7ge\u00b7w\u00e4lzt", "vom", "Ein\u00b7gang", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "VVPP", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "Und auf dem Rande sa\u00df, das Fl\u00fcgelpaar noch regend,", "tokens": ["Und", "auf", "dem", "Ran\u00b7de", "sa\u00df", ",", "das", "Fl\u00fc\u00b7gel\u00b7paar", "noch", "re\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "$,", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Ein Engel, wie der Blitz erscheint,", "tokens": ["Ein", "En\u00b7gel", ",", "wie", "der", "Blitz", "er\u00b7scheint", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Und sein Gewand so wei\u00df wie junger Schnee.", "tokens": ["Und", "sein", "Ge\u00b7wand", "so", "wei\u00df", "wie", "jun\u00b7ger", "Schnee", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADV", "VVFIN", "KOKOM", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "Da st\u00fcrzten sie, wie Leichen, selbst, getroffen,", "tokens": ["Da", "st\u00fcrz\u00b7ten", "sie", ",", "wie", "Lei\u00b7chen", ",", "selbst", ",", "ge\u00b7trof\u00b7fen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWAV", "NN", "$,", "ADV", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "Zu Boden hin, und f\u00fchlten sich wie Staub,", "tokens": ["Zu", "Bo\u00b7den", "hin", ",", "und", "f\u00fchl\u00b7ten", "sich", "wie", "Staub", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKVZ", "$,", "KON", "VVFIN", "PRF", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.30": {"text": "Und meinten, gleich im Glanze zu vergehn:", "tokens": ["Und", "mein\u00b7ten", ",", "gleich", "im", "Glan\u00b7ze", "zu", "ver\u00b7gehn", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ADV", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.31": {"text": "Doch er, er sprach, der Cherub: \u00bbF\u00fcrchtet nicht!", "tokens": ["Doch", "er", ",", "er", "sprach", ",", "der", "Che\u00b7rub", ":", "\u00bb", "F\u00fcrch\u00b7tet", "nicht", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "PPER", "VVFIN", "$,", "ART", "NE", "$.", "$(", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.32": {"text": "Ihr suchtet Jesum, den Gekreuzigten \u2013", "tokens": ["Ihr", "such\u00b7tet", "Je\u00b7sum", ",", "den", "Ge\u00b7kreu\u00b7zig\u00b7ten", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "$,", "ART", "NN", "$("], "meter": "-+--+--+--", "measure": "amphibrach.tri.plus"}, "line.33": {"text": "Der aber ist nicht hier, er ist erstanden:", "tokens": ["Der", "a\u00b7ber", "ist", "nicht", "hier", ",", "er", "ist", "er\u00b7stan\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VAFIN", "PTKNEG", "ADV", "$,", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.34": {"text": "Kommt her, und schaut die \u00f6de St\u00e4tte an.\u00ab", "tokens": ["Kommt", "her", ",", "und", "schaut", "die", "\u00f6\u00b7de", "St\u00e4t\u00b7te", "an", ".", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVIMP", "PTKVZ", "$,", "KON", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.35": {"text": "Und fuhr, als sie, mit hocherhobnen H\u00e4nden,", "tokens": ["Und", "fuhr", ",", "als", "sie", ",", "mit", "ho\u00b7cher\u00b7hob\u00b7nen", "H\u00e4n\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPER", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.36": {"text": "Sprachlos die Grabesst\u00e4tte leer erschaut,", "tokens": ["Sprach\u00b7los", "die", "Gra\u00b7bess\u00b7t\u00e4t\u00b7te", "leer", "er\u00b7schaut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.37": {"text": "In seiner hehren Milde also fort:", "tokens": ["In", "sei\u00b7ner", "heh\u00b7ren", "Mil\u00b7de", "al\u00b7so", "fort", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.38": {"text": "\u00bbgeht hin, ihr Fraun, und k\u00fcndigt es nunmehr", "tokens": ["\u00bb", "geht", "hin", ",", "ihr", "Fraun", ",", "und", "k\u00fcn\u00b7digt", "es", "nun\u00b7mehr"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "VVFIN", "PTKVZ", "$,", "PPOSAT", "NN", "$,", "KON", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.39": {"text": "Den J\u00fcngern an, die er sich auserkoren,", "tokens": ["Den", "J\u00fcn\u00b7gern", "an", ",", "die", "er", "sich", "au\u00b7ser\u00b7ko\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "PRELS", "PPER", "PRF", "VVIZU", "$,"], "meter": "-+-+---+-+-", "measure": "unknown.measure.tetra"}, "line.40": {"text": "Da\u00df sie es allen Erdenv\u00f6lkern lehren,", "tokens": ["Da\u00df", "sie", "es", "al\u00b7len", "Er\u00b7den\u00b7v\u00f6l\u00b7kern", "leh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.41": {"text": "Und tun also, wie er getan\u00ab: und schwand.", "tokens": ["Und", "tun", "al\u00b7so", ",", "wie", "er", "ge\u00b7tan", "\u00ab", ":", "und", "schwand", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "$,", "PWAV", "PPER", "VVPP", "$(", "$.", "KON", "VVFIN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.2": {"line.1": {"text": "Als still und kalt, mit sieben Todeswunden,", "tokens": ["Als", "still", "und", "kalt", ",", "mit", "sie\u00b7ben", "To\u00b7des\u00b7wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "KON", "ADJD", "$,", "APPR", "CARD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Herr in seinem Grabe lag; das Grab,", "tokens": ["Der", "Herr", "in", "sei\u00b7nem", "Gra\u00b7be", "lag", ";", "das", "Grab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$.", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Als sollt es zehn lebendge Riesen fesseln,", "tokens": ["Als", "sollt", "es", "zehn", "le\u00b7bend\u00b7ge", "Rie\u00b7sen", "fes\u00b7seln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VMFIN", "PPER", "CARD", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In eine Felskluft schmetternd eingehauen;", "tokens": ["In", "ei\u00b7ne", "Fels\u00b7kluft", "schmet\u00b7ternd", "ein\u00b7ge\u00b7hau\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Gew\u00e4lzet, mit der M\u00e4nner Kraft, verschlo\u00df", "tokens": ["Ge\u00b7w\u00e4l\u00b7zet", ",", "mit", "der", "M\u00e4n\u00b7ner", "Kraft", ",", "ver\u00b7schlo\u00df"], "token_info": ["word", "punct", "word", "word", "word", "word", "p