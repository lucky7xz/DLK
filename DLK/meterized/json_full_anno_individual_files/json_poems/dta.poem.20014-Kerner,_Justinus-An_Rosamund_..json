{"dta.poem.20014": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "An Rosamund .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905193111", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sommers, wann die Lilien bl\u00fchen,", "tokens": ["Som\u00b7mers", ",", "wann", "die", "Li\u00b7li\u00b7en", "bl\u00fc\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Nelk' und Rose duftend gl\u00fchen,", "tokens": ["Nel\u00b7k'", "und", "Ro\u00b7se", "duf\u00b7tend", "gl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "ADJD", "VVINF", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "M\u00e4gdlein durch die G\u00e4rten wallen,", "tokens": ["M\u00e4gd\u00b7lein", "durch", "die", "G\u00e4r\u00b7ten", "wal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sch\u00f6n begr\u00fc\u00dft von Nachtigallen:", "tokens": ["Sch\u00f6n", "be\u00b7gr\u00fc\u00dft", "von", "Nach\u00b7ti\u00b7gal\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVPP", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Steh' ich wol am fernen Meere \u2014", "tokens": ["Steh'", "ich", "wol", "am", "fer\u00b7nen", "Mee\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPRART", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aber auf der \u00f6den Leere", "tokens": ["A\u00b7ber", "auf", "der", "\u00f6\u00b7den", "Lee\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wird dein Garten mir erbl\u00fchen,", "tokens": ["Wird", "dein", "Gar\u00b7ten", "mir", "er\u00b7bl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Werden deine Rosen gl\u00fchen;", "tokens": ["Wer\u00b7den", "dei\u00b7ne", "Ro\u00b7sen", "gl\u00fc\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Werden sich die blauen Wellen", "tokens": ["Wer\u00b7den", "sich", "die", "blau\u00b7en", "Wel\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PRF", "ART", "ADJA", "NN"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Mir zu euren Bergen schwellen,", "tokens": ["Mir", "zu", "eu\u00b7ren", "Ber\u00b7gen", "schwel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Werd' ich eure Th\u00e4ler, Auen", "tokens": ["Werd'", "ich", "eu\u00b7re", "Th\u00e4\u00b7ler", ",", "Au\u00b7en"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["VAFIN", "PPER", "PPOSAT", "NN", "$,", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bl\u00fchend in der Tief' erschauen,", "tokens": ["Bl\u00fc\u00b7hend", "in", "der", "Tie\u00b7f'", "er\u00b7schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Und dann zieht wol banges Sehnen", "tokens": ["Und", "dann", "zieht", "wol", "ban\u00b7ges", "Seh\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ADV", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mich darnieder, und mit Thr\u00e4nen", "tokens": ["Mich", "dar\u00b7nie\u00b7der", ",", "und", "mit", "Thr\u00e4\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "PAV", "$,", "KON", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Will ich sinken in die Rosen; \u2014", "tokens": ["Will", "ich", "sin\u00b7ken", "in", "die", "Ro\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "PPER", "VVFIN", "APPR", "ART", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aber rings nur Wellen tosen.", "tokens": ["A\u00b7ber", "rings", "nur", "Wel\u00b7len", "to\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}