{"dta.poem.843": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "XxX.  Auff den Sontag de\u00df guten Hirten/ oder  Mi-  \n sericordias Domini,  Johan. 10.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Ertzhirt ach! Ich durch dein Blutt vnd Sterben ", "tokens": ["Mein", "Ertz\u00b7hirt", "ach", "!", "Ich", "durch", "dein", "Blutt", "vnd", "Ster\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ITJ", "$.", "PPER", "APPR", "PPOSAT", "ADJD", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Erkaufftes Schaff/ jrr\u2019 ohne trost vmbher", "tokens": ["Er\u00b7kauff\u00b7tes", "Schaff", "/", "jrr'", "oh\u00b7ne", "trost", "vm\u00b7bher"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$(", "ADV", "APPR", "NN", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "In w\u00fc\u00dfter Welt/ ach stehe bey vnd wehr", "tokens": ["In", "w\u00fc\u00df\u00b7ter", "Welt", "/", "ach", "ste\u00b7he", "bey", "vnd", "wehr"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$(", "ADV", "VVFIN", "APPR", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Dem Wolffe der schon renn\u2019t mich zu verderben.", "tokens": ["Dem", "Wolf\u00b7fe", "der", "schon", "renn't", "mich", "zu", "ver\u00b7der\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADV", "VVFIN", "PPER", "PTKZU", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Gib nicht was du hast m\u00fcssen saw\u2019r erwerben", "tokens": ["Gib", "nicht", "was", "du", "hast", "m\u00fcs\u00b7sen", "sa\u00b7w'r", "er\u00b7wer\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PTKNEG", "PWS", "PPER", "VAFIN", "VMFIN", "PPER", "VVINF"], "meter": "-----+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Dem Thier zur beutt? Hilff da\u00df mich nicht verzehr", "tokens": ["Dem", "Thier", "zur", "beutt", "?", "Hilff", "da\u00df", "mich", "nicht", "ver\u00b7zehr"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "ADJD", "$.", "NN", "KOUS", "PPER", "PTKNEG", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der grimme L\u00f6w. Vertreib den H\u00f6llen Beer/", "tokens": ["Der", "grim\u00b7me", "L\u00f6w", ".", "Ver\u00b7treib", "den", "H\u00f6l\u00b7len", "Beer", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "NN", "ART", "NN", "NE", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Vnd la\u00df mich platz in deinem Stall ererben.", "tokens": ["Vnd", "la\u00df", "mich", "platz", "in", "dei\u00b7nem", "Stall", "er\u00b7er\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ich kenne dich! Mein Heiland schrey mir zu!", "tokens": ["Ich", "ken\u00b7ne", "dich", "!", "Mein", "Hei\u00b7land", "schrey", "mir", "zu", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$.", "PPOSAT", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ich folge dir/ du/ du/ bist einig/ du;", "tokens": ["Ich", "fol\u00b7ge", "dir", "/", "du", "/", "du", "/", "bist", "ei\u00b7nig", "/", "du", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$(", "PPER", "$(", "PPER", "$(", "VAFIN", "ADJD", "$(", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der mir kan weyd/ vnd weg/ vnd Ruhe zeigen.", "tokens": ["Der", "mir", "kan", "weyd", "/", "vnd", "weg", "/", "vnd", "Ru\u00b7he", "zei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VMFIN", "NN", "$(", "KON", "PTKVZ", "$(", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Kein Frembder sch\u00fctzt/ kein Miedling h\u00e4lt hier stand", "tokens": ["Kein", "Fremb\u00b7der", "sch\u00fctzt", "/", "kein", "Mied\u00b7ling", "h\u00e4lt", "hier", "stand"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "$(", "PIAT", "NN", "VVFIN", "ADV", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Drumb kom du selbst vnd schreib mich in die Hand", "tokens": ["Drumb", "kom", "du", "selbst", "vnd", "schreib", "mich", "in", "die", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "KON", "VVFIN", "PRF", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "In die du schleust/ was ewig bleibt dein eigen.", "tokens": ["In", "die", "du", "schleust", "/", "was", "e\u00b7wig", "bleibt", "dein", "ei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "VVFIN", "$(", "PWS", "ADJD", "VVFIN", "PPOSAT", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}