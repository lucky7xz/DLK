{"textgrid.poem.47448": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[nichts ist dumpfer Gem\u00fcther] [1]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nichts ist dumpfer Gem\u00fcther,", "tokens": ["Nichts", "ist", "dum\u00b7pfer", "Ge\u00b7m\u00fc\u00b7ther", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Den die Liebe wandeln hei\u00dft,", "tokens": ["Den", "die", "Lie\u00b7be", "wan\u00b7deln", "hei\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVFIN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Gr\u00fc\u00df mit dem sch\u00f6nsten Gru\u00dfe", "tokens": ["Gr\u00fc\u00df", "mit", "dem", "sch\u00f6ns\u00b7ten", "Gru\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Jene Sch\u00f6ne, die du wei\u00dft.", "tokens": ["Je\u00b7ne", "Sch\u00f6\u00b7ne", ",", "die", "du", "wei\u00dft", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Melde, da\u00df mir in die L\u00fcfte", "tokens": ["Mel\u00b7de", ",", "da\u00df", "mir", "in", "die", "L\u00fcf\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "KOUS", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schwinde der gequ\u00e4lte Geist,", "tokens": ["Schwin\u00b7de", "der", "ge\u00b7qu\u00e4l\u00b7te", "Geist", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn sie nicht zu spenden eile", "tokens": ["Wenn", "sie", "nicht", "zu", "spen\u00b7den", "ei\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PTKNEG", "PTKZU", "VVINF", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Jene Spende, die du wei\u00dft.", "tokens": ["Je\u00b7ne", "Spen\u00b7de", ",", "die", "du", "wei\u00dft", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Denn zu kennen und zu missen,", "tokens": ["Denn", "zu", "ken\u00b7nen", "und", "zu", "mis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was mit Edens Wonne speis't,", "tokens": ["Was", "mit", "E\u00b7dens", "Won\u00b7ne", "speis't", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Es gebiert die tiefe Trauer,", "tokens": ["Es", "ge\u00b7biert", "die", "tie\u00b7fe", "Trau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die gef\u00e4hre, die du wei\u00dft.", "tokens": ["Die", "ge\u00b7f\u00e4h\u00b7re", ",", "die", "du", "wei\u00dft", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Ja, was sind die Paradiese,", "tokens": ["Ja", ",", "was", "sind", "die", "Pa\u00b7ra\u00b7die\u00b7se", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PWS", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Welche der Verz\u00fcckte preis't,", "tokens": ["Wel\u00b7che", "der", "Ver\u00b7z\u00fcck\u00b7te", "preis't", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Lacht sie, jene Flur der Liebe,", "tokens": ["Lacht", "sie", ",", "je\u00b7ne", "Flur", "der", "Lie\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PDAT", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Jene zarte, die du wei\u00dft!", "tokens": ["Je\u00b7ne", "zar\u00b7te", ",", "die", "du", "wei\u00dft", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Nicht der Aar zu sein verlangt mich,", "tokens": ["Nicht", "der", "Aar", "zu", "sein", "ver\u00b7langt", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "PTKZU", "VAINF", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der hinauf zur Sonne reis't;", "tokens": ["Der", "hin\u00b7auf", "zur", "Son\u00b7ne", "reis't", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nachtigall Hafis verg\u00