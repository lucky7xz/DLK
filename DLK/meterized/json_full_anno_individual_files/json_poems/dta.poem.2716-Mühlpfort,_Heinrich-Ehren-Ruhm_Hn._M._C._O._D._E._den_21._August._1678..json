{"dta.poem.2716": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Ehren-Ruhm/  \n  Hn.   M. C. O. D. E.   den 21. August.  1678.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ach theurer Seelen Artzt! Ach schallende Posaune!", "tokens": ["Ach", "theu\u00b7rer", "See\u00b7len", "Artzt", "!", "Ach", "schal\u00b7len\u00b7de", "Po\u00b7sau\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "ADJA", "NN", "NN", "$.", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ach mehr als g\u00f6ldner Mund! so ists mit dir geschehn?", "tokens": ["Ach", "mehr", "als", "g\u00f6ld\u00b7ner", "Mund", "!", "so", "ists", "mit", "dir", "ge\u00b7schehn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "KOUS", "ADJA", "NN", "$.", "ADV", "VAFIN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich wei\u00df nicht was ich schreib\u2019/ ich zittre und erstaune", "tokens": ["Ich", "wei\u00df", "nicht", "was", "ich", "schreib'", "/", "ich", "zitt\u00b7re", "und", "er\u00b7stau\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "PWS", "PPER", "VVFIN", "$(", "PPER", "VVFIN", "KON", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nun ich dich Sions Ruhm sol auf der Bahre sehn/", "tokens": ["Nun", "ich", "dich", "Si\u00b7ons", "Ruhm", "sol", "auf", "der", "Bah\u00b7re", "sehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "PRF", "NE", "NN", "VMFIN", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Unsch\u00e4tzbarer Verlust! Wenn solche Lichter sincken/", "tokens": ["Un\u00b7sch\u00e4tz\u00b7ba\u00b7rer", "Ver\u00b7lust", "!", "Wenn", "sol\u00b7che", "Lich\u00b7ter", "sin\u00b7cken", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "KOUS", "PIAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So glaubt/ da\u00df eine Nacht voll Nebel uns erschreckt.", "tokens": ["So", "glaubt", "/", "da\u00df", "ei\u00b7ne", "Nacht", "voll", "Ne\u00b7bel", "uns", "er\u00b7schreckt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$(", "KOUS", "ART", "NN", "ADJD", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wenn diese Ampeln nicht mehr in der Kirchen blincken/", "tokens": ["Wenn", "die\u00b7se", "Am\u00b7peln", "nicht", "mehr", "in", "der", "Kir\u00b7chen", "blin\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "PTKNEG", "ADV", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So denckt da\u00df Finstern\u00fc\u00df uns aus Egypten deckt.", "tokens": ["So", "denckt", "da\u00df", "Fins\u00b7ter\u00b7n\u00fc\u00df", "uns", "aus", "E\u00b7gyp\u00b7ten", "deckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KOUS", "NN", "PPER", "APPR", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wenn jetzt die Morgenr\u00f6th im ersten Purpur kommen/", "tokens": ["Wenn", "jetzt", "die", "Mor\u00b7gen\u00b7r\u00f6th", "im", "ers\u00b7ten", "Pur\u00b7pur", "kom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "APPRART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und sagt den g\u00f6ldnen Tag mit frischen Rosen an;", "tokens": ["Und", "sagt", "den", "g\u00f6ld\u00b7nen", "Tag", "mit", "fri\u00b7schen", "Ro\u00b7sen", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und drauf der Sonnen-Licht im Augenblick verglommen", "tokens": ["Und", "drauf", "der", "Son\u00b7nen\u00b7Licht", "im", "Au\u00b7gen\u00b7blick", "ver\u00b7glom\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PAV", "ART", "NN", "APPRART", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und schwartzer Wolcken-Dampfbezeucht der Erden Plan;", "tokens": ["Und", "schwart\u00b7zer", "Wol\u00b7cken\u00b7Dampf\u00b7be\u00b7zeucht", "der", "Er\u00b7den", "Plan", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "So steht die Welt best\u00fcrtzt. Die leichten Vogel schweigen/", "tokens": ["So", "steht", "die", "Welt", "be\u00b7st\u00fcrtzt", ".", "Die", "leich\u00b7ten", "Vo\u00b7gel", "schwei\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "VVPP", "$.", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Das stumme Wollen-Vieh vergiest sein Laub und ", "tokens": ["Das", "stum\u00b7me", "Wol\u00b7len\u00b7Vieh", "ver\u00b7giest", "sein", "Laub", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "KON"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Ein banges Zittern rauscht in den begr\u00fcnten Zweigen", "tokens": ["Ein", "ban\u00b7ges", "Zit\u00b7tern", "rauscht", "in", "den", "be\u00b7gr\u00fcn\u00b7ten", "Zwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und Flora macht vor L\u011bid die bunten Augen na\u00df.", "tokens": ["Und", "Flo\u00b7ra", "macht", "vor", "L\u011bid", "die", "bun\u00b7ten", "Au\u00b7gen", "na\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "APPR", "NE", "ART", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Nicht anders gehts mit dir/ du Stern gelehrter Geister/", "tokens": ["Nicht", "an\u00b7ders", "gehts", "mit", "dir", "/", "du", "Stern", "ge\u00b7lehr\u00b7ter", "Geis\u00b7ter", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "VVFIN", "APPR", "PPER", "$(", "PPER", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Du Fackel/ die bi\u00dfher zu aller Nutz gebrennt/", "tokens": ["Du", "Fa\u00b7ckel", "/", "die", "bi\u00df\u00b7her", "zu", "al\u00b7ler", "Nutz", "ge\u00b7brennt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$(", "ART", "ADV", "APPR", "PIAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Du Kern der Wissenschafft/ und hoher K\u00fcnste Meister", "tokens": ["Du", "Kern", "der", "Wis\u00b7sen\u00b7schafft", "/", "und", "ho\u00b7her", "K\u00fcns\u00b7te", "Meis\u00b7ter"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "NN", "ART", "NN", "$(", "KON", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Nach dem du dich so fr\u00fch\u2019/ so fr\u00fch! von uns getrennt.", "tokens": ["Nach", "dem", "du", "dich", "so", "fr\u00fch'", "/", "so", "fr\u00fch", "!", "von", "uns", "ge\u00b7trennt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PRF", "ADV", "ADJD", "$(", "ADV", "ADJD", "$.", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Wie girrt nnd seufftzet nicht jetzt deine Kirchen-Heerde!", "tokens": ["Wie", "girrt", "nnd", "seufft\u00b7zet", "nicht", "jetzt", "dei\u00b7ne", "Kir\u00b7chen\u00b7He\u00b7er\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "KON", "VVFIN", "PTKNEG", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+--", "measure": "unknown.measure.hexa"}, "line.22": {"text": "Wie sehnet sie sich nicht nach deinem Himmel-Brod!", "tokens": ["Wie", "seh\u00b7net", "sie", "sich", "nicht", "nach", "dei\u00b7nem", "Him\u00b7mel\u00b7Brod", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PRF", "PTKNEG", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Wie schreckt sie nicht die Post? Jhr Hirt ist Asch und Erde/", "tokens": ["Wie", "schreckt", "sie", "nicht", "die", "Post", "?", "Ihr", "Hirt", "ist", "Asch", "und", "Er\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PTKNEG", "ART", "NN", "$.", "PPOSAT", "NN", "VAFIN", "ADJD", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Der zu dem Leben wie\u00df ist selbsten kalt und tod.", "tokens": ["Der", "zu", "dem", "Le\u00b7ben", "wie\u00df", "ist", "selbs\u00b7ten", "kalt", "und", "tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "VAFIN", "ADV", "ADJD", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "So schallt gemeines Leid. Was aber thun die jenen", "tokens": ["So", "schallt", "ge\u00b7mei\u00b7nes", "Leid", ".", "Was", "a\u00b7ber", "thun", "die", "je\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJA", "NN", "$.", "PWS", "ADV", "VVFIN", "ART", "PDAT"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Die deiner Tugend Ru m und Treffligkeit verstehn?", "tokens": ["Die", "dei\u00b7ner", "Tu\u00b7gend", "Ru", "m", "und", "Tref\u00b7flig\u00b7keit", "ver\u00b7stehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "NE", "NE", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.27": {"text": "Verl\u00e4st du nicht nach dir ein u abl\u00e4ssig Sehnen?", "tokens": ["Ver\u00b7l\u00e4st", "du", "nicht", "nach", "dir", "ein", "u", "ab\u00b7l\u00e4s\u00b7sig", "Seh\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "APPR", "PPER", "ART", "NE", "ADJD", "NN", "$."], "meter": "-+-+-+-++--+-", "measure": "iambic.hexa.relaxed"}, "line.28": {"text": "Und w\u00fcnschen sie mit dir nicht in das Grab zu gehn?", "tokens": ["Und", "w\u00fcn\u00b7schen", "sie", "mit", "dir", "nicht", "in", "das", "Grab", "zu", "gehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "PPER", "PTKNEG", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Ach ja! die Tugend selbst betrauret deine Leiche/", "tokens": ["Ach", "ja", "!", "die", "Tu\u00b7gend", "selbst", "be\u00b7trau\u00b7ret", "dei\u00b7ne", "Lei\u00b7che", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ITJ", "$.", "ART", "NN", "ADV", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Hier steht die Gottesfurcht/ dort Weisheit und Verstand:", "tokens": ["Hier", "steht", "die", "Got\u00b7tes\u00b7furcht", "/", "dort", "Weis\u00b7heit", "und", "Ver\u00b7stand", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$(", "ADV", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Unb Demuth und Gedult die winden Lorber-Str\u00e4uche/", "tokens": ["Unb", "De\u00b7muth", "und", "Ge\u00b7dult", "die", "win\u00b7den", "Lor\u00b7ber\u00b7Str\u00e4u\u00b7che", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ART", "ADJA", "NN", "$("], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.32": {"text": "Worzu Best\u00e4ndigkeit schenckt ihren Diamant.", "tokens": ["Wor\u00b7zu", "Be\u00b7st\u00e4n\u00b7dig\u00b7keit", "schenckt", "ih\u00b7ren", "Di\u00b7a\u00b7mant", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Ich seh umb deinen Sarg die Musen alle schweben", "tokens": ["Ich", "seh", "umb", "dei\u00b7nen", "Sarg", "die", "Mu\u00b7sen", "al\u00b7le", "schwe\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "ART", "NN", "PIAT", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Weil du mit h\u00f6chstem Ruhm ihr F\u00fchrer kontest seyn.", "tokens": ["Weil", "du", "mit", "h\u00f6chs\u00b7tem", "Ruhm", "ihr", "F\u00fch\u00b7rer", "kon\u00b7test", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "PPOSAT", "NN", "VMFIN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Ich h\u00f6re dieses Lob die gantze Stadt dir geben:", "tokens": ["Ich", "h\u00f6\u00b7re", "die\u00b7ses", "Lob", "die", "gant\u00b7ze", "Stadt", "dir", "ge\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDAT", "NN", "ART", "ADJA", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Der Kirche Trost und Zier deckt nun ein Leichen Stein.", "tokens": ["Der", "Kir\u00b7che", "Trost", "und", "Zier", "deckt", "nun", "ein", "Lei\u00b7chen", "Stein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "NN", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Es wird manch feurig Geist der sp\u00e4ten Nachwelt sagen", "tokens": ["Es", "wird", "manch", "feu\u00b7rig", "Geist", "der", "sp\u00e4\u00b7ten", "Nach\u00b7welt", "sa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PIAT", "ADJD", "NN", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Mit was f\u00fcr Eyfer du des H\u00f6chsten Wort gelehrt:", "tokens": ["Mit", "was", "f\u00fcr", "Ey\u00b7fer", "du", "des", "H\u00f6chs\u00b7ten", "Wort", "ge\u00b7lehrt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "APPR", "NN", "PPER", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Der Nachruhm auch dein Lob auf g\u00f6ldnen Schwingen tragen", "tokens": ["Der", "Nach\u00b7ruhm", "auch", "dein", "Lob", "auf", "g\u00f6ld\u00b7nen", "Schwin\u00b7gen", "tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Und S\u00e4ulen graben ein die keine Zeit zerst\u00f6rt.", "tokens": ["Und", "S\u00e4u\u00b7len", "gra\u00b7ben", "ein", "die", "kei\u00b7ne", "Zeit", "zer\u00b7st\u00f6rt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ART", "ART", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Nunmehr/ erlauchte Seel/ entrissen von den Banden/", "token