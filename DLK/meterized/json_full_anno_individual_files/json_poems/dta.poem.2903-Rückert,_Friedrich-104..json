{"dta.poem.2903": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "104.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "O seliges Gef\u00fchl, zu f\u00fchlen da\u00df du lebest,", "tokens": ["O", "se\u00b7li\u00b7ges", "Ge\u00b7f\u00fchl", ",", "zu", "f\u00fch\u00b7len", "da\u00df", "du", "le\u00b7best", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "PTKZU", "VVINF", "KOUS", "PPER", "VVFIN", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Empfangest Leben von der Welt und Leben gebest;", "tokens": ["Emp\u00b7fan\u00b7gest", "Le\u00b7ben", "von", "der", "Welt", "und", "Le\u00b7ben", "ge\u00b7best", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "APPR", "ART", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ein Glied des Leibs zu seyn, der tausendfach sich gliedert,", "tokens": ["Ein", "Glied", "des", "Leibs", "zu", "seyn", ",", "der", "tau\u00b7send\u00b7fach", "sich", "glie\u00b7dert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PTKZU", "VAINF", "$,", "PRELS", "VVFIN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo Herrschen nicht erh\u00f6ht und Dienen nicht erniedert.", "tokens": ["Wo", "Herr\u00b7schen", "nicht", "er\u00b7h\u00f6ht", "und", "Die\u00b7nen", "nicht", "er\u00b7nie\u00b7dert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "PTKNEG", "VVPP", "KON", "PDS", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Denn alles ist Gef\u00e4\u00df, das immer feiner seigert,", "tokens": ["Denn", "al\u00b7les", "ist", "Ge\u00b7f\u00e4\u00df", ",", "das", "im\u00b7mer", "fei\u00b7ner", "sei\u00b7gert", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "NN", "$,", "PRELS", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wodurch sich Nahrungssaft zum Nervengeiste steigert.", "tokens": ["Wo\u00b7durch", "sich", "Nah\u00b7rungs\u00b7saft", "zum", "Ner\u00b7ven\u00b7geis\u00b7te", "stei\u00b7gert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Die Stell', an die du bist gestellt, bestelle du,", "tokens": ["Die", "Stell'", ",", "an", "die", "du", "bist", "ge\u00b7stellt", ",", "be\u00b7stel\u00b7le", "du", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "PRELS", "PPER", "VAFIN", "VVPP", "$,", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "O Werkzeug im Gewerk des Lebens wirke zu!", "tokens": ["O", "Werk\u00b7zeug", "im", "Ge\u00b7werk", "des", "Le\u00b7bens", "wir\u00b7ke", "zu", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPRART", "NN", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Und f\u00fchle, da\u00df du nicht ent\u00e4u\u00dfernd dich verlierst,", "tokens": ["Und", "f\u00fch\u00b7le", ",", "da\u00df", "du", "nicht", "ent\u00b7\u00e4u\u00b7\u00dfernd", "dich", "ver\u00b7lierst", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPER", "PTKNEG", "VVPP", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df du die Welt aus dir, dich aus der Welt gebierst.", "tokens": ["Da\u00df", "du", "die", "Welt", "aus", "dir", ",", "dich", "aus", "der", "Welt", "ge\u00b7bierst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "PPER", "$,", "PRF", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Du ziehest sie in dich, um sie dir anzugleichen,", "tokens": ["Du", "zie\u00b7hest", "sie", "in", "dich", ",", "um", "sie", "dir", "an\u00b7zu\u00b7glei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PPER", "$,", "KOUI", "PPER", "PPER", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und gehst in sie um aufzudr\u00fccken ihr dein Zeichen.", "tokens": ["Und", "gehst", "in", "sie", "um", "auf\u00b7zu\u00b7dr\u00fc\u00b7cken", "ihr", "dein", "Zei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "KOUI", "VVIZU", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Ein P\u00fcnktchen und zugleich ein Mittelpunkt, ein Ich;", "tokens": ["Ein", "P\u00fcnkt\u00b7chen", "und", "zu\u00b7gleich", "ein", "Mit\u00b7tel\u00b7punkt", ",", "ein", "Ich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ADV", "ART", "NN", "$,", "ART", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So unterordne dir und unterordne dich!", "tokens": ["So", "un\u00b7ter\u00b7ord\u00b7ne", "dir", "und", "un\u00b7ter\u00b7ord\u00b7ne", "dich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KON", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}