{"dta.poem.8647": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "An meine Guitarre.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1832", "urn": "urn:nbn:de:kobv:b4-200905193572", "language": ["de:0.85", "sv:0.14"], "booktitle": "Lenau, Nikolaus: Gedichte. Stuttgart, 1832."}, "poem": {"stanza.1": {"line.1": {"text": "Guitarre, wie du h\u00e4ngst so traurig!", "tokens": ["Gui\u00b7tar\u00b7re", ",", "wie", "du", "h\u00e4ngst", "so", "trau\u00b7rig", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "PPER", "VVFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Saiten t\u00f6nen nimmermehr,", "tokens": ["Die", "Sai\u00b7ten", "t\u00f6\u00b7nen", "nim\u00b7mer\u00b7mehr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die l\u00e4ngst zerrissnen wanken schaurig", "tokens": ["Die", "l\u00e4ngst", "zer\u00b7riss\u00b7nen", "wan\u00b7ken", "schau\u00b7rig"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "VVINF", "VVFIN", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Im Abendwinde hin und her.", "tokens": ["Im", "Ab\u00b7end\u00b7win\u00b7de", "hin", "und", "her", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Auch deine Saiten sind zerrissen,", "tokens": ["Auch", "dei\u00b7ne", "Sai\u00b7ten", "sind", "zer\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Es schweigt dein s\u00fc\u00dfer Liederklang,", "tokens": ["Es", "schweigt", "dein", "s\u00fc\u00b7\u00dfer", "Lie\u00b7der\u00b7klang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Seit in des Busens Finsternissen", "tokens": ["Seit", "in", "des", "Bu\u00b7sens", "Fins\u00b7ter\u00b7nis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mir jede frohe Saite sprang.", "tokens": ["Mir", "je\u00b7de", "fro\u00b7he", "Sai\u00b7te", "sprang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Mir sank der Freund voll Jugendbl\u00fcthe", "tokens": ["Mir", "sank", "der", "Freund", "voll", "Ju\u00b7gend\u00b7bl\u00fc\u00b7the"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Hinunter in die Todesfluth;", "tokens": ["Hin\u00b7un\u00b7ter", "in", "die", "To\u00b7des\u00b7fluth", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die meiner Lieb' entgegengl\u00fchte,", "tokens": ["Die", "mei\u00b7ner", "Lieb'", "ent\u00b7ge\u00b7gen\u00b7gl\u00fch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nun bei den kalten Todten ruht.", "tokens": ["Nun", "bei", "den", "kal\u00b7ten", "Tod\u00b7ten", "ruht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Doch will ich euch nun frisch besaiten,", "tokens": ["Doch", "will", "ich", "euch", "nun", "frisch", "be\u00b7sai\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PPER", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dich, meine Leier, dich, mein Herz!", "tokens": ["Dich", ",", "mei\u00b7ne", "Lei\u00b7er", ",", "dich", ",", "mein", "Herz", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "PPOSAT", "NN", "$,", "PPER", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "R\u00fcckbannen die entflohnen Zeiten,", "tokens": ["R\u00fcck\u00b7ban\u00b7nen", "die", "ent\u00b7floh\u00b7nen", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die alte Lust, den alten Schmerz!", "tokens": ["Die", "al\u00b7te", "Lust", ",", "den", "al\u00b7ten", "Schmerz", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Hinaus in's Dunkel jener Eichen!", "tokens": ["Hin\u00b7aus", "in's", "Dun\u00b7kel", "je\u00b7ner", "Ei\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "PDAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dort findet sich der alte Lauf;", "tokens": ["Dort", "fin\u00b7det", "sich", "der", "al\u00b7te", "Lauf", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dort st\u00f6ren wir die Liederleichen", "tokens": ["Dort", "st\u00f6\u00b7ren", "wir", "die", "Lie\u00b7der\u00b7lei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Aus ihren stillen Gr\u00e4bern auf!", "tokens": ["Aus", "ih\u00b7ren", "stil\u00b7len", "Gr\u00e4\u00b7bern", "auf", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Wenn erst die Lieder nur erwachen,", "tokens": ["Wenn", "erst", "die", "Lie\u00b7der", "nur", "er\u00b7wa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dann ruft, dann zieht ihr lauter Chor", "tokens": ["Dann", "ruft", ",", "dann", "zieht", "ihr", "lau\u00b7ter", "Chor"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Lieben all' in meinen Nachen", "tokens": ["Die", "Lie\u00b7ben", "all'", "in", "mei\u00b7nen", "Na\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "PIS", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Aus dunkler Todesfluth empor.", "tokens": ["Aus", "dunk\u00b7ler", "To\u00b7des\u00b7fluth", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Es klingt! \u2014 doch flieh'n im scheuen Fluge", "tokens": ["Es", "klingt", "!", "doch", "flieh'n", "im", "scheu\u00b7en", "Flu\u00b7ge"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "$(", "ADV", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die T\u00f6ne auf von meiner Hand;", "tokens": ["Die", "T\u00f6\u00b7ne", "auf", "von", "mei\u00b7ner", "Hand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So eilt, versp\u00e4tet, nach dem Zuge", "tokens": ["So", "eilt", ",", "ver\u00b7sp\u00e4\u00b7tet", ",", "nach", "dem", "Zu\u00b7ge"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "VVPP", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das V\u00f6glein \u00fcber's Heideland.", "tokens": ["Das", "V\u00f6\u00b7glein", "\u00fc\u00b7ber's", "Hei\u00b7de\u00b7land", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Nun bin ich meines Herzens Meister!", "tokens": ["Nun", "bin", "ich", "mei\u00b7nes", "Her\u00b7zens", "Meis\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nun rauscht wie einst der Sturmakkord,", "tokens": ["Nun", "rauscht", "wie", "einst", "der", "Stur\u00b7mak\u00b7kord", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KOKOM", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nun springen die versunknen Geister", "tokens": ["Nun", "sprin\u00b7gen", "die", "ver\u00b7sun\u00b7knen", "Geis\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Herauf, herauf an meinen Bord!", "tokens": ["Her\u00b7auf", ",", "her\u00b7auf", "an", "mei\u00b7nen", "Bord", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKVZ", "$,", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "O du mein Freund, so treu und bieder!", "tokens": ["O", "du", "mein", "Freund", ",", "so", "treu", "und", "bie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "PPOSAT", "NN", "$,", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wohl mir, du bist mir wieder nah!", "tokens": ["Wohl", "mir", ",", "du", "bist", "mir", "wie\u00b7der", "nah", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "$,", "PPER", "VAFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dein s\u00fc\u00dfes Wort auch h\u00f6r' ich wieder!", "tokens": ["Dein", "s\u00fc\u00b7\u00dfes", "Wort", "auch", "h\u00f6r'", "ich", "wie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADV", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mein holdes M\u00e4dchen, bist du da? \u2014", "tokens": ["Mein", "hol\u00b7des", "M\u00e4d\u00b7chen", ",", "bist", "du", "da", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "VAFIN", "PPER", "ADV", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Doch nein! mich h\u00f6hnten finstre M\u00e4chte!", "tokens": ["Doch", "nein", "!", "mich", "h\u00f6hn\u00b7ten", "finst\u00b7re", "M\u00e4ch\u00b7te", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKANT", "$.", "PPER", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wo ist der Freund? das blonde Kind?", "tokens": ["Wo", "ist", "der", "Freund", "?", "das", "blon\u00b7de", "Kind", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "$.", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Nebel reicht mir keine Rechte;", "tokens": ["Der", "Ne\u00b7bel", "reicht", "mir", "kei\u00b7ne", "Rech\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Durch blonde Disteln saust der Wind!", "tokens": ["Durch", "blon\u00b7de", "Dis\u00b7teln", "saust", "der", "Wind", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}