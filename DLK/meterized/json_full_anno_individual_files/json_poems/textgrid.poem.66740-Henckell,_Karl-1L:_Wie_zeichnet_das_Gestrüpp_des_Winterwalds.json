{"textgrid.poem.66740": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie zeichnet das Gestr\u00fcpp des Winterwalds", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie zeichnet das Gestr\u00fcpp des Winterwalds", "tokens": ["Wie", "zeich\u00b7net", "das", "Ge\u00b7str\u00fcpp", "des", "Win\u00b7ter\u00b7walds"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Im zarten Reif sich feinver\u00e4stelt aus", "tokens": ["Im", "zar\u00b7ten", "Reif", "sich", "fein\u00b7ver\u00b7\u00e4s\u00b7telt", "aus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "PRF", "ADJD", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und l\u00e4\u00dft mich durch den Mittag Wandelnden", "tokens": ["Und", "l\u00e4\u00dft", "mich", "durch", "den", "Mit\u00b7tag", "Wan\u00b7deln\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Verrankter Zweige Zierat klar erschaun!", "tokens": ["Ver\u00b7rank\u00b7ter", "Zwei\u00b7ge", "Zie\u00b7rat", "klar", "er\u00b7schaun", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Es ist nicht Wirrnis mehr, es ist wie Bild", "tokens": ["Es", "ist", "nicht", "Wirr\u00b7nis", "mehr", ",", "es", "ist", "wie", "Bild"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PTKNEG", "NN", "ADV", "$,", "PPER", "VAFIN", "KOKOM", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der Wirrnis hinter wunderbarem Glas,", "tokens": ["Der", "Wirr\u00b7nis", "hin\u00b7ter", "wun\u00b7der\u00b7ba\u00b7rem", "Glas", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das diese Welt im Urstand l\u00e4\u00dft und doch", "tokens": ["Das", "die\u00b7se", "Welt", "im", "Ur\u00b7stand", "l\u00e4\u00dft", "und", "doch"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "PDAT", "NN", "APPRART", "NN", "VVFIN", "KON", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Zur lichten Schau das Wildverschlungne schlichtet.", "tokens": ["Zur", "lich\u00b7ten", "Schau", "das", "Wild\u00b7ver\u00b7schlung\u00b7ne", "schlich\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Mit meinem festen alten Wanderstab", "tokens": ["Mit", "mei\u00b7nem", "fes\u00b7ten", "al\u00b7ten", "Wan\u00b7der\u00b7stab"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ber\u00fchr' ich s\u00e4umend einen wei\u00dfen Ast,", "tokens": ["Be\u00b7r\u00fchr'", "ich", "s\u00e4u\u00b7mend", "ei\u00b7nen", "wei\u00b7\u00dfen", "Ast", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und wie ich st\u00e4ubend seine Reiflast streife,", "tokens": ["Und", "wie", "ich", "st\u00e4u\u00b7bend", "sei\u00b7ne", "Reif\u00b7last", "strei\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADJD", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Da\u00df puderleicht sich die Kristalle sondern", "tokens": ["Da\u00df", "pu\u00b7der\u00b7leicht", "sich", "die", "Kris\u00b7tal\u00b7le", "son\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "PRF", "ART", "NN", "KON"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Und niederflocken aufs gefrorne Moos,", "tokens": ["Und", "nie\u00b7der\u00b7flo\u00b7cken", "aufs", "ge\u00b7fror\u00b7ne", "Moos", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "F\u00e4llt blitzend ein verst\u00e4rkter Sonnenstrahl", "tokens": ["F\u00e4llt", "blit\u00b7zend", "ein", "ver\u00b7st\u00e4rk\u00b7ter", "Son\u00b7nen\u00b7strahl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Auf allen Wald, und zauberschimmernd bl\u00fchn", "tokens": ["Auf", "al\u00b7len", "Wald", ",", "und", "zau\u00b7ber\u00b7schim\u00b7mernd", "bl\u00fchn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "$,", "KON", "ADJD", "VVINF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Vor meinem Blick gel\u00f6ste Labyrinthe ...", "tokens": ["Vor", "mei\u00b7nem", "Blick", "ge\u00b7l\u00f6s\u00b7te", "La\u00b7by\u00b7rin\u00b7the", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Pl\u00f6tzlich, wie traumgespiegelt, schau ich tief,", "tokens": ["Pl\u00f6tz\u00b7lich", ",", "wie", "traum\u00b7ge\u00b7spie\u00b7gelt", ",", "schau", "ich", "tief", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWAV", "VVFIN", "$,", "VVFIN", "PPER", "ADJD", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Doch nah, vom Wasserfall, der sch\u00e4umend st\u00fcrzt,", "tokens": ["Doch", "nah", ",", "vom", "Was\u00b7ser\u00b7fall", ",", "der", "sch\u00e4u\u00b7mend", "st\u00fcrzt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "APPRART", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da dr\u00fcben eines J\u00fcnglings j\u00e4hen Schritt", "tokens": ["Da", "dr\u00fc\u00b7ben", "ei\u00b7nes", "J\u00fcng\u00b7lings", "j\u00e4\u00b7hen", "Schritt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Durchs Dickicht st\u00fcrmisch suchen seinen Pfad.", "tokens": ["Durchs", "Di\u00b7ckicht", "st\u00fcr\u00b7misch", "su\u00b7chen", "sei\u00b7nen", "Pfad", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "In seinen Augen gl\u00fcht ein hoher Wahn,", "tokens": ["In", "sei\u00b7nen", "Au\u00b7gen", "gl\u00fcht", "ein", "ho\u00b7her", "Wahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Der Sehnsucht wilder Brand in seinem Blute", "tokens": ["Der", "Sehn\u00b7sucht", "wil\u00b7der", "Brand", "in", "sei\u00b7nem", "Blu\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Malt sich im ungest\u00fcmen Muskelspiel,", "tokens": ["Malt", "sich", "im", "un\u00b7ge\u00b7st\u00fc\u00b7men", "Mus\u00b7kel\u00b7spiel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Das gierig, grausam unbefriedigt zuckt.", "tokens": ["Das", "gie\u00b7rig", ",", "grau\u00b7sam", "un\u00b7be\u00b7frie\u00b7digt", "zuckt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "$,", "ADJD", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Der kecke Waghals wei\u00df nicht ein noch aus", "tokens": ["Der", "ke\u00b7cke", "Wag\u00b7hals", "wei\u00df", "nicht", "ein", "noch", "aus"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKNEG", "ART", "ADV", "APPR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mit seinem Willen, seinem Wege mehr,", "tokens": ["Mit", "sei\u00b7nem", "Wil\u00b7len", ",", "sei\u00b7nem", "We\u00b7ge", "mehr", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das Dickicht schlie\u00dft ihn undurchdringlich ein,", "tokens": ["Das", "Di\u00b7ckicht", "schlie\u00dft", "ihn", "un\u00b7durch\u00b7dring\u00b7lich", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Umklammert ist er von Gespensterarmen", "tokens": ["Um\u00b7klam\u00b7mert", "ist", "er", "von", "Ge\u00b7spens\u00b7ter\u00b7ar\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Verworrner \u00c4ste, die ihn niederziehn,", "tokens": ["Ver\u00b7worr\u00b7ner", "\u00c4s\u00b7te", ",", "die", "ihn", "nie\u00b7der\u00b7ziehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PRELS", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und wie er blindlings rast, sich zu befrein,", "tokens": ["Und", "wie", "er", "blind\u00b7lings", "rast", ",", "sich", "zu", "be\u00b7fr\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADV", "VVFIN", "$,", "PRF", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Ihn straucheln lassen h\u00f6hnisch, fratzenhaft,", "tokens": ["Ihn", "strau\u00b7cheln", "las\u00b7sen", "h\u00f6h\u00b7nisch", ",", "frat\u00b7zen\u00b7haft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVINF", "VVINF", "ADJD", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "In Sumpf und Schlick, den er vor Kr\u00e4ftetaumel", "tokens": ["In", "Sumpf", "und", "Schlick", ",", "den", "er", "vor", "Kr\u00e4f\u00b7te\u00b7tau\u00b7mel"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "$,", "PRELS", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Und tollem Durchbruchsfieber nicht gesehn,", "tokens": ["Und", "tol\u00b7lem", "Durch\u00b7bruchs\u00b7fie\u00b7ber", "nicht", "ge\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "In Schlick und Schlamm und trichtertiefen Tod", "tokens": ["In", "Schlick", "und", "Schlamm", "und", "trich\u00b7ter\u00b7tie\u00b7fen", "Tod"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "KON", "VVFIN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Des g\u00e4hnenden, erstickenden Morastes ...", "tokens": ["Des", "g\u00e4h\u00b7nen\u00b7den", ",", "er\u00b7sti\u00b7cken\u00b7den", "Mo\u00b7ras\u00b7tes", "..."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Ein Schleier h\u00fcllt den Gleitenden. \u2013 Verwischt,", "tokens": ["Ein", "Schlei\u00b7er", "h\u00fcllt", "den", "Glei\u00b7ten\u00b7den", ".", "\u2013", "Ver\u00b7wischt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$.", "$(", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Verhaucht des Schemens wirrer Schicksalsweg.", "tokens": ["Ver\u00b7haucht", "des", "Sche\u00b7mens", "wir\u00b7rer", "Schick\u00b7sals\u00b7weg", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Rings zeichnet das Gestr\u00fcpp des Winterwalds", "tokens": ["Rings", "zeich\u00b7net", "das", "Ge\u00b7str\u00fcpp", "des", "Win\u00b7ter\u00b7walds"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN