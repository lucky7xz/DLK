{"dta.poem.10534": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Donna Clara.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "In dem abendlichen Garten", "tokens": ["In", "dem", "a\u00b7bend\u00b7li\u00b7chen", "Gar\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+---+-", "measure": "unknown.measure.tri"}, "line.2": {"text": "Wandelt des Alkaden Tochter;", "tokens": ["Wan\u00b7delt", "des", "Al\u00b7ka\u00b7den", "Toch\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Pauken- und Trommetenjubel", "tokens": ["Pau\u00b7ken", "und", "Trom\u00b7me\u00b7ten\u00b7ju\u00b7bel"], "token_info": ["word", "word", "word"], "pos": ["TRUNC", "KON", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Klingt herunter von dem Schlosse.", "tokens": ["Klingt", "her\u00b7un\u00b7ter", "von", "dem", "Schlos\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "\u201el\u00e4stig werden mir die T\u00e4nze", "tokens": ["\u201e", "l\u00e4s\u00b7tig", "wer\u00b7den", "mir", "die", "T\u00e4n\u00b7ze"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "ADJD", "VAFIN", "PPER", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und die s\u00fc\u00dfen Schmeichelworte,", "tokens": ["Und", "die", "s\u00fc\u00b7\u00dfen", "Schmei\u00b7chel\u00b7wor\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die Ritter, die so zierlich", "tokens": ["Und", "die", "Rit\u00b7ter", ",", "die", "so", "zier\u00b7lich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "ADV", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mich vergleichen mit der Sonne.", "tokens": ["Mich", "ver\u00b7glei\u00b7chen", "mit", "der", "Son\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "\u201eueberl\u00e4stig wird mir Alles,", "tokens": ["\u201e", "ue\u00b7ber\u00b7l\u00e4s\u00b7tig", "wird", "mir", "Al\u00b7les", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "VAFIN", "PPER", "PIS", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Seit ich sah, bei'm Strahl des Mondes,", "tokens": ["Seit", "ich", "sah", ",", "bei'm", "Strahl", "des", "Mon\u00b7des", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Jenen Ritter, dessen Laute", "tokens": ["Je\u00b7nen", "Rit\u00b7ter", ",", "des\u00b7sen", "Lau\u00b7te"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PDAT", "NN", "$,", "PRELAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "N\u00e4chtens mich an's Fenster lockte.", "tokens": ["N\u00e4ch\u00b7tens", "mich", "an's", "Fens\u00b7ter", "lock\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "\u201ewie er stand so schlank und muthig,", "tokens": ["\u201e", "wie", "er", "stand", "so", "schlank", "und", "mut\u00b7hig", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "PPER", "VVFIN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und die Augen leuchtend schossen", "tokens": ["Und", "die", "Au\u00b7gen", "leuch\u00b7tend", "schos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVPP", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Aus dem edelblassen Antlitz,", "tokens": ["Aus", "dem", "e\u00b7del\u00b7blas\u00b7sen", "Ant\u00b7litz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Glich er wahrlich Sanct Georgen.\u201c", "tokens": ["Glich", "er", "wahr\u00b7lich", "Sanct", "Ge\u00b7or\u00b7gen", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "PPER", "ADV", "VVFIN", "NE", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Also dachte Donna Clara,", "tokens": ["Al\u00b7so", "dach\u00b7te", "Don\u00b7na", "Cla\u00b7ra", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und sie schaute auf den Boden;", "tokens": ["Und", "sie", "schau\u00b7te", "auf", "den", "Bo\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie sie aufblickt, steht der sch\u00f6ne,", "tokens": ["Wie", "sie", "auf\u00b7blickt", ",", "steht", "der", "sch\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVPP", "$,", "VVFIN", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Unbekannte Ritter vor ihr.", "tokens": ["Un\u00b7be\u00b7kann\u00b7te", "Rit\u00b7ter", "vor", "ihr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "PPOSAT", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "H\u00e4ndedr\u00fcckend, liebefl\u00fcsternd,", "tokens": ["H\u00e4n\u00b7de\u00b7dr\u00fc\u00b7ckend", ",", "lie\u00b7be\u00b7fl\u00fcs\u00b7ternd", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["VVPP", "$,", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wandeln sie umher im Mondschein,", "tokens": ["Wan\u00b7deln", "sie", "um\u00b7her", "im", "Mond\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PTKVZ", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der Zephyr schmeichelt freundlich,", "tokens": ["Und", "der", "Ze\u00b7phyr", "schmei\u00b7chelt", "freund\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "M\u00e4hrchenartig gr\u00fc\u00dfen Rosen.", "tokens": ["M\u00e4hr\u00b7chen\u00b7ar\u00b7tig", "gr\u00fc\u00b7\u00dfen", "Ro\u00b7sen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "M\u00e4hrchenartig gr\u00fc\u00dfen Rosen,", "tokens": ["M\u00e4hr\u00b7chen\u00b7ar\u00b7tig", "gr\u00fc\u00b7\u00dfen", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und sie gl\u00fch'n wie Liebesboten.", "tokens": ["Und", "sie", "gl\u00fch'n", "wie", "Lie\u00b7bes\u00b7bo\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "KOKOM", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber sage mir, Geliebte,", "tokens": ["A\u00b7ber", "sa\u00b7ge", "mir", ",", "Ge\u00b7lieb\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Warum du so pl\u00f6tzlich roth wirst?", "tokens": ["Wa\u00b7rum", "du", "so", "pl\u00f6tz\u00b7lich", "roth", "wirst", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADJD", "ADJD", "VAFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.8": {"line.1": {"text": "\u201em\u00fccken stachen mich, Geliebter,", "tokens": ["\u201e", "m\u00fc\u00b7cken", "sta\u00b7chen", "mich", ",", "Ge\u00b7lieb\u00b7ter", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "ADJD", "VVFIN", "PPER", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und die M\u00fccken sind, im Sommer,", "tokens": ["Und", "die", "M\u00fc\u00b7cken", "sind", ",", "im", "Som\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "$,", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Mir so tief verha\u00dft, als w\u00e4ren's", "tokens": ["Mir", "so", "tief", "ver\u00b7ha\u00dft", ",", "als", "w\u00e4\u00b7ren's"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "ADV", "ADJD", "VVPP", "$,", "KOUS", "NE"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Langenas'ge Judenrotten.\u201c", "tokens": ["Lan\u00b7ge\u00b7nas'\u00b7ge", "Ju\u00b7den\u00b7rot\u00b7ten", ".", "\u201c"], "token_info": ["word", "word", "punct", "punct"], "pos": ["ADJA", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "La\u00df die M\u00fccken und die Juden,", "tokens": ["La\u00df", "die", "M\u00fc\u00b7cken", "und", "die", "Ju\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Spricht der Ritter, freundlich kosend.", "tokens": ["Spricht", "der", "Rit\u00b7ter", ",", "freund\u00b7lich", "ko\u00b7send", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Von den Mandelb\u00e4umen fallen", "tokens": ["Von", "den", "Man\u00b7del\u00b7b\u00e4u\u00b7men", "fal\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Tausend wei\u00dfe Bl\u00fcthenflocken.", "tokens": ["Tau\u00b7send", "wei\u00b7\u00dfe", "Bl\u00fc\u00b7then\u00b7flo\u00b7cken", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Tausend wei\u00dfe Bl\u00fcthenflocken", "tokens": ["Tau\u00b7send", "wei\u00b7\u00dfe", "Bl\u00fc\u00b7then\u00b7flo\u00b7cken"], "token_info": ["word", "word", "word"], "pos": ["CARD", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Haben ihren Duft ergossen.", "tokens": ["Ha\u00b7ben", "ih\u00b7ren", "Duft", "er\u00b7gos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber sage mir, Geliebte,", "tokens": ["A\u00b7ber", "sa\u00b7ge", "mir", ",", "Ge\u00b7lieb\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist dein Herz mir ganz gewogen?", "tokens": ["Ist", "dein", "Herz", "mir", "ganz", "ge\u00b7wo\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "\u201eja, ich liebe dich, Geliebter,", "tokens": ["\u201e", "ja", ",", "ich", "lie\u00b7be", "dich", ",", "Ge\u00b7lieb\u00b7ter", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "PTKANT", "$,", "PPER", "VVFIN", "PPER", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bei dem Heiland sey's geschworen,", "tokens": ["Bei", "dem", "Hei\u00b7land", "sey's", "ge\u00b7schwo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Den die gottverfluchten Juden", "tokens": ["Den", "die", "gott\u00b7ver\u00b7fluch\u00b7ten", "Ju\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Boshaft t\u00fcckisch einst ermordet.\u201c", "tokens": ["Bos\u00b7haft", "t\u00fc\u00b7ckisch", "einst", "er\u00b7mor\u00b7det", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "ADJD", "ADV", "VVPP", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "La\u00df den Heiland und die Juden,", "tokens": ["La\u00df", "den", "Hei\u00b7land", "und", "die", "Ju\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Spricht der Ritter, freundlich kosend.", "tokens": ["Spricht", "der", "Rit\u00b7ter", ",", "freund\u00b7lich", "ko\u00b7send", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "In der Ferne schwanken traumhaft", "tokens": ["In", "der", "Fer\u00b7ne", "schwan\u00b7ken", "traum\u00b7haft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wei\u00dfe Liljen, lichtumflossen.", "tokens": ["Wei\u00b7\u00dfe", "Lil\u00b7jen", ",", "lich\u00b7tum\u00b7flos\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Wei\u00dfe Liljen, lichtumflossen,", "tokens": ["Wei\u00b7\u00dfe", "Lil\u00b7jen", ",", "lich\u00b7tum\u00b7flos\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Blicken nach den Sternen droben.", "tokens": ["Bli\u00b7cken", "nach", "den", "Ster\u00b7nen", "dro\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber sage mir, Geliebte,", "tokens": ["A\u00b7ber", "sa\u00b7ge", "mir", ",", "Ge\u00b7lieb\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hast du auch nicht falsch geschworen.", "tokens": ["Hast", "du", "auch", "nicht", "falsch", "ge\u00b7schwo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "PTKNEG", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "\u201efalsch ist nicht in mir, Geliebter,", "tokens": ["\u201e", "falsch", "ist", "nicht", "in", "mir", ",", "Ge\u00b7lieb\u00b7ter", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "ADJD", "VAFIN", "PTKNEG", "APPR", "PPER", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie in meiner Brust kein Tropfen", "tokens": ["Wie", "in", "mei\u00b7ner", "Brust", "kein", "Trop\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "PPOSAT", "NN", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Blut ist von dem Blut der Mohren", "tokens": ["Blut", "ist", "von", "dem", "Blut", "der", "Moh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "APPR", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und des schmutz'gen Judenvolkes.\u201c", "tokens": ["Und", "des", "schmutz'\u00b7gen", "Ju\u00b7den\u00b7vol\u00b7kes", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {