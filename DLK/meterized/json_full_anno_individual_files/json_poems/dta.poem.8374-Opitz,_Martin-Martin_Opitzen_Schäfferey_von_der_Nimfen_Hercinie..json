{"dta.poem.8374": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "Martin Opitzen Sch\u00e4fferey  \n von der Nimfen Hercinie.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1630", "urn": "urn:nbn:de:kobv:b4-200905197863", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Sch\u00e4fferey Von der Nimfen Hercinie. Breslau, 1630."}, "poem": {"stanza.1": {"line.1": {"text": "Ich werde recht von dir mein werther stamm geehret/", "tokens": ["Ich", "wer\u00b7de", "recht", "von", "dir", "mein", "wert\u00b7her", "stamm", "ge\u00b7eh\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "PPER", "PPOSAT", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil ich dir namen/ rhum vndt wapen hoch vermehret;", "tokens": ["Weil", "ich", "dir", "na\u00b7men", "/", "rhum", "vndt", "wa\u00b7pen", "hoch", "ver\u00b7meh\u00b7ret", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "NN", "$(", "NN", "KON", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die roten striche hatt kein geldt noch gunst erdacht/", "tokens": ["Die", "ro\u00b7ten", "stri\u00b7che", "hatt", "kein", "geldt", "noch", "gunst", "er\u00b7dacht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "VAFIN", "PIAT", "NN", "ADV", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Keyser hatt sie nur gelobt/ der feindt gemacht.", "tokens": ["Der", "Key\u00b7ser", "hatt", "sie", "nur", "ge\u00b7lobt", "/", "der", "feindt", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punc