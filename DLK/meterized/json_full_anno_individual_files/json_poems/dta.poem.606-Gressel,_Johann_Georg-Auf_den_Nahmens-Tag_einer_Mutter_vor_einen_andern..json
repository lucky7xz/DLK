{"dta.poem.606": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Auf den Nahmens-Tag einer Mutter/  \n vor einen andern.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Es ist mein Wohl mit ihr verkn\u00fcpfft", "tokens": ["Es", "ist", "mein", "Wohl", "mit", "ihr", "ver\u00b7kn\u00fcpfft"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn sie der W\u00fcnsche wird geniessen/", "tokens": ["Wenn", "sie", "der", "W\u00fcn\u00b7sche", "wird", "ge\u00b7nies\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wodurch mein Hertz vor Freuden h\u00fcpfft/", "tokens": ["Wo\u00b7durch", "mein", "Hertz", "vor", "Freu\u00b7den", "h\u00fcpfft", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und sie auf dieses Blat l\u00e4st fliessen/", "tokens": ["Und", "sie", "auf", "die\u00b7ses", "Blat", "l\u00e4st", "flies\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "PDAT", "NN", "VVFIN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Als Zinsen meiner Lieb und Treu/", "tokens": ["Als", "Zin\u00b7sen", "mei\u00b7ner", "Lieb", "und", "Treu", "/"], "token_info": ["word", "word", "word", 