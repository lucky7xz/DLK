{"dta.poem.10589": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "Elegie.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1624", "urn": "urn:nbn:de:kobv:b4-200905197859", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Teutsche P\u00f6emata und: Aristarchvs Wieder die verachtung Teutscher Sprach. Stra\u00dfburg, 1624."}, "poem": {"stanza.1": {"line.1": {"text": "Der helle Vesper Stern gieng auff kaum vor sechs stunden/", "tokens": ["Der", "hel\u00b7le", "Ves\u00b7per", "Stern", "gieng", "auff", "kaum", "vor", "sechs", "stun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VVFIN", "APPR", "ADV", "APPR", "CARD", "ADJA", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Jetzt hat sich Mitternacht in seinen Orth gefunden/", "tokens": ["Jetzt", "hat", "sich", "Mit\u00b7ter\u00b7nacht", "in", "sei\u00b7nen", "Orth", "ge\u00b7fun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PRF", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Vnd in sechs Stunden kompt die klare Morgenr\u00f6th", "tokens": ["Vnd", "in", "sechs", "Stun\u00b7den", "kompt", "die", "kla\u00b7re", "Mor\u00b7gen\u00b7r\u00f6th"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "CARD", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So lang hernach die Sonn am allerh\u00f6chsten steht/", "tokens": ["So", "lang", "her\u00b7nach", "die", "Sonn", "am", "al\u00b7ler\u00b7h\u00f6chs\u00b7ten", "steht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "ART", "NN", "APPRART", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wie lang ist es jetzt wol da\u00df in des Herbstes Tagen/", "tokens": ["Wie", "lang", "ist", "es", "jetzt", "wol", "da\u00df", "in", "des", "Herbs\u00b7tes", "Ta\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPER", "ADV", "ADV", "KOUS", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Viel Aepffel vnd sch\u00f6n Obs bey jhren B\u00e4umen lagen?", "tokens": ["Viel", "A\u00b7e\u00b7pf\u00b7fel", "vnd", "sch\u00f6n", "Obs", "bey", "jhren", "B\u00e4u\u00b7men", "la\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "ADJD", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+----+-+-+-+-", "measure": "dactylic.init"}, "line.7": {"text": "Heunt hat die kalte Lufft des Winters jhr wei\u00df Kleydt", "tokens": ["Heunt", "hat", "die", "kal\u00b7te", "Lufft", "des", "Win\u00b7ters", "jhr", "wei\u00df", "Kleydt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "ART", "NN", "PPER", "VVFIN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Mit frost vnd scharffem Reiff gestrewet weit vnd breit.", "tokens": ["Mit", "frost", "vnd", "scharf\u00b7fem", "Reiff", "ge\u00b7stre\u00b7wet", "weit", "vnd", "breit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "KON", "ADJA", "NN", "VVFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Hernach soll widerumb mit sch\u00f6nem Tireliren", "tokens": ["Her\u00b7nach", "soll", "wi\u00b7de\u00b7rumb", "mit", "sch\u00f6\u00b7nem", "Ti\u00b7re\u00b7li\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Der V\u00f6gel Compani durch Wald vnd Feld spatzieren/", "tokens": ["Der", "V\u00f6\u00b7gel", "Com\u00b7pa\u00b7ni", "durch", "Wald", "vnd", "Feld", "spat\u00b7zie\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "APPR", "NN", "KON", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "So bald der sch\u00f6ne Lentz wirdt Blumen ohne zahl", "tokens": ["So", "bald", "der", "sch\u00f6\u00b7ne", "Lentz", "wirdt", "Blu\u00b7men", "oh\u00b7ne", "zahl"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "VAFIN", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Durch seinen Westen Wind au\u00dfs\u00e4en vberal.", "tokens": ["Durch", "sei\u00b7nen", "Wes\u00b7ten", "Wind", "au\u00df\u00b7s\u00e4\u00b7en", "vbe\u00b7ral", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "VVIZU", "ADJD", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.13": {"text": "Ach wie verg\u00e4nglich ist doch aller Menschen dichten!", "tokens": ["Ach", "wie", "ver\u00b7g\u00e4ng\u00b7lich", "ist", "doch", "al\u00b7ler", "Men\u00b7schen", "dich\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ADJD", "VAFIN", "ADV", "PIAT", "NN", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Wie bald verwirret Gott das alles was wir richten!", "tokens": ["Wie", "bald", "ver\u00b7wir\u00b7ret", "Gott", "das", "al\u00b7les", "was", "wir", "rich\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "NN", "ART", "PIS", "PWS", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Wer wei\u00df/ da Gott f\u00fcr sey/ ob diese sch\u00f6ne Statt", "tokens": ["Wer", "wei\u00df", "/", "da", "Gott", "f\u00fcr", "sey", "/", "ob", "die\u00b7se", "sch\u00f6\u00b7ne", "Statt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "$(", "KOUS", "NN", "APPR", "VAFIN", "$(", "KOUS", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Der Krieg in kurtzer Zeit nicht auffgerieben hat.", "tokens": ["Der", "Krieg", "in", "kurt\u00b7zer", "Zeit", "nicht", "auff\u00b7ge\u00b7rie\u00b7ben", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "PTKNEG", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Wo Schlesien jetzt ist lag alles vor viel Jahren", "tokens": ["Wo", "Schle\u00b7si\u00b7en", "jetzt", "ist", "lag", "al\u00b7les", "vor", "viel", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NE", "ADV", "VAFIN", "VVFIN", "PIS", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Ein\u00f6d vnd vnbewohnt/ wo vorhin W\u00e4lder waren", "tokens": ["Ei\u00b7n\u00f6d", "vnd", "vn\u00b7be\u00b7wohnt", "/", "wo", "vor\u00b7hin", "W\u00e4l\u00b7der", "wa\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "KON", "ADJD", "$(", "PWAV", "ADV", "NN", "VAFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Ist wol gebawtes Landt. Jetzt geht manch sch\u00f6nes Bild", "tokens": ["Ist", "wol", "ge\u00b7baw\u00b7tes", "Landt", ".", "Jetzt", "geht", "manch", "sch\u00f6\u00b7nes", "Bild"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ADJA", "NN", "$.", "ADV", "VVFIN", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Wo vorhin gar nichts war/ als vngez\u00e4mtes Wild.", "tokens": ["Wo", "vor\u00b7hin", "gar", "nichts", "war", "/", "als", "vn\u00b7ge\u00b7z\u00e4m\u00b7tes", "Wild", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADV", "PIS", "VAFIN", "$(", "KOUS", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Ich bin so sehr verst\u00fcrtzt vnd ausser meinen Sinnen", "tokens": ["Ich", "bin", "so", "sehr", "ver\u00b7st\u00fcrtzt", "vnd", "aus\u00b7ser", "mei\u00b7nen", "Sin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "VVPP", "KON", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Da\u00df ich wol gar nicht wei\u00df was ich nur soll beginnen/", "tokens": ["Da\u00df", "ich", "wol", "gar", "nicht", "wei\u00df", "was", "ich", "nur", "soll", "be\u00b7gin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "PTKNEG", "VVFIN", "PWS", "PPER", "ADV", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Ich h\u00f6r vnd sehe nichts/ ich wei\u00df nicht wo ich bin/", "tokens": ["Ich", "h\u00f6r", "vnd", "se\u00b7he", "nichts", "/", "ich", "wei\u00df", "nicht", "wo", "ich", "bin", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PIS", "$(", "PPER", "VVFIN", "PTKNEG", "PWAV", "PPER", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Die eytelkeit der Welt benimpt mir Muth vnd Sinn.", "tokens": ["Die", "ey\u00b7tel\u00b7keit", "der", "Welt", "be\u00b7nimpt", "mir", "Muth", "vnd", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "PPER", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Ich hitz\u2019 vnd bin entz\u00fcndt wie Etna wann er strewet", "tokens": ["Ich", "hitz'", "vnd", "bin", "ent\u00b7z\u00fcndt", "wie", "Et\u00b7na", "wann", "er", "stre\u00b7wet"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VAFIN", "ADJD", "KOKOM", "NE", "PWAV", "PPER", "VVFIN"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.26": {"text": "Die Flammen in die Lufft vnd siedendt Hartz au\u00dfspeyet/", "tokens": ["Die", "Flam\u00b7men", "in", "die", "Lufft", "vnd", "sie\u00b7dendt", "Hartz", "au\u00df\u00b7spe\u00b7yet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "KON", "CARD", "NN", "VVFIN", "$("], "meter": "-+-+-+--++--+", "measure": "iambic.hexa.relaxed"}, "line.27": {"text": "Vnd au\u00df dem holen Schlundt bald schwartze Wolcken bl\u00e4st/", "tokens": ["Vnd", "au\u00df", "dem", "ho\u00b7len", "Schlundt", "bald", "schwart\u00b7ze", "Wol\u00b7cken", "bl\u00e4st", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "ADV", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Bald gantze Kl\u00fcfften Stein vnd Kugeln fliegen l\u00e4st.", "tokens": ["Bald", "gant\u00b7ze", "Kl\u00fcff\u00b7ten", "Stein", "vnd", "Ku\u00b7geln", "flie\u00b7gen", "l\u00e4st", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "NN", "KON", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Ich dencke hin vnd her/ was ist doch vnser Leben/", "tokens": ["Ich", "den\u00b7cke", "hin", "vnd", "her", "/", "was", "ist", "doch", "vn\u00b7ser", "Le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "KON", "PTKVZ", "$(", "PWS", "VAFIN", "ADV", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Die wir ohn end vnd orth in Forcht vnd Hoffnung schweben?", "tokens": ["Die", "wir", "ohn", "end", "vnd", "orth", "in", "Forcht", "vnd", "Hoff\u00b7nung", "schwe\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "KON", "NN", "APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Wir wallen in dem See der sch\u00e4ndtlichen Begierdt/", "tokens": ["Wir", "wal\u00b7len", "in", "dem", "See", "der", "sch\u00e4ndt\u00b7li\u00b7chen", "Be\u00b7gierdt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Niemandt ist den der Wind nach seinem Willen f\u00fchrt.", "tokens": ["Nie\u00b7mandt", "ist", "den", "der", "Wind", "nach", "sei\u00b7nem", "Wil\u00b7len", "f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ART", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Ach welcher Mensch darff wol auff sein Gewissen sagen", "tokens": ["Ach", "wel\u00b7cher", "Mensch", "darff", "wol", "auff", "sein", "Ge\u00b7wis\u00b7sen", "sa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "PWAT", "NN", "VMFIN", "ADV", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Es habe seinem Sinn gar niemals fehl geschlagen?", "tokens": ["Es", "ha\u00b7be", "sei\u00b7nem", "Sinn", "gar", "nie\u00b7mals", "fehl", "ge\u00b7schla\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "ADV", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Vnd es kan noch geschehn geschicht es jetzundt nicht/", "tokens": ["Vnd", "es", "kan", "noch", "ge\u00b7schehn", "ge\u00b7schicht", "es", "je\u00b7tzundt", "nicht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "ADV", "ADJD", "VVPP", "PPER", "ADV", "PTKNEG", "$("], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.36": {"text": "Das Gla\u00df je mehr es gl\u00e4ntzt je leichter es zubricht.", "tokens": ["Das", "Gla\u00df", "je", "mehr", "es", "gl\u00e4ntzt", "je", "leich\u00b7ter", "es", "zu\u00b7bricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "PPER", "VVFIN", "ADV", "ADJD", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Was hilfft es dich doch wol viel Reichthumb zubewahren/", "tokens": ["Was", "hilfft", "es", "dich", "doch", "wol", "viel", "Reicht\u00b7humb", "zu\u00b7be\u00b7wah\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PRF", "ADV", "ADV", "PIAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Viel Silber Gold vnd Gelt durch kargen zu ersparen?", "tokens": ["Viel", "Sil\u00b7ber", "Gold", "vnd", "Gelt", "durch", "kar\u00b7gen", "zu", "er\u00b7spa\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "NN", "KON", "NN", "APPR", "VVINF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Die Zeit die M\u00f6rderin so alles fressen kan/", "tokens": ["Die", "Zeit", "die", "M\u00f6r\u00b7de\u00b7rin", "so", "al\u00b7les", "fres\u00b7sen", "kan", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADV", "PIS", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Bringt schwerlich auch mit dir was newes auff die bahn.", "tokens": ["Bringt", "schwer\u00b7lich", "auch", "mit", "dir", "was", "ne\u00b7wes", "auff", "die", "bahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ADV", "APPR", "PPER", "PIS", "ADJA", "APPR", "ART", "NN", "$."], "meter": "-+-+-++--+-+", "measure": "iambic.hexa.relaxed"}, "line.41": {"text": "Bekenne bitt