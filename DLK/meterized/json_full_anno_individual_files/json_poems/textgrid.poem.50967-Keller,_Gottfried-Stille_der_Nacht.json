{"textgrid.poem.50967": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Stille der Nacht", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Willkommen, klare Sommernacht,", "tokens": ["Will\u00b7kom\u00b7men", ",", "kla\u00b7re", "Som\u00b7mer\u00b7nacht", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die auf betauten Fluren liegt!", "tokens": ["Die", "auf", "be\u00b7tau\u00b7ten", "Flu\u00b7ren", "liegt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gegr\u00fc\u00dft mir, goldne Sternenpracht,", "tokens": ["Ge\u00b7gr\u00fc\u00dft", "mir", ",", "gold\u00b7ne", "Ster\u00b7nen\u00b7pracht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die spielend sich im Weltraum wiegt!", "tokens": ["Die", "spie\u00b7lend", "sich", "im", "Welt\u00b7raum", "wiegt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PRF", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Das Urgebirge um mich her", "tokens": ["Das", "Ur\u00b7ge\u00b7bir\u00b7ge", "um", "mich", "her"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PRF", "APZR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ist schweigend, wie mein Nachtgebet;", "tokens": ["Ist", "schwei\u00b7gend", ",", "wie", "mein", "Nacht\u00b7ge\u00b7bet", ";"], "token_info": ["word", "