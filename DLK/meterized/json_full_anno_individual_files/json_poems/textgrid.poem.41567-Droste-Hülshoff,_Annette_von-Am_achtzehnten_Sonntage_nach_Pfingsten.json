{"textgrid.poem.41567": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Am achtzehnten Sonntage nach Pfingsten", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sechs Tage sollst du tun", "tokens": ["Sechs", "Ta\u00b7ge", "sollst", "du", "tun"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["CARD", "NN", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Dein Werk mit aller Treue;", "tokens": ["Dein", "Werk", "mit", "al\u00b7ler", "Treu\u00b7e", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und sollst am siebten ruhn,", "tokens": ["Und", "sollst", "am", "sieb\u00b7ten", "ruhn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "APPRART", "ADJA", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Er tr\u00e4gt des Herren Weihe.", "tokens": ["Er", "tr\u00e4gt", "des", "Her\u00b7ren", "Wei\u00b7he", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "So ward es uns gesetzet", "tokens": ["So", "ward", "es", "uns", "ge\u00b7set\u00b7zet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und also folgen wir,", "tokens": ["Und", "al\u00b7so", "fol\u00b7gen", "wir", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Recht wie den Schnabel wetzet", "tokens": ["Recht", "wie", "den", "Schna\u00b7bel", "wet\u00b7zet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KOKOM", "ART", "NN", "VVFIN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.8": {"text": "Das l\u00fcstern stumpfe Tier.", "tokens": ["Das", "l\u00fcs\u00b7tern", "stump\u00b7fe", "Tier", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der feiert bei dem Spiel,", "tokens": ["Der", "fei\u00b7ert", "bei", "dem", "Spiel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Und jener bei der Flasche,", "tokens": ["Und", "je\u00b7ner", "bei", "der", "Fla\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sinnt jeder lang und viel,", "tokens": ["Sinnt", "je\u00b7der", "lang", "und", "viel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADJD", "KON", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wie er sich Lust erhasche.", "tokens": ["Wie", "er", "sich", "Lust", "er\u00b7ha\u00b7sche", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Was nicht den Herrn mag loben,", "tokens": ["Was", "nicht", "den", "Herrn", "mag", "lo\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PTKNEG", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und was den Sinn bet\u00f6rt,", "tokens": ["Und", "was", "den", "Sinn", "be\u00b7t\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wem wird es aufgehoben?", "tokens": ["Wem", "wird", "es", "auf\u00b7ge\u00b7ho\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Dem heil'gen Sonntag wert.", "tokens": ["Dem", "heil'\u00b7gen", "Sonn\u00b7tag", "wert", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ja, wenn man h\u00e4ufen mag", "tokens": ["Ja", ",", "wenn", "man", "h\u00e4u\u00b7fen", "mag"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "KOUS", "PIS", "VVINF", "VMFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Der ganzen Woche S\u00fcnden", "tokens": ["Der", "gan\u00b7zen", "Wo\u00b7che", "S\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Gen was an diesem Tag", "tokens": ["Gen", "was", "an", "die\u00b7sem", "Tag"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PRELS", "APPR", "PDAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Mu\u00df seine Ernte finden,", "tokens": ["Mu\u00df", "sei\u00b7ne", "Ern\u00b7te", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "So wird, o Schmach! es zollen", "tokens": ["So", "wird", ",", "o", "Schmach", "!", "es", "zol\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VAFIN", "$,", "FM", "NN", "$.", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wie gen geh\u00e4uftes Ma\u00df,", "tokens": ["Wie", "gen", "ge\u00b7h\u00e4uf\u00b7tes", "Ma\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ADJA", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "Von dem die K\u00f6rner rollen,", "tokens": ["Von", "dem", "die", "K\u00f6r\u00b7ner", "rol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Zwei \u00c4hren, so man las.", "tokens": ["Zwei", "\u00c4h\u00b7ren", ",", "so", "man", "las", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "$,", "ADV", "PIS", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Stehn denn die Kirchen leer,", "tokens": ["Stehn", "denn", "die", "Kir\u00b7chen", "leer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Flieht seinen Herrn der S\u00fcnder?", "tokens": ["Flieht", "sei\u00b7nen", "Herrn", "der", "S\u00fcn\u00b7der", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "O wenn dem also w\u00e4r'!", "tokens": ["O", "wenn", "dem", "al\u00b7so", "w\u00e4r'", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "ART", "ADV", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Der Frevel dr\u00fcckte minder,", "tokens": ["Der", "Fre\u00b7vel", "dr\u00fcck\u00b7te", "min\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Doch aus dem Weihrauchwallen,", "tokens": ["Doch", "aus", "dem", "Weih\u00b7rauch\u00b7wal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.6": {"text": "Das unsern Gott umflie\u00dft,", "tokens": ["Das", "un\u00b7sern", "Gott", "um\u00b7flie\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Zu des Verderbens Hallen", "tokens": ["Zu", "des", "Ver\u00b7der\u00b7bens", "Hal\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Man wie ein Geier schie\u00dft.", "tokens": ["Man", "wie", "ein", "Gei\u00b7er", "schie\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "KOKOM", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "In alten Bundes Pflicht,", "tokens": ["In", "al\u00b7ten", "Bun\u00b7des", "Pflicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Als keimend noch die Gnade", "tokens": ["Als", "kei\u00b7mend", "noch", "die", "Gna\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und d\u00e4mmernd nur das Licht", "tokens": ["Und", "d\u00e4m\u00b7mernd", "nur", "das", "Licht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "ADV", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Fiel auf der Menschen Pfade:", "tokens": ["Fiel", "auf", "der", "Men\u00b7schen", "Pfa\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Da trug der S\u00fcnde Flecken", "tokens": ["Da", "trug", "der", "S\u00fcn\u00b7de", "Fle\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Noch nicht der Sabbat, doch", "tokens": ["Noch", "nicht", "der", "Sab\u00b7bat", ",", "doch"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "PTKNEG", "ART", "NN", "$,", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Mu\u00dft' er den Gl\u00e4ub'gen schrecken,", "tokens": ["Mu\u00dft'", "er", "den", "Gl\u00e4ub'\u00b7gen", "schre\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.8": {"text": "Ach, wie ein eisern Joch.", "tokens": ["Ach", ",", "wie", "ein", "ei\u00b7sern", "Joch", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWAV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Wohl mag es t\u00f6richt sein,", "tokens": ["Wohl", "mag", "es", "t\u00f6\u00b7richt", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Dem h\u00f6chsten Gott zu Ehren", "tokens": ["Dem", "h\u00f6chs\u00b7ten", "Gott", "zu", "Eh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Zu liegen wie ein Stein,", "tokens": ["Zu", "lie\u00b7gen", "wie", "ein", "Stein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und jeder Regung wehren;", "tokens": ["Und", "je\u00b7der", "Re\u00b7gung", "weh\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Doch eitlen L\u00fcsten f\u00fcgen", "tokens": ["Doch", "eit\u00b7len", "L\u00fcs\u00b7ten", "f\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der Sinne kirren Bund \u2013", "tokens": ["Der", "Sin\u00b7ne", "kir\u00b7ren", "Bund", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "O besser zehnfach liegen", "tokens": ["O", "bes\u00b7ser", "zehn\u00b7fach", "lie\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "ADJD", "ADJD", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wie eine Scholl' am Grund.", "tokens": ["Wie", "ei\u00b7ne", "Scholl'", "am", "Grund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "So hat der Heiland nicht", "tokens": ["So", "hat", "der", "Hei\u00b7land", "nicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "PTKNEG"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Den alten Bund gehoben:", "tokens": ["Den", "al\u00b7ten", "Bund", "ge\u00b7ho\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Durch Taten wie das Licht", "tokens": ["Durch", "Ta\u00b7ten", "wie", "das", "Licht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KOKOM", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Sollst du den H\u00f6chsten loben!", "tokens": ["Sollst", "du", "den", "H\u00f6chs\u00b7ten", "lo\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sei mit der milden Spende", "tokens": ["Sei", "mit", "der", "mil\u00b7den", "Spen\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der Arme dir gegr\u00fc\u00dft:", "tokens": ["Der", "Ar\u00b7me", "dir", "ge\u00b7gr\u00fc\u00dft", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Nicht unrein sind die H\u00e4nde,", "tokens": ["Nicht", "un\u00b7rein", "sind", "die", "H\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Aus denen Segen flie\u00dft.", "tokens": ["Aus", "de\u00b7nen", "Se\u00b7gen", "flie\u00dft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Und wer gering und klein", "tokens": ["Und", "wer", "ge\u00b7ring", "und", "klein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "ADJD", "KON", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Im Schmerzenslager r\u00fccket,", "tokens": ["Im", "Schmer\u00b7zens\u00b7la\u00b7ger", "r\u00fc\u00b7cket", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wo schlimmer als die Pein", "tokens": ["Wo", "schlim\u00b7mer", "als", "die", "Pein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "KOKOM", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Verlassenheit ihn dr\u00fccket,", "tokens": ["Ver\u00b7las\u00b7sen\u00b7heit", "ihn", "dr\u00fc\u00b7cket", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Verbinde dessen Wunden", "tokens": ["Ver\u00b7bin\u00b7de", "des\u00b7sen", "Wun\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["NN", "PRELAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und l\u00e4chle ihm dazu;", "tokens": ["Und", "l\u00e4ch\u00b7le", "ihm", "da\u00b7zu", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Dann hast du sie gefunden", "tokens": ["Dann", "hast", "du", "sie", "ge\u00b7fun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die echte Sabbatsruh!", "tokens": ["Die", "ech\u00b7te", "Sab\u00b7bats\u00b7ruh", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Sechs Tage sollst du tun", "tokens": ["Sechs", "Ta\u00b7ge", "sollst", "du", "tun"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["CARD", "NN", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Dein Werk mit aller Treue;", "tokens": ["Dein", "Werk", "mit", "al\u00b7ler", "Treu\u00b7e", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und sollst am siebten ruhn,", "tokens": ["Und", "sollst", "am", "sieb\u00b7ten", "ruhn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "APPRART", "ADJA", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Er tr\u00e4gt des Herren Weihe.", "tokens": ["Er", "tr\u00e4gt", "des", "Her\u00b7ren", "Wei\u00b7he", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "So ward es uns gesetzet", "tokens": ["So", "ward", "es", "uns", "ge\u00b7set\u00b7zet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und also folgen wir,", "tokens": ["Und", "al\u00b7so", "fol\u00b7gen", "wir", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Recht wie den Schnabel wetzet", "tokens": ["Recht", "wie", "den", "Schna\u00b7bel", "wet\u00b7zet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KOKOM", "ART", "NN", "VVFIN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.8": {"text": "Das l\u00fcstern stumpfe Tier.", "tokens": ["Das", "l\u00fcs\u00b7tern", "stump\u00b7fe", "Tier", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Der feiert bei dem Spiel,", "tokens": ["Der", "fei\u00b7ert", "bei", "dem", "Spiel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Und jener bei der Flasche,", "tokens": ["Und", "je\u00b7ner", "bei", "der", "Fla\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sinnt jeder lang und viel,", "tokens": ["Sinnt", "je\u00b7der", "lang", "und", "viel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADJD", "KON", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wie er sich Lust erhasche.", "tokens": ["Wie", "er", "sich", "Lust", "er\u00b7ha\u00b7sche", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Was nicht den Herrn mag loben,", "tokens": ["Was", "nicht", "den", "Herrn", "mag", "lo\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PTKNEG", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und was den Sinn bet\u00f6rt,", "tokens": ["Und", "was", "den", "Sinn", "be\u00b7t\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wem wird es aufgehoben?", "tokens": ["Wem", "wird", "es", "auf\u00b7ge\u00b7ho\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Dem heil'gen Sonntag wert.", "tokens": ["Dem", "heil'\u00b7gen", "Sonn\u00b7tag", "wert", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Ja, wenn man h\u00e4ufen mag", "tokens": ["Ja", ",", "wenn", "man", "h\u00e4u\u00b7fen", "mag"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "KOUS", "PIS", "VVINF", "VMFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Der ganzen Woche S\u00fcnden", "tokens": ["Der", "gan\u00b7zen", "Wo\u00b7che", "S\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Gen was an diesem Tag", "tokens": ["Gen", "was", "an", "die\u00b7sem", "Tag"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PRELS", "APPR", "PDAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Mu\u00df seine Ernte finden,", "tokens": ["Mu\u00df", "sei\u00b7ne", "Ern\u00b7te", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "So wird, o Schmach! es zollen", "tokens": ["So", "wird", ",", "o", "Schmach", "!", "es", "zol\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VAFIN", "$,", "FM", "NN", "$.", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wie gen geh\u00e4uftes Ma\u00df,", "tokens": ["Wie", "gen", "ge\u00b7h\u00e4uf\u00b7tes", "Ma\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ADJA", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "Von dem die K\u00f6rner rollen,", "tokens": ["Von", "dem", "die", "K\u00f6r\u00b7ner", "rol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Zwei \u00c4hren, so man las.", "tokens": ["Zwei", "\u00c4h\u00b7ren", ",", "so", "man", "las", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "$,", "ADV", "PIS", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Stehn denn die Kirchen leer,", "tokens": ["Stehn", "denn", "die", "Kir\u00b7chen", "leer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Flieht seinen Herrn der S\u00fcnder?", "tokens": ["Flieht", "sei\u00b7nen", "Herrn", "der", "S\u00fcn\u00b7der", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "O wenn dem also w\u00e4r'!", "tokens": ["O", "wenn", "dem", "al\u00b7so", "w\u00e4r'", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "ART", "ADV", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Der Frevel dr\u00fcckte minder,", "tokens": ["Der", "Fre\u00b7vel", "dr\u00fcck\u00b7te", "min\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Doch aus dem Weihrauchwallen,", "tokens": ["Doch", "aus", "dem", "Weih\u00b7rauch\u00b7wal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.6": {"text": "Das unsern Gott umflie\u00dft,", "tokens": ["Das", "un\u00b7sern", "Gott", "um\u00b7flie\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Zu des Verderbens Hallen", "tokens": ["Zu", "des", "Ver\u00b7der\u00b7bens", "Hal\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Man wie ein Geier schie\u00dft.", "tokens": ["Man", "wie", "ein", "Gei\u00b7er", "schie\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "KOKOM", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "In alten Bundes Pflicht,", "tokens": ["In", "al\u00b7ten", "Bun\u00b7des", "Pflicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Als keimend noch die Gnade", "tokens": ["Als", "kei\u00b7mend", "noch", "die", "Gna\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und d\u00e4mmernd nur das Licht", "tokens": ["Und", "d\u00e4m\u00b7mernd", "nur", "das", "Licht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "ADV", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Fiel auf der Menschen Pfade:", "tokens": ["Fiel", "auf", "der", "Men\u00b7schen", "Pfa\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Da trug der S\u00fcnde Flecken", "tokens": ["Da", "trug", "der", "S\u00fcn\u00b7de", "Fle\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Noch nicht der Sabbat, doch", "tokens": ["Noch", "nicht", "der", "Sab\u00b7bat", ",", "doch"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "PTKNEG", "ART", "NN", "$,", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Mu\u00dft' er den Gl\u00e4ub'gen schrecken,", "tokens": ["Mu\u00dft'", "er", "den", "Gl\u00e4ub'\u00b7gen", "schre\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.8": {"text": "Ach, wie ein eisern Joch.", "tokens": ["Ach", ",", "wie", "ein", "ei\u00b7sern", "Joch", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWAV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Wohl mag es t\u00f6richt sein,", "tokens": ["Wohl", "mag", "es", "t\u00f6\u00b7richt", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Dem h\u00f6chsten Gott zu Ehren", "tokens": ["Dem", "h\u00f6chs\u00b7ten", "Gott", "zu", "Eh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Zu liegen wie ein Stein,", "tokens": ["Zu", "lie\u00b7gen", "wie", "ein", "Stein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und jeder Regung wehren;", "tokens": ["Und", "je\u00b7der", "Re\u00b7gung", "weh\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Doch eitlen L\u00fcsten f\u00fcgen", "tokens": ["Doch", "eit\u00b7len", "L\u00fcs\u00b7ten", "f\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der Sinne kirren Bund \u2013", "tokens": ["Der", "Sin\u00b7ne", "kir\u00b7ren", "Bund", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "O besser zehnfach liegen", "tokens": ["O", "bes\u00b7ser", "zehn\u00b7fach", "lie\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "ADJD", "ADJD", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wie eine Scholl' am Grund.", "tokens": ["Wie", "ei\u00b7ne", "Scholl'", "am", "Grund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "So hat der Heiland nicht", "tokens": ["So", "hat", "der", "Hei\u00b7land", "nicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "PTKNEG"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Den alten Bund gehoben:", "tokens": ["Den", "al\u00b7ten", "Bund", "ge\u00b7ho\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Durch Taten wie das Licht", "tokens": ["Durch", "Ta\u00b7ten", "wie", "das", "Licht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KOKOM", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Sollst du den H\u00f6chsten loben!", "tokens": ["Sollst", "du", "den", "H\u00f6chs\u00b7ten", "lo\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sei mit der milden Spende", "tokens": ["Sei", "mit", "der", "mil\u00b7den", "Spen\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der Arme dir gegr\u00fc\u00dft:", "tokens": ["Der", "Ar\u00b7me", "dir", "ge\u00b7gr\u00fc\u00dft", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Nicht unrein sind die H\u00e4nde,", "tokens": ["Nicht", "un\u00b7rein", "sind", "die", "H\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Aus denen Segen flie\u00dft.", "tokens": ["Aus", "de\u00b7nen", "Se\u00b7gen", "flie\u00dft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.16": {"line.1": {"text": "Und wer gering und klein", "tokens": ["Und", "wer", "ge\u00b7ring", "und", "klein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "ADJD", "KON", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Im Schmerzenslager r\u00fccket,", "tokens": ["Im", "Schmer\u00b7zens\u00b7la\u00b7ger", "r\u00fc\u00b7cket", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wo schlimmer als die Pein", "tokens": ["Wo", "schlim\u00b7mer", "als", "die", "Pein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "KOKOM", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Verlassenheit ihn dr\u00fccket,", "tokens": ["Ver\u00b7las\u00b7sen\u00b7heit", "ihn", "dr\u00fc\u00b7cket", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Verbinde dessen Wunden", "tokens": ["Ver\u00b7bin\u00b7de", "des\u00b7sen", "Wun\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["NN", "PRELAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und l\u00e4chle ihm dazu;", "tokens": ["Und", "l\u00e4ch\u00b7le", "ihm", "da\u00b7zu", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Dann hast du sie gefunden", "tokens": ["Dann", "hast", "du", "sie", "ge\u00b7fun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die echte Sabbatsruh!", "tokens": ["Die", "ech\u00b7te", "Sab\u00b7bats\u00b7ruh", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}