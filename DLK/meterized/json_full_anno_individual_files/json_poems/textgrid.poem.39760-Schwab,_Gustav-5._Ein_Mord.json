.tri"}, "line.3": {"text": "Und doch \u2013 wer k\u00f6nnte morden", "tokens": ["Und", "doch", "\u2013", "wer", "k\u00f6nn\u00b7te", "mor\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "$(", "PWS", "VMFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In solcher Fr\u00fchlingszier?", "tokens": ["In", "sol\u00b7cher", "Fr\u00fch\u00b7lings\u00b7zier", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.17": {"line.1": {"text": "Mich schaudert, ich entweiche.", "tokens": ["Mich", "schau\u00b7dert", ",", "ich", "ent\u00b7wei\u00b7che", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was thut er? \u2013 Mensch! Abschaum!", "tokens": ["Was", "thut", "er", "?", "\u2013", "Mensch", "!", "Ab\u00b7schaum", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$.", "$(", "NN", "$.", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du f\u00fchrst die Todesstreiche", "tokens": ["Du", "f\u00fchrst", "die", "To\u00b7des\u00b7strei\u00b7che"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Auf einen Bl\u00fctenbaum!", "tokens": ["Auf", "ei\u00b7nen", "Bl\u00fc\u00b7ten\u00b7baum", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.18": {"line.1": {"text": "Weh! Hieb auf Hieb! dem zweiten,", "tokens": ["Weh", "!", "Hieb", "auf", "Hieb", "!", "dem", "zwei\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$.", "NN", "APPR", "NN", "$.", "ART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dem dritten thut er's an;", "tokens": ["Dem", "drit\u00b7ten", "thut", "er's", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "PIS", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dem Baume, der nicht streiten,", "tokens": ["Dem", "Bau\u00b7me", ",", "der", "nicht", "strei\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der sich nicht wehren kann!", "tokens": ["Der", "sich", "nicht", "weh\u00b7ren", "kann", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.19": {"line.1": {"text": "Halt ein! \u2013 er ist entflohen,", "tokens": ["Halt", "ein", "!", "\u2013", "er", "ist", "ent\u00b7flo\u00b7hen", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$.", "$(", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Er schwindet in den Wald;", "tokens": ["Er", "schwin\u00b7det", "in", "den", "Wald", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Von fern seh' ich ihn drohen,", "tokens": ["Von", "fern", "seh'", "ich", "ihn", "dro\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "VVFIN", "PPER", "PPER", "VVFIN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Als k\u00e4m' er wieder bald.", "tokens": ["Als", "k\u00e4m'", "er", "wie\u00b7der", "bald", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.20": {"line.1": {"text": "Mein Herz ist fast gebrochen", "tokens": ["Mein", "Herz", "ist", "fast", "ge\u00b7bro\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vor seiner Streiche Wucht.", "tokens": ["Vor", "sei\u00b7ner", "Strei\u00b7che", "Wucht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die B\u00e4umchen werden sochen;", "tokens": ["Die", "B\u00e4um\u00b7chen", "wer\u00b7den", "so\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sie sterben vor der Frucht.", "tokens": ["Sie", "ster\u00b7ben", "vor", "der", "Frucht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.21": {"line.1": {"text": "Umsonst bin ich entronnen", "tokens": ["Um\u00b7sonst", "bin", "ich", "ent\u00b7ron\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Stadt, die B\u00f6ses pflegt,", "tokens": ["Der", "Stadt", ",", "die", "B\u00f6\u00b7ses", "pflegt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wenn hinterm Licht der Sonnen", "tokens": ["Wenn", "hin\u00b7term", "Licht", "der", "Son\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Flur noch Schw\u00e4rzres hegt;", "tokens": ["Die", "Flur", "noch", "Schw\u00e4rz\u00b7res", "hegt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.22": {"line.1": {"text": "Wenn in die milde Sprache,", "tokens": ["Wenn", "in", "die", "mil\u00b7de", "Spra\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Gott den Fr\u00fchling lehrt,", "tokens": ["Die", "Gott", "den", "Fr\u00fch\u00b7ling", "lehrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Mensch mit seiner Rache", "tokens": ["Der", "Mensch", "mit", "sei\u00b7ner", "Ra\u00b7che"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Auch hier verh\u00f6hnend f\u00e4hrt.", "tokens": ["Auch", "hier", "ver\u00b7h\u00f6h\u00b7nend", "f\u00e4hrt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVPP", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}