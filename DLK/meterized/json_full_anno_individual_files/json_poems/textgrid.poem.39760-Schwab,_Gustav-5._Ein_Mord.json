{"textgrid.poem.39760": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "5. Ein Mord", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gott gr\u00fc\u00df' euch, liebe B\u00e4ume!", "tokens": ["Gott", "gr\u00fc\u00df'", "euch", ",", "lie\u00b7be", "B\u00e4u\u00b7me", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie bl\u00fcht ihr so getreu,", "tokens": ["Wie", "bl\u00fcht", "ihr", "so", "ge\u00b7treu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Macht unsrer Jugend Tr\u00e4ume", "tokens": ["Macht", "uns\u00b7rer", "Ju\u00b7gend", "Tr\u00e4u\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Allj\u00e4hrlich wahr und neu.", "tokens": ["All\u00b7j\u00e4hr\u00b7lich", "wahr", "und", "neu", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die s\u00fc\u00dfe M\u00e4dchenbl\u00fcte", "tokens": ["Die", "s\u00fc\u00b7\u00dfe", "M\u00e4d\u00b7chen\u00b7bl\u00fc\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Gl\u00e4nzt einmal nur, nicht mehr.", "tokens": ["Gl\u00e4nzt", "ein\u00b7mal", "nur", ",", "nicht", "mehr", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "$,", "PTKNEG", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Euch schenkt des Himmels G\u00fcte", "tokens": ["Euch", "schenkt", "des", "Him\u00b7mels", "G\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Bl\u00fcten Wiederkehr. \u2013", "tokens": ["Der", "Bl\u00fc\u00b7ten", "Wie\u00b7der\u00b7kehr", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Was st\u00f6rt mir die Gedanken", "tokens": ["Was", "st\u00f6rt", "mir", "die", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein finsterer Gesell?", "tokens": ["Ein", "fins\u00b7te\u00b7rer", "Ge\u00b7sell", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie seine Schritte wanken", "tokens": ["Wie", "sei\u00b7ne", "Schrit\u00b7te", "wan\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Jetzt langsam und jetzt schnell!", "tokens": ["Jetzt", "lang\u00b7sam", "und", "jetzt", "schnell", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Er schie\u00dft so gift'ge Blicke,", "tokens": ["Er", "schie\u00dft", "so", "gift'\u00b7ge", "Bli\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein Beil schwingt seine Hand,", "tokens": ["Ein", "Beil", "schwingt", "sei\u00b7ne", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Als w\u00fcrd' es ins Genicke", "tokens": ["Als", "w\u00fcrd'", "es", "ins", "Ge\u00b7ni\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "VAFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Des Feindes j\u00e4h gesandt.", "tokens": ["Des", "Fein\u00b7des", "j\u00e4h", "ge\u00b7sandt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Es ist schon Abend worden,", "tokens": ["Es", "ist", "schon", "A\u00b7bend", "wor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "NN", "VAPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und nicht geheuer hier!", "tokens": ["Und", "nicht", "ge\u00b7heu\u00b7er", "hier", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ADJD", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und doch \u2013 wer k\u00f6nnte morden", "tokens": ["Und", "doch", "\u2013", "wer", "k\u00f6nn\u00b7te", "mor\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "$(", "PWS", "VMFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In solcher Fr\u00fchlingszier?", "tokens": ["In", "sol\u00b7cher", "Fr\u00fch\u00b7lings\u00b7zier", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Mich schaudert, ich entweiche.", "tokens": ["Mich", "schau\u00b7dert", ",", "ich", "ent\u00b7wei\u00b7che", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was thut er? \u2013 Mensch! Abschaum!", "tokens": ["Was", "thut", "er", "?", "\u2013", "Mensch", "!", "Ab\u00b7schaum", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$.", "$(", "NN", "$.", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du f\u00fchrst die Todesstreiche", "tokens": ["Du", "f\u00fchrst", "die", "To\u00b7des\u00b7strei\u00b7che"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Auf einen Bl\u00fctenbaum!", "tokens": ["Auf", "ei\u00b7nen", "Bl\u00fc\u00b7ten\u00b7baum", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Weh! Hieb auf Hieb! dem zweiten,", "tokens": ["Weh", "!", "Hieb", "auf", "Hieb", "!", "dem", "zwei\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$.", "NN", "APPR", "NN", "$.", "ART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dem dritten thut er's an;", "tokens": ["Dem", "drit\u00b7ten", "thut", "er's", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "PIS", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dem Baume, der nicht streiten,", "tokens": ["Dem", "Bau\u00b7me", ",", "der", "nicht", "strei\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der sich nicht wehren kann!", "tokens": ["Der", "sich", "nicht", "weh\u00b7ren", "kann", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Halt ein! \u2013 er ist entflohen,", "tokens": ["Halt", "ein", "!", "\u2013", "er", "ist", "ent\u00b7flo\u00b7hen", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$.", "$(", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Er schwindet in den Wald;", "tokens": ["Er", "schwin\u00b7det", "in", "den", "Wald", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Von fern seh' ich ihn drohen,", "tokens": ["Von", "fern", "seh'", "ich", "ihn", "dro\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "VVFIN", "PPER", "PPER", "VVFIN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Als k\u00e4m' er wieder bald.", "tokens": ["Als", "k\u00e4m'", "er", "wie\u00b7der", "bald", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Mein Herz ist fast gebrochen", "tokens": ["Mein", "Herz", "ist", "fast", "ge\u00b7bro\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vor seiner Streiche Wucht.", "tokens": ["Vor", "sei\u00b7ner", "Strei\u00b7che", "Wucht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die B\u00e4umchen werden sochen;", "tokens": ["Die", "B\u00e4um\u00b7chen", "wer\u00b7den", "so\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sie sterben vor der Frucht.", "tokens": ["Sie", "ster\u00b7ben", "vor", "der", "Frucht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Umsonst bin ich entronnen", "tokens": ["Um\u00b7sonst", "bin", "ich", "ent\u00b7ron\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Stadt, die B\u00f6ses pflegt,", "tokens": ["Der", "Stadt", ",", "die", "B\u00f6\u00b7ses", "pflegt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wenn hinterm Licht der Sonnen", "tokens": ["Wenn", "hin\u00b7term", "Licht", "der", "Son\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Flur noch Schw\u00e4rzres hegt;", "tokens": ["Die", "Flur", "noch", "Schw\u00e4rz\u00b7res", "hegt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Wenn in die milde Sprache,", "tokens": ["Wenn", "in", "die", "mil\u00b7de", "Spra\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Gott den Fr\u00fchling lehrt,", "tokens": ["Die", "Gott", "den", "Fr\u00fch\u00b7ling", "lehrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Mensch mit seiner Rache", "tokens": ["Der", "Mensch", "mit", "sei\u00b7ner", "Ra\u00b7che"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Auch hier verh\u00f6hnend f\u00e4hrt.", "tokens": ["Auch", "hier", "ver\u00b7h\u00f6h\u00b7nend", "f\u00e4hrt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVPP", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Gott gr\u00fc\u00df' euch, liebe B\u00e4ume!", "tokens": ["Gott", "gr\u00fc\u00df'", "euch", ",", "lie\u00b7be", "B\u00e4u\u00b7me", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie bl\u00fcht ihr so getreu,", "tokens": ["Wie", "bl\u00fcht", "ihr", "so", "ge\u00b7treu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Macht unsrer Jugend Tr\u00e4ume", "tokens": ["Macht", "uns\u00b7rer", "Ju\u00b7gend", "Tr\u00e4u\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Allj\u00e4hrlich wahr und neu.", "tokens": ["All\u00b7j\u00e4hr\u00b7lich", "wahr", "und", "neu", "."], "token_info": ["word", "word", "word", "word", "punct"], "