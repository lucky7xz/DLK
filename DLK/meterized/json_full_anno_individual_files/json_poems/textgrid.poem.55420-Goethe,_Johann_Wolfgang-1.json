{"textgrid.poem.55420": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "1", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zu w\u00fcrdiger Umgebung deines Bildes,", "tokens": ["Zu", "w\u00fcr\u00b7di\u00b7ger", "Um\u00b7ge\u00b7bung", "dei\u00b7nes", "Bil\u00b7des", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPOSAT", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Wie es mir immerfort im Geiste waltet,", "tokens": ["Wie", "es", "mir", "im\u00b7mer\u00b7fort", "im", "Geis\u00b7te", "wal\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "W\u00e4hlt ich in Tagen, wo der Fr\u00fchling schaltet,", "tokens": ["W\u00e4hlt", "ich", "in", "Ta\u00b7gen", ",", "wo", "der", "Fr\u00fch\u00b7ling", "schal\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Des Gartens Blumen, Blumen des Gefildes.", "tokens": ["Des", "Gar\u00b7tens", "Blu\u00b7men", ",", "Blu\u00b7men", "des", "Ge\u00b7fil\u00b7des", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Dann schien der Rand des Achilleischen Schildes,", "tokens": ["Dann", "schien", "der", "Rand", "des", "A\u00b7chil\u00b7lei\u00b7schen", "Schil\u00b7des", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So reich er war, nicht reich genug gestaltet;", "tokens": ["So", "reich", "er", "war", ",", "nicht", "reich", "ge\u00b7nug", "ge\u00b7stal\u00b7tet", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "VAFIN", "$,", "PTKNEG", "ADJD", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ja, w\u00fcrd ein Purpurteppich umgefaltet,", "tokens": ["Ja", ",", "w\u00fcrd", "ein", "Pur\u00b7pur\u00b7tep\u00b7pich", "um\u00b7ge\u00b7fal\u00b7tet", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VAFIN", "ART", "ADJD", "VVPP", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Darauf ges\u00e4t der Sterne blendend Mildes.", "tokens": ["Da\u00b7rauf", "ge\u00b7s\u00e4t", "der", "Ster\u00b7ne", "blen\u00b7dend", "Mil\u00b7des", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVPP", "ART", "NN", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Nun aber wird ein zierlich Heft geschm\u00fccket,", "tokens": ["Nun", "a\u00b7ber", "wird", "ein", "zier\u00b7lich", "Heft", "ge\u00b7schm\u00fc\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "ART", "ADJD", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein treuer Diener widmet's deiner Hoheit,", "tokens": ["Ein", "treu\u00b7er", "Die\u00b7ner", "wid\u00b7met's", "dei\u00b7ner", "Ho\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und du verg\u00f6nnest mir die erste Weihe.", "tokens": ["Und", "du", "ver\u00b7g\u00f6n\u00b7nest", "mir", "die", "ers\u00b7te", "Wei\u00b7he", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Wie sprech ich aus, wie sehr mich das begl\u00fccket.", "tokens": ["Wie", "sprech", "ich", "aus", ",", "wie", "sehr", "mich", "das", "be\u00b7gl\u00fc\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PTKVZ", "$,", "PWAV", "ADV", "PPER", "PDS", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Jetzt f\u00fchl ich erst in neubelebter Froheit:", "tokens": ["Jetzt", "f\u00fchl", "ich", "erst", "in", "neu\u00b7be\u00b7leb\u00b7ter", "Fro\u00b7heit", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Zu w\u00fcrdiger Umgebung deines Bildes,", "tokens": ["Zu", "w\u00fcr\u00b7di\u00b7ger", "Um\u00b7ge\u00b7bung", "dei\u00b7nes", "Bil\u00b7des", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPOSAT", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Wie es mir immerfort im Geiste waltet,", "tokens": ["Wie", "es", "mir", "im\u00b7mer\u00b7fort", "im", "Geis\u00b7te", "wal\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "me