{"dta.poem.2654": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Bey Absterben/  \n Hn.   D. J. A.   J\u00fcngsten Jungf. Tochter/  \n den 26. Novembr. 1673.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was kan/ Bochw\u00fcrdiger/ ich seinen Ohren singen?", "tokens": ["Was", "kan", "/", "Boch\u00b7w\u00fcr\u00b7di\u00b7ger", "/", "ich", "sei\u00b7nen", "Oh\u00b7ren", "sin\u00b7gen", "?"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "$(", "NE", "$(", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Kein \u00e4ngstlich Jammer-Lied tilgt bittre Schmer-", "tokens": ["Kein", "\u00e4ngst\u00b7lich", "Jam\u00b7mer\u00b7Lied", "tilgt", "bitt\u00b7re", "Schmer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "ADJD", "NN", "VVFIN", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Jhm/ Brunnquell alles Trosts/ Trost-Worte wollen", "tokens": ["Jhm", "/", "Brunn\u00b7quell", "al\u00b7les", "Trosts", "/", "Trost\u00b7Wor\u00b7te", "wol\u00b7len"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "$(", "NE", "PIAT", "NN", "$(", "NN", "VMFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Heist Fackeln tragen ein in Titans g\u00fcldnes Haus.", "tokens": ["Heist", "Fa\u00b7ckeln", "tra\u00b7gen", "ein", "in", "Ti\u00b7tans", "g\u00fcld\u00b7nes", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "ART", "APPR", "NE", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die Brunnen Jsraels/ wo frisches Wasser quillet/", "tokens": ["Die", "Brun\u00b7nen", "Js\u00b7raels", "/", "wo", "fri\u00b7sches", "Was\u00b7ser", "quil\u00b7let", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$(", "PWAV", "ADJA", "NN", "VVFIN", "$("], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Woraus der Silber-Thau der Seeligkeit entspringt/", "tokens": ["Wo\u00b7raus", "der", "Sil\u00b7ber\u00b7\u00b7T\u00b7hau", "der", "See\u00b7lig\u00b7keit", "ent\u00b7springt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "VVFIN", "$("], "meter": "---+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Die haben seinen ", "tokens": ["Die", "ha\u00b7ben", "sei\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["PDS", "VAFIN", "PPOSAT"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Da\u00df auch kein Aesculap ein besser Pflaster bringt.", "tokens": ["Da\u00df", "auch", "kein", "A\u00b7e\u00b7scu\u00b7lap", "ein", "bes\u00b7ser", "Pflas\u00b7ter", "bringt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PIAT", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Alleine weil wir nur was Menschlich m\u00fcssen leiden/", "tokens": ["Al\u00b7lei\u00b7ne", "weil", "wir", "nur", "was", "Menschlich", "m\u00fcs\u00b7sen", "lei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "ADV", "PWS", "PPER", "VMFIN", "VVINF", "$("], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Und un\u00dfrer Sterbligkeit Empfindnu\u00df schuldig seyn/", "tokens": ["Und", "un\u00df\u00b7rer", "Ster\u00b7blig\u00b7keit", "Emp\u00b7find\u00b7nu\u00df", "schul\u00b7dig", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "NN", "ADJD", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "So glaub ich/ da\u00df kein Schmertz kan mehr ins Hertze schneiden/", "tokens": ["So", "glaub", "ich", "/", "da\u00df", "kein", "Schmertz", "kan", "mehr", "ins", "Hert\u00b7ze", "schnei\u00b7den", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$(", "KOUS", "PIAT", "NN", "VMFIN", "ADV", "APPRART", "VVFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Als wenn wir Fleisch und Blut die Augen dr\u00fccken ein.", "tokens": ["Als", "wenn", "wir", "Fleisch", "und", "Blut", "die", "Au\u00b7gen", "dr\u00fc\u00b7cken", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "NN", "KON", "NN", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Zwar ein Pr\u00e4xaspes schaut des Kindes Hertz aufschlitzen/", "tokens": ["Zwar", "ein", "Pr\u00e4\u00b7xas\u00b7pes", "schaut", "des", "Kin\u00b7des", "Hertz", "auf\u00b7schlit\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "ART", "NN", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Wenn des Cambyses Pfeil sein blutig Lust-Spiel treibt.", "tokens": ["Wenn", "des", "Cam\u00b7by\u00b7ses", "Pfeil", "sein", "blu\u00b7tig", "Lust\u00b7Spiel", "treibt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "PPOSAT", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Harpag kan unbewegt ans Persen Tafel sitzen/", "tokens": ["Har\u00b7pag", "kan", "un\u00b7be\u00b7wegt", "ans", "Per\u00b7sen", "Ta\u00b7fel", "sit\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "ADJD", "APPRART", "NN", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und essen seinen Sohn/ der neulich nur entleibt/", "tokens": ["Und", "es\u00b7sen", "sei\u00b7nen", "Sohn", "/", "der", "neu\u00b7lich", "nur", "ent\u00b7leibt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$(", "ART", "ADV", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Hingegen wer ein Christ/ und wei\u00df/ da\u00df Kinder Gaben/", "tokens": ["Hin\u00b7ge\u00b7gen", "wer", "ein", "Christ", "/", "und", "wei\u00df", "/", "da\u00df", "Kin\u00b7der", "Ga\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "ART", "NN", "$(", "KON", "VVFIN", "$(", "KOUS", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Au\u00df grosser Gnaden-Hand der H\u00f6chsten Majest\u00e4t/", "tokens": ["Au\u00df", "gros\u00b7ser", "Gna\u00b7den\u00b7Hand", "der", "H\u00f6chs\u00b7ten", "Ma\u00b7jes\u00b7t\u00e4t", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Da\u00df sie ein Vorrecht auch ans Himmels Erbtheil haben/", "tokens": ["Da\u00df", "sie", "ein", "Vor\u00b7recht", "auch", "ans", "Him\u00b7mels", "E\u00b7rbtheil", "ha\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "APPRART", "NN", "NN", "VAFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Von dem wird solcher Sinn und steinern Hertz verschm\u00e4ht.", "tokens": ["Von", "dem", "wird", "sol\u00b7cher", "Sinn", "und", "stei\u00b7nern", "Hertz", "ver\u00b7schm\u00e4ht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "VAFIN", "PIAT", "NN", "KON", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Eh\u2019 wird der Sonnen-Glantz mit tieffer Nacht sich decken/", "tokens": ["Eh'", "wird", "der", "Son\u00b7nen\u00b7Glantz", "mit", "tief\u00b7fer", "Nacht", "sich", "de\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "APPR", "ADJA", "NN", "PRF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Die Erde schiffbar seyn/ die Wellen angebaut/", "tokens": ["Die", "Er\u00b7de", "schiff\u00b7bar", "seyn", "/", "die", "Wel\u00b7len", "an\u00b7ge\u00b7baut", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VAINF", "$(", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Eh\u2019 wir die Regungen in unser Brust verstecken/", "tokens": ["Eh'", "wir", "die", "Re\u00b7gun\u00b7gen", "in", "un\u00b7ser", "Brust", "ver\u00b7ste\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+---+-+-+-", "measure": "unknown.measure.penta"}, "line.24": {"text": "Und man die Seinigen mit trucknen Augen schaut.", "tokens": ["Und", "man", "die", "Sei\u00b7ni\u00b7gen", "mit", "truck\u00b7nen", "Au\u00b7gen", "schaut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "ART", "PPOSS", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Bevorab wenn der Tod ist bey uns eingestiegen/", "tokens": ["Be\u00b7vor\u00b7ab", "wenn", "der", "Tod", "ist", "bey", "uns", "ein\u00b7ge\u00b7stie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ART", "NN", "VAFIN", "APPR", "PPER", "VVPP", "$("], "meter": "-++--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.26": {"text": "Und eh\u2019 ein Jahr verstreicht sich zweymal grimmig weist;", "tokens": ["Und", "eh'", "ein", "Jahr", "ver\u00b7streicht", "sich", "zwey\u00b7mal", "grim\u00b7mig", "weist", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVFIN", "PRF", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Wenn man jetzt einen Sohn sieht auf der Bahre liegen/", "tokens": ["Wenn", "man", "jetzt", "ei\u00b7nen", "Sohn", "sieht", "auf", "der", "Bah\u00b7re", "lie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Und drauf der Eltern Lust die Tochter ohne Geist.", "tokens": ["Und", "drauf", "der", "El\u00b7tern", "Lust", "die", "Toch\u00b7ter", "oh\u00b7ne", "Geist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "ART", "NN", "NN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Denn schallts: die Trauben sind vom Weinstock abgerissen/", "tokens": ["Denn", "schallts", ":", "die", "Trau\u00b7ben", "sind", "vom", "Wein\u00b7stock", "ab\u00b7ge\u00b7ris\u00b7sen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "ART", "NN", "VAFIN", "APPRART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Des Oelbaums fruchtbar Stamm wirfft seine Bl\u00fcthen ab.", "tokens": ["Des", "O\u00b7el\u00b7baums", "frucht\u00b7bar", "Stamm", "wirfft", "sei\u00b7ne", "Bl\u00fc\u00b7then", "ab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-++-+-+-+-+", "measure": "unknown.measure.septa"}, "line.31": {"text": "Die Rosen m\u00fcssen sich in erster Knospeschliessen/", "tokens": ["Die", "Ro\u00b7sen", "m\u00fcs\u00b7sen", "sich", "in", "ers\u00b7ter", "Knos\u00b7pe\u00b7schlies\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PRF", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Die Nelcken sch\u00f6nster Zucht verfallen in das Grab/", "tokens": ["Die", "Nel\u00b7cken", "sch\u00f6ns\u00b7ter", "Zucht", "ver\u00b7fal\u00b7len", "in", "das", "Grab", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVINF", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Denn bricht uns unser Hertz/ denn ist der Geist zerknicket.", "tokens": ["Denn", "bricht", "uns", "un\u00b7ser", "Hertz", "/", "denn", "ist", "der", "Geist", "zer\u00b7kni\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN", "$(", "KON", "VAFIN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Wiewol des H\u00f6chsten Mund hinwieder Trost ausgiebt/", "tokens": ["Wie\u00b7wol", "des", "H\u00f6chs\u00b7ten", "Mund", "hin\u00b7wie\u00b7der", "Trost", "aus\u00b7giebt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "ADV", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Da\u00df die Verstorbenen sind vieler Qual entr\u00fccket/", "tokens": ["Da\u00df", "die", "Ver\u00b7stor\u00b7be\u00b7nen", "sind", "vie\u00b7ler", "Qual", "ent\u00b7r\u00fc\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VAFIN", "PIAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Da\u00df seine Allmacht mehr als Eltern sie geliebt.", "tokens": ["Da\u00df", "sei\u00b7ne", "All\u00b7macht", "mehr", "als", "El\u00b7tern", "sie", "ge\u00b7liebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PIAT", "KOKOM", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Drumb hat er fort geeilt im Morgen erster Jahre", "tokens": ["Drumb", "hat", "er", "fort", "ge\u00b7eilt", "im", "Mor\u00b7gen", "ers\u00b7ter", "Jah\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VAFIN", "PPER", "PTKVZ", "VVPP", "APPRART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Als sich des Lebens-Tag mit g\u00fcldnen Blicken wie\u00df/", "tokens": ["Als", "sich", "des", "Le\u00b7bens\u00b7Tag", "mit", "g\u00fcld\u00b7nen", "Bli\u00b7cken", "wie\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Drumb legt\u2019 er sie noch jung voll Unschuld auff die Bahre/", "tokens": ["Drumb", "legt'", "er", "sie", "noch", "jung", "voll", "Un\u00b7schuld", "auff", "die", "Bah\u00b7re", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PPER", "ADV", "ADJD", "ADJD", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Eh\u2019 als sie angesteckt der L\u00e4ster-Gifft und Dr\u00fc\u00df.", "tokens": ["Eh'", "als", "sie", "an\u00b7ge\u00b7steckt", "der", "L\u00e4s\u00b7ter\u00b7Gifft", "und", "Dr\u00fc\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOUS", "PPER", "VVPP", "ART", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Jeschneller wir den Fu\u00df aus diesem Welt-Krei\u00df setzen/", "tokens": ["Je\u00b7schnel\u00b7ler", "wir", "den", "Fu\u00df", "aus", "die\u00b7sem", "Welt\u00b7Krei\u00df", "set\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ART", "NN", "APPR", "PDAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Je gr\u00f6sser der Gewinn/ je seliger der Tod:", "tokens": ["Je