{"textgrid.poem.38569": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "An eine Leidende", "genre": "verse", "period": "N.A.", "pub_year": 1796, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auch f\u00fcr dich, du liebe Hofnungslose,", "tokens": ["Auch", "f\u00fcr", "dich", ",", "du", "lie\u00b7be", "Hof\u00b7nungs\u00b7lo\u00b7se", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ob dein Fu\u00df auch izt im Dunkel irrt,", "tokens": ["Ob", "dein", "Fu\u00df", "auch", "izt", "im", "Dun\u00b7kel", "irrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Bl\u00fcht im Hain der Zukunft manche Rose", "tokens": ["Bl\u00fcht", "im", "Hain", "der", "Zu\u00b7kunft", "man\u00b7che", "Ro\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPRART", "NN", "ART", "NN", "PIAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Die kein Erdensturm entbl\u00e4ttern wird.", "tokens": ["Die", "kein", "Er\u00b7dens\u00b7turm", "ent\u00b7bl\u00e4t\u00b7tern", "wird", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVINF", "VAFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Wenn sich je zu jenen Seligkeiten", "tokens": ["Wenn", "sich", "je", "zu", "je\u00b7nen", "Se\u00b7lig\u00b7kei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "ADV", "APPR", "PDAT", "NN"], 