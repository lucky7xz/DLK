{"textgrid.poem.46870": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[du glaubtest l\u00e4ngst dich vorbereitet]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du glaubtest l\u00e4ngst dich vorbereitet", "tokens": ["Du", "glaub\u00b7test", "l\u00e4ngst", "dich", "vor\u00b7be\u00b7rei\u00b7tet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PPER", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit willigem Entsagen;", "tokens": ["Mit", "wil\u00b7li\u00b7gem", "Ent\u00b7sa\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und nun das Schicksal dich bestreitet,", "tokens": ["Und", "nun", "das", "Schick\u00b7sal", "dich", "be\u00b7strei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So mu\u00dft du dennoch klagen.", "tokens": ["So", "mu\u00dft", "du", "den\u00b7noch", "kla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der K\u00e4mpfer war mit Muth gebr\u00fcstet,", "tokens": ["Der", "K\u00e4mp\u00b7fer", "war", "mit", "Muth", "ge\u00b7br\u00fcs\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und glaubte sich, wie gut! ger\u00fcstet;", "tokens": ["Und", "glaub\u00b7te", "sich", ",", "wie", "gut", "!", "ge\u00b7r\u00fcs\u00b7tet", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "$,", "PWAV", "ADJD", "$.", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Doch wenn hervor der Schrecken schreitet", "tokens": ["Doch", "wenn", "her\u00b7vor", "der", "Schre\u00b7cken", "schrei\u00b7tet"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ADV", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Des Kampfes, wird er zagen,", "tokens": ["Des", "Kamp\u00b7fes", ",", "wird", "er", "za\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VAFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Was hilft's auch, die Gedanken lenken", "tokens": ["Was", "hilft's", "auch", ",", "die", "Ge\u00b7dan\u00b7ken", "len\u00b7ken"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ADV", "$,", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Auf das im voraus, und sie senken", "tokens": ["Auf", "das", "im", "vo\u00b7raus", ",", "und", "sie", "sen\u00b7ken"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "APPRART", "PTKVZ", "$,", "KON", "PPER", "VVINF"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.11": {"text": "In das, was gar sich nicht l\u00e4\u00dft denken,", "tokens": ["In", "das", ",", "was", "gar", "sich", "nicht", "l\u00e4\u00dft", "den\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDS", "$,", "PRELS", "ADV", "PRF", "PTKNEG", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Eh' man es mu\u00df ertragen.", "tokens": ["Eh'", "man", "es", "mu\u00df", "er\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Du glaubtest l\u00e4ngst dich vorbereitet", "tokens": ["Du", "glaub\u00b7test", "l\u00e4ngst", "dich", "vor\u00b7be\u00b7rei\u00b7tet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PPER", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit willigem Entsagen;", "tokens": ["Mit", "wil\u00b7li\u00b7gem", "Ent\u00b7sa\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und nun das Schicksal dich bestreitet,", "tokens": ["Und", "nun", "das", "Schick\u00b7sal", "dich", "be\u00b7strei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So mu\u00dft du dennoch klagen.", "tokens": ["So", "mu\u00dft", "du", "den\u00b7noch", "kla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der K\u00e4mpfer war mit Muth gebr\u00fcstet,", "tokens": ["Der", "K\u00e4mp\u00b7fer", "war", "mit", "Muth", "ge\u00b7br\u00fcs\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und glaubte sich, wie gut! ger\u00fcstet;", "tokens": ["Und", "glaub\u00b7te", "sich", ",", "wie", "gut", "!", "ge\u00b7r\u00fcs\u00b7tet", ";"], "token_info": ["word", 