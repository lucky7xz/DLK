{"textgrid.poem.65637": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Griechenlands Hoffnung", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Br\u00fcder, schaut nicht in die Ferne nach der Fremden Schutz hinaus,", "tokens": ["Br\u00fc\u00b7der", ",", "schaut", "nicht", "in", "die", "Fer\u00b7ne", "nach", "der", "Frem\u00b7den", "Schutz", "hin\u00b7aus", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PTKNEG", "APPR", "ART", "NN", "APPR", "ART", "NN", "NN", "APZR", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Schaut, wenn ihr wollt sicher schauen, nur in euer Herz und Haus.", "tokens": ["Schaut", ",", "wenn", "ihr", "wollt", "si\u00b7cher", "schau\u00b7en", ",", "nur", "in", "eu\u00b7er", "Herz", "und", "Haus", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PPER", "VMFIN", "ADJD", "VVINF", "$,", "ADV", "APPR", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "+--+--+-+-+-+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Findet ihr f\u00fcr eure Freiheit da nicht heilige Gew\u00e4hr,", "tokens": ["Fin\u00b7det", "ihr", "f\u00fcr", "eu\u00b7re", "Frei\u00b7heit", "da", "nicht", "hei\u00b7li\u00b7ge", "Ge\u00b7w\u00e4hr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "ADV", "PTKNEG", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Nun und nimmer, Br\u00fcder, nimmer k\u00f6mmt sie euch von au\u00dfen her.", "tokens": ["Nun", "und", "nim\u00b7mer", ",", "Br\u00fc\u00b7der", ",", "nim\u00b7mer", "k\u00f6mmt", "sie", "euch", "von", "au\u00b7\u00dfen", "her", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KON", "ADV", "$,", "NN", "$,", "ADV", "VVFIN", "PPER", "PPER", "APPR", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.5": {"text": "Selber hast du aufgeladen dir der Knechtschaft schweres Joch,", "tokens": ["Sel\u00b7ber", "hast", "du", "auf\u00b7ge\u00b7la\u00b7den", "dir", "der", "Knecht\u00b7schaft", "schwe\u00b7res", "Joch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVFIN", "PPER", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.6": {"text": "Selber hast du es getragen, und du tr\u00fcgst es heute noch,", "tokens": ["Sel\u00b7ber", "hast", "du", "es", "ge\u00b7tra\u00b7gen", ",", "und", "du", "tr\u00fcgst", "es", "heu\u00b7te", "noch", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "VVPP", "$,", "KON", "PPER", "VVFIN", "PPER", "ADV", "ADV", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.7": {"text": "H\u00e4ttest du darauf gewartet, hochgelobtes Griechenland,", "tokens": ["H\u00e4t\u00b7test", "du", "da\u00b7rauf", "ge\u00b7war\u00b7tet", ",", "hoch\u00b7ge\u00b7lob\u00b7tes", "Grie\u00b7chen\u00b7land", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PAV", "VVPP", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.8": {"text": "Da\u00df es dir vom Nacken sollte heben eine fremde Hand.", "tokens": ["Da\u00df", "es", "dir", "vom", "Na\u00b7cken", "soll\u00b7te", "he\u00b7ben", "ei\u00b7ne", "frem\u00b7de", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "APPRART", "NN", "VMFIN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.9": {"text": "Selber mu\u00dft du f\u00fcr dich k\u00e4mpfen, wie du selber dich befreit,", "tokens": ["Sel\u00b7ber", "mu\u00dft", "du", "f\u00fcr", "dich", "k\u00e4mp\u00b7fen", ",", "wie", "du", "sel\u00b7ber", "dich", "be\u00b7freit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "APPR", "PPER", "VVFIN", "$,", "PWAV", "PPER", "ADV", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.10": {"text": "Dein die Schuld und dein die Bu\u00dfe, dein die Palme nach dem Streit.", "tokens": ["Dein", "die", "Schuld", "und", "dein", "die", "Bu\u00b7\u00dfe", ",", "dein", "die", "Pal\u00b7me", "nach", "dem", "Streit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ART", "NN", "KON", "PPOSAT", "ART", "NN", "$,", "PPOSAT", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.11": {"text": "Viele werden dich beklagen, Viele dir Gebete weihn,", "tokens": ["Vie\u00b7le", "wer\u00b7den", "dich", "be\u00b7kla\u00b7gen", ",", "Vie\u00b7le", "dir", "Ge\u00b7be\u00b7te", "weihn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPER", "VVFIN", "$,", "PIS", "PPER", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.12": {"text": "Viele sich f\u00fcr dich verwenden, Viele deine Rather sein \u2013", "tokens": ["Vie\u00b7le", "sich", "f\u00fcr", "dich", "ver\u00b7wen\u00b7den", ",", "Vie\u00b7le", "dei\u00b7ne", "Ra\u00b7ther", "sein", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PRF", "APPR", "PPER", "VVINF", "$,", "PIS", "PPOSAT", "NN", "VAINF", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.13": {"text": "Hoffst du mehr? Bau' aus ", "tokens": ["Hoffst", "du", "mehr", "?", "Bau'", "aus"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "$.", "NN", "APPR"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.14": {"text": "Da\u00df der Grund, auf dem sie ruhet, nicht den Bau zu Tr\u00fcmmern bricht.", "tokens": ["Da\u00df", "der", "Grund", ",", "auf", "dem", "sie", "ru\u00b7het", ",", "nicht", "den", "Bau", "zu", "Tr\u00fcm\u00b7mern", "bricht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "APPR", "PRELS", "PPER", "VVFIN", "$,", "PTKNEG", "ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.15": {"text": "Deiner alten Freiheit Ehre ist der neuen Welt gerecht,", "tokens": ["Dei\u00b7ner", "al\u00b7ten", "Frei\u00b7heit", "Eh\u00b7re", "ist", "der", "neu\u00b7en", "Welt", "ge\u00b7recht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "NN", "VAFIN", "ART", "ADJA", "NN", "ADJD", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.16": {"text": "Denn der Freie schl\u00e4ft im Grabe so geduldig, wie der Knecht.", "tokens": ["Denn", "der", "Frei\u00b7e", "schl\u00e4ft", "im", "Gra\u00b7be", "so", "ge\u00b7dul\u00b7dig", ",", "wie", "der", "Knecht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPRART", "NN", "ADV", "ADJD", "$,", "PWAV", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.17": {"text": "Lege reuig deine Waffen nieder vor des T\u00fcrken Thron,", "tokens": ["Le\u00b7ge", "reu\u00b7ig", "dei\u00b7ne", "Waf\u00b7fen", "nie\u00b7der", "vor", "des", "T\u00fcr\u00b7ken", "Thron", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PPOSAT", "NN", "PTKVZ", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.18": {"text": "Beuge friedlich deinen Nacken zu dem alten Sklavenfrohn:", "tokens": ["Beu\u00b7ge", "fried\u00b7lich", "dei\u00b7nen", "Na\u00b7cken", "zu", "dem", "al\u00b7ten", "Skla\u00b7ven\u00b7frohn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PPOSAT", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.19": {"text": "Dann, dann magst du sicher bauen auf die Macht der Christenheit,", "tokens": ["Dann", ",", "dann", "magst", "du", "si\u00b7cher", "bau\u00b7en", "auf", "die", "Macht", "der", "Chris\u00b7ten\u00b7heit", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "VMFIN", "PPER", "PRF", "VVINF", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.20": {"text": "Dann, dann magst du sicher hoffen, da\u00df der T\u00fcrke dir verzeiht.", "tokens": ["Dann", ",", "dann", "magst", "du", "si\u00b7cher", "hof\u00b7fen", ",", "da\u00df", "der", "T\u00fcr\u00b7ke", "dir", "ver\u00b7zeiht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "VMFIN", "PPER", "PRF", "VVINF", "$,", "KOUS", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.21": {"text": "Ruh' und Friede will Europa \u2013 Warum hast du sie gest\u00f6rt?", "tokens": ["Ruh'", "und", "Frie\u00b7de", "will", "Eu\u00b7ro\u00b7pa", "\u2013", "Wa\u00b7rum", "hast", "du", "sie", "ge\u00b7st\u00f6rt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VMFIN", "NE", "$(", "PWAV", "VAFIN", "PPER", "PPER", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.22": {"text": "Warum mit dem Wahn der Freiheit eigenm\u00e4chtig dich beth\u00f6rt?", "tokens": ["Wa\u00b7rum", "mit", "dem", "Wahn", "der", "Frei\u00b7heit", "ei\u00b7gen\u00b7m\u00e4ch\u00b7tig", "dich", "be\u00b7th\u00f6rt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "ART", "NN", "ADJD", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.23": {"text": "Hoff' auf keines Herren H\u00fclfe gegen eines Herren Frohn,", "tokens": ["Hoff'", "auf", "kei\u00b7nes", "Her\u00b7ren", "H\u00fcl\u00b7fe", "ge\u00b7gen", "ei\u00b7nes", "Her\u00b7ren", "Frohn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PIAT", "NN", "NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.24": {"text": "Auch des T\u00fcrkenkaisers Polster nennt Europa einen Thron.", "tokens": ["Auch", "des", "T\u00fcr\u00b7ken\u00b7kai\u00b7sers", "Pols\u00b7ter", "nennt", "Eu\u00b7ro\u00b7pa", "ei\u00b7nen", "Thron", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NN", "VVFIN", "NE", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.25": {"text": "Hellas, wohin schaut dein Auge? \u2013 Sohn, ich schau' empor zu Gott \u2013", "tokens": ["Hel\u00b7las", ",", "wo\u00b7hin", "schaut", "dein", "Au\u00b7ge", "?", "\u2013", "Sohn", ",", "ich", "schau'", "em\u00b7por", "zu", "Gott", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "VVFIN", "PPOSAT", "NN", "$.", "$(", "NN", "$,", "PPER", "VVFIN", "PTKVZ", "APPR", "NN", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.26": {"text": "Gott, mein Trost in Schuld und Bu\u00dfe, Gott, mein Hort in Kampf und Tod!", "tokens": ["Gott", ",", "mein", "Trost", "in", "Schuld", "und", "Bu\u00b7\u00dfe", ",", "Gott", ",", "mein", "Hort", "in", "Kampf", "und", "Tod", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "APPR", "NN", "KON", "NN", "$,", "NN", "$,", "PPOSAT", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.2": {"line.1": {"text": "Br\u00fcder, schaut nicht in die Ferne nach der Fremden Schutz hinaus,", "tokens": ["Br\u00fc\u00b7der", ",", "schaut", "nicht", "in", "die", "Fer\u00b7ne", "nach", "der", "Frem\u00b7den", "Schutz", "hin\u00b7aus", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PTKNEG", "APPR", "ART", "NN", "APPR", "ART", "NN", "NN", "APZR", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Schaut, wenn ihr wollt sicher schauen, nur in euer Herz und Haus.", "tokens": ["Schaut", ",", "wenn", "ihr", "wollt", "si\u00b7cher", "schau\u00b7en", ",