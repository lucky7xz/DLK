{"textgrid.poem.32648": {"metadata": {"author": {"name": "Pet\u00f6fi, S\u00e1ndor", "birth": "N.A.", "death": "N.A."}, "title": "Glatt ist der Schnee ...", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Glatt ist der Schnee, der Schlitten fliegt,", "tokens": ["Glatt", "ist", "der", "Schnee", ",", "der", "Schlit\u00b7ten", "fliegt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zur Stra\u00dfe, wo die Kirche liegt,", "tokens": ["Zur", "Stra\u00b7\u00dfe", ",", "wo", "die", "Kir\u00b7che", "liegt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Getraut wird dort mein Liebchen hold,", "tokens": ["Ge\u00b7traut", "wird", "dort", "mein", "Lieb\u00b7chen", "hold", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ADV", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nur \u2013 weil es andre so gewollt.", "tokens": ["Nur", "\u2013", "weil", "es", "and\u00b7re", "so", "ge\u00b7wollt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "KOUS", "PPER", "PIS", "ADV", "VMPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der Schnee wollt' ich jetzt selber sein,", "tokens": ["Der", "Schnee", "wollt'", "ich", "jetzt", "sel\u00b7ber", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "ADV", "ADV", "VAINF", "$,