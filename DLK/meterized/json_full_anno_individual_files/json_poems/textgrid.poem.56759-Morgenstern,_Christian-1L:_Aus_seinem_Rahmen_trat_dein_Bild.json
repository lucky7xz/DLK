{"textgrid.poem.56759": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Aus seinem Rahmen trat dein Bild", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aus seinem Rahmen trat dein Bild", "tokens": ["Aus", "sei\u00b7nem", "Rah\u00b7men", "trat", "dein", "Bild"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "und schlang den Arm mir ums Genick \u2013", "tokens": ["und", "schlang", "den", "Arm", "mir", "ums", "Ge\u00b7nick", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PPER", "APPRART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "und, eingewurzelt Blick in Blick,", "tokens": ["und", ",", "ein\u00b7ge\u00b7wur\u00b7zelt", "Blick", "in", "Blick", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VVFIN", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "durchgingen wir ein fremd Gefild ...", "tokens": ["durch\u00b7gin\u00b7gen", "wir", "ein", "fremd", "Ge\u00b7fild", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJD", "NN", "$("], "meter": "-+--++-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Und gingen stumm und unverwandt", "tokens": ["Und", "gin\u00b7gen", "stumm", "und", "un\u00b7ver\u00b7wandt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "und tranken unsrer Seelen Glanz", "tokens": ["und", "tran\u00b7ken", "uns\u00b7rer", "See\u00b7len", "Glanz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "und wurden ", "tokens": ["und", "wur\u00b7den"], "token_info": ["word", "word"], "pos": ["KON", "VAFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "und f\u00fchlten, was wir nie gekannt ...", "tokens": ["und", "f\u00fchl\u00b7ten", ",", "was", "wir", "nie", "ge\u00b7kannt", "..."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PRELS", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Da schlug ein L\u00e4rm an unser Ohr \u2013", "tokens": ["Da", "schlug", "ein", "L\u00e4rm", "an", "un\u00b7ser", "Ohr", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "ich sprach ein Wort \u2013 du fuhrst zur\u00fcck \u2013.", "tokens": ["ich", "sprach", "ein", "Wort", "\u2013", "du", "fuhrst", "zu\u00b7r\u00fcck", "\u2013", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$(", "PPER", "VVFIN", "PTKVZ", "$(", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zerflossen war das kurze Gl\u00fcck,", "tokens": ["Zer\u00b7flos\u00b7sen", "war", "das", "kur\u00b7ze", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "und alles wieder wie zuvor.", "tokens": ["und", "al\u00b7les", "wie\u00b7der", "wie", "zu\u00b7vor", "."], "token_info": ["word", "word", "word", "word", "word", "punct