{"dta.poem.19745": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Wo ' s schneiet rothe Rosen,  \n Da regnet ' s Thr\u00e4nen drein .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519168", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wohl heute noch und Morgen,               ", "tokens": ["Wohl", "heu\u00b7te", "noch", "und", "Mor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da bleibe ich bei dir;", "tokens": ["Da", "blei\u00b7be", "ich", "bei", "dir", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wenn aber k\u00f6mmt der dritte Tag,", "tokens": ["Wenn", "a\u00b7ber", "k\u00f6mmt", "der", "drit\u00b7te", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So mu\u00df ich fort von hier.", "tokens": ["So", "mu\u00df", "ich", "fort", "von", "hier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PTKVZ", "APPR", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wann k\u00f6mmst du aber wieder,", "tokens": ["Wann", "k\u00f6mmst", "du", "a\u00b7ber", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Herzallerliebster mein;", "tokens": ["Her\u00b7zal\u00b7ler\u00b7liebs\u00b7ter", "mein", ";"], "token_info": ["word", "word", "punct"], "pos": ["NN", "PPOSAT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und brichst die rothen Rosen,", "tokens": ["Und", "brichst", "die", "ro\u00b7then", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und trinkst den k\u00fchlen Wein?", "tokens": ["Und", "trinkst", "den", "k\u00fch\u00b7len", "Wein", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wenns schneiet rothe Rosen,", "tokens": ["Wenns", "schnei\u00b7et", "ro\u00b7the", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wenns regnet k\u00fchlen Wein;", "tokens": ["Wenns", "reg\u00b7net", "k\u00fch\u00b7len", "Wein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So lang sollst du noch harren,", "tokens": ["So", "lang", "sollst", "du", "noch", "har\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Herzallerliebste mein.", "tokens": ["Her\u00b7zal\u00b7ler\u00b7liebs\u00b7te", "mein", "."], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ging sie ins Vaters G\u00e4rtelein,", "tokens": ["Ging", "sie", "ins", "Va\u00b7ters", "G\u00e4r\u00b7tel\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Legt nieder sich, schlief ein;", "tokens": ["Legt", "nie\u00b7der", "sich", ",", "schlief", "ein", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "PRF", "$,", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da tr\u00e4umet ihr ein Tr\u00e4umelein,", "tokens": ["Da", "tr\u00e4u\u00b7met", "ihr", "ein", "Tr\u00e4u\u00b7me\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wies regnet k\u00fchlen Wein.", "tokens": ["Wies", "reg\u00b7net", "k\u00fch\u00b7len", "Wein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und als sie da erwachte,", "tokens": ["Und", "als", "sie", "da", "er\u00b7wach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da war es lauter Nichts;", "tokens": ["Da", "war", "es", "lau\u00b7ter", "Nichts", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da bl\u00fchten wohl die Rosen,", "tokens": ["Da", "bl\u00fch\u00b7ten", "wohl", "die", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und bl\u00fchten \u00fcber sie.", "tokens": ["Und", "bl\u00fch\u00b7ten", "\u00fc\u00b7ber", "sie", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Ein Haus th\u00e4t sie sich bauen,", "tokens": ["Ein", "Haus", "th\u00e4t", "sie", "sich", "bau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Von lauter gr\u00fcnem Klee;", "tokens": ["Von", "lau\u00b7ter", "gr\u00fc\u00b7nem", "Klee", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Th\u00e4t aus zum Himmel schauen,", "tokens": ["Th\u00e4t", "aus", "zum", "Him\u00b7mel", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wohl nach dem Rosenschnee.", "tokens": ["Wohl", "nach", "dem", "Ro\u00b7sen\u00b7schnee", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$."], "meter": "---+-+", "measure": "unknown.measure.di"}}, "stanza.7": {"line.1": {"text": "Mit gelb Wachs th\u00e4t sies decken,", "tokens": ["Mit", "gelb", "Wachs", "th\u00e4t", "sies", "de\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "NN", "VVFIN", "PIS", "VVINF", "$,"], "meter": "-++--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Mit gelber Lielie rein,", "tokens": ["Mit", "gel\u00b7ber", "Lie\u00b7lie", "rein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df sie sich k\u00f6nnt verstecken,", "tokens": ["Da\u00df", "sie", "sich", "k\u00f6nnt", "ver\u00b7ste\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wenns regnet k\u00fchlen Wein.", "tokens": ["Wenns", "reg\u00b7net", "k\u00fch\u00b7len", "Wein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Und als das Haus gebauet war,", "tokens": ["Und", "als", "das", "Haus", "ge\u00b7bau\u00b7et", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Trank sie den Herrgotts Wein,", "tokens": ["Trank", "sie", "den", "Herr\u00b7gotts", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Ein Rosenkr\u00e4nzlein in der Hand,", "tokens": ["Ein", "Ro\u00b7sen\u00b7kr\u00e4nz\u00b7lein", "in", "der", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Schlief sie darinnen ein.", "tokens": ["Schlief", "sie", "da\u00b7rin\u00b7nen", "ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Der Knabe kehrt zur\u00fccke,", "tokens": ["Der", "Kna\u00b7be", "kehrt", "zu\u00b7r\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Geht zu dem Garten ein,", "tokens": ["Geht", "zu", "dem", "Gar\u00b7ten", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Tr\u00e4gt einen Kranz von Rosen,", "tokens": ["Tr\u00e4gt", "ei\u00b7nen", "Kranz", "von", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und einen Becher Wein.", "tokens": ["Und", "ei\u00b7nen", "Be\u00b7cher", "Wein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Hat mit dem Fu\u00df gesto\u00dfen", "tokens": ["Hat", "mit", "dem", "Fu\u00df", "ge\u00b7sto\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "ART", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wohl an das H\u00fcgelein,", "tokens": ["Wohl", "an", "das", "H\u00fc\u00b7ge\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er fiel, da schneit' es Rosen,", "tokens": ["Er", "fiel", ",", "da", "schneit'", "es", "Ro\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da regnets k\u00fchlen Wein.", "tokens": ["Da", "reg\u00b7nets", "k\u00fch\u00b7len", "Wein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}