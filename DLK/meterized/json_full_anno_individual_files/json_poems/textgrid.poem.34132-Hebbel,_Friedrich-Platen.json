{"textgrid.poem.34132": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Platen", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vieles hast du gethan, man soll es mit Liebe dir danken,", "tokens": ["Vie\u00b7les", "hast", "du", "ge\u00b7than", ",", "man", "soll", "es", "mit", "Lie\u00b7be", "dir", "dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPER", "VVPP", "$,", "PIS", "VMFIN", "PPER", "APPR", "NN", "PPER", "VVINF", "$,"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Hast der \u00e4u\u00dferen Form streng, wie kein Zweiter, gen\u00fcgt,", "tokens": ["Hast", "der", "\u00e4u\u00b7\u00dfe\u00b7ren", "Form", "streng", ",", "wie", "kein", "Zwei\u00b7ter", ",", "ge\u00b7n\u00fcgt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "VVFIN", "$,", "PWAV", "PIAT", "NN", "$,", "VVFIN", "$,"], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Hast die inn're erkannt und alle Reifen der Sprache,", "tokens": ["Hast", "die", "inn'\u00b7re", "er\u00b7kannt", "und", "al\u00b7le", "Rei\u00b7fen", "der", "Spra\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "VVPP", "KON", "PIAT", "NN", "ART", "NN", "$,"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Welche der Leichtsinn sprengt, wieder zusammen geschwei\u00dft.", "tokens": ["Wel\u00b7che", "der", "Leicht\u00b7sinn", "sprengt", ",", "wie\u00b7der", "zu\u00b7sam\u00b7men", "ge\u00b7schwei\u00dft", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVFIN", "$,", "ADV", "ADV", "VVPP", "$."], "meter": "+--+-+-+-+--+", "measure": "iambic.hexa.invert"}, "line.5": {"text": "Eines fehlt dir jedoch, die sanfte Wallung des Lebens,", "tokens": ["Ei\u00b7nes", "fehlt", "dir", "je\u00b7doch", ",", "die", "sanf\u00b7te", "Wal\u00b7lung", "des", "Le\u00b7bens", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADV", "$,", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+--+-+--+-", "measure": "hexameter"}, "line.6": {"text": "Die in ein reizendes Spiel gaukelnder Willk\u00fcr den Ernst", "tokens": ["Die", "in", "ein", "rei\u00b7zen\u00b7des", "Spiel", "gau\u00b7keln\u00b7der", "Will\u00b7k\u00fcr", "den", "Ernst"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.7": {"text": "Des Gesetzes verwandelt und das im Tiefsten Gebund'ne", "tokens": ["Des", "Ge\u00b7set\u00b7zes", "ver\u00b7wan\u00b7delt", "und", "das", "im", "Tiefs\u00b7ten", "Ge\u00b7bun\u00b7d'\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVPP", "KON", "PDS", "APPRART", "ADJA", "NN"], "meter": "--+--+--+-+--++-", "measure": "anapaest.tri.plus"}, "line.8": {"text": "So weit l\u00f6s't, bis es scheint, da\u00df es sich selbst nur gehorcht.", "tokens": ["So", "weit", "l\u00f6s't", ",", "bis", "es", "scheint", ",", "da\u00df", "es", "sich", "selbst", "nur", "ge\u00b7horcht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "$,", "KOUS", "PPER", "VVFIN", "$,", "KOUS", "PPER", "PRF", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Dennoch verschmilzt nur die\u00df die \u00e4u\u00dfere Form mit der innern,", "tokens": ["Den\u00b7noch", "ver\u00b7schmilzt", "nur", "die\u00df", "die", "\u00e4u\u00b7\u00dfe\u00b7re", "Form", "mit", "der", "in\u00b7nern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PDS", "ART", "ADJA", "NN", "APPR", "ART", "ADJA", "$,"], "meter": "-+-+-+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Und man erreicht es nur so, da\u00df die Gebilde der Kunst", "tokens": ["Und", "man", "er\u00b7reicht", "es", "nur", "so", ",", "da\u00df", "die", "Ge\u00b7bil\u00b7de", "der", "Kunst"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "VVFIN", "PPER", "ADV", "ADV", "$,", "KOUS", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "Wirken, wie die der Natur, und da\u00df, wie Blumen und B\u00e4ume,", "tokens": ["Wir\u00b7ken", ",", "wie", "die", "der", "Na\u00b7tur", ",", "und", "da\u00df", ",", "wie", "Blu\u00b7men", "und", "B\u00e4u\u00b7me", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ART", "ART", "NN", "$,", "KON", "KOUS", "$,", "PWAV", "NN", "KON", "NN", "$,"], "meter": "---+--+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Keiner sich auch ein Gedicht anders noch denkt, als es ist.", "tokens": ["Kei\u00b7ner", "sich", "auch", "ein", "Ge\u00b7dicht", "an\u00b7ders", "noch", "denkt", ",", "als", "es", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "PRF", "ADV", "ART", "NN", "ADV", "ADV", "VVFIN", "$,", "KOUS", 