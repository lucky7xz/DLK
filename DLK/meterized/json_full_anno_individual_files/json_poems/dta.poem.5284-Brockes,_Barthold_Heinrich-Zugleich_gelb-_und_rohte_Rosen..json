{"dta.poem.5284": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Zugleich gelb- und rohte Rosen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Indem ich j\u00fcngst, in Amts-Gesch\u00e4ften, im fruchtbaren", "tokens": ["In\u00b7dem", "ich", "j\u00fcngst", ",", "in", "Amts\u00b7Ge\u00b7sch\u00e4f\u00b7ten", ",", "im", "frucht\u00b7ba\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "$,", "APPR", "NN", "$,", "APPRART", "ADJA"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Billwerder fahr", "tokens": ["Bill\u00b7wer\u00b7der", "fahr"], "token_info": ["word", "word"], "pos": ["NN", "VVFIN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Und, nebst dem treflichen Stampeel, des Rahts und Ham-", "tokens": ["Und", ",", "nebst", "dem", "tref\u00b7li\u00b7chen", "Stam\u00b7peel", ",", "des", "Rahts", "und", "Ham"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "$,", "APPR", "ART", "ADJA", "NN", "$,", "ART", "NN", "KON", "TRUNC"], "meter": "-+-+---+-+-+", "measure": "unknown.measure.penta"}, "line.4": {"text": "burgs Zier und Ehre,", "tokens": ["burgs", "Zier", "und", "Eh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Des Fr\u00fchlings Wunder-Glantz besehe; werd\u2019 ich von un-", "tokens": ["Des", "Fr\u00fch\u00b7lings", "Wun\u00b7der\u00b7Glantz", "be\u00b7se\u00b7he", ";", "werd'", "ich", "von", "un"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVFIN", "$.", "VAFIN", "PPER", "APPR", "TRUNC"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "gefehr gewahr,", "tokens": ["ge\u00b7fehr", "ge\u00b7wahr", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Wie von bestralten bunten Blumen ein funckelnd gelb-", "tokens": ["Wie", "von", "be\u00b7stral\u00b7ten", "bun\u00b7ten", "Blu\u00b7men", "ein", "fun\u00b7ckelnd", "gelb"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADJA", "ADJA", "NN", "ART", "VVPP", "TRUNC"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "und rohtes Licht", "tokens": ["und", "roh\u00b7tes", "Licht"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Durch dicht-verwachsne Hecken bricht.", "tokens": ["Durch", "dicht\u00b7ver\u00b7wachs\u00b7ne", "He\u00b7cken", "bricht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Wir halten still, und schicken hin, von diesen Blumen,", "tokens": ["Wir", "hal\u00b7ten", "still", ",", "und", "schi\u00b7cken", "hin", ",", "von", "die\u00b7sen", "Blu\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "KON", "VVFIN", "PTKVZ", "$,", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "die wie Kolen,", "tokens": ["die", "wie", "Ko\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "KOKOM", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.12": {"text": "So man erst angefachet, glimmten, uns einige herbey zu", "tokens": ["So", "man", "erst", "an\u00b7ge\u00b7fa\u00b7chet", ",", "glimm\u00b7ten", ",", "uns", "ei\u00b7ni\u00b7ge", "her\u00b7bey", "zu"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "PIS", "ADV", "VVPP", "$,", "VVFIN", "$,", "PPER", "PIS", "ADV", "APPR"], "meter": "-+-+-+-+--+--+-+", "measure": "iambic.septa.relaxed"}, "line.13": {"text": "holen,", "tokens": ["ho\u00b7len", ","], "token_info": ["word", "punct"], "pos": ["VVINF", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.14": {"text": "Die uns der Land-Mann willig gab;", "tokens": ["Die", "uns", "der", "Lan\u00b7dMann", "wil\u00b7lig", "gab", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Er schnitt mit seinem schnellen Messer verschiedne grosse", "tokens": ["Er", "schnitt", "mit", "sei\u00b7nem", "schnel\u00b7len", "Mes\u00b7ser", "ver\u00b7schied\u00b7ne", "gros\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.16": {"text": "Str\u00e4ucher ab.", "tokens": ["Str\u00e4u\u00b7cher", "ab", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKVZ", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.17": {"text": "Wie man uns nun dieselbe brachte, erschracken wir f\u00fcr", "tokens": ["Wie", "man", "uns", "nun", "die\u00b7sel\u00b7be", "brach\u00b7te", ",", "er\u00b7schra\u00b7cken", "wir", "f\u00fcr"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "PIS", "PPER", "ADV", "PDAT", "VVFIN", "$,", "VVFIN", "PPER", "APPR"], "meter": "-+--+--+--+--+", "measure": "amphibrach.penta.plus"}, "line.18": {"text": "neuer Freude", "tokens": ["neu\u00b7er", "Freu\u00b7de"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.19": {"text": "Bey diesem unverhoften Blick, und f\u00fcr Verwundrung,", "tokens": ["Bey", "die\u00b7sem", "un\u00b7ver\u00b7hof\u00b7ten", "Blick", ",", "und", "f\u00fcr", "Ver\u00b7wund\u00b7rung", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "alle beide,", "tokens": ["al\u00b7le", "bei\u00b7de", ","], "token_info": ["word", "word", "punct"], "pos": ["PIAT", "PIS", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.21": {"text": "Indem wir eine neue Art von Rosen, welche wundersch\u00f6n", "tokens": ["In\u00b7dem", "wir", "ei\u00b7ne", "neu\u00b7e", "Art", "von", "Ro\u00b7sen", ",", "wel\u00b7che", "wun\u00b7der\u00b7sch\u00f6n"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "APPR", "NE", "$,", "PRELS", "ADJD"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.22": {"text": "So wol an Form, als Farb und Glantz, und welche wir", "tokens": ["So", "wol", "an", "Form", ",", "als", "Farb", "und", "Glantz", ",", "und", "wel\u00b7che", "wir"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "NN", "$,", "KOUS", "NN", "KON", "NN", "$,", "KON", "PRELS", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "noch nie gesehn", "tokens": ["noch", "nie", "ge\u00b7sehn"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADV", "VVPP"], "meter": "-+-+", "measure": "iambic.di"}, "line.24": {"text": "Recht ungemein ger\u00fchrt erblickten. Den roth- und weissen", "tokens": ["Recht", "un\u00b7ge\u00b7mein", "ge\u00b7r\u00fchrt", "er\u00b7blick\u00b7ten", ".", "Den", "ro\u00b7th", "und", "weis\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "ADV", "VVPP", "VVFIN", "$.", "ART", "TRUNC", "KON", "VVINF"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.25": {"text": "Rosen-Strauch", "tokens": ["Ro\u00b7sen\u00b7Strauch"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.26": {"text": "Die wilden Rosen, gelben Rosen, und andrer Rosen Arten", "tokens": ["Die", "wil\u00b7den", "Ro\u00b7sen", ",", "gel\u00b7ben", "Ro\u00b7sen", ",", "und", "an\u00b7drer", "Ro\u00b7sen", "Ar\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ADJA", "NN", "$,", "KON", "ADJA", "NN", "NN"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.27": {"text": "auch", "tokens": ["auch"], "token_info": ["word"], "pos": ["ADV"], "meter": "-", "measure": "single.down"}, "line.28": {"text": "Hab ich bewundert und beschrieben. Hier, dacht ich, will", "tokens": ["Hab", "ich", "be\u00b7wun\u00b7dert", "und", "be\u00b7schrie\u00b7ben", ".", "Hier", ",", "dacht", "ich", ",", "will"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word"], "pos": ["NN", "PPER", "VVPP", "KON", "VVINF", "$.", "ADV", "$,", "VVFIN", "PPER", "$,", "VMFIN"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.29": {"text": "in neuem Schein", "tokens": ["in", "neu\u00b7em", "Schein"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.30": {"text": "Der grosse Sch\u00f6pffer aller Dinge bewundert und verehret", "tokens": ["Der", "gros\u00b7se", "Sch\u00f6pf\u00b7fer", "al\u00b7ler", "Din\u00b7ge", "be\u00b7wun\u00b7dert", "und", "ver\u00b7eh\u00b7ret"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PIAT", "NN", "VVPP", "KON", "VVFIN"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.31": {"text": "seyn.", "tokens": ["seyn", "."], "token_info": ["word", "punct"], "pos": ["VAINF", "$."], "meter": "+", "measure": "single.up"}}}}}