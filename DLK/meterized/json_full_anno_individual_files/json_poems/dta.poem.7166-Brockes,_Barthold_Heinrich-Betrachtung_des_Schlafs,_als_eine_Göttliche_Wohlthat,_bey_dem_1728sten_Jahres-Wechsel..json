{"dta.poem.7166": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Betrachtung des Schlafs,  \n als  \n eine G\u00f6ttliche Wohlthat,  \n bey dem 1728sten Jahres-Wechsel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ach HERR! ach la\u00df es nicht geschehn,", "tokens": ["Ach", "HeRR", "!", "ach", "la\u00df", "es", "nicht", "ge\u00b7schehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$.", "XY", "VVIMP", "PPER", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df, durch Affecten weggerissen,", "tokens": ["Da\u00df", ",", "durch", "Af\u00b7fec\u00b7ten", "weg\u00b7ge\u00b7ris\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ich \u00fcbereilet, mich im Urtheln mag vergehn!", "tokens": ["Ich", "\u00fc\u00b7be\u00b7rei\u00b7let", ",", "mich", "im", "Ur\u00b7theln", "mag", "ver\u00b7gehn", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PRF", "APPRART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ach gieb mir Wissen und Gewissen!", "tokens": ["Ach", "gieb", "mir", "Wis\u00b7sen", "und", "Ge\u00b7wis\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVIMP", "PPER", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Gieb mir Geduld, bed\u00e4chtlich anzuh\u00f6ren,", "tokens": ["Gieb", "mir", "Ge\u00b7duld", ",", "be\u00b7d\u00e4cht\u00b7lich", "an\u00b7zu\u00b7h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "NN", "$,", "ADJD", "VVIZU", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und la\u00df nie Ungeduld gerechte Klagen st\u00f6hren!", "tokens": ["Und", "la\u00df", "nie", "Un\u00b7ge\u00b7duld", "ge\u00b7rech\u00b7te", "Kla\u00b7gen", "st\u00f6h\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "ADV", "NN", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ach mache mein Ged\u00e4chtni\u00df kr\u00e4fftig!", "tokens": ["Ach", "ma\u00b7che", "mein", "Ge\u00b7d\u00e4cht\u00b7ni\u00df", "kr\u00e4ff\u00b7tig", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ach sch\u00e4rfft mir den denckenden Verstand!", "tokens": ["Ach", "sch\u00e4rfft", "mir", "den", "den\u00b7cken\u00b7den", "Ver\u00b7stand", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.9": {"text": "Ach mache mir das rechte Recht bekannt!", "tokens": ["Ach", "ma\u00b7che", "mir", "das", "rech\u00b7te", "Recht", "be\u00b7kannt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "PPER", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Mein Wille sey nie tr\u00e4g! vielmehr gesch\u00e4fftig!", "tokens": ["Mein", "Wil\u00b7le", "sey", "nie", "tr\u00e4g", "!", "viel\u00b7mehr", "ge\u00b7sch\u00e4ff\u00b7tig", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ADJD", "$.", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "La\u00df mich aus Leidenschafft doch keine frembde Sachen", "tokens": ["La\u00df", "mich", "aus", "Lei\u00b7den\u00b7schafft", "doch", "kei\u00b7ne", "fremb\u00b7de", "Sa\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "APPR", "NN", "ADV", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Zu meiner Sache machen!", "tokens": ["Zu", "mei\u00b7ner", "Sa\u00b7che", "ma\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "La\u00df mich, aus Ungunst, Ha\u00df und Neid,", "tokens": ["La\u00df", "mich", ",", "aus", "Un\u00b7gunst", ",", "Ha\u00df", "und", "Neid", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Am h\u00f6chsten Recht zu starck nicht kleben,", "tokens": ["Am", "h\u00f6chs\u00b7ten", "Recht", "zu", "starck", "nicht", "kle\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "APPR", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Auch niemand etwas Unrechts geben,", "tokens": ["Auch", "nie\u00b7mand", "et\u00b7was", "Un\u00b7rechts", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Aus ungegr\u00fcndeter Barmhertzigkeit!", "tokens": ["Aus", "un\u00b7ge\u00b7gr\u00fcn\u00b7de\u00b7ter", "Barm\u00b7hert\u00b7zig\u00b7keit", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Wenn meine Zung\u2019 ein Urtheil spricht,", "tokens": ["Wenn", "mei\u00b7ne", "Zung'", "ein", "Ur\u00b7theil", "spricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Sey es dem Redlichen erg\u00f6tzlich.", "tokens": ["Sey", "es", "dem", "Red\u00b7li\u00b7chen", "er\u00b7g\u00f6tz\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "ADJD", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.19": {"text": "Ach HERR! es sey mir Dein Gericht,", "tokens": ["Ach", "HeRR", "!", "es", "sey", "mir", "Dein", "Ge\u00b7richt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$.", "PPER", "VAFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Mehr, als den Streitenden, entsetzlich.", "tokens": ["Mehr", ",", "als", "den", "Strei\u00b7ten\u00b7den", ",", "ent\u00b7setz\u00b7lich", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PIS", "$,", "KOUS", "ART", "NN", "$,", "ADJD", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}}}}