{"textgrid.poem.25747": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Seh' ich die Welt aus meinem H\u00e4uschen an,", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Seh' ich die Welt aus meinem H\u00e4uschen an,", "tokens": ["Seh'", "ich", "die", "Welt", "aus", "mei\u00b7nem", "H\u00e4usc\u00b7hen", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Umgeben rund herum von Menschen, die ich liebe,", "tokens": ["Um\u00b7ge\u00b7ben", "rund", "he\u00b7rum", "von", "Men\u00b7schen", ",", "die", "ich", "lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PTKVZ", "APPR", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und inniglich mich wieder lieben: dann", "tokens": ["Und", "in\u00b7nig\u00b7lich", "mich", "wie\u00b7der", "lie\u00b7ben", ":", "dann"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "ADJD", "PPER", "ADV", "VVINF", "$.", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ist mir, o Freund, die Aussicht nicht so tr\u00fcbe.", "tokens": ["Ist", "mir", ",", "o", "Freund", ",", "die", "Aus\u00b7sicht", "nicht", "so", "tr\u00fc\u00b7be", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "FM", "NN", "$,", "ART", "NN", "PTKNEG", "ADV", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Denn, lieber ", "tokens": ["Denn", ",", "lie\u00b7ber"], "token_info": ["word", "punct", "word"], "pos": ["KON", "$,", "ADV"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Ist wahrlich nicht der Mensch. An meinem Rosenbache", "tokens": ["Ist", "wahr\u00b7lich", "nicht", "der", "Mensch", ".", "An", "mei\u00b7nem", "Ro\u00b7sen\u00b7ba\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "ADV", "PTKNEG", "ART", "NN", "$.", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "H\u00e4tt' ich wohl sonst gewi\u00df so oft geweint,", "tokens": ["H\u00e4tt'", "ich", "wohl", "sonst", "ge\u00b7wi\u00df", "so", "oft", "ge\u00b7weint", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "ADV", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Als ich daran mit Weib und Kinde lache.", "tokens": ["Als", "ich", "da\u00b7ran", "mit", "Weib", "und", "Kin\u00b7de", "la\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PAV", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Gesetzt, der Mensch sey solch ein b\u00f6ses Thier,", "tokens": ["Ge\u00b7setzt", ",", "der", "Mensch", "sey", "solch", "ein", "b\u00f6\u00b7ses", "Thier", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "ART", "NN", "VAFIN", "PIAT", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Verfolgen w\u00fcrd' er mich selbst bis in diese Stille,", "tokens": ["Ver\u00b7fol\u00b7gen", "w\u00fcrd'", "er", "mich", "selbst", "bis", "in", "die\u00b7se", "Stil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "PRF", "ADV", "ADV", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und ach! der Ruhe warmen Mantel mir", "tokens": ["Und", "ach", "!", "der", "Ru\u00b7he", "war\u00b7men", "Man\u00b7tel", "mir"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "XY", "$.", "ART", "NN", "ADJA", "NN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Entrei\u00dfen, Freund, in den ich hier mich h\u00fclle.", "tokens": ["Ent\u00b7rei\u00b7\u00dfen", ",", "Freund", ",", "in", "den", "ich", "hier", "mich", "h\u00fcl\u00b7le", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "APPR", "PRELS", "PPER", "ADV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Doch g\u00f6nnt er gern mir diese Einsamkeit,", "tokens": ["Doch", "g\u00f6nnt", "er", "gern", "mir", "die\u00b7se", "Ein\u00b7sam\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "PPER", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Denn ich verlange nichts von seinen kargen H\u00e4nden,", "tokens": ["Denn", "ich", "ver\u00b7lan\u00b7ge", "nichts", "von", "sei\u00b7nen", "kar\u00b7gen", "H\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PIS", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Er aber wei\u00df, wie leicht den letzten Deut,", "tokens": ["Er", "a\u00b7ber", "wei\u00df", ",", "wie", "leicht", "den", "letz\u00b7ten", "Deut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "$,", "PWAV", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Auch ohne Dank, die meinen ihm verschwenden.", "tokens": ["Auch", "oh\u00b7ne", "Dank", ",", "die", "mei\u00b7nen", "ihm", "ver\u00b7schwen\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$,", "PRELS", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Mir ist der Mensch nicht b\u00f6se, sondern schwach;", "tokens": ["Mir", "ist", "der", "Mensch", "nicht", "b\u00f6\u00b7se", ",", "son\u00b7dern", "schwach", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PTKNEG", "ADJD", "$,", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Entkr\u00e4ftet von dem Gift' des Luxus, und verblendet", "tokens": ["Ent\u00b7kr\u00e4f\u00b7tet", "von", "dem", "Gift'", "des", "Lu\u00b7xus", ",", "und", "ver\u00b7blen\u00b7det"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "ART", "NN", "$,", "KON", "VVFIN"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.19": {"text": "Vom Schein' der falschen Ehre. Gib ihm nach!", "tokens": ["Vom", "Schein'", "der", "fal\u00b7schen", "Eh\u00b7re", ".", "Gib", "ihm", "nach", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$.", "VVIMP", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Statt, wider sich, das Schwert ein Cato wendet.", "tokens": ["Statt", ",", "wi\u00b7der", "sich", ",", "das", "Schwert", "ein", "Ca\u00b7to", "wen\u00b7det", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "PRF", "$,", "ART", "NN", "ART", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Ach! bring du selbst auf der Capelle Herd", "tokens": ["Ach", "!", "bring", "du", "selbst", "auf", "der", "Ca\u00b7pel\u00b7le", "Herd"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Nicht alle Freund', o ", "tokens": ["Nicht", "al\u00b7le", "Freund'", ",", "o"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["PTKNEG", "PIAT", "NN", "$,", "FM"], "meter": "-+-+-", "measure": "iambic.di"}, "line.23": {"text": "Best\u00e4ndig, und wie lautres Gold bew\u00e4hrt:", "tokens": ["Be\u00b7st\u00e4n\u00b7dig", ",", "und", "wie", "laut\u00b7res", "Gold", "be\u00b7w\u00e4hrt", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "KON", "PWAV", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "(versuch' es, Freund!) so sey der Mensch dir theuer.", "tokens": ["(", "ver\u00b7such'", "es", ",", "Freund", "!", ")", "so", "sey", "der", "Mensch", "dir", "theu\u00b7er", "."], "token_info": ["punct", "word", "word", "punct", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "$,", "NN", "$.", "$(", "ADV", "VAFIN", "ART", "NN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "Sag' nicht, das Leben sey dir eine Last,", "tokens": ["Sag'", "nicht", ",", "das", "Le\u00b7ben", "sey", "dir", "ei\u00b7ne", "Last", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "$,", "ART", "NN", "VAFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "So lang zur St\u00fctze noch sich meine H\u00e4nd' ihm bieten.", "tokens": ["So", "lang", "zur", "St\u00fct\u00b7ze", "noch", "sich", "mei\u00b7ne", "H\u00e4nd'", "ihm", "bie\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPRART", "NN", "ADV", "PRF", "PPOSAT", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "So lang du hier noch deinen ", "tokens": ["So", "lang", "du", "hier", "noch", "dei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "PPER", "ADV", "ADV", "PPOSAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.28": {"text": "Flieh in die Wildni\u00df nicht, gleich Eremiten.", "tokens": ["Flieh", "in", "die", "Wild\u00b7ni\u00df", "nicht", ",", "gleich", "E\u00b7re\u00b7mi\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PTKNEG", "$,", "ADV", "NN", "$."], "meter": "+--+-++--+-", "measure": "iambic.penta.invert"}, "line.29": {"text": "Zwar ward ich selbst schon l\u00e4ngst ein Eremit,", "tokens": ["Zwar", "ward", "ich", "selbst", "schon", "l\u00e4ngst", "ein", "E\u00b7re\u00b7mit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.30": {"text": "Doch mitten in der Stadt; wer will mich drum verdammen?", "tokens": ["Doch", "mit\u00b7ten", "in", "der", "Stadt", ";", "wer", "will", "mich", "drum", "ver\u00b7dam\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "$.", "PWS", "VMFIN", "PRF", "PAV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Denn was mein Herz als Freund nicht an sich zieht,", "tokens": ["Denn", "was", "mein", "Herz", "als", "Freund", "nicht", "an", "sich", "zieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPOSAT", "NN", "KOUS", "NN", "PTKNEG", "APPR", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.32": {"text": "Das h\u00e4ngt mit ihm durch Faden nur zusammen.", "tokens": ["Das", "h\u00e4ngt", "mit", "ihm", "durch", "Fa\u00b7den", "nur", "zu\u00b7sam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "PPER", "APPR", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.33": {"text": "Und ob Ein solcher zarter Faden rei\u00dft?", "tokens": ["Und", "ob", "Ein", "sol\u00b7cher", "zar\u00b7ter", "Fa\u00b7den", "rei\u00dft", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "PIAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.34": {"text": "Ob Hundert? o! das macht das Auge mir nicht n\u00e4sser;", "tokens": ["Ob", "Hun\u00b7dert", "?", "o", "!", "das", "macht", "das", "Au\u00b7ge", "mir", "nicht", "n\u00e4s\u00b7ser", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$.", "FM", "$.", "PDS", "VVFIN", "ART", "NN", "PPER", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Denn ich bedarf der keines, wie du weist;", "tokens": ["Denn", "ich", "be\u00b7darf", "der", "kei\u00b7nes", ",", "wie", "du", "weist", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "PIS", "$,", "PWAV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.36": {"text": "Bed\u00fcrfen sie auch mein nicht? desto besser!", "tokens": ["Be\u00b7d\u00fcr\u00b7fen", "sie", "auch", "mein", "nicht", "?", "des\u00b7to", "bes\u00b7ser", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PPOSAT", "PTKNEG", "$.", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.37": {"text": "Hast du dich, Freund, mit dicken Seilen, (ach!", "tokens": ["Hast", "du", "dich", ",", "Freund", ",", "mit", "di\u00b7cken", "Sei\u00b7len", ",", "(", "ach", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["VAFIN", "PPER", "PRF", "$,", "NN", "$,", "APPR", "ADJA", "NN", "$,", "$(", "ITJ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.38": {"text": "Gutherzig, doch zu rasch,) an einen Duns gebunden,", "tokens": ["Gu\u00b7ther\u00b7zig", ",", "doch", "zu", "rasch", ",", ")", "an", "ei\u00b7nen", "Duns", "ge\u00b7bun\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADV", "PTKA", "ADJD", "$,", "$(", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Und schleppest du, gleich einem Klotz', ihn nach:", "tokens": ["Und", "schlep\u00b7pest", "du", ",", "gleich", "ei\u00b7nem", "Klotz'", ",", "ihn", "nach", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "ADV", "ART", "NN", "$,", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.40": {"text": "Er wird die Fers' auf jeden Schritt verwunden.", "tokens": ["Er", "wird", "die", "Fer\u00b7s'", "auf", "je\u00b7den", "Schritt", "ver\u00b7wun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "PIAT", "NN", "VVPP", "$."], "meter": "-+----+-+-+-", "measure": "dactylic.init"}, "line.41": {"text": "Wer hat f\u00fcr Freiheit, unser h\u00f6chstes Gut,", "tokens": ["Wer", "hat", "f\u00fcr", "Frei\u00b7heit", ",", "un\u00b7ser", "h\u00f6chs\u00b7tes", "Gut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "APPR", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.42": {"text": "Noch je zu viel gethan, noch je zu viel geboten?", "tokens": ["Noch", "je", "zu", "viel", "ge\u00b7than", ",", "noch", "je", "zu", "viel", "ge\u00b7bo\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PTKA", "PIS", "VVPP", "$,", "ADV", "ADV", "PTKA", "PIS", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "So habe denn auch Alexanders Muth:", "tokens": ["So", "ha\u00b7be", "denn", "auch", "A\u00b7lex\u00b7an\u00b7ders", "Muth", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ADV", "NE", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.44": {"text": "Zerhaue du den oft verschlungnen Knoten!", "tokens": ["Zer\u00b7hau\u00b7e", "du", "den", "oft", "ver\u00b7schlung\u00b7nen", "Kno\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.45": {"text": "Dann sieh den Erdball an, wie ein Spital", "tokens": ["Dann", "sieh", "den", "Erd\u00b7ball", "an", ",", "wie", "ein", "Spi\u00b7tal"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKVZ", "$,", "PWAV", "ART", "NN"], "meter": "-+-+-+--++", "measure": "iambic.penta.relaxed"}, "line.46": {"text": "Voll siecher Narren, Freund, verpflegt von den Gesunden.", "tokens": ["Voll", "sie\u00b7cher", "Nar\u00b7ren", ",", "Freund", ",", "ver\u00b7pflegt", "von", "den", "Ge\u00b7sun\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$,", "NN", "$,", "VVPP", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Sey Arzt darin! So hast du auf einmal,", "tokens": ["Sey", "Arzt", "da\u00b7rin", "!", "So", "hast", "du", "auf", "ein\u00b7mal", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "PAV", "$.", "ADV", "VAFIN", "PPER", "APPR", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.48": {"text": "Was diese Welt gew\u00e4hren kann, gesunden.", "tokens": ["Was", "die\u00b7se", "Welt", "ge\u00b7w\u00e4h\u00b7ren", "kann", ",", "ge\u00b7sun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "PDAT", "NN", "VVINF", "VMFIN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Seh' ich die Welt aus meinem H\u00e4uschen an,", "tokens": ["Seh'", "ich", "die", "Welt", "aus", "mei\u00b7nem", "H\u00e4usc\u00b7hen", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Umgeben rund herum von Menschen, die ich liebe,", "tokens": ["Um\u00b7ge\u00b7ben", "rund", "he\u00b7rum", "von", "Men\u00b7schen", ",", "die", "ich", "lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PTKVZ", "APPR", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und inniglich mich wieder lieben: dann", "tokens": ["Und", "in\u00b7nig\u00b7lich", "mich", "wie\u00b7der", "lie\u00b7ben", ":", "dann"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "ADJD", "PPER", "ADV", "VVINF", "$.", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ist mir, o Freund, die Aussicht nicht so tr\u00fcbe.", "tokens": ["Ist", "mir", ",", "o", "Freund", ",", "die", "Aus\u00b7sicht", "nicht", "so", "tr\u00fc\u00b7be", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "FM", "NN", "$,", "ART", "NN", "PTKNEG", "ADV", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Denn, lieber ", "tokens": ["Denn", ",", "lie\u00b7ber"], "token_info": ["word", "punct", "word"], "pos": ["KON", "$,", "ADV"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Ist wahrlich nicht der Mensch. An meinem Rosenbache", "tokens": ["Ist", "wahr\u00b7lich", "nicht", "der", "Mensch", ".", "An", "mei\u00b7nem", "Ro\u00b7sen\u00b7ba\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "ADV", "PTKNEG", "ART", "NN", "$.", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "H\u00e4tt' ich wohl sonst gewi\u00df so oft geweint,", "tokens": ["H\u00e4tt'", "ich", "wohl", "sonst", "ge\u00b7wi\u00df", "so", "oft", "ge\u00b7weint", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "ADV", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Als ich daran mit Weib und Kinde lache.", "tokens": ["Als", "ich", "da\u00b7ran", "mit", "Weib", "und", "Kin\u00b7de", "la\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PAV", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Gesetzt, der Mensch sey solch ein b\u00f6ses Thier,", "tokens": ["Ge\u00b7setzt", ",", "der", "Mensch", "sey", "solch", "ein", "b\u00f6\u00b7ses", "Thier", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "ART", "NN", "VAFIN", "PIAT", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Verfolgen w\u00fcrd' er mich selbst bis in diese Stille,", "tokens": ["Ver\u00b7fol\u00b7gen", "w\u00fcrd'", "er", "mich", "selbst", "bis", "in", "die\u00b7se", "Stil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "PRF", "ADV", "ADV", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und ach! der Ruhe warmen Mantel mir", "tokens": ["Und", "ach", "!", "der", "Ru\u00b7he", "war\u00b7men", "Man\u00b7tel", "mir"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "XY", "$.", "ART", "NN", "ADJA", "NN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Entrei\u00dfen, Freund, in den ich hier mich h\u00fclle.", "tokens": ["Ent\u00b7rei\u00b