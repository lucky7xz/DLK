 "line.22": {"text": "Nicht alle Freund', o ", "tokens": ["Nicht", "al\u00b7le", "Freund'", ",", "o"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["PTKNEG", "PIAT", "NN", "$,", "FM"], "meter": "-+-+-", "measure": "iambic.di"}, "line.23": {"text": "Best\u00e4ndig, und wie lautres Gold bew\u00e4hrt:", "tokens": ["Be\u00b7st\u00e4n\u00b7dig", ",", "und", "wie", "laut\u00b7res", "Gold", "be\u00b7w\u00e4hrt", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "KON", "PWAV", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "(versuch' es, Freund!) so sey der Mensch dir theuer.", "tokens": ["(", "ver\u00b7such'", "es", ",", "Freund", "!", ")", "so", "sey", "der", "Mensch", "dir", "theu\u00b7er", "."], "token_info": ["punct", "word", "word", "punct", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "$,", "NN", "$.", "$(", "ADV", "VAFIN", "ART", "NN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "Sag' nicht, das Leben sey dir eine Last,", "tokens": ["Sag'", "nicht", ",", "das", "Le\u00b7ben", "sey", "dir", "ei\u00b7ne", "Last", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "$,", "ART", "NN", "VAFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "So lang zur St\u00fctze noch sich meine H\u00e4nd' ihm bieten.", "tokens": ["So", "lang", "zur", "St\u00fct\u00b7ze", "noch", "sich", "mei\u00b7ne", "H\u00e4nd'", "ihm", "bie\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPRART", "NN", "ADV", "PRF", "PPOSAT", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2