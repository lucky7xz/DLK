{"dta.poem.2801": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn nur f\u00fcr fremde Lust dein Wirken ist bestrebt,", "tokens": ["Wenn", "nur", "f\u00fcr", "frem\u00b7de", "Lust", "dein", "Wir\u00b7ken", "ist", "be\u00b7strebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPR", "ADJA", "NN", "PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Kein Frohgef\u00fchl die Brust dabei dir selbst erhebt,", "tokens": ["Kein", "Froh\u00b7ge\u00b7f\u00fchl", "die", "Brust", "da\u00b7bei", "dir", "selbst", "er\u00b7hebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "NN", "PAV", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Auch du nicht deine Lust am Thun der andern hast,", "tokens": ["Auch", "du", "nicht", "dei\u00b7ne", "Lust", "am", "Thun", "der", "an\u00b7dern", "hast", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "PTKNEG", "PPOSAT", "NN", "APPRART", "NN", "ART", "ADJA", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So ist dir, was du thust und sie thun, eine Last.", "tokens": ["So", "ist", "dir", ",", "was", "du", "thust", "und", "sie", "thun", ",", "ei\u00b7ne", "Last", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "PWS", "PPER", "VVFIN", "KON", "PPER", "VVINF", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+++-+", "measure": "unknown.measure.septa"}}, "stanza.3": {"line.1": {"text": "Komm, und mit Heiterkeit den Drang des Lebens w\u00fcrze!", "tokens": ["Komm", ",", "und", "mit", "Hei\u00b7ter\u00b7keit", "den", "Drang", "des", "Le\u00b7bens", "w\u00fcr\u00b7ze", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KON", "APPR", "NN", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nicht \u00fcber Hals und Kopf dumpf in den Wirbel st\u00fcrze!", "tokens": ["Nicht", "\u00fc\u00b7ber", "Hals", "und", "Kopf", "dumpf", "in", "den", "Wir\u00b7bel", "st\u00fcr\u00b7ze", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "NN", "KON", "NN", "ADJD", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Es steht bei dir, da\u00df aus der Welt M\u00fchseligkeit", "tokens": ["Es", "steht", "bei", "dir", ",", "da\u00df", "aus", "der", "Welt", "M\u00fch\u00b7se\u00b7lig\u00b7keit"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "$,", "KOUS", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In jedem Augenblick dir aufbl\u00fch' Seligkeit.", "tokens": ["In", "je\u00b7dem", "Au\u00b7gen\u00b7blick", "dir", "auf\u00b7bl\u00fch'", "Se\u00b7lig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}