{"textgrid.poem.57290": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "1L: Still auf dem Blatt ruhte das Lied, noch erschrocken", "genre": "verse", "period": "N.A.", "pub_year": 1767, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Still auf dem Blatt ruhte das Lied, noch erschrocken", "tokens": ["Still", "auf", "dem", "Blatt", "ruh\u00b7te", "das", "Lied", ",", "noch", "er\u00b7schro\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,", "ADV", "VVINF"], "meter": "-+-+---+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Vor dem Get\u00f6s des Rhapsoden, der es herlas,", "tokens": ["Vor", "dem", "Ge\u00b7t\u00f6s", "des", "Rhap\u00b7so\u00b7den", ",", "der", "es", "her\u00b7las", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Unbekant mit der sanftern Stimme", "tokens": ["Un\u00b7be\u00b7kant", "mit", "der", "sanf\u00b7tern", "Stim\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Laut', und dem volleren Ton.", "tokens": ["Laut'", ",", "und", "dem", "vol\u00b7le\u00b7ren", "Ton", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KON", "ART", "ADJA", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.2": {"line.1": {"text": "Da, wo er schrie, lag ein Homer. Auf den Dreyfuss", "tokens": ["Da", ",", "wo", "er", "schrie", ",", "lag", "ein", "Ho\u00b7mer", ".", "Auf", "den", "Drey\u00b7fuss"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "PWAV", "PPER", "VVFIN", "$,", "VVFIN", "ART", "NE", "$.", "APPR", "ART", "NN"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Setzt' ihn sein Wahn, und verbarg ihm, dass ihm stutzte,", "tokens": ["Setzt'", "ihn", "sein", "Wahn", ",", "und", "ver\u00b7barg", "ihm", ",", "dass", "ihm", "stutz\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$,", "KON", "VVFIN", "PPER", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "Stand der Strom des Gesangs, des Dichters", "tokens": ["Stand", "der", "Strom", "des", "Ge\u00b7sangs", ",", "des", "Dich\u00b7ters"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "ART", "NN", "ART", "NN", "$,", "ART", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Genius zornig entfloh.", "tokens": ["Ge\u00b7nius", "zor\u00b7nig", "ent\u00b7floh", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Aber o lern, S\u00e4ngerin selbst, von Teonens", "tokens": ["A\u00b7ber", "o", "lern", ",", "S\u00e4n\u00b7ge\u00b7rin", "selbst", ",", "von", "Teo\u00b7nens"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KON", "FM", "VVINF", "$,", "NN", "ADV", "$,", "APPR", "NN"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Zaubernden Kunst, wenn dem Inhalt sie wie Wachs schmilzt,", "tokens": ["Zau\u00b7bern\u00b7den", "Kunst", ",", "wenn", "dem", "In\u00b7halt", "sie", "wie", "Wachs", "schmilzt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "KOUS", "ART", "NN", "PPER", "KOKOM", "NN", "VVFIN", "$,"], "meter": "+--+--++--+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "Und der Seele des Liedes gleiche,", "tokens": ["Und", "der", "See\u00b7le", "des", "Lie\u00b7des", "glei\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "ADJA", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Sch\u00f6ne Gespielinnen w\u00e4hlt.", "tokens": ["Sch\u00f6\u00b7ne", "Ge\u00b7spie\u00b7lin\u00b7nen", "w\u00e4hlt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$."], "meter": "+--++-+", "measure": "dactylic.init"}}, "stanza.4": {"line.1": {"text": "H\u00f6rst du, wie sie an der Gewalt des Rhapsoden", "tokens": ["H\u00f6rst", "du", ",", "wie", "sie", "an", "der", "Ge\u00b7walt", "des", "Rhap\u00b7so\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "PPER", "APPR", "ART", "NN", "ART", "NN"], "meter": "+-+-+--+-+--", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "R\u00e4chet das Lied! wie dem Ohre sie es bildet!", "tokens": ["R\u00e4\u00b7chet", "das", "Lied", "!", "wie", "dem", "Oh\u00b7re", "sie", "es", "bil\u00b7det", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$.", "PWAV", "ART", "NN", "PPER", "PPER", "VVFIN", "$."], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "Sind nicht, S\u00e4ngerin, dieser T\u00f6ne", "tokens": ["Sind", "nicht", ",", "S\u00e4n\u00b7ge\u00b7rin", ",", "die\u00b7ser", "T\u00f6\u00b7ne"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["VAFIN", "PTKNEG", "$,", "NN", "$,", "PDAT", "NN"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Wendungen auch Melodie?", "tokens": ["Wen\u00b7dun\u00b7gen", "auch", "Me\u00b7lo\u00b7die", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Ja Melodie innig vertraut mit des Herzens", "tokens": ["Ja", "Me\u00b7lo\u00b7die", "in\u00b7nig", "ver\u00b7traut", "mit", "des", "Her\u00b7zens"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "NN", "ADJD", "ADJD", "APPR", "ART", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Feinstem Gef\u00fchl! nicht die Haltung, wie die Fl\u00f6te", "tokens": ["Feins\u00b7tem", "Ge\u00b7f\u00fchl", "!", "nicht", "die", "Hal\u00b7tung", ",", "wie", "die", "Fl\u00f6\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "PTKNEG", "ART", "NN", "$,", "PWAV", "ART", "NN"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "T\u00f6net, oder wie deine Stimme", "tokens": ["T\u00f6\u00b7net", ",", "o\u00b7der", "wie", "dei\u00b7ne", "Stim\u00b7me"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "KON", "KOKOM", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "\u00dcber die Fl\u00f6te sich hebt.", "tokens": ["\u00dc\u00b7ber", "die", "Fl\u00f6\u00b7te", "sich", "hebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.6": {"line.1": {"text": "Sage, warum bebst du? was st\u00fcrzt dir die Thr\u00e4ne", "tokens": ["Sa\u00b7ge", ",", "wa\u00b7rum", "bebst", "du", "?", "was", "st\u00fcrzt", "dir", "die", "Thr\u00e4\u00b7ne"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PWAV", "VVFIN", "PPER", "$.", "PWS", "VVFIN", "PPER", "ART", "NN"], "meter": "+-+-+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Eilend herab? was bes\u00e4nftigt nun dein Herz dir?", "tokens": ["Ei\u00b7lend", "her\u00b7ab", "?", "was", "be\u00b7s\u00e4nf\u00b7tigt", "nun", "dein", "Herz", "dir", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "$.", "PWS", "VVFIN", "ADV", "PPOSAT", "NN", "PPER", "$."], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "Thats Teone nicht auch? und r\u00fchrt dich", "tokens": ["Thats", "Te\u00b7o\u00b7ne", "nicht", "auch", "?", "und", "r\u00fchrt", "dich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NE", "PTKNEG", "ADV", "$.", "KON", "VVFIN", "PPER"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Etwa der Dichter allein?", "tokens": ["Et\u00b7wa", "der", "Dich\u00b7ter", "al\u00b7lein", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADV", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.7": {"line.1": {"text": "H\u00f6re, f\u00fcr sie dichtet' er! h\u00f6r', auch die kleinste", "tokens": ["H\u00f6\u00b7re", ",", "f\u00fcr", "sie", "dich\u00b7tet'", "er", "!", "h\u00f6r'", ",", "auch", "die", "kleins\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "APPR", "PPER", "VVFIN", "PPER", "$.", "VVFIN", "$,", "ADV", "ART", "ADJA"], "meter": "+-+--+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Kunst des Gesangs ist Teonen nicht verborgen!", "tokens": ["Kunst", "des", "Ge\u00b7sangs", "ist", "Teo\u00b7nen", "nicht", "ver\u00b7bor\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VAFIN", "NN", "PTKNEG", "VVPP", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Folg ihr, wie in des stolzen Rythmus", "tokens": ["Folg", "ihr", ",", "wie", "in", "des", "stol\u00b7zen", "Ry\u00b7th\u00b7mus"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "$,", "PWAV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Tanz sie mit Leichtigkeit schwebt!", "tokens": ["Tanz", "sie", "mit", "Leich\u00b7tig\u00b7keit", "schwebt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.8": {"line.1": {"text": "Pflanze f\u00fcr sie Blumen im Hain an dem Bache,", "tokens": ["Pflan\u00b7ze", "f\u00fcr", "sie", "Blu\u00b7men", "im", "Hain", "an", "dem", "Ba\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "NN", "APPRART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Nossa, dass ich, wenn mit Einklang sie vielleicht einst", "tokens": ["Nos\u00b7sa", ",", "dass", "ich", ",", "wenn", "mit", "Ein\u00b7klang", "sie", "viel\u00b7leicht", "einst"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "KOUS", "PPER", "$,", "KOUS", "APPR", "NN", "PPER", "ADV", "ADV"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Meiner Lieder Gef\u00fchl begleitet,", "tokens": ["Mei\u00b7ner", "Lie\u00b7der", "Ge\u00b7f\u00fchl", "be\u00b7glei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "VVPP", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Kr\u00e4nze Teonen ihr Haar!", "tokens": ["Kr\u00e4n\u00b7ze", "Teo\u00b7nen", "ihr", "Haar", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "PPOSAT", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.9": {"line.1": {"text": "Still auf dem Blatt ruhte das Lied, noch erschrocken", "tokens": ["Still", "auf", "dem", "Blatt", "ruh\u00b7te", "das", "Lied", ",", "noch", "er\u00b7schro\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,", "ADV", "VVINF"], "meter": "-+-+---+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Vor dem Get\u00f6s des Rhapsoden, der es herlas,", "tokens": ["Vor", "dem", "Ge\u00b7t\u00f6s", "des", "Rhap\u00b7so\u00b7den", ",", "der", "es", "her\u00b7las", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Unbekant mit der sanftern Stimme", "tokens": ["Un\u00b7be\u00b7kant", "mit", "der", "sanf\u00b7tern", "Stim\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Laut', und dem volleren Ton.", "tokens": ["Laut'", ",", "und", "dem", "vol\u00b7le\u00b7ren", "Ton", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KON", "ART", "ADJA", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.10": {"line.1": {"text": "Da, wo er schrie, lag ein Homer. Auf den Dreyfuss", "tokens": ["Da", ",", "wo", "er", "schrie", ",", "lag", "ein", "Ho\u