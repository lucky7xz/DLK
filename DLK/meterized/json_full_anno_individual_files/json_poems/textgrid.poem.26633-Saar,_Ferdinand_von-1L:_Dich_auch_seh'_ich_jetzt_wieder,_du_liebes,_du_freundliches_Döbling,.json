{"textgrid.poem.26633": {"metadata": {"author": {"name": "Saar, Ferdinand von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Dich auch seh' ich jetzt wieder, du liebes, du freundliches D\u00f6bling,", "genre": "verse", "period": "N.A.", "pub_year": 1869, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dich auch seh' ich jetzt wieder, du liebes, du freundliches D\u00f6bling,", "tokens": ["Dich", "auch", "seh'", "ich", "jetzt", "wie\u00b7der", ",", "du", "lie\u00b7bes", ",", "du", "freund\u00b7li\u00b7ches", "D\u00f6b\u00b7ling", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "ADV", "ADV", "$,", "PPER", "ADJA", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Das ich vor Jahren begr\u00fc\u00dft als ein erw\u00fcnschtes Asyl.", "tokens": ["Das", "ich", "vor", "Jah\u00b7ren", "be\u00b7gr\u00fc\u00dft", "als", "ein", "er\u00b7w\u00fcnschtes", "A\u00b7syl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "APPR", "NN", "VVPP", "KOKOM", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Damals warst du ein Dorf mit stillen, sonnigen Gassen,", "tokens": ["Da\u00b7mals", "warst", "du", "ein", "Dorf", "mit", "stil\u00b7len", ",", "son\u00b7ni\u00b7gen", "Gas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "APPR", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Wo sich der Wiener Quirit wohlige H\u00e4user gebaut:", "tokens": ["Wo", "sich", "der", "Wie\u00b7ner", "Qui\u00b7rit", "woh\u00b7li\u00b7ge", "H\u00e4u\u00b7ser", "ge\u00b7baut", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ART", "ADJA", "NN", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Schmucklos, aber bequem, mit fest gegr\u00fcndeten Mauern,", "tokens": ["Schmuck\u00b7los", ",", "a\u00b7ber", "be\u00b7quem", ",", "mit", "fest", "ge\u00b7gr\u00fcn\u00b7de\u00b7ten", "Mau\u00b7ern", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADV", "ADJD", "$,", "APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.6": {"text": "Lauschigen G\u00e4rten, die sich traut in einander verzweigt.", "tokens": ["Lau\u00b7schi\u00b7gen", "G\u00e4r\u00b7ten", ",", "die", "sich", "traut", "in", "ein\u00b7an\u00b7der", "ver\u00b7zweigt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PRELS", "PRF", "VVFIN", "APPR", "PRF", "VVPP", "$."], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}, "line.7": {"text": "Heute geh\u00f6rst du zur Stadt und hast dich darnach auch ver\u00e4ndert;", "tokens": ["Heu\u00b7te", "ge\u00b7h\u00f6rst", "du", "zur", "Stadt", "und", "hast", "dich", "dar\u00b7nach", "auch", "ver\u00b7\u00e4n\u00b7dert", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "KON", "VAFIN", "PPER", "PAV", "ADV", "VVPP", "$."], "meter": "+--+--+-+-+-+-+-", "measure": "dactylic.di.plus"}, "line.8": {"text": "Kaum zu erkennen mehr bist du dem nahenden Blick.", "tokens": ["Kaum", "zu", "er\u00b7ken\u00b7nen", "mehr", "bist", "du", "dem", "na\u00b7hen\u00b7den", "Blick", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKZU", "VVINF", "ADV", "VAFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.9": {"text": "Wo ist die Reihe der Linden, die einst vom Linienwalle,", "tokens": ["Wo", "ist", "die", "Rei\u00b7he", "der", "Lin\u00b7den", ",", "die", "einst", "vom", "Li\u00b7ni\u00b7en\u00b7wal\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "ART", "NE", "$,", "PRELS", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+--+--+----+-", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "K\u00fchlend und duftend zugleich, mich dir entgegen gef\u00fchrt?", "tokens": ["K\u00fch\u00b7lend", "und", "duf\u00b7tend", "zu\u00b7gleich", ",", "mich", "dir", "ent\u00b7ge\u00b7gen", "ge\u00b7f\u00fchrt", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "ADV", "$,", "PPER", "PPER", "PTKVZ", "VVPP", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.11": {"text": "Wo, zur Rechten, das Feld, das ausgedehnte, umplankte,", "tokens": ["Wo", ",", "zur", "Rech\u00b7ten", ",", "das", "Feld", ",", "das", "aus\u00b7ge\u00b7dehn\u00b7te", ",", "um\u00b7plank\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "$,", "APPRART", "NN", "$,", "ART", "NN", "$,", "PDS", "VVFIN", "$,", "VVFIN", "$,"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.12": {"text": "D'rin Cyanen und Mohn wallende Aehren geschm\u00fcckt?", "tokens": ["D'\u00b7rin", "Cy\u00b7a\u00b7nen", "und", "Mohn", "wal\u00b7len\u00b7de", "A\u00b7eh\u00b7ren", "ge\u00b7schm\u00fcckt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "KON", "NN", "ADJA", "NN", "VVPP", "$."], "meter": "+--+--+----+--+", "measure": "dactylic.tri.plus"}, "line.13": {"text": "Ach, verschwunden der Reiz des l\u00e4ndlichen Anblicks! Es ragen", "tokens": ["Ach", ",", "ver\u00b7schwun\u00b7den", "der", "Reiz", "des", "l\u00e4nd\u00b7li\u00b7chen", "An\u00b7blicks", "!", "Es", "ra\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ITJ", "$,", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$.", "PPER", "VVFIN"], "meter": "+-+--+-+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.14": {"text": "N\u00fcchtern, einf\u00f6rmig und hoch neue Geb\u00e4ude empor.", "tokens": ["N\u00fcch\u00b7tern", ",", "ein\u00b7f\u00f6r\u00b7mig", "und", "hoch", "neu\u00b7e", "Ge\u00b7b\u00e4u\u00b7de", "em\u00b7por", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "KON", "ADJD", "ADJA", "NN", "PTKVZ", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.15": {"text": "Baugrund wurde der Acker, und das Geleise des Tramway", "tokens": ["Baug\u00b7rund", "wur\u00b7de", "der", "A\u00b7cker", ",", "und", "das", "Ge\u00b7lei\u00b7se", "des", "Tram\u00b7way"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "ART", "NN", "$,", "KON", "ART", "NN", "ART", "NN"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.16": {"text": "F\u00e4llte die s\u00e4uselnde Pracht schattiger Wipfel schon l\u00e4ngst.", "tokens": ["F\u00e4ll\u00b7te", "die", "s\u00e4u\u00b7seln\u00b7de", "Pracht", "schat\u00b7ti\u00b7ger", "Wip\u00b7fel", "schon", "l\u00e4ngst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "ADJA", "NN", "ADV", "ADV", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.17": {"text": "Aber getr\u00f6ste dich, Herz! Noch wei\u00df ich Gassen zu finden,", "tokens": ["A\u00b7ber", "ge\u00b7tr\u00f6s\u00b7te", "dich", ",", "Herz", "!", "Noch", "wei\u00df", "ich", "Gas\u00b7sen", "zu", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "NN", "$.", "ADV", "VVFIN", "PPER", "NN", "PTKZU", "VVINF", "$,"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.18": {"text": "Die sich auch heute gewi\u00df, was dich erfreute, bewahrt.", "tokens": ["Die", "sich", "auch", "heu\u00b7te", "ge\u00b7wi\u00df", ",", "was", "dich", "er\u00b7freu\u00b7te", ",", "be\u00b7wahrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PRF", "ADV", "ADV", "ADV", "$,", "PWS", "PPER", "VVFIN", "$,", "VVPP", "$."], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.19": {"text": "Sieh: da stehen ja schon und gr\u00fc\u00dfen bekanntere H\u00e4user \u2013", "tokens": ["Sieh", ":", "da", "ste\u00b7hen", "ja", "schon", "und", "gr\u00fc\u00b7\u00dfen", "be\u00b7kann\u00b7te\u00b7re", "H\u00e4u\u00b7ser", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ADV", "VVFIN", "ADV", "ADV", "KON", "ADJA", "ADJA", "NN", "$("], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.20": {"text": "Manches darunter, das jetzt holdes Erinnern mir weckt.", "tokens": ["Man\u00b7ches", "da\u00b7run\u00b7ter", ",", "das", "jetzt", "hol\u00b7des", "E\u00b7rin\u00b7nern", "mir", "weckt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PAV", "$,", "PRELS", "ADV", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}, "line.21": {"text": "Freilich haben dazwischen gedr\u00e4ngt sich putzige Villen,", "tokens": ["Frei\u00b7lich", "ha\u00b7ben", "da\u00b7zwi\u00b7schen", "ge\u00b7dr\u00e4ngt", "sich", "put\u00b7zi\u00b7ge", "Vil\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PAV", "VVFIN", "PRF", "ADJA", "NN", "$,"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.22": {"text": "Th\u00fcrmchen- und erkerbespickt, wie's die \u00bbModerne\u00ab verlangt.", "tokens": ["Th\u00fcrm\u00b7chen", "und", "er\u00b7ker\u00b7be\u00b7spickt", ",", "wie's", "die", "\u00bb", "Mo\u00b7der\u00b7ne", "\u00ab", "ver\u00b7langt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["TRUNC", "KON", "VVFIN", "$,", "KOUS", "ART", "$(", "NN", "$(", "VVPP", "$."], "meter": "+--+--+--++--+", "measure": "dactylic.tri.plus"}, "line.23": {"text": "Hier auch die j\u00fcngste der Stra\u00dfen, gef\u00fchrt durch verw\u00fcstete G\u00e4rten \u2013", "tokens": ["Hier", "auch", "die", "j\u00fcngs\u00b7te", "der", "Stra\u00b7\u00dfen", ",", "ge\u00b7f\u00fchrt", "durch", "ver\u00b7w\u00fcs\u00b7te\u00b7te", "G\u00e4r\u00b7ten", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "ART", "NN", "$,", "VVPP", "APPR", "ADJA", "NN", "$("], "meter": "-+-+--+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.24": {"text": "Und, o Himmel, dort spreizt, riesig, sich gar ein Palast!", "tokens": ["Und", ",", "o", "Him\u00b7mel", ",", "dort", "spreizt", ",", "rie\u00b7sig", ",", "sich", "gar", "ein", "Pa\u00b7last", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "FM", "NN", "$,", "ADV", "VVFIN", "$,", "ADJD", "$,", "PRF", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.25": {"text": "Aber er st\u00f6rt mich nicht mehr; denn schon gewahr' ich der Kirche", "tokens": ["A\u00b7ber", "er", "st\u00f6rt", "mich", "nicht", "mehr", ";", "denn", "schon", "ge\u00b7wahr'", "ich", "der", "Kir\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPER", "PTKNEG", "ADV", "$.", "KON", "ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "+--+-+-+--+--+-", "measure": "hexameter"}, "line.26": {"text": "Taubenumflattertes Dach \u2013 sehe ein reinliches Haus:", "tokens": ["Tau\u00b7be\u00b7num\u00b7flat\u00b7ter\u00b7tes", "Dach", "\u2013", "se\u00b7he", "ein", "rein\u00b7li\u00b7ches", "Haus", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$(", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.27": {"text": "Schimmernd get\u00fcncht, mit zwei Stockwerken, die Reihen der Fenster", "tokens": ["Schim\u00b7mernd", "ge\u00b7t\u00fcncht", ",", "mit", "zwei", "Stock\u00b7wer\u00b7ken", ",", "die", "Rei\u00b7hen", "der", "Fens\u00b7ter"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "VVPP", "$,", "APPR", "CARD", "NN", "$,", "ART", "NN", "ART", "NN"], "meter": "+--+--+---+--+-", "measure": "dactylic.tri.plus"}, "line.28": {"text": "Jalousienverh\u00fcllt gegen den sengenden Strahl.", "tokens": ["Ja\u00b7lou\u00b7si\u00b7en\u00b7ver\u00b7h\u00fcllt", "ge\u00b7gen", "den", "sen\u00b7gen\u00b7den", "Strahl", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.29": {"text": "Ja, ich kenn' es genau. Dort oben in einsamer Stube,", "tokens": ["Ja", ",", "ich", "kenn'", "es", "ge\u00b7nau", ".", "Dort", "o\u00b7ben", "in", "ein\u00b7sa\u00b7mer", "Stu\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "PPER", "ADJD", "$.", "ADV", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "--+--+-+-+-+-+-", "measure": "anapaest.di.plus"}, "line.30": {"text": "D\u00fcrftigem Hausrath gesellt, tr\u00e4umte und sann der Poet;", "tokens": ["D\u00fcrf\u00b7ti\u00b7gem", "Haus\u00b7rath", "ge\u00b7sellt", ",", "tr\u00e4um\u00b7te", "und", "sann", "der", "Po\u00b7et", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "$,", "VVFIN", "KON", "VVFIN", "ART", "NN", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.31": {"text": "Sann und blickte dabei auf ein Meer von gr\u00fcnenden Wipfeln", "tokens": ["Sann", "und", "blick\u00b7te", "da\u00b7bei", "auf", "ein", "Meer", "von", "gr\u00fc\u00b7nen\u00b7den", "Wip\u00b7feln"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "KON", "VVFIN", "PAV", "APPR", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.32": {"text": "Und auf die Th\u00fcrme der Stadt, die in der Ferne verschwamm.", "tokens": ["Und", "auf", "die", "Th\u00fcr\u00b7me", "der", "Stadt", ",", "die", "in", "der", "Fer\u00b7ne", "ver\u00b7schwamm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.33": {"text": "Selige Qualen des Schaffens und selige Qualen der Liebe,", "tokens": ["Se\u00b7li\u00b7ge", "Qua\u00b7len", "des", "Schaf\u00b7fens", "und", "se\u00b7li\u00b7ge", "Qua\u00b7len", "der", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", 