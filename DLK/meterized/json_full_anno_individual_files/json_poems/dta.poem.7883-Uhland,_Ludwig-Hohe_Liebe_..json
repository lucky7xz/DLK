{"dta.poem.7883": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Hohe Liebe .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "In Liebesarmen ruht ihr trunken,", "tokens": ["In", "Lie\u00b7be\u00b7sar\u00b7men", "ruht", "ihr", "trun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Des Lebens Fr\u00fcchte winken euch;", "tokens": ["Des", "Le\u00b7bens", "Fr\u00fcch\u00b7te", "win\u00b7ken", "euch", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Blick nur ist auf mich gesunken,", "tokens": ["Ein", "Blick", "nur", "ist", "auf", "mich", "ge\u00b7sun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Doch bin ich vor euch allen reich.", "tokens": ["Doch", "bin", "ich", "vor", "euch", "al\u00b7len", "reich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "APPR", "PPER", "PIAT", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Das Gl\u00fcck der Erde miss\u2019 ich gerne,", "tokens": ["Das", "Gl\u00fcck", "der", "Er\u00b7de", "miss'", "ich", "ger\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und blick\u2019, ein M\u00e4rtyrer, hinan,", "tokens": ["Und", "blick'", ",", "ein", "M\u00e4r\u00b7ty\u00b7rer", ",", "hi\u00b7nan", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ART", "NN", "$,", "ADV", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Denn \u00fcber mir, in goldner Ferne,", "tokens": ["Denn", "\u00fc\u00b7ber", "mir", ",", "in", "gold\u00b7ner", "Fer\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hat sich der Himmel aufgethan.", "tokens": ["Hat", "sich", "der", "Him\u00b7mel", "auf\u00b7ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PRF", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}