{"textgrid.poem.49164": {"metadata": {"author": {"name": "R\u00f6ling, Johann", "birth": "N.A.", "death": "N.A."}, "title": "Christliches Sterblied", "genre": "verse", "period": "N.A.", "pub_year": 1656, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein End' ist, liebster Jesu, hier,", "tokens": ["Mein", "End'", "ist", ",", "liebs\u00b7ter", "Je\u00b7su", ",", "hier", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "$,", "ADJA", "NE", "$,", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich sehe deinen Knecht vor mir,", "tokens": ["Ich", "se\u00b7he", "dei\u00b7nen", "Knecht", "vor", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den Tod, mich zu dir abzuholen;", "tokens": ["Den", "Tod", ",", "mich", "zu", "dir", "ab\u00b7zu\u00b7ho\u00b7len", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRF", "APPR", "PPER", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ich bin vergn\u00fcgt mit diesem Schlu\u00df,", "tokens": ["Ich", "bin", "ver\u00b7gn\u00fcgt", "mit", "die\u00b7sem", "Schlu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da ich, auch wolt ich ungern, mu\u00df,", "tokens": ["Da", "ich", ",", "auch", "wolt", "ich", "un\u00b7gern", ",", "mu\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "ADV", "VMFIN", "PPER", "ADV", "$,", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wenn du, mein Herr, es ihm befohlen;", "tokens": ["Wenn", "du", ",", "mein", "Herr", ",", "es", "ihm", "be\u00b7foh\u00b7len", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "PPOSAT", "NN", "$,", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Nun kannst du mich so gern nicht sehn,", "tokens": ["Nun", "kannst", "du", "mich", "so", "gern", "nicht", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "ADV", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Als mich verlangt, zu dir zu gehn.", "tokens": ["Als", "mich", "ver\u00b7langt", ",", "zu", "dir", "zu", "gehn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "APPR", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Mein Geist ist her aus deiner Hand", "tokens": ["Mein", "Geist", "ist", "her", "aus", "dei\u00b7ner", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und deinem ew'gen Licht verwandt;", "tokens": ["Und", "dei\u00b7nem", "ew'\u00b7gen", "Licht", "ver\u00b7wandt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Leib, womit du ihn umgeben,", "tokens": ["Der", "Leib", ",", "wo\u00b7mit", "du", "ihn", "um\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ist mehr sein Kerker als sein Haus;", "tokens": ["Ist", "mehr", "sein", "Ker\u00b7ker", "als", "sein", "Haus", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "KOKOM", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie wollt' ich nicht hier gern heraus", "tokens": ["Wie", "wollt'", "ich", "nicht", "hier", "gern", "he\u00b7raus"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "PPER", "PTKNEG", "ADV", "ADV", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und bei dir, meinem Sch\u00f6pfer, leben,", "tokens": ["Und", "bei", "dir", ",", "mei\u00b7nem", "Sch\u00f6p\u00b7fer", ",", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "APPR", "PPER", "$,", "PPOSAT", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Wo meine liebe Vaterstadt", "tokens": ["Wo", "mei\u00b7ne", "lie\u00b7be", "Va\u00b7ter\u00b7stadt"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und meine Seel' ihr Sitzthum hat?", "tokens": ["Und", "mei\u00b7ne", "Seel'", "ihr", "Sitz\u00b7thum", "hat", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Du bist mein Haupt und ich dein Glied,", "tokens": ["Du", "bist", "mein", "Haupt", "und", "ich", "dein", "Glied", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "KON", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wo du hingehst, da folg' ich mit,", "tokens": ["Wo", "du", "hin\u00b7gehst", ",", "da", "fol\u00b7g'", "ich", "mit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Du hast zur Erden dich begeben,", "tokens": ["Du", "hast", "zur", "Er\u00b7den", "dich", "be\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df mein der Himmel m\u00f6chte sein,", "tokens": ["Da\u00df", "mein", "der", "Him\u00b7mel", "m\u00f6ch\u00b7te", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ART", "NN", "VMFIN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du gingst ins Grab und H\u00f6llen ein,", "tokens": ["Du", "gingst", "ins", "Grab", "und", "H\u00f6l\u00b7len", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "KON", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Auf da\u00df ich ewig m\u00f6chte leben;", "tokens": ["Auf", "da\u00df", "ich", "e\u00b7wig", "m\u00f6ch\u00b7te", "le\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "ADJD", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Sollt' ich nicht gerne sein um dich,", "tokens": ["Sollt'", "ich", "nicht", "ger\u00b7ne", "sein", "um", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "ADV", "PPOSAT", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Da du so viel gethan f\u00fcr mich?", "tokens": ["Da", "du", "so", "viel", "ge\u00b7than", "f\u00fcr", "mich", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "VVPP", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Dein heil'ges Mahl, dein Leib und Blut,", "tokens": ["Dein", "heil'\u00b7ges", "Mahl", ",", "dein", "Leib", "und", "Blut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das du mir angericht zu gut,", "tokens": ["Das", "du", "mir", "an\u00b7ge\u00b7richt", "zu", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "VVPP", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hab' ich zum \u00f6ftern hier genossen;", "tokens": ["Hab'", "ich", "zum", "\u00f6f\u00b7tern", "hier", "ge\u00b7nos\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPRART", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So hast du dich vereint mit mir", "tokens": ["So", "hast", "du", "dich", "ver\u00b7eint", "mit", "mir"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "VVPP", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und ist dein Leben ganz von dir,", "tokens": ["Und", "ist", "dein", "Le\u00b7ben", "ganz", "von", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPOSAT", "NN", "ADV", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Herr, meinem Leben eingeflossen,", "tokens": ["Herr", ",", "mei\u00b7nem", "Le\u00b7ben", "ein\u00b7ge\u00b7flos\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Da\u00df du, mein Heiland, mein, ich dein,", "tokens": ["Da\u00df", "du", ",", "mein", "Hei\u00b7land", ",", "mein", ",", "ich", "dein", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "$,", "PPER", "PPOSAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und wir dies ewig sollen sein.", "tokens": ["Und", "wir", "dies", "e\u00b7wig", "sol\u00b7len", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PDS", "ADJD", "VMFIN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Drauf geh' ich fr\u00f6hlich in das Grab", "tokens": ["Drauf", "geh'", "ich", "fr\u00f6h\u00b7lich", "in", "das", "Grab"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und lege hier mein Sterbkleid ab,", "tokens": ["Und", "le\u00b7ge", "hier", "mein", "Sterb\u00b7kleid", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Um seelig mit dir aufzustehen.", "tokens": ["Um", "see\u00b7lig", "mit", "dir", "auf\u00b7zu\u00b7ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADJD", "APPR", "PPER", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Es fall' und faule mein Gebein,", "tokens": ["Es", "fall'", "und", "fau\u00b7le", "mein", "Ge\u00b7bein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So mu\u00df ich erst gel\u00e4utert sein,", "tokens": ["So", "mu\u00df", "ich", "erst", "ge\u00b7l\u00e4u\u00b7tert", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Soll ich mit dir zum Himmel gehen", "tokens": ["Soll", "ich", "mit", "dir", "zum", "Him\u00b7mel", "ge\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "APPR", "PPER", "APPRART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und ziehen deinen Heilsrock an,", "tokens": ["Und", "zie\u00b7hen", "dei\u00b7nen", "Heils\u00b7rock", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Den mir dein Leiden angethan.", "tokens": ["Den", "mir", "dein", "Lei\u00b7den", "an\u00b7ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ach s\u00fcsse Hoffnung, sch\u00f6ne Lust,", "tokens": ["Ach", "s\u00fcs\u00b7se", "Hoff\u00b7nung", ",", "sch\u00f6\u00b7ne", "Lust", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Womit du, Jesu, meine Brust", "tokens": ["Wo\u00b7mit", "du", ",", "Je\u00b7su", ",", "mei\u00b7ne", "Brust"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PWAV", "PPER", "$,", "NE", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und ganzes Herze hast erf\u00fcllet!", "tokens": ["Und", "gan\u00b7zes", "Her\u00b7ze", "hast", "er\u00b7f\u00fcl\u00b7let", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VAFIN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Komm', lieber Tod, o meine Ruh',", "tokens": ["Komm'", ",", "lie\u00b7ber", "Tod", ",", "o", "mei\u00b7ne", "Ruh'", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADV", "NN", "$,", "FM", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und dr\u00fccke mir die Augen zu,", "tokens": ["Und", "dr\u00fc\u00b7cke", "mir", "die", "Au\u00b7gen", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die nur ihr Heil zu sehn gewillet;", "tokens": ["Die", "nur", "ihr", "Heil", "zu", "sehn", "ge\u00b7wil\u00b7let", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPOSAT", "NN", "PTKZU", "VVINF", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Ich bin ja nunmehr nicht mehr mein,", "tokens": ["Ich", "bin", "ja", "nun\u00b7mehr", "nicht", "mehr", "mein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "PTKNEG", "ADV", "PPOSAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Um meines Heilands ganz zu sein.", "tokens": ["Um", "mei\u00b7nes", "Hei\u00b7lands", "ganz", "zu", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "ADV", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Mein End' ist, liebster Jesu, hier,", "tokens": ["Mein", "End'", "ist", ",", "liebs\u00b7ter", "Je\u00b7su", ",", "hier", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "$,", "ADJA", "NE", "$,", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich sehe deinen Knecht vor mir,", "tokens": ["Ich", "se\u00b7he", "dei\u00b7nen", "Knecht", "vor", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den Tod, mich zu dir abzuholen;", "tokens": ["Den", "Tod", ",", "mich", "zu", "dir", "ab\u00b7zu\u00b7ho\u00b7len", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRF", "APPR", "PPER", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ich bin vergn\u00fcgt mit diesem Schlu\u00df,", "tokens": ["Ich", "bin", "ver\u00b7gn\u00fcgt", "mit", "die\u00b7sem", "Schlu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da ich, auch wolt ich ungern, mu\u00df,", "tokens": ["Da", "ich", ",", "auch", "wolt", "ich", "un\u00b7gern", ",", "mu\u00df", ","], "token_info"