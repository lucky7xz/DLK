ns": ["Bald", "fin\u00b7det", "man", "es", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "PPER", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Betr\u00fcbt an finstrer Kluft.", "tokens": ["Be\u00b7tr\u00fcbt", "an", "finst\u00b7rer", "Kluft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.31": {"line.1": {"text": "Bald frisch und freudig schwebend", "tokens": ["Bald", "frisch", "und", "freu\u00b7dig", "schwe\u00b7bend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "KON", "ADJD", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hoch mit der s\u00fc\u00dfen Lerch',", "tokens": ["Hoch", "mit", "der", "s\u00fc\u00b7\u00dfen", "Ler\u00b7ch'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "---+-+-", "measure": "unknown.measure.di"}, "line.3": {"text": "Gott lobend und umgebend", "tokens": ["Gott", "lo\u00b7bend", "und", "um\u00b7ge\u00b7bend"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVPP", "KON", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den trauervollen Berg.", "tokens": ["Den", "trau\u00b7er\u00b7vol\u00b7len", "Berg", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.32": {"line.1": {"text": "Auch wieder da nicht bleibet,", "tokens": ["Auch", "wie\u00b7der", "da", "nicht", "blei\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hebt sich in Wind hinein;", "tokens": ["Hebt", "sich", "in", "Wind", "hin\u00b7ein", ";"], "token_inf