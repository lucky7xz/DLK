{"textgrid.poem.36372": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wann Morgenr\u00f6t sich zieret", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wann Morgenr\u00f6t sich zieret", "tokens": ["Wann", "Mor\u00b7gen\u00b7r\u00f6t", "sich", "zie\u00b7ret"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "NN", "PRF", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit zartem Rosenglanz,", "tokens": ["Mit", "zar\u00b7tem", "Ro\u00b7sen\u00b7glanz", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wann sittsam sich verlieret", "tokens": ["Wann", "sitt\u00b7sam", "sich", "ver\u00b7lie\u00b7ret"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "PRF", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der n\u00e4chtlich' Sternentanz,", "tokens": ["Der", "n\u00e4cht\u00b7lich'", "Ster\u00b7nen\u00b7tanz", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Gleich l\u00fcstet mich spazieren", "tokens": ["Gleich", "l\u00fcs\u00b7tet", "mich", "spa\u00b7zie\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Im gr\u00fcnen Lorbeerwald,", "tokens": ["Im", "gr\u00fc\u00b7nen", "Lor\u00b7beer\u00b7wald", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da lieblich musizieren", "tokens": ["Da", "lieb\u00b7lich", "mu\u00b7si\u00b7zie\u00b7ren"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADJD", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Stimmlein mannigfalt.", "tokens": ["Die", "Stimm\u00b7lein", "man\u00b7nig\u00b7falt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Die fl\u00fcgelreichen Scharen,", "tokens": ["Die", "fl\u00fc\u00b7gel\u00b7rei\u00b7chen", "Scha\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das Federv\u00f6lklein zart,", "tokens": ["Das", "Fe\u00b7der\u00b7v\u00f6l\u00b7klein", "zart", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Im s\u00fc\u00dfen Sang erfahren,", "tokens": ["Im", "s\u00fc\u00b7\u00dfen", "Sang", "er\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Noch Kunst noch Atem spart.", "tokens": ["Noch", "Kunst", "noch", "A\u00b7tem", "spart", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "ADV", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Mit Schn\u00e4blein wohl geschliffen", "tokens": ["Mit", "Schn\u00e4b\u00b7lein", "wohl", "ge\u00b7schlif\u00b7fen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "ADV", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sie klingen wunderfein,", "tokens": ["Sie", "klin\u00b7gen", "wun\u00b7der\u00b7fein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In L\u00fcften munter schiffen", "tokens": ["In", "L\u00fcf\u00b7ten", "mun\u00b7ter", "schif\u00b7fen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "ADJD", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit leichten Ruderlein.", "tokens": ["Mit", "leich\u00b7ten", "Ru\u00b7der\u00b7lein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Der dichte Wald ert\u00f6net", "tokens": ["Der", "dich\u00b7te", "Wald", "er\u00b7t\u00f6\u00b7net"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Von ihrem lauten Sang,", "tokens": ["Von", "ih\u00b7rem", "lau\u00b7ten", "Sang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit Stauden stolz gekr\u00f6net", "tokens": ["Mit", "Stau\u00b7den", "stolz", "ge\u00b7kr\u00f6\u00b7net"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "ADJD", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Berge geben Klang.", "tokens": ["Die", "Ber\u00b7ge", "ge\u00b7ben", "Klang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Die B\u00e4chlein krumm geflochten,", "tokens": ["Die", "B\u00e4ch\u00b7lein", "krumm", "ge\u00b7floch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Auch rieselnd stimmen ein,", "tokens": ["Auch", "rie\u00b7selnd", "stim\u00b7men", "ein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Von Steinlein angefochten,", "tokens": ["Von", "Stein\u00b7lein", "an\u00b7ge\u00b7foch\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Gar lieblich sausen drein.", "tokens": ["Gar", "lieb\u00b7lich", "sau\u00b7sen", "drein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Doch s\u00fc\u00dfer noch erklinget", "tokens": ["Doch", "s\u00fc\u00b7\u00dfer", "noch", "er\u00b7klin\u00b7get"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJD", "ADV", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein sonders V\u00f6gelein;", "tokens": ["Ein", "son\u00b7ders", "V\u00f6\u00b7ge\u00b7lein", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es seinen Sang vollbringet", "tokens": ["Es", "sei\u00b7nen", "Sang", "voll\u00b7brin\u00b7get"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bei Mond- und Sonnenschein.", "tokens": ["Bei", "Mon\u00b7d", "und", "Son\u00b7nen\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "TRUNC", "KON", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.8": {"line.1": {"text": "Trutz Nachtigall mit Namen", "tokens": ["Trutz", "Nach\u00b7ti\u00b7gall", "mit", "Na\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "NN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Es nunmehr wird genannt,", "tokens": ["Es", "nun\u00b7mehr", "wird", "ge\u00b7nannt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und es den Wild' und Zahmen", "tokens": ["Und", "es", "den", "Wild'", "und", "Zah\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "ART", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Obsieget unbekannt.", "tokens": ["Ob\u00b7sie\u00b7get", "un\u00b7be\u00b7kannt", "."], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Trutz Nachtigall man's nennet", "tokens": ["Trutz", "Nach\u00b7ti\u00b7gall", "man's", "nen\u00b7net"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "NN", "PIS", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ist wund vom s\u00fc\u00dfen Pfeil,", "tokens": ["Ist", "wund", "vom", "s\u00fc\u00b7\u00dfen", "Pfeil", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In Lieb' es lieblich brennet,", "tokens": ["In", "Lieb'", "es", "lieb\u00b7lich", "bren\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wird nie der Wunden heil.", "tokens": ["Wird", "nie", "der", "Wun\u00b7den", "heil", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Geld, Pomp und Pracht auf Erden,", "tokens": ["Geld", ",", "Pomp", "und", "Pracht", "auf", "Er\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die L\u00fcste es verspott't,", "tokens": ["Die", "L\u00fcs\u00b7te", "es", "ver\u00b7spott't", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die achtet's f\u00fcr Beschwerden,", "tokens": ["Die", "ach\u00b7tet's", "f\u00fcr", "Be\u00b7schwer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nur suchend seinen Gott.", "tokens": ["Nur", "su\u00b7chend", "sei\u00b7nen", "Gott", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Es singet aller Orten", "tokens": ["Es", "sin\u00b7get", "al\u00b7ler", "Or\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Von Gott und Gottes Sohn,", "tokens": ["Von", "Gott", "und", "Got\u00b7tes", "Sohn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und zu den Himmelspforten", "tokens": ["Und", "zu", "den", "Him\u00b7mel\u00b7spfor\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Verweiset's jeden Ton.", "tokens": ["Ver\u00b7wei\u00b7set's", "je\u00b7den", "Ton", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Von Baum zu Baume springet,", "tokens": ["Von", "Baum", "zu", "Bau\u00b7me", "sprin\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Durchstreichet Berg und Tal,", "tokens": ["Durch\u00b7strei\u00b7chet", "Berg", "und", "Tal", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In Feld und W\u00e4ldern singet,", "tokens": ["In", "Feld", "und", "W\u00e4l\u00b7dern", "sin\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wei\u00df nicht der Lieder Zahl.", "tokens": ["Wei\u00df", "nicht", "der", "Lie\u00b7der", "Zahl", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ART", "NN", "NN", "$."], "meter": "---+-+", "measure": "unknown.measure.di"}}, "stanza.13": {"line.1": {"text": "Es flieget auf und nieder,", "tokens": ["Es", "flie\u00b7get", "auf", "und", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Verwechselt Ort und Luft.", "tokens": ["Ver\u00b7wech\u00b7selt", "Ort", "und", "Luft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bald findet man es wieder", "tokens": ["Bald", "fin\u00b7det", "man", "es", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "PPER", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Betr\u00fcbt an finstrer Kluft.", "tokens": ["Be\u00b7tr\u00fcbt", "an", "finst\u00b7rer", "Kluft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Bald frisch und freudig schwebend", "tokens": ["Bald", "frisch", "und", "freu\u00b7dig", "schwe\u00b7bend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "KON", "ADJD", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hoch mit der s\u00fc\u00dfen Lerch',", "tokens": ["Hoch", "mit", "der", "s\u00fc\u00b7\u00dfen", "Ler\u00b7ch'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "---+-+-", "measure": "unknown.measure.di"}, "line.3": {"text": "Gott lobend und umgebend", "tokens": ["Gott", "lo\u00b7bend", "und", "um\u00b7ge\u00b7bend"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVPP", "KON", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den trauervollen Berg.", "tokens": ["Den", "trau\u00b7er\u00b7vol\u00b7len", "Berg", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Auch wieder da nicht bleibet,", "tokens": ["Auch", "wie\u00b7der", "da", "nicht", "blei\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hebt sich in Wind hinein;", "tokens": ["Hebt", "sich", "in", "Wind", "hin\u00b7ein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In leerer Luft es treibet", "tokens": ["In", "lee\u00b7rer", "Luft", "es", "trei\u00b7bet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit schwankem Fl\u00fcgelein.", "tokens": ["Mit", "schwan\u00b7kem", "Fl\u00fc\u00b7ge\u00b7lein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.16": {"line.1": {"text": "Mit ihm will mich erschwingen", "tokens": ["Mit", "ihm", "will", "mich", "er\u00b7schwin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VMFIN", "PRF", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und manchem schwebend ob,", "tokens": ["Und", "man\u00b7chem", "schwe\u00b7bend", "ob", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJD", "KOUS", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Den Lorbeerkranz erringen", "tokens": ["Den", "Lor\u00b7beer\u00b7kranz", "er\u00b7rin\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In deutschem Gotteslob.", "tokens": ["In", "deut\u00b7schem", "Got\u00b7tes\u00b7lob", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.17": {"line.1": {"text": "Dem Leser nicht verdrie\u00dfe", "tokens": ["Dem", "Le\u00b7ser", "nicht", "ver\u00b7drie\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PTKNEG", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Zeit und Stunden lang,", "tokens": ["Der", "Zeit", "und", "Stun\u00b7den", "lang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iam