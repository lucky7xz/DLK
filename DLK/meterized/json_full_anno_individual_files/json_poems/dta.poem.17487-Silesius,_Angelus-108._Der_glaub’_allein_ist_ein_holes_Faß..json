{"dta.poem.17487": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "108.  Der glaub\u2019 allein ist ein holes Fa\u00df.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Der glaub\u2019/ ohn lieb\u2019/ allein/ (wie ich mich wol besinne)", "tokens": ["Der", "glaub'", "/", "ohn", "lieb'", "/", "al\u00b7lein", "/", "(", "wie", "ich", "mich", "wol", "be\u00b7sin\u00b7ne", ")"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$(", "APPR", "VVFIN", "$(", "ADV", "$(", "$(", "PWAV", "PPER", "PRF", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ist wie ein holes Fa\u00df: E\u00df klingt und hat nichts drinne.", "tokens": ["Ist", "wie", "ein", "ho\u00b7les", "Fa\u00df", ":", "E\u00df", "klingt", "und", "hat", "nichts", "drin\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KOKOM", "ART", "ADJA", "NN", "$.", "NN", "VVFIN", "KON", "VAFIN", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}