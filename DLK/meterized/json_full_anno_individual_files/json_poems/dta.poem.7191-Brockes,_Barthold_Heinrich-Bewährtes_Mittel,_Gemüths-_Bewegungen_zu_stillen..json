{"dta.poem.7191": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Bew\u00e4hrtes Mittel, Gem\u00fcths-  \n Bewegungen zu stillen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.85", "fr:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Zu leugnen ist es nicht,", "tokens": ["Zu", "leug\u00b7nen", "ist", "es", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "VAFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Da\u00df eine starcke Leidenschafft", "tokens": ["Da\u00df", "ei\u00b7ne", "star\u00b7cke", "Lei\u00b7den\u00b7schafft"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Muskeln im Gesicht", "tokens": ["Die", "Mus\u00b7keln", "im", "Ge\u00b7sicht"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ver\u00e4nder\u2019 und bewege,", "tokens": ["Ver\u00b7\u00e4n\u00b7der'", "und", "be\u00b7we\u00b7ge", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Mithin ein\u2019 and\u2019re Min\u2019 errege.", "tokens": ["Mi\u00b7thin", "ein'", "an\u00b7d'\u00b7re", "Min'", "er\u00b7re\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Nun fragt sichs, ob so lang\u2019, als solche Stellung w\u00e4hrt,", "tokens": ["Nun", "fragt", "sichs", ",", "ob", "so", "lang'", ",", "als", "sol\u00b7che", "Stel\u00b7lung", "w\u00e4hrt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "$,", "KOUS", "ADV", "ADV", "$,", "KOUS", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Auch der Affect nicht wenigstens besteht,", "tokens": ["Auch", "der", "Af\u00b7fect", "nicht", "we\u00b7nigs\u00b7tens", "be\u00b7steht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PTKNEG", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und ob denn folglich auch, wenn man sie anders kehrt;", "tokens": ["Und", "ob", "denn", "folg\u00b7lich", "auch", ",", "wenn", "man", "sie", "an\u00b7ders", "kehrt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "ADV", "ADV", "$,", "KOUS", "PIS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Die Leidenschafft nicht eh\u2019 vergeht?", "tokens": ["Die", "Lei\u00b7den\u00b7schafft", "nicht", "eh'", "ver\u00b7geht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Mir kommt es glaublich vor, da\u00df es sich so verh\u00e4lt.", "tokens": ["Mir", "kommt", "es", "glaub\u00b7lich", "vor", ",", "da\u00df", "es", "sich", "so", "ver\u00b7h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,", "KOUS", "PPER", "PRF", "ADV", "VVFIN", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}}}}}