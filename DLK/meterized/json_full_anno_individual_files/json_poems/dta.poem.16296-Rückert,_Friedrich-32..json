{"dta.poem.16296": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "32.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du sagst: Begier ist b\u00f6s', es sei nun da\u00df sie r\u00fchre", "tokens": ["Du", "sagst", ":", "Be\u00b7gier", "ist", "b\u00f6s'", ",", "es", "sei", "nun", "da\u00df", "sie", "r\u00fch\u00b7re"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "NN", "VAFIN", "ADJD", "$,", "PPER", "VAFIN", "ADV", "KOUS", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vom B\u00f6sen her, es sei da\u00df sie zum B\u00f6sen f\u00fchre.", "tokens": ["Vom", "B\u00f6\u00b7sen", "her", ",", "es", "sei", "da\u00df", "sie", "zum", "B\u00f6\u00b7sen", "f\u00fch\u00b7re", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "$,", "PPER", "VAFIN", "KOUS", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ich aber sage dir: Begier begehret nur", "tokens": ["Ich", "a\u00b7ber", "sa\u00b7ge", "dir", ":", "Be\u00b7gier", "be\u00b7ge\u00b7hret", "nur"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "$.", "NN", "VVFIN", "ADV"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ihr Gutes, und verabscheut B\u00f6ses von Natur.", "tokens": ["Ihr", "Gu\u00b7tes", ",", "und", "ver\u00b7ab\u00b7scheut", "B\u00f6\u00b7ses", "von", "Na\u00b7tur", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "KON", "VVFIN", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Vielmehr: Was sie begehrt, wird darum gut sie nennen,", "tokens": ["Viel\u00b7mehr", ":", "Was", "sie", "be\u00b7gehrt", ",", "wird", "da\u00b7rum", "gut", "sie", "nen\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "PWS", "PPER", "VVPP", "$,", "VAFIN", "PAV", "ADJD", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und was sie scheuet, das davon als B\u00f6ses trennen.", "tokens": ["Und", "was", "sie", "scheu\u00b7et", ",", "das", "da\u00b7von", "als", "B\u00f6\u00b7ses", "tren\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVFIN", "$,", "PRELS", "PAV", "KOUS", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Die Trennung b\u00f6s' und gut bringst du nur in die Welt,", "tokens": ["Die", "Tren\u00b7nung", "b\u00f6s'", "und", "gut", "bringst", "du", "nur", "in", "die", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "ADJD", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Indem du sagst wie sie sich zur Begier verh\u00e4lt.", "tokens": ["In\u00b7dem", "du", "sagst", "wie", "sie", "sich", "zur", "Be\u00b7gier", "ver\u00b7h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "KOKOM", "PPER", "PRF", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.5": {"line.1": {"text": "Und h\u00e4tte b\u00f6s' und gut der Mensch nicht unterschieden,", "tokens": ["Und", "h\u00e4t\u00b7te", "b\u00f6s'", "und", "gut", "der", "Mensch", "nicht", "un\u00b7ter\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADJD", "KON", "ADJD", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "W\u00e4r' er begierdelos, mit der Natur im Frieden.", "tokens": ["W\u00e4r'", "er", "be\u00b7gier\u00b7de\u00b7los", ",", "mit", "der", "Na\u00b7tur", "im", "Frie\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "$,", "APPR", "ART", "NN", "APPRART", "NN", "$."], "meter": "---+-+-+-+-+-", "measure": "unknown.measure.penta"}}}}}