{"dta.poem.713": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "2. Zusatz.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Du weist/ da\u00df ich durch mich nichts kan vollbringen:", "tokens": ["Du", "weist", "/", "da\u00df", "ich", "durch", "mich", "nichts", "kan", "voll\u00b7brin\u00b7gen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "KOUS", "PPER", "APPR", "PPER", "PIS", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich wei\u00df da\u00df du durch mich kanst alles thun:", "tokens": ["Ich", "wei\u00df", "da\u00df", "du", "durch", "mich", "kanst", "al\u00b7les", "thun", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOUS", "PPER", "APPR", "PPER", "VMFIN", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Drumb bitt ich HERR: La\u00df meiner Faust gelingen", "tokens": ["Drumb", "bitt", "ich", "HeRR", ":", "La\u00df", "mei\u00b7ner", "Faust", "ge\u00b7lin\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "NN", "$.", "VVIMP", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Was du befihlst: bi\u00df da\u00df mein Fleisch wird ruhn.", "tokens": ["Was", "du", "be\u00b7fihlst", ":", "bi\u00df", "da\u00df", "mein", "Fleisch", "wird", "ruhn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$.", "APPR", "KOUS", "PPOSAT", "NN", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Gib weil di\u00df ", "tokens": ["Gib", "weil", "di\u00df"], "token_info": ["word", "word", "word"], "pos": ["VVIMP", "KOUS", "PDS"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Ein Hertz das nichts bewegt:", "tokens": ["Ein", "Hertz", "das", "nichts", "be\u00b7wegt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "PIS", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Gib wenn mein Geist/ di\u00df Fleisch sein Hau\u00df ablegt ", "tokens": ["Gib", "wenn", "mein", "Geist", "/", "di\u00df", "Fleisch", "sein", "Hau\u00df", "ab\u00b7legt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "KOUS", "PPOSAT", "NN", "$(", "PDS", "NN", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-++-", "measure": "unknown.measure.penta"}, "line.8": {"text": "Was die/ die seelig/ tr\u00e4gt. ", "tokens": ["Was", "die", "/", "die", "see\u00b7lig", "/", "tr\u00e4gt", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "ART", "$(", "ART", "ADJD", "$(", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}