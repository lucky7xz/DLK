{"textgrid.poem.56422": {"metadata": {"author": {"name": "Heym, Georg", "birth": "N.A.", "death": "N.A."}, "title": "10.", "genre": "verse", "period": "N.A.", "pub_year": 1899, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie dichte Wolken liegen Dunst und Hauch", "tokens": ["Wie", "dich\u00b7te", "Wol\u00b7ken", "lie\u00b7gen", "Dunst", "und", "Hauch"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJA", "NN", "VVFIN", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Des hei\u00dfen Mittags auf der Ebnen Weiten.", "tokens": ["Des", "hei\u00b7\u00dfen", "Mit\u00b7tags", "auf", "der", "Eb\u00b7nen", "Wei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-----+-+-", "measure": "dactylic.init"}, "line.3": {"text": "Die Sonnenstrahlen wie durch Nebel gleiten,", "tokens": ["Die", "Son\u00b7nen\u00b7strah\u00b7len", "wie", "durch", "Ne\u00b7bel", "glei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KOKOM", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Schwarz w\u00e4lzt sich hin verbrannter Felder Rauch.", "tokens": ["Schwarz", "w\u00e4lzt", "sich", "hin", "ver\u00b7brann\u00b7ter", "Fel\u00b7der", "Rauch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PRF", "ADV", "ADJA", "NN", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.2": {"line.1": {"text": "Der Toten Blut und Wunden faulend stinken.", "tokens": ["Der", "To\u00b7ten", "Blut", "und", "Wun\u00b7den", "fau\u00b7lend", "stin\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "po