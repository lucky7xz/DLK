{"dta.poem.17763": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "8.  Der Spruch \u00fcber die Seeligen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.42", "no:0.28", "nl:0.14", "da:0.14"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Kombt jhr gesegneten/ embfahet eure Kronen", "tokens": ["Kombt", "jhr", "ge\u00b7se\u00b7gne\u00b7ten", "/", "emb\u00b7fa\u00b7het", "eu\u00b7re", "Kro\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "VVPP", "$(", "VVFIN", "PPOSAT", "NN"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Die jhr erworben habt durch meinen Lauf und Tod:", "tokens": ["Die", "jhr", "er\u00b7wor\u00b7ben", "habt", "durch", "mei\u00b7nen", "Lauf", "und", "Tod", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "VAFIN", "APPR", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Kombt und besitzt da\u00df Reich der Herrligkeit mit GOtt:", "tokens": ["Kombt", "und", "be\u00b7sitzt", "da\u00df", "Reich", "der", "Herr\u00b7lig\u00b7keit", "mit", "Gott", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "KOUS", "NN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ich wil euch ewiglich f\u00fcr eure Gutthat lohnen.", "tokens": ["Ich", "wil", "euch", "e\u00b7wig\u00b7lich", "f\u00fcr", "eu\u00b7re", "Gut\u00b7that", "loh\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Jhr habet mich getr\u00f6st/ und bey euch lassen wohnen/", "tokens": ["Ihr", "ha\u00b7bet", "mich", "ge\u00b7tr\u00f6st", "/", "und", "bey", "euch", "las\u00b7sen", "woh\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVPP", "$(", "KON", "APPR", "PPER", "VVINF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Jhr habet mich gespeist/ getr\u00e4nkt/ besucht in Noth.", "tokens": ["Ihr", "ha\u00b7bet", "mich", "ge\u00b7speist", "/", "ge\u00b7tr\u00e4nkt", "/", "be\u00b7sucht", "in", "Noth", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVPP", "$(", "VVPP", "$(", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Bekleidet und bedekt nach meinem Liebsgeboth/", "tokens": ["Be\u00b7klei\u00b7det", "und", "be\u00b7dekt", "nach", "mei\u00b7nem", "Liebs\u00b7ge\u00b7both", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "ADJD", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Nu solt jhr auch mit mir besitzen eure Thronen/", "tokens": ["Nu", "solt", "jhr", "auch", "mit", "mir", "be\u00b7sit\u00b7zen", "eu\u00b7re", "Thro\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "APPR", "PPER", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Und ewig triumphirn. Jhr sollet euch nu freun", "tokens": ["Und", "e\u00b7wig", "tri\u00b7um\u00b7phirn", ".", "Ihr", "sol\u00b7let", "euch", "nu", "freun"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVINF", "$.", "PPER", "VMFIN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "F\u00fcr eure Trew und M\u00fch/ und jmmer bey mir sein.", "tokens": ["F\u00fcr", "eu\u00b7re", "Trew", "und", "M\u00fch", "/", "und", "jm\u00b7mer", "bey", "mir", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "NN", "$(", "KON", "ADV", "APPR", "PPER", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Den\u0304 was jhr habt gethan dem kleinsten auf der Erden/", "tokens": ["Den\u0304", "was", "jhr", "habt", "ge\u00b7than", "dem", "kleins\u00b7ten", "auf", "der", "Er\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VAFIN", "VVPP", "ART", "ADJA", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Dasselb\u2019 ist mir geschehn/ und sol in Ewigkeit/", "tokens": ["Das\u00b7selb'", "ist", "mir", "ge\u00b7schehn", "/", "und", "sol", "in", "E\u00b7wig\u00b7keit", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "VVPP", "$(", "KON", "VMFIN", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Mit allem was ihr nur euch w\u00fcntscht/ vergolten werden.", "tokens": ["Mit", "al\u00b7lem", "was", "ihr", "nur", "euch", "w\u00fcnt\u00b7scht", "/", "ver\u00b7gol\u00b7ten", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PIS", "PWS", "PPER", "ADV", "PPER", "VVFIN", "$(", "VVPP", "VAINF", "$."], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "Kombt und geniest mich selbst und alle Seeligkeit.", "tokens": ["Kombt", "und", "ge\u00b7niest", "mich", "selbst", "und", "al\u00b7le", "See\u00b7lig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "PPER", "ADV", "KON", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}