{"dta.poem.7173": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Gottes Gr\u00f6sse.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Seh\u2019 ich dort ein Sternchen strahlen,", "tokens": ["Seh'", "ich", "dort", "ein", "Stern\u00b7chen", "strah\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und erwege, wie sein Schein", "tokens": ["Und", "er\u00b7we\u00b7ge", ",", "wie", "sein", "Schein"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "PWAV", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Meistens pflegt viel hundert mahlen", "tokens": ["Meis\u00b7tens", "pflegt", "viel", "hun\u00b7dert", "mah\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "CARD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gr\u00f6sser, als die Welt zu seyn;", "tokens": ["Gr\u00f6s\u00b7ser", ",", "als", "die", "Welt", "zu", "seyn", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "ART", "NN", "PTKZU", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Seh\u2019 ich, wie ein and\u2019rer blitzet,", "tokens": ["Seh'", "ich", ",", "wie", "ein", "an\u00b7d'\u00b7rer", "blit\u00b7zet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "ART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der beym ersten nahe sitzet;", "tokens": ["Der", "beym", "ers\u00b7ten", "na\u00b7he", "sit\u00b7zet", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "ADJA", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "So gedenck\u2019 ich wer wol misst,", "tokens": ["So", "ge\u00b7denck", "ich", "wer", "wol", "misst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "PWS", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Welch ein Raum dazwischen ist?", "tokens": ["Welch", "ein", "Raum", "da\u00b7zwi\u00b7schen", "ist", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "NN", "PAV", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}