{"textgrid.poem.49323": {"metadata": {"author": {"name": "Rist, Johann", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Kaiser von Bysanz sa\u00df einsmals sonder Sorgen", "genre": "verse", "period": "N.A.", "pub_year": 1637, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Kaiser von Bysanz sa\u00df einsmals sonder Sorgen", "tokens": ["Der", "Kai\u00b7ser", "von", "By\u00b7sanz", "sa\u00df", "eins\u00b7mals", "son\u00b7der", "Sor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NE", "VVFIN", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bei seinen R\u00e4ten bis schier an den lichten Morgen,", "tokens": ["Bei", "sei\u00b7nen", "R\u00e4\u00b7ten", "bis", "schier", "an", "den", "lich\u00b7ten", "Mor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Vom Wein und Freuden voll, stund endlich auf und kam", "tokens": ["Vom", "Wein", "und", "Freu\u00b7den", "voll", ",", "stund", "end\u00b7lich", "auf", "und", "kam"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "KON", "NN", "ADJD", "$,", "VVFIN", "ADV", "PTKVZ", "KON", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Zu einem klugen Rat, den er besonders nahm,", "tokens": ["Zu", "ei\u00b7nem", "klu\u00b7gen", "Rat", ",", "den", "er", "be\u00b7son\u00b7ders", "nahm", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Sprach: \u00bbFreund, du bist mir lieb, jetzt solt du was begehren;", "tokens": ["Sprach", ":", "\u00bb", "Freund", ",", "du", "bist", "mir", "lieb", ",", "jetzt", "solt", "du", "was", "be\u00b7geh\u00b7ren", ";"], "token_info": ["word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "$(", "NN", "$,", "PPER", "VAFIN", "PPER", "ADJD", "$,", "ADV", "VMFIN", "PPER", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Sag an, was sol es sein? ich wil dirs schnell gew\u00e4hren;", "tokens": ["Sag", "an", ",", "was", "sol", "es", "sein", "?", "ich", "wil", "dirs", "schnell", "ge\u00b7w\u00e4h\u00b7ren", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "PWS", "VMFIN", "PPER", "VAINF", "$.", "PPER", "VMFIN", "PIS", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Denn deine gro\u00dfe Treu hats ja vorl\u00e4ngst gemacht,", "tokens": ["Denn", "dei\u00b7ne", "gro\u00b7\u00dfe", "Treu", "hats", "ja", "vor\u00b7l\u00e4ngst", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VAFIN", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Da\u00df ich mit Gaben dich zu ehren bin bedacht.\u00ab", "tokens": ["Da\u00df", "ich", "mit", "Ga\u00b7ben", "dich", "zu", "eh\u00b7ren", "bin", "be\u00b7dacht", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "PPER", "PTKZU", "VVINF", "VAFIN", "VVPP", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "\u00bbnein\u00ab, sprach der tapfer Mann, \u00bbich lasse mich begn\u00fcgen", "tokens": ["\u00bb", "nein", "\u00ab", ",", "sprach", "der", "tap\u00b7fer", "Mann", ",", "\u00bb", "ich", "las\u00b7se", "mich", "be\u00b7gn\u00fc\u00b7gen"], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["$(", "PTKANT", "$(", "$,", "VVFIN", "ART", "ADJA", "NN", "$,", "$(", "PPER", "VVFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "An kaiserlicher Gnad', als die mir kan zuf\u00fcgen", "tokens": ["An", "kai\u00b7ser\u00b7li\u00b7cher", "Gnad'", ",", "als", "die", "mir", "kan", "zu\u00b7f\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "KOUS", "ART", "PPER", "VMFIN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Das, was mir n\u00f6tig ist; ich habe schon so viel,", "tokens": ["Das", ",", "was", "mir", "n\u00f6\u00b7tig", "ist", ";", "ich", "ha\u00b7be", "schon", "so", "viel", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PWS", "PPER", "ADJD", "VAFIN", "$.", "PPER", "VAFIN", "ADV", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Als ich zu diesem mal nicht bitten kan noch wil.\u00ab", "tokens": ["Als", "ich", "zu", "die\u00b7sem", "mal", "nicht", "bit\u00b7ten", "kan", "noch", "wil", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "APPR", "PDAT", "ADV", "PTKNEG", "VVINF", "VMFIN", "ADV", "VMFIN", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "\u00bbwol\u00ab, sprach der Kaiser dann, \u00bbdieweil dirs nicht ist eben,", "tokens": ["\u00bb", "wol", "\u00ab", ",", "sprach", "der", "Kai\u00b7ser", "dann", ",", "\u00bb", "die\u00b7weil", "dirs", "nicht", "ist", "e\u00b7ben", ","], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "$(", "$,", "VVFIN", "ART", "NN", "ADV", "$,", "$(", "KOUS", "PIS", "PTKNEG", "VAFIN", "ADV", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Zu bitten, kan ich auch wol ohngebeten geben:", "tokens": ["Zu", "bit\u00b7ten", ",", "kan", "ich", "auch", "wol", "ohn\u00b7ge\u00b7be\u00b7ten", "ge\u00b7ben", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "VMFIN", "PPER", "ADV", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Ich wil, was heimlich ist, dir alles zeigen an,", "tokens": ["Ich", "wil", ",", "was", "heim\u00b7lich", "ist", ",", "dir", "al\u00b7les", "zei\u00b7gen", "an", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "PRELS", "ADJD", "VAFIN", "$,", "PPER", "PIS", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Ja, was mein Ehgemahl kaum recht erfahren kan;", "tokens": ["Ja", ",", "was", "mein", "Eh\u00b7ge\u00b7mahl", "kaum", "recht", "er\u00b7fah\u00b7ren", "kan", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PRELS", "PPOSAT", "NN", "ADV", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Was meine Seele wei\u00df, das wil ich dir vertrauen.\u00ab", "tokens": ["Was", "mei\u00b7ne", "See\u00b7le", "wei\u00df", ",", "das", "wil", "ich", "dir", "ver\u00b7trau\u00b7en", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "PPOSAT", "NN", "VVFIN", "$,", "PDS", "VMFIN", "PPER", "PPER", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "\u00bbnein\u00ab, rief der kluge Rat, \u00bbach, Herr, jetzt thut mir grauen,", "tokens": ["\u00bb", "nein", "\u00ab", ",", "rief", "der", "klu\u00b7ge", "Rat", ",", "\u00bb", "ach", ",", "Herr", ",", "jetzt", "thut", "mir", "grau\u00b7en", ","], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKANT", "$(", "$,", "VVFIN", "ART", "ADJA", "NN", "$,", "$(", "ITJ", "$,", "NN", "$,", "ADV", "VVFIN", "PPER", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Die Gnad' ist gar zu gro\u00df, drum wil ich den Bericht,", "tokens": ["Die", "Gnad'", "ist", "gar", "zu", "gro\u00df", ",", "drum", "wil", "ich", "den", "Be\u00b7richt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "PTKA", "ADJD", "$,", "PAV", "VMFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Den niemand wissen sol, auch selber wissen nicht.\u00ab", "tokens": ["Den", "nie\u00b7mand", "wis\u00b7sen", "sol", ",", "auch", "sel\u00b7ber", "wis\u00b7sen", "nicht", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "PIS", "VVINF", "VMFIN", "$,", "ADV", "ADV", "VVFIN", "PTKNEG", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Der Kaiser wolt' hierauf der Reden Ursach h\u00f6ren.", "tokens": ["Der", "Kai\u00b7ser", "wolt'", "hier\u00b7auf", "der", "Re\u00b7den", "Ur\u00b7sach", "h\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PAV", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "\u00bbganz gerne\u00ab, sprach der Rat, \u00bbdas wil ich schleunigst lehren,", "tokens": ["\u00bb", "ganz", "ger\u00b7ne", "\u00ab", ",", "sprach", "der", "Rat", ",", "\u00bb", "das", "wil", "ich", "schleu\u00b7nigst", "leh\u00b7ren", ","], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADV", "$(", "$,", "VVFIN", "ART", "NN", "$,", "$(", "PDS", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Warum, o gro\u00dfer Herr, ich g\u00e4nzlich nicht begehr',", "tokens": ["Wa\u00b7rum", ",", "o", "gro\u00b7\u00dfer", "Herr", ",", "ich", "g\u00e4nz\u00b7lich", "nicht", "be\u00b7gehr'", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "FM", "ADJA", "NN", "$,", "PPER", "ADJD", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Zu wissen, das mir einst kan schaffen viel Beschwer.", "tokens": ["Zu", "wis\u00b7sen", ",", "das", "mir", "einst", "kan", "schaf\u00b7fen", "viel", "Be\u00b7schwer", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "PRELS", "PPER", "ADV", "VMFIN", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Es ist ja mancher Herr, der kaum sich selber zwinget,", "tokens": ["Es", "ist", "ja", "man\u00b7cher", "Herr", ",", "der", "kaum", "sich", "sel\u00b7ber", "zwin\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PIAT", "NN", "$,", "PRELS", "ADV", "PRF", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Da\u00df er nicht unters Volk sein' Heimlichkeiten bringet,", "tokens": ["Da\u00df", "er", "nicht", "un\u00b7ters", "Volk", "sein'", "Heim\u00b7lich\u00b7kei\u00b7ten", "brin\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "APPRART", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Und wann sichs dann begibt, da\u00df das, was heimlich war,", "tokens": ["Und", "wann", "sichs", "dann", "be\u00b7gibt", ",", "da\u00df", "das", ",", "was", "heim\u00b7lich", "war", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PIS", "ADV", "VVFIN", "$,", "KOUS", "PDS", "$,", "PRELS", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Wird durch ihr eignes Maul den Leuten offenbar,", "tokens": ["Wird", "durch", "ihr", "eig\u00b7nes", "Maul", "den", "Leu\u00b7ten", "of\u00b7fen\u00b7bar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPOSAT", "ADJA", "NN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "So mu\u00df der oft die Schuld, der ohne Schuld ist, tragen,", "tokens": ["So", "mu\u00df", "der", "oft", "die", "Schuld", ",", "der", "oh\u00b7ne", "Schuld", "ist", ",", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "ADV", "ART", "NN", "$,", "PRELS", "APPR", "NN", "VAFIN", "$,", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "F\u00e4llt drob in Ungenad'.\u00ab Hie mu\u00df ich nochmals sagen:", "tokens": ["F\u00e4llt", "drob", "in", "Un\u00b7ge\u00b7nad'", ".", "\u00ab", "Hie", "mu\u00df", "ich", "noch\u00b7mals", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "NN", "$.", "$(", "ADV", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Ein recht verst\u00e4ndig Herz das h\u00fctet sich f\u00fcrwahr", "tokens": ["Ein", "recht", "ver\u00b7st\u00e4n\u00b7dig", "Herz", "das", "h\u00fc\u00b7tet", "sich", "f\u00fcr\u00b7wahr"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "ADJD", "NN", "PDS", "VVFIN", "PRF", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Vor Herren Heimlichkeit, so bleibt es ohn' Gefahr.", "tokens": ["Vor", "Her\u00b7ren", "Heim\u00b7lich\u00b7keit", ",", "so", "bleibt", "es", "ohn'", "Ge\u00b7fahr", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "$,", "ADV", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Der Kaiser von Bysanz sa\u00df einsmals sonder Sorgen", "tokens": ["Der", "Kai\u00b7ser", "von", "By\u00b7sanz", "sa\u00df", "eins\u00b7mals", "son\u00b7der", "Sor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NE", "VVFIN", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bei seinen R\u00e4ten bis schier an den lichten Morgen,", "tokens": ["Bei", "sei\u00b7nen", "R\u00e4\u00b7ten", "bis", "schier", "an", "den", "lich\u00b7ten", "Mor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Vom Wein und Freuden voll, stund endlich auf und kam", "tokens": ["Vom", "Wein", "und", "Freu\u00b7den", "voll", ",", "stund", "end\u00b7lich", "auf", "und", "kam"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "KON", "NN", "ADJD", "$,", "VVFIN", "ADV", "PTKVZ", "KON", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Zu einem klugen Rat, den er besonders nahm,", "tokens": ["Zu", "ei\u00b7nem", "klu\u00b7gen", "Rat", ",", "den", "er", "be\u00b7son\u00b7ders", "nahm", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Sprach: \u00bbFreund, du bist mir lieb, jetzt solt du was begehren;", "tokens": ["Sprach", ":", "\u00bb", "Freund", ",", "du", "bist", "mir", "lieb", ",", "jetzt", "solt", "du", "was", "be\u00b7geh\u00b7ren", ";"], "token_info": ["word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "$(", "NN", "$,", "PPER", "VAFIN", "PPER", "ADJD", "$,", "ADV", "VMFIN", "PPER", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Sag an, was sol es sein? ich wil dirs schnell gew\u00e4hren;", "tokens": ["Sag", "an", ",", "was", "sol", "es", "sein", "?", "ich", "wil", "dirs", "schnell", "ge\u00b7w\u00e4h\u00b7ren", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "PWS", "VMFIN", "PPER", "VAINF", "$.", "PPER", "VMFIN", "PIS", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Denn deine gro\u00dfe Treu hats ja vorl\u00e4ngst gemacht,", "tokens": ["Denn", "dei\u00b7ne", "gro\u00b7\u00dfe", "Treu", "hats", "ja", "vor\u00b7l\u00e4ngst", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VAFIN", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Da\u00df ich mit Gaben dich zu ehren bin bedacht.\u00ab", "tokens": ["Da\u00df", "ich", "mit", "Ga\u00b7ben", "dich", "zu", "eh\u00b7ren", "bin", "be\u00b7dacht", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "PPER", "PTKZU", "VVINF", "VAFIN", "VVPP", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "\u00bbnein\u00ab, sprach der tapfer Mann, \u00bbich lasse mich begn\u00fcgen", "tokens": ["\u00bb", "nein", "\u00ab", ",", "sprach", "der", "tap\u00b7fer", "Mann", ",", "\u00bb", "ich", "las\u00b7se", "mich", "be\u00b7gn\u00fc\u00b7gen"], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["$(", "PTKANT", "$(", "$,", "VVFIN", "ART", "ADJA", "NN", "$,", "$(", "PPER", "VVFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "An kaiserlicher Gnad', als die mir kan zuf\u00fcgen", "tokens": ["An", "kai\u00b7ser\u00b7li\u00b7cher", "Gnad'", ",", "als", "die", "mir", "kan", "zu\u00b7f\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "KOUS", "ART", "PPER", "VMFIN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Das, was mir n\u00f6tig ist; ich habe schon so viel,", "tokens": ["Das", ",", "was", "mir", "n\u00f6\u00b7tig", "ist", ";", "ich", "ha\u00b7be", "schon", "so", "viel", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PWS", "PPER", "ADJD", "VAFIN", "$.", "PPER", "VAFIN", "ADV", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Als ich zu diesem mal nicht bitten kan noch wil.\u00ab", "tokens": ["Als", "ich", "zu", "die\u00b7sem", "mal", "nicht", "bit\u00b7ten", "kan", "noch", "wil", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "APPR", "PDAT", "ADV", "PTKNEG", "VVINF", "VMFIN", "ADV", "VMFIN", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "\u00bbwol\u00ab, sprach der Kaiser dann, \u00bbdieweil dirs nicht ist eben,", "tokens": ["\u00bb", "wol", "\u00ab", ",", "sprach", "der", "Kai\u00b7ser", "dann", ",", "\u00bb", "die\u00b7weil", "dirs", "nicht", "ist", "e\u00b7ben", ","], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "$(", "$,", "VVFIN", "ART", "NN", "ADV", "$,", "$(", "KOUS", "PIS", "PTKNEG", "VAFIN", "ADV", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Zu bitten, kan ich auch wol ohngebeten geben:", "tokens": ["Zu", "bit\u00b7ten", ",", "kan", "ich", "auch", "wol", "ohn\u00b7ge\u00b7be\u00b7ten", "ge\u00b7ben", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "VMFIN", "PPER", "ADV", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Ich wil, was heimlich ist, dir alles zeigen an,", "tokens": ["Ich", "wil", ",", "was", "heim\u00b7lich", "ist", ",", "dir", "al\u00b7les", "zei\u00b7gen", "an", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "PRELS", "ADJD", "VAFIN", "$,", "PPER", "PIS", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Ja, was mein Ehgemahl kaum recht erfahren kan;", "tokens": ["Ja", ",", "was", "mein", "Eh\u00b7ge\u00b7mahl", "kaum", "recht", "er\u00b7fah\u00b7ren", "kan", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PRELS", "PPOSAT", "NN", "ADV", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Was meine Seele wei\u00df, das wil ich dir vertrauen.\u00ab", "tokens": ["Was", "mei\u00b7ne", "See\u00b7le", "wei\u00df", ",", "das", "wil", "ich", "dir", "ver\u00b7trau\u00b7en", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "PPOSAT", "NN", "VVFIN", "$,", "PDS", "VMFIN", "PPER", "PPER", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "\u00bbnein\u00ab, rief der kluge Rat, \u00bbach, Herr, jetzt thut mir grauen,", "tokens": ["\u00bb", "nein", "\u00ab", ",", "rief", "der", "klu\u00b7ge", "Rat", ",", "\u00bb", "ach", ",", "Herr", ",", "jetzt", "thut", "mir", "grau\u00b7en", ","], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKANT", "$(", "$,", "VVFIN", "ART", "ADJA", "NN", "$,", "$(", "ITJ", "$,", "NN", "$,", "ADV", "VVFIN", "PPER", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Die Gnad' ist gar zu gro\u00df, drum wil ich den Bericht,", "tokens": ["Die", "Gnad'", "ist", "gar", "zu", "gro\u00df", ",", "drum", "wil", "ich", "den", "Be\u00b7richt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "PTKA", "ADJD", "$,", "PAV", "VMFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Den niemand wissen sol, auch selber wissen nicht.\u00ab", "tokens": ["Den", "nie\u00b7mand", "wis\u00b7sen", "sol", ",", "auch", "sel\u00b7ber", "wis\u00b7sen", "nicht", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "PIS", "VVINF", "VMFIN", "$,", "ADV", "ADV", "VVFIN", "PTKNEG", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Der Kaiser wolt' hierauf der Reden Ursach h\u00f6ren.", "tokens": ["Der", "Kai\u00b7ser", "wolt'", "hier\u00b7auf", "der", "Re\u00b7den", "Ur\u00b7sach", "h\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PAV", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "\u00bbganz gerne\u00ab, sprach der Rat, \u00bbdas wil ich