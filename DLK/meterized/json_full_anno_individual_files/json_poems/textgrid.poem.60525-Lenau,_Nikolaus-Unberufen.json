: ["Als", "die", "h\u00f6chs\u00b7te", "Kunst", "von", "al\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "APPR", "PIAT", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Hat kein Urteil hinzulallen.", "tokens": ["Hat", "kein", "Ur\u00b7teil", "hin\u00b7zu\u00b7lal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Eben weil ihm ihre Zeichen", "tokens": ["E\u00b7ben", "weil", "ihm", "ih\u00b7re", "Zei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Altvertraut sind, d\u00fcnkt ihm alt", "tokens": ["Alt\u00b7ver\u00b7traut", "sind", ",", "d\u00fcnkt", "ihm", "alt"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "$,", "VVFIN", "PPER", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.23": {"text": "Und vertraut auch ihr Gehalt,", "tokens": ["Und", "ver\u00b7traut", "auch", "ihr", "Ge\u00b7halt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.24": {"text": "Und er wird ihn nie erreichen;", "tokens": ["Und", "er", "wird", "ihn", "nie", "er\u00b7rei\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PPER", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "Ewig schlie\u00dft f\u00fcr ihn die Pforte;", "tokens": ["E\u00b7wig", "schlie\u00dft", "f\u00fcr", "ihn", "die", "Pfor\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN