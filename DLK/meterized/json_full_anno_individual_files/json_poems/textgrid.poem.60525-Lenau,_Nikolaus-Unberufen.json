{"textgrid.poem.60525": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Unberufen", "genre": "verse", "period": "N.A.", "pub_year": 1839, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nicht ein jeder wagt zu richten", "tokens": ["Nicht", "ein", "je\u00b7der", "wagt", "zu", "rich\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ART", "PIS", "VVFIN", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Meister, so in Farben dichten,", "tokens": ["Meis\u00b7ter", ",", "so", "in", "Far\u00b7ben", "dich\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "APPR", "NN", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Noch des Meisters Flug in T\u00f6nen", "tokens": ["Noch", "des", "Meis\u00b7ters", "Flug", "in", "T\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schnell zu tadeln, flink zu kr\u00f6nen;", "tokens": ["Schnell", "zu", "ta\u00b7deln", ",", "flink", "zu", "kr\u00f6\u00b7nen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKZU", "VVINF", "$,", "ADJD", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Denn mit Farben und Gestalten", "tokens": ["Denn", "mit", "Far\u00b7ben", "und", "Ge\u00b7stal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wei\u00df der Laie nicht zu schalten,", "tokens": ["Wei\u00df", "der", "Lai\u00b7e", "nicht", "zu", "schal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und im Kontrapunkt zu reden,", "tokens": ["Und", "im", "Kon\u00b7tra\u00b7punkt", "zu", "re\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ist nicht Sache eines jeden.", "tokens": ["Ist", "nicht", "Sa\u00b7che", "ei\u00b7nes", "je\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "NN", "ART", "PIAT", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Doch des Worts ist, so und so,", "tokens": ["Doch", "des", "Worts", "ist", ",", "so", "und", "so", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "$,", "ADV", "KON", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Wer nicht stumm, ein jeder froh.", "tokens": ["Wer", "nicht", "stumm", ",", "ein", "je\u00b7der", "froh", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PTKNEG", "ADJD", "$,", "ART", "PIS", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Darum wer in Worten dichtet,", "tokens": ["Da\u00b7rum", "wer", "in", "Wor\u00b7ten", "dich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PWS", "APPR", "NN", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.12": {"text": "Wird vom ganzen Tro\u00df gerichtet;", "tokens": ["Wird", "vom", "gan\u00b7zen", "Tro\u00df", "ge\u00b7rich\u00b7tet", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "ADJA", "N