{"textgrid.poem.53245": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nimm, O Churf\u00fcrst, Dir die Zeit,", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nimm, O Churf\u00fcrst, Dir die Zeit,", "tokens": ["Nimm", ",", "O", "Chur\u00b7f\u00fcrst", ",", "Dir", "die", "Zeit", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "NE", "$,", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Thu nur jetzund an die Seit", "tokens": ["Thu", "nur", "je\u00b7tzund", "an", "die", "Seit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Des verwirrten Krieges Sorgen,", "tokens": ["Des", "ver\u00b7wirr\u00b7ten", "Krie\u00b7ges", "Sor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dieser Tag, so Dich der Welt", "tokens": ["Die\u00b7ser", "Tag", ",", "so", "Dich", "der", "Welt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDAT", "NN", "$,", "ADV", "PPER", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und Dich uns geschenckt, O Held,", "tokens": ["Und", "Dich", "uns", "ge\u00b7schenckt", ",", "O", "Held", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "PPER", "VVPP", "$,", "NE", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Die gew\u00fcnschte Stund' ist morgen.", "tokens": ["Die", "ge\u00b7w\u00fcnschte", "Stund'", "ist", "mor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Auff, mach' Anstand selbst mit Dir,", "tokens": ["Auff", ",", "mach'", "An\u00b7stand", "selbst", "mit", "Dir", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "VVFIN", "NN", "ADV", "APPR", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Gib dem Tage seine Zier,", "tokens": ["Gib", "dem", "Ta\u00b7ge", "sei\u00b7ne", "Zier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Der von keiner Noht wil wissen,", "tokens": ["Der", "von", "kei\u00b7ner", "Noht", "wil", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PIAT", "NN", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sondern sich mit Danck allein,", "tokens": ["Son\u00b7dern", "sich", "mit", "Danck", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "APPR", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Welcher Gott genehm kan seyn", "tokens": ["Wel\u00b7cher", "Gott", "ge\u00b7nehm", "kan", "seyn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAT", "NN", "ADJD", "VMFIN", "PPOSAT"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Und mit Fr\u00f6licheit wil schliessen.", "tokens": ["Und", "mit", "Fr\u00f6\u00b7lic\u00b7heit", "wil", "schlies\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VMFIN", "VVINF", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}}, "stanza.3": {"line.1": {"text": "Sey uns gn\u00e4dig, g\u00f6nn dein Ohr", "tokens": ["Sey", "uns", "gn\u00e4\u00b7dig", ",", "g\u00f6nn", "dein", "Ohr"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADJD", "$,", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Unsrer Musen hellem Chor,", "tokens": ["Uns\u00b7rer", "Mu\u00b7sen", "hel\u00b7lem", "Chor", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die schier mit dem Tode ringen,", "tokens": ["Die", "schier", "mit", "dem", "To\u00b7de", "rin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Aber Deinetwegen itzt", "tokens": ["A\u00b7ber", "Dei\u00b7net\u00b7we\u00b7gen", "itzt"], "token_info": ["word", "word", "word"], "pos": ["KON", "PPOSAT", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sich erholen, und erhitzt", "tokens": ["Sich", "er\u00b7ho\u00b7len", ",", "und", "er\u00b7hitzt"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PRF", "VVINF", "$,", "KON", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Gott f\u00fcr Dich ein Opffer bringen.", "tokens": ["Gott", "f\u00fcr", "Dich", "ein", "Opf\u00b7fer", "brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Ihrer Stimmen Werck und Ziel", "tokens": ["Ih\u00b7rer", "Stim\u00b7men", "Werck", "und", "Ziel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Das bist Du und Carl Aemil,", "tokens": ["Das", "bist", "Du", "und", "Carl", "A\u00b7e\u00b7mil", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "KON", "NE", "NE", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Welche wir zugleich erheben,", "tokens": ["Wel\u00b7che", "wir", "zu\u00b7gleich", "er\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Weil des weisen Himmels Schlu\u00df,", "tokens": ["Weil", "des", "wei\u00b7sen", "Him\u00b7mels", "Schlu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Dem sein Werck bekant seyn mu\u00df,", "tokens": ["Dem", "sein", "Werck", "be\u00b7kant", "seyn", "mu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADJD", "VAINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ein Geburts-Fest Euch gegeben.", "tokens": ["Ein", "Ge\u00b7burts\u00b7Fest", "Euch", "ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Wir gestehen, Herr, da\u00df Du", "tokens": ["Wir", "ge\u00b7ste\u00b7hen", ",", "Herr", ",", "da\u00df", "Du"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PPER", "VVPP", "$,", "NN", "$,", "KOUS", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Seyst nechst Gott noch unsre Ruh'", "tokens": ["Seyst", "nechst", "Gott", "noch", "uns\u00b7re", "Ruh'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "NN", "ADV", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und vom Himmel uns erkohren,", "tokens": ["Und", "vom", "Him\u00b7mel", "uns", "er\u00b7koh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Seyst der Ancker unserm Boht,", "tokens": ["Seyst", "der", "An\u00b7cker", "un\u00b7serm", "Boht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sonderlich in dieser Noht,", "tokens": ["Son\u00b7der\u00b7lich", "in", "die\u00b7ser", "Noht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Denn ohn Dich sind wir verlohren.", "tokens": ["Denn", "ohn", "Dich", "sind", "wir", "ver\u00b7loh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VAFIN", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Leb' uns, sprechen wir zu hauff,", "tokens": ["Leb'", "uns", ",", "spre\u00b7chen", "wir", "zu", "hauff", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Weil die Sonn' h\u00e4lt ihren Lauff", "tokens": ["Weil", "die", "Sonn'", "h\u00e4lt", "ih\u00b7ren", "Lauff"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die Sterne sich bewegen,", "tokens": ["Und", "die", "Ster\u00b7ne", "sich", "be\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "M\u00fcsse deines Samens Hand", "tokens": ["M\u00fcs\u00b7se", "dei\u00b7nes", "Sa\u00b7mens", "Hand"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Herrschen \u00fcber Leut' und Land", "tokens": ["Herr\u00b7schen", "\u00fc\u00b7ber", "Leut'", "und", "Land"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Und empfinden Macht und Segen.", "tokens": ["Und", "emp\u00b7fin\u00b7den", "Macht", "und", "Se\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Auch Loysa, deine Rast,", "tokens": ["Auch", "Loy\u00b7sa", ",", "dei\u00b7ne", "Rast", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "NE", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Sampt der M\u00fctterlichen Last", "tokens": ["Sampt", "der", "M\u00fct\u00b7ter\u00b7li\u00b7chen", "Last"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "M\u00fcsse sich wolauff befinden,", "tokens": ["M\u00fcs\u00b7se", "sich", "wo\u00b7lauff", "be\u00b7fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schrecken, Fall und Traurigheit", "tokens": ["Schre\u00b7cken", ",", "Fall", "und", "Trau\u00b7rig\u00b7heit"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Bleibe fern, da\u00df ihre Zeit", "tokens": ["Blei\u00b7be", "fern", ",", "da\u00df", "ih\u00b7re", "Zeit"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PTKVZ", "$,", "KOUS", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Sie in Freuden m\u00f6g' entbinden.", "tokens": ["Sie", "in", "Freu\u00b7den", "m\u00f6g'", "ent\u00b7bin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Dieses w\u00fcnschen in gemein", "tokens": ["Die\u00b7ses", "w\u00fcn\u00b7schen", "in", "ge\u00b7mein"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Arm und Reich und Gro\u00df und Klein", "tokens": ["Arm", "und", "Reich", "und", "Gro\u00df", "und", "Klein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "KON", "NE", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und di\u00df edle St\u00fcck daneben,", "tokens": ["Und", "di\u00df", "ed\u00b7le", "St\u00fcck", "da\u00b7ne\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "ADJA", "NN", "PAV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df dein Helden-fester Sinn", "tokens": ["Da\u00df", "dein", "Hel\u00b7den\u00b7fes\u00b7ter", "Sinn"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Bald leg' alle Waffen hin", "tokens": ["Bald", "leg'", "al\u00b7le", "Waf\u00b7fen", "hin"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "PTKVZ"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Und uns schencke Fried' und Leben.", "tokens": ["Und", "uns", "schen\u00b7cke", "Fried'", "und", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.9": {"line.1": {"text": "La\u00df Dir doch zu Hertzen gehn", "tokens": ["La\u00df", "Dir", "doch", "zu", "Hert\u00b7zen", "gehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ADV", "APPR", "NN", "VVINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Unsre Noht, darin wir stehn!", "tokens": ["Uns\u00b7re", "Noht", ",", "da\u00b7rin", "wir", "stehn", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PAV", "PPER", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Zwar ein Held liebt Schlacht und Kriege", "tokens": ["Zwar", "ein", "Held", "liebt", "Schlacht", "und", "Krie\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VVFIN", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und sucht also Ruhm und Ehr',", "tokens": ["Und", "sucht", "al\u00b7so", "Ruhm", "und", "Ehr'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "NN", "KON", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Aber ein Fried' ist weit mehr", "tokens": ["A\u00b7ber", "ein", "Fried'", "ist", "weit", "mehr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VAFIN", "ADJD", "ADV"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.6": {"text": "Als viel hundert tausent Siege.", "tokens": ["Als", "viel", "hun\u00b7dert", "tau\u00b7sent", "Sie\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "CARD", "CARD", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Nimm, O Churf\u00fcrst, Dir die Zeit,", "tokens": ["Nimm", ",", "O", "Chur\u00b7f\u00fcrst", ",", "Dir", "die", "Zeit", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "NE", "$,", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Thu nur jetzund an die Seit", "tokens": ["Thu", "nur", "je\u00b7tzund", "an", "die", "Seit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Des verwirrten Krieges Sorgen,", "tokens": ["Des", "ver\u00b7wirr\u00b7ten", "Krie\u00b7ges", "Sor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dieser Tag, so Dich der Welt", "tokens": ["Die\u00b7ser", "Tag", ",", "so", "Dich", "der", "Welt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDAT", "NN", "$,", "ADV", "PPER", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und Dich uns geschenckt, O Held,", "tokens": ["Und", "Dich", "uns", "ge\u00b7schenckt", ",", "O", "Held", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "PPER", "VVPP", "$,", "NE