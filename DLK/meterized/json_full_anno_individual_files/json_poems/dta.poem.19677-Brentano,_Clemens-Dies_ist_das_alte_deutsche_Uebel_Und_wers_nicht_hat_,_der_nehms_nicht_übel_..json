{"dta.poem.19677": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Dies ist das alte deutsche Uebel  \n Und wers nicht hat ,  der nehms nicht \u00fcbel .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519168", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Welcher Mann ein Henn hat die nicht Eyer legt.               ", "tokens": ["Wel\u00b7cher", "Mann", "ein", "Henn", "hat", "die", "nicht", "Ey\u00b7er", "legt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "ART", "NN", "VAFIN", "ART", "PTKNEG", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Und ein Sau die nicht Junge tr\u00e4gt,", "tokens": ["Und", "ein", "Sau", "die", "nicht", "Jun\u00b7ge", "tr\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "PTKNEG", "NN", "VVFIN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Und ein Kuh die nicht Milch giebt,", "tokens": ["Und", "ein", "Kuh", "die", "nicht", "Milch", "giebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "PTKNEG", "NN", "VVFIN", "$,"], "meter": "+-+-+++", "measure": "unknown.measure.penta"}, "line.4": {"text": "Und ein Tochter die all Nacht ausliegt,", "tokens": ["Und", "ein", "Toch\u00b7ter", "die", "all", "Nacht", "aus\u00b7liegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "PIAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Und ein Sohn der allzeit gern spielt,", "tokens": ["Und", "ein", "Sohn", "der", "all\u00b7zeit", "gern", "spielt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "ADV", "ADV", "VVFIN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.6": {"text": "Und ein Frau die ihm heimlich abstiehlt,", "tokens": ["Und", "ein", "Frau", "die", "ihm", "heim\u00b7lich", "ab\u00b7stiehlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "PPER", "ADJD", "VVFIN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.7": {"text": "Und ein Magd die da geht mit einem Kind,", "tokens": ["Und", "ein", "Magd", "die", "da", "geht", "mit", "ei\u00b7nem", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "ADV", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.8": {"text": "F\u00fcrwahr der hat ein unn\u00fctz Hausgesind.", "tokens": ["F\u00fcr\u00b7wahr", "der", "hat", "ein", "un\u00b7n\u00fctz", "Haus\u00b7ge\u00b7sind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "VAFIN", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Doch ist noch eine schlimmre Qual,", "tokens": ["Doch", "ist", "noch", "ei\u00b7ne", "schlimm\u00b7re", "Qual", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Die trit die Leute an auf einmal,", "tokens": ["Die", "trit", "die", "Leu\u00b7te", "an", "auf", "ein\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "APPR", "APPR", "ADV", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Auf den hohen Rossen die Reitersknaben,", "tokens": ["Auf", "den", "ho\u00b7hen", "Ros\u00b7sen", "die", "Rei\u00b7ters\u00b7kna\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.12": {"text": "Die k\u00f6nnen ihr nicht leicht enttraben,", "tokens": ["Die", "k\u00f6n\u00b7nen", "ihr", "nicht", "leicht", "ent\u00b7tra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "PTKNEG", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Die kommt von freundlicher Botschaft schicken,", "tokens": ["Die", "kommt", "von", "freund\u00b7li\u00b7cher", "Bot\u00b7schaft", "schi\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "Brieflein schreiben, Augen blicken,", "tokens": ["Brie\u00b7flein", "schrei\u00b7ben", ",", "Au\u00b7gen", "bli\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "M\u00fcndlein k\u00fcssen, H\u00e4ndlein greifen,", "tokens": ["M\u00fcnd\u00b7lein", "k\u00fcs\u00b7sen", ",", "H\u00e4nd\u00b7lein", "grei\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Lauten spielen, Nachtes Pfeifen,", "tokens": ["Lau\u00b7ten", "spie\u00b7len", ",", "Nach\u00b7tes", "Pfei\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Unter dem Tisch die F\u00fc\u00dflein treten,", "tokens": ["Un\u00b7ter", "dem", "Tisch", "die", "F\u00fc\u00df\u00b7lein", "tre\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.18": {"text": "Untern B\u00e4nken die Knielein kneten,", "tokens": ["Un\u00b7tern", "B\u00e4n\u00b7ken", "die", "Knie\u00b7lein", "kne\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.19": {"text": "Darnach dann zusammen rucken", "tokens": ["Dar\u00b7nach", "dann", "zu\u00b7sam\u00b7men", "ru\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "ADV", "ADV", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.20": {"text": "Und in die heimlichen Winkel schmucken,", "tokens": ["Und", "in", "die", "heim\u00b7li\u00b7chen", "Win\u00b7kel", "schmu\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.21": {"text": "Die rothen W\u00e4nglein dreschen,", "tokens": ["Die", "ro\u00b7then", "W\u00e4ng\u00b7lein", "dre\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.22": {"text": "Die schwarzen Hemdlein w\u00e4schen,", "tokens": ["Die", "schwar\u00b7zen", "Hemd\u00b7lein", "w\u00e4\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.23": {"text": "Silbern Kleinod schenken,", "tokens": ["Sil\u00b7bern", "Klei\u00b7nod", "schen\u00b7ken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.24": {"text": "Mit den Augbr\u00f6lein wenken,", "tokens": ["Mit", "den", "Aug\u00b7br\u00f6\u00b7lein", "wen\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.25": {"text": "Aus der Kirchen sich verstehlen,", "tokens": ["Aus", "der", "Kir\u00b7chen", "sich", "ver\u00b7steh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "Und in engen Gassen sich verhehlen,", "tokens": ["Und", "in", "en\u00b7gen", "Gas\u00b7sen", "sich", "ver\u00b7heh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "PRF", "VVINF", "$,"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.27": {"text": "All Stunden verbey laufen,", "tokens": ["All", "Stun\u00b7den", "ver\u00b7bey", "lau\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "VVINF", "$,"], "meter": "-+--++-", "measure": "iambic.tri.relaxed"}, "line.28": {"text": "Heut schlagen, morgen raufen.", "tokens": ["Heut", "schla\u00b7gen", ",", "mor\u00b7gen", "rau\u00b7fen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVINF", "$,", "ADV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.29": {"text": "Wer nun ein solches Uebel hat,", "tokens": ["Wer", "nun", "ein", "sol\u00b7ches", "Ue\u00b7bel", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "PIAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Der merk, wie es hernach ihm gaht,", "tokens": ["Der", "merk", ",", "wie", "es", "her\u00b7nach", "ihm", "gaht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "PPER", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+--++-+", "measure": "iambic.tetra.relaxed"}, "line.31": {"text": "Sein Schlaf wird ihm genommen gar,", "tokens": ["Sein", "Schlaf", "wird", "ihm", "ge\u00b7nom\u00b7men", "gar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "VVPP", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "So mu\u00df er laufen her und dar", "tokens": ["So", "mu\u00df", "er", "lau\u00b7fen", "her", "und", "dar"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "VVFIN", "PTKVZ", "KON", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Gleich wie ein w\u00fctender Hund,", "tokens": ["Gleich", "wie", "ein", "w\u00fc\u00b7ten\u00b7der", "Hund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.34": {"text": "Und kann geruhn zu keiner Stund.", "tokens": ["Und", "kann", "ge\u00b7ruhn", "zu", "kei\u00b7ner", "Stund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Wann er soll zu Tische sitzen,", "tokens": ["Wann", "er", "soll", "zu", "Ti\u00b7sche", "sit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VMFIN", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.36": {"text": "So wird er vor Aengsten schwitzen,", "tokens": ["So", "wird", "er", "vor", "A\u00b7engs\u00b7ten", "schwit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.37": {"text": "Hat manchen seltsamen Gedank,", "tokens": ["Hat", "man\u00b7chen", "selt\u00b7sa\u00b7men", "Ge\u00b7dank", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-++--+", "measure": "iambic.tetra.chol"}, "line.38": {"text": "Zeit und Weil wird ihm lang", "tokens": ["Zeit", "und", "Weil", "wird", "ihm", "lang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "VAFIN", "PPER", "ADJD"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.39": {"text": "Und thut nichts als H\u00f6lzlein schnitzen,", "tokens": ["Und", "thut", "nichts", "als", "H\u00f6lz\u00b7lein", "schnit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "KOKOM", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.40": {"text": "Mit denselben die W\u00e4nd zerkritzen,", "tokens": ["Mit", "den\u00b7sel\u00b7ben", "die", "W\u00e4nd", "zer\u00b7krit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ART", "NN", "VVINF", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.41": {"text": "Henkt unter sich sein Haupt,", "tokens": ["Henkt", "un\u00b7ter", "sich", "sein", "Haupt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PRF", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.42": {"text": "Von ihm wird gar niemand erfreut.", "tokens": ["Von", "ihm", "wird", "gar", "nie\u00b7mand", "er\u00b7freut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "ADV", "PIS", "VVFIN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.43": {"text": "F\u00fc\u00df rutschen, Teller stupfen,", "tokens": ["F\u00fc\u00df", "rut\u00b7schen", ",", "Tel\u00b7ler", "stup\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "NN", "VVINF", "$,"], "meter": "---+-+-", "measure": "unknown.measure.di"}, "line.44": {"text": "Hand-Zwehlen kn\u00fcpfen", "tokens": ["Han\u00b7dZweh\u00b7len", "kn\u00fcp\u00b7fen"], "token_info": ["word", "word"], "pos": ["NN", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.45": {"text": "Und auch die Gl\u00e4ser klenken,", "tokens": ["Und", "auch", "die", "Gl\u00e4\u00b7ser", "klen\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.46": {"text": "Manchen tiefen Seufzer senken,", "tokens": ["Man\u00b7chen", "tie\u00b7fen", "Seuf\u00b7zer", "sen\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.47": {"text": "Mit Messern Brod klopfen,", "tokens": ["Mit", "Mes\u00b7sern", "Brod", "klop\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.48": {"text": "Und die Finger ropfen,", "tokens": ["Und", "die", "Fin\u00b7ger", "rop\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.49": {"text": "Dazu auch \u00fcber sich sehen,", "tokens": ["Da\u00b7zu", "auch", "\u00fc\u00b7ber", "sich", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "APPR", "PRF", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.50": {"text": "Treibt er viel, es mu\u00df geschehen,", "tokens": ["Treibt", "er", "viel", ",", "es", "mu\u00df", "ge\u00b7sche\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "PPER", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.51": {"text": "Die Augen wirft er hin und dar", "tokens": ["Die", "Au\u00b7gen", "wirft", "er", "hin", "und", "dar"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ", "KON", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.52": {"text": "Und jetzt wird er der Metze Narr.", "tokens": ["Und", "jetzt", "wird", "er", "der", "Met\u00b7ze", "Narr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ART", "NN", "NE", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.53": {"text": "Alte Schuld und Schaden r\u00e4chen,", "tokens": ["Al\u00b7te", "Schuld", "und", "Scha\u00b7den", "r\u00e4\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.54": {"text": "Niemand mehr freundlich gesprechen", "tokens": ["Nie\u00b7mand", "mehr", "freund\u00b7lich", "ge\u00b7spre\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["PIS", "ADV", "ADJD", "VVPP"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.55": {"text": "Und Tischlacken schaben,", "tokens": ["Und", "Tisc\u00b7hla\u00b7cken", "scha\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.56": {"text": "Solche Zeichen mu\u00df er an sich haben.", "tokens": ["Sol\u00b7che", "Zei\u00b7chen", "mu\u00df", "er", 