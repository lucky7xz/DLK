{"dta.poem.15845": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "50.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Herr, deine Welt ist sch\u00f6n, Herr, deine Welt ist gut;", "tokens": ["Herr", ",", "dei\u00b7ne", "Welt", "ist", "sch\u00f6n", ",", "Herr", ",", "dei\u00b7ne", "Welt", "ist", "gut", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "VAFIN", "ADJD", "$,", "NN", "$,", "PPOSAT", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gib mir nur hellen Sinn, gib mir nur frohen Mut!", "tokens": ["Gib", "mir", "nur", "hel\u00b7len", "Sinn", ",", "gib", "mir", "nur", "fro\u00b7hen", "Mut", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ADJA", "NN", "$,", "VVIMP", "PPER", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ich f\u00fchle, da\u00df ich bin, ich f\u00fchle, da\u00df du bist,", "tokens": ["Ich", "f\u00fch\u00b7le", ",", "da\u00df", "ich", "bin", ",", "ich", "f\u00fch\u00b7le", ",", "da\u00df", "du", "bist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "VAFIN", "$,", "PPER", "VVFIN", "$,", "KOUS", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und da\u00df mein Seyn von dir ein sel'ger Abglanz ist.", "tokens": ["Und", "da\u00df", "mein", "Seyn", "von", "dir", "ein", "sel'\u00b7ger", "Ab\u00b7glanz", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPOSAT", "NN", "APPR", "PPER", "ART", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Die Welt beseligst du, beseligst dich in ihr;", "tokens": ["Die", "Welt", "be\u00b7se\u00b7ligst", "du", ",", "be\u00b7se\u00b7ligst", "dich", "in", "ihr", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "VVFIN", "PRF", "APPR", "PPOSAT", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sollt' ich nicht selig seyn, Allseliger, in dir!", "tokens": ["Sollt'", "ich", "nicht", "se\u00b7lig", "seyn", ",", "All\u00b7se\u00b7li\u00b7ger", ",", "in", "dir", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "ADJD", "VAINF", "$,", "ADJA", "$,", "APPR", "PPER", "$."], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}}}}}