{"textgrid.poem.60576": {"metadata": {"author": {"name": "Uz, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie wenig gleichen wir den Alten!", "genre": "verse", "period": "N.A.", "pub_year": 1758, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie wenig gleichen wir den Alten!", "tokens": ["Wie", "we\u00b7nig", "glei\u00b7chen", "wir", "den", "Al\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Was wir f\u00fcr wohlgesittet halten,", "tokens": ["Was", "wir", "f\u00fcr", "wohl\u00b7ge\u00b7sit\u00b7tet", "hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Hie\u00df ihnen Weichlichkeit.", "tokens": ["Hie\u00df", "ih\u00b7nen", "Weich\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Nur wenig \u00e4chte deutsche Sitten", "tokens": ["Nur", "we\u00b7nig", "\u00e4ch\u00b7te", "deut\u00b7sche", "Sit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PIAT", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Sind unverj\u00e4hrt und wohl gelitten", "tokens": ["Sind", "un\u00b7ver\u00b7j\u00e4hrt", "und", "wohl", "ge\u00b7lit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADJD", "KON", "ADV", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Zu ihrer Enkel Zeit.", "tokens": ["Zu", "ih\u00b7rer", "En\u00b7kel", "Zeit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Zusammen kommen, um zu zechen,", "tokens": ["Zu\u00b7sam\u00b7men", "kom\u00b7men", ",", "um", "zu", "ze\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "KOUI", "PTKZU", "VVINF", "$,"], "meter": "+--+--+--", "measure": "dactylic.tri"}, "line.2": {"text": "Bis Sprache, Sinn und Witz gebrechen,", "tokens": ["Bis", "Spra\u00b7che", ",", "Sinn", "und", "Witz", "ge\u00b7bre\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Hie\u00df ihnen Fr\u00f6hlichkeit.", "tokens": ["Hie\u00df", "ih\u00b7nen", "Fr\u00f6h\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Noch fliehn Vernunft und witzig Lachen,", "tokens": ["Noch", "fliehn", "Ver\u00b7nunft", "und", "wit\u00b7zig", "La\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "KON", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wo Bacchus herrschet, Platz zu machen,", "tokens": ["Wo", "Bac\u00b7chus", "herr\u00b7schet", ",", "Platz", "zu", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "VVFIN", "$,", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Zu ihrer Enkel Zeit.", "tokens": ["Zu", "ih\u00b7rer", "En\u00b7kel", "Zeit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Doch da\u00df, wer L\u00e4ndern rathen wollte,", "tokens": ["Doch", "da\u00df", ",", "wer", "L\u00e4n\u00b7dern", "ra\u00b7then", "woll\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "$,", "PWS", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Auch reifen Witz beweisen sollte,", "tokens": ["Auch", "rei\u00b7fen", "Witz", "be\u00b7wei\u00b7sen", "soll\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Hie\u00df ihnen Billigkeit.", "tokens": ["Hie\u00df", "ih\u00b7nen", "Bil\u00b7lig\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die n\u00fctzlichste von allen Gaben", "tokens": ["Die", "n\u00fctz\u00b7lichs\u00b7te", "von", "al\u00b7len", "Ga\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ist, einen schweren Seckel haben,", "tokens": ["Ist", ",", "ei\u00b7nen", "schwe\u00b7ren", "Se\u00b7ckel", "ha\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "ART", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Zu ihrer Enkel Zeit.", "tokens": ["Zu", "ih\u00b7rer", "En\u00b7kel", "Zeit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Da\u00df sich getreue Weiber funden,", "tokens": ["Da\u00df", "sich", "ge\u00b7treu\u00b7e", "Wei\u00b7ber", "fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die auch dem Golde widerstunden,", "tokens": ["Die", "auch", "dem", "Gol\u00b7de", "wi\u00b7der\u00b7stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Hie\u00df keine Seltenheit.", "tokens": ["Hie\u00df", "kei\u00b7ne", "Sel\u00b7ten\u00b7heit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Man sagt, zur Schande karger Reichen,", "tokens": ["Man", "sagt", ",", "zur", "Schan\u00b7de", "kar\u00b7ger", "Rei\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "APPRART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Es geb auch etliche dergleichen", "tokens": ["Es", "geb", "auch", "et\u00b7li\u00b7che", "derg\u00b7lei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PIAT", "PIS"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Zu ihrer Enkel Zeit.", "tokens": ["Zu", "ih\u00b7rer", "En\u00b7kel", "Zeit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Da\u00df aber auch in mannbarn Jahren", "tokens": ["Da\u00df", "a\u00b7ber", "auch", "in", "mann\u00b7barn", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ADV", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die T\u00f6chter neu im K\u00fcssen waren,", "tokens": ["Die", "T\u00f6ch\u00b7ter", "neu", "im", "K\u00fcs\u00b7sen", "wa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPRART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Hie\u00df ihnen Ehrbarkeit.", "tokens": ["Hie\u00df", "ih\u00b7nen", "Ehr\u00b7bar\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Das ist nur eine Sch\u00e4fertugend:", "tokens": ["Das", "ist", "nur", "ei\u00b7ne", "Sch\u00e4\u00b7fer\u00b7tu\u00b7gend", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wie altklug k\u00fc\u00dft die zarte Jugend", "tokens": ["Wie", "alt\u00b7klug", "k\u00fc\u00dft", "die", "zar\u00b7te", "Ju\u00b7gend"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Zu ihrer Enkel Zeit.", "tokens": ["Zu", "ih\u00b7rer", "En\u00b7kel", "Zeit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Da\u00df stets der k\u00fchne Junker jagte,", "tokens": ["Da\u00df", "stets", "der", "k\u00fch\u00b7ne", "Jun\u00b7ker", "jag\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Auch eh es auf den Bergen tagte,", "tokens": ["Auch", "eh", "es", "auf", "den", "Ber\u00b7gen", "tag\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Hie\u00df ihnen Tapferkeit.", "tokens": ["Hie\u00df", "ih\u00b7nen", "Tap\u00b7fer\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Noch jagt und schmaust er um die Wette,", "tokens": ["Noch", "jagt", "und", "schmaust", "er", "um", "die", "Wet\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Inde\u00df besorgt ein Freund sein Bette,", "tokens": ["In\u00b7de\u00df", "be\u00b7sorgt", "ein", "Freund", "sein", "Bet\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Zu ihrer Enkel Zeit.", "tokens": ["Zu", "ih\u00b7rer", "En\u00b7kel", "Zeit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "O la\u00dft sie ruhn, die guten Alten!", "tokens": ["O", "la\u00dft", "sie", "ruhn", ",", "die", "gu\u00b7ten", "Al\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "VVINF", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Treue, Wort und Bund zu halten,", "tokens": ["Die", "Treu\u00b7e", ",", "Wort", "und", "Bund", "zu", "hal\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "KON", "NN", "PTKZU", "VVINF