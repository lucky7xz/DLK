as", "Vn\u00b7recht", ",", "die", "Ge\u00b7walt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Die h\u00e4uffig offt mit eingerissen", "tokens": ["Die", "h\u00e4uf\u00b7fig", "offt", "mit", "ein\u00b7ge\u00b7ris\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "ADV", "APPR", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "In vielerley Gestalt?", "tokens": ["In", "vie\u00b7ler\u00b7ley", "Ge\u00b7stalt", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Ich habe mir ein Gut erlesen", "tokens": ["Ich", "ha\u00b7be", "mir", "ein", "Gut", "er\u00b7le\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das nicht wird unttergehn,", "tokens": ["Das", "nicht", "wird", "unt\u00b7ter\u00b7gehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "PTKNEG", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das Zeit und Fall und alles Wesen", "tokens": ["Das", "Zeit", "und", "Fall", "und", "al\u00b7les", "We\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN", "KON", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Welt wird \u00fcberstehn.", "tokens": ["Der", "Welt", "wird", "\u00fc\u00b7bers\u00b7tehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "An dem kein Vnrecht Theil kan haben,", "tokens": ["An", "dem", "kein", "Vn\u00b7recht", "Theil", "kan", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PIAT", "NN", "NN", "VMFIN", "VAINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df mich in dieser Zeit,", "tokens": ["Da\u00df", "mich", "in", "die\u00b7ser", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Was Ungl\u00fcck sich ereugt, kan laben", "tokens": ["Was", "Un\u00b7gl\u00fcck", "sich", "e\u00b7reugt", ",", "kan", "la\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWS", "NN", "PRF", "VVPP", "$,", "VMFIN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und dort in Ewigkeit.", "tokens": ["Und", "dort", "in", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Ich habe Lust aus diesem Leiden", "tokens": ["Ich", "ha\u00b7be", "Lust", "aus", "die\u00b7sem", "Lei\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "NN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Durch einen selign Tod,", "tokens": ["Durch", "ei\u00b7nen", "se\u00b7lign", "Tod", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In festem Glauben ab zu scheiden", "tokens": ["In", "fes\u00b7tem", "Glau\u00b7ben", "ab", "zu", "schei\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zu meinem wahren GOTT.", "tokens": ["Zu", "mei\u00b7nem", "wah\u00b7ren", "GoTT", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Vnd weil ich Christum mir erkohren,", "tokens": ["Vnd", "weil", "ich", "Chris\u00b7tum", "mir", "er\u00b7koh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "NE", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "W\u00fcnsch ich bey Ihm zu seyn,", "tokens": ["W\u00fcnsch", "ich", "bey", "Ihm", "zu", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "PPER", "PTKZU", "VAINF", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Denn alles halt ich sonst verlohren", "tokens": ["Denn", "al\u00b7les", "halt", "ich", "sonst", "ver\u00b7loh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "VVFIN", "PPER", "ADV", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ohn seine Gunst allein.", "tokens": ["Ohn", "sei\u00b7ne", "Gunst", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Kan dieser sch\u00f6ne Wunsch mir werden", "tokens": ["Kan", "die\u00b7ser", "sch\u00f6\u00b7ne", "Wunsch", "mir", "wer\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PDAT", "ADJA", "NN", "PPER", "VAFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sag' ich dem schn\u00f6den Pracht,", "tokens": ["Sag'", "ich", "dem", "schn\u00f6\u00b7den", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dem falschen Schein der eitlen Erden", "tokens": ["Dem", "fal\u00b7schen", "Schein", "der", "eit\u00b7len", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Vnd allem gutte Nacht.", "tokens": ["Vnd", "al\u00b7lem", "gut\u00b7te", "Nacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Herr Jesu steh mir nur zur Seiten,", "tokens": ["Herr", "Je\u00b7su", "steh", "mir", "nur", "zur", "Sei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Lehr mich die Welt-Begier", "tokens": ["Lehr", "mich", "die", "Welt\u00b7Be\u00b7gier"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PPER", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Sammt S\u00fcnde, Zeit und Tod bestreiten,", "tokens": ["Sammt", "S\u00fcn\u00b7de", ",", "Zeit", "und", "Tod", "be\u00b7strei\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$,", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und nimb mich bald zu Dir.", "tokens": ["Und", "nimb", "mich", "bald", "zu", "Dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}