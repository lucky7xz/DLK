{"dta.poem.5732": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Allgemeine Betrachtungen der Materie  \n und der Bewegung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Stoff verheelet sich den allersch\u00e4rffsten Augen,", "tokens": ["Der", "Stoff", "ver\u00b7hee\u00b7let", "sich", "den", "al\u00b7ler\u00b7sch\u00e4rffs\u00b7ten", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den Klumpen h\u00fcllet stets ein Schleyer ein;", "tokens": ["Den", "Klum\u00b7pen", "h\u00fcl\u00b7let", "stets", "ein", "Schle\u00b7yer", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und nur die \u00e4uss\u2019re Fl\u00e4ch allein,", "tokens": ["Und", "nur", "die", "\u00e4uss'\u00b7re", "Fl\u00e4ch", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kan unser Sinn zu fassen taugen.", "tokens": ["Kan", "un\u00b7ser", "Sinn", "zu", "fas\u00b7sen", "tau\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die Kr\u00e4ffte dauren stets, die fl\u00fcchtige Figur", "tokens": ["Die", "Kr\u00e4ff\u00b7te", "dau\u00b7ren", "stets", ",", "die", "fl\u00fcch\u00b7ti\u00b7ge", "Fi\u00b7gur"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ver\u00e4ndert sich, verdirbet nur.", "tokens": ["Ver\u00b7\u00e4n\u00b7dert", "sich", ",", "ver\u00b7dir\u00b7bet", "nur", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "$,", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ja k\u00f6nnte sich der Stoff, von allen fl\u00fccht\u2019gen Decken,", "tokens": ["Ja", "k\u00f6nn\u00b7te", "sich", "der", "Stoff", ",", "von", "al\u00b7len", "fl\u00fccht'\u00b7gen", "De\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "VMFIN", "PRF", "ART", "NN", "$,", "APPR", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "In eine nach der anderen verstecken,", "tokens": ["In", "ei\u00b7ne", "nach", "der", "an\u00b7de\u00b7ren", "ver\u00b7ste\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "APPR", "ART", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "So, da\u00df, durch stetiges Ver\u00e4ndern und Verkehren,", "tokens": ["So", ",", "da\u00df", ",", "durch", "ste\u00b7ti\u00b7ges", "Ver\u00b7\u00e4n\u00b7dern", "und", "Ver\u00b7keh\u00b7ren", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "$,", "APPR", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Der Aend\u2019rung Arten all ersch\u00f6pffet w\u00e4ren;", "tokens": ["Der", "A\u00b7en\u00b7d'\u00b7rung", "Ar\u00b7ten", "all", "er\u00b7sch\u00f6pf\u00b7fet", "w\u00e4\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PIAT", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "So w\u00fcrd\u2019 er sich aufs neu zur ersten Art bequemen,", "tokens": ["So", "w\u00fcrd'", "er", "sich", "aufs", "neu", "zur", "ers\u00b7ten", "Art", "be\u00b7que\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "APPRART", "ADJD", "APPRART", "ADJA", "NN", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Um die Ver\u00e4nderung noch einmal vorzunehmen.", "tokens": ["Um", "die", "Ver\u00b7\u00e4n\u00b7de\u00b7rung", "noch", "ein\u00b7mal", "vor\u00b7zu\u00b7neh\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "ADV", "ADV", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}