{"textgrid.poem.39971": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "12. Wie die K\u00f6nige Abendmahl hielten und starben", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vom Geiste waren sie erf\u00fcllet,", "tokens": ["Vom", "Geis\u00b7te", "wa\u00b7ren", "sie", "er\u00b7f\u00fcl\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Getauft mit seines Feuers Glut;", "tokens": ["Ge\u00b7tauft", "mit", "sei\u00b7nes", "Feu\u00b7ers", "Glut", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vom priesterlichen Kleid umh\u00fcllet,", "tokens": ["Vom", "pries\u00b7ter\u00b7li\u00b7chen", "Kleid", "um\u00b7h\u00fcl\u00b7let", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hoch hielten sie des Mittlers Blut", "tokens": ["Hoch", "hiel\u00b7ten", "sie", "des", "Mitt\u00b7lers", "Blut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "In einer goldnen Opferschale,", "tokens": ["In", "ei\u00b7ner", "gold\u00b7nen", "Op\u00b7fer\u00b7scha\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und hoch des Mittlers Haupt empor,", "tokens": ["Und", "hoch", "des", "Mitt\u00b7lers", "Haupt", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "NN", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und riefen zu dem Abendmahle", "tokens": ["Und", "rie\u00b7fen", "zu", "dem", "A\u00b7bend\u00b7mah\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die Br\u00fcder in des Tempels Chor.", "tokens": ["Die", "Br\u00fc\u00b7der", "in", "des", "Tem\u00b7pels", "Chor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Hier theilten sie die hohen Gaben", "tokens": ["Hier", "theil\u00b7ten", "sie", "die", "ho\u00b7hen", "Ga\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit milden H\u00e4nden selig aus,", "tokens": ["Mit", "mil\u00b7den", "H\u00e4n\u00b7den", "se\u00b7lig", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie sie einst froh geopfert haben", "tokens": ["Wie", "sie", "einst", "froh", "ge\u00b7o\u00b7pfert", "ha\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "ADJD", "VVPP", "VAINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In dem zerfallnen Tempelhaus.", "tokens": ["In", "dem", "zer\u00b7fall\u00b7nen", "Tem\u00b7pel\u00b7haus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Jetzt kommt das Opfer von dem Kinde,", "tokens": ["Jetzt", "kommt", "das", "Op\u00b7fer", "von", "dem", "Kin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Das \u00fcberschwengliche, herab,", "tokens": ["Das", "\u00fc\u00b7bersc\u00b7hweng\u00b7li\u00b7che", ",", "her\u00b7ab", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Das Opfer, das vertilgt die S\u00fcnde,", "tokens": ["Das", "Op\u00b7fer", ",", "das", "ver\u00b7tilgt", "die", "S\u00fcn\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PDS", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und das den Stachel nimmt dem Grab.", "tokens": ["Und", "das", "den", "Sta\u00b7chel", "nimmt", "dem", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Als wunderbar mit Trank und Speise", "tokens": ["Als", "wun\u00b7der\u00b7bar", "mit", "Trank", "und", "Spei\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sie darauf Alle rings erfreut,", "tokens": ["Sie", "da\u00b7rauf", "Al\u00b7le", "rings", "er\u00b7freut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PAV", "PIAT", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da nahte sich der Greis dem Greise,", "tokens": ["Da", "nah\u00b7te", "sich", "der", "Greis", "dem", "Grei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das Mahl dem Andern jeder beut.", "tokens": ["Das", "Mahl", "dem", "An\u00b7dern", "je\u00b7der", "beut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie ward ihr welkes Haupt erhoben,", "tokens": ["Wie", "ward", "ihr", "wel\u00b7kes", "Haupt", "er\u00b7ho\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "PWAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wie ward ihr m\u00fcdes Herz erquickt!", "tokens": ["Wie", "ward", "ihr", "m\u00fc\u00b7des", "Herz", "er\u00b7quickt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und auf ihr Antlitz ward von oben", "tokens": ["Und", "auf", "ihr", "Ant\u00b7litz", "ward", "von", "o\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VAFIN", "APPR", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ein lichter Stral herabgeschickt.", "tokens": ["Ein", "lich\u00b7ter", "Stral", "her\u00b7ab\u00b7ge\u00b7schickt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und feurig brennt es durch die Scheiben,", "tokens": ["Und", "feu\u00b7rig", "brennt", "es", "durch", "die", "Schei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das Haus f\u00fcllt sich mit weicher Glut;", "tokens": ["Das", "Haus", "f\u00fcllt", "sich", "mit", "wei\u00b7cher", "Glut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "ADJA", "NN", "$."], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.3": {"text": "Die Steine wollen Rosen treiben,", "tokens": ["Die", "Stei\u00b7ne", "wol\u00b7len", "Ro\u00b7sen", "trei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die W\u00e4nde f\u00e4rben sich wie Blut,", "tokens": ["Die", "W\u00e4n\u00b7de", "f\u00e4r\u00b7ben", "sich", "wie", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der K\u00f6n'ge Purpur steht in Flammen,", "tokens": ["Der", "K\u00f6n'\u00b7ge", "Pur\u00b7pur", "steht", "in", "Flam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Im Jugendschein ihr Angesicht \u2013", "tokens": ["Im", "Ju\u00b7gend\u00b7schein", "ihr", "An\u00b7ge\u00b7sicht", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Woher str\u00f6mt so viel Licht zusammen?", "tokens": ["Wo\u00b7her", "str\u00f6mt", "so", "viel", "Licht", "zu\u00b7sam\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ADV", "PIAT", "NN", "PTKVZ", "$."], "meter": "-++--+-+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Der Stern ist's, der durch Wolken bricht!", "tokens": ["Der", "Stern", "ist's", ",", "der", "durch", "Wol\u00b7ken", "bricht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$,", "PRELS", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Da hebt der Greise Blick sich trunken", "tokens": ["Da", "hebt", "der", "Grei\u00b7se", "Blick", "sich", "trun\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "PRF", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und senkt sich wieder s\u00e4nftiglich;", "tokens": ["Und", "senkt", "sich", "wie\u00b7der", "s\u00e4nf\u00b7tig\u00b7lich", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da ist ihr altes Haupt gesunken,", "tokens": ["Da", "ist", "ihr", "al\u00b7tes", "Haupt", "ge\u00b7sun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Als neiget' es zum Schlafe sich;", "tokens": ["Als", "nei\u00b7get'", "es", "zum", "Schla\u00b7fe", "sich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "APPRART", "NN", "PRF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da weichen Knie' und alle Glieder", "tokens": ["Da", "wei\u00b7chen", "Knie'", "und", "al\u00b7le", "Glie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "KON", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Des s\u00fc\u00dfen, ew'gen Schlummers Drang:", "tokens": ["Des", "s\u00fc\u00b7\u00dfen", ",", "ew'\u00b7gen", "Schlum\u00b7mers", "Drang", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da legt ihr Geist die H\u00fclle nieder,", "tokens": ["Da", "legt", "ihr", "Geist", "die", "H\u00fcl\u00b7le", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der sich hinauf zum Sterne schwang.", "tokens": ["Der", "sich", "hin\u00b7auf", "zum", "Ster\u00b7ne", "schwang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Es l\u00e4chelt nieder auf die Leichen", "tokens": ["Es", "l\u00e4\u00b7chelt", "nie\u00b7der", "auf", "die", "Lei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sein Lebensbild der Stern noch lang;", "tokens": ["Sein", "Le\u00b7bens\u00b7bild", "der", "Stern", "noch", "lang", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Bote winkt mit stillem Zeichen,", "tokens": ["Der", "Bo\u00b7te", "winkt", "mit", "stil\u00b7lem", "Zei\u00b7chen"