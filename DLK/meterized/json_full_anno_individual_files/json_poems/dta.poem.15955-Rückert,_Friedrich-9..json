{"dta.poem.15955": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "9.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn du nur die Natur, wenn du nur die Geschichte", "tokens": ["Wenn", "du", "nur", "die", "Na\u00b7tur", ",", "wenn", "du", "nur", "die", "Ge\u00b7schich\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "$,", "KOUS", "PPER", "ADV", "ART", "NN"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Befragtest, was dir die und die von Gott berichte;", "tokens": ["Be\u00b7frag\u00b7test", ",", "was", "dir", "die", "und", "die", "von", "Gott", "be\u00b7rich\u00b7te", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWS", "PPER", "ART", "KON", "ART", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "In manigfacher Kr\u00e4ft' und ew'gen Streits Urkunden,", "tokens": ["In", "ma\u00b7nig\u00b7fa\u00b7cher", "Kr\u00e4ft'", "und", "ew'\u00b7gen", "Streits", "Ur\u00b7kun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "KON", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Viel G\u00f6tter h\u00e4ttest du, nicht Einen Gott, gefunden;", "tokens": ["Viel", "G\u00f6t\u00b7ter", "h\u00e4t\u00b7test", "du", ",", "nicht", "Ei\u00b7nen", "Gott", ",", "ge\u00b7fun\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PPER", "$,", "PTKNEG", "ART", "NN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Und wenn nicht mehrere, doch statt des Einen Zwei,", "tokens": ["Und", "wenn", "nicht", "meh\u00b7re\u00b7re", ",", "doch", "statt", "des", "Ei\u00b7nen", "Zwei", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PTKNEG", "PIAT", "$,", "ADV", "APPR", "ART", "ART", "CARD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wovon der eine gut, der andre b\u00f6se sei.", "tokens": ["Wo\u00b7von", "der", "ei\u00b7ne", "gut", ",", "der", "and\u00b7re", "b\u00f6\u00b7se", "sei", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ART", "ADJD", "$,", "PRELS", "PIS", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "In dir nur findest du, nicht in der Welt Get\u00f6se,", "tokens": ["In", "dir", "nur", "fin\u00b7dest", "du", ",", "nicht", "in", "der", "Welt", "Ge\u00b7t\u00f6\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "VVFIN", "PPER", "$,", "PTKNEG", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df Einer nur ist gut, und nichts durch ihn das B\u00f6se.", "tokens": ["Da\u00df", "Ei\u00b7ner", "nur", "ist", "gut", ",", "und", "nichts", "durch", "ihn", "das", "B\u00f6\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "VAFIN", "ADJD", "$,", "KON", "PIS", "APPR", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}