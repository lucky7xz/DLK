{"textgrid.poem.51191": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Wanderlied", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gl\u00fcck auf! nun will ich wandern", "tokens": ["Gl\u00fcck", "auf", "!", "nun", "will", "ich", "wan\u00b7dern"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PTKVZ", "$.", "ADV", "VMFIN", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Von fr\u00fch bis abends sp\u00e4t,", "tokens": ["Von", "fr\u00fch", "bis", "a\u00b7bends", "sp\u00e4t", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "APPR", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So weit auf dieser Erde", "tokens": ["So", "weit", "auf", "die\u00b7ser", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "APPR", "PDAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Sonne mit mir geht!", "tokens": ["Die", "Son\u00b7ne", "mit", "mir", "geht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich f\u00fchre nur Stab und Becher,", "tokens": ["Ich", "f\u00fch\u00b7re", "nur", "Stab", "und", "Be\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "NN", "KON", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Mein leichtes Saitenget\u00f6n;", "tokens": ["Mein", "leich\u00b7tes", "Sai\u00b7ten\u00b7ge\u00b7t\u00f6n", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Ich wundre mich \u00fcber die Ma\u00dfen,", "tokens": ["Ich", "wund\u00b7re", "mich", "\u00fc\u00b7ber", "die", "Ma\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Wie's \u00fcberall so sch\u00f6n!", "tokens": ["Wie's", "\u00fc\u00b7be\u00b7rall", "so", "sch\u00f6n", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Oft ist die Ebene sch\u00f6ner", "tokens": ["Oft", "ist", "die", "E\u00b7be\u00b7ne", "sch\u00f6\u00b7ner"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADJA"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Als meine Berge, so hoch!", "tokens": ["Als", "mei\u00b7ne", "Ber\u00b7ge", ",", "so", "hoch", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "$,", "ADV", "ADJD", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Und wo kein blauer Himmel,", "tokens": ["Und", "wo", "kein", "blau\u00b7er", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Gibt's Purpurwolken doch.", "tokens": ["Gibt's", "Pur\u00b7pur\u00b7wol\u00b7ken", "doch", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und wo kein schmachtender Lotos,", "tokens": ["Und", "wo", "kein", "schmach\u00b7ten\u00b7der", "Lo\u00b7tos", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "W\u00e4chst bl\u00fchendes Heidekraut;", "tokens": ["W\u00e4chst", "bl\u00fc\u00b7hen\u00b7des", "Hei\u00b7de\u00b7kraut", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wo keine gotischen Dome,", "tokens": ["Wo", "kei\u00b7ne", "go\u00b7ti\u00b7schen", "Do\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sind jonische Tempel gebaut.", "tokens": ["Sind", "jo\u00b7ni\u00b7sche", "Tem\u00b7pel", "ge\u00b7baut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.5": {"line.1": {"text": "Und bin ich des Griechischen m\u00fcde,", "tokens": ["Und", "bin", "ich", "des", "Grie\u00b7chi\u00b7schen", "m\u00fc\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So lockt mich die Moschee:", "tokens": ["So", "lockt", "mich", "die", "Mo\u00b7schee", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich kleid in maurische Schn\u00f6rkel", "tokens": ["Ich", "kleid", "in", "mau\u00b7ri\u00b7sche", "Schn\u00f6r\u00b7kel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Mein abendl\u00e4ndisches Weh.", "tokens": ["Mein", "a\u00b7bend\u00b7l\u00e4n\u00b7di\u00b7sches", "Weh", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.6": {"line.1": {"text": "Das Heimweh nach der Wirtin!", "tokens": ["Das", "Heim\u00b7weh", "nach", "der", "Wir\u00b7tin", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sie find ich in keinem Haus,", "tokens": ["Sie", "find", "ich", "in", "kei\u00b7nem", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PIAT", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und nach der einzig einen", "tokens": ["Und", "nach", "der", "ein\u00b7zig", "ei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJD", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Jag ich Welt ein und aus.", "tokens": ["Jag", "ich", "Welt", "ein", "und", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "NN", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Hei da, du wilder J\u00e4ger,", "tokens": ["Hei", "da", ",", "du", "wil\u00b7der", "J\u00e4\u00b7ger", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "---+-+-", "measure": "unknown.measure.di"}, "line.2": {"text": "Du Bauer dort im Kraut,", "tokens": ["Du", "Bau\u00b7er", "dort", "im", "Kraut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Hast du, verwegner Schiffer,", "tokens": ["Hast", "du", ",", "ver\u00b7