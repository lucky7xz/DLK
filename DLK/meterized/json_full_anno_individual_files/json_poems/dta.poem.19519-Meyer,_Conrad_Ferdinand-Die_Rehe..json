ord", "punct"], "pos": ["ART", "NE", "VVFIN", "PRF", "ADJD", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Da\u00df man sich leichtlich im Walde verirrt.", "tokens": ["Da\u00df", "man", "sich", "leicht\u00b7lich", "im", "Wal\u00b7de", "ver\u00b7irrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PRF", "ADJD", "APPRART", "NN", "VVPP", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Und er bekennt, vom Gewissen ger\u00fchrt,", "tokens": ["Und", "er", "be\u00b7kennt", ",", "vom", "Ge\u00b7wis\u00b7sen", "ge\u00b7r\u00fchrt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Da\u00df eine Rehe vom Wege verf\u00fchrt.", "tokens": ["Da\u00df", "ei\u00b7ne", "Re\u00b7he", "vom", "We\u00b7ge", "ver\u00b7f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Murmelnd verlangt er ein Blatt, einen Stift,", "tokens": ["Mur\u00b7melnd", "ver\u00b7langt", "er", "ein", "Blatt", ",", "ei\u00b7nen", "Stift", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.6": {"text": "Schreibt eine Zeile mit schwankender Schrift:", "tokens": ["Schreibt", "ei\u00b7ne", "Zei\u00b7le", "mit", "schwan\u00b7ken\u00b7der", "Schrift", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.7": {"text": "\u201e