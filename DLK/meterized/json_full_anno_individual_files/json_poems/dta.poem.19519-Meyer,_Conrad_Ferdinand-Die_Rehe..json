{"dta.poem.19519": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Die Rehe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "Fern von dem f\u00fcrstlichen keuschen Gemahl", "tokens": ["Fern", "von", "dem", "f\u00fcrst\u00b7li\u00b7chen", "keu\u00b7schen", "Ge\u00b7mahl"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "ADJA", "NN"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Jubelt ein bl\u00fchender J\u00fcngling im Saal:", "tokens": ["Ju\u00b7belt", "ein", "bl\u00fc\u00b7hen\u00b7der", "J\u00fcng\u00b7ling", "im", "Saal", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "\u201ehebet die Becher und ruft da\u00df es schallt:", "tokens": ["\u201e", "he\u00b7bet", "die", "Be\u00b7cher", "und", "ruft", "da\u00df", "es", "schallt", ":"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "ART", "NN", "KON", "VVFIN", "KOUS", "PPER", "VVFIN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "Freiheit, sie lebe! Die Freiheit im Wald!\u201c", "tokens": ["Frei\u00b7heit", ",", "sie", "le\u00b7be", "!", "Die", "Frei\u00b7heit", "im", "Wald", "!", "\u201c"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "$.", "ART", "NN", "APPRART", "NN", "$.", "$("], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.5": {"text": "All die Genossen der weidlichen Lust", "tokens": ["All", "die", "Ge\u00b7nos\u00b7sen", "der", "weid\u00b7li\u00b7chen", "Lust"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "ART", "ADJA", "NN"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.6": {"text": "Bringen das Hoch aus ergl\u00fchender Brust:", "tokens": ["Brin\u00b7gen", "das", "Hoch", "aus", "er\u00b7gl\u00fc\u00b7hen\u00b7der", "Brust", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.7": {"text": "\u201elebe die Jugend und Bacchus' Gewalt!", "tokens": ["\u201e", "le\u00b7be", "die", "Ju\u00b7gend", "und", "Bac\u00b7chus'", "Ge\u00b7walt", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "ART", "NN", "KON", "NE", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.8": {"text": "Freiheit, sie lebe! Die Freiheit im Wald!\u201c", "tokens": ["Frei\u00b7heit", ",", "sie", "le\u00b7be", "!", "Die", "Frei\u00b7heit", "im", "Wald", "!", "\u201c"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "$.", "ART", "NN", "APPRART", "NN", "$.", "$("], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.2": {"line.1": {"text": "Schmetternde H\u00f6rner! Dann fl\u00fcstern sie sacht,", "tokens": ["Schmet\u00b7tern\u00b7de", "H\u00f6r\u00b7ner", "!", "Dann", "fl\u00fcs\u00b7tern", "sie", "sacht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "ADV", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Scherzen und locken die Elfen der Nacht", "tokens": ["Scher\u00b7zen", "und", "lo\u00b7cken", "die", "El\u00b7fen", "der", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Aus ihren Waldesverstecken hervor \u2014", "tokens": ["Aus", "ih\u00b7ren", "Wal\u00b7des\u00b7ver\u00b7ste\u00b7cken", "her\u00b7vor"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$("], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Aengstliche Schl\u00e4ge best\u00fcrmen das Thor.", "tokens": ["A\u00b7engst\u00b7li\u00b7che", "Schl\u00e4\u00b7ge", "be\u00b7st\u00fcr\u00b7men", "das", "Thor", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "\u201esetz dich ans Feuer, du herziges Kind!\u201c", "tokens": ["\u201e", "setz", "dich", "ans", "Feu\u00b7er", ",", "du", "her\u00b7zi\u00b7ges", "Kind", "!", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVIMP", "PPER", "APPRART", "NN", "$,", "PPER", "ADJA", "NN", "$.", "$("], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.6": {"text": "L\u00e4rmt im erleuchteten Hof das Gesind.", "tokens": ["L\u00e4rmt", "im", "er\u00b7leuch\u00b7te\u00b7ten", "Hof", "das", "Ge\u00b7sind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "\u201ef\u00fcrstlich bewirthen mit Kuchen dich wir!", "tokens": ["\u201e", "f\u00fcrst\u00b7lich", "be\u00b7wirt\u00b7hen", "mit", "Ku\u00b7chen", "dich", "wir", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "ADJA", "APPR", "NN", "PPER", "PPER", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.8": {"text": "Drinnen was suchst du? Bescheide dich hier!\u201c", "tokens": ["Drin\u00b7nen", "was", "suchst", "du", "?", "Be\u00b7schei\u00b7de", "dich", "hier", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "PWS", "VVFIN", "PPER", "$.", "VVFIN", "PPER", "ADV", "$.", "$("], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.3": {"line.1": {"text": "Rasch in den Saal, in den f\u00fcrstlichen, tritt", "tokens": ["Rasch", "in", "den", "Saal", ",", "in", "den", "f\u00fcrst\u00b7li\u00b7chen", ",", "tritt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ADJD", "APPR", "ART", "NN", "$,", "APPR", "ART", "ADJA", "$,", "VVFIN"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Eine Gescheuchte mit hastigem Schritt,", "tokens": ["Ei\u00b7ne", "Ge\u00b7scheuch\u00b7te", "mit", "has\u00b7ti\u00b7gem", "Schritt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Ueber den Busen, vom Laufe bewegt,", "tokens": ["Ue\u00b7ber", "den", "Bu\u00b7sen", ",", "vom", "Lau\u00b7fe", "be\u00b7wegt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPRART", "NN", "VVFIN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "Kreuzweis die flehenden Arme gelegt \u2014", "tokens": ["Kreuz\u00b7weis", "die", "fle\u00b7hen\u00b7den", "Ar\u00b7me", "ge\u00b7legt"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.5": {"text": "Bl\u00e4tter am R\u00f6cklein, herbstr\u00f6thlich und falb!", "tokens": ["Bl\u00e4t\u00b7ter", "am", "R\u00f6ck\u00b7lein", ",", "herbs\u00b7tr\u00f6th\u00b7lich", "und", "falb", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.6": {"text": "Krausdunkle Haare, noch flattern sie halb,", "tokens": ["Kraus\u00b7dunk\u00b7le", "Haa\u00b7re", ",", "noch", "flat\u00b7tern", "sie", "halb", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADV", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "S\u00fc\u00dfbraune Augen und schmerzlich dabei,", "tokens": ["S\u00fc\u00df\u00b7brau\u00b7ne", "Au\u00b7gen", "und", "schmerz\u00b7lich", "da\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "ADJD", "PAV", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Blutende F\u00fc\u00dfe \u2014 nicht die einer Fei!", "tokens": ["Blu\u00b7ten\u00b7de", "F\u00fc\u00b7\u00dfe", "nicht", "die", "ei\u00b7ner", "Fei", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$(", "PTKNEG", "ART", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "\u201esage, wer bist du, krauslockiges Haupt,", "tokens": ["\u201e", "sa\u00b7ge", ",", "wer", "bist", "du", ",", "kraus\u00b7lo\u00b7cki\u00b7ges", "Haupt", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$,", "PWS", "VAFIN", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Schimmernd von purpurnen Bl\u00e4ttern umlaubt?\u201c", "tokens": ["Schim\u00b7mernd", "von", "pur\u00b7pur\u00b7nen", "Bl\u00e4t\u00b7tern", "um\u00b7laubt", "?", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "APPR", "ADJA", "NN", "VVPP", "$.", "$("], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "\u2014 \u201eRehe, die Rehe, so hei\u00df' ich im Land", "tokens": ["\u201e", "Re\u00b7he", ",", "die", "Re\u00b7he", ",", "so", "hei\u00df'", "ich", "im", "Land"], "token_info": ["punct", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "$(", "NN", "$,", "ART", "NN", "$,", "ADV", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Von meinem braunen Gelock und Gewand\u201c \u2014", "tokens": ["Von", "mei\u00b7nem", "brau\u00b7nen", "Ge\u00b7lock", "und", "Ge\u00b7wand", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "KON", "NN", "$(", "$("], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "\u201emein ist die Rehe! Des Herrn ist die Jagd!\u201c", "tokens": ["\u201e", "mein", "ist", "die", "Re\u00b7he", "!", "Des", "Herrn", "ist", "die", "Jagd", "!", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPOSAT", "VAFIN", "ART", "NN", "$.", "ART", "NN", "VAFIN", "ART", "NN", "$.", "$("], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Jubelt der J\u00fcngling, es str\u00e4ubt sich die Magd ...", "tokens": ["Ju\u00b7belt", "der", "J\u00fcng\u00b7ling", ",", "es", "str\u00e4ubt", "sich", "die", "Magd", "..."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "PPER", "VVFIN", "PRF", "ART", "NN", "$("], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.7": {"text": "Halali! hetzt es und tobt es und hallt.", "tokens": ["Ha\u00b7la\u00b7li", "!", "hetzt", "es", "und", "tobt", "es", "und", "hallt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "VVFIN", "PPER", "KON", "VVFIN", "PPER", "KON", "VVFIN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.8": {"text": "Ringend entwindet sie sich der Gewalt.", "tokens": ["Rin\u00b7gend", "ent\u00b7win\u00b7det", "sie", "sich", "der", "Ge\u00b7walt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PRF", "ART", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.5": {"line.1": {"text": "Lodernde Augen, wie Blitze der Nacht \u2014", "tokens": ["Lo\u00b7dern\u00b7de", "Au\u00b7gen", ",", "wie", "Blit\u00b7ze", "der", "Nacht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PWAV", "NN", "ART", "NN", "$("], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Doch sie besinnt sich. Dann redet sie sacht:", "tokens": ["Doch", "sie", "be\u00b7sinnt", "sich", ".", "Dann", "re\u00b7det", "sie", "sacht", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PRF", "$.", "ADV", "VVFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-----+", "measure": "zehnsilber"}, "line.3": {"text": "\u201erehe, die Rehe, so hei\u00df' ich im Land,", "tokens": ["\u201e", "re\u00b7he", ",", "die", "Re\u00b7he", ",", "so", "hei\u00df'", "ich", "im", "Land", ","], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$,", "ART", "NN", "$,", "ADV", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "Wilpert, der Sch\u00fctz, ist der Vater genannt \u2014", "tokens": ["Wil\u00b7pert", ",", "der", "Sch\u00fctz", ",", "ist", "der", "Va\u00b7ter", "ge\u00b7nannt"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "$,", "VAFIN", "ART", "NN", "VVPP", "$("], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.5": {"text": "Auf eine Jagd, die dem Herrn nur geb\u00fchrt,", "tokens": ["Auf", "ei\u00b7ne", "Jagd", ",", "die", "dem", "Herrn", "nur", "ge\u00b7b\u00fchrt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Hat ihn ein \u00e4tzendes Rudel verf\u00fchrt.", "tokens": ["Hat", "ihn", "ein", "\u00e4t\u00b7zen\u00b7des", "Ru\u00b7del", "ver\u00b7f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Siehe, da kniet er, da zielt er und knallt \u2014", "tokens": ["Sie\u00b7he", ",", "da", "kniet", "er", ",", "da", "zielt", "er", "und", "knallt"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "ADV", "VVFIN", "PPER", "$,", "ADV", "VVFIN", "PPER", "KON", "VVFIN", "$("], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Heut hat der Vater gefrevelt im Wald!", "tokens": ["Heut", "hat", "der", "Va\u00b7ter", "ge\u00b7fre\u00b7velt", "im", "Wald", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVPP", "APPRART", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Doch deine F\u00f6rster ergriffen ihn, weh,", "tokens": ["Doch", "dei\u00b7ne", "F\u00f6rs\u00b7ter", "er\u00b7grif\u00b7fen", "ihn", ",", "weh", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVPP", "PPER", "$,", "PTKVZ", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Ihn und das s\u00fcndlich erbeutete Reh.", "tokens": ["Ihn", "und", "das", "s\u00fcnd\u00b7lich", "er\u00b7beu\u00b7te\u00b7te", "Reh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KON", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Ich, von der Angst und dem Jammer gejagt,", "tokens": ["Ich", ",", "von", "der", "Angst", "und", "dem", "Jam\u00b7mer", "ge\u00b7jagt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "APPR", "ART", "NN", "KON", "ART", "NN", "VVPP", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.12": {"text": "Lief in den Wald, eine hilflose Magd.", "tokens": ["Lief", "in", "den", "Wald", ",", "ei\u00b7ne", "hilf\u00b7lo\u00b7se", "Magd", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.13": {"text": "Da schier das Herz mir im Busen zersprang,", "tokens": ["Da", "schier", "das", "Herz", "mir", "im", "Bu\u00b7sen", "zer\u00b7sprang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "Sah ich die Kerzen und h\u00f6rte den Klang \u2014", "tokens": ["Sah", "ich", "die", "Ker\u00b7zen", "und", "h\u00f6r\u00b7te", "den", "Klang"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "po