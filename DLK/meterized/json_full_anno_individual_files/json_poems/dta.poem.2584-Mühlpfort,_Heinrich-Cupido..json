{"dta.poem.2584": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Cupido.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Gefl\u00fcgelter Mercur/ bey dem Wahrsagungs-Stab/", "tokens": ["Ge\u00b7fl\u00fc\u00b7gel\u00b7ter", "Mer\u00b7cur", "/", "bey", "dem", "Wahr\u00b7sa\u00b7gungs\u00b7Stab", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$(", "APPR", "ART", "NN", "$("], "meter": "-+----+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Den deine Rechte f\u00fchrt/ bey deinem sch\u00f6nen Singen/", "tokens": ["Den", "dei\u00b7ne", "Rech\u00b7te", "f\u00fchrt", "/", "bey", "dei\u00b7nem", "sch\u00f6\u00b7nen", "Sin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "$(", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wenn du den Argus kanst umb hundert Augen bringen/", "tokens": ["Wenn", "du", "den", "Ar\u00b7gus", "kanst", "umb", "hun\u00b7dert", "Au\u00b7gen", "brin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VMFIN", "APPR", "CARD", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und hauest noch darzu mit List den Kopff ihm ab/", "tokens": ["Und", "hau\u00b7est", "noch", "dar\u00b7zu", "mit", "List", "den", "Kopff", "ihm", "ab", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PAV", "APPR", "NN", "ART", "NN", "PPER", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich lasse dich nicht weg/ du wirst noch h\u00f6ren m\u00fcssen/", "tokens": ["Ich", "las\u00b7se", "dich", "nicht", "weg", "/", "du", "wirst", "noch", "h\u00f6\u00b7ren", "m\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "PTKVZ", "$(", "PPER", "VAFIN", "ADV", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Was die Verliebten jetzt f\u00fcr Wechsel werden schliessen.", "tokens": ["Was", "die", "Ver\u00b7lieb\u00b7ten", "jetzt", "f\u00fcr", "Wech\u00b7sel", "wer\u00b7den", "schlies\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "ADV", "APPR", "NN", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}