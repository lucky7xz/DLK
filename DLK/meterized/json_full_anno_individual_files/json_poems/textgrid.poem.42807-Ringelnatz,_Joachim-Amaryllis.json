{"textgrid.poem.42807": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "Amaryllis", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Atelier ist hei\u00df.", "tokens": ["Das", "A\u00b7te\u00b7lier", "ist", "hei\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Drau\u00dfen, drunten die andere Welt", "tokens": ["Drau\u00b7\u00dfen", ",", "drun\u00b7ten", "die", "an\u00b7de\u00b7re", "Welt"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "ADV", "ART", "ADJA", "NN"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Klopft ihre Teppiche, schreit und bellt.", "tokens": ["Klopft", "ih\u00b7re", "Tep\u00b7pi\u00b7che", ",", "schreit", "und", "bellt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,", "ADJD", "KON", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Der Maler, der das wu\u00dfte, er wei\u00df", "tokens": ["Der", "Ma\u00b7ler", ",", "der", "das", "wu\u00df\u00b7te", ",", "er", "wei\u00df"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PDS", "VVFIN", "$,", "PPER", "VVFIN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Es jetzt nicht mehr. Die Zeit steht still.", "tokens": ["Es", "jetzt", "nicht", "mehr", ".", "Die", "Zeit", "steht", "still", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PTKNEG", "ADV", "$.", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Pinsel zecht, l\u00e4uft, zecht, l\u00e4uft schnell", "tokens": ["Der", "Pin\u00b7sel", "zecht", ",", "l\u00e4uft", ",", "zecht", ",", "l\u00e4uft", "schnell"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und weiter, als er darf und will.", "tokens": ["Und", "wei\u00b7ter", ",", "als", "er", "darf", "und", "will", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "KOUS", "PPER", "VMFIN", "KON", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Reglos im Stuhle das sch\u00f6ne Modell", "tokens": ["Reg\u00b7los", "im", "Stuh\u00b7le", "das", "sch\u00f6\u00b7ne", "Mo\u00b7dell"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPRART", "NN", "ART", "ADJA", "NN"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.9": {"text": "Tr\u00e4umt von sich selber, von Amaryll.", "tokens": ["Tr\u00e4umt", "von", "sich", "sel\u00b7ber", ",", "von", "A\u00b7ma\u00b7ryll", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPR", "PRF", "ADV", "$,", "APPR", "NE", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Das Atelier ist hei\u00df.", "tokens": ["Das", "A\u00b7te\u00b7lier", "ist", "hei\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Drau\u00dfen, drunten die andere Welt", "tokens": ["Drau\u00b7\u00dfen", ",", "drun\u00b7ten", "die", "an\u00b7de\u00b7re", "Welt"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "ADV", "ART"