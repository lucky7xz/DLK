{"textgrid.poem.51248": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1", "genre": "verse", "period": "N.A.", "pub_year": 1846, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Klagt mich nicht an, da\u00df ich vor Leid", "tokens": ["Klagt", "mich", "nicht", "an", ",", "da\u00df", "ich", "vor", "Leid"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKNEG", "PTKVZ", "$,", "KOUS", "PPER", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein eigen Bild nur k\u00f6nne sehen!", "tokens": ["Mein", "ei\u00b7gen", "Bild", "nur", "k\u00f6n\u00b7ne", "se\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADV", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ich seh durch meinen grauen Flor", "tokens": ["Ich", "seh", "durch", "mei\u00b7nen", "grau\u00b7en", "Flor"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Fern euere Gestalten gehen.", "tokens": ["Fern", "eu\u00b7e\u00b7re", "Ge\u00b7stal\u00b7ten", "ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Und durch den starken Wellenschlag", "tokens": ["Und", "durch", "den", "star\u00b7ken", "Wel\u00b7len\u00b7schlag"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der See, die gegen mich verschworen,", "tokens": ["Der", "See", ",", "die", "ge\u00b7gen", "mich", "ver\u00b7schwo\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Geht mir von euerem Gesang,", "tokens": ["Geht", "mir", "von", "eu\u00b7e\u00b7rem", "Ge\u00b7sang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Wenn auch ged\u00e4mpft, kein Ton verloren.", "tokens": ["Wenn", "auch", "ge\u00b7d\u00e4mpft", ",", "kein", "Ton", "ver\u00b7lo\u00b7ren", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "VVPP", "$,", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und wie die m\u00fcde Danaide wohl,", "tokens": ["Und", "wie", "die", "m\u00fc\u00b7de", "Da\u00b7na\u00b7i\u00b7de", "wohl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Das Sieb gesenkt, neugierig um sich blicket,", "tokens": ["Das", "Sieb", "ge\u00b7senkt", ",", "neu\u00b7gie\u00b7rig", "um", "sich", "bli\u00b7cket", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "ADJD", "APPR", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So schau ich euch verwundert nach,", "tokens": ["So", "schau", "ich", "euch", "ver\u00b7wun\u00b7dert", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Besorgt, wie ihr e