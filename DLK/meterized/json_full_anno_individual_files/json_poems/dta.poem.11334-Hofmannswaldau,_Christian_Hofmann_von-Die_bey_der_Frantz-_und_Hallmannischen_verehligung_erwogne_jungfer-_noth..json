{"dta.poem.11334": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Die bey der Frantz- und Hallmannischen  \n verehligung erwogne jungfer-  \n noth.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das ungl\u00fcckseeligste geschlechte dieser welt,", "tokens": ["Das", "un\u00b7gl\u00fcck\u00b7see\u00b7ligs\u00b7te", "ge\u00b7schlech\u00b7te", "die\u00b7ser", "welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der auszug aller noth, der sammel-platz der schmertzen,", "tokens": ["Der", "aus\u00b7zug", "al\u00b7ler", "noth", ",", "der", "sam\u00b7mel\u00b7platz", "der", "schmert\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "$,", "ART", "NN", "ART", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wo kummer und verdru\u00df die stete hofstadt h\u00e4lt,", "tokens": ["Wo", "kum\u00b7mer", "und", "ver\u00b7dru\u00df", "die", "ste\u00b7te", "hof\u00b7stadt", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Seyd ihr, wie mich bed\u00fcnckt, ihr guten jungfer-hertzen!", "tokens": ["Seyd", "ihr", ",", "wie", "mich", "be\u00b7d\u00fcnckt", ",", "ihr", "gu\u00b7ten", "jung\u00b7fer\u00b7hert\u00b7zen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAIMP", "PPER", "$,", "PWAV", "PPER", "VVFIN", "$,", "PPOSAT", "ADJA", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der jammer, der euch dr\u00fcckt, ist nicht zu \u00fcbersehn:", "tokens": ["Der", "jam\u00b7mer", ",", "der", "euch", "dr\u00fcckt", ",", "ist", "nicht", "zu", "\u00fc\u00b7ber\u00b7sehn", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "VAFIN", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Euch armen kindern mu\u00df fast nichts als alles fehlen;", "tokens": ["Euch", "ar\u00b7men", "kin\u00b7dern", "mu\u00df", "fast", "nichts", "als", "al\u00b7les", "feh\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "VMFIN", "ADV", "PIS", "KOKOM", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und will ich, wo es nur vor wehmuth kan geschehn,", "tokens": ["Und", "will", "ich", ",", "wo", "es", "nur", "vor", "weh\u00b7muth", "kan", "ge\u00b7schehn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "$,", "PWAV", "PPER", "ADV", "APPR", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Hier nur das wenigste von eurer noth erzehlen.", "tokens": ["Hier", "nur", "das", "we\u00b7nigs\u00b7te", "von", "eu\u00b7rer", "noth", "er\u00b7zeh\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PDS", "VVFIN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wie qu\u00e4let euch der tag! wie martert euch die nacht!", "tokens": ["Wie", "qu\u00e4\u00b7let", "euch", "der", "tag", "!", "wie", "mar\u00b7tert", "euch", "die", "nacht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ART", "NN", "$.", "PWAV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Wie macht die einsamkeit euch doch so unvergn\u00fcget!", "tokens": ["Wie", "macht", "die", "ein\u00b7sam\u00b7keit", "euch", "doch", "so", "un\u00b7ver\u00b7gn\u00fc\u00b7get", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "PPER", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und wer ein wenig giebt auf eure seufftzer acht,", "tokens": ["Und", "wer", "ein", "we\u00b7nig", "giebt", "auf", "eu\u00b7re", "seufft\u00b7zer", "acht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "PIS", "VVFIN", "APPR", "PPOSAT", "ADJA", "CARD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Merckt, da\u00df die kranckheit euch in allen gliedern lieget:", "tokens": ["Merckt", ",", "da\u00df", "die", "kran\u00b7ck\u00b7heit", "euch", "in", "al\u00b7len", "glie\u00b7dern", "lie\u00b7get", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "ART", "NN", "PPER", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "+--+--+-+-+-+-", "measure": "elegiambus"}, "line.13": {"text": "Die augen d\u00fcrffet ihr nicht, wie ihr wollet, drehn,", "tokens": ["Die", "au\u00b7gen", "d\u00fcrf\u00b7fet", "ihr", "nicht", ",", "wie", "ihr", "wol\u00b7let", ",", "drehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "PTKNEG", "$,", "PWAV", "PPER", "VMFIN", "$,", "CARD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und keinen freyen blick nach manns-personen wagen,", "tokens": ["Und", "kei\u00b7nen", "frey\u00b7en", "blick", "nach", "manns\u00b7per\u00b7so\u00b7nen", "wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJA", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Gleich f\u00e4ngt die mutter an: Solt du nach kerlen sehn?", "tokens": ["Gleich", "f\u00e4ngt", "die", "mut\u00b7ter", "an", ":", "Solt", "du", "nach", "ker\u00b7len", "sehn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKVZ", "$.", "VMFIN", "PPER", "APPR", "VVINF", "VVINF", "$."], "meter": "-+-+--+-+--+", "measure": "iambic.penta.relaxed"}, "line.16": {"text": "Du geiles raben-aas! dich soll der guckguck plagen!", "tokens": ["Du", "gei\u00b7les", "ra\u00b7ben\u00b7aas", "!", "dich", "soll", "der", "guck\u00b7guck", "pla\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$.", "PPER", "VMFIN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Jhr m\u00fcsset kopf und hal\u00df, den ro\u00df und m\u00e4ulern gleich,", "tokens": ["Ihr", "m\u00fcs\u00b7set", "kopf", "und", "hal\u00df", ",", "den", "ro\u00df", "und", "m\u00e4u\u00b7lern", "gleich", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "NN", "KON", "VVFIN", "$,", "PRELS", "ADJD", "KON", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Jm zaum und im gebi\u00df, ja im gewichte, tragen:", "tokens": ["Jm", "zaum", "und", "im", "ge\u00b7bi\u00df", ",", "ja", "im", "ge\u00b7wich\u00b7te", ",", "tra\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "KON", "APPRART", "NN", "$,", "ADV", "APPRART", "ADJA", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Die ohren sind wie taub, und ihr err\u00f6thet euch,", "tokens": ["Die", "oh\u00b7ren", "sind", "wie", "taub", ",", "und", "ihr", "er\u00b7r\u00f6t\u00b7het", "euch", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "KOKOM", "ADJD", "$,", "KON", "PPER", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "So offt man einen schertz und lustig wort will sagen:", "tokens": ["So", "offt", "man", "ei\u00b7nen", "schertz", "und", "lus\u00b7tig", "wort", "will", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PIS", "ART", "NN", "KON", "ADJD", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Fast jede sylbe kehrt bey euch auf schrauben ein:", "tokens": ["Fast", "je\u00b7de", "syl\u00b7be", "kehrt", "bey", "euch", "auf", "schrau\u00b7ben", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "ADJA", "VVFIN", "APPR", "PPER", "APPR", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Die lippen m\u00fcsset ihr in enge falten fassen,", "tokens": ["Die", "lip\u00b7pen", "m\u00fcs\u00b7set", "ihr", "in", "en\u00b7ge", "fal\u00b7ten", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "APPR", "ADJA", "VVINF", "VVINF", "$,"], "meter": "-+-++--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.23": {"text": "Und da\u00df kein bissen ja zu wichtig m\u00f6chte seyn,", "tokens": ["Und", "da\u00df", "kein", "bis\u00b7sen", "ja", "zu", "wich\u00b7tig", "m\u00f6ch\u00b7te", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIAT", "ADV", "ADV", "PTKA", "ADJD", "VMFIN", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Mu\u00df offt ein mandelkern sich viermahl theilen lassen:", "tokens": ["Mu\u00df", "offt", "ein", "man\u00b7del\u00b7kern", "sich", "vier\u00b7mahl", "thei\u00b7len", "las\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "PIS", "PRF", "ADV", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Was aber f\u00e4ngt inde\u00df der arme magen an?", "tokens": ["Was", "a\u00b7ber", "f\u00e4ngt", "in\u00b7de\u00df", "der", "ar\u00b7me", "ma\u00b7gen", "an", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "ADV", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Der mu\u00df vor erbarkeit so durst als hunger leiden,", "tokens": ["Der", "mu\u00df", "vor", "er\u00b7bar\u00b7keit", "so", "durst", "als", "hun\u00b7ger", "lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "APPR", "NN", "ADV", "ADV", "KOUS", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Es wird kein guter trunck und rechter bi\u00df gethan,", "tokens": ["Es", "wird", "kein", "gu\u00b7ter", "trunck", "und", "rech\u00b7ter", "bi\u00df", "ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "ADJA", "NN", "KON", "ADJA", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Wenn ihr nicht erst dabey k\u00f6nnt die gesellschafft meiden:", "tokens": ["Wenn", "ihr", "nicht", "erst", "da\u00b7bey", "k\u00f6nnt", "die", "ge\u00b7sell\u00b7schafft", "mei\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADV", "PAV", "VVFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Das starcke schn\u00fcren pre\u00dft euch lung und leber ein:", "tokens": ["Das", "star\u00b7cke", "schn\u00fc\u00b7ren", "pre\u00dft", "euch", "lung", "und", "le\u00b7ber", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "VVINF", "VVFIN", "PPER", "NN", "KON", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-----+", "measure": "unknown.measure.tetra"}, "line.30": {"text": "Und das beklemmte hertz, das so viel seufftzer nagen,", "tokens": ["Und", "das", "be\u00b7klemm\u00b7te", "hertz", ",", "das", "so", "viel", "seufft\u00b7zer", "na\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,", "PRELS", "ADV", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Als federn offtermahls in eurem bette seyn,", "tokens": ["Als", "fe\u00b7dern", "off\u00b7ter\u00b7mahls", "in", "eu\u00b7rem", "bet\u00b7te", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADV", "APPR", "PPOSAT", "VVFIN", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Darf einen kaum davon der liebsten schwester sagen:", "tokens": ["Darf", "ei\u00b7nen", "kaum", "da\u00b7von", "der", "liebs\u00b7ten", "schwes\u00b7ter", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "ADV", "PAV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Die h\u00e4nde f\u00fcllt man euch mit zwirn und nadeln an,", "tokens": ["Die", "h\u00e4n\u00b7de", "f\u00fcllt", "man", "euch", "mit", "zwirn", "und", "na\u00b7deln", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "PIS", "PPER", "APPR", "NN", "KON", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "In die ihr lieber w\u00fcnscht was m\u00e4nnliches zu schliessen:", "tokens": ["In", "die", "ihr", "lie\u00b7ber", "w\u00fcnscht", "was", "m\u00e4nn\u00b7li\u00b7ches", "zu", "schlies\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADV", "VVFIN", "PIS", "ADJA", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Und lencket euer fu\u00df sich auf die liebes-bahn,", "tokens": ["Und", "len\u00b7cket", "eu\u00b7er", "fu\u00df", "sich", "auf", "die", "lie\u00b7bes\u00b7bahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "So f\u00fchrt die mutter euch alsbald zum kleppel-k\u00fcssen.", "tokens": ["So", "f\u00fchrt", "die", "mut\u00b7ter", "euch", "als\u00b7bald", "zum", "klep\u00b7pel\u00b7k\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "ADV", "APPRART", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "So ist auch sonst an euch kein glied von schmertzen frey,", "tokens": ["So", "ist", "auch", "sonst", "an", "euch", "kein", "glied", "von", "schmert\u00b7zen", "frey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ADV", "APPR", "PPER", "PIAT", "NN", "APPR", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Und so verbringet ihr, gleich siechen, eure tage,", "tokens": ["Und", "so", "ver\u00b7brin\u00b7get", "ihr", ",", "gleich", "sie\u00b7chen", ",", "eu\u00b7re", "ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "$,", "ADV", "VVINF", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Und m\u00fcsset zugestehn, die jungfer-plage sey", "tokens": ["Und", "m\u00fcs\u00b7set", "zu\u00b7ge\u00b7stehn", ",", "die", "jung\u00b7fer\u00b7pla\u00b7ge", "sey"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VMFIN", "VVINF", "$,", "ART", "ADJA", "VAFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Noch viel beschwerlicher, als wohl die mutter-plage.", "tokens": ["Noch", "viel", "be\u00b7schwer\u00b7li\u00b7cher", ",", "als", "wohl", "die", "mut\u00b7ter\u00b7pla\u00b7ge", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "$,", "KOUS", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Hochwerther br\u00e4utigam! die schrifft, die seine hand", "tokens": ["Hoch\u00b7wert\u00b7her", "br\u00e4u\u00b7ti\u00b7gam", "!", "die", "schrifft", ",", "die", "sei\u00b7ne", "hand"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NE", "$.", "ART", "VVFIN", "$,", "PRELS", "PPOSAT", "NN"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.42": {"text": "Einst von dem krancken-weh der jungfern hat geschrieben,", "tokens": ["Einst", "von", "dem", "kran\u00b7cken\u00b7weh", "der", "jung\u00b7fern", "hat", "ge\u00b7schrie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ART", "ADJA", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Hat uns so wohl gelehrt derselben \u00fcbelstand,", "tokens": ["Hat", "uns", "so", "wohl", "ge\u00b7lehrt", "der\u00b7sel\u00b7ben", "\u00fc\u00b7bel\u00b7stand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "VVPP", "PDS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Als mittel angezeigt, wodurch er wird vertrieben;", "tokens": ["Als", "mit\u00b7tel", "an\u00b7ge\u00b7zeigt", ",", "wo\u00b7durch", "er", "wird", "ver\u00b7trie\u00b7ben", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVPP", "$,", "PWAV", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Jtzt schenckt der himmel ihm ein angenehmes kind,", "tokens": ["Jtzt", "schenckt", "der", "him\u00b7mel", "ihm", "ein", "an\u00b7ge\u00b7neh\u00b7mes", "kind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Das bald die frohe schaar der Nymphen wird vermissen,", "tokens": ["Das", "bald", "die", "fro\u00b7he", "schaar", "der", "Nym\u00b7phen", "wird", "ver\u00b7mis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ART", "ADJA", "NN", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Und das sich gegen ihm vor liebe kranck befindt:", "tokens": ["Und", "das", "sich", "ge\u00b7gen", "ihm", "vor", "lie\u00b7be", "kranck", "be\u00b7findt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "PRF", "APPR", "PPER", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "So wird er es begl\u00fcckt denn auch zu heilen wissen,", "tokens": ["So", "wird", "er", "es", "be\u00b7gl\u00fcckt", "denn", "auch", "zu", "hei\u00b7len", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "VVPP", "KON", "ADV", "PTKZU", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.49": {"text": "Ich wei\u00df, die liebe wird hiebey mit eigner hand", "tokens": ["Ich", "wei\u00df", ",", "die", "lie\u00b7be", "wird", "hie\u00b7bey", "mit", "eig\u00b7ner", "hand"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PRELS", "VVFIN", "VAFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.50": {"text": "Von viel vergn\u00fcgungen und tausend lieblichkeiten", "tokens": ["Von", "viel", "ver\u00b7gn\u00fc\u00b7gun\u00b7gen", "und", "tau\u00b7send", "lieb\u00b7lich\u00b7kei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "VVPP", "KON", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.51": {"text": "Ein labsal, welches ihr noch beyde nicht erkannt,", "tokens": ["Ein", "lab\u00b7sal", ",", "wel\u00b7ches", "ihr", "noch", "bey\u00b7de", "nicht", "er\u00b7kannt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADV", "PIS", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.52": {"text": "Zu eurer st\u00e4rckung euch verlobten zu bereiten.", "tokens": ["Zu", "eu\u00b7rer", "st\u00e4r\u00b7ckung", "euch", "ver\u00b7lob\u00b7ten", "zu", "be\u00b7rei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "VVINF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.53": {"text": "Wohl ihr! begl\u00fcckte braut! da\u00df ihr befreyter fu\u00df", "tokens": ["Wohl", "ihr", "!", "be\u00b7gl\u00fcck\u00b7te", "braut", "!", "da\u00df", "ihr", "be\u00b7frey\u00b7ter", "fu\u00df"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "$.", "VVFIN", "VVFIN", "$.", "KOUS", "PPER", "ADJD", "PTKVZ"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.54": {"text": "Kan aus dem lazareth der jungfer-schaar entweichen!", "tokens": ["Kan", "aus", "dem", "la\u00b7