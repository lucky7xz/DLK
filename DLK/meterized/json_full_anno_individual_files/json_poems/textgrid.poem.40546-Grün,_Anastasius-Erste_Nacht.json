{"textgrid.poem.40546": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "Erste Nacht", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es w\u00e4re Schlafenszeit; \u2013 doch das ist schlimm,", "tokens": ["Es", "w\u00e4\u00b7re", "Schla\u00b7fens\u00b7zeit", ";", "\u2013", "doch", "das", "ist", "schlimm", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "$.", "$(", "KON", "PDS", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Nicht schlafen l\u00e4\u00dft mich hier der Ache Grimm,", "tokens": ["Nicht", "schla\u00b7fen", "l\u00e4\u00dft", "mich", "hier", "der", "A\u00b7che", "Grimm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "VVFIN", "PPER", "ADV", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Grad' unterm Fenster schl\u00e4gt ihr Katarakt", "tokens": ["Grad'", "un\u00b7term", "Fens\u00b7ter", "schl\u00e4gt", "ihr", "Ka\u00b7ta\u00b7rakt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Auf Felsenpulte dr\u00f6hnend seinen Takt!", "tokens": ["Auf", "Fel\u00b7sen\u00b7pul\u00b7te", "dr\u00f6h\u00b7nend", "sei\u00b7nen", "Takt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Musik zur Unzeit! Was zu thun da sei?", "tokens": ["Mu\u00b7sik", "zur", "Un\u00b7zeit", "!", "Was", "zu", "thun", "da", "sei", "?"], "token_info": ["word", "word", "word", "