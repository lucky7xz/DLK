{"textgrid.poem.37659": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Der Thron", "genre": "verse", "period": "N.A.", "pub_year": 1807, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Krieger baut sich seinen Thron", "tokens": ["Der", "Krie\u00b7ger", "baut", "sich", "sei\u00b7nen", "Thron"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PRF", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Aus jedem frischen Holz,", "tokens": ["Aus", "je\u00b7dem", "fri\u00b7schen", "Holz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da meinet er sich Gottes Sohn", "tokens": ["Da", "mei\u00b7net", "er", "sich", "Got\u00b7tes", "Sohn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und dehnet sich so stolz,", "tokens": ["Und", "deh\u00b7net", "sich", "so", "stolz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und baumelt mit den Beinen dann", "tokens": ["Und", "bau\u00b7melt", "mit", "den", "Bei\u00b7nen", "dann"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und knackt mit jedem Glied,", "tokens": ["Und", "knackt", "mit", "je\u00b7dem", "Glied", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Die Welt erzittert vor dem Mann,", "tokens": ["Die", "Welt", "er\u00b7zit\u00b7tert", "vor", "dem", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wenn sie die Unart sieht.", "tokens": ["Wenn", "sie", "die", "Un\u00b7art", "sieht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "So in der dunklen Kammer sitzt", "tokens": ["So", "in", "der", "dunk\u00b7len", "Kam\u00b7mer", "sitzt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Kriegers Poltergeist,", "tokens": ["Des", "Krie\u00b7gers", "Pol\u00b7ter\u00b7geist", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der wie ein Thron zur Sonne blitzt,", "tokens": ["Der", "wie", "ein", "Thron", "zur", "Son\u00b7ne", "blitzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KOKOM", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sein Schwert heraus schon rei\u00dft;", "tokens": ["Sein", "Schwert", "he\u00b7raus", "schon", "rei\u00dft", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Was scheinst du Sonne auch so stark,", "tokens": ["Was", "scheinst", "du", "Son\u00b7ne", "auch", "so", "stark", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "NN", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Siehst so geschwind und weit,", "tokens": ["Siehst", "so", "ge\u00b7schwind", "und", "weit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "Wo sich der gute F\u00fcrst noch barg,", "tokens": ["Wo", "sich", "der", "gu\u00b7te", "F\u00fcrst", "noch", "barg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ART", "ADJA", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Verr\u00e4th ihn Menschenfreud.", "tokens": ["Ver\u00b7r\u00e4\u00b7th", "ihn", "Men\u00b7schen\u00b7freud", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Der Krieger haut in seinen Thron,", "tokens": ["Der", "Krie\u00b7ger", "haut", "in", "sei\u00b7nen", "Thron", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Splittern fliegen dicht,", "tokens": ["Die", "Split\u00b7tern", "flie\u00b7gen", "dicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Holt siegend gleich den andern schon,", "tokens": ["Holt", "sie\u00b7gend", "gleich", "den", "an\u00b7dern", "schon", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "ADV", "ART", "ADJA", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Eh' noch der alte bricht;", "tokens": ["Eh'", "noch", "der", "al\u00b7te", "bricht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der Krieger holt ihn gleich zum Hohn,", "tokens": ["Der", "Krie\u00b7ger", "holt", "ihn", "gleich", "zum", "Hohn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den F\u00fcrsten st\u00f6\u00dft hinab,", "tokens": ["Den", "F\u00fcrs\u00b7ten", "st\u00f6\u00dft", "hin\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "In dunkler Kammer wird der Thron", "tokens": ["In", "dunk\u00b7ler", "Kam\u00b7mer", "wird", "der", "Thron"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "So dunkel wie das Grab.", "tokens": ["So", "dun\u00b7kel", "wie", "das", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Seht das geschah am gr\u00fcnen Holz.", "tokens": ["Seht", "das", "ge\u00b7schah", "am", "gr\u00fc\u00b7nen", "Holz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDS", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie wird's am d\u00fcrren sein,", "tokens": ["Wie", "wird's", "am", "d\u00fcr\u00b7ren", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PTKA", "ADJD", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der alte Krieger wird so stolz,", "tokens": ["Der", "al\u00b7te", "Krie\u00b7ger", "wird", "so", "stolz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Meint alles endlich sein;", "tokens": ["Meint", "al\u00b7les", "end\u00b7lich", "sein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Vorletzt er nahm den \u00e4ltesten Thron", "tokens": ["Vor\u00b7letzt", "er", "nahm", "den", "\u00e4l\u00b7tes\u00b7ten", "Thron"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Aus Eisen fest erbaut,", "tokens": ["Aus", "Ei\u00b7sen", "fest", "er\u00b7baut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Nach ewig ferner Nordlichtskron", "tokens": ["Nach", "e\u00b7wig", "fer\u00b7ner", "Nord\u00b7lichts\u00b7kron"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Er nun mit Sehnsucht schaut.", "tokens": ["Er", "nun", "mit", "Sehn\u00b7sucht", "schaut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Er haut auf seinen Eisenthron,", "tokens": ["Er", "haut", "auf", "sei\u00b7nen", "Ei\u00b7sen\u00b7thron", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als w\u00e4r's des Feindes Rest,", "tokens": ["Als", "w\u00e4r's", "des", "Fein\u00b7des", "Rest", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da springt sein Schwert in Funkenton,", "tokens": ["Da", "springt", "sein", "Schwert", "in", "Fun\u00b7ken\u00b7ton", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "APPR", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Kraft ihn da verl\u00e4\u00dft,", "tokens": ["Die", "Kraft", "ihn", "da", "ver\u00b7l\u00e4\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Gest\u00e4hlt war ihm sein Herz so hart,", "tokens": ["Ge\u00b7st\u00e4hlt", "war", "ihm", "sein", "Herz", "so", "hart", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "PPOSAT", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nun wird es menschlich weich,", "tokens": ["Nun", "wird", "es", "menschlich", "weich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "ADJD", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Es bieget sich nach Menschenart,", "tokens": ["Es", "bie\u00b7get", "sich", "nach", "Men\u00b7schen\u00b7art", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und f\u00fchlt mit andern gleich.", "tokens": ["Und", "f\u00fchlt", "mit", "an\u00b7dern", "gleich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PIS", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Wie einer, der erwacht aus Wuth,", "tokens": ["Wie", "ei\u00b7ner", ",", "der", "er\u00b7wacht", "aus", "Wuth", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "$,", "PRELS", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er sieht mit Schrecken nun,", "tokens": ["Er", "sieht", "mit", "Schre\u00b7cken", "nun", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er sog aus sich fast alles Blut", "tokens": ["Er", "sog", "aus", "sich", "fast", "al\u00b7les", "Blut"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PRF", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und meint's dem Feind zu thun;", "tokens": ["Und", "meint's", "dem", "Feind", "zu", "thun", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mu\u00df seine Bahn so klein nun sehn,", "tokens": ["Mu\u00df", "sei\u00b7ne", "Bahn", "so", "klein", "nun", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "ADV", "ADJD", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nur andre zog er gro\u00df,", "tokens": ["Nur", "and\u00b7re", "zog", "er", "gro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Die es verschm\u00e4hten hoch zu stehn,", "tokens": ["Die", "es", "ver\u00b7schm\u00e4h\u00b7ten", "hoch", "zu", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Weil sie schon \u00fcbergro\u00df.", "tokens": ["Weil", "sie", "schon", "\u00fc\u00b7ber\u00b7gro\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Noch keiner wei\u00df, wo er da blieb,", "tokens": ["Noch", "kei\u00b7ner", "wei\u00df", ",", "wo", "er", "da", "blieb", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "$,", "PWAV", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bis keiner an ihn glaubt,", "tokens": ["Bis", "kei\u00b7ner", "an", "ihn", "glaubt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das gr\u00fcne Holz ist allen lieb", "tokens": ["Das", "gr\u00fc\u00b7ne", "Holz", "ist", "al\u00b7len", "lieb"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PIAT", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und ist so frisch belaubt:", "tokens": ["Und", "ist", "so", "frisch", "be\u00b7laubt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die Schwerter bindet, baut den Thron", "tokens": ["Die", "Schwer\u00b7ter", "bin\u00b7det", ",", "baut", "den", "Thron"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Aus Eisen, nicht aus Holz,", "tokens": ["Aus", "Ei\u00b7sen", ",", "nicht", "aus", "Holz", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PTKNEG", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "So spricht euch nie ein Fremdling Hohn,", "tokens": ["So", "spricht", "euch", "nie", "ein", "Fremd\u00b7ling", "Hohn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Verwirft sich auch nicht stolz.", "tokens": ["Ver\u00b7wirft", "sich", "auch", "nicht", "stolz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Doch in der Hand, mein F\u00fcrstensohn,", "tokens": ["Doch", "in", "der", "Hand", ",", "mein", "F\u00fcrs\u00b7ten\u00b7sohn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Halt nicht das scharfe Schwert,", "tokens": ["Halt", "nicht", "das", "schar\u00b7fe", "Schwert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Halt einen gr\u00fcnen Zweig zum Lohn,", "tokens": ["Halt", "ei\u00b7nen", "gr\u00fc\u00b7nen", "Zweig", "zum", "Lohn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "pu