{"dta.poem.21405": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Caput XXIII.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-30602-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201eein andrer Verleger h\u00e4tte mich", "tokens": ["\u201e", "ein", "an\u00b7drer", "Ver\u00b7le\u00b7ger", "h\u00e4t\u00b7te", "mich"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "ART", "ADJA", "NN", "VAFIN", "PPER"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Vielleicht verhungern lassen,", "tokens": ["Viel\u00b7leicht", "ver\u00b7hun\u00b7gern", "las\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der aber giebt mir zu trinken sogar;", "tokens": ["Der", "a\u00b7ber", "giebt", "mir", "zu", "trin\u00b7ken", "so\u00b7gar", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "PPER", "PTKZU", "VVINF", "ADV", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Werde ihn niemals verlassen.", "tokens": ["Wer\u00b7de", "ihn", "nie\u00b7mals", "ver\u00b7las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "VVINF", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.2": {"line.1": {"text": "\u201eich danke dem Sch\u00f6pfer in der H\u00f6h\u2019,", "tokens": ["\u201e", "ich", "dan\u00b7ke", "dem", "Sch\u00f6p\u00b7fer", "in", "der", "H\u00f6h'", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der diesen Saft der Reben", "tokens": ["Der", "die\u00b7sen", "Saft", "der", "Re\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PDAT", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Erschuf, und zum Verleger mir", "tokens": ["Er\u00b7schuf", ",", "und", "zum", "Ver\u00b7le\u00b7ger", "mir"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "KON", "APPRART", "NN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Julius Campe gegeben!", "tokens": ["Den", "Ju\u00b7lius", "Cam\u00b7pe", "ge\u00b7ge\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "NE", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "\u201eich danke dem Sch\u00f6pfer in der H\u00f6h\u2019,", "tokens": ["\u201e", "ich", "dan\u00b7ke", "dem", "Sch\u00f6p\u00b7fer", "in", "der", "H\u00f6h'", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der, durch sein gro\u00dfes Werde,", "tokens": ["Der", ",", "durch", "sein", "gro\u00b7\u00dfes", "Wer\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Austern erschaffen in der See", "tokens": ["Die", "Aus\u00b7tern", "er\u00b7schaf\u00b7fen", "in", "der", "See"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVPP", "APPR", "ART", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und den Rheinwein auf der Erde!", "tokens": ["Und", "den", "Rhein\u00b7wein", "auf", "der", "Er\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "\u201eder auch Citronen wachsen lie\u00df,", "tokens": ["\u201e", "der", "auch", "Cit\u00b7ro\u00b7nen", "wach\u00b7sen", "lie\u00df", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PRELS", "ADV", "NN", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Austern zu bethauen \u2013", "tokens": ["Die", "Aus\u00b7tern", "zu", "be\u00b7thau\u00b7en", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Nun la\u00df mich, Vater, diese Nacht", "tokens": ["Nun", "la\u00df", "mich", ",", "Va\u00b7ter", ",", "die\u00b7se", "Nacht"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ADV", "VVIMP", "PPER", "$,", "NN", "$,", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Essen gut verdauen!\u201c", "tokens": ["Das", "Es\u00b7sen", "gut", "ver\u00b7dau\u00b7en", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "ADJD", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}