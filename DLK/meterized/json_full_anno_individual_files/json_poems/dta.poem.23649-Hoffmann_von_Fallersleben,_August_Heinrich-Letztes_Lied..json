{"dta.poem.23649": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Letztes Lied.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1840", "urn": "urn:nbn:de:kobv:b4-200905192626", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nackt ein, nackt aus,", "tokens": ["Nackt", "ein", ",", "nackt", "aus", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "ADJD", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Zur Welt hinaus:", "tokens": ["Zur", "Welt", "hin\u00b7aus", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Mein B\u00fcndel Sorgen mit hinab", "tokens": ["Mein", "B\u00fcn\u00b7del", "Sor\u00b7gen", "mit", "hin\u00b7ab"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "NN", "APPR", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ins dunkle Grab!", "tokens": ["Ins", "dunk\u00b7le", "Grab", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Nun schaufelt zu und immerzu!", "tokens": ["Nun", "schau\u00b7felt", "zu", "und", "im\u00b7mer\u00b7zu", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKVZ", "KON", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ich schlafe fest und habe Ruh.", "tokens": ["Ich", "schla\u00b7fe", "fest", "und", "ha\u00b7be", "Ruh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "KON", "VAFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "In Liebesmuth", "tokens": ["In", "Lie\u00b7bes\u00b7muth"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Voll Jugendglut", "tokens": ["Voll", "Ju\u00b7gend\u00b7glut"], "token_info": ["word", "word"], "pos": ["ADJD", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Ein halbes Leben mir verschwand;", "tokens": ["Ein", "hal\u00b7bes", "Le\u00b7ben", "mir", "ver\u00b7schwand", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das andre fand", "tokens": ["Das", "and\u00b7re", "fand"], "token_info": ["word", "word", "word"], "pos": ["PDS", "PIS", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "In dieser Welt nicht Rast noch Ruh \u2014", "tokens": ["In", "die\u00b7ser", "Welt", "nicht", "Rast", "noch", "Ruh"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "PTKNEG", "NN", "ADV", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Drum, Br\u00fcder, scharret zu, nur zu!", "tokens": ["Drum", ",", "Br\u00fc\u00b7der", ",", "schar\u00b7ret", "zu", ",", "nur", "zu", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "$,", "NN", "$,", "VVFIN", "PTKVZ", "$,", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}