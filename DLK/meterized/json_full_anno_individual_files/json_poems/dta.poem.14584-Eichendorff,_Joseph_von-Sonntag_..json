{"dta.poem.14584": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Sonntag .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.85", "sv:0.14"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "\u201eschon klingen Morgenglocken,", "tokens": ["\u201e", "schon", "klin\u00b7gen", "Mor\u00b7gen\u00b7glo\u00b7cken", ","], "token_info": ["punct", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der liebe Gott nun bald", "tokens": ["Der", "lie\u00b7be", "Gott", "nun", "bald"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADV", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Geht durch den stillen Wald.\u201c \u2014", "tokens": ["Geht", "durch", "den", "stil\u00b7len", "Wald", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$.", "$(", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Da kniet' ich froherschrocken.", "tokens": ["Da", "kniet'", "ich", "fro\u00b7her\u00b7schro\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}