{"textgrid.poem.67467": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "An den Schnee", "genre": "verse", "period": "N.A.", "pub_year": 1793, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wei\u00dfer, stockiger Schnee, o Du des alten Saturnus", "tokens": ["Wei\u00b7\u00dfer", ",", "sto\u00b7cki\u00b7ger", "Schnee", ",", "o", "Du", "des", "al\u00b7ten", "Sa\u00b7tur\u00b7nus"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "ADJA", "NN", "$,", "FM", "PPER", "ART", "ADJA", "NN"], "meter": "+-+--+-+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Graue Locke, die jetzt unsre Gefilde bestreut!", "tokens": ["Grau\u00b7e", "Lo\u00b7cke", ",", "die", "jetzt", "uns\u00b7re", "Ge\u00b7fil\u00b7de", "be\u00b7streut", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PRELS", "ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Oder bist Du Gefieder der Mutterhenne, die w\u00e4rmend", "tokens": ["O\u00b7der", "bist", "Du", "Ge\u00b7fie\u00b7der", "der", "Mut\u00b7ter\u00b7hen\u00b7ne", ",", "die", "w\u00e4r\u00b7mend"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "NN", "ART", "NN", "$,", "PRELS", "ADJD"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Auch im t\u00f6dtenden Frost decket die schlafende Brut?", "tokens": ["Auch", "im", "t\u00f6d\u00b7ten\u00b7den", "Frost", "de\u00b7cket", "die", "schla\u00b7fen\u00b7de", "Brut", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "--+--+-+-+--+",