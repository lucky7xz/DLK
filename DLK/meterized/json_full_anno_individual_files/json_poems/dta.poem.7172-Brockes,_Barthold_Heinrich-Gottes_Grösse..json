{"dta.poem.7172": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Gottes Gr\u00f6sse.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.85", "nl:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn nun meine Seele sp\u00fcret,", "tokens": ["Wenn", "nun", "mei\u00b7ne", "See\u00b7le", "sp\u00fc\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df, nebst der Gedancken Heer,", "tokens": ["Da\u00df", ",", "nebst", "der", "Ge\u00b7dan\u00b7cken", "Heer", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Sie sich selbst in GOTT verliehret,", "tokens": ["Sie", "sich", "selbst", "in", "GoTT", "ver\u00b7lieh\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PRF", "ADV", "APPR", "NE", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie ein Tropffen in dem Meer;", "tokens": ["Wie", "ein", "Tropf\u00b7fen", "in", "dem", "Meer", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "So soll mir, zu Deinen Ehren,", "tokens": ["So", "soll", "mir", ",", "zu", "Dei\u00b7nen", "Eh\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "++--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Dein Gesch\u00f6pff die Gr\u00f6ss\u2019 erkl\u00e4ren.", "tokens": ["Dein", "Ge\u00b7sch\u00f6pff", "die", "Gr\u00f6ss'", "er\u00b7kl\u00e4\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Jede Deiner Creatur", "tokens": ["Je\u00b7de", "Dei\u00b7ner", "Crea\u00b7tur"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "PPOSAT", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.8": {"text": "Zeiget Deiner Gr\u00f6sse Spur.", "tokens": ["Zei\u00b7get", "Dei\u00b7ner", "Gr\u00f6s\u00b7se", "Spur", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}