{"dta.poem.19634": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Milton's Rache.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "Am Grab der Republik ist er gestanden,", "tokens": ["Am", "Grab", "der", "Re\u00b7pub\u00b7lik", "ist", "er", "ge\u00b7stan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch sah er nicht des Stuart Schiffe landen,", "tokens": ["Doch", "sah", "er", "nicht", "des", "Stu\u00b7art", "Schif\u00b7fe", "lan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ihn h\u00fcllt' in Dunkel eine g\u00fct'ge Macht:", "tokens": ["Ihn", "h\u00fcllt'", "in", "Dun\u00b7kel", "ei\u00b7ne", "g\u00fct'\u00b7ge", "Macht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Er ist erblindet! Herrlich f\u00fcllt mit lichten", "tokens": ["Er", "ist", "er\u00b7blin\u00b7det", "!", "Herr\u00b7lich", "f\u00fcllt", "mit", "lich\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "VVPP", "$.", "NE", "VVFIN", "APPR", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Gebilden und d\u00e4monischen Gesichten", "tokens": ["Ge\u00b7bil\u00b7den", "und", "d\u00e4\u00b7mo\u00b7ni\u00b7schen", "Ge\u00b7sich\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KON", "ADJA", "NN"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Die Muse seines Auges Nacht ...", "tokens": ["Die", "Mu\u00b7se", "sei\u00b7nes", "Au\u00b7ges", "Nacht", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ein eifrig M\u00e4dchenantlitz neigt sich neben", "tokens": ["Ein", "eif\u00b7rig", "M\u00e4d\u00b7chen\u00b7ant\u00b7litz", "neigt", "sich", "ne\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "NN", "VVFIN", "PRF", "APPR"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der m\u00fcden Ampel, feine Finger schweben,", "tokens": ["Der", "m\u00fc\u00b7den", "Am\u00b7pel", ",", "fei\u00b7ne", "Fin\u00b7ger", "schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Auf leichte Bl\u00e4tter schreibt des Dichters Kind", "tokens": ["Auf", "leich\u00b7te", "Bl\u00e4t\u00b7ter", "schreibt", "des", "Dich\u00b7ters", "Kind"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mit eines Stiftes ungeh\u00f6rtem Gleiten", "tokens": ["Mit", "ei\u00b7nes", "Stif\u00b7tes", "un\u00b7ge\u00b7h\u00f6r\u00b7tem", "Glei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Die Wucht der Worte, die f\u00fcr alle Zeiten", "tokens": ["Die", "Wucht", "der", "Wor\u00b7te", ",", "die", "f\u00fcr", "al\u00b7le", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "$,", "PRELS", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "In Marmelstein gehauen sind ...", "tokens": ["In", "Mar\u00b7mel\u00b7stein", "ge\u00b7hau\u00b7en", "sind", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVPP", "VAFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Er spricht: \u201eZur Stunde, da\u201c \u2014 Hohnrufe gellen,", "tokens": ["Er", "spricht", ":", "\u201e", "Zur", "Stun\u00b7de", ",", "da", "\u201c", "Hohn\u00b7ru\u00b7fe", "gel\u00b7len", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct", "word", "punct", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "$(", "APPRART", "NN", "$,", "KOUS", "$(", "$(", "NN", "VVPP", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Das Haupt, das blinde, bleiche, zuckt in grellen,", "tokens": ["Das", "Haupt", ",", "das", "blin\u00b7de", ",", "blei\u00b7che", ",", "zuckt", "in", "grel\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "$,", "VVFIN", "$,", "VVFIN", "APPR", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Lodernden Fackelgluten, z\u00fcrnt und lauscht ...", "tokens": ["Lo\u00b7dern\u00b7den", "Fa\u00b7ckel\u00b7glu\u00b7ten", ",", "z\u00fcrnt", "und", "lauscht", "..."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "KON", "VVFIN", "$("], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "Durch Londons Gassen wandern um die Horden", "tokens": ["Durch", "Lon\u00b7dons", "Gas\u00b7sen", "wan\u00b7dern", "um", "die", "Hor\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Der Cavaliere, Schlaf und Scham zu morden,", "tokens": ["Der", "Ca\u00b7va\u00b7lie\u00b7re", ",", "Schlaf", "und", "Scham", "zu", "mor\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "KON", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Von Wein und Uebermuth berauscht:", "tokens": ["Von", "Wein", "und", "Ue\u00b7ber\u00b7muth", "be\u00b7rauscht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "\u201eschaut auf! Das ist des Puritaners Erker!", "tokens": ["\u201e", "schaut", "auf", "!", "Das", "ist", "des", "Pu\u00b7ri\u00b7ta\u00b7ners", "Er\u00b7ker", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PTKVZ", "$.", "PDS", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Schreiber h\u00e4lt ein bl\u00fchend Kind im Kerker!", "tokens": ["Der", "Schrei\u00b7ber", "h\u00e4lt", "ein", "bl\u00fc\u00b7hend", "Kind", "im", "Ker\u00b7ker", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJD", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Schuhu h\u00fctet einen duft'gen Kranz!", "tokens": ["Der", "Schu\u00b7hu", "h\u00fc\u00b7tet", "ei\u00b7nen", "duft'\u00b7gen", "Kranz", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wir schreiten schlank und jung, wir sind die S\u00fcnden", "tokens": ["Wir", "schrei\u00b7ten", "schlank", "und", "jung", ",", "wir", "sind", "die", "S\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "KON", "ADJD", "$,", "PPER", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und kommen ihr das Herzchen zu entz\u00fcnden", "tokens": ["Und", "kom\u00b7men", "ihr", "das", "Herz\u00b7chen", "zu", "ent\u00b7z\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Mit Saitenspiel und Reigentanz!", "tokens": ["Mit", "Sai\u00b7ten\u00b7spiel", "und", "Rei\u00b7gen\u00b7tanz", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Vertreibt den Kauz vom Nest! Umarmt die Dirne!\u201c", "tokens": ["Ver\u00b7treibt", "den", "Kauz", "vom", "Nest", "!", "Um\u00b7armt", "die", "Dir\u00b7ne", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "ART", "NN", "APPRART", "NN", "$.", "VVFIN", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Geklirr! Ein Stein! ... Still blutet eine Stirne,", "tokens": ["Ge\u00b7klirr", "!", "Ein", "Stein", "!", "...", "Still", "blu\u00b7tet", "ei\u00b7ne", "Stir\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ART", "NN", "$.", "$(", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Den Vater schirmt das M\u00e4dchen mit dem Leib,", "tokens": ["Den", "Va\u00b7ter", "schirmt", "das", "M\u00e4d\u00b7chen", "mit", "dem", "Leib", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die Bleiche dr\u00fcckt er auf den Schemel nieder,", "tokens": ["Die", "Blei\u00b7che", "dr\u00fcckt", "er", "auf", "den", "Sche\u00b7mel", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ein Richter, kehrt zu seinem Lied er wieder:", "tokens": ["Ein", "Rich\u00b7ter", ",", "kehrt", "zu", "sei\u00b7nem", "Lied", "er", "wie\u00b7der", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "APPR", "PPOSAT", "NN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "\u201enimm deinen Stift, mein Kind, und schreib!", "tokens": ["\u201e", "nimm", "dei\u00b7nen", "Stift", ",", "mein", "Kind", ",", "und", "schreib", "!"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "VVIMP", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Zur Stunde, da des Lasterk\u00f6nigs Knechte", "tokens": ["Zur", "Stun\u00b7de", ",", "da", "des", "Las\u00b7ter\u00b7k\u00f6\u00b7nigs", "Knech\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "KOUS", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Umwandern, die Entheiliger der N\u00e4chte ...", "tokens": ["Um\u00b7wan\u00b7dern", ",", "die", "En\u00b7thei\u00b7li\u00b7ger", "der", "N\u00e4ch\u00b7te", "..."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "ART", "NN", "$("], "meter": "-+--++-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Zur Stunde, da die H\u00f6lle frechen Schalls", "tokens": ["Zur", "Stun\u00b7de", ",", "da", "die", "H\u00f6l\u00b7le", "fre\u00b7chen", "Schalls"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "KOUS", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Aufschreit, empor zu den erhabnen Th\u00fcrmen ...", "tokens": ["Auf\u00b7schreit", ",", "em\u00b7por", "zu", "den", "er\u00b7hab\u00b7nen", "Th\u00fcr\u00b7men", "..."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PTKVZ", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Zur Stunde, da die Riesenstadt durchst\u00fcrmen", "tokens": ["Zur", "Stun\u00b7de", ",", "da", "die", "Rie\u00b7sen\u00b7stadt", "durchs\u00b7t\u00fcr\u00b7men"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "KOUS", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die blut'gen S\u00f6hne Belials .....\u201c", "tokens": ["Die", "blut'\u00b7gen", "S\u00f6h\u00b7ne", "Be\u00b7li\u00b7als", ".....", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "So sang mit wunder Stirn der geisterblasse", "tokens": ["So", "sang", "mit", "wun\u00b7der", "Stirn", "der", "geis\u00b7ter\u00b7blas\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Poet. Verschollen ist der L\u00e4rm der Gasse,", "tokens": ["Po\u00b7et", ".", "Ver\u00b7schol\u00b7len", "ist", "der", "L\u00e4rm", "der", "Gas\u00b7se", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "NN", "VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch ob Jahrhundert um Jahrhundert flieht,", "tokens": ["Doch", "ob", "Jahr\u00b7hun\u00b7dert", "um", "Jahr\u00b7hun\u00b7dert", "flieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Von einem bangen M\u00e4dchen aufgeschrieben,", "tokens": ["Von", "ei\u00b7nem", "ban\u00b7gen", "M\u00e4d\u00b7chen", "auf\u00b7ge\u00b7schrie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Sind Miltons R\u00e4cherverse stehn geblieben,", "tokens": ["Sind", "Mil\u00b7tons", "R\u00e4\u00b7cher\u00b7ver\u00b7se", "stehn", "ge\u00b7blie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "NN", "VVINF", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Verwoben in sein ewig Lied.", "tokens": ["Ver\u00b7wo\u00b7ben", "in", "sein", "e\u00b7wig", "Lied", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}