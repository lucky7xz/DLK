{"textgrid.poem.48740": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "27. An die gro\u00dfe Stadt Moskaw, als er schiede", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Prinzessin deines Reichs, die Holstein Mume nennt,", "tokens": ["Prin\u00b7zes\u00b7sin", "dei\u00b7nes", "Reichs", ",", "die", "Hol\u00b7stein", "Mu\u00b7me", "nennt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "du wahre Freundin du, durch welcher Gunst wir wagen,", "tokens": ["du", "wah\u00b7re", "Freun\u00b7din", "du", ",", "durch", "wel\u00b7cher", "Gunst", "wir", "wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "PPER", "$,", "APPR", "PWAT", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "was F\u00fcrsten ward versagt und K\u00f6n'gen abgeschlagen,", "tokens": ["was", "F\u00fcrs\u00b7ten", "ward", "ver\u00b7sagt", "und", "K\u00f6n'\u00b7gen", "ab\u00b7ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VAFIN", "VVPP", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "den Weg nach Aufgang zu, wir haben nun erkennt,", "tokens": ["den", "Weg", "nach", "Auf\u00b7gang", "zu", ",", "wir", "ha\u00b7ben", "nun", "er\u00b7kennt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "PTKVZ", "$,", "PPER", "VAFIN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "wie sehr dein freundlichs Herz in unsrer Liebe brennt.", "tokens": ["wie", "sehr", "dein", "freund\u00b7lichs", "Herz", "in", "uns\u00b7rer", "Lie\u00b7be", "brennt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPOSAT", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Treue wollen wir mit uns nach Osten tragen,", "tokens": ["Die", "Treu\u00b7e", "wol\u00b7len", "wir", "mit", "uns", "nach", "Os\u00b7ten", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "APPR", "PPER", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "und bei der Wiederkunft in unsern Landen sagen,", "tokens": ["und", "bei", "der", "Wie\u00b7der\u00b7kunft", "in", "un\u00b7sern", "Lan\u00b7den", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "das B\u00fcndn\u00fc\u00df ist gemacht, das keine Zeit zertrennt.", "tokens": ["das", "B\u00fcnd\u00b7n\u00fc\u00df", "ist", "ge\u00b7macht", ",", "das", "kei\u00b7ne", "Zeit", "zer\u00b7trennt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,", "PRELS", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Des frommen Himmels Gunst, die m\u00fcsse dich erfreuen,", "tokens": ["Des", "from\u00b7men", "Him\u00b7mels", "Gunst", ",", "die", "m\u00fcs\u00b7se", "dich", "er\u00b7freu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$,", "PRELS", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "und alles, was du tust, nach Wundsche dir gedeien,", "tokens": ["und", "al\u00b7les", ",", "was", "du", "tust", ",", "nach", "Wund\u00b7sche", "dir", "ge\u00b7dei\u00b7en", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "$,", "PWS", "PPER", "VVFIN", "$,", "APPR", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "kein Mars und kein Vulkan dir \u00fcberl\u00e4stig sein!", "tokens": ["kein", "Mars", "und", "kein", "Vul\u00b7kan", "dir", "\u00fc\u00b7berl\u00b7\u00e4s\u00b7tig", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "PIAT", "NN", "PPER", "ADJD", "VAINF", "$."], "meter": "-+--++-+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "Nim itzo di\u00df Sonnet. Komm ich mit Gl\u00fccke wieder,", "tokens": ["Nim", "it\u00b7zo", "di\u00df", "Son\u00b7net", ".", "Komm", "ich", "mit", "Gl\u00fc\u00b7cke", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PDS", "NN", "$.", "VVFIN", "PPER", "APPR", "NN", "ADV", "$,"], "meter": "-+--+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "so will ich deinen Preis erh\u00f6hn durch st\u00e4rkre Lieder,", "tokens": ["so", "will", "ich", "dei\u00b7nen", "Preis", "er\u00b7h\u00f6hn", "durch", "st\u00e4r\u00b7kre", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct