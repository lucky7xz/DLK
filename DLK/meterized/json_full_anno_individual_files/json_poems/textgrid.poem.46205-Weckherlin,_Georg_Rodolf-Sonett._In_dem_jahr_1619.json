{"textgrid.poem.46205": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Sonett. In dem jahr 1619", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Verfolgung, m\u00fch und leid ist allein das banier,", "tokens": ["Ver\u00b7fol\u00b7gung", ",", "m\u00fch", "und", "leid", "ist", "al\u00b7lein", "das", "ba\u00b7nier", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "KON", "ADJD", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "darunder durch die welt sich gottes kinder schlagen:", "tokens": ["da\u00b7run\u00b7der", "durch", "die", "welt", "sich", "got\u00b7tes", "kin\u00b7der", "schla\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "APPR", "ART", "NN", "PRF", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "und der h\u00f6chst (general) hat acht, wie man sie f\u00fchr,", "tokens": ["und", "der", "h\u00f6chst", "(", "ge\u00b7ne\u00b7ral", ")", "hat", "acht", ",", "wie", "man", "sie", "f\u00fchr", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADV", "$(", "ADJD", "$(", "VAFIN", "CARD", "$,", "PWAV", "PIS", "PPER", "VVFIN", "$,"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "und wie ein jeder sich begehr f\u00fcr ihn zu wagen.", "tokens": ["und", "wie", "ein", "je\u00b7der", "sich", "be\u00b7gehr", "f\u00fcr", "ihn", "zu", "wa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "PIAT", "PRF", "ADJD", "APPR", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Oftmal erlaubet er, da\u00df ihr feind triumfier,", "tokens": ["Oft\u00b7mal", "er\u00b7lau\u00b7bet", "er", ",", "da\u00df", "ihr", "feind", "tri\u00b7um\u00b7fier", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+--+-+--+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "doch l\u00e4sset er sein volk g\u00e4nzlich niemal verzagen;", "tokens": ["doch", "l\u00e4s\u00b7set", "er", "sein", "volk", "g\u00e4nz\u00b7lich", "nie\u00b7mal", "ver\u00b7za\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "ADJD", "ADV", "VVINF", "$."], "meter": "--+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "sondern damit sein feind nicht gar zu vil stolzier,", "tokens": ["son\u00b7dern", "da\u00b7mit", "sein", "feind", "nicht", "gar", "zu", "vil", "stol\u00b7zier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "PPOSAT", "NN", "PTKNEG", "ADV", "APPR", "PIAT", "NN", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.8": {"text": "verkehret (m\u00e4chtig) er sein jauchzen bald in klagen.", "tokens": ["ver\u00b7keh\u00b7ret", "(", "m\u00e4ch\u00b7tig", ")", "er", "sein", "jauch\u00b7zen", "bald", "in", "kla\u00b7gen", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "ADJD", "$(", "PPER", "PPOSAT", "NN", "ADV", "APPR", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Darum ihr, deren will, des teufels willen gleich,", "tokens": [