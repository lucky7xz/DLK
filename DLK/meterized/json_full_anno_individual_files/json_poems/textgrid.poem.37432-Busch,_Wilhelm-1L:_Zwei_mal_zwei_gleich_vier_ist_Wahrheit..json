{"textgrid.poem.37432": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Zwei mal zwei gleich vier ist Wahrheit.", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zwei mal zwei gleich vier ist Wahrheit.", "tokens": ["Zwei", "mal", "zwei", "gleich", "vier", "ist", "Wahr\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "ADV", "CARD", "ADV", "CARD", "VAFIN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schade, da\u00df sie leicht und leer ist,", "tokens": ["Scha\u00b7de", ",", "da\u00df", "sie", "leicht", "und", "leer", "ist", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "KOUS", "PPER", "ADJD", "KON", "ADJD", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn ich wollte lieber Klarheit", "tokens": ["Denn", "ich", "woll\u00b7te", "lie\u00b7ber", "Klar\u00b7heit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VMFIN", "ADV", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "\u00dcber das, was voll und schwer ist.", "tokens": ["\u00dc\u00b7ber", "das", ",", "was", "voll", "und", "schwer", "ist", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDS", "$,", "PRELS", "ADJD", "KON", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Emsig sucht ich aufzufinden,", "tokens": ["Em\u00b7sig", "sucht", "ich", "auf\u00b7zu\u00b7fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "VVIZU", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was im tiefsten Grunde wurzelt,", "tokens": ["Was", "im", "tiefs\u00b7ten", "Grun\u00b7de", "wur\u00b7zelt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Lief umher nach allen Winden", "tokens": ["Lief", "um\u00b7her", "nach", "al\u00b7len", "Win\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKVZ", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und bin oft dabei gepurzelt.", "tokens": ["Und", "bin", "oft", "da\u00b7bei", "ge\u00b7pur\u00b7zelt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "PAV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Endlich baut ich eine H\u00fctte.", "tokens": ["End\u00b7lich", "baut", "ich", "ei\u00b7ne", "H\u00fct\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Still nun zwischen ihren W\u00e4nden", "tokens": ["Still", "nun", "zwi\u00b7schen", "ih\u00b7ren", "W\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sitz ich in der Welten Mitte,", "tokens": ["Sitz", "ich", "in", "der", "Wel\u00b7ten", "Mit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Unbek\u00fcmmert um die Enden.", "tokens": ["Un\u00b7be\u00b7k\u00fcm\u00b7mert", "um", "die", "En\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["A