{"textgrid.poem.67216": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wenn aus sich lebt der Mensch und wenn sein Rest sich zeiget,", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn aus sich lebt der Mensch und wenn sein Rest sich zeiget,", "tokens": ["Wenn", "aus", "sich", "lebt", "der", "Mensch", "und", "wenn", "sein", "Rest", "sich", "zei\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PRF", "VVFIN", "ART", "NN", "KON", "KOUS", "PPOSAT", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So ists, als wenn ein Tag sich Tagen unterscheidet,", "tokens": ["So", "ists", ",", "als", "wenn", "ein", "Tag", "sich", "Ta\u00b7gen", "un\u00b7ter\u00b7schei\u00b7det", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "$,", "KOKOM", "KOUS", "ART", "NN", "PRF", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da\u00df ausgezeichnet sich der Mensch zum Reste neiget,", "tokens": ["Da\u00df", "aus\u00b7ge\u00b7zeich\u00b7net", "sich", "der", "Mensch", "zum", "Res\u00b7te", "nei\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PRF", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Von der Natur getrennt und unbeneidet.", "tokens": ["Von", "der", "Na\u00b7tur", "ge\u00b7trennt", "und", "un\u00b7be\u00b7nei\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Als wie allein ist er im andern weiten Leben,", "tokens": ["Als", "wie", "al\u00b7lein", "ist", "er", "im", "an\u00b7dern", "wei\u00b7ten", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "ADV", "VAFIN", "PPER", "APPRART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo rings der Fr\u00fchling gr\u00fcnt, der Sommer freundlich weilet,", "tokens": ["Wo", "rings", "der", "Fr\u00fch\u00b7ling", "gr\u00fcnt", ",", "der", "Som\u00b7mer", "freund\u00b7lich", "wei\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "VVFIN", "$,", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Bis da\u00df das Jahr im Herbst hinunter eilet,", "tokens": ["Bis", "da\u00df", "das", "Jahr", "im", "Herbst", "hin\u00b7un\u00b7ter", "ei\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "ART", "NN", "APPRART", "NN", "APPO", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und immerdar die Wolken uns umschweben.", "tokens": ["Und", "im\u00b7mer\u00b7dar", "die", "Wol\u00b7ken", "uns", "um\u00b7schwe\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wenn aus sich lebt der Mensch und wenn sein Rest sich zeiget,", "tokens": ["Wenn", "aus", "sich", "lebt", "der", "Mensch", "und", "wenn", "sein", "Rest", "sich", "zei\u00b7get", ","], "token_inf