{"dta.poem.16090": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "144.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wer vom gebahnten Weg im Unverstand abirrt,", "tokens": ["Wer", "vom", "ge\u00b7bahn\u00b7ten", "Weg", "im", "Un\u00b7ver\u00b7stand", "a\u00b7birrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPRART", "ADJA", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und sich im Waldgeheg des Eigensinns verwirrt,", "tokens": ["Und", "sich", "im", "Wald\u00b7ge\u00b7heg", "des", "Ei\u00b7gen\u00b7sinns", "ver\u00b7wirrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "APPRART", "NN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Dann klagt, da\u00df \u00fcberal sich Schwierigkeiten finden,", "tokens": ["Dann", "klagt", ",", "da\u00df", "\u00fc\u00b7be\u00b7ral", "sich", "Schwie\u00b7rig\u00b7kei\u00b7ten", "fin\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "KOUS", "ADJD", "PRF", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und niemand weg sie r\u00e4umt, der ist wol gleich dem Blinden,", "tokens": ["Und", "nie\u00b7mand", "weg", "sie", "r\u00e4umt", ",", "der", "ist", "wol", "gleich", "dem", "Blin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "ADV", "PPER", "VVFIN", "$,", "PRELS", "VAFIN", "ADV", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-++--+-", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Der von dem Sehenden sich nicht will lassen leiten,", "tokens": ["Der", "von", "dem", "Se\u00b7hen\u00b7den", "sich", "nicht", "will", "las\u00b7sen", "lei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "PRF", "PTKNEG", "VMFIN", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und lieber auf gut Gl\u00fcck und seine Fahr hinschreiten,", "tokens": ["Und", "lie\u00b7ber", "auf", "gut", "Gl\u00fcck", "und", "sei\u00b7ne", "Fahr", "hin\u00b7schrei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ADJD", "NN", "KON", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Bald tritt in einen Dorn, bald st\u00f6\u00dft an einen Stein,", "tokens": ["Bald", "tritt", "in", "ei\u00b7nen", "Dorn", ",", "bald", "st\u00f6\u00dft", "an", "ei\u00b7nen", "Stein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "$,", "ADV", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bald in den Graben f\u00e4llt, bald stolpert \u00fcber'n Rain,", "tokens": ["Bald", "in", "den", "Gra\u00b7ben", "f\u00e4llt", ",", "bald", "stol\u00b7pert", "\u00fc\u00b7ber'n", "Rain", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "$,", "ADV", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Hier rennt an einen Baum, dort wider eine Mauer,", "tokens": ["Hier", "rennt", "an", "ei\u00b7nen", "Baum", ",", "dort", "wi\u00b7der", "ei\u00b7ne", "Mau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "$,", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den Pflanzer hier verw\u00fcnscht, und flucht dort dem Erbauer,", "tokens": ["Den", "Pflan\u00b7zer", "hier", "ver\u00b7w\u00fcnscht", ",", "und", "flucht", "dort", "dem", "Er\u00b7bau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVPP", "$,", "KON", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Und klagt, die Welt sei schief und jeder Weg verbaut,", "tokens": ["Und", "klagt", ",", "die", "Welt", "sei", "schief", "und", "je\u00b7der", "Weg", "ver\u00b7baut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ART", "NN", "VAFIN", "ADJD", "KON", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da er nur zwischendurch den graden Weg nicht schaut.", "tokens": ["Da", "er", "nur", "zwi\u00b7schen\u00b7durch", "den", "gra\u00b7den", "Weg", "nicht", "schaut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "ART", "ADJA", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}