{"dta.poem.8493": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Die todte Lerche.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1860", "urn": "urn:nbn:de:kobv:b4-200905191007", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich stand an deines Landes Gr&#228;nzen,", "tokens": ["Ich", "stand", "an", "dei\u00b7nes", "Lan\u00b7des", "Gr", "&#228;", "nzen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "XML_entity", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "NE", "$(", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "An deinem gr\u00fcnen Saatenwald,", "tokens": ["An", "dei\u00b7nem", "gr\u00fc\u00b7nen", "Saa\u00b7ten\u00b7wald", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und mit des ersten Strahles Gl\u00e4nzen", "tokens": ["Und", "mit", "des", "ers\u00b7ten", "Strah\u00b7les", "Gl\u00e4n\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ist dein Gesang herabgewallt.", "tokens": ["Ist", "dein", "Ge\u00b7sang", "her\u00b7ab\u00b7ge\u00b7wallt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Sonne schwirrtest du entgegen,", "tokens": ["Der", "Son\u00b7ne", "schwirr\u00b7test", "du", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Gleich einer M\u00fccke um das Licht,", "tokens": ["Gleich", "ei\u00b7ner", "M\u00fc\u00b7cke", "um", "das", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Dein Lied war wie ein Bl\u00fcthenregen,", "tokens": ["Dein", "Lied", "war", "wie", "ein", "Bl\u00fc\u00b7then\u00b7re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Dein Fl\u00fcgelschlag wie ein Gedicht.", "tokens": ["Dein", "Fl\u00fc\u00b7gel\u00b7schlag", "wie", "ein", "Ge\u00b7dicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Da ward es mir, als m\u00fcss\u2019 ich ringen", "tokens": ["Da", "ward", "es", "mir", ",", "als", "m\u00fcss'", "ich", "rin\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "$,", "KOUS", "VMFIN", "PPER", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und flattern in den jungen Tag,", "tokens": ["Und", "flat\u00b7tern", "in", "den", "jun\u00b7gen", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als h\u00f6re ich mein eignes Singen", "tokens": ["Als", "h\u00f6\u00b7re", "ich", "mein", "eig\u00b7nes", "Sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und meinen eignen Fl\u00fcgelschlag;", "tokens": ["Und", "mei\u00b7nen", "eig\u00b7nen", "Fl\u00fc\u00b7gel\u00b7schlag", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Sonne spr\u00fchte gl\u00fche Funken,", "tokens": ["Die", "Son\u00b7ne", "spr\u00fch\u00b7te", "gl\u00fc\u00b7he", "Fun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "In Flammen brannte mein Gesicht,", "tokens": ["In", "Flam\u00b7men", "brann\u00b7te", "mein", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ich selber taumelte wie trunken,", "tokens": ["Ich", "sel\u00b7ber", "tau\u00b7mel\u00b7te", "wie", "trun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "KOKOM", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Wie eine M\u00fccke nach dem Licht.", "tokens": ["Wie", "ei\u00b7ne", "M\u00fc\u00b7cke", "nach", "dem", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Da pl\u00f6tzlich sank und sank es nieder,", "tokens": ["Da", "pl\u00f6tz\u00b7lich", "sank", "und", "sank", "es", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "KON", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Gleich todter Kohle in die Saat,", "tokens": ["Gleich", "tod\u00b7ter", "Koh\u00b7le", "in", "die", "Saat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Noch zucken sah ich kleine Lider", "tokens": ["Noch", "zu\u00b7cken", "sah", "ich", "klei\u00b7ne", "Li\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVINF", "VVFIN", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und bin erschrocken dann genaht;", "tokens": ["Und", "bin", "er\u00b7schro\u00b7cken", "dann", "ge\u00b7naht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "VVPP", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dein letztes Lied, es war verklungen,", "tokens": ["Dein", "letz\u00b7tes", "Lied", ",", "es", "war", "ver\u00b7klun\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Du lagst, ein armer kalter Rest,", "tokens": ["Du", "lagst", ",", "ein", "ar\u00b7mer", "kal\u00b7ter", "Rest", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Am Strahl zerflattert und versungen", "tokens": ["Am", "Strahl", "zer\u00b7flat\u00b7tert", "und", "ver\u00b7sun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVPP", "KON", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Bei deinem halbgebauten Nest.", "tokens": ["Bei", "dei\u00b7nem", "halb\u00b7ge\u00b7bau\u00b7ten", "Nest", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ich m\u00f6chte hei\u00dfe Thr\u00e4nen weinen,", "tokens": ["Ich", "m\u00f6ch\u00b7te", "hei\u00b7\u00dfe", "Thr\u00e4\u00b7nen", "wei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie sie das Weh vom Herzen dr\u00e4ngt,", "tokens": ["Wie", "sie", "das", "Weh", "vom", "Her\u00b7zen", "dr\u00e4ngt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Denn auch mein Leben wird verscheinen,", "tokens": ["Denn", "auch", "mein", "Le\u00b7ben", "wird", "ver\u00b7schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPOSAT", "NN", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ich f\u00fchl\u2019s, versungen und versengt;", "tokens": ["Ich", "f\u00fchl's", ",", "ver\u00b7sun\u00b7gen", "und", "ver\u00b7sengt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVPP", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du siecher Leib, ihr armen Reste!", "tokens": ["Du", "sie\u00b7cher", "Leib", ",", "ihr", "ar\u00b7men", "Res\u00b7te", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Dann nur ein Grab auf gr\u00fcner Flur,", "tokens": ["Dann", "nur", "ein", "Grab", "auf", "gr\u00fc\u00b7ner", "Flur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und nah, nur nah bei meinem Neste,", "tokens": ["Und", "nah", ",", "nur", "nah", "bei", "mei\u00b7nem", "Nes\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "ADV", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "In meiner s\u00fc\u00dfen Heimath nur.", "tokens": ["In", "mei\u00b7ner", "s\u00fc\u00b7\u00dfen", "Hei\u00b7math", "nur", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}