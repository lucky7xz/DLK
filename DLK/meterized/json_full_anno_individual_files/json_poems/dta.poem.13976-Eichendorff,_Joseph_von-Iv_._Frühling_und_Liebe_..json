{"dta.poem.13976": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Iv .   Fr\u00fchling und Liebe .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Der Jugend Glanz, der Sehnsucht irre Weisen,", "tokens": ["Der", "Ju\u00b7gend", "Glanz", ",", "der", "Sehn\u00b7sucht", "ir\u00b7re", "Wei\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die tausend Str\u00f6me durch das duft'ge Land,", "tokens": ["Die", "tau\u00b7send", "Str\u00f6\u00b7me", "durch", "das", "duft'\u00b7ge", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "CARD", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Es zieht uns All' zu seinen Zauberkreisen. \u2013", "tokens": ["Es", "zieht", "uns", "All'", "zu", "sei\u00b7nen", "Zau\u00b7ber\u00b7krei\u00b7sen", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIS", "APPR", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wem Gottesdienst in tiefster Brust entbrannt,", "tokens": ["Wem", "Got\u00b7tes\u00b7dienst", "in", "tiefs\u00b7ter", "Brust", "ent\u00b7brannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Der sieht mit Wehmuth ein unendlich Reisen", "tokens": ["Der", "sieht", "mit", "Weh\u00b7muth", "ein", "un\u00b7end\u00b7lich", "Rei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPR", "NN", "ART", "ADJD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Zu ferner Heimath, die er fromm erkannt;", "tokens": ["Zu", "fer\u00b7ner", "Hei\u00b7math", ",", "die", "er", "fromm", "er\u00b7kannt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "PRELS", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und was sich ", "tokens": ["Und", "was", "sich"], "token_info": ["word", "word", "word"], "pos": ["KON", "PWS", "PRF"], "meter": "++-", "measure": "unknown.measure.di"}, "line.8": {"text": "W\u00f6lbt still den Kelch zum ", "tokens": ["W\u00f6lbt", "still", "den", "Kelch", "zum"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "ART", "NN", "APPRART"], "meter": "-+-+-", "measure": "iambic.di"}}}}}