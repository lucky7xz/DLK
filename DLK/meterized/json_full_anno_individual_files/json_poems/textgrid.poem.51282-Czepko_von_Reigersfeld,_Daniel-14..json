{"textgrid.poem.51282": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "14.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wo sich dem Leib iemals der Schatten wiedersetzt:", "tokens": ["Wo", "sich", "dem", "Leib", "ie\u00b7mals", "der", "Schat\u00b7ten", "wie\u00b7der\u00b7setzt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ART", "NN", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo vor der Seelen sich der Leib bew\u00e4hrter sch\u00e4tzt;", "tokens": ["Wo", "vor", "der", "See\u00b7len", "sich", "der", "Leib", "be\u00b7w\u00e4hr\u00b7ter", "sch\u00e4tzt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "PRF", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wo in der Seele Gott das schlechste schaffen kan,", "tokens": ["Wo", "in", "der", "See\u00b7le", "Gott", "das", "schlechs\u00b7te", "schaf\u00b7fen", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "NN", "ART", "ADJA", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So klag' ich Schatten, Leib und Seele billich an.", "tokens": ["So", "klag'", "ich", "Schat\u00b7ten", ",", "Leib", "und", "See\u00b7le", "bil\u00b7lich", "an", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "$,", "NN", "KON", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Nu schafft die Seele Gott das allerschlechste nicht;", "tokens": ["Nu", "schafft", "die", "See\u00b7le", "Gott", "das", "al\u00b7ler\u00b7schlechs\u00b7te", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "ART", "ADJA", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nu ists, da\u00df nicht der Leib der Seelen wieder spricht:", "tokens": ["Nu", "ists", ",", "da\u00df", "nicht", "der", "Leib", "der", "See\u00b7len", "wie\u00b7der", "spricht", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "$,", "KOUS", "PTKNEG", "ART", "NN", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nu kan vom Leibe nicht der Schatten trennen sich", "tokens": ["Nu", "kan", "vom", "Lei\u00b7be", "nicht", "der", "Schat\u00b7ten", "tren\u00b7nen", "sich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "APPRART", "NN", "PTKNEG", "ART", "NN", "VVFIN", "PRF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Drumb ist nicht Schatten, Leib und Seele wieder mich.", "tokens": ["Drumb", "ist", "nicht", "Schat\u00b7ten", ",", "Leib", "und", "See\u00b7le", "wie\u00b7der", "mich", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PTKNEG", "NN", "$,", "NN", "KON", "NN", "ADV", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Weil Gott die edle Seel aus seinem Athem schafft,", "tokens": ["Weil", "Gott", "die", "ed\u00b7le", "Seel", "aus", "sei\u00b7nem", "A\u00b7them", "schafft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und weil die Seel im Leib erweiset ihre Krafft:", "tokens": ["Und", "weil", "die", "Seel", "im", "Leib", "er\u00b7wei\u00b7set", "ih\u00b7re", "Krafft", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "APPRART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Weil er der Leib von sich den Schatten wirfft und giebt,", "tokens": ["Weil", "er", "der", "Leib", "von", "sich", "den", "Schat\u00b7ten", "wirfft", "und", "giebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "PRF", "ART", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wird Schatten Leib und Seel ohn Gott ja nicht beliebt.", "tokens": ["Wird", "Schat\u00b7ten", "Leib", "und", "Seel", "ohn", "Gott", "ja", "nicht", "be\u00b7liebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "NN", "KON", "NN", "APPR", "NN", "ADV", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Drumb wo der Schatten ist zum Leibe stets gekehrt,", "tokens": ["Drumb", "wo", "der", "Schat\u00b7ten", "ist", "zum", "Lei\u00b7be", "stets", "ge\u00b7kehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PWAV", "ART", "NN", "VAFIN", "APPRART", "NN", "ADV", "VVPP", "$,"], "meter": "++-+-+-+-+-+", "measure": "unknown.measure.septa"}, "line.2": {"text": "Wo ob der Seelen sich der Leib niemals beschwert:", "tokens": ["Wo", "ob", "der", "See\u00b7len", "sich", "der", "Leib", "nie\u00b7mals", "be\u00b7schwert", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "KOUS", "ART", "NN", "PRF", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-++--+", "measure": "iambic.hexa.chol"}, "line.3": {"text": "Und wo die Seele strebt und suchet Gott allein,", "tokens": ["Und", "wo", "die", "See\u00b7le", "strebt", "und", "su\u00b7chet", "Gott", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "VVFIN", "KON", "VVFIN", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mu\u00df Schatten, Leib und Seel in sich best\u00e4ndig seyn.", "tokens": ["Mu\u00df", "Schat\u00b7ten", ",", "Leib", "und", "Seel", "in", "sich", "be\u00b7st\u00e4n\u00b7dig", "seyn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "$,", "NN", "KON", "NN", "APPR", "PRF", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Und so steht unser Gott zur Seelen stets gewandt,", "tokens": ["Und", "so", "steht", "un\u00b7ser", "Gott", "zur", "See\u00b7len", "stets", "ge\u00b7wandt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPOSAT", "NN", "APPRART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So wircket in dem Leib die Seel ohn Wiederstand:", "tokens": ["So", "wir\u00b7cket", "in", "dem", "Leib", "die", "Seel", "ohn", "Wie\u00b7der\u00b7stand", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So wirfft von sich der Leib den Schatten auff sein Ziel,", "tokens": ["So", "wirfft", "von", "sich", "der", "Leib", "den", "Schat\u00b7ten", "auff", "sein", "Ziel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PRF", "ART", "NN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df Seele, Schatten, Leib mit Gott das beste wil.", "tokens": ["Da\u00df", "See\u00b7le", ",", "Schat\u00b7ten", ",", "Leib", "mit", "Gott", "das", "bes\u00b7te", "wil", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "NN", "$,", "NN", "APPR", "NN", "ART", "ADJA", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "O Seel ergieb dich Gott, und leide seine Macht,", "tokens": ["O", "Seel", "er\u00b7gieb", "dich", "Gott", ",", "und", "lei\u00b7de", "sei\u00b7ne", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "PPER", "NN", "$,", "KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "O Leib nihm den Befehl der Seelen stets in Acht:", "tokens": ["O", "Leib", "nihm", "den", "Be\u00b7fehl", "der", "See\u00b7len", "stets", "in", "Acht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "ART", "NN", "ART", "NN", "ADV", "APPR", "CARD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "O Schatten tritt dem Leib in solchem Lichte bey,", "tokens": ["O", "Schat\u00b7ten", "tritt", "dem", "Leib", "in", "sol\u00b7chem", "Lich\u00b7te", "bey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "ART", "NN", "APPR", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df Gottes Seel und Leib und Schatten eigen sey.", "tokens": ["Da\u00df", "Got\u00b7tes", "Seel", "und", "Leib", "und", "Schat\u00b7ten", "ei\u00b7gen", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "NN", "KON", "NN", "KON", "NN", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Weil ja die Seel aus Gott den rechten Ursprung nimmt,", "tokens": ["Weil", "ja", "die", "Seel", "aus", "Gott", "den", "rech\u00b7ten", "Ur\u00b7sprung", "nimmt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "APPR", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil in den Leib die Seel ohn alles Mittel k\u00f6mmt:", "tokens": ["Weil", "in", "den", "Leib", "die", "Seel", "ohn", "al\u00b7les", "Mit\u00b7tel", "k\u00f6mmt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "ART", "NN", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Weil aus dem Leibe f\u00e4llt der Schatten auff die Erd,", "tokens": ["Weil", "aus", "dem", "Lei\u00b7be", "f\u00e4llt", "der", "Schat\u00b7ten", "auff", "die", "Erd", ","], "token_info": ["word", "word", "word", "