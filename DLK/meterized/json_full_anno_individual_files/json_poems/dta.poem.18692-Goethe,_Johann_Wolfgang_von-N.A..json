{"dta.poem.18692": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "H\u00f6re den Rath den die Leyer t\u00f6nt;", "tokens": ["H\u00f6\u00b7re", "den", "Rath", "den", "die", "Le\u00b7yer", "t\u00f6nt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "ART", "ART", "NN", "VVFIN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Doch er nutzet nur wenn du f\u00e4hig bist;", "tokens": ["Doch", "er", "nut\u00b7zet", "nur", "wenn", "du", "f\u00e4\u00b7hig", "bist", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "KOUS", "PPER", "ADJD", "VAFIN", "$."], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Das gl\u00fccklichste Wort es wird verh\u00f6hnt", "tokens": ["Das", "gl\u00fcck\u00b7lichs\u00b7te", "Wort", "es", "wird", "ver\u00b7h\u00f6hnt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PPER", "VAFIN", "VVPP"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wenn der H\u00f6rer ein Schiefohr ist.", "tokens": ["Wenn", "der", "H\u00f6\u00b7rer", "ein", "Schie\u00b7fohr", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "VAFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "\u201ewas t\u00f6nt denn die Leyer?\u201c sie t\u00f6net laut:", "tokens": ["\u201e", "was", "t\u00f6nt", "denn", "die", "Le\u00b7yer", "?", "\u201c", "sie", "t\u00f6\u00b7net", "laut", ":"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VVFIN", "ADV", "ART", "NN", "$.", "$(", "PPER", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die sch\u00f6nste das ist nicht die beste Braut,", "tokens": ["Die", "sch\u00f6ns\u00b7te", "das", "ist", "nicht", "die", "bes\u00b7te", "Braut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PDS", "VAFIN", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch wenn wir dich unter uns z\u00e4hlen sollen,", "tokens": ["Doch", "wenn", "wir", "dich", "un\u00b7ter", "uns", "z\u00e4h\u00b7len", "sol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PRF", "APPR", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "So musst du das Sch\u00f6nste, das Beste wollen.", "tokens": ["So", "musst", "du", "das", "Sch\u00f6ns\u00b7te", ",", "das", "Bes\u00b7te", "wol\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ART", "NN", "$,", "ART", "NN", "VMFIN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}}}}