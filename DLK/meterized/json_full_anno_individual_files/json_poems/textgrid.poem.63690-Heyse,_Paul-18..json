{"textgrid.poem.63690": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "18.", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein liebes Kreuth, du schattig Waldasyl", "tokens": ["Mein", "lie\u00b7bes", "Kreuth", ",", "du", "schat\u00b7tig", "Wal\u00b7da\u00b7syl"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PPER", "ADJD", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und Wallfahrtsst\u00e4tte ruhbed\u00fcrft'ger Leute,", "tokens": ["Und", "Wall\u00b7fahrts\u00b7st\u00e4t\u00b7te", "ruh\u00b7be\u00b7d\u00fcrft'\u00b7ger", "Leu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Von deinen stillen Pfaden scheid ich heute", "tokens": ["Von", "dei\u00b7nen", "stil\u00b7len", "Pfa\u00b7den", "scheid", "ich", "heu\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und f\u00fchle tief, ich danke dir so viel.", "tokens": ["Und", "f\u00fch\u00b7le", "tief", ",", "ich", "dan\u00b7ke", "dir", "so", "viel", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "$,", "PPER", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Zwar, was ich hoffte, da\u00df man wie im Spiel", "tokens": ["Zwar", ",", "was", "ich", "hoff\u00b7te", ",", "da\u00df", "man", "wie", "im", "Spiel"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PWS", "PPER", "VVFIN", "$,", "KOUS", "PIS", "KOKOM", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Genesung hier und Jugendmut erbeute,", "tokens": ["Ge\u00b7ne\u00b7sung", "hier", "und", "Ju\u00b7gend\u00b7mut", "er\u00b7beu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "War nur ein Wahn. Denn nicht so rasch erneute", "tokens": ["War", "nur", "ein", "Wahn", ".", "Denn", "nicht", "so", "rasch", "er\u00b7neu\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ART", "NN", "$.", "KON", "PTKNEG", "ADV", "ADJD", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Kraft sich, die der Zeit zum Opfer fiel.", "tokens": ["Die", "Kraft", "sich", ",", "die", "der", "Zeit", "zum", "Op\u00b7fer", "fiel", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "$,", "PRELS", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Doch da\u00df auch einem lahmen Invaliden", "tokens": ["Doch", "da\u00df", "auch", "ei\u00b7nem", "lah\u00b7men", "In\u00b7va\u00b7li\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Verg\u00f6nnt noch sei, vergn\u00fcgt herumzuhinken,", "tokens": ["Ver\u00b7g\u00f6nnt", "noch", "sei", ",", "ver\u00b7gn\u00fcgt", "her\u00b7um\u00b7zu\u00b7hin\u00b7ken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "VAFIN", "$,", "VVPP", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das zu erleben, war mir hier beschieden.", "tokens": ["Das", "zu", "er\u00b7le\u00b7ben", ",", "war", "mir", "hier", "be\u00b7schie\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PTKZU", "VVINF", "$,", "VAFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und s\u00fc\u00df war's, mich noch einmal satt zu trinken", "tokens": ["Und", "s\u00fc\u00df", "wa\u00b7r's", ",", "mich", "noch", "ein\u00b7mal", "satt", "zu", "trin\u00b7ken"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VAFIN", "$,", "PPER", "ADV", "ADV", "ADJD", "PTKZU", "VVINF"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Am Urquell der Natur im Waldesfrieden,", "tokens": ["Am", "Ur\u00b7quell", "der", "Na\u00b7tur", "im", "Wal\u00b7des\u00b7frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Eh' wir dem Winter in die Arme sinken.", "tokens": ["Eh'", "wir", "dem", "Win\u00b7ter", "in", "die", "Ar\u00b7me", "sin\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Mein liebes Kreuth, du schattig Waldasyl", "tokens": ["Mein", "lie\u00b7bes", "Kreuth", ",", "du", "schat\u00b7tig", "Wal\u00b7da\u00b7syl"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PPER", "ADJD", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und Wallf