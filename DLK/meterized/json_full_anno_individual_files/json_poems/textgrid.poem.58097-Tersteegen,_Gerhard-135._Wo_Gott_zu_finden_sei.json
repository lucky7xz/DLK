{"textgrid.poem.58097": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "135. Wo Gott zu finden sei", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gott ist ein stiller Geist, der \u00fcberall zugegen,", "tokens": ["Gott", "ist", "ein", "stil\u00b7ler", "Geist", ",", "der", "\u00fc\u00b7be\u00b7rall", "zu\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "ADJA", "NN", "$,", "PRELS", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Drum, wer ihm nahen will, darf sich nicht viel bewegen;", "tokens": ["Drum", ",", "wer", "ihm", "na\u00b7hen", "will", ",", "darf", "sich", "nicht", "viel", "be\u00b7we\u00b7gen", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "PWS", "PPER", "ADJA", "VMFIN", "$,", "VMFIN", "PRF", "PTKNEG", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Verlier, was bildlich ist, und brauch nicht viel Gewalt,", "tokens": ["Ver\u00b7lier", ",", "was", "bild\u00b7lich", "ist", ",", "und", "brauch", "nicht", "viel", "Ge\u00b7walt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ADJD", "VAFIN", "$,", "KON", "VVFIN", "PTKNEG", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Kehr sanft in'n stillen Geist! Ich wei\u00df, du findst ihn bald.", "tokens": ["Kehr", "sanft", "in'n", "stil\u00b7len", "Geist", "!", "Ich", "wei\u00df", ",", "du", "findst", "ihn", "bald", "."], "token_info": ["word", "word", "word", "word