{"dta.poem.13200": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Z ehndes  B uch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201egemahl! mein Augenmerck! mein Ich! und meine Lust!", "tokens": ["\u201e", "ge\u00b7mahl", "!", "mein", "Au\u00b7gen\u00b7merck", "!", "mein", "Ich", "!", "und", "mei\u00b7ne", "Lust", "!"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "$.", "PPOSAT", "NN", "$.", "PPOSAT", "PPER", "$.", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "\u201edir ist, was mein Gem\u00fcth bi\u00dfher empfand, bewu\u00dft.", "tokens": ["\u201e", "dir", "ist", ",", "was", "mein", "Ge\u00b7m\u00fcth", "bi\u00df\u00b7her", "emp\u00b7fand", ",", "be\u00b7wu\u00dft", "."], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "$,", "PRELS", "PPOSAT", "NN", "ADV", "VVFIN", "$,", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "195\u201dSeit dem du mich, mein Herz und Wollen \u00fcberwunden,", "tokens": ["\"", "Seit", "dem", "du", "mich", ",", "mein", "Herz", "und", "Wol\u00b7len", "\u00fc\u00b7berw\u00b7un\u00b7den", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PRELS", "PPER", "PRF", "$,", "PPOSAT", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u201ehab ich bey dir so Trost als Hilff und Rath gefunden.", "tokens": ["\u201e", "hab", "ich", "bey", "dir", "so", "Trost", "als", "Hilff", "und", "Rath", "ge\u00b7fun\u00b7den", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "PPER", "APPR", "PPER", "ADV", "NN", "KOUS", "NN", "KON", "NN", "VVPP", "$."], "meter": "---+-+-+-+-+-", "measure": "unknown.measure.penta"}, "line.5": {"text": "\u201eo k\u00f6nnt\u2019 ich dich, wie mich, mit einer K\u00f6nigs-Kron", "tokens": ["\u201e", "o", "k\u00f6nnt'", "ich", "dich", ",", "wie", "mich", ",", "mit", "ei\u00b7ner", "K\u00f6\u00b7nigs\u00b7Kron"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "FM", "VMFIN", "PPER", "PRF", "$,", "PWAV", "PPER", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "\u201ein Majest\u00e4t geziert, und auf dergleichen Thron", "tokens": ["\u201e", "in", "Ma\u00b7jes\u00b7t\u00e4t", "ge\u00b7ziert", ",", "und", "auf", "derg\u00b7lei\u00b7chen", "Thron"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "APPR", "NN", "VVPP", "$,", "KON", "APPR", "PIS", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "\u201ein Sieg, in Ruh und Heil langwierig herrschen sehen!", "tokens": ["\u201e", "in", "Sieg", ",", "in", "Ruh", "und", "Heil", "lang\u00b7wie\u00b7rig", "herr\u00b7schen", "se\u00b7hen", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "NN", "$,", "APPR", "NN", "KON", "NN", "ADJD", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "200\u201dNichts k\u00f6nnte meinem Geist gl\u00fcckseeligers geschehen.", "tokens": ["\"", "Nichts", "k\u00f6nn\u00b7te", "mei\u00b7nem", "Geist", "gl\u00fcck\u00b7see\u00b7li\u00b7gers", "ge\u00b7sche\u00b7hen", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PIS", "VMFIN", "PPOSAT", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "\u201eso viel in meiner Macht dir beyzulegen war,", "tokens": ["\u201e", "so", "viel", "in", "mei\u00b7ner", "Macht", "dir", "bey\u00b7zu\u00b7le\u00b7gen", "war", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADV", "APPR", "PPOSAT", "NN", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "\u201eso vieles bot\u2019 ich dir zum Liebes-Zeichen dar.", "tokens": ["\u201e", "so", "vie\u00b7les", "bot'", "ich", "dir", "zum", "Lie\u00b7bes\u00b7Zei\u00b7chen", "dar", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "PIS", "VVFIN", "PPER", "PPER", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "\u201eich habe nichts, was ich verm\u00f6gend war, vers\u00e4umet,", "tokens": ["\u201e", "ich", "ha\u00b7be", "nichts", ",", "was", "ich", "ver\u00b7m\u00f6\u00b7gend", "war", ",", "ver\u00b7s\u00e4u\u00b7met", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "PIS", "$,", "PWS", "PPER", "VVPP", "VAFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "\u201edir gleich den halben Theil des Thrones einger\u00e4umet;", "tokens": ["\u201e", "dir", "gleich", "den", "hal\u00b7ben", "Theil", "des", "Thro\u00b7nes", "ein\u00b7ge\u00b7r\u00e4u\u00b7met", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "ADV", "ART", "ADJA", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "205\u201dDa du mir beyzustehn, des Zepters gr\u00f6\u00dfte Last", "tokens": ["\"", "Da", "du", "mir", "bey\u00b7zu\u00b7stehn", ",", "des", "Zep\u00b7ters", "gr\u00f6\u00df\u00b7te", "Last"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "KOUS", "PPER", "PPER", "VVINF", "$,", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "\u201ein steter Wachsamkeit mit mir getragen hast.", "tokens": ["\u201e", "in", "ste\u00b7ter", "Wach\u00b7sam\u00b7keit", "mit", "mir", "ge\u00b7tra\u00b7gen", "hast", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "ADJA", "NN", "APPR", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "\u201enimm auch von mir des Dancks verpflichtetes Erkennen:", "tokens": ["\u201e", "nimm", "auch", "von", "mir", "des", "Dancks", "ver\u00b7pflich\u00b7te\u00b7tes", "Er\u00b7ken\u00b7nen", ":"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "ADV", "APPR", "PPER", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "\u201eich will auf ewig mich dir neu verbunden nennen.", "tokens": ["\u201e", "ich", "will", "auf", "e\u00b7wig", "mich", "dir", "neu", "ver\u00b7bun\u00b7den", "nen\u00b7nen", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VMFIN", "APPR", "ADJD", "PPER", "PPER", "ADJD", "VVPP", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}