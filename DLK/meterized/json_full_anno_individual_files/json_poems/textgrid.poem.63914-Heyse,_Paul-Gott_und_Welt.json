{"textgrid.poem.63914": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "Gott und Welt", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Bilderbuch ist diese Welt,", "tokens": ["Ein", "Bil\u00b7der\u00b7buch", "ist", "die\u00b7se", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das manchem herzlich wohlgef\u00e4llt,", "tokens": ["Das", "man\u00b7chem", "herz\u00b7lich", "wohl\u00b7ge\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der bl\u00e4tternd Bild um Bild genie\u00dft,", "tokens": ["Der", "bl\u00e4t\u00b7ternd", "Bild", "um", "Bild", "ge\u00b7nie\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vom Text nicht eine Zeile liest.", "tokens": ["Vom", "Text", "nicht", "ei\u00b7ne", "Zei\u00b7le", "liest", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKNEG", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die goldne Mittelm\u00e4\u00dfigkeit", "tokens": ["Die", "gold\u00b7ne", "Mit\u00b7tel\u00b7m\u00e4\u00b7\u00dfig\u00b7keit"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mu\u00df wohl Unmittelbares hassen.", "tokens": ["Mu\u00df", "wohl", "Un\u00b7mit\u00b7tel\u00b7ba\u00b7res", "has\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Drum hat sie sich zu aller Zeit", "tokens": ["Drum", "hat", "sie", "sich", "zu", "al\u00b7ler", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VAFIN", "PPER", "PRF", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Natur, Geist, Gottes Herrlichkeit", "tokens": ["Na\u00b7tur", ",", "Geist", ",", "Got\u00b7tes", "Herr\u00b7lich\u00b7keit"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "NN"], "meter": "--++-+-+", "measure": "anapaest.init"}, "line.5": {"text": "Anthropomorphisch lang und breit", "tokens": ["An\u00b7th\u00b7ro\u00b7po\u00b7mor\u00b7phisch", "lang", "und", "breit"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Zum Schulgebrauch \u00fcbersetzen lassen.", "tokens": ["Zum", "Schul\u00b7ge\u00b7brauch", "\u00fc\u00b7bers\u00b7et\u00b7zen", "las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "VVINF", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "War's auch human, im Wurmgeschlecht", "tokens": ["Wa\u00b7r's", "auch", "hu\u00b7man", ",", "im", "Wurm\u00b7ge\u00b7schlecht"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "ADV", "NE", "$,", "APPRART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Den Gottesfunken anzufachen?", "tokens": ["Den", "Got\u00b7tes\u00b7fun\u00b7ken", "an\u00b7zu\u00b7fa\u00b7chen", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Mu\u00dft' er den gottbewu\u00dften Knecht", "tokens": ["Mu\u00dft'", "er", "den", "gott\u00b7be\u00b7wu\u00df\u00b7ten", "Knecht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nicht vollends erst zum armen Teufel machen?", "tokens": ["Nicht", "vol\u00b7lends", "erst", "zum", "ar\u00b7men", "Teu\u00b7fel", "ma\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ADV", "APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "R\u00e4tsel, die zu l\u00f6sen endlich,", "tokens": ["R\u00e4t\u00b7sel", ",", "die", "zu", "l\u00f6\u00b7sen", "end\u00b7lich", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PTKZU", "VVINF", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Werden sie \u00bbnat\u00fcrlich\u00ab schelten.", "tokens": ["Wer\u00b7den", "sie", "\u00bb", "na\u00b7t\u00fcr\u00b7lich", "\u00ab", "schel\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VAFIN", "PPER", "$(", "ADV", "$(", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Nur was ewig unverst\u00e4ndlich,", "tokens": ["Nur", "was", "e\u00b7wig", "un\u00b7ver\u00b7st\u00e4nd\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "ADJD", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wird als Offenbarung gelten.", "tokens": ["Wird", "als", "Of\u00b7fen\u00b7ba\u00b7rung", "gel\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KOKOM", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Im Buch der B\u00fccher offenbar", "tokens": ["Im", "Buch", "der", "B\u00fc\u00b7cher", "of\u00b7fen\u00b7bar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Steht Gottes Wort. Doch sagt, ihr Frommen,", "tokens": ["Steht", "Got\u00b7tes", "Wort", ".", "Doch", "sagt", ",", "ihr", "From\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "NN", "NN", "$.", "KON", "VVFIN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ist Gott durch so viel tausend Jahr", "tokens": ["Ist", "Gott", "durch", "so", "viel", "tau\u00b7send", "Jahr"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "NN", "APPR", "ADV", "ADV", "CARD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sonst nie zu Wort gekommen?", "tokens": ["Sonst", "nie", "zu", "Wort", "ge\u00b7kom\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Wie gegen die Kirche wir auch uns wehren,", "tokens": ["Wie", "ge\u00b7gen", "die", "Kir\u00b7che", "wir", "auch", "uns", "weh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "PPER", "ADV", "PPER", "VVINF", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Der Andacht k\u00f6nnen wir nicht entbehren.", "tokens": ["Der", "An\u00b7dacht", "k\u00f6n\u00b7nen", "wir", "nicht", "ent\u00b7beh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "Gern auf den Knieen verehrt' ich ihn,", "tokens": ["Gern", "auf", "den", "Kni\u00b7e\u00b7en", "ver\u00b7ehrt'", "ich", "ihn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "PPER", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Lie\u00df' er im feurigen Busch sich sp\u00fcren.", "tokens": ["Lie\u00df'", "er", "im", "feu\u00b7ri\u00b7gen", "Busch", "sich", "sp\u00fc\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "ADJA", "NN", "PRF", "VVINF", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "Doch mag ich nicht so obenhin", "tokens": ["Doch", "mag", "ich", "nicht", "so", "o\u00b7ben\u00b7hin"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "PTKNEG", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Seinen Namen unn\u00fctzlich f\u00fchren.", "tokens": ["Sei\u00b7nen", "Na\u00b7men", "un\u00b7n\u00fctz\u00b7lich", "f\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "VVINF", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.8": {"line.1": {"text": "Es ist ein Trost in mancher Not,", "tokens": ["Es", "ist", "ein", "Trost", "in", "man\u00b7cher", "Not", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu denken, das lumpige Leben", "tokens": ["Zu", "den\u00b7ken", ",", "das", "lum\u00b7pi\u00b7ge", "Le\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "$,", "ART", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Sei ein Kontrakt mit dem lieben Gott,", "tokens": ["Sei", "ein", "Kon\u00b7trakt", "mit", "dem", "lie\u00b7ben", "Gott", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Einseitig aufzuheben.", "tokens": ["Ein\u00b7sei\u00b7tig", "auf\u00b7zu\u00b7he\u00b7ben", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVIZU", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "G\u00f6nnt doch den Wahn dem armen Schlucker,", "tokens": ["G\u00f6nnt", "doch", "den", "Wahn", "dem", "ar\u00b7men", "Schlu\u00b7cker", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der nur des Lebens Bitterkeit genie\u00dft!", "tokens": ["Der", "nur", "des", "Le\u00b7bens", "Bit\u00b7ter\u00b7keit", "ge\u00b7nie\u00dft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Unsterblichkeit ist ja der Zucker,", "tokens": ["U\u00b7nsterb\u00b7lich\u00b7keit", "ist", "ja", "der", "Zu\u00b7cker", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "++-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der ihm den herben Trank der Zeit vers\u00fc\u00dft.", "tokens": ["Der", "ihm", "den", "her\u00b7ben", "Trank", "der", "Zeit", "ver\u00b7s\u00fc\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "ADJA", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Verg\u00fcten reichen Alters Garben", "tokens": ["Ver\u00b7g\u00fc\u00b7ten", "rei\u00b7chen", "Al\u00b7ters", "Gar\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mi\u00dfwachs der Jugendzeit und langes Darben,", "tokens": ["Mi\u00df\u00b7wachs", "der", "Ju\u00b7gend\u00b7zeit", "und", "lan\u00b7ges", "Dar\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und sollt's Ersatz im Himmel geben", "tokens": ["Und", "sollt's", "Er\u00b7satz", "im", "Him\u00b7mel", "ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "APPRART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fcr ein verpfuschtes Erdenleben?", "tokens": ["F\u00fcr", "ein", "ver\u00b7pfuschtes", "Er\u00b7den\u00b7le\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.11": {"line.1": {"text": "Ein Bilderbuch ist diese Welt,", "tokens": ["Ein", "Bil\u00b7der\u00b7buch", "ist", "die\u00b7se", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das manchem herzlich wohlgef\u00e4llt,", "tokens": ["Das", "man\u00b7chem", "herz\u00b7lich", "wohl\u00b7ge\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der bl\u00e4tternd Bild u