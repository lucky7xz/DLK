{"textgrid.poem.61297": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Zweifel ist ein Falk mit scharfen Klauen;", "tokens": ["Der", "Zwei\u00b7fel", "ist", "ein", "Falk", "mit", "schar\u00b7fen", "Klau\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des Glaubens wei\u00dfe Taube sieht er kaum,", "tokens": ["Des", "Glau\u00b7bens", "wei\u00b7\u00dfe", "Tau\u00b7be", "sieht", "er", "kaum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So beizt er nieder durch den luft'gen Raum,", "tokens": ["So", "beizt", "er", "nie\u00b7der", "durch", "den", "luft'\u00b7gen", "Raum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die Krallen in ihr zitternd Fleisch zu hauen.", "tokens": ["Die", "Kral\u00b7len", "in", "ihr", "zit\u00b7ternd", "Fleisch", "zu", "hau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "ADJD", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Da flockt zerrupft hernieder aus dem Blauen", "tokens": ["Da", "flockt", "zer\u00b7rupft", "her\u00b7nie\u00b7der", "aus", "dem", "Blau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "VVFIN", "PTKVZ", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das schimmernde Gefieder Flaum f\u00fcr Flaum,", "tokens": ["Das", "schim\u00b7mern\u00b7de", "Ge\u00b7fie\u00b7der", "Flaum", "f\u00fcr", "Flaum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mit jeder Feder f\u00e4llt ein Gottestraum,", "tokens": ["Mit", "je\u00b7der", "Fe\u00b7der", "f\u00e4llt", "ein", "Got\u00b7tes\u00b7traum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und langsam blutet hin das Gottvertrauen.", "tokens": ["Und", "lang\u00b7sam", "blu\u00b7tet", "hin", "das", "Gott\u00b7ver\u00b7trau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ein Engel sieht herab vom Himmelszelt", "tokens": ["Ein", "En\u00b7gel", "sieht", "her\u00b7ab", "vom", "Him\u00b7mels\u00b7zelt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und wendet tr\u00fcb mit fragenden Geb\u00e4rden", "tokens": ["Und", "wen\u00b7det", "tr\u00fcb", "mit", "fra\u00b7gen\u00b7den", "Ge\u00b7b\u00e4r\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das Angesicht empor zum Herrn der Welt.", "tokens": ["Das", "An\u00