 "line.14": {"text": "Holt' er unerm\u00fcdet aus der See,", "tokens": ["Holt'", "er", "un\u00b7er\u00b7m\u00fc\u00b7det", "aus", "der", "See", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.15": {"text": "Trug zum Feuer Reiser Holz zusammen,", "tokens": ["Trug", "zum", "Feu\u00b7er", "Rei\u00b7ser", "Holz", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.16": {"text": "Fing mir alle Tag' ein junges Reh;", "tokens": ["Fing", "mir", "al\u00b7le", "Tag'", "ein", "jun\u00b7ges", "Reh", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.17": {"text": "V\u00f6gel, die ihn noch nicht kannten,", "tokens": ["V\u00f6\u00b7gel", ",", "die", "ihn", "noch", "nicht", "kann\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "ADV", "PTKNEG", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Hascht' er listig in dem hohen Klee.", "tokens": ["Hascht'", "er", "lis\u00b7tig", "in", "dem", "ho\u00b7hen", "Klee", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.19": {"text": "Dann so lagen wir bei unserm Feuer,", "tokens": ["Dann", "so", "la\u00b7gen", "wir", "bei", "un\u00b7serm", "Feu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.20": {"text": "A\u00dfen mit einander; s\u00fc\u00dfer Ruh',", "tokens": ["A\u00b7\u00dfen", "mit", "ein\u00b7an\u00b7der", ";", "s\u00fc\u00b7\u00dfer", "Ruh'", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPR", "PRF", "$.", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.21": {"text": "Caro, guter Caro! mein Getreuer!", "tokens": ["Ca\u00b7ro", ",", "gu\u00b7ter", "Ca\u00b7ro", "!", "mein", "Ge\u00b7treu\u00b7er", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NE", "$.", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.22": {"text": "Pflegten wir zusammen, ich und du.", "tokens": ["Pfleg\u00b7ten", "wir", "zu\u00b7sam\u00b7men", ",", "ich", "und", "du", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PTKVZ", "$,", "PPER", "KON", "PPER", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.23": {"text": "Aber, welcher Kokus-Wipfel", "tokens": ["A\u00b7ber", ",", "wel\u00b7cher", "Ko\u00b7kus\u00b7Wip\u00b7fel"], "token_info": ["word", "punct", "word", "word"], "pos": ["KON", "$,", "PWAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "S\u00e4uselt nun mir wieder Schlummer zu?", "tokens": ["S\u00e4u\u00b7selt", "nun", "mir", "wie\u00b7der", "Schlum\u00b7mer", "zu", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPER", "ADV", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.25": {"text": "Wachen werd' ich, mich zu Tode wachen,", "tokens": ["Wa\u00b7chen", "werd'", "ich", ",", "mich", "zu", "To\u00b7de", "wa\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "$,", "PRF", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.26": {"text": "Und das will ich, bester Caro, gern,", "tokens": ["Und", "das", "will", "ich", ",", "bes\u00b7ter", "Ca\u00b7ro", ",", "gern", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PDS", "VMFIN", "PPER", "$,", "ADJA", "NE", "$,", "ADV", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.27": {"text": "Wenn nicht fr\u00fcher eines Tigers Rachen", "tokens": ["Wenn", "nicht", "fr\u00fc\u00b7her", "ei\u00b7nes", "Ti\u00b7gers", "Ra\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PTKNEG", "ADJD", "ART", "NN", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.28": {"text": "Schon erw\u00fcrget deinen alten Herrn;", "tokens": ["Schon", "er\u00b7w\u00fcr\u00b7get", "dei\u00b7nen", "al\u00b7ten", "Herrn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.29": {"text": "O! er sollt' ihn nicht erw\u00fcrgen,", "tokens": ["O", "!", "er", "sollt'", "ihn", "nicht", "er\u00b7w\u00fcr\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.30": {"text": "W\u00e4rest du, mein Tapfrer! nur nicht fern.", "tokens": ["W\u00e4\u00b7rest", "du", ",", "mein", "Tapf\u00b7rer", "!", "nur", "nicht", "fern", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "PPOSAT", "NN", "$.", "ADV", "PTKNEG", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.31": {"text": "Aber trennen sollen wir uns, trennen?", "tokens": ["A\u00b7ber", "tren\u00b7nen", "sol\u00b7len", "wir", "uns", ",", "tren\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVINF", "VMFIN", "PPER", "PPER", "$,", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.32": {"text": "Niemals wieder in der bessern Welt,", "tokens": ["Nie\u00b7mals", "wie\u00b7der", "in", "der", "bes\u00b7sern", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.33": {"text": "Als zwei Wesen bessrer Art, uns kennen?", "tokens": ["Als", "zwei", "We\u00b7sen", "bess\u00b7rer", "Art", ",", "uns", "ken\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "CARD", "NN", "ADJA", "NN", "$,", "PPER", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.34": {"text": "War dein Geist nur Stoff, der auch zerf\u00e4llt?", "tokens": ["War", "dein", "Geist", "nur", "Stoff", ",", "der", "auch", "zer\u00b7f\u00e4llt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADV", "NN", "$,", "PRELS", "ADV", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.35": {"text": "Nein! auch er geht nicht verloren,", "tokens": ["Nein", "!", "auch", "er", "geht", "nicht", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "ADV", "PPER", "VVFIN", "PTKNEG", "VVPP", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.36": {"text": "Wenn die Treue dort erst Lohn erh\u00e4lt.", "tokens": ["Wenn", "die", "Treu\u00b7e", "dort", "erst", "Lohn", "er\u00b7h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word",