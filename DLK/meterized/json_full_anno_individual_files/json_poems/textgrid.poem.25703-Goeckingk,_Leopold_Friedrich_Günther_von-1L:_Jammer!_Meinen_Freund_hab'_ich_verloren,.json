{"textgrid.poem.25703": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Jammer! Meinen Freund hab' ich verloren,", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Jammer! Meinen Freund hab' ich verloren,", "tokens": ["Jam\u00b7mer", "!", "Mei\u00b7nen", "Freund", "hab'", "ich", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPOSAT", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Meinen einzigen auf dieser Welt!", "tokens": ["Mei\u00b7nen", "ein\u00b7zi\u00b7gen", "auf", "die\u00b7ser", "Welt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "APPR", "PDAT", "NN", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Ha! da liegt er mit gesenkten Ohren,", "tokens": ["Ha", "!", "da", "liegt", "er", "mit", "ge\u00b7senk\u00b7ten", "Oh\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Der mir oft noch Muth ins Herz gebellt,", "tokens": ["Der", "mir", "oft", "noch", "Muth", "ins", "Herz", "ge\u00b7bellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADV", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Und mir Trost hat zugewedelt! \u2013", "tokens": ["Und", "mir", "Trost", "hat", "zu\u00b7ge\u00b7we\u00b7delt", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPER", "NN", "VAFIN", "VVPP", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ha! da liegt \u2013 mein letztes in der Welt!", "tokens": ["Ha", "!", "da", "liegt", "\u2013", "mein", "letz\u00b7tes", "in", "der", "Welt", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ADV", "VVFIN", "$(", "PPOSAT", "ADJA", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Damals, als auf einer kleinen Tr\u00fcmmer", "tokens": ["Da\u00b7mals", ",", "als", "auf", "ei\u00b7ner", "klei\u00b7nen", "Tr\u00fcm\u00b7mer"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Mich die Fluth an diese W\u00fcste trieb,", "tokens": ["Mich", "die", "Fluth", "an", "die\u00b7se", "W\u00fcs\u00b7te", "trieb", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "APPR", "PDAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.9": {"text": "Sprang er aus dem Wracke, schwamm mir immer", "tokens": ["Sprang", "er", "aus", "dem", "Wra\u00b7cke", ",", "schwamm", "mir", "im\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$,", "VVFIN", "PPER", "ADV"], "meter": "+-+-+-+-++", "measure": "unknown.measure.hexa"}, "line.10": {"text": "Hinten nach, und sah nur, wo ich blieb;", "tokens": ["Hin\u00b7ten", "nach", ",", "und", "sah", "nur", ",", "wo", "ich", "blieb", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$,", "KON", "VVFIN", "ADV", "$,", "PWAV", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.11": {"text": "Leckte mich am Ufer trocken! \u2013", "tokens": ["Leck\u00b7te", "mich", "am", "U\u00b7fer", "tro\u00b7cken", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "ADJD", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Welcher Mensch hat seinen Freund so lieb?", "tokens": ["Wel\u00b7cher", "Mensch", "hat", "sei\u00b7nen", "Freund", "so", "lieb", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "VAFIN", "PPOSAT", "NN", "ADV", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.13": {"text": "H\u00fct' und Schuhe, die am Ufer schwammen,", "tokens": ["H\u00fct'", "und", "Schu\u00b7he", ",", "die", "am", "U\u00b7fer", "schwam\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.14": {"text": "Holt' er unerm\u00fcdet aus der See,", "tokens": ["Holt'", "er", "un\u00b7er\u00b7m\u00fc\u00b7det", "aus", "der", "See", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.15": {"text": "Trug zum Feuer Reiser Holz zusammen,", "tokens": ["Trug", "zum", "Feu\u00b7er", "Rei\u00b7ser", "Holz", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.16": {"text": "Fing mir alle Tag' ein junges Reh;", "tokens": ["Fing", "mir", "al\u00b7le", "Tag'", "ein", "jun\u00b7ges", "Reh", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.17": {"text": "V\u00f6gel, die ihn noch nicht kannten,", "tokens": ["V\u00f6\u00b7gel", ",", "die", "ihn", "noch", "nicht", "kann\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "ADV", "PTKNEG", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Hascht' er listig in dem hohen Klee.", "tokens": ["Hascht'", "er", "lis\u00b7tig", "in", "dem", "ho\u00b7hen", "Klee", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.19": {"text": "Dann so lagen wir bei unserm Feuer,", "tokens": ["Dann", "so", "la\u00b7gen", "wir", "bei", "un\u00b7serm", "Feu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.20": {"text": "A\u00dfen mit einander; s\u00fc\u00dfer Ruh',", "tokens": ["A\u00b7\u00dfen", "mit", "ein\u00b7an\u00b7der", ";", "s\u00fc\u00b7\u00dfer", "Ruh'", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPR", "PRF", "$.", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.21": {"text": "Caro, guter Caro! mein Getreuer!", "tokens": ["Ca\u00b7ro", ",", "gu\u00b7ter", "Ca\u00b7ro", "!", "mein", "Ge\u00b7treu\u00b7er", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NE", "$.", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.22": {"text": "Pflegten wir zusammen, ich und du.", "tokens": ["Pfleg\u00b7ten", "wir", "zu\u00b7sam\u00b7men", ",", "ich", "und", "du", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PTKVZ", "$,", "PPER", "KON", "PPER", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.23": {"text": "Aber, welcher Kokus-Wipfel", "tokens": ["A\u00b7ber", ",", "wel\u00b7cher", "Ko\u00b7kus\u00b7Wip\u00b7fel"], "token_info": ["word", "punct", "word", "word"], "pos": ["KON", "$,", "PWAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "S\u00e4uselt nun mir wieder Schlummer zu?", "tokens": ["S\u00e4u\u00b7selt", "nun", "mir", "wie\u00b7der", "Schlum\u00b7mer", "zu", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPER", "ADV", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.25": {"text": "Wachen werd' ich, mich zu Tode wachen,", "tokens": ["Wa\u00b7chen", "werd'", "ich", ",", "mich", "zu", "To\u00b7de", "wa\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "$,", "PRF", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.26": {"text": "Und das will ich, bester Caro, gern,", "tokens": ["Und", "das", "will", "ich", ",", "bes\u00b7ter", "Ca\u00b7ro", ",", "gern", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PDS", "VMFIN", "PPER", "$,", "ADJA", "NE", "$,", "ADV", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.27": {"text": "Wenn nicht fr\u00fcher eines Tigers Rachen", "tokens": ["Wenn", "nicht", "fr\u00fc\u00b7her", "ei\u00b7nes", "Ti\u00b7gers", "Ra\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PTKNEG", "ADJD", "ART", "NN", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.28": {"text": "Schon erw\u00fcrget deinen alten Herrn;", "tokens": ["Schon", "er\u00b7w\u00fcr\u00b7get", "dei\u00b7nen", "al\u00b7ten", "Herrn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.29": {"text": "O! er sollt' ihn nicht erw\u00fcrgen,", "tokens": ["O", "!", "er", "sollt'", "ihn", "nicht", "er\u00b7w\u00fcr\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.30": {"text": "W\u00e4rest du, mein Tapfrer! nur nicht fern.", "tokens": ["W\u00e4\u00b7rest", "du", ",", "mein", "Tapf\u00b7rer", "!", "nur", "nicht", "fern", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "PPOSAT", "NN", "$.", "ADV", "PTKNEG", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.31": {"text": "Aber trennen sollen wir uns, trennen?", "tokens": ["A\u00b7ber", "tren\u00b7nen", "sol\u00b7len", "wir", "uns", ",", "tren\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVINF", "VMFIN", "PPER", "PPER", "$,", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.32": {"text": "Niemals wieder in der bessern Welt,", "tokens": ["Nie\u00b7mals", "wie\u00b7der", "in", "der", "bes\u00b7sern", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.33": {"text": "Als zwei Wesen bessrer Art, uns kennen?", "tokens": ["Als", "zwei", "We\u00b7sen", "bess\u00b7rer", "Art", ",", "uns", "ken\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "CARD", "NN", "ADJA", "NN", "$,", "PPER", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.34": {"text": "War dein Geist nur Stoff, der auch zerf\u00e4llt?", "tokens": ["War", "dein", "Geist", "nur", "Stoff", ",", "der", "auch", "zer\u00b7f\u00e4llt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADV", "NN", "$,", "PRELS", "ADV", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.35": {"text": "Nein! auch er geht nicht verloren,", "tokens": ["Nein", "!", "auch", "er", "geht", "nicht", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "ADV", "PPER", "VVFIN", "PTKNEG", "VVPP", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.36": {"text": "Wenn die Treue dort erst Lohn erh\u00e4lt.", "tokens": ["Wenn", "die", "Treu\u00b7e", "dort", "erst", "Lohn", "er\u00b7h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "ADV", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Jammer! Meinen Freund hab' ich verloren,", "tokens": ["Jam\u00b7mer", "!", "Mei\u00b7nen", "Freund", "hab'", "ich", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPOSAT", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Meinen einzigen auf dieser Welt!", "tokens": ["Mei\u00b7nen", "ein\u00b7zi\u00b7gen", "auf", "die\u00b7ser", "Welt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "APPR", "PDAT", "NN", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Ha! da liegt er mit gesenkten Ohren,", "token