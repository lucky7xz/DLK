{"dta.poem.13147": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Neuntes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201eder Tempel stehet schon, den ich f\u00fcr euch erw\u00e4hle;", "tokens": ["\u201e", "der", "Tem\u00b7pel", "ste\u00b7het", "schon", ",", "den", "ich", "f\u00fcr", "euch", "er\u00b7w\u00e4h\u00b7le", ";"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "ADV", "$,", "PRELS", "PPER", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "220\u201dJedoch erlaubt, da\u00df ich noch in dem Sinn verh\u00f6hle,", "tokens": ["\"", "Je\u00b7doch", "er\u00b7laubt", ",", "da\u00df", "ich", "noch", "in", "dem", "Sinn", "ver\u00b7h\u00f6h\u00b7le", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVPP", "$,", "KOUS", "PPER", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "\u201emit was vor einer Schrift das Friese prangen soll,", "tokens": ["\u201e", "mit", "was", "vor", "ei\u00b7ner", "Schrift", "das", "Frie\u00b7se", "pran\u00b7gen", "soll", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PRELS", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u201eund wem ich das Geb\u00e4u zum Denckmahl weihen woll.", "tokens": ["\u201e", "und", "wem", "ich", "das", "Ge\u00b7b\u00e4u", "zum", "Denck\u00b7mahl", "wei\u00b7hen", "woll", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "PWS", "PPER", "ART", "NN", "APPRART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "\u201eich werde gleich dem Saal den Ri\u00df vor Augen legen,", "tokens": ["\u201e", "ich", "wer\u00b7de", "gleich", "dem", "Saal", "den", "Ri\u00df", "vor", "Au\u00b7gen", "le\u00b7gen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "ADV", "ART", "NN", "ART", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "\u201edamit ihr dessen Pracht und Gr\u00f6sse k\u00f6nnt erwegen.", "tokens": ["\u201e", "da\u00b7mit", "ihr", "des\u00b7sen", "Pracht", "und", "Gr\u00f6s\u00b7se", "k\u00f6nnt", "er\u00b7we\u00b7gen", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "PDS", "NN", "KON", "NN", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}