{"dta.poem.6380": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Hitze und von der K\u00e4lte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Auch, sonder Eis und Frost, so uns der Nord-Pol schicket,", "tokens": ["Auch", ",", "son\u00b7der", "Eis", "und", "Frost", ",", "so", "uns", "der", "Nord\u00b7Pol", "schi\u00b7cket", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KON", "NN", "KON", "NN", "$,", "ADV", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und, ohn\u2019 des Winters Grimm, Schnee, Schlossen, Sturm", "tokens": ["Und", ",", "ohn'", "des", "Win\u00b7ters", "Grimm", ",", "Schnee", ",", "Schlos\u00b7sen", ",", "Sturm"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["KON", "$,", "KOUI", "ART", "NN", "NE", "$,", "NN", "$,", "NN", "$,", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "und Krachen,", "tokens": ["und", "Kra\u00b7chen", ","], "token_info": ["word", "word", "punct"], "pos": ["KON", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Selbst, wenn man reiffes Korn auf unserm Feld erblickt,", "tokens": ["Selbst", ",", "wenn", "man", "reif\u00b7fes", "Korn", "auf", "un\u00b7serm", "Feld", "er\u00b7blickt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PIS", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Besitzt man itzt die Kunst, aus Wasser Eis zu machen.", "tokens": ["Be\u00b7sitzt", "man", "itzt", "die", "Kunst", ",", "aus", "Was\u00b7ser", "Eis", "zu", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "ART", "NN", "$,", "APPR", "NN", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die Kunst, so gantz gemein, ist diese: Man erf\u00fcllt", "tokens": ["Die", "Kunst", ",", "so", "gantz", "ge\u00b7mein", ",", "ist", "die\u00b7se", ":", "Man", "er\u00b7f\u00fcllt"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "ADV", "ADV", "ADJD", "$,", "VAFIN", "PDAT", "$.", "PIS", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Mit Wasser ein Gef\u00e4\u00df, umgiebet und verh\u00fcllt", "tokens": ["Mit", "Was\u00b7ser", "ein", "Ge\u00b7f\u00e4\u00df", ",", "um\u00b7gie\u00b7bet", "und", "ver\u00b7h\u00fcllt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "ART", "NN", "$,", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Mit gleich viel Eis und Saltz dasselbe Glas. So dann", "tokens": ["Mit", "gleich", "viel", "Eis", "und", "Saltz", "das\u00b7sel\u00b7be", "Glas", ".", "So", "dann"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ADV", "PIAT", "NN", "KON", "NN", "PDAT", "NN", "$.", "ADV", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Sieht unser Auge fast nicht ohn\u2019 Erstaunen an,", "tokens": ["Sieht", "un\u00b7ser", "Au\u00b7ge", "fast", "nicht", "ohn'", "Er\u00b7stau\u00b7nen", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADV", "PTKNEG", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Wie sich im Augenblick das Saltz verliert,", "tokens": ["Wie", "sich", "im", "Au\u00b7gen\u00b7blick", "das", "Saltz", "ver\u00b7liert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Das Eis zergeht, die Fluth im Glase friert.", "tokens": ["Das", "Eis", "zer\u00b7geht", ",", "die", "Fluth", "im", "Gla\u00b7se", "friert", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}