{"textgrid.poem.46333": {"metadata": {"author": {"name": "Spee, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[o Gott, will dich nun preisen]", "genre": "verse", "period": "N.A.", "pub_year": 1613, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Gott, will dich nun preisen,", "tokens": ["O", "Gott", ",", "will", "dich", "nun", "prei\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VMFIN", "PRF", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Preisen ohn vnterla\u00df.", "tokens": ["Prei\u00b7sen", "ohn", "vn\u00b7ter\u00b7la\u00df", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Ehr will ich dich beweisen,", "tokens": ["Ehr", "will", "ich", "dich", "be\u00b7wei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df hell vnd Teuffel ra\u00df.", "tokens": ["Da\u00df", "hell", "vnd", "Teuf\u00b7fel", "ra\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wan dich nur recht mag loben,", "tokens": ["Wan", "dich", "nur", "recht", "mag", "lo\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADJD", "VMFIN", "VVINF", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.6": {"text": "Frag weiters nirgend nach,", "tokens": ["Frag", "wei\u00b7ters", "nir\u00b7gend", "nach", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wan schon all welt solt toben,", "tokens": ["Wan", "schon", "all", "welt", "solt", "to\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PIAT", "NN", "VMFIN", "VVINF", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.8": {"text": "Wolt ich fortfahren doch.", "tokens": ["Wolt", "ich", "fort\u00b7fah\u00b7ren", "doch", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Dich meinen Gott vnd Herren", "tokens": ["Dich", "mei\u00b7nen", "Gott", "vnd", "Her\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "PPOSAT", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Will ich hertzwillig sein", "tokens": ["Will", "ich", "hert\u00b7zwil\u00b7lig", "sein"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADJD", "VAINF"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit lobgesang zu ehren,", "tokens": ["Mit", "lob\u00b7ge\u00b7sang", "zu", "eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bi\u00df in das grabe mein:", "tokens": ["Bi\u00df", "in", "das", "gra\u00b7be", "mein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "PDS", "VVFIN", "PPOSAT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ja wan schon ich geh schlaffen,", "tokens": ["Ja", "wan", "schon", "ich", "geh", "schlaf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PWAV", "ADV", "PPER", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Schlaffen wol in da\u00df grab,", "tokens": ["Schlaf\u00b7fen", "wol", "in", "da\u00df", "grab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "KOUS", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Will dir doch lob verschaffen;", "tokens": ["Will", "dir", "doch", "lob", "ver\u00b7schaf\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Soll drum nit nemen ab.", "tokens": ["Soll", "drum", "nit", "ne\u00b7men", "ab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PAV", "PTKNEG", "ADV", "PTKVZ", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Nach mir will ich verla\u00dfen", "tokens": ["Nach", "mir", "will", "ich", "ver\u00b7la\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VMFIN", "PPER", "VVINF"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "In meinem Testament,", "tokens": ["In", "mei\u00b7nem", "Tes\u00b7ta\u00b7ment", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein liedlein sch\u00f6n ohn massen", "tokens": ["Ein", "lied\u00b7lein", "sch\u00f6n", "ohn", "mas\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zum Gottes lob verwendt.", "tokens": ["Zum", "Got\u00b7tes", "lob", "ver\u00b7wendt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da\u00df wird noch wol erklingen.", "tokens": ["Da\u00df", "wird", "noch", "wol", "er\u00b7klin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ob ich schon storben bin:", "tokens": ["Ob", "ich", "schon", "stor\u00b7ben", "bin", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVINF", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Es werdens andre singen,", "tokens": ["Es", "wer\u00b7dens", "and\u00b7re", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wan ich schon bin dahin.", "tokens": ["Wan", "ich", "schon", "bin", "da\u00b7hin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VAFIN", "PAV", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.4": {"line.1": {"text": "Dan auch mein geist wird springen,", "tokens": ["Dan", "auch", "mein", "geist", "wird", "sprin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPOSAT", "NN", "VAFIN", "VVINF", "$,"], "meter": "---+-+-", "measure": "unknown.measure.di"}, "line.2": {"text": "Hoch in des Himmels Saal,", "tokens": ["Hoch", "in", "des", "Him\u00b7mels", "Saal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vnd fr\u00f6lich sich erschwingen,", "tokens": ["Vnd", "fr\u00f6\u00b7lich", "sich", "er\u00b7schwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PRF", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit hellem frewdenschall.", "tokens": ["Mit", "hel\u00b7lem", "frew\u00b7den\u00b7schall", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Auff harffen wird er schlagen", "tokens": ["Auff", "harf\u00b7fen", "wird", "er", "schla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "VVINF", "VAFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Das Alleluja rein,", "tokens": ["Das", "Al\u00b7le\u00b7lu\u00b7ja", "rein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Mag dan wol frewdig sagen,", "tokens": ["Mag", "dan", "wol", "frew\u00b7dig", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Da recht, so mu\u00df es sein.", "tokens": ["Da", "recht", ",", "so", "mu\u00df", "es", "sein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "VMFIN", "PPER", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ach springt nun vmb vnd vmben,", "tokens": ["Ach", "springt", "nun", "vmb", "vnd", "vm\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "ADV", "APPR", "KON", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ihr Engel Gottes all,", "tokens": ["Ihr", "En\u00b7gel", "Got\u00b7tes", "all", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "PIAT", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Last jauchtzen, spielen, trummen,", "tokens": ["Last", "jaucht\u00b7zen", ",", "spie\u00b7len", ",", "trum\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "VVINF", "$,", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "La\u00dft gehn posaunen schall.", "tokens": ["La\u00dft", "gehn", "po\u00b7sau\u00b7nen", "schall", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "La\u00dft pfeiffen, lauten, geigen,", "tokens": ["La\u00dft", "pfeif\u00b7fen", ",", "lau\u00b7ten", ",", "gei\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVIMP", "VVFIN", "$,", "VVFIN", "$,", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Gar lieblich gehn zu hauff,", "tokens": ["Gar", "lieb\u00b7lich", "gehn", "zu", "hauff", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVINF", "PTKZU", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "All ehr last Gott erzeigen:", "tokens": ["All", "ehr", "last", "Gott", "er\u00b7zei\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Nur bald; bald auff, bald auff.", "tokens": ["Nur", "bald", ";", "bald", "auff", ",", "bald", "auff", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "$.", "ADV", "PTKVZ", "$,", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "O Gott, will dich nun preisen,", "tokens": ["O", "Gott", ",", "will", "dich", "nun", "prei\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VMFIN", "PRF", "ADV", "VVFIN", "$,"], 