, "and\u00b7re", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wan ich schon bin dahin.", "tokens": ["Wan", "ich", "schon", "bin", "da\u00b7hin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VAFIN", "PAV", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.9": {"line.1": {"text": "Dan auch mein geist wird springen,", "tokens": ["Dan", "auch", "mein", "geist", "wird", "sprin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPOSAT", "NN", "VAFIN", "VVINF", "$,"], "meter": "---+-+-", "measure": "unknown.measure.di"}, "line.2": {"text": "Hoch in des Himmels Saal,", "tokens": ["Hoch", "in", "des", "Him\u00b7mels", "Saal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vnd fr\u00f6lich sich erschwingen,", "tokens": ["Vnd", "fr\u00f6\u00b7lich", "sich", "er\u00b7schwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PRF", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit hellem frewdenschall.", "tokens": ["Mit", "hel\u00b7lem", "frew\u00b7den\u00b7schall", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Auff harffen wird er schlagen", "tokens": ["Auff", "harf\u00b7fen", "wird", "er", "schla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "VVINF", "VAFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Das Alleluja rein,", "tokens": ["Das", "Al\u00b7le\u00b7lu\u00b7ja", "rein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Mag dan wol frewdig sagen,", "tokens": ["Mag", "dan", "wol", "frew\u00b7dig", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Da recht, so mu\u00df es sein.", "tokens": ["Da", "recht", ",", "so", "mu\u00df", "es", "sein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "VMFIN", "PPER", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Ach springt nun vmb vnd vmben,", "tokens": ["Ach", "springt", "nun", "vmb", "vnd", "vm\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "ADV", "APPR", "KON", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ihr Engel Gottes all,", "tokens": ["Ihr", "En\u00b7gel", "Got\u00b7tes", "all", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "PIAT", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Last jauchtzen, spielen, trummen,", "tokens": ["Last", "jaucht\u00b7zen", ",", "spie\u00b7len", ",", "trum\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "VVINF", "$,", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "La\u00dft gehn posaunen schall.", "tokens": ["La\u00dft", "gehn", "po\u00b7sau\u00b7nen", "schall", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "La\u00dft pfeiffen, lauten, geigen,", "tokens": ["La\u00dft", "pfeif\u00b7fen", ",", "lau\u00b7ten", ",", "gei\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVIMP", "VVFIN", "$,", "VVFIN", "$,", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Gar lieblich gehn zu hauff,", "tokens": ["Gar", "lieb\u00b7lich", "gehn", "zu", "hauff", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVINF", "PTKZU", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "All ehr last Gott erzeigen:", "tokens": ["All", "ehr", "last", "Gott", "er\u00b7zei\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Nur bald; bald auff, bald auff.", "tokens": ["Nur", "bald", ";", "bald", "auff", ",", "bald", "auff", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "$.", "ADV", "PTKVZ", "$,", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}