{"dta.poem.21182": {"metadata": {"author": {"name": "Zachariae, Justus Friedrich Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "An  \n  die G\u00f6ttin der Gesundheit.  \n   \n Als sich der Erbprinz im Achner Bade  \n befand.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-20676-2", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Opfer dampfen dir zu Ehren,", "tokens": ["Die", "Op\u00b7fer", "damp\u00b7fen", "dir", "zu", "Eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die du im Himmel wohnst,", "tokens": ["Die", "du", "im", "Him\u00b7mel", "wohnst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und von den seegensreichen Sph\u00e4ren", "tokens": ["Und", "von", "den", "see\u00b7gens\u00b7rei\u00b7chen", "Sph\u00e4\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das Flehn der Sterblichen belohnst.", "tokens": ["Das", "Flehn", "der", "Sterb\u00b7li\u00b7chen", "be\u00b7lohnst", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "O G\u00f6ttin, huldreich schaue nieder", "tokens": ["O", "G\u00f6t\u00b7tin", ",", "huld\u00b7reich", "schau\u00b7e", "nie\u00b7der"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NE", "$,", "ADJD", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vom Thron, der dich erhebt;", "tokens": ["Vom", "Thron", ",", "der", "dich", "er\u00b7hebt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wo dich mit goldenem Gefieder", "tokens": ["Wo", "dich", "mit", "gol\u00b7de\u00b7nem", "Ge\u00b7fie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gl\u00fcck und Zufriedenheit umschwebt;", "tokens": ["Gl\u00fcck", "und", "Zu\u00b7frie\u00b7den\u00b7heit", "um\u00b7schwebt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.3": {"line.1": {"text": "Auf ihn, den Helden, der vorm Heere", "tokens": ["Auf", "ihn", ",", "den", "Hel\u00b7den", ",", "der", "vorm", "Hee\u00b7re"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPER", "$,", "ART", "NN", "$,", "PRELS", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Geliebt ward; selbst vom Feind;", "tokens": ["Ge\u00b7liebt", "ward", ";", "selbst", "vom", "Feind", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "$.", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Auf Jhn, der edlern Menschheit Ehre,", "tokens": ["Auf", "Jhn", ",", "der", "ed\u00b7lern", "Menschheit", "Eh\u00b7re", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Jhn, jeder Tugend wahren Freund.", "tokens": ["Jhn", ",", "je\u00b7der", "Tu\u00b7gend", "wah\u00b7ren", "Freund", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PIAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Den Kranz, der Ueberwinder lohnet,", "tokens": ["Den", "Kranz", ",", "der", "Ue\u00b7ber\u00b7win\u00b7der", "loh\u00b7net", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Brach er mit tapfrer Hand;", "tokens": ["Brach", "er", "mit", "tapf\u00b7rer", "Hand", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Hat seines Blutes nicht geschonet,", "tokens": ["Hat", "sei\u00b7nes", "Blu\u00b7tes", "nicht", "ge\u00b7scho\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hat es verspritzt f\u00fcrs Vaterland.", "tokens": ["Hat", "es", "ver\u00b7spritzt", "f\u00fcrs", "Va\u00b7ter\u00b7land", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Als nach der ungl\u00fccksvollen Wunde", "tokens": ["Als", "nach", "der", "un\u00b7gl\u00fccks\u00b7vol\u00b7len", "Wun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Uns sein Verlust gedroht,", "tokens": ["Uns", "sein", "Ver\u00b7lust", "ge\u00b7droht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie jauchzten da in schwarzer Stunde", "tokens": ["Wie", "jauchz\u00b7ten", "da", "in", "schwar\u00b7zer", "Stun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Kriegesfurien, der Tod!", "tokens": ["Die", "Krie\u00b7ges\u00b7fu\u00b7ri\u00b7en", ",", "der", "Tod", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Viel Tage giengen da verh\u00fcllet", "tokens": ["Viel", "Ta\u00b7ge", "gien\u00b7gen", "da", "ver\u00b7h\u00fcl\u00b7let"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "ADV", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In Traurigkeit vorbey!", "tokens": ["In", "Trau\u00b7rig\u00b7keit", "vor\u00b7bey", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch unser Flehen ward erf\u00fcllet,", "tokens": ["Doch", "un\u00b7ser", "Fle\u00b7hen", "ward", "er\u00b7f\u00fcl\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Du gabst Jhn, G\u00f6ttin, uns aufs neu.", "tokens": ["Du", "gabst", "Jhn", ",", "G\u00f6t\u00b7tin", ",", "uns", "aufs", "neu", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "NN", "$,", "PPER", "APPRART", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "La\u00df ietzt f\u00fcr Jhn die warmen Quellen", "tokens": ["La\u00df", "ietzt", "f\u00fcr", "Jhn", "die", "war\u00b7men", "Quel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "ADV", "APPR", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zwiefach wohlth\u00e4tig seyn!", "tokens": ["Zwie\u00b7fach", "wohlt\u00b7h\u00e4\u00b7tig", "seyn", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAINF", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "O sprudelt sanft, ihr Heilungswellen,", "tokens": ["O", "spru\u00b7delt", "sanft", ",", "ihr", "Hei\u00b7lungs\u00b7wel\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADJD", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Du, Himmel um Jhn her, sey rein!", "tokens": ["Du", ",", "Him\u00b7mel", "um", "Jhn", "her", ",", "sey", "rein", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "APPR", "PPER", "PTKVZ", "$,", "VAFIN", "ADJD", "$."], "meter": "-+---+-+", "measure": "dactylic.init"}}, "stanza.8": {"line.1": {"text": "Gr\u00fcnt sch\u00f6ner um Jhn her, ihr Felder,", "tokens": ["Gr\u00fcnt", "sch\u00f6\u00b7ner", "um", "Jhn", "her", ",", "ihr", "Fel\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "APPR", "PPER", "PTKVZ", "$,", "PPOSAT", "NN", "$,"], "meter": "-+--++-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Rausch Jhm, o Wasserfall!", "tokens": ["Rausch", "Jhm", ",", "o", "Was\u00b7ser\u00b7fall", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "FM", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Umschattet frischer Jhn, ihr W\u00e4lder,", "tokens": ["Um\u00b7schat\u00b7tet", "fri\u00b7scher", "Jhn", ",", "ihr", "W\u00e4l\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "PPER", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sing Jhm noch s\u00fc\u00dfer, Nachtigall!", "tokens": ["Sing", "Jhm", "noch", "s\u00fc\u00b7\u00dfer", ",", "Nach\u00b7ti\u00b7gall", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJD", "$,", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Ich seh\u2019s! \u2014 Schon sinkt Ruh und Vergn\u00fcgen", "tokens": ["Ich", "seh's", "!", "Schon", "sinkt", "Ruh", "und", "Ver\u00b7gn\u00fc\u00b7gen"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "NE", "$.", "$(", "ADV", "VVFIN", "NN", "KON", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Von des Olympus H\u00f6hn.", "tokens": ["Von", "des", "O\u00b7lym\u00b7pus", "H\u00f6hn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NE", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der G\u00f6ttersohn soll nach den Siegen", "tokens": ["Der", "G\u00f6t\u00b7ter\u00b7sohn", "soll", "nach", "den", "Sie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Belohnung seiner Thaten sehn.", "tokens": ["Be\u00b7loh\u00b7nung", "sei\u00b7ner", "Tha\u00b7ten", "sehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}