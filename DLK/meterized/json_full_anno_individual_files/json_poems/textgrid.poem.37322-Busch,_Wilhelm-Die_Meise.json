{"textgrid.poem.37322": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Die Meise", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auguste, wie fast jede Nichte,", "tokens": ["Au\u00b7gus\u00b7te", ",", "wie", "fast", "je\u00b7de", "Nich\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wei\u00df wenig von Naturgeschichte.", "tokens": ["Wei\u00df", "we\u00b7nig", "von", "Na\u00b7tur\u00b7ge\u00b7schich\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Zu bilden sie in diesem Fache,", "tokens": ["Zu", "bil\u00b7den", "sie", "in", "die\u00b7sem", "Fa\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PPER", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ist f\u00fcr den Onkel Ehrensache.", "tokens": ["Ist", "f\u00fcr", "den", "On\u00b7kel", "Eh\u00b7ren\u00b7sa\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Auguste, sprach er, glaub es mir,", "tokens": ["Au\u00b7gus\u00b7te", ",", "sprach", "er", ",", "glaub", "es", "mir", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Meise ist ein nettes Tier.", "tokens": ["Die", "Mei\u00b7se", "ist", "ein", "net\u00b7tes", "Tier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gar zierlich ist ihr Leibesbau,", "tokens": ["Gar", "zier\u00b7lich", "ist", "ihr", "Lei\u00b7bes\u00b7bau", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auch ist sie schwarz, wei\u00df, gelb und blau.", "tokens": ["Auch", "ist", "sie", "schwarz", ",", "wei\u00df", ",", "gelb", "und", "blau", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "$,", "VVFIN", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Hell fl\u00f6tet sie und klettert munter", "tokens": ["Hell", "fl\u00f6\u00b7tet", "sie", "und", "klet\u00b7tert", "mun\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "KON", "VVFIN", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Am Strauch kopf\u00fcber und kopfunter.", "tokens": ["Am", "Strauch", "kopf\u00b7\u00fc\u00b7ber", "und", "ko\u00b7pfun\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NE", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Das h\u00e4rt'ste Korn verschm\u00e4ht sie nicht,", "tokens": ["Das", "h\u00e4rt'\u00b7ste", "Korn", "ver\u00b7schm\u00e4ht", "sie", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Sie h\u00e4mmert, bis die Schale bricht.", "tokens": ["Sie", "h\u00e4m\u00b7mert", ",", "bis", "die", "Scha\u00b7le", "bricht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Mohnk\u00f6pfen bohrt sie mit Verstand", "tokens": ["Mohn\u00b7k\u00f6p\u00b7fen", "bohrt", "sie", "mit", "Ver\u00b7stand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Ein L\u00f6chlein in den Unterrand,", "tokens": ["Ein", "L\u00f6c\u00b7hlein", "in", "den", "Un\u00b7ter\u00b7rand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Weil dann die S\u00e4merei gelind", "tokens": ["Weil", "dann", "die", "S\u00e4\u00b7me\u00b7rei", "ge\u00b7lind"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Von selbst in ihren Schnabel rinnt.", "tokens": ["Von", "selbst", "in", "ih\u00b7ren", "Schna\u00b7bel", "rinnt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Nicht immer liebt man Fastenspeisen,", "tokens": ["Nicht", "im\u00b7mer", "liebt", "man", "Fas\u00b7ten\u00b7spei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "VVFIN", "PIS", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Der Grundsatz gilt auch f\u00fcr die Meisen.", "tokens": ["Der", "Grund\u00b7satz", "gilt", "auch", "f\u00fcr", "die", "Mei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Sie gucken scharf in alle Ritzen,", "tokens": ["Sie", "gu\u00b7cken", "scharf", "in", "al\u00b7le", "Rit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Wo fette K\u00e4ferlarven sitzen,", "tokens": ["Wo", "fet\u00b7te", "K\u00e4\u00b7fer\u00b7lar\u00b7ven", "sit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Und fangen sonst noch Myriaden", "tokens": ["Und", "fan\u00b7gen", "sonst", "noch", "My\u00b7ria\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "ADV", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.18": {"text": "Insekten, die dem Menschen schaden,", "tokens": ["In\u00b7sek\u00b7ten", ",", "die", "dem", "Men\u00b7schen", "scha\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Und hieran siehst du au\u00dferdem,", "tokens": ["Und", "hie\u00b7ran", "siehst", "du", "au\u00b7\u00dfer\u00b7dem", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "VVFIN", "PPER", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Wie weise das Natursystem. \u2013", "tokens": ["Wie", "wei\u00b7se", "das", "Na\u00b7tur\u00b7sys\u00b7tem", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "$.", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.21": {"text": "So zeigt er wie die Sache lag.", "tokens": ["So", "zeigt", "er", "wie", "die", "Sa\u00b7che", "lag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KOKOM", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Es war kurz vor Martinitag.", "tokens": ["Es", "war", "kurz", "vor", "Mar\u00b7ti\u00b7ni\u00b7tag", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "APPR", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Wer dann vern\u00fcnftig ist und kann's", "tokens": ["Wer", "dann", "ver\u00b7n\u00fcnf\u00b7tig", "ist", "und", "kann's"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "ADJD", "VAFIN", "KON", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sich leisten, kauft sich eine Gans.", "tokens": ["Sich", "leis\u00b7ten", ",", "kauft", "sich", "ei\u00b7ne", "Gans", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVINF", "$,", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Auch an des Onkels Au\u00dfengiebel", "tokens": ["Auch", "an", "des", "On\u00b7kels", "Au\u00b7\u00dfen\u00b7gie\u00b7bel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Hing eine solche, die nicht \u00fcbel,", "tokens": ["Hing", "ei\u00b7ne", "sol\u00b7che", ",", "die", "nicht", "\u00fc\u00b7bel", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "$,", "PRELS", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Um, nackt im Freien aufgehangen,", "tokens": ["Um", ",", "nackt", "im", "Frei\u00b7en", "auf\u00b7ge\u00b7han\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "$,", "ADJD", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die rechte Reife zu erlangen.", "tokens": ["Die", "rech\u00b7te", "Rei\u00b7fe", "zu", "er\u00b7lan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Auf diesen Braten freute sich", "tokens": ["Auf", "die\u00b7sen", "Bra\u00b7ten", "freu\u00b7te", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Onkel sehr und namentlich", "tokens": ["Der", "On\u00b7kel", "sehr", "und", "na\u00b7ment\u00b7lich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "KON", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Vor allem auf die braune Haut,", "tokens": ["Vor", "al\u00b7lem", "auf", "die", "brau\u00b7ne", "Haut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Obgleich er sie nur schwer verdaut.", "tokens": ["Ob\u00b7gleich", "er", "sie", "nur", "schwer", "ver\u00b7daut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Martini kam, doch kein Arom", "tokens": ["Mar\u00b7ti\u00b7ni", "kam", ",", "doch", "kein", "A\u00b7rom"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VVFIN", "$,", "ADV", "PIAT", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Von Braten sp\u00fcrt der gute Ohm.", "tokens": ["Von", "Bra\u00b7ten", "sp\u00fcrt", "der", "gu\u00b7te", "Ohm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Statt dessen trat voll Ungest\u00fcm", "tokens": ["Statt", "des\u00b7sen", "trat", "voll", "Un\u00b7ge\u00b7st\u00fcm"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PDS", "VVFIN", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Nichte ein und zeigte ihm", "tokens": ["Die", "Nich\u00b7te", "ein", "und", "zeig\u00b7te", "ihm"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PTKVZ", "KON", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Gans, die kaum noch Gans zu nennen,", "tokens": ["Die", "Gans", ",", "die", "kaum", "noch", "Gans", "zu", "nen\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "ADV", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ein Scheusal, nicht zum Wiederkennen,", "tokens": ["Ein", "Scheu\u00b7sal", ",", "nicht", "zum", "Wie\u00b7der\u00b7ken\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PTKNEG", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Zernagt beinah bis auf die Knochen.", "tokens": ["Zer\u00b7nagt", "bei\u00b7nah", "bis", "auf", "die", "Kno\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Kein Zweifel war, wer dies verbrochen,", "tokens": ["Kein", "Zwei\u00b7fel", "war", ",", "wer", "dies", "ver\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "$,", "PWS", "PDS", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Denn deutlich lehrt der Augenschein,", "tokens": ["Denn", "deut\u00b7lich", "lehrt", "der", "Au\u00b7gen\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Es konnten nur die Meisen sein.", "tokens": ["Es", "konn\u00b7ten", "nur", "die", "Mei\u00b7sen", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Also ade! du braune Kruste.", "tokens": ["Al\u00b7so", "a\u00b7de", "!", "du", "brau\u00b7ne", "Krus\u00b7te", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$.", "PPER", "ADJA", "NN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Ja, lieber Onkel, sprach Auguste,", "tokens": ["Ja", ",", "lie\u00b7ber", "On\u00b7kel", ",", "sprach", "Au\u00b7gus\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "NN", "$,", "VVFIN", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die gern, nach weiblicher Manier,", "tokens": ["Die", "gern", ",", "nach", "weib\u00b7li\u00b7cher", "Ma\u00b7nier", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bei einem Irrtum ihn ertappt:", "tokens": ["Bei", "ei\u00b7nem", "Irr\u00b7tum", "ihn", "er\u00b7tappt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Meise ist ein nettes Tier.", "tokens": ["Die", "Mei\u00b7se", "ist", "ein", "net\u00b7tes", "Tier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da hast du wieder recht gehabt.", "tokens": ["Da", "hast", "du", "wie\u00b7der", "recht", "ge\u00b7habt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADJD", "VAPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Auguste, wie fast jede Nichte,", "tokens": ["Au\u00b7gus\u00b7te", ",", "wie", "fast", "je\u00b7de", "Nich\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wei\u00df wenig von Naturgeschichte.", "tokens": ["Wei\u00df", "we\u00b7nig", "von", "Na\u00b7tur\u00b7ge\u00b7schich\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Zu bilden sie in diesem Fache,", "tokens": ["Zu", "bil\u00b7den", "sie", "in", "die\u00b7sem", "Fa\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PPER", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ist f\u00fcr den Onkel Ehrensache.", "tokens": ["Ist", "f\u00fcr", "den", "On\u00b7kel", "Eh\u00b7ren\u00b7sa\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Auguste, sprach er, glaub es mir,", "tokens": ["Au\u00b7gus\u00b7te", ",", "sprach", "er", ",", "glaub", "es", "mir", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Meise ist ein nettes Tier.", "tokens": ["Die", "Mei\u00b7se", "ist", "ein", "net\u00b7tes", "Tier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gar zierlich ist ihr Leibesbau,", "tokens": ["Gar", "zier\u00b7lich", "ist", "ihr", "Lei\u00b7bes\u00b7bau", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auch ist sie schwarz, wei\u00df, gelb und blau.", "tokens": ["Auch", "ist", "sie", "schwarz", ",", "wei\u00df", ",", "gelb", "und", "blau", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "$,", "VVFIN", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Hell fl\u00f6tet sie und klettert munter", "tokens": ["Hell", "fl\u00f6\u00b7tet", "sie", "und", "klet\u00b7tert", "mun\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "KON", "VVFIN", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Am Strauch kopf\u00fcber und kopfunter.", "tokens": ["Am", "Strauch", "kopf\u00b7\u00fc\u00b7ber", "und", "ko\u00b7pfun\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NE", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Das h\u00e4rt'ste Korn verschm\u00e4ht sie nicht,", "tokens": ["Das", "h\u00e4rt'\u00b7ste", "Korn", "ver\u00b7schm\u00e4ht", "sie", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Sie h\u00e4mmert, bis die Schale bricht.", "tokens": ["Sie", "h\u00e4m\u00b7mert", ",", "bis", "die", "Scha\u00b7le", "bricht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Mohnk\u00f6pfen bohrt sie mit Verstand", "tokens": ["Mohn\u00b7k\u00f6p\u00b7fen", "bohrt", "sie", "mit", "Ver\u00b7stand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Ein L\u00f6chlein in den Unterrand,", "tokens": ["Ein", "L\u00f6c\u00b7hlein", "in", "den", "Un\u00b7ter\u00b7rand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Weil dann die S\u00e4merei gelind", "tokens": ["Weil", "dann", "die", "S\u00e4\u00b7me\u00b7rei", "ge\u00b7lind"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Von selbst in ihren Schnabel rinnt.", "tokens": ["Von", "selbst", "in", "ih\u00b7ren", "Schna\u00b7bel", "rinnt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Nicht immer liebt man Fastenspeisen,", "tokens": ["Nicht", "im\u00b7mer", "liebt", "man", "Fas\u00b7ten\u00b7spei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "VVFIN", "PIS", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Der Grundsatz gilt auch f\u00fcr die Meisen.", "tokens": ["Der", "Grund\u00b7satz", "gilt", "auch", "f\u00fcr", "die", "Mei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Sie gucken scharf in alle Ritzen,", "tokens": ["Sie", "gu\u00b7cken", "scharf", "in", "al\u00b7le", "Rit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Wo fette K\u00e4ferlarven sitzen,", "tokens": ["Wo", "fet\u00b7te", "K\u00e4\u00b7fer\u00b7lar\u00b7ven", "sit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Und fangen sonst noch Myriaden", "tokens": ["Und", "fan\u00b7gen", "sonst", "noch", "My\u00b7ria\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "ADV", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.18": {"text": "Insekten, die dem Menschen schaden,", "tokens": ["In\u00b7sek\u00b7ten", ",", "die", "dem", "Men\