 "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Jo! ich preise dich, Evius!", "tokens": ["Jo", "!", "ich", "prei\u00b7se", "dich", ",", "E\u00b7vius", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "$.", "PPER", "VVFIN", "PPER", "$,", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}