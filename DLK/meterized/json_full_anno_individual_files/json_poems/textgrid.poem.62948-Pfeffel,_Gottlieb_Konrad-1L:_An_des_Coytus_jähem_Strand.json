{"textgrid.poem.62948": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: An des Coytus j\u00e4hem Strand", "genre": "verse", "period": "N.A.", "pub_year": 1781, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "An des Coytus j\u00e4hem Strand", "tokens": ["An", "des", "Coy\u00b7tus", "j\u00e4\u00b7hem", "Strand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Lag trotzig, gleich dem H\u00f6llengotte,", "tokens": ["Lag", "trot\u00b7zig", ",", "gleich", "dem", "H\u00f6l\u00b7len\u00b7got\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "$,", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Im Eingang einer Lavagrotte,", "tokens": ["Im", "Ein\u00b7gang", "ei\u00b7ner", "La\u00b7va\u00b7grot\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Des Cato Schatten. Sein Gewand,", "tokens": ["Des", "Ca\u00b7to", "Schat\u00b7ten", ".", "Sein", "Ge\u00b7wand", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NE", "NN", "$.", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Noch starr von seinem Heldenblute,", "tokens": ["Noch", "starr", "von", "sei\u00b7nem", "Hel\u00b7den\u00b7blu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Bedeckte halb nur seine Hand,", "tokens": ["Be\u00b7deck\u00b7te", "halb", "nur", "sei\u00b7ne", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die traulich in der Wunde ruhte,", "tokens": ["Die", "trau\u00b7lich", "in", "der", "Wun\u00b7de", "ruh\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Durch die sein Geist die Freyheit fand.", "tokens": ["Durch", "die", "sein", "Geist", "die", "Frey\u00b7heit", "fand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPOSAT", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Da kam ein Zwerg mit leerer Stirne", "tokens": ["Da", "kam", "ein", "Zwerg", "mit", "lee\u00b7rer", "Stir\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "(denn rauchend triefte sein Gehirne", "tokens": ["(", "denn", "rau\u00b7chend", "trief\u00b7te", "sein", "Ge\u00b7hir\u00b7ne"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "KON", "ADJD", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Noch \u00fcber seine Schl\u00e4fe hin)", "tokens": ["Noch", "\u00fc\u00b7ber", "sei\u00b7ne", "Schl\u00e4\u00b7fe", "hin", ")"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Den schwarzen Wall hinaufgeklommen,", "tokens": ["Den", "schwar\u00b7zen", "Wall", "hin\u00b7auf\u00b7ge\u00b7klom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Und dr\u00e4ngte keck sich neben ihn.", "tokens": ["Und", "dr\u00e4ng\u00b7te", "keck", "sich", "ne\u00b7ben", "ihn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PRF", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Ah, Bruder Cato, sey willkommen!", "tokens": ["Ah", ",", "Bru\u00b7der", "Ca\u00b7to", ",", "sey", "will\u00b7kom\u00b7men", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "NE", "$,", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Schlag ein, rief er, auch hier ist Kraft.", "tokens": ["Schlag", "ein", ",", "rief", "er", ",", "auch", "hier", "ist", "Kraft", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "VVFIN", "PPER", "$,", "ADV", "ADV", "VAFIN", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.16": {"text": "Hum, sprach der R\u00f6mer, sachte, sachte,", "tokens": ["Hum", ",", "sprach", "der", "R\u00f6\u00b7mer", ",", "sach\u00b7te", ",", "sach\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "ART", "NN", "$,", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Wie kommen wir zur Br\u00fcderschaft?", "tokens": ["Wie", "kom\u00b7men", "wir", "zur", "Br\u00fc\u00b7der\u00b7schaft", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "\u00bbey kennst du mich denn nicht?\u00ab \u2013 Hier lachte", "tokens": ["\u00bb", "ey", "kennst", "du", "mich", "denn", "nicht", "?", "\u00ab", "\u2013", "Hier", "lach\u00b7te"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct", "word", "word"], "pos": ["$(", "NE", "VVFIN", "PPER", "PRF", "ADV", "PTKNEG", "$.", "$(", "$(", "ADV", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Zum erstenmal der ernste Held. \u2013", "tokens": ["Zum", "ers\u00b7ten\u00b7mal", "der", "erns\u00b7te", "Held", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "ADV", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "\u00bbich bin ein Enkel Teuts, und machte", "tokens": ["\u00bb", "ich", "bin", "ein", "En\u00b7kel", "Teuts", ",", "und", "mach\u00b7te"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["$(", "PPER", "VAFIN", "ART", "NN", "NE", "$,", "KON", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Doch Lerms genug in jener Welt:", "tokens": ["Doch", "Lerms", "ge\u00b7nug", "in", "je\u00b7ner", "Welt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "F\u00fcr meines Freundes Weib entbrannte", "tokens": ["F\u00fcr", "mei\u00b7nes", "Freun\u00b7des", "Weib", "ent\u00b7brann\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Wie ein Vulkan mein L\u00f6wenherz.", "tokens": ["Wie", "ein", "Vul\u00b7kan", "mein", "L\u00f6\u00b7wen\u00b7herz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NE", "PPOSAT", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.24": {"text": "Nichts glich Elmiren; ich bekannte", "tokens": ["Nichts", "glich", "El\u00b7mi\u00b7ren", ";", "ich", "be\u00b7kann\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PIS", "VVFIN", "NE", "$.", "PPER", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Ihr auf den Knien meinen Schmerz:", "tokens": ["Ihr", "auf", "den", "Kni\u00b7en", "mei\u00b7nen", "Schmerz", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Sie lie\u00df mich schmachten, sie verbannte", "tokens": ["Sie", "lie\u00df", "mich", "schmach\u00b7ten", ",", "sie", "ver\u00b7bann\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "VVFIN", "$,", "PPER", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Mich als den M\u00f6rder ihrer Ruh,", "tokens": ["Mich", "als", "den", "M\u00f6r\u00b7der", "ih\u00b7rer", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KOUS", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Mit hohem Ernst von ihrer Schwelle,", "tokens": ["Mit", "ho\u00b7hem", "Ernst", "von", "ih\u00b7rer", "Schwel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "Und ich verschlo\u00df mich auf der Stelle", "tokens": ["Und", "ich", "ver\u00b7schlo\u00df", "mich", "auf", "der", "Stel\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PRF", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "In mein Gemach und \u2013 starb wie du.\u00ab", "tokens": ["In", "mein", "Ge\u00b7mach", "und", "\u2013", "starb", "wie", "du", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "$(", "VVFIN", "KOKOM", "PPER", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Das ist zuviel, selbst f\u00fcr die H\u00f6lle", "tokens": ["Das", "ist", "zu\u00b7viel", ",", "selbst", "f\u00fcr", "die", "H\u00f6l\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "PIS", "$,", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.32": {"text": "Zuviel! rief Cato w\u00fcthend aus,", "tokens": ["Zu\u00b7viel", "!", "rief", "Ca\u00b7to", "w\u00fct\u00b7hend", "aus", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "NE", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Ha, C\u00e4sar, komm und \u00fcberwinde", "tokens": ["Ha", ",", "C\u00e4\u00b7sar", ",", "komm", "und", "\u00fc\u00b7berw\u00b7in\u00b7de"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "$,", "NE", "$,", "VVFIN", "KON", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.34": {"text": "Mich noch in einem zweyten Straus,", "tokens": ["Mich", "noch", "in", "ei\u00b7nem", "zwey\u00b7ten", "Straus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Erdro\u00dfle mich gleich einem Kinde", "tokens": ["Er\u00b7dro\u00df\u00b7le", "mich", "gleich", "ei\u00b7nem", "Kin\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ADV", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.36": {"text": "Und schleudre mit Triumphgeschrey", "tokens": ["Und", "schleud\u00b7re", "mit", "Tri\u00b7umph\u00b7ge\u00b7schrey"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.37": {"text": "Mein Haupt in eine Schindergrube;", "tokens": ["Mein", "Haupt", "in", "ei\u00b7ne", "Schin\u00b7der\u00b7gru\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.38": {"text": "Nur r\u00fchme sich kein Lotterbube,", "tokens": ["Nur", "r\u00fch\u00b7me", "sich", "kein", "Lot\u00b7ter\u00b7bu\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.39": {"text": "Da\u00df er wie ich gestorben sey.", "tokens": ["Da\u00df", "er", "wie", "ich", "ge\u00b7stor\u00b7ben", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PWAV", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "An des Coytus j\u00e4hem Strand", "tokens": ["An", "des", "Coy\u00b7tus", "j\u00e4\u00b7hem", "Strand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Lag trotzig, gleich dem H\u00f6llengotte,", "tokens": ["Lag", "trot\u00b7zig", ",", "gleich", "dem", "H\u00f6l\u00b7len\u00b7got\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "$,", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Im Eingang einer Lavagrotte,", "tokens": ["Im", "Ein\u00b7gang", "ei\u00b7ner", "La\u00