{"dta.poem.7065": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Entwurff einiger Betrachtungen,  \n \u00fcber den  \n  Zustand der Seeligen.  \n Beym Anfang des 1727sten Jahres.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mit tausendfacher Lust und innigem Vergn\u00fcgen", "tokens": ["Mit", "tau\u00b7send\u00b7fa\u00b7cher", "Lust", "und", "in\u00b7ni\u00b7gem", "Ver\u00b7gn\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wird ihnen dann zugleich zu wissen f\u00fcgen,", "tokens": ["Wird", "ih\u00b7nen", "dann", "zu\u00b7gleich", "zu", "wis\u00b7sen", "f\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "PTKZU", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Schaar der ", "tokens": ["Die", "Schaar", "der"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Wie sie, durch GOTTES Huld darzu bewogen,", "tokens": ["Wie", "sie", ",", "durch", "GoT\u00b7TES", "Huld", "dar\u00b7zu", "be\u00b7wo\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "APPR", "NE", "NE", "PAV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Sich auf der Welt vom B\u00f6sen abgezogen;", "tokens": ["Sich", "auf", "der", "Welt", "vom", "B\u00f6\u00b7sen", "ab\u00b7ge\u00b7zo\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wie sie, von GOTT ger\u00fchrt, durch ein unstr\u00e4fflich Leben,", "tokens": ["Wie", "sie", ",", "von", "GoTT", "ge\u00b7r\u00fchrt", ",", "durch", "ein", "uns\u00b7tr\u00e4ff\u00b7lich", "Le\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "APPR", "NE", "VVPP", "$,", "APPR", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Bestrebet, GOTT sich zu ergeben;", "tokens": ["Be\u00b7stre\u00b7bet", ",", "GoTT", "sich", "zu", "er\u00b7ge\u00b7ben", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NE", "PRF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Wie sie so manchesmal, von Lust und Geitz verf\u00fchrt,", "tokens": ["Wie", "sie", "so", "man\u00b7ches\u00b7mal", ",", "von", "Lust", "und", "Geitz", "ver\u00b7f\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADV", "$,", "APPR", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Vom Hochmuth fortgefchleppt, von Tr\u00fcbsal weggerissen,", "tokens": ["Vom", "Hoch\u00b7muth", "fort\u00b7gefc\u00b7hleppt", ",", "von", "Tr\u00fcb\u00b7sal", "weg\u00b7ge\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Fast in Verzweifflung fincken m\u00fcssen,", "tokens": ["Fast", "in", "Ver\u00b7zweif\u00b7flung", "fin\u00b7cken", "m\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Wor\u00fcber sie jedoch zuletzt noch triumphirt.", "tokens": ["Wo\u00b7r\u00fc\u00b7ber", "sie", "je\u00b7doch", "zu\u00b7letzt", "noch", "tri\u00b7um\u00b7phirt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+---+-+-+", "measure": "unknown.measure.penta"}}}}}