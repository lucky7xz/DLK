{"dta.poem.4016": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Winter .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905197104", "language": ["de:0.99"], "booktitle": "H\u00f6lderlin, Friedrich: Gedichte. Stuttgart u. a., 1826."}, "poem": {"stanza.1": {"line.1": {"text": "Jetzt komm und h\u00fclle, zaubrischer Phantasus,", "tokens": ["Jetzt", "komm", "und", "h\u00fcl\u00b7le", ",", "zaub\u00b7ri\u00b7scher", "Phan\u00b7ta\u00b7sus", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Den zarten Sinn der Frauen in Wolken ein,", "tokens": ["Den", "zar\u00b7ten", "Sinn", "der", "Frau\u00b7en", "in", "Wol\u00b7ken", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "In goldne Tr\u00e4um' und sch\u00fctze sie, die", "tokens": ["In", "gold\u00b7ne", "Tr\u00e4um'", "und", "sch\u00fct\u00b7ze", "sie", ",", "die"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "ADJA", "NN", "KON", "VVFIN", "PPER", "$,", "PRELS"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Bl\u00fchende Ruhe der Immerguten.", "tokens": ["Bl\u00fc\u00b7hen\u00b7de", "Ru\u00b7he", "der", "Im\u00b7mer\u00b7gu\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Dem Manne la\u00df sein Sinnen und sein Gesch\u00e4ft", "tokens": ["Dem", "Man\u00b7ne", "la\u00df", "sein", "Sin\u00b7nen", "und", "sein", "Ge\u00b7sch\u00e4ft"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVIMP", "PPOSAT", "NN", "KON", "PPOSAT", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und seiner Kerze Schein und den k\u00fcnft'gen Tag", "tokens": ["Und", "sei\u00b7ner", "Ker\u00b7ze", "Schein", "und", "den", "k\u00fcnft'\u00b7gen", "Tag"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "KON", "ART", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Gefallen, la\u00df des Unmuths ihm, der", "tokens": ["Ge\u00b7fal\u00b7len", ",", "la\u00df", "des", "Un\u00b7muths", "ihm", ",", "der"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["NN", "$,", "VVIMP", "ART", "NN", "PPER", "$,", "PRELS"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "H\u00e4\u00dflichen Sorge zu viel nicht werden,", "tokens": ["H\u00e4\u00df\u00b7li\u00b7chen", "Sor\u00b7ge", "zu", "viel", "nicht", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "PIS", "PTKNEG", "VAINF", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Wenn jetzt der immerz\u00fcrnende Boreas,", "tokens": ["Wenn", "jetzt", "der", "im\u00b7mer\u00b7z\u00fcr\u00b7nen\u00b7de", "Bo\u00b7re\u00b7as", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Mein Erbfeind, \u00fcber Nacht mit dem Frost das Land", "tokens": ["Mein", "Erb\u00b7feind", ",", "\u00fc\u00b7ber", "Nacht", "mit", "dem", "Frost", "das", "Land"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "APPR", "NN", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Bef\u00e4llt, und sp\u00e4t, zur Schlummerstunde,", "tokens": ["Be\u00b7f\u00e4llt", ",", "und", "sp\u00e4t", ",", "zur", "Schlum\u00b7mer\u00b7stun\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KON", "ADJD", "$,", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Spottend der Menschen, sein schrecklich Lied", "tokens": ["Spot\u00b7tend", "der", "Men\u00b7schen", ",", "sein", "schreck\u00b7lich", "Lied"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ART", "NN", "$,", "PPOSAT", "ADJD", "NN"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.5": {"text": "singt,", "tokens": ["singt", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}}, "stanza.4": {"line.1": {"text": "Und unsrer St\u00e4dte Mauern und unserm Zaun,", "tokens": ["Und", "uns\u00b7rer", "St\u00e4d\u00b7te", "Mau\u00b7ern", "und", "un\u00b7serm", "Zaun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Den flei\u00dfig wir gesetzt, und den stillen Hain", "tokens": ["Den", "flei\u00b7\u00dfig", "wir", "ge\u00b7setzt", ",", "und", "den", "stil\u00b7len", "Hain"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "PPER", "VVPP", "$,", "KON", "ART", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Zerrei\u00dft, und selber im Gesang die", "tokens": ["Zer\u00b7rei\u00dft", ",", "und", "sel\u00b7ber", "im", "Ge\u00b7sang", "die"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "KON", "ADV", "APPRART", "NN", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Seele mir st\u00f6ret, der Allverderber.", "tokens": ["See\u00b7le", "mir", "st\u00f6\u00b7ret", ",", "der", "All\u00b7ver\u00b7der\u00b7ber", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "VVFIN", "$,", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.5": {"line.1": {"text": "Und rastlos tobend \u00fcber den sanften Strom", "tokens": ["Und", "rast\u00b7los", "to\u00b7bend", "\u00fc\u00b7ber", "den", "sanf\u00b7ten", "Strom"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVPP", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Sein schwarz Gew\u00f6lk aussch\u00fcttet, da\u00df weit umher", "tokens": ["Sein", "schwarz", "Ge\u00b7w\u00f6lk", "aus\u00b7sch\u00fct\u00b7tet", ",", "da\u00df", "weit", "um\u00b7her"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "ADJD", "NN", "VVPP", "$,", "KOUS", "ADJD", "PTKVZ"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Das Thal g\u00e4hrt und, wie fallend Laub, vom", "tokens": ["Das", "Thal", "g\u00e4hrt", "und", ",", "wie", "fal\u00b7lend", "Laub", ",", "vom"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "VVFIN", "KON", "$,", "PWAV", "ADJD", "NN", "$,", "APPRART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Berstenden H\u00fcgel herab der Fels f\u00e4llt.", "tokens": ["Bers\u00b7ten\u00b7den", "H\u00fc\u00b7gel", "her\u00b7ab", "der", "Fels", "f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.6": {"line.1": {"text": "Wohl frommer ist, denn andre Lebendige,", "tokens": ["Wohl", "from\u00b7mer", "ist", ",", "denn", "and\u00b7re", "Le\u00b7ben\u00b7di\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "$,", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Mensch; doch z\u00fcrnt es drau\u00dfen, geh\u00f6rt er", "tokens": ["Der", "Mensch", ";", "doch", "z\u00fcrnt", "es", "drau\u00b7\u00dfen", ",", "ge\u00b7h\u00f6rt", "er"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$.", "ADV", "VVFIN", "PPER", "ADV", "$,", "VVFIN", "PPER"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "auch", "tokens": ["auch"], "token_info": ["word"], "pos": ["ADV"], "meter": "-", "measure": "single.down"}, "line.4": {"text": "Sich eigner an und sinnt und ruht in", "tokens": ["Sich", "eig\u00b7ner", "an", "und", "sinnt", "und", "ruht", "in"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PRF", "ADJD", "PTKVZ", "KON", "VVFIN", "KON", "VVFIN", "APPR"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Sicherer H\u00fctte, der Freigeborne.", "tokens": ["Si\u00b7che\u00b7rer", "H\u00fct\u00b7te", ",", "der", "Frei\u00b7ge\u00b7bor\u00b7ne", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.7": {"line.1": {"text": "Und immer wohnt der freundlichen Genien", "tokens": ["Und", "im\u00b7mer", "wohnt", "der", "freund\u00b7li\u00b7chen", "Ge\u00b7ni\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Noch einer gerne segnend mit ihm, und wenn", "tokens": ["Noch", "ei\u00b7ner", "ger\u00b7ne", "seg\u00b7nend", "mit", "ihm", ",", "und", "wenn"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ART", "ADV", "ADJD", "APPR", "PPER", "$,", "KON", "KOUS"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Sie z\u00fcrnten all', die ungelehr'gen", "tokens": ["Sie", "z\u00fcrn\u00b7ten", "all'", ",", "die", "un\u00b7ge\u00b7lehr'\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PIS", "$,", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Geniuskr\u00e4fte, doch liebt die Liebe.", "tokens": ["Ge\u00b7nius\u00b7kr\u00e4f\u00b7te", ",", "doch", "liebt", "die", "Lie\u00b7be", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}