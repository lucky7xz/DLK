{"textgrid.poem.25939": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Die Nelken gl\u00fchen auf allen Altanen", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Laub gibt sich dem Abend hin,", "tokens": ["Das", "Laub", "gibt", "sich", "dem", "A\u00b7bend", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nur Wolken prunkend ans Fenster ziehn,", "tokens": ["Nur", "Wol\u00b7ken", "prun\u00b7kend", "ans", "Fens\u00b7ter", "ziehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVPP", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Die sind so feurig anzuschauen", "tokens": ["Die", "sind", "so", "feu\u00b7rig", "an\u00b7zu\u00b7schau\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ADV", "ADJD", "VVIZU"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie kleiderlose sch\u00f6ne Frauen.", "tokens": ["Wie", "klei\u00b7der\u00b7lo\u00b7se", "sch\u00f6\u00b7ne", "Frau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wie Frauen, die nach Freiern fahnen,", "tokens": ["Wie", "Frau\u00b7en", ",", "die", "nach", "Frei\u00b7ern", "fah\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "$,", "PRELS", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sehn sie dem Abend brennend entgegen.", "toke