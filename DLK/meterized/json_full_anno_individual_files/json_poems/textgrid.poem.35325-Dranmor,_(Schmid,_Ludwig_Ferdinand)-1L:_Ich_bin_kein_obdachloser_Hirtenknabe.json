{"textgrid.poem.35325": {"metadata": {"author": {"name": "Dranmor, (Schmid, Ludwig Ferdinand)", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich bin kein obdachloser Hirtenknabe", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich bin kein obdachloser Hirtenknabe", "tokens": ["Ich", "bin", "kein", "ob\u00b7dach\u00b7lo\u00b7ser", "Hir\u00b7ten\u00b7kna\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PIAT", "ADJA", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Von rauhen Worten und von groben Sitten,", "tokens": ["Von", "rau\u00b7hen", "Wor\u00b7ten", "und", "von", "gro\u00b7ben", "Sit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Marilia, ohne Herd und andre Habe,", "tokens": ["Ma\u00b7ri\u00b7lia", ",", "oh\u00b7ne", "Herd", "und", "and\u00b7re", "Ha\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUI", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der heute Hitze, gestern Frost erlitten;", "tokens": ["Der", "heu\u00b7te", "Hit\u00b7ze", ",", "ge\u00b7stern", "Frost", "er\u00b7lit\u00b7ten", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "NN", "$,", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Nein, ich besitze selber Haus und Weide", "tokens": ["Nein", ",", "ich", "be\u00b7sit\u00b7ze", "sel\u00b7ber", "Haus", "und", "Wei\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "ADV", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und ziehe mir Gem\u00fcse, Oel und Wein;", "tokens": ["Und", "zie\u00b7he", "mir", "Ge\u00b7m\u00fc\u00b7se", ",", "O\u00b7el", "und", "Wein", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.7": {"text": "Ich h\u00fcte Schafe, doch die Milch ist mein,", "tokens": ["Ich", "h\u00fc\u00b7te", "Scha\u00b7fe", ",", "doch", "die", "Milch", "ist", "mein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,", "ADV", "ART", "NN", "VAFIN", "PPOSAT", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Die Wolle auch, mit der ich mich bekleide.", "tokens": ["Die", "Wol\u00b7le", "auch", ",", "mit", "der", "ich", "mich", "be\u00b7klei\u00b7de", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "APPR", "PRELS", "PPER", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Mein Antlitz sah ich j\u00fcngst in einer Quelle", "tokens": ["Mein", "Ant\u00b7litz", "sah", "ich", "j\u00fcngst", "in", "ei\u00b7ner", "Quel\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und brauchte nicht vor Runzeln zu erschrecken;", "tokens": ["Und", "brauch\u00b7te", "nicht", "vor", "Run\u00b7zeln", "zu", "er\u00b7schre\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Kommt einer mir zu nahe \u2013 auf der Stelle", "tokens": ["Kommt", "ei\u00b7ner", "mir", "zu", "na\u00b7he", "\u2013", "auf", "der", "Stel\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ART", "PPER", "PTKA", "ADJD", "$(", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Bedien' ich ihn mit meinem langen Stecken.", "tokens": ["Be\u00b7dien'", "ich", "ihn", "mit", "mei\u00b7nem", "lan\u00b7gen", "Ste\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPER", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Gut bin ich auf der Fl\u00f6te unterrichtet,", "tokens": ["Gut", "bin", "ich", "auf", "der", "Fl\u00f6\u00b7te", "un\u00b7ter\u00b7rich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Zu meines eignen Meisters Neid und Grimm;", "tokens": ["Zu", "mei\u00b7nes", "eig\u00b7nen", "Meis\u00b7ters", "Neid", "und", "Grimm", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "NN", "KON", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Auch sing' ich \u2013 meine Stimme ist nicht schlimm \u2013", "tokens": ["Auch", "sing'", "ich", "\u2013", "mei\u00b7ne", "Stim\u00b7me", "ist", "nicht", "schlimm", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$(", "PPOSAT", "NN", "VAFIN", "PTKNEG", "ADJD", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Nur solche Lieder, die ich selbst gedichtet.", "tokens": ["Nur", "sol\u00b7che", "Lie\u00b7der", ",", "die", "ich", "selbst", "ge\u00b7dich\u00b7tet", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "$,", "PRELS", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Doch bin ich deshalb gl\u00fccklich? \u2013 Gott beh\u00fcte!", "tokens": ["Doch", "bin", "ich", "des\u00b7halb", "gl\u00fcck\u00b7lich", "?", "\u2013", "Gott", "be\u00b7h\u00fc\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PAV", "ADJD", "$.", "$(", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Jetzt mu\u00dft du das Gest\u00e4ndnis mir verzeihen:", "tokens": ["Jetzt", "mu\u00dft", "du", "das", "Ge\u00b7st\u00e4nd\u00b7nis", "mir", "ver\u00b7zei\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "O s\u00fc\u00dfe Sch\u00e4ferin, nur deine G\u00fcte", "tokens": ["O", "s\u00fc\u00b7\u00dfe", "Sch\u00e4\u00b7fe\u00b7rin", ",", "nur", "dei\u00b7ne", "G\u00fc\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "ADJA", "NN", "$,", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Kann meinen Sch\u00e4tzen wahren Wert verleihen;", "tokens": ["Kann", "mei\u00b7nen", "Sch\u00e4t\u00b7zen", "wah\u00b7ren", "Wert", "ver\u00b7lei\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Nimm alles, alles hin, Marilia, throne", "tokens": ["Nimm", "al\u00b7les", ",", "al\u00b7les", "hin", ",", "Ma\u00b7ri\u00b7lia", ",", "thro\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word"], "pos": ["NE", "PIS", "$,", "PIS", "PTKVZ", "$,", "NE", "$,", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Als Herrin \u00fcber Herden, Haus und Land;", "tokens": ["Als", "Her\u00b7rin", "\u00fc\u00b7ber", "Her\u00b7den", ",", "Haus", "und", "Land", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Sch\u00f6n ist der Reichtum, \u2013 doch f\u00fcr deine Hand", "tokens": ["Sch\u00f6n", "ist", "der", "Reich\u00b7tum", ",", "\u2013", "doch", "f\u00fcr", "dei\u00b7ne", "Hand"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "ART", "NN", "$,", "$(", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "G\u00e4b' ich mit Freuden eines K\u00f6nigs Krone.", "tokens": ["G\u00e4b'", "ich", "mit", "Freu\u00b7den", "ei\u00b7nes", "K\u00f6\u00b7nigs", "Kro\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "NN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Aus deinen Augen strahlt des Himmels Wonne,", "tokens": ["Aus", "dei\u00b7nen", "Au\u00b7gen", "strahlt", "des", "Him\u00b7mels", "Won\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie frischgefallner Schnee sind deine Wangen,", "tokens": ["Wie", "frischge\u00b7fall\u00b7ner", "Schnee", "sind", "dei\u00b7ne", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Die in der vollen Glut der Mittagssonne", "tokens": ["Die", "in", "der", "vol\u00b7len", "Glut", "der", "Mit\u00b7tags\u00b7son\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie junger Mohn, wie zarte Rosen prangen;", "tokens": ["Wie", "jun\u00b7ger", "Mohn", ",", "wie", "zar\u00b7te", "Ro\u00b7sen", "pran\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "$,", "PWAV", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Die Locken wie gedreht aus feinstem Golde,", "tokens": ["Die", "Lo\u00b7cken", "wie", "ge\u00b7dreht", "aus", "feins\u00b7tem", "Gol\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KOKOM", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Balsamisch duftet deine N\u00e4he \u2013 nie,", "tokens": ["Bal\u00b7sa\u00b7misch", "duf\u00b7tet", "dei\u00b7ne", "N\u00e4\u00b7he", "\u2013", "nie", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$(", "ADV", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.7": {"text": "Marilia, rief des Dichters Phantasie", "tokens": ["Ma\u00b7ri\u00b7lia", ",", "rief", "des", "Dich\u00b7ters", "Phan\u00b7ta\u00b7sie"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Ein Bild hervor wie deins, du Einzigholde.", "tokens": ["Ein", "Bild", "her\u00b7vor", "wie", "deins", ",", "du", "Ein\u00b7zig\u00b7hol\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "KOKOM", "NE", "$,", "PPER", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Wenn auch der Flu\u00df aus seinem Bette tr\u00e4te,", "tokens": ["Wenn", "auch", "der", "Flu\u00df", "aus", "sei\u00b7nem", "Bet\u00b7te", "tr\u00e4\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df meine wohlbestellte Saat verd\u00fcrbe;", "tokens": ["Da\u00df", "mei\u00b7ne", "wohl\u00b7be\u00b7stell\u00b7te", "Saat", "ver\u00b7d\u00fcr\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wenn sich die Pest bei mir zu Gaste b\u00e4te,", "tokens": ["Wenn", "sich", "die", "Pest", "bei", "mir", "zu", "Gas\u00b7te", "b\u00e4\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "APPR", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df mir ein Sch\u00e4fchen nach dem andern st\u00fcrbe \u2013", "tokens": ["Da\u00df", "mir", "ein", "Sch\u00e4f\u00b7chen", "nach", "dem", "an\u00b7dern", "st\u00fcr\u00b7be", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "ART", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Das w\u00fcrde jetzt mir wenig Sorgen machen;", "tokens": ["Das", "w\u00fcr\u00b7de", "jetzt", "mir", "we\u00b7nig", "Sor\u00b7gen", "ma\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "PPER", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und blendet mich der Glanz der St\u00e4dte? \u2013 Nein!", "tokens": ["Und", "blen\u00b7det", "mich", "der", "Glanz", "der", "St\u00e4d\u00b7te", "?", "\u2013", "Nein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "ART", "NN", "$.", "$(", "PTKANT", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Marilia, reich und gl\u00fccklich werd' ich sein,", "tokens": ["Ma\u00b7ri\u00b7lia", ",", "reich", "und", "gl\u00fcck\u00b7lich", "werd'", "ich", "sein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJD", "KON", "ADJD", "VAFIN", "PPER", "VAINF", "$,"], "meter": "---+-+-+-+", "measure": "zehnsilber"}, "line.8": {"text": "Wenn deine Augen mir entgegenlachen.", "tokens": ["Wenn", "dei\u00b7ne", "Au\u00b7gen", "mir", "ent\u00b7ge\u00b7gen\u00b7la\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Wie wird fortan, am Arme deines Gatten,", "tokens": ["Wie", "wird", "for\u00b7tan", ",", "am", "Ar\u00b7me", "dei\u00b7nes", "Gat\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ADV", "$,", "APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der W\u00e4lder Einsamkeit dein Herz erquicken!", "tokens": ["Der", "W\u00e4l\u00b7der", "Ein\u00b7sam\u00b7keit", "dein", "Herz", "er\u00b7qui\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zur Mittagsstunde mu\u00dft du mir gestatten,", "tokens": ["Zur", "Mit\u00b7tags\u00b7stun\u00b7de", "mu\u00dft", "du", "mir", "ge\u00b7stat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VMFIN", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In deinem Scho\u00dfe leise einzunicken.", "tokens": ["In", "dei\u00b7nem", "Scho\u00b7\u00dfe", "lei\u00b7se", "ein\u00b7zu\u00b7ni\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "An Lustbarkeiten, die ich gern vers\u00e4ume,", "tokens": ["An", "Lust\u00b7bar\u00b7kei\u00b7ten", ",", "die", "ich", "gern", "ver\u00b7s\u00e4u\u00b7me", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Erg\u00f6tze sich der Nachbarn wilde Schar \u2013", "tokens": ["Er\u00b7g\u00f6t\u00b7ze", "sich", "der", "Nach\u00b7barn", "wil\u00b7de", "Schar", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ich flechte Bl\u00fcmchen in dein blondes Haar", "tokens": ["Ich", "flech\u00b7te", "Bl\u00fcm\u00b7c