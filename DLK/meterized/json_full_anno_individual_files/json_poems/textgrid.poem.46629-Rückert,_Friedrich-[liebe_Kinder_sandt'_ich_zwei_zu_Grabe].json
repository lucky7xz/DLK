{"textgrid.poem.46629": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[liebe Kinder sandt' ich zwei zu Grabe]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Liebe Kinder sandt' ich zwei zu Grabe,", "tokens": ["Lie\u00b7be", "Kin\u00b7der", "sandt'", "ich", "zwei", "zu", "Gra\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "CARD", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Und mich selber schleppt' ich krank am Stabe,", "tokens": ["Und", "mich", "sel\u00b7ber", "schleppt'", "ich", "krank", "am", "Sta\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "VVFIN", "PPER", "ADJD", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Hielt mich wohl f\u00fcr einen armen Mann,", "tokens": ["Hielt", "mich", "wohl", "f\u00fcr", "ei\u00b7nen", "ar\u00b7men", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Einer sprach mich doch um eine Gabe,", "tokens": ["Ei\u00b7ner", "sprach", "mich", "doch", "um", "ei\u00b7ne", "Ga\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Der noch \u00e4rmer sein mu\u00df, an.", "tokens": ["Der", "noch", "\u00e4r\u00b7mer", "sein", "mu\u00df", ",", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"