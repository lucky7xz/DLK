{"dta.poem.10537": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "IiI.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "In dem Schlo\u00df zu Alkolea", "tokens": ["In", "dem", "Schlo\u00df", "zu", "Al\u00b7ko\u00b7lea"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist verschollen Lust und Klingen,", "tokens": ["Ist", "ver\u00b7schol\u00b7len", "Lust", "und", "Klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Herr'n und Damen sind verschwunden,", "tokens": ["Herr'n", "und", "Da\u00b7men", "sind", "ver\u00b7schwun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und erloschen sind die Lichter.", "tokens": ["Und", "er\u00b7lo\u00b7schen", "sind", "die", "Lich\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Donna Clara und Almansor", "tokens": ["Don\u00b7na", "Cla\u00b7ra", "und", "Al\u00b7man\u00b7sor"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "NE", "KON", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Sind allein im Saal geblieben;", "tokens": ["Sind", "al\u00b7lein", "im", "Saal", "ge\u00b7blie\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPRART", "NN", "VVPP", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Einsam streut die letzte Lampe", "tokens": ["Ein\u00b7sam", "streut", "die", "letz\u00b7te", "Lam\u00b7pe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ueber beide ihren Schimmer.", "tokens": ["Ue\u00b7ber", "bei\u00b7de", "ih\u00b7ren", "Schim\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Auf dem Sessel sitzt die Dame,", "tokens": ["Auf", "dem", "Ses\u00b7sel", "sitzt", "die", "Da\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf dem Schemel sitzt der Ritter,", "tokens": ["Auf", "dem", "Sche\u00b7mel", "sitzt", "der", "Rit\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und sein Haupt, das schlummerm\u00fcde,", "tokens": ["Und", "sein", "Haupt", ",", "das", "schlum\u00b7mer\u00b7m\u00fc\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "PRELS", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ruht auf den geliebten Knieen.", "tokens": ["Ruht", "auf", "den", "ge\u00b7lieb\u00b7ten", "Kni\u00b7e\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+--", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Rosen\u00f6hl, aus gold'nen Fl\u00e4schchen,", "tokens": ["Ro\u00b7se\u00b7n\u00f6hl", ",", "aus", "gold'\u00b7nen", "Fl\u00e4\u00b7schchen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gie\u00dft die Dame, sorgsam sinnend,", "tokens": ["Gie\u00dft", "die", "Da\u00b7me", ",", "sorg\u00b7sam", "sin\u00b7nend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Auf Almansors braune Locken \u2014", "tokens": ["Auf", "Al\u00b7man\u00b7sors", "brau\u00b7ne", "Lo\u00b7cken"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADJA", "NN", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und er seufzt aus Herzenstiefe.", "tokens": ["Und", "er", "seufzt", "aus", "Her\u00b7zen\u00b7stie\u00b7fe", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "S\u00fc\u00dfen Ku\u00df, mit sanftem Munde,", "tokens": ["S\u00fc\u00b7\u00dfen", "Ku\u00df", ",", "mit", "sanf\u00b7tem", "Mun\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dr\u00fcckt die Dame, sorgsam sinnend,", "tokens": ["Dr\u00fcckt", "die", "Da\u00b7me", ",", "sorg\u00b7sam", "sin\u00b7nend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Auf Almansors braune Locken \u2014", "tokens": ["Auf", "Al\u00b7man\u00b7sors", "brau\u00b7ne", "Lo\u00b7cken"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADJA", "NN", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und es w\u00f6lkt sich seine Stirne.", "tokens": ["Und", "es", "w\u00f6lkt", "sich", "sei\u00b7ne", "Stir\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PRF", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Thr\u00e4nenfluth, aus lichten Augen,", "tokens": ["Thr\u00e4\u00b7nen\u00b7fluth", ",", "aus", "lich\u00b7ten", "Au\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Weint die Dame, sorgsam sinnend,", "tokens": ["Weint", "die", "Da\u00b7me", ",", "sorg\u00b7sam", "sin\u00b7nend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Auf Almansors braune Locken \u2014", "tokens": ["Auf", "Al\u00b7man\u00b7sors", "brau\u00b7ne", "Lo\u00b7cken"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADJA", "NN", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und es zuckt um seine Lippen.", "tokens": ["Und", "es", "zuckt", "um", "sei\u00b7ne", "Lip\u00b7pen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Und er tr\u00e4umt: er stehe wieder,", "tokens": ["Und", "er", "tr\u00e4umt", ":", "er", "ste\u00b7he", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$.", "PPER", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tief das Haupt gebeugt und triefend,", "tokens": ["Tief", "das", "Haupt", "ge\u00b7beugt", "und", "trie\u00b7fend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "VVPP", "KON", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "In dem Dom zu Corduva,", "tokens": ["In", "dem", "Dom", "zu", "Cor\u00b7du\u00b7va", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und er h\u00f6rt' viel dunkle Stimmen.", "tokens": ["Und", "er", "h\u00f6rt'", "viel", "dunk\u00b7le", "Stim\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PIAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "All die hohen Riesens\u00e4ulen", "tokens": ["All", "die", "ho\u00b7hen", "Rie\u00b7sen\u00b7s\u00e4u\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "H\u00f6rt er murmeln unmuthgrimmig,", "tokens": ["H\u00f6rt", "er", "mur\u00b7meln", "un\u00b7muth\u00b7grim\u00b7mig", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "L\u00e4nger wollen sie's nicht tragen,", "tokens": ["L\u00e4n\u00b7ger", "wol\u00b7len", "sie's", "nicht", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PIS", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und sie wanken und sie zittern;", "tokens": ["Und", "sie", "wan\u00b7ken", "und", "sie", "zit\u00b7tern", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVINF", "KON", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Und sie brechen wild zusammen,", "tokens": ["Und", "sie", "bre\u00b7chen", "wild", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Es erbleichen Volk und Priester,", "tokens": ["Es", "er\u00b7blei\u00b7chen", "Volk", "und", "Pries\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Krachend st\u00fcrzt herab die Kuppel,", "tokens": ["Kra\u00b7chend", "st\u00fcrzt", "her\u00b7ab", "die", "Kup\u00b7pel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die Christeng\u00f6tter wimmern.", "tokens": ["Und", "die", "Chris\u00b7ten\u00b7g\u00f6t\u00b7ter", "wim\u00b7mern", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}