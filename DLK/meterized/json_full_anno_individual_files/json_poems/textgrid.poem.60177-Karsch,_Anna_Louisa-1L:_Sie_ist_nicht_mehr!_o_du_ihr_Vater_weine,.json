{"textgrid.poem.60177": {"metadata": {"author": {"name": "Karsch, Anna Louisa", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sie ist nicht mehr! o du ihr Vater weine,", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie ist nicht mehr! o du ihr Vater weine,", "tokens": ["Sie", "ist", "nicht", "mehr", "!", "o", "du", "ihr", "Va\u00b7ter", "wei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADV", "$.", "FM", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auf jenen Ueberrest entseeleter Gebeine,", "tokens": ["Auf", "je\u00b7nen", "Ue\u00b7ber\u00b7rest", "ent\u00b7see\u00b7le\u00b7ter", "Ge\u00b7bei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dein in dich dringend Leid!", "tokens": ["Dein", "in", "dich", "drin\u00b7gend", "Leid", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "APPR", "PRF", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Nichts half die Kunst, und nichts da\u00df du gerungen", "tokens": ["Nichts", "half", "die", "Kunst", ",", "und", "nichts", "da\u00df", "du", "ge\u00b7run\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "ART", "NN", "$,", "KON", "PIS", "KOUS", "PPER", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Hast im Gebet, sie ging auf gr\u00f6\u00dfre Foderungen", "tokens": ["Hast", "im", "Ge\u00b7bet", ",", "sie", "ging", "auf", "gr\u00f6\u00df\u00b7re", "Fo\u00b7de\u00b7run\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPRART", "NN", "$,", "PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Hin in die Ewigkeit!", "tokens": ["Hin", "in", "die", "E\u00b7wig\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Da wird sie sich mit jenem Geiste k\u00fcssen", "tokens": ["Da", "wird", "sie", "sich", "mit", "je\u00b7nem", "Geis\u00b7te", "k\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "APPR", "PDAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der deine Liebe hat mit sich dahin gerissen", "tokens": ["Der", "dei\u00b7ne", "Lie\u00b7be", "hat", "mit", "sich", "da\u00b7hin", "ge\u00b7ris\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPOSAT", "NN", "VAFIN", "APPR", "PRF", "PAV", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wo nichts, als Liebe lebt!", "tokens": ["Wo", "nichts", ",", "als", "Lie\u00b7be", "lebt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "$,", "KOUS", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Da wird sie nun im Schoo\u00df der Mutter liegen", "tokens": ["Da", "wird", "sie", "nun", "im", "Schoo\u00df", "der", "Mut\u00b7ter", "lie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPRART", "NN", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und ihr erz\u00e4hlen, wie dein einziges Vergn\u00fcgen,", "tokens": ["Und", "ihr", "er\u00b7z\u00e4h\u00b7len", ",", "wie", "dein", "ein\u00b7zi\u00b7ges", "Ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVINF", "$,", "PWAV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Ihr Schatten, um dich schwebt!", "tokens": ["Ihr", "Schat\u00b7ten", ",", "um", "dich", "schwebt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "KOUI", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Sie wird ihr sagen, wie du dich vergessen,", "tokens": ["Sie", "wird", "ihr", "sa\u00b7gen", ",", "wie", "du", "dich", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVINF", "$,", "PWAV", "PPER", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Oft Stunden lang bey ihr am Sterbebett gesessen,", "tokens": ["Oft", "Stun\u00b7den", "lang", "bey", "ihr", "am", "Ster\u00b7be\u00b7bett", "ge\u00b7ses\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "ADJD", "APPR", "PPOSAT", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und ihren Schmerz beklagt,", "tokens": ["Und", "ih\u00b7ren", "Schmerz", "be\u00b7klagt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und wie du sie, wenn sie voll Schmerzen st\u00f6hnte", "tokens": ["Und", "wie", "du", "sie", ",", "wenn", "sie", "voll", "Schmer\u00b7zen", "st\u00f6hn\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "PPER", "$,", "KOUS", "PPER", "ADJD", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Nach ihrer W\u00fcnsche Ziel, nach welchem sie sich sehnte,", "tokens": ["Nach", "ih\u00b7rer", "W\u00fcn\u00b7sche", "Ziel", ",", "nach", "wel\u00b7chem", "sie", "sich", "sehn\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,", "APPR", "PRELS", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So z\u00e4rtlich hast gefragt.", "tokens": ["So", "z\u00e4rt\u00b7lich", "hast", "ge\u00b7fragt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Freund, klage nicht, nein singe Lobges\u00e4nge;", "tokens": ["Freund", ",", "kla\u00b7ge", "nicht", ",", "nein", "sin\u00b7ge", "Lob\u00b7ge\u00b7s\u00e4n\u00b7ge", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PTKNEG", "$,", "PTKANT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Denn auf ihr lag zu schwer mit ungeheurer Menge", "tokens": ["Denn", "auf", "ihr", "lag", "zu", "schwer", "mit", "un\u00b7ge\u00b7heu\u00b7rer", "Men\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "VVFIN", "PTKA", "ADJD", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Von Qualen schon der Tod!", "tokens": ["Von", "Qua\u00b7len", "schon", "der", "Tod", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Er sa\u00df in eingefallnen blassen Wangen,", "tokens": ["Er", "sa\u00df", "in", "ein\u00b7ge\u00b7fall\u00b7nen", "blas\u00b7sen", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und war der Frost des Fiebers nun vergangen", "tokens": ["Und", "war", "der", "Frost", "des", "Fie\u00b7bers", "nun", "ver\u00b7gan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ART", "NN", "ART", "NN", "ADV", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "So gl\u00fcht' er in dem Roth.", "tokens": ["So", "gl\u00fcht'", "er", "in", "dem", "Roth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Nicht unerwartet kam er sie zu holen;", "tokens": ["Nicht", "un\u00b7er\u00b7war\u00b7tet", "kam", "er", "sie", "zu", "ho\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "VVFIN", "PPER", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nein, lange schon ward dir, wann er gedroht, befohlen:", "tokens": ["Nein", ",", "lan\u00b7ge", "schon", "ward", "dir", ",", "wann", "er", "ge\u00b7droht", ",", "be\u00b7foh\u00b7len", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "ADV", "VAFIN", "PPER", "$,", "PWAV", "PPER", "VVPP", "$,", "VVPP", "$."], "meter": "-+--+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Bereite dich! sey stark!", "tokens": ["Be\u00b7rei\u00b7te", "dich", "!", "sey", "stark", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$.", "VAFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "So sey auch nun ein Mann in deiner Klage,", "tokens": ["So", "sey", "auch", "nun", "ein", "Mann", "in", "dei\u00b7ner", "Kla\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Dein Kind ruht jetzt, und ihrer Krankheit Plage,", "tokens": ["Dein", "Kind", "ruht", "jetzt", ",", "und", "ih\u00b7rer", "Krank\u00b7heit", "Pla\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "$,", "KON", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Bleibt diesseits vor dem Sarg.", "tokens": ["Bleibt", "dies\u00b7seits", "vor", "dem", "Sarg", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Ihr Witz, der hier noch unenth\u00fcllt geblieben,", "tokens": ["Ihr", "Witz", ",", "der", "hier", "noch", "un\u00b7ent\u00b7h\u00fcllt", "ge\u00b7blie\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "ADV", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wird dort sich in dem Buch der h\u00f6chsten Weisheit \u00fcben,", "tokens": ["Wird", "dort", "sich", "in", "dem", "Buch", "der", "h\u00f6chs\u00b7ten", "Weis\u00b7heit", "\u00fc\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PRF", "APPR", "ART", "NN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wird keiner Zeiten Raub;", "tokens": ["Wird", "kei\u00b7ner", "Zei\u00b7ten", "Raub", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und kommt einmal der Herr, den Erdcrey\u00df richten.", "tokens": ["Und", "kommt", "ein\u00b7mal", "der", "Herr", ",", "den", "Erd\u00b7crey\u00df", "rich\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "$,", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Dann weckt ein Engel sie zu ewgen Pflichten", "tokens": ["Dann", "weckt", "ein", "En\u00b7gel", "sie", "zu", "ew\u00b7gen", "Pflich\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Des Dankes aus dem Staub!", "tokens": ["Des", "Dan\u00b7kes", "aus", "dem", "Staub", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Aus einem Staube der sie wieder giebet;", "tokens": ["Aus", "ei\u00b7nem", "Stau\u00b7be", "der", "sie", "wie\u00b7der", "gie\u00b7bet", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dann siehst du das, o Freund! was du an ihr geliebet", "tokens": ["Dann", "siehst", "du", "das", ",", "o", "Freund", "!", "was", "du", "an", "ihr", "ge\u00b7lie\u00b7bet"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PDS", "$,", "FM", "NN", "$.", "PWS", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Vollkommner reizend seyn;", "tokens": ["Voll\u00b7komm\u00b7ner", "rei\u00b7zend", "seyn", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVPP", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Itzt fragst du: o warum ist sie geschieden?", "tokens": ["Itzt", "fragst", "du", ":", "o", "wa\u00b7rum", "ist", "sie", "ge\u00b7schie\u00b7den", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "FM", "PWAV", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Erwarte nur den Uebergang zum Frieden", "tokens": ["Er\u00b7war\u00b7te", "nur", "den", "Ue\u00b7ber\u00b7gang", "zum", "Frie\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Dann leuchtet dir es ein!", "tokens": ["Dann", "leuch\u00b7tet", "dir", "es", "ein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Hier in der Welt voll Unruh, voller Sorgen", "tokens": ["Hier", "in", "der", "Welt", "voll", "Un\u00b7ruh", ",", "vol\u00b7ler", "Sor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "ADJD", "NN", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bleibts vor dem tr\u00fcben Blick des Sterblichen verborgen,", "tokens": ["Bleibts", "vor", "dem", "tr\u00fc\u00b7ben", "Blick", "des", "Sterb\u00b7li\u00b7chen", "ver\u00b7bor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Warum Gott so verf\u00e4hrt;", "tokens": ["Wa\u00b7rum", "Gott", "so", "ver\u00b7f\u00e4hrt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADV", "VVPP", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Dort aber, wo vor hundert tausend Sonnen", "tokens": ["Dort", "a\u00b7ber", ",", "wo", "vor", "hun\u00b7dert", "tau\u00b7send", "Son\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "$,", "PWAV", "APPR", "CARD", "CARD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Die Finsterni\u00df nie einen Sitz gewonnen,", "tokens": ["Die", "Fins\u00b7ter\u00b7ni\u00df", "nie", "ei\u00b7nen", "Sitz", "ge\u00b7won\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ist alles aufgekl\u00e4rt.", "tokens": ["Ist", "al\u00b7les", "auf\u00b7ge\u00b7kl\u00e4rt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Sie ist nicht mehr! o du ihr Vater weine,", "tokens": ["Sie", "ist", "nicht", "mehr", "!", "o", "du", "ihr", "Va\u00b7ter", "wei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADV", "$.", "FM", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auf jenen Ueberrest entseeleter Gebeine,", "tokens": ["Auf", "je\u00b7nen", "Ue\u00b7ber\u00b7rest", "ent\u00b7see\u00b7le\u00b7ter", "Ge\u00b7bei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dein in dich dringend Leid!", "tokens": ["Dein", "in", "dich", "drin\u00b7gend", "Leid", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "APPR", "PRF", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Nichts half die Kunst, und nichts da\u00df du gerungen", "tokens": ["Nichts", "half", "die", "Kunst", ",", "und", "nichts", "da\u00df", "du", "ge\u00b7run\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "ART", "NN", "$,", "KON", "PIS", "KOUS", "PPER", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Hast im Gebet, sie ging auf gr\u00f6\u00dfre Foderungen", "tokens": ["Hast", "im", "Ge\u00b7bet", ",", "sie", "ging", "auf", "gr\u00f6\u00df\u00b7re", "Fo\u00b7de\u00b7run\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPRART", "NN", "$,", "PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Hin in die Ewigkeit!", "tokens": ["Hin", "in", "die", "E\u00b7wig\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Da wird sie sich mit jenem Geiste k\u00fcssen", "tokens": ["Da", "wird", "sie", "sich", "mit", "je\u00b7nem", "Geis\u00b7te", "k\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "APPR", "PDAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der deine Liebe hat mit sich dahin gerissen", "tokens": ["Der", "dei\u00b7ne", "Lie\u00b7be", "hat", "mit", "sich", "da\u00b7hin", "ge\u00b7ris\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPOSAT", "NN", "VAFIN", "APPR", "PRF", "PAV", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wo nichts, als Liebe lebt!", "tokens": ["Wo", "nichts", ",", "als", "Lie\u00b7be", "lebt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "$,", "KOUS", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Da wird sie nun im Schoo\u00df der Mutter liegen", "tokens": ["Da", "wird", "sie", "nun", "im", "Schoo\u00df", "der", "Mut\u00b7ter", "lie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPRART", "NN", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und ihr erz\u00e4hlen, wie dein einziges Vergn\u00fcgen,", "tokens": ["Und", "ihr", "er\u00b7z\u00e4h\u00b7len", ",", "wie", "dein", "ein\u00b7zi\u00b7ges", "Ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVINF", "$,", "PWAV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Ihr Schatten, um dich schwebt!", "tokens": ["Ihr", "Schat\u00b7ten", ",", "um", "dich", "schwebt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "KOUI", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Sie wird ihr sagen, wie du dich vergessen,", "tokens": ["Sie", "wird", "ihr", "sa\u00b7gen", ",", "wie", "du", "dich", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVINF", "$,", "PWAV", "PPER", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Oft Stunden lang bey ihr am Sterbebett gesessen,", "tokens": ["Oft", "Stun\u00b7den", "lang", "bey", "ihr", "am", "Ster\u00b7be\u00b7bett", "ge\u00b7ses\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "ADJD", "APPR", "PPOSAT", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und ihren Schmerz beklagt,", "tokens": ["Und", "ih\u00b7ren", "Schmerz", "be\u00b7klagt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und wie du sie, wenn sie voll Schmerzen st\u00f6hnte", "tokens": ["Und", "wie", "du", "sie", ",", "wenn", "sie", "voll", "Schmer\u00b7zen", "st\u00f6hn\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "PPER", "$,", "KOUS", "PPER", "ADJD", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Nach ihrer W\u00fcnsche Ziel, nach welchem sie sich sehnte,", "tokens": ["Nach", "ih\u00b7rer", "W\u00fcn\u00b7sche", "Ziel", ",", "nach", "wel\u00b7chem", "sie", "sich", "sehn\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,", "APPR", "PRELS", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So z\u00e4rtlich hast gefragt.", "tokens": ["So", "z\u00e4rt\u00b7lich", "hast", "ge\u00b7fragt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Freund, klage nicht, nein singe Lobges\u00e4nge;", "tokens": ["Freund", ",", "kla\u00b7ge", "nicht", ",", "nein", "sin\u00b7ge", "Lob\u00b7ge\u00b7s\u00e4n\u00b7ge", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PTKNEG", "$,", "PTKANT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Denn auf ihr lag zu schwer mit ungeheurer Menge", "tokens": ["Denn", "auf", "ihr", "lag", "zu", "schwer", "mit", "un\u00b7ge\u00b7heu\u00b7rer", "Men\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "VVFIN", "PTKA", "ADJD", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Von Qualen schon der Tod!", "tokens": ["Von", "Qua\u00b7len", "schon", "der", "Tod", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Er sa\u00df in eingefallnen blassen Wangen,", "tokens": ["Er", "sa\u00df", "in", "ein\u00b7ge\u00b7fall\u00b7nen", "blas\u00b7sen", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und war der Frost des Fiebers nun vergangen", "tokens": ["Und", "war", "der", "Frost", "des", "Fie\u00b7bers", "nun", "ver\u00b7gan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ART", "NN", "ART", "NN", "ADV", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "So gl\u00fcht' er in dem Roth.", "tokens": ["So", "gl\u00fcht'", "er", "in", "dem", "Roth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Nicht unerwartet kam er sie zu holen;", "tokens": ["Nicht", "un\u00b7er\u00b7war\u00b7tet", "kam", "er", "sie", "zu", "ho\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "VVFIN", "PPER", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nein, lange schon ward dir, wann er gedroht, befohlen:", "tokens": ["Nein", ",", "lan\u00b7ge", "schon", "ward", "dir", ",", "wann", "er", "ge\u00b7droht", ",", "be\u00b7foh\u00b7len", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "ADV", "VAFIN", "PPER", "$,", "PWAV", "PPER", "VVPP", "$,", "VVPP", "$."], "meter": "-+--+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Bereite dich! sey stark!", "tokens": ["Be\u00b7rei\u00b7te", "dich", "!", "sey", "stark", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$.", "VAFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "So sey auch nun ein Mann in deiner Klage,", "tokens": ["So", "sey", "auch", "nun", "ein", "Mann", "in", "dei\u00b7ner", "Kla\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN