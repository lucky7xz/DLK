{"textgrid.poem.41703": {"metadata": {"author": {"name": "Otto, Louise", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O haltet mich mit Bitten nicht zur\u00fccke", "tokens": ["O", "hal\u00b7tet", "mich", "mit", "Bit\u00b7ten", "nicht", "zu\u00b7r\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PRF", "APPR", "NN", "PTKNEG", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn ich im Sehnen nach der Freiheit Lichte", "tokens": ["Wenn", "ich", "im", "Seh\u00b7nen", "nach", "der", "Frei\u00b7heit", "Lich\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "NN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zu hohen Zielen meine Blicke richte,", "tokens": ["Zu", "ho\u00b7hen", "Zie\u00b7len", "mei\u00b7ne", "Bli\u00b7cke", "rich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Von keinem Gl\u00fcck wei\u00df als vom V\u00f6lkergl\u00fccke.", "tokens": ["Von", "kei\u00b7nem", "Gl\u00fcck", "wei\u00df", "als", "vom", "V\u00f6l\u00b7ker\u00b7gl\u00fc\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "KOKOM", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Mir ward einmal die Weisung vom Geschicke", "tokens": ["Mir", "ward", "ein\u00b7mal", "die", "Wei\u00b7sung", "vom", "Ge\u00b7schi\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word", "w