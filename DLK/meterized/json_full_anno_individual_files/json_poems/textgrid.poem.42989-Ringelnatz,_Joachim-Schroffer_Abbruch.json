{"textgrid.poem.42989": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "Schroffer Abbruch", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "La\u00df mich doch allein,", "tokens": ["La\u00df", "mich", "doch", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ADV", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Bitte, bitte!", "tokens": ["Bit\u00b7te", ",", "bit\u00b7te", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["PTKANT", "$,", "PTKANT", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Meine Schritte", "tokens": ["Mei\u00b7ne", "Schrit\u00b7te"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Sind deinen zu klein.", "tokens": ["Sind", "dei\u00b7nen", "zu", "klein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "PTKA", "ADJD", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Merkst du denn nicht,", "tokens": ["Merkst", "du", "denn", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PTKNEG", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Was h\u00f6fliche Worte sind?", "tokens": ["Was", "h\u00f6f\u00b7li\u00b7che", "Wor\u00b7te", "sind", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJA", "NN", "VAFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Deine Blicke stellen sich blind.", "tokens": ["Dei\u00b7ne", "Bli\u00b7cke", "stel\u00b7len", "sich", "blind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VV