_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das hier geathmet und geblickt nach oben.", "tokens": ["Das", "hier", "ge\u00b7ath\u00b7met", "und", "ge\u00b7blickt", "nach", "o\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "VVPP", "KON", "VVFIN", "APPR", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Mein eig'nes Ich, mit tiefgeheimem Beben,", "tokens": ["Mein", "eig'\u00b7nes", "Ich", ",", "mit", "tief\u00b7ge\u00b7hei\u00b7mem", "Be\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "PPER", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Seh' ich zur Welt erweitert und erhoben \u2013", "tokens": ["Seh'", "ich", "zur", "Welt", "er\u00b7wei\u00b7tert", "und", "er\u00b7ho\u00b7ben", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "VVPP", "KON", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und mit ihr, wie ein Traum, in Nichts verschweben.", "tokens": ["Und", "mit", "ihr", ",", "wie", "ein", "Traum", ",", "in", "Nichts", "ver\u00b7schwe\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "$,", "PWAV", "ART", "NN", "$,", "APPR", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Oft ist es mir, als s\u00e4h' ich niedergleiten", "tokens": ["Oft", "ist", "es", "mir", ",", "als", "s\u00e4h'", "ich", "nie\u00b7derg\u00b7lei\u00b7ten"], "token_info": ["word", "word", "word", "w