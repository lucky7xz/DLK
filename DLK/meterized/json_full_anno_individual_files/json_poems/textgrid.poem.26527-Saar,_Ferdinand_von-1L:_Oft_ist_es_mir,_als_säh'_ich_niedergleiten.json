{"textgrid.poem.26527": {"metadata": {"author": {"name": "Saar, Ferdinand von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Oft ist es mir, als s\u00e4h' ich niedergleiten", "genre": "verse", "period": "N.A.", "pub_year": 1869, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Oft ist es mir, als s\u00e4h' ich niedergleiten", "tokens": ["Oft", "ist", "es", "mir", ",", "als", "s\u00e4h'", "ich", "nie\u00b7derg\u00b7lei\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "$,", "KOUS", "VVFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Schleier still und leise vor den Dingen;", "tokens": ["Die", "Schlei\u00b7er", "still", "und", "lei\u00b7se", "vor", "den", "Din\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "KON", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mein Auge kann das weite All durchdringen", "tokens": ["Mein", "Au\u00b7ge", "kann", "das", "wei\u00b7te", "All", "durch\u00b7drin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VMFIN", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und blickt zur\u00fcck zum Urquell aller Zeiten.", "tokens": ["Und", "blickt", "zu\u00b7r\u00fcck", "zum", "Ur\u00b7quell", "al\u00b7ler", "Zei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "APPRART", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ich sehe, wie die F\u00e4den sich bereiten,", "tokens": ["Ich", "se\u00b7he", ",", "wie", "die", "F\u00e4\u00b7den", 