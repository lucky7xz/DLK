{"dta.poem.9288": {"metadata": {"author": {"name": "Weise, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Das vierdte Gespr\u00e4ch.", "genre": "Lyrik; Drama; Prosa", "period": "N.A.", "pub_year": "1701", "urn": "urn:nbn:de:kobv:b4-25043-0", "language": ["de:0.99"], "booktitle": "Weise, Christian: \u00dcberfl\u00fc\u00dfige Gedancken Der gr\u00fcnenden jugend. Leipzig, 1701."}, "poem": {"stanza.1": {"line.1": {"text": "Weil das angenehme gl\u00fccke", "tokens": ["Weil", "das", "an\u00b7ge\u00b7neh\u00b7me", "gl\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mich zu einen schuldner macht/", "tokens": ["Mich", "zu", "ei\u00b7nen", "schuld\u00b7ner", "macht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "ADJA", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hab ich auch in diesem st\u00fccke/", "tokens": ["Hab", "ich", "auch", "in", "die\u00b7sem", "st\u00fc\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "APPR", "PDAT", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Meine treue pflicht bedacht/", "tokens": ["Mei\u00b7ne", "treu\u00b7e", "pflicht", "be\u00b7dacht", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVPP", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Welche durch den s\u00fcssen rath", "tokens": ["Wel\u00b7che", "durch", "den", "s\u00fcs\u00b7sen", "rath"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAT", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Jhre sch\u00f6ne richtschnur hat.", "tokens": ["Ih\u00b7re", "sch\u00f6\u00b7ne", "richt\u00b7schnur", "hat", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "2. Hier gibt sich die pomerantze/", "tokens": ["Hier", "gibt", "sich", "die", "po\u00b7me\u00b7rant\u00b7ze", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Hier ein schlechtes str\u00e4u\u00dfgen an/", "tokens": ["Hier", "ein", "schlech\u00b7tes", "str\u00e4u\u00df\u00b7gen", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "VVFIN", "PTKVZ", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Weil ich solches ihrem glantze", "tokens": ["Weil", "ich", "sol\u00b7ches", "ih\u00b7rem", "glant\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PIAT", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nach belieben bringen kan/", "tokens": ["Nach", "be\u00b7lie\u00b7ben", "brin\u00b7gen", "kan", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "VVINF", "VVINF", "VMFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Denn mein hertze sagt mir zu/", "tokens": ["Denn", "mein", "hert\u00b7ze", "sagt", "mir", "zu", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PPER", "PTKZU", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Da\u00df ich ihren willen thu.", "tokens": ["Da\u00df", "ich", "ih\u00b7ren", "wil\u00b7len", "thu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "3. Eins ist rund das andre spitzig/", "tokens": ["Eins", "ist", "rund", "das", "and\u00b7re", "spit\u00b7zig", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADJD", "ART", "ADJA", "ADJD", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Eins ist frisch das andre nicht/", "tokens": ["Eins", "ist", "frisch", "das", "and\u00b7re", "nicht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADJD", "ART", "ADJA", "PTKNEG", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Eins ist kalt das andre hitzig.", "tokens": ["Eins", "ist", "kalt", "das", "and\u00b7re", "hit\u00b7zig", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADJD", "ART", "ADJA", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Also mu\u00df auch meine pflicht/", "tokens": ["Al\u00b7so", "mu\u00df", "auch", "mei\u00b7ne", "pflicht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ADV", "PPOSAT", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Uber ihren zarten schein/", "tokens": ["U\u00b7ber", "ih\u00b7ren", "zar\u00b7ten", "schein", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Halb erfreut/ halb traurig seyn.", "tokens": ["Halb", "er\u00b7freut", "/", "halb", "trau\u00b7rig", "seyn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$(", "ADJD", "ADJD", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "4. Bald versprechen die gedancken", "tokens": ["Bald", "ver\u00b7spre\u00b7chen", "die", "ge\u00b7dan\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mir die h\u00f6chste freundligkeit:", "tokens": ["Mir", "die", "h\u00f6chs\u00b7te", "freund\u00b7lig\u00b7keit", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bald gerath ich in das wancken/", "tokens": ["Bald", "ge\u00b7rath", "ich", "in", "das", "wan\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Weil ich die zufriedenheit", "tokens": ["Weil", "ich", "die", "zu\u00b7frie\u00b7den\u00b7heit"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Gar zu selten auff der bahn", "tokens": ["Gar", "zu", "sel\u00b7ten", "auff", "der", "bahn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PTKA", "ADJD", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Jhrer freundschafft finden kan.", "tokens": ["Ih\u00b7rer", "freund\u00b7schafft", "fin\u00b7den", "kan", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "5. Nun sie nehme di\u00df geschencke/", "tokens": ["Nun", "sie", "neh\u00b7me", "di\u00df", "ge\u00b7schen\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "PDS", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Welches sie erwehlet hat:", "tokens": ["Wel\u00b7ches", "sie", "er\u00b7weh\u00b7let", "hat", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sie belieb\u2019 es/ und gedencke", "tokens": ["Sie", "be\u00b7lieb'", "es", "/", "und", "ge\u00b7den\u00b7cke"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$(", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Meiner an desselben statt/", "tokens": ["Mei\u00b7ner", "an", "des\u00b7sel\u00b7ben", "statt", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "APPR", "PDS", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Bi\u00df ihr rath-schlu\u00df ordre gibt/", "tokens": ["Bi\u00df", "ihr", "ra\u00b7th\u00b7schlu\u00df", "ord\u00b7re", "gibt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df ihr etwas mehr beliebt.", "tokens": ["Da\u00df", "ihr", "et\u00b7was", "mehr", "be\u00b7liebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}