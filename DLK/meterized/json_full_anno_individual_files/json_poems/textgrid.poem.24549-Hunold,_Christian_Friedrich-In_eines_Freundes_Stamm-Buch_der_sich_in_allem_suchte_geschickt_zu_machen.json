{"textgrid.poem.24549": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "In eines Freundes Stamm-Buch/ der sich in allem suchte geschickt zu machen", "genre": "verse", "period": "N.A.", "pub_year": 1701, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In alle S\u00e4ttel mu\u00df ein kluger Mensch sich schicken/", "tokens": ["In", "al\u00b7le", "S\u00e4t\u00b7tel", "mu\u00df", "ein", "klu\u00b7ger", "Mensch", "sich", "schi\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VMFIN", "ART", "ADJA", "NN", "PRF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn er die Bahn der Welt geschickt beschreiten will.", "tokens": ["Wenn", "er", "die", "Bahn", "der", "Welt", "ge\u00b7schickt", "be\u00b7schrei\u00b7ten", "will", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "NN", "VVPP", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die B\u00fccher mu\u00df sein Flei\u00df vor allen Dingen dr\u00fccken/", "tokens": ["Die", "B\u00fc\u00b7cher", "mu\u00df", "sein", "Flei\u00df", "vor", "al\u00b7len", "Din\u00b7gen", "dr\u00fc\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPOSAT", "NN", "APPR", "PIAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Doch in ", "tokens": ["Doch", "in"], "token_info": ["word", "word"], "pos": ["KON", "APPR"], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "Ein wohlgewandtes Pferd besteigt er auch darneben/", "tokens": ["Ein", "wohl\u00b7ge\u00b7wand\u00b7tes", "Pferd", "be\u00b7steigt", "er", "auch", "dar\u00b7ne\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ADV", "PAV", "$("], "meter": "-+-+-+-+--+--", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Und bringt durch ein ", "tokens": ["Und", "bringt", "durch", "ein"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Auch einen Zierath wei\u00df das Tantzen ihm zu geben/", "tokens": ["Auch", "ei\u00b7nen", "Zie\u00b7rath", "wei\u00df", "das", "Tant\u00b7zen", "ihm", "zu", "ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "PDS", "VVFIN", "PPER", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Denn thut er in die Welt durch Reisen einen Blick.", "tokens": ["Denn", "thut", "er", "in", "die", "Welt", "durch", "Rei\u00b7sen", "ei\u00b7nen", "Blick", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "APPR", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "So wird der Kopf gescheut/ der Leib geschm\u00fcckt von Gaben/", "tokens": ["So", "wird", "der", "Kopf", "ge\u00b7scheut", "/", "der", "Leib", "ge\u00b7schm\u00fcckt", "von", "Ga\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVPP", "$(", "ART", "NN", "VVPP", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Weil die ", "tokens": ["Weil", "die"], "token_info": ["word", "word"], "pos": ["KOUS", "A