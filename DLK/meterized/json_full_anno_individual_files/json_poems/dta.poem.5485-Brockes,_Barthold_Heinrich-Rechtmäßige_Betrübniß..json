{"dta.poem.5485": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Rechtm\u00e4\u00dfige Betr\u00fcbni\u00df.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Aus einem tieffen Schlaf war ich an einem Morgen,", "tokens": ["Aus", "ei\u00b7nem", "tief\u00b7fen", "Schlaf", "war", "ich", "an", "ei\u00b7nem", "Mor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VAFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie es schon ziemlich spat, erwacht;", "tokens": ["Wie", "es", "schon", "ziem\u00b7lich", "spat", ",", "er\u00b7wacht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADV", "VVFIN", "$,", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es hielte mich des Vorhangs falsche Nacht", "tokens": ["Es", "hiel\u00b7te", "mich", "des", "Vor\u00b7hangs", "fal\u00b7sche", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wie schon die rechte Nacht vorbey, annoch verborgen:", "tokens": ["Wie", "schon", "die", "rech\u00b7te", "Nacht", "vor\u00b7bey", ",", "an\u00b7noch", "ver\u00b7bor\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "ADJA", "NN", "PTKVZ", "$,", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Als ich, noch halb verwirrt durch einen schweren Traum,", "tokens": ["Als", "ich", ",", "noch", "halb", "ver\u00b7wirrt", "durch", "ei\u00b7nen", "schwe\u00b7ren", "Traum", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "ADV", "ADJD", "ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Den gr\u00fcnen Vorhang schnell zur\u00fccke,", "tokens": ["Den", "gr\u00fc\u00b7nen", "Vor\u00b7hang", "schnell", "zu\u00b7r\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Die Augen aufw\u00e4rts, schlug: gleich traf die tr\u00e4gen Blicke", "tokens": ["Die", "Au\u00b7gen", "auf\u00b7w\u00e4rts", ",", "schlug", ":", "gleich", "traf", "die", "tr\u00e4\u00b7gen", "Bli\u00b7cke"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "$,", "VVFIN", "$.", "ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ein gr\u00fcn so helles Feur von einem Linden-Baum,", "tokens": ["Ein", "gr\u00fcn", "so", "hel\u00b7les", "Feur", "von", "ei\u00b7nem", "Lin\u00b7den\u00b7Baum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADV", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Der meine Fenster deckt\u2019 und welcher von der Sonnen", "tokens": ["Der", "mei\u00b7ne", "Fens\u00b7ter", "deckt'", "und", "wel\u00b7cher", "von", "der", "Son\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "KON", "PRELS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "So herrlich angestrahlt, da\u00df meine Augen kaum,", "tokens": ["So", "herr\u00b7lich", "an\u00b7ge\u00b7strahlt", ",", "da\u00df", "mei\u00b7ne", "Au\u00b7gen", "kaum", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "$,", "KOUS", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und zwar in einigen Secunden,", "tokens": ["Und", "zwar", "in", "ei\u00b7ni\u00b7gen", "Se\u00b7cun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Die\u00df durch das zarte Laub gef\u00e4rbte Sonnen-Licht", "tokens": ["Die\u00df", "durch", "das", "zar\u00b7te", "Laub", "ge\u00b7f\u00e4rb\u00b7te", "Son\u00b7nen\u00b7Licht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "APPR", "ART", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Recht anzusehn sich f\u00e4hig funden.", "tokens": ["Recht", "an\u00b7zu\u00b7sehn", "sich", "f\u00e4\u00b7hig", "fun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVIZU", "PRF", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Es sah mein fast f\u00fcr Lust verblendetes Gesicht,", "tokens": ["Es", "sah", "mein", "fast", "f\u00fcr", "Lust", "ver\u00b7blen\u00b7de\u00b7tes", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADV", "APPR", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das hin und her mit schnellen Blicken lieffe,", "tokens": ["Das", "hin", "und", "her", "mit", "schnel\u00b7len", "Bli\u00b7cken", "lief\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PTKVZ", "KON", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In dieses sch\u00f6nen Baumes Tieffe,", "tokens": ["In", "die\u00b7ses", "sch\u00f6\u00b7nen", "Bau\u00b7mes", "Tief\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nebst tausend sch\u00f6n-bestrahlten hellen,", "tokens": ["Nebst", "tau\u00b7send", "sch\u00f6n\u00b7be\u00b7strahl\u00b7ten", "hel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Viel tausend dunckel-gr\u00fcne Stellen,", "tokens": ["Viel", "tau\u00b7send", "dun\u00b7ckel\u00b7gr\u00fc\u00b7ne", "Stel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die alle dem Smaragd an gr\u00fcner Sch\u00f6nheit gleich,", "tokens": ["Die", "al\u00b7le", "dem", "Sma\u00b7ragd", "an", "gr\u00fc\u00b7ner", "Sch\u00f6n\u00b7heit", "gleich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ART", "NN", "APPR", "ADJA", "NN", "ADV", "$,"], "meter": "-+--++-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Und ja so sehr, wie er, an Glantz und Schimmer reich,", "tokens": ["Und", "ja", "so", "sehr", ",", "wie", "er", ",", "an", "Glantz", "und", "Schim\u00b7mer", "reich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ADV", "$,", "PWAV", "PPER", "$,", "APPR", "NN", "KON", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Noch sch\u00f6ner an Figur. Es ist nicht zu beschreiben", "tokens": ["Noch", "sch\u00f6\u00b7ner", "an", "Fi\u00b7gur", ".", "Es", "ist", "nicht", "zu", "be\u00b7schrei\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "APPR", "NN", "$.", "PPER", "VAFIN", "PTKNEG", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wie lieblich alles war;", "tokens": ["Wie", "lieb\u00b7lich", "al\u00b7les", "war", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PIS", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Zumahl da durch die gro\u00df- und klaren Fenster-Scheiben", "tokens": ["Zu\u00b7mahl", "da", "durch", "die", "gro\u00df", "und", "kla\u00b7ren", "Fens\u00b7ter\u00b7Schei\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "ART", "TRUNC", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Das, was man sah, noch einst so klar.", "tokens": ["Das", ",", "was", "man", "sah", ",", "noch", "einst", "so", "klar", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "PIS", "VVFIN", "$,", "ADV", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Nicht m\u00f6glich ists, wenn auch ein Feuer-Werck", "tokens": ["Nicht", "m\u00f6g\u00b7lich", "ists", ",", "wenn", "auch", "ein", "Feu\u00b7er\u00b7\u00b7Werck"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADJD", "VAFIN", "$,", "KOUS", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "In gr\u00fcnen Flammen brennte,", "tokens": ["In", "gr\u00fc\u00b7nen", "Flam\u00b7men", "brenn\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Da\u00df es noch herrlicher, als dieses, gl\u00e4ntzen k\u00f6nnte.", "tokens": ["Da\u00df", "es", "noch", "herr\u00b7li\u00b7cher", ",", "als", "die\u00b7ses", ",", "gl\u00e4nt\u00b7zen", "k\u00f6nn\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJA", "$,", "KOUS", "PDAT", "$,", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ich ward durch alle Lust, die ich durchs Auge sp\u00fcrte,", "tokens": ["Ich", "ward", "durch", "al\u00b7le", "Lust", ",", "die", "ich", "durchs", "Au\u00b7ge", "sp\u00fcr\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PIAT", "NN", "$,", "PRELS", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Durchdrungen und so sehr bewegt,", "tokens": ["Durch\u00b7drun\u00b7gen", "und", "so", "sehr", "be\u00b7wegt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df mich, f\u00fcr Lust, ein heiligs Trauren r\u00fchrte,", "tokens": ["Da\u00df", "mich", ",", "f\u00fcr", "Lust", ",", "ein", "hei\u00b7ligs", "Trau\u00b7ren", "r\u00fchr\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "APPR", "NN", "$,", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie ich bedachtsam \u00fcberlegt,", "tokens": ["Wie", "ich", "be\u00b7dacht\u00b7sam", "\u00fc\u00b7ber\u00b7legt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da\u00df, f\u00fcr so manche Lust, die hier in diesem Leben", "tokens": ["Da\u00df", ",", "f\u00fcr", "so", "man\u00b7che", "Lust", ",", "die", "hier", "in", "die\u00b7sem", "Le\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "$,", "APPR", "ADV", "PIAT", "NN", "$,", "PRELS", "ADV", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der grosse Sch\u00f6pfer uns gegeben,", "tokens": ["Der", "gros\u00b7se", "Sch\u00f6p\u00b7fer", "uns", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Die uns belustigen und n\u00fctzen,", "tokens": ["Die", "uns", "be\u00b7lus\u00b7ti\u00b7gen", "und", "n\u00fct\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Wir so gar wenig F\u00e4higkeit,", "tokens": ["Wir", "so", "gar", "we\u00b7nig", "F\u00e4\u00b7hig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Die GOttheit kr\u00e4ftiger und \u00f6fters zu erheben,", "tokens": ["Die", "Got\u00b7theit", "kr\u00e4f\u00b7ti\u00b7ger", "und", "\u00f6f\u00b7ters", "zu", "er\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Da man so viel besitzt, besitzen,", "tokens": ["Da", "man", "so", "viel", "be\u00b7sitzt", ",", "be\u00b7sit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "ADV", "VVPP", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Und da\u00df wir seine Werck in Andacht anzusehn,", "tokens": ["Und", "da\u00df", "wir", "sei\u00b7ne", "Werck", "in", "An\u00b7dacht", "an\u00b7zu\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPOSAT", "NN", "APPR", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und ihn im Sehen zu erh\u00f6hn,", "tokens": ["Und", "ihn", "im", "Se\u00b7hen", "zu", "er\u00b7h\u00f6hn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Mit solcher Tr\u00e4gheit uns bestreben.", "tokens": ["Mit", "sol\u00b7cher", "Tr\u00e4g\u00b7heit", "uns", "be\u00b7stre\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Ich selber f\u00fchl\u2019 in mir", "tokens": ["Ich", "sel\u00b7ber", "f\u00fchl'", "in", "mir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "APPR", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.15": {"text": "Noch lange, leider! nicht so viele Danck-Begier,", "tokens": ["Noch", "lan\u00b7ge", ",", "lei\u00b7der", "!", "nicht", "so", "vie\u00b7le", "Dan\u00b7ck\u00b7Be\u00b7gier", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "ADV", "$.", "PTKNEG", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.16": {"text": "Als wie ich wol zuweilen wollte,", "tokens": ["Als", "wie", "ich", "wol", "zu\u00b7wei\u00b7len", "woll\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "PPER", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Und als ich, billig, stets empfinden sollte.", "tokens": ["Und", "als", "ich", ",", "bil\u00b7lig", ",", "stets", "emp\u00b7fin\u00b7den", "soll\u00b7te", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "$,", "ADJD", "$,", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Da ich doch mehr vielleicht, als iemand, \u00fcberf\u00fchrt,", "tokens": ["Da", "ich", "doch", "mehr", "viel\u00b7leicht", ",", "als", "ie\u00b7mand", ",", "\u00fc\u00b7berf\u00b7\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ADV", "$,", "KOUS", "PIS", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Wie sehr in unsrer Lust dem Sch\u00f6pfer Danck geb\u00fchrt.", "tokens": ["Wie", "sehr", "in", "uns\u00b7rer", "Lust", "dem", "Sch\u00f6p\u00b7fer", "Danck", "ge\u00b7b\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "APPR", "PPOSAT", "NN", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Ich wei\u00df dabey nichts anders anzufangen,", "tokens": ["Ich", "wei\u00df", "da\u00b7bey", "nichts", "an\u00b7ders", "an\u00b7zu\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "PIS", "ADV", "VVIZU", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Als meinen Sch\u00f6pfer anzuflehn,", "tokens": ["Als", "mei\u00b7nen", "Sch\u00f6p\u00b7fer", "an\u00b7zu\u00b7flehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Da\u00df ich, sein herrlich Werck mit Lust oft anzusehn,", "tokens": ["Da\u00df", "ich", ",", "sein", "herr\u00b7lich", "Werck", "mit", "Lust", "oft", "an\u00b7zu\u00b7sehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "PPOSAT", "ADJD", "NN", "APPR", "NN", "ADV", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Die Gnad\u2019 und F\u00e4higkeit von ihm doch m\u00f6g\u2019 erlangen!", "tokens": ["Die", "Gnad'", "und", "F\u00e4\u00b7hig\u00b7keit", "von", "ihm", "doch", "m\u00f6g'", "er\u00b7lan\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "APPR", "PPER", "ADV", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}