{"dta.poem.20879": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Der  \n Sterbende  \n  Socrates.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.85", "nl:0.14"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Weil ich den Socratem in dieser Welt gekennet/", "tokens": ["Weil", "ich", "den", "So\u00b7cra\u00b7tem", "in", "die\u00b7ser", "Welt", "ge\u00b7ken\u00b7net", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "PDAT", "NN", "VVPP", "$("], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "So hat mich seine Art zu reden stets entz\u00fcckt/", "tokens": ["So", "hat", "mich", "sei\u00b7ne", "Art", "zu", "re\u00b7den", "stets", "ent\u00b7z\u00fcckt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "NN", "PTKZU", "VVINF", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch hat mich dessen Krafft fast nie so starck berennet/", "tokens": ["Doch", "hat", "mich", "des\u00b7sen", "Krafft", "fast", "nie", "so", "starck", "be\u00b7ren\u00b7net", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PDS", "NN", "ADV", "ADV", "ADV", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Als solches Sonnenklar itz und herf\u00fcr geblickt,", "tokens": ["Als", "sol\u00b7ches", "Son\u00b7nen\u00b7klar", "itz", "und", "her\u00b7f\u00fcr", "ge\u00b7blickt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "NE", "KON", "ADV", "VVPP", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Der Unmuth so sich starck in meinem Geist erreget/", "tokens": ["Der", "Un\u00b7muth", "so", "sich", "starck", "in", "mei\u00b7nem", "Geist", "er\u00b7re\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PRF", "ADJD", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wie auch die schwere Pein/ so mir sein Todt gebracht/", "tokens": ["Wie", "auch", "die", "schwe\u00b7re", "Pein", "/", "so", "mir", "sein", "Todt", "ge\u00b7bracht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "ADJA", "NN", "$(", "ADV", "PPER", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die haben mir die Lust nicht gantz in Staub geleget/", "tokens": ["Die", "ha\u00b7ben", "mir", "die", "Lust", "nicht", "gantz", "in", "Staub", "ge\u00b7le\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ART", "NN", "PTKNEG", "ADV", "APPR", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Weil mir f\u00fcr Augen schwebt/ wie er den Todt verlacht/", "tokens": ["Weil", "mir", "f\u00fcr", "Au\u00b7gen", "schwebt", "/", "wie", "er", "den", "Todt", "ver\u00b7lacht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "VVFIN", "$(", "PWAV", "PPER", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Die Gr\u00fcnde/ so er uns hat an das Licht gestellet/", "tokens": ["Die", "Gr\u00fcn\u00b7de", "/", "so", "er", "uns", "hat", "an", "das", "Licht", "ge\u00b7stel\u00b7let", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ADV", "PPER", "PPER", "VAFIN", "APPR", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und nicht gewohnet seyn zu f\u00fchren falschen Schein/", "tokens": ["Und", "nicht", "ge\u00b7woh\u00b7net", "seyn", "zu", "f\u00fch\u00b7ren", "fal\u00b7schen", "Schein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "VVPP", "VAINF", "PTKZU", "VVINF", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Die haben alles di\u00df/ was irrig war/ gef\u00e4llet:", "tokens": ["Die", "ha\u00b7ben", "al\u00b7les", "di\u00df", "/", "was", "ir\u00b7rig", "war", "/", "ge\u00b7f\u00e4l\u00b7let", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "VAFIN", "PIS", "PDS", "$(", "PWS", "ADJD", "VAFIN", "$(", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und machen uns besch\u00e4mt/ da\u00df wir so t\u00f6lpisch seyn.", "tokens": ["Und", "ma\u00b7chen", "uns", "be\u00b7sch\u00e4mt", "/", "da\u00df", "wir", "so", "t\u00f6l\u00b7pisch", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "$(", "KOUS", "PPER", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Man konte keinen Zorn in seinen Augen mercken/", "tokens": ["Man", "kon\u00b7te", "kei\u00b7nen", "Zorn", "in", "sei\u00b7nen", "Au\u00b7gen", "mer\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PIAT", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Wiewol der Jrrthum uns tieff im Gehirne stund/", "tokens": ["Wie\u00b7wol", "der", "Jrr\u00b7thum", "uns", "tieff", "im", "Ge\u00b7hir\u00b7ne", "stund", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "ADJD", "APPRART", "NN", "VVFIN", "$("], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.15": {"text": "Und wie er freudig ist in Worten und in Wercken/", "tokens": ["Und", "wie", "er", "freu\u00b7dig", "ist", "in", "Wor\u00b7ten", "und", "in", "Wer\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADJD", "VAFIN", "APPR", "NN", "KON", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "So war der Zweiffel uns zu sagen frey vergunt;", "tokens": ["So", "war", "der", "Zweif\u00b7fel", "uns", "zu", "sa\u00b7gen", "frey", "ver\u00b7gunt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PPER", "PTKZU", "VVINF", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Er brachte Pflaster her f\u00fcr alle diese Wunden/", "tokens": ["Er", "brach\u00b7te", "Pflas\u00b7ter", "her", "f\u00fcr", "al\u00b7le", "die\u00b7se", "Wun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "APZR", "APPR", "PIS", "PDAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Und r\u00fchret uns den Geist mit so gelehrter Macht/", "tokens": ["Und", "r\u00fch\u00b7ret", "uns", "den", "Geist", "mit", "so", "ge\u00b7lehr\u00b7ter", "Macht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "APPR", "ADV", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Da\u00df er in kurtzer Zeit des Zweiffels war entbunden/", "tokens": ["Da\u00df", "er", "in", "kurt\u00b7zer", "Zeit", "des", "Zweif\u00b7fels", "war", "ent\u00b7bun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "ART", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Und endlich wiederum zu seiner Freyheit bracht/", "tokens": ["Und", "end\u00b7lich", "wie\u00b7de\u00b7rum", "zu", "sei\u00b7ner", "Frey\u00b7heit", "bracht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Wie ein geschlagen Volck/ so Noth und Schrecken r\u00fchret/", "tokens": ["Wie", "ein", "ge\u00b7schla\u00b7gen", "Volck", "/", "so", "Noth", "und", "Schre\u00b7cken", "r\u00fch\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$(", "ADV", "NN", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Wann der ergrimte Feind durch Glied und Ordnung bricht/", "tokens": ["Wann", "der", "er\u00b7grim\u00b7te", "Feind", "durch", "Glied", "und", "Ord\u00b7nung", "bricht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Gar offt ein kluges Haupt in seine Reyen f\u00fchret/", "tokens": ["Gar", "offt", "ein", "klu\u00b7ges", "Haupt", "in", "sei\u00b7ne", "Re\u00b7yen", "f\u00fch\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Und da\u00df zertrennte Heer mit Kunst zusammenflicht;", "tokens": ["Und", "da\u00df", "zer\u00b7trenn\u00b7te", "Heer", "mit", "Kunst", "zu\u00b7sam\u00b7men\u00b7flicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADJA", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "So brach auch Socrates mit freudigem Erk\u00fchnen/", "tokens": ["So", "brach", "auch", "So\u00b7cra\u00b7tes", "mit", "freu\u00b7di\u00b7gem", "Er\u00b7k\u00fch\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "NE", "APPR", "ADJA", "NN", "$("], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.26": {"text": "Durch di\u00df/ was der Vernunfft viel Ubels angethan/", "tokens": ["Durch", "di\u00df", "/", "was", "der", "Ver\u00b7nunfft", "viel", "U\u00b7bels", "an\u00b7ge\u00b7than", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDS", "$(", "PWS", "ART", "NN", "PIAT", "NN", "VVPP", "$("], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.27": {"text": "Er zwang den schwachen Geist uns wiederum zu dienen/", "tokens": ["Er", "zwang", "den", "schwa\u00b7chen", "Geist", "uns", "wie\u00b7de\u00b7rum", "zu", "die\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "PPER", "ADV", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Und trieb ihn meisterlich auf seine rechte Bahn.", "tokens": ["Und", "trieb", "ihn", "meis\u00b7ter\u00b7lich", "auf", "sei\u00b7ne", "rech\u00b7te", "Bahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Wiewol der sch\u00f6ne Grieff/ damit er uns gelehret/", "tokens": ["Wie\u00b7wol", "der", "sch\u00f6\u00b7ne", "Grieff", "/", "da\u00b7mit", "er", "uns", "ge\u00b7leh\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "$(", "KOUS", "PPER", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Noch heute bey der Welt f\u00fcr lauter Wunder gilt/", "tokens": ["Noch", "heu\u00b7te", "bey", "der", "Welt", "f\u00fcr", "lau\u00b7ter", "Wun\u00b7der", "gilt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "APPR", "PIAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "So ward ihm doch die Krafft und dessen Glantz vermehret/", "tokens": ["So", "ward", "ihm", "doch", "die", "Krafft", "und", "des\u00b7sen", "Glantz", "ver\u00b7meh\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ART", "NN", "KON", "PRELAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Als er voll Freud und Lust mit uns Gespr\u00e4che hielt.", "tokens": ["Als", "er", "voll", "Freud", "und", "Lust", "mit", "uns", "Ge\u00b7spr\u00e4\u00b7che", "hielt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "NN", "KON", "NN", "APPR", "PPER", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Ich sa\u00df gleich ohngefehr zu seines Bettes F\u00fcssen/", "tokens": ["Ich", "sa\u00df", "gleich", "ohn\u00b7ge\u00b7fehr", "zu", "sei\u00b7nes", "Bet\u00b7tes", "F\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "In welchem ihm beschwang des langen Schlaffes Nacht/", "tokens": ["In", "wel\u00b7chem", "ihm", "be\u00b7schwang", "des", "lan\u00b7gen", "Schlaf\u00b7fes", "Nacht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "VVFIN", "ART", "ADJA", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Er hatte sich mit Flei\u00df was hoch zu seyn beflissen/", "tokens": ["Er", "hat\u00b7te", "sich", "mit", "Flei\u00df", "was", "hoch", "zu", "seyn", "be\u00b7flis\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PRF", "APPR", "NN", "PWS", "ADJD", "PTKZU", "VAINF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Mir aber hatte man ein niedrig B\u00e4ncklein bracht.", "tokens": ["Mir", "a\u00b7ber", "hat\u00b7te", "man", "ein", "nied\u00b7rig", "B\u00e4nc\u00b7klein", "bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "PIS", "ART", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Er schaute mir mit Ernst recht unter das Gesichte/", "tokens": ["Er", "schau\u00b7te", "mir", "mit", "Ernst", "recht", "un\u00b7ter", "das", "Ge\u00b7sich\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "NE", "ADJD", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Und fuhr mit seiner Hand um meine Kolbe her;", "tokens": ["Und", "fuhr", "mit", "sei\u00b7ner", "Hand", "um", "mei\u00b7ne", "Kol\u00b7be", "her", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Man schaute/ wie nunmehr das himmlische Gerichte/", "tokens": ["Man", "schau\u00b7te", "/", "wie", "nun\u00b7mehr", "das", "himm\u00b7li\u00b7sche", "Ge\u00b7rich\u00b7te", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$(", "KOKOM", "ADV", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Jhm einen Spruch gethan nach Willen und Begehr.", "tokens": ["Jhm", "ei\u00b7nen", "Spruch", "ge\u00b7than", "nach", "Wil\u00b7len", "und", "Be\u00b7gehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "VVPP", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Als ieder fertg war die Ohren zu erheben/", "tokens": ["Als", "ie\u00b7der", "fertg", "war", "die", "Oh\u00b7ren", "zu", "er\u00b7he\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "VAFIN", "ART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.42": {"text": "So fuhr er noch einmal mir in das Haar hinein/", "tokens": ["So", "fuhr", "er", "noch", "ein\u00b7mal", "mir", "in", "das", "Haar", "hin\u00b7ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "PPER", "APPR", "ART", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Er lie\u00df die tr\u00e4ue Faust um meine Schlafe schweben/", "tokens": ["Er", "lie\u00df", "die", "tr\u00e4u\u00b7e", "Faust", "um", "mei\u00b7ne", "Schla\u00b7fe", "schwe\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Und sprach: es wird das Haar wol morgen runder seyn.", "tokens": ["Und", "sprach", ":", "es", "wird", "das", "Haar", "wol", "mor\u00b7gen", "run\u00b7der", "seyn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "PPER", "VAFIN", "ART", "NN", "ADV", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Ich sagte drauf/ wolan/ und konte nicht ersinnen/", "tokens": ["Ich", "sag\u00b7te", "drauf", "/", "wo\u00b7lan", "/", "und", "kon\u00b7te", "nicht", "er\u00b7sin\u00b7nen", "/"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "$(", "ADV", "$(", "KON", "VMFIN", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Warum er diese Art zu reden angestimt/", "tokens": ["Wa\u00b7rum", "er", "die\u00b7se", "Art", "zu", "re\u00b7den", "an\u00b7ge\u00b7stimt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PDAT", "NN", "PTKZU", "VVINF", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Ach Ph\u00e4don/ fuhr er fort/ was wollen wir beginnen/", "tokens": ["Ach", "Ph\u00e4\u00b7don", "/", "fuhr", "er", "fort", "/", "was", "wol\u00b7len", "wir", "be\u00b7gin\u00b7nen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$(", "VVFIN", "PPER", "PTKVZ", "$(", "PWS", "VMFIN", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Wann die Verzweiffelung uns in die Armen nimt.", "tokens": ["Wann", "die", "Ver\u00b7zweif\u00b7fe\u00b7lung", "uns", "in", "die", "Ar\u00b7men", "nimt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.49": {"text": "Man mu\u00df beschoren seyn/ weil Phoebus Fackel siehet/", "tokens": ["Man", "mu\u00df", "be\u00b7scho\u00b7ren", "seyn", "/", "weil", "Phoe\u00b7bus", "Fa\u00b7ckel", "sie\u00b7het", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "VVPP", "VAINF", "$(", "KOUS", "NE", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.50": {"text": "Und dieses Auge noch das letzte Feuer f\u00fchrt;", "tokens": ["Und", "die\u00b7ses", "Au\u00b7ge", "noch", "das", "letz\u00b7te", "Feu\u00b7er", "f\u00fchrt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "ADV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.51": {"text": "Wir wollen beyde dran/ so uns der Witz entgehet/", "tokens": ["Wir", "wol\u00b7len", "bey\u00b7de", "dran", "/", "so", "uns", "der", "Witz", "ent\u00b7ge\u00b7het", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIS", "PAV", "$(", "ADV", "PPER", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.52": {"text": "Zum Zeugni\u00df/ da\u00df der Todt nun alles hat ger\u00fchrt.", "tokens": ["Zum", "Zeug\u00b7ni\u00df", "/", "da\u00df", "der", "Todt", "nun", "al\u00b7les", "hat", "ge\u00b7r\u00fchrt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$(", "KOUS", "ART", "NN", "ADV", "PIS", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.53": {"text": "Wie der Argiver Volck/ wann Speer und Schwerter strichen/", "tokens": ["Wie", "der", "Ar\u00b7gi\u00b7ver", "Volck", "/", "wann", "Speer", "und", "Schwer\u00b7ter", "stri\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "$(", "PWAV", "NN", "KON", "NN", "VVFIN", "$("], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.54": {"text": "Geschwohren ohne Haar/ und stetig kahl zu seyn/", "tokens": ["Ge\u00b7schwoh\u00b7ren", "oh\u00b7ne", "Haar", "/", "und", "ste\u00b7tig", "kahl", "zu", "seyn", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$(", "KON", "ADJD", "ADJD", "PTKZU", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.55": {"text": "Bi\u00df da\u00df der Feinde Heer hin in die Flucht gewichen/", "tokens": ["Bi\u00df", "da\u00df", "der", "Fein\u00b7de", "Heer", "hin", "in", "die", "Flucht", "ge\u00b7wi\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "ART", "NN", "NN", "ADV", "APPR", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.56": {"text": "Und der verblaste Todt bricht durch die Hauffen ein.", "tokens": ["Und", "der", "ver\u00b7blas\u00b7te", "Todt", "bricht", "durch", "die", "Hauf\u00b7fen", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.57": {"text": "Wer ich an Ph\u00e4don statt/ ich wolt es tapffer wagen/", "tokens": ["Wer", "ich", "an", "Ph\u00e4\u00b7don", "statt", "/", "ich", "wolt", "es", "tapf\u00b7fer", "wa\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "NE", "PTKVZ", "$(", "PPER", "VMFIN", "PPER", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.58": {"text": "Wann mir der Simias und Cebes Kampff antr\u00e4gt/", "tokens": ["Wann", "mir", "der", "Si\u00b7mi\u00b7as", "und", "Ce\u00b7bes", "Kampff", "an\u00b7tr\u00e4gt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NE", "KON", "NN", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.59": {"text": "Ich wolte