u00dft", "auf", "sich", "al\u00b7lein", "/", "und", "traut", "den", "Freun\u00b7den", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PRF", "ADV", "$(", "KON", "VVFIN", "ART", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.105": {"text": "Man lasse sich doch nun so leichtlich nicht verleiten/", "tokens": ["Man", "las\u00b7se", "sich", "doch", "nun", "so", "leicht\u00b7lich", "nicht", "ver\u00b7lei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PRF", "ADV", "ADV", "ADV", "ADJD", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.106": {"text": "Und ihm der Welt Betrug zu sehr zu Hertzen gehn/", "tokens": ["Und", "ihm", "der", "Welt", "Be\u00b7trug", "zu", "sehr", "zu", "Hert\u00b7zen", "gehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "NN", "NN", "APPR", "ADV", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.107": {"text": "Dann deren Seelen nicht aus dem Gewichte schreiten/", "tokens": ["Dann", "de\u00b7ren", "See\u00b7len", "nicht", "aus", "dem", "Ge\u00b7wich\u00b7te", "schrei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PRELAT", "NN", "PTKNEG", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.108": {"text": "Die wissen wie allhier dergleichen Sachen stehn.", "tokens": ["Die", "wis\u00b7sen", "wie", "all\u00b7hier", "derg\u00b7lei\u00b7chen", "Sa\u00b7chen", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "KOKOM", "ADV", "PIS", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.109": {"text": "Es ist ein sch\u00f6ner Grieff die Rosen recht zu lesen/", "tokens": ["Es", "ist", "ein", "sch\u00f6\u00b7ner", "Grieff", "die", "Ro\u00b7sen", "recht", "zu", "le\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "ART", "NN", "ADJD", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.110": {"text": "Es ist ein kluges Werck zu f\u00fchren Leut und Land/", "tokens": ["Es", "ist", "ein", "klu\u00b7ges", "Werck", "zu", "f\u00fch\u00b7ren", "Leut", "und", "Land", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "PTKZU", "VVINF", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.111": {"text": "Der Weise kennt allein der Sachen reines Wesen/", "tokens": ["Der", "Wei\u00b7se", "kennt", "al\u00b7lein", "der", "Sa\u00b7chen", "rei\u00b7nes", "We\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.112": {"text": "So alles in gemein der Welt ist unbekant.", "tokens": ["So", "al\u00b7les", "in", "ge\u00b7mein", "der", "Welt", "ist", "un\u00b7be\u00b7kant", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "APPR", "ADJD", "ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}