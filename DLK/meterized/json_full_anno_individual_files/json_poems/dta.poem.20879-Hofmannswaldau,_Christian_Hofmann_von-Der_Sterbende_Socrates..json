"meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.68": {"text": "Ich wil Jolas seyn/ der auch wol Thaten kan.", "tokens": ["Ich", "wil", "Jo\u00b7las", "seyn", "/", "der", "auch", "wol", "Tha\u00b7ten", "kan", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "NE", "VAINF", "$(", "ART", "ADV", "ADV", "NN", "VMFIN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.69": {"text": "Er/ sagt ich/ Hercules/ doch mangelt mir die St\u00e4rcke/", "tokens": ["Er", "/", "sagt", "ich", "/", "Her\u00b7cu\u00b7les", "/", "doch", "man\u00b7gelt", "mir", "die", "St\u00e4r\u00b7cke", "/"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$(", "VVFIN", "PPER", "$(", "NE", "$(", "ADV", "VVFIN", "PPER", "ART", "NN", "$("], "meter": "-+-+---+-+-+-", "measure": "unknown.measure.penta"}, "line.70": {"text": "Vor den Jolas recht mit Ehren zu bestehn/", "tokens": ["Vor", "den", "Jo\u00b7las", "recht", "mit", "Eh\u00b7ren", "zu", "be\u00b7stehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "APPR", "NN", "PTKZU", "VVINF", "$("], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.71": {"text": "Doch da\u00df mein schwacher Arm nicht sch\u00e4ndet eure Wercke/", "tokens": ["Doch", "da\u00df", "mein", "schwa\u00b7cher", "Arm", "nicht", "sch\u00e4n\u00b7det", "eu\u00b7re", "Wer\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPOSAT", "ADJA", "NN", "PTKNEG", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.72": {"text": "So la\u00df ich euch allein auf diese Feinde gehn.", "tokens": ["So", "la\u00df", "ich", "euch", "al\u00b7lein", "auf", "di