{"textgrid.poem.59270": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der neue Rattenf\u00e4nger", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Juchheisa! und ich f\u00fchr den Zug", "tokens": ["Juch\u00b7hei\u00b7sa", "!", "und", "ich", "f\u00fchr", "den", "Zug"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$.", "KON", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hopp \u00fcber Feld und Graben.", "tokens": ["Hopp", "\u00fc\u00b7ber", "Feld", "und", "Gra\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Des alten Plunders ist genug,", "tokens": ["Des", "al\u00b7ten", "Plun\u00b7ders", "ist", "ge\u00b7nug", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wir wollen neuen haben.", "tokens": ["Wir", "wol\u00b7len", "neu\u00b7en", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Was! wir gering? Ihr vornehm, reich?", "tokens": ["Was", "!", "wir", "ge\u00b7ring", "?", "Ihr", "vor\u00b7nehm", ",", "reich", "?"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "$.", "PPER", "ADJD", "$.", "PPER", "ADJD", "$,", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Planierend schwirrt die Schere,", "tokens": ["Pla\u00b7nie\u00b7rend", "schwirrt", "die", "Sche\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Seid Lumps wie wir, so sind wir gleich,", "tokens": ["Seid", "Lumps", "wie", "wir", ",", "so", "sind", "wir", "gleich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "NN", "KOKOM", "PPER", "$,", "ADV", "VAFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "H\u00fcbsch breit wird die Misere!", "tokens": ["H\u00fcbsch", "breit", "wird", "die", "Mi\u00b7se\u00b7re", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "VAFIN", "ART", "NN", "$."], "meter": "-+--+--", "measure": "iambic.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Das alte Lied, das spiel ich neu,", "tokens": ["Das", "al\u00b7te", "Lied", ",", "das", "spiel", "ich", "neu", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PDS", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da tanzen alle Leute,", "tokens": ["Da", "tan\u00b7zen", "al\u00b7le", "Leu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das ist die Vaterl\u00e4nderei,", "tokens": ["Das", "ist", "die", "Va\u00b7ter\u00b7l\u00e4n\u00b7de\u00b7rei", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "O Herr, mach uns gescheute! \u2013", "tokens": ["O", "Herr", ",", "mach", "uns", "ge\u00b7scheu\u00b7te", "!", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NE", "NN