{"textgrid.poem.24566": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Die beste Manier einen gelehrten Streit zu f\u00fchren", "genre": "verse", "period": "N.A.", "pub_year": 1701, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Man streite mit Verstand/ so h\u00f6flich als gelind/", "tokens": ["Man", "strei\u00b7te", "mit", "Ver\u00b7stand", "/", "so", "h\u00f6f\u00b7lich", "als", "ge\u00b7lind", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "NN", "$(", "ADV", "ADJD", "KOKOM", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In seinen Gr\u00fcnden scharf/ und nicht bey andern blind/", "tokens": ["In", "sei\u00b7nen", "Gr\u00fcn\u00b7den", "scharf", "/", "und", "nicht", "bey", "an\u00b7dern", "blind", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$(", "KON", "PTKNEG", "APPR", "PIS", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Bey Fehlern/ als ein Mensch/ der in den Wald spatzirt/", "tokens": ["Bey", "Feh\u00b7lern", "/", "als", "ein", "Mensch", "/", "der", "in", "den", "Wald", "spat\u00b7zirt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$(", "KOUS", "ART", "NN", "$(", "ART", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und dem verbunden ist/ der ihn zurecht gef\u00fchrt/", "tokens": ["Und", "dem", "ver\u00b7bun\u00b7den", "ist", "/", "der", "ihn", "zu\u00b7recht", "ge\u00b7f\u00fchrt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "VVPP", "VAFIN", "$(", "PRELS", "PPER", "PTKVZ", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der sich zum Tugend-Schlo\u00df die Warheits-Bahn erkiesen/", "tokens": ["Der", "sich", "zum", "Tu\u00b7gen\u00b7dSchlo\u00df", "die", "Wa\u00b7rheits\u00b7Bahn", "er\u00b7kie\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPRART", "NN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wenn auch ein Bettler ihm den Weg dahin gewiesen.", "tokens": ["Wenn", "auch", "ein", "Bett\u00b7ler", "ihm", "den", "Weg", "da\u00b7hin", "ge\u00b7wie\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "PPER", "ART", "NN", "PAV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Man streite mit Verstand/ so h\u00f6flich als gelind/", "tokens": ["Man", "strei\u00b7te", "mit", "Ver\u00b7stand", "/", "so", "h\u00f6f\u00b7lich", "als", "ge\u00b7lind", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "NN", "$(", "ADV", "ADJD", "KOKOM", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In seinen Gr\u00fcnden scharf/ und nicht bey andern blind/", "tokens": ["In", "sei\u00b7nen", "Gr\u00fcn\u00b7den", "scharf", "/", "und", "nicht", "bey", "an\u00b7dern", "blind", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$(", "KON", "PTKNEG", "APPR", "PIS", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Bey Fehlern/ al