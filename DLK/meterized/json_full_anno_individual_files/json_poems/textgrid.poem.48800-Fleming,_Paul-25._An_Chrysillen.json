{"textgrid.poem.48800": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "25. An Chrysillen", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gold ist dein treflichs Haar, Gold deiner Augen Licht,", "tokens": ["Gold", "ist", "dein", "tref\u00b7lichs", "Haar", ",", "Gold", "dei\u00b7ner", "Au\u00b7gen", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "ADJA", "NN", "$,", "NN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gold dein gemalter Mund, Gold deine sch\u00f6ne Wangen,", "tokens": ["Gold", "dein", "ge\u00b7mal\u00b7ter", "Mund", ",", "Gold", "dei\u00b7ne", "sch\u00f6\u00b7ne", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$,", "NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "der Hals, die Brust, der Leib und was uns macht Verlangen,", "tokens": ["der", "Hals", ",", "die", "Brust", ",", "der", "Leib", "und", "was", "uns", "macht", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "KON", "PWS", "PPER", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Gold ist die Rede selbst, die deine Zunge spricht,", "tokens": ["Gold", "ist", "die", "Re\u00b7de", "selbst", ",", "die", "dei\u00b7ne", "Zun\u00b7ge", "spricht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"],