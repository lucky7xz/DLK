{"dta.poem.5799": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Gesetzen der Bewegung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie aber die Materie in sich,", "tokens": ["Wie", "a\u00b7ber", "die", "Ma\u00b7te\u00b7rie", "in", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "APPR", "PRF", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wie sehr sie auch ver\u00e4nderlich,", "tokens": ["Wie", "sehr", "sie", "auch", "ver\u00b7\u00e4n\u00b7der\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch Schrancken hat, die sie nicht \u00fcbersteigen kan;", "tokens": ["Doch", "Schran\u00b7cken", "hat", ",", "die", "sie", "nicht", "\u00fc\u00b7bers\u00b7tei\u00b7gen", "kan", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "$,", "PRELS", "PPER", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ist die ", "tokens": ["Ist", "die"], "token_info": ["word", "word"], "pos": ["VAFIN", "ART"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Die unver\u00e4nderlich, nicht minder unterthan.", "tokens": ["Die", "un\u00b7ver\u00b7\u00e4n\u00b7der\u00b7lich", ",", "nicht", "min\u00b7der", "un\u00b7ter\u00b7than", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "$,", "PTKNEG", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}