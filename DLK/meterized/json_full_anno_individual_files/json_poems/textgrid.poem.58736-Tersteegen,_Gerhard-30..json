{"textgrid.poem.58736": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "30.", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit Saitenspielen und Pfeifen. Psalm 150, 4", "tokens": ["Mit", "Sai\u00b7ten\u00b7spie\u00b7len", "und", "Pfei\u00b7fen", ".", "Psalm", "150", ",", "4"], "token_info": ["word", "word", "word", "word", "punct", "word", "number", "punct", "number"], "pos": ["APPR", "NN", "KON", "NN", "$.", "NN", "CARD", "$,", "CARD"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "O treffliche Musik, o sch\u00f6nste Harmonie", "tokens": ["O", "treff\u00b7li\u00b7che", "Mu\u00b7sik", ",", "o", "sch\u00f6ns\u00b7te", "Har\u00b7mo\u00b7nie"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "ADJA", "NN", "$,", "FM", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des Br\u00e4ut'gams und der Braut, wie froh ist er und sie!", "tokens": ["Des", "Br\u00e4ut'\u00b7gams", "und", "der", "Braut", ",", "wie", "froh", "ist", "er", "und", "sie", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "KON", "ART", "NN", "$,", "PWAV", "ADJD", "VAFIN", "PPER", "KON", "PPER", "$."], "meter": "-+---+-+-+-+", "measure": "dactylic.init"}, "line.3": {"text": "Du gibst ihr, Herr, die Prob' vom himmelischen Singen,", "tokens": ["Du", "gibst", "ihr", ",", "Herr", ",", "die", "Prob'", "vom", "him\u00b7me\u00b7li\u00b7schen", "Sin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "NN", "$,", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Du spielst, die Seele singt; was kann wohl s\u00fc\u00dfer klingen?", "tokens": ["Du", "spielst", ",", "die", "See\u00b7le", "singt", ";", "was", "kann", "wohl", "s\u00fc\u00b7\u00dfer", "klin\u00b7gen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ART", "NN", "VVFIN", "$.", "PWS", "VMFIN", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Anbetungsw\u00fcrdig's Gut, mein Meister, m\u00f6cht' ich hier", "tokens": ["An\u00b7be\u00b7tungs\u00b7w\u00fcr\u00b7dig's", "Gut", ",", "mein", "Meis\u00b7ter", ",", "m\u00f6cht'", "ich", "hier"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "ADJD", "$,", "PPOSAT", "NN", "$,", "VMFIN", "PPER", "ADV"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Den g\u00f6ttlichen Gesang auch singen einst mit dir!", "tokens": ["Den", "g\u00f6tt\u00b7li\u00b7chen", "Ge\u00b7sang", "auch", "sin\u00b7gen", "einst", "mit", "dir", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVFIN", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nimm meinen Willen hin, nimm mein Gem\u00fct, mein Wesen,", "tokens": ["Nimm", "mei\u00b7nen", "Wil\u00b7len", "hin", ",", "nimm", "mein", "Ge\u00b7m\u00fct", ",", "mein", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "PTKVZ", "$,", "VVIMP", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ei lehr mich, schlage drauf, ", "tokens": ["Ei", "lehr", "mich", ",", "schla\u00b7ge", "drauf", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,", "VVFIN", "PTKVZ", "$,"], "meter": "---+-+", "measure": "unknown.measure.di"}}, "stanza.4": {"line.1": {"text": "Die Harmonie mu\u00df sch\u00f6n, das Spiel mu\u00df trefflich sein,", "tokens": ["Die", "Har\u00b7mo\u00b7nie", "mu\u00df", "sch\u00f6n", ",", "das", "Spiel", "mu\u00df", "treff\u00b7lich", "sein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADJD", "$,", "ART", "NN", "VMFIN", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn unser Wille geht in deinen Willen ein;", "tokens": ["Wenn", "un\u00b7ser", "Wil\u00b7le", "geht", "in", "dei\u00b7nen", "Wil\u00b7len", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die s\u00fc\u00dfe Melodie kann man in Gott nur h\u00f6ren \u2013", "tokens": ["Die", "s\u00fc\u00b7\u00dfe", "Me\u00b7lo\u00b7die", "kann", "man", "in", "Gott", "nur", "h\u00f6\u00b7ren", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "PIS", "APPR", "NN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der find't's, der sich in ihn verliert ohn' Wiederkehren.", "tokens": ["Der", "fin\u00b7d't's", ",", "der", "sich", "in", "ihn", "ver\u00b7liert", "ohn'", "Wie\u00b7der\u00b7keh\u00b7ren", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "PRELS", "PRF", "APPR", "PPER", "VVFIN", "APPR", "NN", "$."], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.5": {"line.1": {"text": "Dann akkordiert man stets, obgleich der Noten viel,", "tokens": ["Dann", "ak\u00b7kor\u00b7diert", "man", "stets", ",", "ob\u00b7gleich", "der", "No\u00b7ten", "viel", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "$,", "KOUS", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dies ist die Sch\u00f6nheit erst vom Singen und vom Spiel;", "tokens": ["Dies", "ist", "die", "Sch\u00f6n\u00b7heit", "erst", "vom", "Sin\u00b7gen", "und", "vom", "Spiel", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ADV", "APPRART", "NN", "KON", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Je mehr Ver\u00e4ndrung kommt im Ton, je mehr sich freue", "tokens": ["Je", "mehr", "Ver\u00b7\u00e4n\u00b7drung", "kommt", "im", "Ton", ",", "je", "mehr", "sich", "freu\u00b7e"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "PIAT", "NN", "VVFIN", "APPRART", "NN", "$,", "ADV", "ADV", "PRF", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Seele und beweis' Gehorsamsein und Treue!", "tokens": ["Die", "See\u00b7le", "und", "be\u00b7weis'", "Ge\u00b7hor\u00b7sam\u00b7sein", "und", "Treu\u00b7e", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Mit Saitenspielen und Pfeifen. Psalm 150, 4", "tokens": ["Mit", "Sai\u00b7ten\u00b7spie\u00b7len", "und", "Pfei\u00b7fe