Wie", "die", "Nach\u00b7ti\u00b7gall", "sin\u00b7get", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Denn sie singet zum Tanz!", "tokens": ["Denn", "sie", "sin\u00b7get", "zum", "Tanz", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.4": {"text": "O geschwinder, geschwinder!", "tokens": ["O", "ge\u00b7schwin\u00b7der", ",", "ge\u00b7schwin\u00b7der", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NE", "ADJA", "$,", "ADJD", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "Rundherum, wie die Kinder!", "tokens": ["Rund\u00b7he\u00b7rum", ",", "wie", "die", "Kin\u00b7der", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "ART", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.6": {"text": "Ringel Ringelein Rosenkranz!", "tokens": ["Rin\u00b7gel", "Rin\u00b7ge\u00b7lein", "Ro\u00b7sen\u00b7kranz", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "NE", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.4": {"line.1": {"text": "Alles tanzet vor Freude:", "tokens": ["Al\u00b7les", "tan\u00b7zet", "vor", "Freu\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Dort das Reh in der Heide,", "tokens": ["Dort", "das", "Reh", "in", "der", "Hei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Hier das L\u00e4mmchen im Thal,", "tokens": ["Hier", "das", "L\u00e4mm\u00b7chen", "im", "Thal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "V\u00f6gel hier im Geb\u00fcsche,", "tokens": ["V\u00f6\u00b7gel", "hier", "im", "Ge\u00b7b\u00fc\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPRART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "Dort im Teiche die Fische,", "tokens": ["Dort", "im", "Tei\u00b7che", "die", "Fi\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.6": {"text": "Tausend M\u00fccken im Sonnenstrahl.", "tokens": ["Tau\u00b7send", "M\u00fc\u00b7cken", "im", "Son\u00b7nen\u00b7strahl", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "APPRART", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.5": {"line.1": {"text": "Ha! wie pocht's mir so bange!", "tokens": ["Ha", "!", "wie", "pocht's", "mir", "so", "ban\u00b7ge", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PWAV", "VVFIN", "PPER", "ADV", "ADJD", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Ha! wie gl\u00fcht mir die Wange!", "tokens": ["Ha", "!", "wie", "gl\u00fcht", "mir", "die", "Wan\u00b7ge", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PWAV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "M\u00e4dchen, bin ich nicht sch\u00f6n?", "tokens": ["M\u00e4d\u00b7chen", ",", "bin", "ich", "nicht", "sch\u00f6n", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VAFIN", "PPER", "PTKNEG", "ADJD", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "H\u00fcpf' ich nicht wie ein Kr\u00e4usel,", "tokens": ["H\u00fcpf'", "ich", "nicht", "wie", "ein", "Kr\u00e4u\u00b7sel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKNEG", "KOKOM", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "Da\u00df mir unterm Ges\u00e4usel", "tokens": ["Da\u00df", "mir", "un\u00b7term", "Ge\u00b7s\u00e4u\u00b7sel"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.6": {"text": "Meines Kranzes die Locken wehn?", "tokens": ["Mei\u00b7nes", "Kran\u00b7zes", "die", "Lo\u00b7cken", "wehn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "VVINF", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.6": {"line.1": {"text": "Frei und ohne Gesetze,", "tokens": ["Frei", "und", "oh\u00b7ne", "Ge\u00b7set\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "APPR", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "H\u00fcpf' ich noch um die Netze,", "tokens": ["H\u00fcpf'", "ich", "noch", "um", "die", "Net\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "---+-+-", "measure": "unknown.measure.di"}, "line.3": {"text": "Die Cupido mir stellt:", "tokens": ["Die", "Cu\u00b7pi\u00b7do", "mir", "stellt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "All sein schmeichelndes B\u00fcbeln,", "tokens": ["All", "sein", "schmei\u00b7cheln\u00b7des", "B\u00fc\u00b7beln", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "All sein Kosen und Liebeln,", "tokens": ["All", "sein", "Ko\u00b7sen", "und", "Lie\u00b7beln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.6": {"text": "Hat noch nimmer mein Herz beschnellt!", "tokens": ["Hat", "noch", "nim\u00b7mer", "mein", "Herz", "be\u00b7schnellt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.7": {"line.1": {"text": "Traun! der seligen Triebe!", "tokens": ["Traun", "!", "der", "se\u00b7li\u00b7gen", "Trie\u00b7be", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Wann ein M\u00e4dchen vor Liebe", "tokens": ["Wann", "ein", "M\u00e4d\u00b7chen", "vor", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPR", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Und Empfindsamkeit stirbt,", "tokens": ["Und", "Emp\u00b7find\u00b7sam\u00b7keit", "stirbt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Nach dem Monde nur blicket,", "tokens": ["Nach", "dem", "Mon\u00b7de", "nur", "bli\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NE", "ADV", "VVFIN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "Nur Vergi\u00dfmeinnicht pfl\u00fccket,", "tokens": ["Nur", "Ver\u00b7gi\u00df\u00b7mein\u00b7nicht", "pfl\u00fc\u00b7cket", ","], "token_info": [