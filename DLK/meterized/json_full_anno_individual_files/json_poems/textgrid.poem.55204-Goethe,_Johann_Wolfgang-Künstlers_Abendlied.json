{"textgrid.poem.55204": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "K\u00fcnstlers Abendlied", "genre": "verse", "period": "N.A.", "pub_year": 1774, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach, da\u00df die innre Sch\u00f6pfungskraft", "tokens": ["Ach", ",", "da\u00df", "die", "inn\u00b7re", "Sch\u00f6p\u00b7fungs\u00b7kraft"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Durch meinen Sinn ersch\u00f6lle!", "tokens": ["Durch", "mei\u00b7nen", "Sinn", "er\u00b7sch\u00f6l\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df eine Bildung voller Saft", "tokens": ["Da\u00df", "ei\u00b7ne", "Bil\u00b7dung", "vol\u00b7ler", "Saft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Aus meinen Fingern qu\u00f6lle!", "tokens": ["Aus", "mei\u00b7nen", "Fin\u00b7gern", "qu\u00f6l\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich zittre nur, ich stottre nur", "tokens": ["Ich", "zitt\u00b7re", "nur", ",", "ich", "stott\u00b7re", "nur"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PPER", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und kann es doch nicht lassen;", "tokens": ["Und", "kann", "es", "doch", "nicht", "las\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "w