{"textgrid.poem.61054": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Der letzte Skalde", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im F\u00f6hrenwalde ging der Sturm,", "tokens": ["Im", "F\u00f6h\u00b7ren\u00b7wal\u00b7de", "ging", "der", "Sturm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mitternacht war die Stunde,", "tokens": ["Mit\u00b7ter\u00b7nacht", "war", "die", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Da trat in des alten S\u00e4ngers Turm", "tokens": ["Da", "trat", "in", "des", "al\u00b7ten", "S\u00e4n\u00b7gers", "Turm"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "ADJA", "NN", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Der Knab' mit tr\u00fcber Kunde:", "tokens": ["Der", "Knab'", "mit", "tr\u00fc\u00b7ber", "Kun\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "\u00bbh\u00f6rt auf mit dem Lesen nun, Herr Skiold,", "tokens": ["\u00bb", "h\u00f6rt", "auf", "mit", "dem", "Le\u00b7sen", "nun", ",", "Herr", "Skiold", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "VVFIN", "APPR", "APPR", "ART", "NN", "ADV", "$,", "NN", "NE", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Schaut auf von Eurem Buche!", "tokens": ["Schaut", "auf", "von", "Eu\u00b7rem", "Bu\u00b7che", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der alte Swerker lieb und hold,", "tokens": ["Der", "al\u00b7te", "Swer\u00b7ker", "lieb", "und", "hold", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der liegt im Leichentuche.\u00ab", "tokens": ["Der", "liegt", "im", "Lei\u00b7chen\u00b7tu\u00b7che", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VVFIN", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Da seufzte der S\u00e4nger tief empor:", "tokens": ["Da", "seufz\u00b7te", "der", "S\u00e4n\u00b7ger", "tief", "em\u00b7por", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "\u00bbsei Friede mit dem Biedern!", "tokens": ["\u00bb", "sei", "Frie\u00b7de", "mit", "dem", "Bie\u00b7dern", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Doch weh! Mir starb das letzte Ohr,", "tokens": ["Doch", "weh", "!", "Mir", "starb", "das", "letz\u00b7te", "Ohr", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKVZ", "$.", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das horchte meinen Liedern.", "tokens": ["Das", "horch\u00b7te", "mei\u00b7nen", "Lie\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wohl fechten die andern tagaus, tagein,", "tokens": ["Wohl", "fech\u00b7ten", "die", "an\u00b7dern", "ta\u00b7gaus", ",", "ta\u00b7gein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Doch sind sie des Skalden vergessen", "tokens": ["Doch", "sind", "sie", "des", "Skal\u00b7den", "ver\u00b7ges\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "ART", "NN", "VVPP"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Und werden einst selber vergessen sein,", "tokens": ["Und", "wer\u00b7den", "einst", "sel\u00b7ber", "ver\u00b7ges\u00b7sen", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADV", "VVPP", "VAINF", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "So k\u00fchn sie des Ruhms sich vermessen.", "tokens": ["So", "k\u00fchn", "sie", "des", "Ruhms", "sich", "ver\u00b7mes\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "ART", "NN", "PRF", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.5": {"line.1": {"text": "Ich aber habe zur Neige nun", "tokens": ["Ich", "a\u00b7ber", "ha\u00b7be", "zur", "Nei\u00b7ge", "nun"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VAFIN", "APPRART", "NN", "ADV"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Des Lebens Kelch geleeret;", "tokens": ["Des", "Le\u00b7bens", "Kelch", "ge\u00b7lee\u00b7ret", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wohl mag der S\u00e4nger gehn und ruhn,", "tokens": ["Wohl", "mag", "der", "S\u00e4n\u00b7ger", "gehn", "und", "ruhn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "VVINF", "KON", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wo niemand sein begehret.", "tokens": ["Wo", "nie\u00b7mand", "sein", "be\u00b7ge\u00b7hret", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VAINF", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.6": {"line.1": {"text": "Auf, Knabe, schwinge die Fackel stolz", "tokens": ["Auf", ",", "Kna\u00b7be", ",", "schwin\u00b7ge", "die", "Fa\u00b7ckel", "stolz"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "$,", "NN", "$,", "VVFIN", "ART", "NN", "ADJD"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Empor zur Balkendecke,", "tokens": ["Em\u00b7por", "zur", "Bal\u00b7ken\u00b7de\u00b7cke", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df prasselnd von dem d\u00fcrren Holz", "tokens": ["Da\u00df", "pras\u00b7selnd", "von", "dem", "d\u00fcr\u00b7ren", "Holz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "VVPP", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die volle Flamme lecke!", "tokens": ["Die", "vol\u00b7le", "Flam\u00b7me", "le\u00b7cke", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Dann eil' hinaus zum Walde frei,", "tokens": ["Dann", "eil'", "hin\u00b7aus", "zum", "Wal\u00b7de", "frei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nimm mit, was du erworben,", "tokens": ["Nimm", "mit", ",", "was", "du", "er\u00b7wor\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "PWS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und sage den Leuten rings, es sei", "tokens": ["Und", "sa\u00b7ge", "den", "Leu\u00b7ten", "rings", ",", "es", "sei"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "ADV", "$,", "PPER", "VAFIN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Der letzte Skalde gestorben.\u00ab -", "tokens": ["Der", "letz\u00b7te", "Skal\u00b7de", "ge\u00b7stor\u00b7ben", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word"], "pos": ["ART", "ADJA", "NN", "VVPP", "$.", "$(", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.8": {"line.1": {"text": "Und als der Knabe floh, da stand", "tokens": ["Und", "als", "der", "Kna\u00b7be", "floh", ",", "da", "stand"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "VVFIN", "$,", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schon auf den Zinnen der Hohe,", "tokens": ["Schon", "auf", "den", "Zin\u00b7nen", "der", "Ho\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Und wie ein k\u00f6niglich Gewand", "tokens": ["Und", "wie", "ein", "k\u00f6\u00b7nig\u00b7lich", "Ge\u00b7wand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Schlug um ihn her die Lohe.", "tokens": ["Schlug", "um", "ihn", "her", "die", "Lo\u00b7he", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Die Harfe hielt er goldesschwer", "tokens": ["Die", "Har\u00b7fe", "hielt", "er", "gol\u00b7des\u00b7schwer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und sang vom Turmesgipfel,", "tokens": ["Und", "sang", "vom", "Tur\u00b7mes\u00b7gip\u00b7fel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da neigten die F\u00f6hren rings umher", "tokens": ["Da", "neig\u00b7ten", "die", "F\u00f6h\u00b7ren", "rings", "um\u00b7her"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "PTKVZ"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ihre ger\u00f6teten Wipfel.", "tokens": ["Ih\u00b7re", "ge\u00b7r\u00f6\u00b7te\u00b7ten", "Wip\u00b7fel", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.10": {"line.1": {"text": "Doch als gemach das Lied verscholl,", "tokens": ["Doch", "als", "ge\u00b7mach", "das", "Lied", "ver\u00b7scholl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Verloschen auch die Flammen;", "tokens": ["Ver\u00b7lo\u00b7schen", "auch", "die", "Flam\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Es st\u00fcrzte dampfend mit Geroll", "tokens": ["Es", "st\u00fcrz\u00b7te", "damp\u00b7fend", "mit", "Ge\u00b7roll"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der alte Turm zusammen.", "tokens": ["Der", "al\u00b7te", "Turm", "zu\u00b7sam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Da lag nun unter Schutt und Brand", "tokens": ["Da", "lag", "nun", "un\u00b7ter", "Schutt", "und", "Brand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Begraben der letzte Skalde,", "tokens": ["Be\u00b7gra\u00b7ben", "der", "letz\u00b7te", "Skal\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und niemand sang im ganzen Land,", "tokens": ["Und", "nie\u00b7mand", "sang", "im", "gan\u00b7zen", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als nur die V\u00f6gel im Walde.", "tokens": ["Als", "nur", "die", "V\u00f6\u00b7gel", "im", "Wal\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.12": {"line.1": {"text": "Im F\u00f6hrenwalde ging der Sturm,", "tokens": ["Im", "F\u00f6h\u00b7ren\u00b7wal\u00b7de", "ging", "der", "Sturm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mitternacht war die Stunde,", "tokens": ["Mit\u00b7ter\u00b7nacht", "war", "die", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Da trat in des alten S\u00e4ngers Turm", "tokens": ["Da", "trat", "in", "des", "al\u00b7ten", "S\u00e4n\u00b7gers", "Turm"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "ADJA", "NN", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Der Knab' mit tr\u00fcber Kunde:", "tokens": ["Der", "Knab'", "mit", "tr\u00fc\u00b7ber", "Kun\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "\u00bbh\u00f6rt auf mit dem Lesen nun, Herr Skiold,", "tokens": ["\u00bb", "h\u00f6rt", "auf", "mit", "dem", "Le\u00b7sen", "nun", ",", "Herr", "Skiold", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "VVFIN", "APPR", "APPR", "ART", "NN", "ADV", "$,", "NN", "NE", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Schaut auf von Eurem Buche!", "tokens": ["Schaut", "auf", "von", "Eu\u00b7rem", "Bu\u00b7che", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "APPR", "PPOSAT", "NN", "$."], "meter": "-+