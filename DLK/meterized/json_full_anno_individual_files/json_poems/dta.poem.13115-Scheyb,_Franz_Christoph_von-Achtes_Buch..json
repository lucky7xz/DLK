{"dta.poem.13115": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Achtes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201ewas k\u00f6nnen Stein und Erz und Farb und Winckelma\u00df,", "tokens": ["\u201e", "was", "k\u00f6n\u00b7nen", "Stein", "und", "Erz", "und", "Farb", "und", "Win\u00b7ckel\u00b7ma\u00df", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VMFIN", "NN", "KON", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "\u201ewann ich fie Sprache-lo\u00df in ihrem Prangen la\u00df?", "tokens": ["\u201e", "wann", "ich", "fie", "Spra\u00b7che\u00b7lo\u00df", "in", "ih\u00b7rem", "Pran\u00b7gen", "la\u00df", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "PPER", "VVFIN", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "\u201ejhr habt es diese Nacht mit Wunder angesehen:", "tokens": ["\u201e", "jhr", "habt", "es", "die\u00b7se", "Nacht", "mit", "Wun\u00b7der", "an\u00b7ge\u00b7se\u00b7hen", ":"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "PPER", "PDAT", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u201ekonnt etwas ohne mich mit wahrer Zier bestehen?", "tokens": ["\u201e", "konnt", "et\u00b7was", "oh\u00b7ne", "mich", "mit", "wah\u00b7rer", "Zier", "be\u00b7ste\u00b7hen", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VMFIN", "PIS", "APPR", "PRF", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "485\u201dEs braucht des Lobens nicht; fragt nur den eignen Sinn!", "tokens": ["\"", "Es", "braucht", "des", "Lo\u00b7bens", "nicht", ";", "fragt", "nur", "den", "eig\u00b7nen", "Sinn", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "ART", "NN", "PTKNEG", "$.", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+---+-+", "measure": "unknown.measure.penta"}, "line.6": {"text": "\u201eso nennt ihr mich gewi\u00df der K\u00fcnste Meisterinn.", "tokens": ["\u201e", "so", "nennt", "ihr", "mich", "ge\u00b7wi\u00df", "der", "K\u00fcns\u00b7te", "Meis\u00b7te\u00b7rinn", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "PRF", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}