{"textgrid.poem.54197": {"metadata": {"author": {"name": "Abschatz, Hans A\u00dfmann von", "birth": "N.A.", "death": "N.A."}, "title": "Morgen-Andacht", "genre": "verse", "period": "N.A.", "pub_year": 1672, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der begl\u00e4nzte Mond erbleichet", "tokens": ["Der", "be\u00b7gl\u00e4nz\u00b7te", "Mond", "er\u00b7blei\u00b7chet"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Von der nahen Sonne Pracht/", "tokens": ["Von", "der", "na\u00b7hen", "Son\u00b7ne", "Pracht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aller Sternen Heer entweichet", "tokens": ["Al\u00b7ler", "Ster\u00b7nen", "Heer", "ent\u00b7wei\u00b7chet"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit der hingelegten Nacht:", "tokens": ["Mit", "der", "hin\u00b7ge\u00b7leg\u00b7ten", "Nacht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Auff mein Hertz/ und la\u00df der S\u00fcnden", "tokens": ["Auff", "mein", "Hertz", "/", "und", "la\u00df", "der", "S\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$(", "KON", "VVIMP", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Finsterni\u00df und Schlaff dahinden.", "tokens": ["Fins\u00b7ter\u00b7ni\u00df", "und", "Schlaff", "da\u00b7hin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Den gew\u00f6lbten Himmels-Bogen/", "tokens": ["Den", "ge\u00b7w\u00f6lb\u00b7ten", "Him\u00b7mels\u00b7Bo\u00b7gen", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Den Saffirnen Wunder-Bau/", "tokens": ["Den", "Saf\u00b7fir\u00b7nen", "Wun\u00b7der\u00b7Bau", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$("], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Hielt die dunckle Nacht umzogen/", "tokens": ["Hielt", "die", "dunck\u00b7le", "Nacht", "um\u00b7zo\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "VVIZU", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die geraume Sternen-Au", "tokens": ["Die", "ge\u00b7rau\u00b7me", "Ster\u00b7nen\u00b7Au"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Hegte zu des H\u00f6chsten Ruhme", "tokens": ["Heg\u00b7te", "zu", "des", "H\u00f6chs\u00b7ten", "Ruh\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Manche Licht- und Feuer-Blume.", "tokens": ["Man\u00b7che", "Licht", "und", "Feu\u00b7er\u00b7Blu\u00b7me", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "TRUNC", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ihre Zier mu\u00df nun erblassen/", "tokens": ["Ih\u00b7re", "Zier", "mu\u00df", "nun", "er\u00b7blas\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "ADV", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ihr entlehnter Glantz stirbt hin;", "tokens": ["Ihr", "ent\u00b7lehn\u00b7ter", "Glantz", "stirbt", "hin", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "So mu\u00df auch der Mensch verlassen", "tokens": ["So", "mu\u00df", "auch", "der", "Mensch", "ver\u00b7las\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ADV", "ART", "NN", "VVINF"], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.4": {"text": "Ehre/ Wollust und Gewinn:", "tokens": ["Eh\u00b7re", "/", "Wol\u00b7lust", "und", "Ge\u00b7winn", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$(", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "M\u00fche dich das Licht zu finden/", "tokens": ["M\u00fc\u00b7he", "dich", "das", "Licht", "zu", "fin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "ART", "NN", "PTKZU", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Das zu keiner Zeit kan schwinden.", "tokens": ["Das", "zu", "kei\u00b7ner", "Zeit", "kan", "schwin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "PIAT", "NN", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Mu\u00df die Welt die Sternen missen/", "tokens": ["Mu\u00df", "die", "Welt", "die", "Ster\u00b7nen", "mis\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "ART", "NN", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und ohn alle Lichter stehn/", "tokens": ["Und", "ohn", "al\u00b7le", "Lich\u00b7ter", "stehn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIAT", "NN", "VVINF", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Eh sie kan die Sonne gr\u00fcssen/", "tokens": ["Eh", "sie", "kan", "die", "Son\u00b7ne", "gr\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VMFIN", "ART", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Also pflegt es uns zu gehn/", "tokens": ["Al\u00b7so", "pflegt", "es", "uns", "zu", "gehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "PTKZU", "VVINF", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Da\u00df wir durch des Todes Schatten", "tokens": ["Da\u00df", "wir", "durch", "des", "To\u00b7des", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Erst zur Ewigkeit gerathen.", "tokens": ["Erst", "zur", "E\u00b7wig\u00b7keit", "ge\u00b7ra\u00b7then", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Folgt der schn\u00f6den Eitelkeiten", "tokens": ["Folgt", "der", "schn\u00f6\u00b7den", "Ei\u00b7tel\u00b7kei\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wandelbarer Mond der Nacht/", "tokens": ["Wan\u00b7del\u00b7ba\u00b7rer", "Mond", "der", "Nacht", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": 