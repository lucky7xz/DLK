], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mu\u00df vollf\u00fchren seinen Lauff;", "tokens": ["Mu\u00df", "voll\u00b7f\u00fch\u00b7ren", "sei\u00b7nen", "Lauff", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "La\u00df die Menge solcher Wunder", "tokens": ["La\u00df", "die", "Men\u00b7ge", "sol\u00b7cher", "Wun\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "ART", "NN", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Seyn der todten Andacht Zunder.", "tokens": ["Seyn", "der", "tod\u00b7ten", "An\u00b7dacht", "Zun\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ART", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Sieh/ die Perlen-Thr\u00e4nen thauen/", "tokens": ["Sieh", "/", "die", "Per\u00b7len\u00b7Thr\u00e4\u00b7nen", "thau\u00b7en", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$(", "ART", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zu erquicken Laub und Gra\u00df/", "tokens": ["Zu", "er\u00b7qui\u00b7cken", "Laub", "und", "Gra\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "NN", "KON", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo du deiner Wangen Auen", "tokens": ["Wo", "du", "dei\u00b7ner", "Wan\u00b7gen", "Au\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Machst in wahrer Busse na\u00df/", "tokens": ["Machst", "in", "wah\u00b7rer", "Bus\u0