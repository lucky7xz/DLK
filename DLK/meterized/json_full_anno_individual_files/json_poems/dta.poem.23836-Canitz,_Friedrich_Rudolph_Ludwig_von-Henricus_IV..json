{"dta.poem.23836": {"metadata": {"author": {"name": "Canitz, Friedrich Rudolph Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "Henricus IV.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1700", "urn": "urn:nbn:de:kobv:b4-200905197532", "language": ["de:0.99"], "booktitle": "[Canitz, Friedrich Rudolph Ludwig von]: Neben-Stunden Unterschiedener Gedichte. [Hrsg. v. Joachim Lange]. Berlin, 1700."}, "poem": {"stanza.1": {"line.1": {"text": "Nunmehr verf\u00e4llt das Reich in Auffruhr/ Mord und", "tokens": ["Nun\u00b7mehr", "ver\u00b7f\u00e4llt", "das", "Reich", "in", "Auf\u00b7fruhr", "/", "Mord", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "NN", "$(", "NN", "KON"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und ob ich gleich mit Ruhm viel Gegen-K\u00e4yser d\u00e4mpfe/", "tokens": ["Und", "ob", "ich", "gleich", "mit", "Ruhm", "viel", "Ge\u00b7gen\u00b7K\u00e4y\u00b7ser", "d\u00e4mp\u00b7fe", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "APPR", "NN", "PIAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und mehr als 60. mahl in Schlachten gl\u00fccklich k\u00e4mpfe/", "tokens": ["Und", "mehr", "als", "60.", "mahl", "in", "Schlach\u00b7ten", "gl\u00fcck\u00b7lich", "k\u00e4mp\u00b7fe", "/"], "token_info": ["word", "word", "word", "ordinal", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "KOKOM", "ADJA", "ADV", "APPR", "NN", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Beh\u00e4lt der P\u00e4bste Bann doch endlich Oberhand.", "tokens": ["Be\u00b7h\u00e4lt", "der", "P\u00e4bs\u00b7te", "Bann", "doch", "end\u00b7lich", "O\u00b7ber\u00b7hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "ADV", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Darauf mir wiederf\u00e4hrt/ was kaum die Nachwelt", "tokens": ["Da\u00b7rauf", "mir", "wie\u00b7der\u00b7f\u00e4hrt", "/", "was", "kaum", "die", "Nach\u00b7welt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PAV", "PPER", "VVFIN", "$(", "PWS", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Da\u00df mir mein eigner Sohn/ die Cron und Ehre raubt.", "tokens": ["Da\u00df", "mir", "mein", "eig\u00b7ner", "Sohn", "/", "die", "Cron", "und", "Eh\u00b7re", "raubt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "ADJA", "NN", "$(", "ART", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}