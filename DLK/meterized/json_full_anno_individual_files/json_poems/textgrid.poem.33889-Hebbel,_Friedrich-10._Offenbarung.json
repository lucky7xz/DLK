r der Tod vertraut.", "tokens": ["Was", "dir", "der", "Tod", "ver\u00b7traut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und wenn das gro\u00dfe L\u00f6sungswort", "tokens": ["Und", "wenn", "das", "gro\u00b7\u00dfe", "L\u00f6\u00b7sungs\u00b7wort"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Auch mit dem Traum entschwand,", "tokens": ["Auch", "mit", "dem", "Traum", "ent\u00b7schwand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "---+-+", "measure": "unknown.measure.di"}, "line.7": {"text": "So wirkt es doch im Tiefsten fort,", "tokens": ["So", "wirkt", "es", "doch", "im", "Tiefs\u00b7ten", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Gewaltig, unerkannt!", "tokens": ["Ge\u00b7wal\u00b7tig", ",", "un\u00b7er\u00b7kannt", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}