{"dta.poem.19875": {"metadata": {"author": {"name": "B\u00fcrger, Gottfried August", "birth": "N.A.", "death": "N.A."}, "title": "Der Bauer .  \n An seinen Durchlauchtigen Tyrannen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1778", "urn": "urn:nbn:de:kobv:b4-20090519672", "language": ["de:0.99"], "booktitle": "B\u00fcrger, Gottfried August: Gedichte. G\u00f6ttingen, 1778."}, "poem": {"stanza.1": {"line.1": {"text": "Wer bist du, F\u00fcrst? da\u00df ohne Scheu               ", "tokens": ["Wer", "bist", "du", ",", "F\u00fcrst", "?", "da\u00df", "oh\u00b7ne", "Scheu"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PWS", "VAFIN", "PPER", "$,", "NN", "$.", "KOUS", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "ZerrrollenZerrollen mich dein Wagenrad,               ", "tokens": ["Zerr\u00b7rol\u00b7len", "Zer\u00b7rol\u00b7len", "mich", "dein", "Wa\u00b7gen\u00b7rad", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Dein Ros zerschlagen darf.", "tokens": ["Dein", "Ros", "zer\u00b7schla\u00b7gen", "darf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wer bist du, F\u00fcrst? da\u00df in mein Fleisch", "tokens": ["Wer", "bist", "du", ",", "F\u00fcrst", "?", "da\u00df", "in", "mein", "Fleisch"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "PPER", "$,", "NN", "$.", "KOUS", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dein Freund, dein Jagdhund, ungebla\u00fct", "tokens": ["Dein", "Freund", ",", "dein", "Jagd\u00b7hund", ",", "un\u00b7ge\u00b7bla\u00fct"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Darf Klau\u2019 und Rachen haun.", "tokens": ["Darf", "Klau'", "und", "Ra\u00b7chen", "haun", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wer bist du? da\u00df, durch Saat und Forst,", "tokens": ["Wer", "bist", "du", "?", "da\u00df", ",", "durch", "Saat", "und", "Forst", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "$.", "KOUS", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Hurrah deiner Jagd mich treibt,", "tokens": ["Das", "Hur\u00b7rah", "dei\u00b7ner", "Jagd", "mich", "treibt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Entathmet, wie das Wild. \u2014", "tokens": ["En\u00b7tath\u00b7met", ",", "wie", "das", "Wild", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "$,", "PWAV", "ART", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Die Saat, so deine Jagd zertrit,", "tokens": ["Die", "Saat", ",", "so", "dei\u00b7ne", "Jagd", "zer\u00b7trit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was Ros, und Hund, und Du verschlingst,", "tokens": ["Was", "Ros", ",", "und", "Hund", ",", "und", "Du", "ver\u00b7schlingst", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "NE", "$,", "KON", "NN", "$,", "KON", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das Brod, du F\u00fcrst, ist mein.", "tokens": ["Das", "Brod", ",", "du", "F\u00fcrst", ",", "ist", "mein", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "NN", "$,", "VAFIN", "PPOSAT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Du F\u00fcrst hast nicht, bei Egg\u2019 und Pflug,", "tokens": ["Du", "F\u00fcrst", "hast", "nicht", ",", "bei", "Eg\u00b7g'", "und", "Pflug", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "VAFIN", "PTKNEG", "$,", "APPR", "NE", "KON", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Hast nicht den Erntetag durchschwizt.", "tokens": ["Hast", "nicht", "den", "Ern\u00b7te\u00b7tag", "durch\u00b7schwizt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mein, mein ist Fleis und Brod! \u2014", "tokens": ["Mein", ",", "mein", "ist", "Fleis", "und", "Brod", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "$,", "PPOSAT", "VAFIN", "NN", "KON", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Ha! du w\u00e4rst Obrigkeit von Gott?", "tokens": ["Ha", "!", "du", "w\u00e4rst", "Ob\u00b7rig\u00b7keit", "von", "Gott", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PPER", "VAFIN", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gott spendet Segen aus; du raubst!", "tokens": ["Gott", "spen\u00b7det", "Se\u00b7gen", "aus", ";", "du", "raubst", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NN", "PTKVZ", "$.", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du nicht von Gott, Tyran!", "tokens": ["Du", "nicht", "von", "Gott", ",", "Ty\u00b7ran", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "PTKNEG", "APPR", "NN", "$,", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}