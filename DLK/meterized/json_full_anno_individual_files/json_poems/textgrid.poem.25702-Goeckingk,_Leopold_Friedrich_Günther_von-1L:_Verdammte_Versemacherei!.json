{"textgrid.poem.25702": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Verdammte Versemacherei!", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Verdammte Versemacherei!", "tokens": ["Ver\u00b7damm\u00b7te", "Ver\u00b7se\u00b7ma\u00b7che\u00b7rei", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was hast du angerichtet?", "tokens": ["Was", "hast", "du", "an\u00b7ge\u00b7rich\u00b7tet", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Uns unsers Lebens einz'gen Mai", "tokens": ["Uns", "un\u00b7sers", "Le\u00b7bens", "einz'\u00b7gen", "Mai"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zum Kuckuck hingedichtet?", "tokens": ["Zum", "Ku\u00b7ckuck", "hin\u00b7ge\u00b7dich\u00b7tet", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Gevatter ", "tokens": ["Ge\u00b7vat\u00b7ter"], "token_info": ["word"], "pos": ["NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Sind wir nicht brave Thoren,", "tokens": ["Sind", "wir", "nicht", "bra\u00b7ve", "Tho\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Da\u00df wir, durch selbstgemachte Qual,", "tokens": ["Da\u00df", "wir", ",", "durch", "selbst\u00b7ge\u00b7mach\u00b7te", "Qual", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Den sch\u00f6nen Mai verloren?", "tokens": ["Den", "sch\u00f6\u00b7nen", "Mai", "ver\u00b7lo\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Was hat man von dem Dichten? Hum!", "tokens": ["Was", "hat", "man", "von", "dem", "Dich\u00b7ten", "?", "Hum", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "VAFIN", "PIS", "APPR", "ART", "NN", "$.", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Vielleicht das bischen Ehre:", "tokens": ["Viel\u00b7leicht", "das", "bi\u00b7schen", "Eh\u00b7re", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Gekannt zu seyn vom Publikum? \u2013", "tokens": ["Ge\u00b7kannt", "zu", "seyn", "vom", "Pub\u00b7li\u00b7kum", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVPP", "PTKZU", "VAINF", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Ich dachte was mir w\u00e4re!", "tokens": ["Ich", "dach\u00b7te", "was", "mir", "w\u00e4\u00b7re", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PWS", "PPER", "VAFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Mag seyn, da\u00df man bei Tafeln spricht,", "tokens": ["Mag", "seyn", ",", "da\u00df", "man", "bei", "Ta\u00b7feln", "spricht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VAINF", "$,", "KOUS", "PIS", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Wenn den durchlauchten B\u00e4uchen", "tokens": ["Wenn", "den", "durch\u00b7lauch\u00b7ten", "B\u00e4u\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "Die Zeit lang w\u00e4hrt: Ist ", "tokens": ["Die", "Zeit", "lang", "w\u00e4hrt", ":", "Ist"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "ADJD", "VVFIN", "$.", "VAFIN"], "meter": "--+-+", "measure": "anapaest.init"}, "line.16": {"text": "Amtmann zu ", "tokens": ["Amt\u00b7mann", "zu"], "token_info": ["word", "word"], "pos": ["NN", "PTKZU"], "meter": "+-+", "measure": "trochaic.di"}, "line.17": {"text": "Ein Fr\u00e4ulein thut dir wohl sogar", "tokens": ["Ein", "Fr\u00e4u\u00b7lein", "thut", "dir", "wohl", "so\u00b7gar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Die Gnad', und fragt nicht minder:", "tokens": ["Die", "Gnad'", ",", "und", "fragt", "nicht", "min\u00b7der", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KON", "VVFIN", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.19": {"text": "Tr\u00e4gt denn der ", "tokens": ["Tr\u00e4gt", "denn", "der"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ADV", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.20": {"text": "Hat er schon Frau und Kinder?", "tokens": ["Hat", "er", "schon", "Frau", "und", "Kin\u00b7der", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.21": {"text": "Ein Amtsauditor geht, bepackt", "tokens": ["Ein", "Amt\u00b7sau\u00b7di\u00b7tor", "geht", ",", "be\u00b7packt"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Mit deinem Buch', zu Sch\u00f6nen,", "tokens": ["Mit", "dei\u00b7nem", "Buch'", ",", "zu", "Sch\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.23": {"text": "Und lieset, da\u00df der Balken knackt", "tokens": ["Und", "lie\u00b7set", ",", "da\u00df", "der", "Bal\u00b7ken", "knackt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "KOUS", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Und alle Fenster dr\u00f6hnen.", "tokens": ["Und", "al\u00b7le", "Fens\u00b7ter", "dr\u00f6h\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.25": {"text": "Das h\u00f6rt denn ein Student und schreit:", "tokens": ["Das", "h\u00f6rt", "denn", "ein", "Stu\u00b7dent", "und", "schreit", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ART", "NN", "KON", "ADJD", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.26": {"text": "\u00bbund wohnt' er bei den Sternen!", "tokens": ["\u00bb", "und", "wohnt'", "er", "bei", "den", "Ster\u00b7nen", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.27": {"text": "Ich mu\u00df \u2013 ist ", "tokens": ["Ich", "mu\u00df", "\u2013", "ist"], "token_info": ["word", "word", "punct", "word"], "pos": ["PPER", "VMFIN", "$(", "VAFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.28": {"text": "Mu\u00df ", "tokens": ["Mu\u00df"], "token_info": ["word"], "pos": ["VMFIN"], "meter": "+", "measure": "single.up"}, "line.29": {"text": "Und eh' Herr ", "tokens": ["Und", "eh'", "Herr"], "token_info": ["word", "word", "word"], "pos": ["KON", "KOUS", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.30": {"text": "Kommt mein Signor geritten,", "tokens": ["Kommt", "mein", "Sig\u00b7nor", "ge\u00b7rit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.31": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.32": {"text": "Mu\u00df ihn zum Essen bitten.", "tokens": ["Mu\u00df", "ihn", "zum", "Es\u00b7sen", "bit\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.33": {"text": "Da schlingt er nun den Truthahn ein,", "tokens": ["Da", "schlingt", "er", "nun", "den", "Trut\u00b7hahn", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Den du mir aufbewahrtest,", "tokens": ["Den", "du", "mir", "auf\u00b7be\u00b7wahr\u00b7test", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.35": {"text": "Und trinkt \u2013 hol' ihn der Fuchs! \u2013 den Wein,", "tokens": ["Und", "trinkt", "\u2013", "hol'", "ihn", "der", "Fuchs", "!", "\u2013", "den", "Wein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$(", "VVIMP", "PPER", "ART", "NE", "$.", "$(", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "Den du f\u00fcr mich erspartest.", "tokens": ["Den", "du", "f\u00fcr", "mich", "er\u00b7spar\u00b7test", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.37": {"text": "Er r\u00fchmt dir ba\u00df sein gutes Herz,", "tokens": ["Er", "r\u00fchmt", "dir", "ba\u00df", "sein", "gu\u00b7tes", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.38": {"text": "Will Freundschaft mit dir treiben,", "tokens": ["Will", "Freund\u00b7schaft", "mit", "dir", "trei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.39": {"text": "Und droht sogar, \u2013 o H\u00f6llenschmerz! \u2013", "tokens": ["Und", "droht", "so\u00b7gar", ",", "\u2013", "o", "H\u00f6l\u00b7len\u00b7schmerz", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "ADV", "$,", "$(", "FM", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.40": {"text": "Recht oft an dich zu schreiben.", "tokens": ["Recht", "oft", "an", "dich", "zu", "schrei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.41": {"text": "Das macht: Manch ehrliches Journal", "tokens": ["Das", "macht", ":", "Manch", "ehr\u00b7li\u00b7ches", "Jour\u00b7nal"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VVFIN", "$.", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.42": {"text": "Lie\u00df laut dein Lob erschallen;", "tokens": ["Lie\u00df", "laut", "dein", "Lob", "er\u00b7schal\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.43": {"text": "Allein, wann las denn wohl einmal", "tokens": ["Al\u00b7lein", ",", "wann", "las", "denn", "wohl", "ein\u00b7mal"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PWAV", "VVFIN", "ADV", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.44": {"text": "Herr ", "tokens": ["Herr"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.45": {"text": "Und lie\u00df ich dich in Kupfer, schier", "tokens": ["Und", "lie\u00df", "ich", "dich", "in", "Kup\u00b7fer", ",", "schier"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "VVFIN", "PPER", "PRF", "APPR", "NN", "$,", "ADJA"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.46": {"text": "Von ", "tokens": ["Von"], "token_info": ["word"], "pos": ["APPR"], "meter": "-", "measure": "single.down"}, "line.47": {"text": "Hilft dir es etwas, wenn von dir", "tokens": ["Hilft", "dir", "es", "et\u00b7was", ",", "wenn", "von", "dir"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "$,", "KOUS", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.48": {"text": "Die Leut' ein Weilchen sprechen?", "tokens": ["Die", "Leut'", "ein", "Weil\u00b7chen", "spre\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.49": {"text": "Was hast du von dem allen? Sklav!", "tokens": ["Was", "hast", "du", "von", "dem", "al\u00b7len", "?", "Sklav", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "APPR", "ART", "PIAT", "$.", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.50": {"text": "Wenn ich's zusammenpresse,", "tokens": ["Wenn", "ich's", "zu\u00b7sam\u00b7men\u00b7pres\u00b7se", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.51": {"text": "Was ist es, als: Despoten-Schlaf", "tokens": ["Was", "ist", "es", ",", "als", ":", "Despo\u00b7ten\u00b7Schlaf"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["PWS", "VAFIN", "PPER", "$,", "KOUS", "$.", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.52": {"text": "Und Inquisiten-Bl\u00e4sse?", "tokens": ["Und", "In\u00b7qui\u00b7si\u00b7ten\u00b7Bl\u00e4s\u00b7se", "?"], "token_info": ["word", "word", "punct"], "pos": ["KON", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.53": {"text": "H\u00f6r' auf! Ich gab mein Herz dir hin,", "tokens": ["H\u00f6r'", "auf", "!", "Ich", "gab", "mein", "Herz", "dir", "hin", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$.", "PPER", "VVFIN", "PPOSAT", "NN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.54": {"text": "Eh' du ein Blatt geschrieben;", "tokens": ["Eh'", "du", "ein", "Blatt", "ge\u00b7schrie\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.55": {"text": "H\u00f6r' auf! und die Frau Amtmannin", "tokens": ["H\u00f6r'", "auf", "!", "und", "die", "Frau", "Amt\u00b7man\u00b7nin"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "PTKVZ", "$.", "KON", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.56": {"text": "Wird dich noch lieber lieben.", "tokens": ["Wird", "dich", "noch", "lie\u00b7ber", "lie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.57": {"text": "H\u00f6r' auf! Als Dichter kennt man dich,", "tokens": ["H\u00f6r'", "auf", "!", "Als", "Dich\u00b7ter", "kennt", "man", "dich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$.", "KOUS", "NN", "VVFIN", "PIS", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.58": {"text": "Als Mensch lebst du verborgen;", "tokens": ["Als", "Mensch", "lebst", "du", "ver\u00b7bor\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.59": {"text": "Kein Christenkind bek\u00fcmmert sich", "tokens": ["Kein", "Chris\u00b7ten\u00b7kind", "be\u00b7k\u00fcm\u00b7mert", "sich"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.60": {"text": "Um alle deine Sorgen.", "tokens": ["Um", "al\u00b7le", "dei\u00b7ne", "Sor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "PIAT", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.61": {"text": "Ja solltest du auch den Homer", "tokens": ["Ja", "soll\u00b7test", "du", "auch", "den", "Ho\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "VMFIN", "PPER", "ADV", "ART", "NE"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.62": {"text": "In Jamben \u00fcbersetzen,", "tokens": [