{"textgrid.poem.54299": {"metadata": {"author": {"name": "Ziegler, Christiana Mariana von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich warff ohnl\u00e4ngstens meine Fl\u00f6the", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich warff ohnl\u00e4ngstens meine Fl\u00f6the", "tokens": ["Ich", "warff", "ohn\u00b7l\u00e4ngs\u00b7tens", "mei\u00b7ne", "Fl\u00f6\u00b7the"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "F\u00fcnff Klafftern tieff in Abgrund nein,", "tokens": ["F\u00fcnff", "Klaff\u00b7tern", "tieff", "in", "Ab\u00b7grund", "nein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "ADJD", "APPR", "NN", "PTKANT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und dachte: du wilst kein Poete", "tokens": ["Und", "dach\u00b7te", ":", "du", "wilst", "kein", "Poe\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$.", "PPER", "VMFIN", "PIAT", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Noch Maclaturen Dichter seyn.", "tokens": ["Noch", "Ma\u00b7cla\u00b7tu\u00b7ren", "Dich\u00b7ter", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein jeder St\u00fcmper nimmt die Leyer,", "tokens": ["Ein", "je\u00b7der", "St\u00fcm\u00b7per", "nimmt", "die", "Le\u00b7yer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "In die noch ungewaschne Hand,", "tokens": ["In", "die", "noch", "un\u00b7ge\u00b7waschne", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Wird ihm davor nur noch zwey Dreyer", "tokens": ["Wird", "ihm", "da\u00b7vor", "nur", "noch", "zwey", "Drey\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PAV", "ADV", "ADV", "CARD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Zum Gratiale zugewand.", "tokens": ["Zum", "Gra\u00b7ti\u00b7a\u00b7le", "zu\u00b7ge\u00b7wand", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Man geht mit Mi\u00dfgebuhrten tr\u00e4chtig,", "tokens": ["Man", "geht", "mit", "Mi\u00df\u00b7ge\u00b7buhr\u00b7ten", "tr\u00e4ch\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die man zur Welt mit Greisen bringt,", "tokens": ["Die", "man", "zur", "Welt", "mit", "Grei\u00b7sen", "bringt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "APPRART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und handelt nicht dabey bed\u00e4chtig,", "tokens": ["Und", "han\u00b7delt", "nicht", "da\u00b7bey", "be\u00b7d\u00e4ch\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "PAV", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Was Wunder, wann es l\u00e4ppisch klingt?", "tokens": ["Was", "Wun\u00b7der", ",", "wann", "es", "l\u00e4p\u00b7pisch", "klingt", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "$,", "PWAV", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Schaar beth\u00f6rter Musen-S\u00f6hne", "tokens": ["Die", "Schaar", "be\u00b7th\u00f6r\u00b7ter", "Mu\u00b7sen\u00b7S\u00f6h\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Vermehret sich von Tag zu Tag,", "tokens": ["Ver\u00b7meh\u00b7ret", "sich", "von", "Tag", "zu", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da\u00df man ihr Klimpern und Geth\u00f6ne", "tokens": ["Da\u00df", "man", "ihr", "Klim\u00b7pern", "und", "Ge\u00b7th\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "PPOSAT", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Mit anzuh\u00f6ren kaum vermag.", "tokens": ["Mit", "an\u00b7zu\u00b7h\u00f6\u00b7ren", "kaum", "ver\u00b7mag", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "VVIZU", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Die G\u00e4nse werden um sich beissen,", "tokens": ["Die", "G\u00e4n\u00b7se", "wer\u00b7den", "um", "sich", "beis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wofern, ihr St\u00fcmper unsrer Zeit!", "tokens": ["Wo\u00b7fern", ",", "ihr", "St\u00fcm\u00b7per", "uns\u00b7rer", "Zeit", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den Kiel wolt aus den Schwingen reissen,", "tokens": ["Den", "Kiel", "wolt", "aus", "den", "Schwin\u00b7gen", "reis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VMFIN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der klugen Dichtern nur geweyht.", "tokens": ["Der", "klu\u00b7gen", "Dich\u00b7tern", "nur", "ge\u00b7weyht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Eur Pegasus mu\u00df stetig hincken,", "tokens": ["Eur", "Pe\u00b7ga\u00b7sus", "mu\u00df", "ste\u00b7tig", "hin\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Weil ihn Vulcanus nicht beschl\u00e4gt.", "tokens": ["Weil", "ihn", "Vul\u00b7ca\u00b7nus", "nicht", "be\u00b7schl\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NE", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der Musen-Quell hebt an zu stincken,", "tokens": ["Der", "Mu\u00b7sen\u00b7Quell", "hebt", "an", "zu", "stin\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ihr habt Morast und Sumpff erregt.", "tokens": ["Ihr", "habt", "Mo\u00b7rast", "und", "Sumpff", "er\u00b7regt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ihr d\u00fcrfft euch nicht zum H\u00fcgel wagen,", "tokens": ["Ihr", "d\u00fcrfft", "euch", "nicht", "zum", "H\u00fc\u00b7gel", "wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ich meyn Apollens hohen Thron;", "tokens": ["Ich", "meyn", "A\u00b7pol\u00b7lens", "ho\u00b7hen", "Thron", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er kan das Leyern nicht vertragen,", "tokens": ["Er", "kan", "das", "Ley\u00b7ern", "nicht", "ver\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sein Ohr g\u00e4llt ihm von solchen Thon.", "tokens": ["Sein", "Ohr", "g\u00e4llt", "ihm", "von", "sol\u00b7chen", "Thon", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Musen l\u00f6fflen nicht mit Narren,", "tokens": ["Die", "Mu\u00b7sen", "l\u00f6ff\u00b7len", "nicht", "mit", "Nar\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sie lieben den, der Kunst besitzt,", "tokens": ["Sie", "lie\u00b7ben", "den", ",", "der", "Kunst", "be\u00b7sitzt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "$,", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der Pindus braucht nicht eure Sparren,", "tokens": ["Der", "Pin\u00b7dus", "braucht", "nicht", "eu\u00b7re", "Spar\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Er ist mit Pfeilern schon gest\u00fctzt.", "tokens": ["Er", "ist", "mit", "Pfei\u00b7lern", "schon", "ge\u00b7st\u00fctzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "O lasset doch eur tummes Schreiben,", "tokens": ["O", "las\u00b7set", "doch", "eur", "tum\u00b7mes", "Schrei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das ihr uns h\u00e4uffig zugedacht,", "tokens": ["Das", "ihr", "uns", "h\u00e4uf\u00b7fig", "zu\u00b7ge\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "PRF", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und das Geschmiere k\u00fcnfftig bleiben,", "tokens": ["Und", "das", "Ge\u00b7schmie\u00b7re", "k\u00fcnff\u00b7tig", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dieweil die Welt nur dr\u00fcber lacht.", "tokens": ["Die\u00b7weil", "die", "Welt", "nur", "dr\u00fc\u00b7ber", "lacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "PAV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ihr reicht Euterpen nicht an N--", "tokens": ["Ihr", "reicht", "Eu\u00b7ter\u00b7pen", "nicht", "an", "N\u00b7"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NE", "PTKNEG", "APPR", "TRUNC"], "meter": "meter.parsing.error", "measure": "measure.parsing.error"}, "line.6": {"text": "Geschweige da\u00df ihr selbge k\u00fcst.", "tokens": ["Ge\u00b7schwei\u00b7ge", "da\u00df", "ihr", "selb\u00b7ge", "k\u00fcst", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOUS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Redt nur forthin, wie euch der Schnabel", "tokens": ["Redt", "nur", "for\u00b7thin", ",", "wie", "euch", "der", "Schna\u00b7bel"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "ADV", "PTKVZ", "$,", "PWAV", "PPER", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Zur Prosa blo\u00df gewachsen ist.", "tokens": ["Zur", "Pro\u00b7sa", "blo\u00df", "ge\u00b7wach\u00b7sen", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ich warff ohnl\u00e4ngstens meine Fl\u00f6the", "tokens": ["Ich", "warff", "ohn\u00b7l\u00e4ngs\u00b7tens", "mei\u00b7ne", "Fl\u00f6\u00b7the"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "F\u00fcnff Klafftern tieff in Abgrund nein,", "tokens": ["F\u00fcnff", "Klaff\u00b7tern", "tieff", "in", "Ab\u00b7grund", "nein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "ADJD", "APPR", "NN", "PTK