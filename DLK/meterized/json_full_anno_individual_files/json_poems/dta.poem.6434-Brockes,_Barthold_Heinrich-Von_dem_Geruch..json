{"dta.poem.6434": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Geruch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Stimmt man demselben Schein der Wahrheit ferner bey;", "tokens": ["Stimmt", "man", "dem\u00b7sel\u00b7ben", "Schein", "der", "Wahr\u00b7heit", "fer\u00b7ner", "bey", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PDAT", "NN", "ART", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So zeugen die verschiedene Gespinnste,", "tokens": ["So", "zeu\u00b7gen", "die", "ver\u00b7schie\u00b7de\u00b7ne", "Ge\u00b7spinns\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Verschiedner C\u00f6rperlein die unterschiedne D\u00fcnste.", "tokens": ["Ver\u00b7schied\u00b7ner", "C\u00f6r\u00b7per\u00b7lein", "die", "un\u00b7ter\u00b7schied\u00b7ne", "D\u00fcns\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}