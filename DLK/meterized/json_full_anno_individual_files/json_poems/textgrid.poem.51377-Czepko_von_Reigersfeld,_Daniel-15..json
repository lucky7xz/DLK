{"textgrid.poem.51377": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "15.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie artig ist mein Schalck. Sie nahm ihr goldnes Haar,", "tokens": ["Wie", "ar\u00b7tig", "ist", "mein", "Schalck", ".", "Sie", "nahm", "ihr", "gold\u00b7nes", "Haar", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPOSAT", "NN", "$.", "PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Druckt es an Mund, und warff dadurch der Augen Paar:", "tokens": ["Druckt", "es", "an", "Mund", ",", "und", "warff", "da\u00b7durch", "der", "Au\u00b7gen", "Paar", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "$,", "KON", "VVFIN", "PAV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nicht Haar, vielmehr ein Netz: in dem ich mich verfitzt,", "tokens": ["Nicht", "Haar", ",", "viel\u00b7mehr", "ein", "Netz", ":", "in", "dem", "ich", "mich", "ver\u00b7fitzt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "$,", "ADV", "ART", "NN", "$.", "APPR", "PRELS", "PPER", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dadurch viel tausend Blick auf mich gefach geplitzt.", "tokens": ["Da\u00b7durch", "viel", "tau\u00b7send", "Blick", "auf", "mich", "ge\u00b7fach", "ge\u00b7plitzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "CARD", "NN", "APPR", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Nicht Netz, ein schlauer Raub: weil sie durch diesen Strahl", "tokens": ["Nicht", "Netz", ",", "ein", "schlau\u00b7er", "Raub", ":", "weil", "sie", "durch", "die\u00b7sen", "Strahl"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "NN", "$,", "ART", "ADJA", "NN", "$.", "KOUS", "PPER", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Mir Hertze, Seel und Muth und Sinn u. Leben stahl.", "tokens": ["Mir", "Hert\u00b7ze", ",", "Seel", "und", "Muth", "und", "Sinn", "u.", "Le\u00b7ben", "stahl", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "abbreviation", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "NN", "KON", "NN", "KON", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.7": {"text": "Niemand nahm es in acht. Nun es hat keine Noth,", "tokens": ["Nie\u00b7mand", "nahm", "es", "in", "acht", ".", "Nun", "es", "hat", "kei\u00b7ne", "Noth", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "APPR", "CARD", "$.", "ADV", "PPER", "VAFIN", "PIAT", "NN", "$,"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.8": {"text": "Der Diebstahl ist entdeckt. Wie? Sie ward dr\u00fcber roth.", "tokens": ["Der", "Dieb\u00b7stahl", "ist", "ent\u00b7deckt", ".", "Wie", "?", "Sie", "ward", "dr\u00fc\u00b7ber", "roth", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$.", "PWAV", "$.", "PPER", "VAFIN", "PAV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambi