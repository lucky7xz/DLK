{"textgrid.poem.55064": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Kophtisches Lied", "genre": "verse", "period": "N.A.", "pub_year": 1787, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lasset Gelehrte sich zanken und streiten,", "tokens": ["Las\u00b7set", "Ge\u00b7lehr\u00b7te", "sich", "zan\u00b7ken", "und", "strei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "PRF", "VVINF", "KON", "VVFIN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Streng und bed\u00e4chtig die Lehrer auch sein!", "tokens": ["Streng", "und", "be\u00b7d\u00e4ch\u00b7tig", "die", "Leh\u00b7rer", "auch", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJD", "ART", "NN", "ADV", "VAINF", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Alle die Weisesten aller der Zeiten", "tokens": ["Al\u00b7le", "die", "Wei\u00b7ses\u00b7ten", "al\u00b7ler", "der", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "ART", "NN", "PIAT", "ART", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "L\u00e4cheln und winken und stimmen mit ein:", "tokens": ["L\u00e4\u00b7cheln", "und", "win\u00b7ken", "und", "stim\u00b7men", "mit", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "KON", "VVFIN", "APPR", "ART", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.5": {"text": "\u00bbt\u00f6richt, auf Be\u00dfrung der Toren zu harren!", "tokens": ["\u00bb", "t\u00f6\u00b7richt", ",", "auf", "Be\u00df\u00b7rung", "der", "To\u00b7ren", "zu", "har\u00b7ren", "!"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "