b\u00e4ndigt,", "tokens": ["Auf", "!", "mein", "Trotz", ",", "sei", "un\u00b7ge\u00b7b\u00e4n\u00b7digt", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "$.", "PPOSAT", "NN", "$,", "VAFIN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schau die Felsen an, die steilen.", "tokens": ["Schau", "die", "Fel\u00b7sen", "an", ",", "die", "stei\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PTKVZ", "$,", "PRELS", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.23": {"line.1": {"text": "Wie sich ihre Steine th\u00fcrmen", "tokens": ["Wie", "sich", "ih\u00b7re", "Stei\u00b7ne", "th\u00fcr\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PRF", "PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und sich keinem Willen beugen,", "tokens": ["Und", "sich", "kei\u00b7nem", "Wil\u00b7len", "beu\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "PIAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Also aufrecht streb' mein Herze,", "tokens": ["Al\u00b7so", "auf\u00b7recht", "streb'", "mein", "Her\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PPOSAT", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df du Sturm, Blitz, Strom, nicht scheuest.", "tokens": ["Da\u00df", "du", "Sturm", ",", "Blitz", ",", "Strom", ",", "nicht", "scheu\u00b7est", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "$,", "NN", "$,", "NN", "$,", "PTKNEG", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.24": {"line.1": {"text": "Rei\u00dft euch nur, ihr wilden Strudel,", "tokens": ["Rei\u00dft", "euch", "nur", ",", "ihr", "wil\u00b7den", "Stru\u00b7del", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Steh entgegen, wild Gesteine,", "tokens": ["Steh", "ent\u00b7ge\u00b7gen", ",", "wild", "Ge\u00b7stei\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "ADJD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Werft euch auf mich, Eichenst\u00e4mme,", "tokens": ["Werft", "euch", "auf", "mich", ",", "Ei\u00b7chen\u00b7st\u00e4m\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPER", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Fallt hernieder, Donnerkeile!", "tokens": ["Fallt", "her\u00b7nie\u00b7der", ",", "Don\u00b7ner\u00b7kei\u00b7le", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.25": {"line.1": {"text": "Um so eh bin ich gesunder", "tokens": ["Um", "so", "eh", "bin", "ich", "ge\u00b7sun\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "ADV", "ADJD", "VAFIN", "PPER", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und mein Wille springt in's Freye,", "tokens": ["Und", "mein", "Wil\u00b7le", "springt", "in's", "Frey\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn ihr mich Bergunter w\u00e4lzet,", "tokens": ["Wenn", "ihr", "mich", "Ber\u00b7gun\u00b7ter", "w\u00e4l\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In die tiefen Kl\u00fcfte schleudert.", "tokens": ["In", "die", "tie\u00b7fen", "Kl\u00fcf\u00b7te", "schleu\u00b7dert", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.26": {"line.1": {"text": "Tiefer liegt der Wald schon unten", "tokens": ["Tie\u00b7fer", "liegt", "der", "Wald", "schon", "un\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN", "ADV", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und die Finsterni\u00df entfleuget,", "tokens": ["Und", "die", "Fins\u00b7ter\u00b7ni\u00df", "ent\u00b7fleu\u00b7get", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Auf die Felsen tret' ich herrschend,", "tokens": ["Auf", "die", "Fel\u00b7sen", "tret'", "ich", "herr\u00b7schend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mancher Ast entgegen beuget.", "tokens": ["Man\u00b7cher", "Ast", "ent\u00b7ge\u00b7gen", "beu\u00b7get", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PTKVZ", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.27": {"line.1": {"text": "In der H\u00f6hle Arm gefangen,", "tokens": ["In", "der", "H\u00f6h\u00b7le", "Arm", "ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bin ich dennoch ohne Scheue,", "tokens": ["Bin", "ich", "den\u00b7noch", "oh\u00b7ne", "Scheu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Mein Bem\u00fchn war nicht vergeblich", "tokens": ["Mein", "Be\u00b7m\u00fchn", "war", "nicht", "ver\u00b7geb\u00b7lich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "PTKNEG", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und ich f\u00fchle keine Reue.", "tokens": ["Und", "ich", "f\u00fch\u00b7le", "kei\u00b7ne", "Reu\u00b7e", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.28": {"line.1": {"text": "Tief und tiefer will ich klimmen,", "tokens": ["Tief", "und", "tie\u00b7fer", "will", "ich", "klim\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und der Oede widerstreiten,", "tokens": ["Und", "der", "O\u00b7e\u00b7de", "wi\u00b7der\u00b7strei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Will kein Klang mir weiter folgen,", "tokens": ["Will", "kein", "Klang", "mir", "wei\u00b7ter", "fol\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIAT", "NN", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Muthigt mich doch mein Geschreye.", "tokens": ["Mut\u00b7higt", "mich", "doch", "mein", "Ge\u00b7schre\u00b7ye", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.29": {"line.1": {"text": "Zu den Waffen! zu den Waffen!", "tokens": ["Zu", "den", "Waf\u00b7fen", "!", "zu", "den", "Waf\u00b7fen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$.", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In mir tobt ein wilder Leue,", "tokens": ["In", "mir", "tobt", "ein", "wil\u00b7der", "Leu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und dem Stahl des Schwerdtes zucken", "tokens": ["Und", "dem", "Stahl", "des", "Schwerd\u00b7tes", "zu\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Funken aus dem Stein, die leuchten.", "tokens": ["Fun\u00b7ken", "aus", "dem", "Stein", ",", "die", "leuch\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,", "PRELS", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.30": {"line.1": {"text": "Und es springen mir die W\u00e4nde,", "tokens": ["Und", "es", "sprin\u00b7gen", "mir", "die", "W\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und ich sehe schon die Bl\u00e4ue.", "tokens": ["Und", "ich", "se\u00b7he", "schon", "die", "Bl\u00e4u\u00b7e", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Meinem Ringen flieht das Dunkel,", "tokens": ["Mei\u00b7nem", "Rin\u00b7gen", "flieht", "das", "Dun\u00b7kel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Oben gl\u00e4nzt die Sternen-Reihe.", "tokens": ["O\u00b7ben", "gl\u00e4nzt", "die", "Ster\u00b7nen\u00b7Rei\u00b7he", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.31": {"line.1": {"text": "Nieder knie' ich nun und danke,", "tokens": ["Nie\u00b7der", "knie'", "ich", "nun", "und", "dan\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Goldne Strahlen ziehn erfreuend", "tokens": ["Gold\u00b7ne", "Strah\u00b7len", "ziehn", "er\u00b7freu\u00b7end"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVINF", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Liebesnetze um den K\u00e4mpfer,", "tokens": ["Lie\u00b7bes\u00b7net\u00b7ze", "um", "den", "K\u00e4mp\u00b7fer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der die Waffen weit weg streute.", "tokens": ["Der", "die", "Waf\u00b7fen", "weit", "weg", "streu\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADJD", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.32": {"line.1": {"text": "Endlich, ruft er triumphirend,", "tokens": ["End\u00b7lich", ",", "ruft", "er", "tri\u00b7um\u00b7phi\u00b7rend", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "VVFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist mein Herz und Leben euer,", "tokens": ["Ist", "mein", "Herz", "und", "Le\u00b7ben", "eu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "KON", "NN", "PPOSAT", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Alle Kl\u00fcfte liegen unten:", "tokens": ["Al\u00b7le", "Kl\u00fcf\u00b7te", "lie\u00b7gen", "un\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nun verzehr' mich Liebesfeuer!", "tokens": ["Nun", "ver\u00b7zehr'", "mich", "Lie\u00b7bes\u00b7feu\u00b7er", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}