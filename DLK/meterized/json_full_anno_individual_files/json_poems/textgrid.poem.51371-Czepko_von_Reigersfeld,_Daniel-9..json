{"textgrid.poem.51371": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "9.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du Schmeltz Glas schwartz als Pech, das du umkettelt hast", "tokens": ["Du", "Sch\u00b7meltz", "Glas", "schwartz", "als", "Pech", ",", "das", "du", "um\u00b7ket\u00b7telt", "hast"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "NN", "NN", "ADJD", "KOKOM", "NE", "$,", "PRELS", "PPER", "VVPP", "VAFIN"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Die Lilgen weisse Hand, und zehnmal umgefast:", "tokens": ["Die", "Lil\u00b7gen", "weis\u00b7se", "Hand", ",", "und", "zehn\u00b7mal", "um\u00b7ge\u00b7fast", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,", "KON", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich z\u00fcrne doch mit mir, ob meiner Farbe du,", "tokens": ["Ich", "z\u00fcr\u00b7ne", "doch", "mit", "mir", ",", "ob", "mei\u00b7ner", "Far\u00b7be", "du", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPER", "$,", "KOUS", "PPOSAT", "NN", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die mein Betr\u00fcbn\u00fcs zeigt, gleicht triffst am nechsten zu:", "tokens": ["Die", "mein", "Be\u00b7tr\u00fcb\u00b7n\u00fcs", "zeigt", ",", "gleicht", "triffst", "am", "nechs\u00b7ten", "zu", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "$,", "VVFIN", "VVFIN", "APPRART", "ADJA", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Nicht mache dich so gro\u00df: Du bist und bleibest Glas:", "tokens": ["Nicht", "ma\u00b7che", "dich", "so", "gro\u00df", ":", "Du", "bist", "und", "blei\u00b7best", "Glas", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "PPER", "ADV", "ADJD", "$.", "PPER", "VAFIN", "KON", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die Reiffen sind zu schlecht umb ein so edles Fa\u00df.", "tokens": ["Die", "Reif\u00b7fen", "sind", "zu", "schlecht", "umb", "ein", "so", "ed\u00b7les", "Fa\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKA", "ADJD", "APPR", "ART", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ach G\u00f6ttin! Diese Schnur erkie\u00df ich nicht umbsonst,", "tokens": ["Ach", "G\u00f6t\u00b7tin", "!", "Die\u00b7se", "Schnur", "er\u00b7kie\u00df", "ich", "nicht", "um\u00b7bsonst", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$.", "PDAT", "NN", "VVFIN", "PPER", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die Farb ist meine Pein, das Glas ist deine Gunst.", "tokens": ["Die", "Farb", "ist", "mei\u00b7ne", "Pein", ",", "das", "Glas", "ist", "dei\u00b7ne", "Gunst", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "$,", "ART", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Du Schmeltz Gla