{"textgrid.poem.58048": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "86. Schalksauge und Einfaltsauge", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn du in Eigenheit suchst immer heimlich dich,", "tokens": ["Wenn", "du", "in", "Ei\u00b7gen\u00b7heit", "suchst", "im\u00b7mer", "heim\u00b7lich", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "VVFIN", "ADV", "ADJD", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wirst du im Finstern stets in Angst und Unruh schweben;", "tokens": ["Wirst", "du", "im", "Fins\u00b7tern", "stets", "in", "Angst", "und", "Un\u00b7ruh", "schwe\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPRART", "NN", "ADV", "APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch wenn dein Einfaltsaug' Gott meinet lauterlich,", "tokens": ["Doch", "wenn", "dein", "Ein\u00b7faltsaug'", "Gott", "mei\u00b7net", "lau\u00b7ter\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPOSAT", "NN", "NN", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "So kannst du frei im Geist und still im Lichte leben.", "tokens": ["So", "kannst", "du", "frei", "im", "Geist", "und", "still", "im", "Lich\u00b7te", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "APPRART", "NN", "KON", "ADJD", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-