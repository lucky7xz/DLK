{"dta.poem.4156": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Fastnachtslied  \n von den goldenen Z\u00f6pfen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1821", "urn": "urn:nbn:de:kobv:b4-200905194209", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "M\u00e4gdlein mit dem goldnen Haar!", "tokens": ["M\u00e4gd\u00b7lein", "mit", "dem", "gold\u00b7nen", "Haar", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Oder ist es wohl von Seide,", "tokens": ["O\u00b7der", "ist", "es", "wohl", "von", "Sei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "APPR", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Oder ist's von beiden gar?", "tokens": ["O\u00b7der", "ist's", "von", "bei\u00b7den", "gar", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "APPR", "PIAT", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Nenn' ich's goldgediegne Seide?", "tokens": ["Nenn'", "ich's", "gold\u00b7ge\u00b7dieg\u00b7ne", "Sei\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Nenn' ich's seidenfeines Gold?", "tokens": ["Nenn'", "ich's", "sei\u00b7den\u00b7fei\u00b7nes", "Gold", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Und welch zartes Elfenh\u00e4ndchen", "tokens": ["Und", "welch", "zar\u00b7tes", "El\u00b7fen\u00b7h\u00e4nd\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PWAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Hat die Flechten dir gerollt?", "tokens": ["Hat", "die", "Flech\u00b7ten", "dir", "ge\u00b7rollt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "M\u00e4gdlein mit den goldnen Z\u00f6pfen! \u2014", "tokens": ["M\u00e4gd\u00b7lein", "mit", "den", "gold\u00b7nen", "Z\u00f6p\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und an jedem h\u00e4ngt ein Herz,", "tokens": ["Und", "an", "je\u00b7dem", "h\u00e4ngt", "ein", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIS", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hier ein junges, da ein altes,", "tokens": ["Hier", "ein", "jun\u00b7ges", ",", "da", "ein", "al\u00b7tes", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "$,", "KOUS", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hier mit Lust, und da mit Schmerz.", "tokens": ["Hier", "mit", "Lust", ",", "und", "da", "mit", "Schmerz", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$,", "KON", "ADV", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und das meine, ach das meine! \u2014", "tokens": ["Und", "das", "mei\u00b7ne", ",", "ach", "das", "mei\u00b7ne", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PDS", "VVFIN", "$,", "XY", "PDS", "VVFIN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ist kein einzig Z\u00f6pfchen leer?", "tokens": ["Ist", "kein", "ein\u00b7zig", "Z\u00f6pf\u00b7chen", "leer", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "ADJD", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "M\u00e4gdlein mit den goldnen Z\u00f6pfen,", "tokens": ["M\u00e4gd\u00b7lein", "mit", "den", "gold\u00b7nen", "Z\u00f6p\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Dichterherzen sind nicht schwer.", "tokens": ["Dich\u00b7ter\u00b7her\u00b7zen", "sind", "nicht", "schwer", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PTKNEG", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und die goldnen Z\u00f6pfe fliegen", "tokens": ["Und", "die", "gold\u00b7nen", "Z\u00f6p\u00b7fe", "flie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Um den Nacken, um den Leib,", "tokens": ["Um", "den", "Na\u00b7cken", ",", "um", "den", "Leib", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "$,", "KOUI", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und das Fliegen und das Schmiegen", "tokens": ["Und", "das", "Flie\u00b7gen", "und", "das", "Schmie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "KON", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist der Herzen Zeitvertreib.", "tokens": ["Ist", "der", "Her\u00b7zen", "Zeit\u00b7ver\u00b7treib", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Einer hat sich fast verirret", "tokens": ["Ei\u00b7ner", "hat", "sich", "fast", "ver\u00b7ir\u00b7ret"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VAFIN", "PRF", "ADV", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Um die Schulter ganz allein:", "tokens": ["Um", "die", "Schul\u00b7ter", "ganz", "al\u00b7lein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "ADV", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "M\u00e4gdlein, streich' ihn nicht zur\u00fccke,", "tokens": ["M\u00e4gd\u00b7lein", ",", "streich'", "ihn", "nicht", "zu\u00b7r\u00fc\u00b7cke", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "PTKNEG", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Freiheit steht dem Haar so fein.", "tokens": ["Frei\u00b7heit", "steht", "dem", "Haar", "so", "fein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}