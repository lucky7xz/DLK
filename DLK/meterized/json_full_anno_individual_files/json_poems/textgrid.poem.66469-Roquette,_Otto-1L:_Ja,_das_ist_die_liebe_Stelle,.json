{"textgrid.poem.66469": {"metadata": {"author": {"name": "Roquette, Otto", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ja, das ist die liebe Stelle,", "genre": "verse", "period": "N.A.", "pub_year": 1860, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ja, das ist die liebe Stelle,", "tokens": ["Ja", ",", "das", "ist", "die", "lie\u00b7be", "Stel\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PDS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo ich sie zuerst gesehn!", "tokens": ["Wo", "ich", "sie", "zu\u00b7erst", "ge\u00b7sehn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie so leis erklingt die Welle,", "tokens": ["Wie", "so", "leis", "er\u00b7klingt", "die", "Wel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie so sanft die L\u00fcfte wehn!", "tokens": ["Wie", "so", "sanft", "die", "L\u00fcf\u00b7te", "wehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADJD", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Jene Felsen, jene B\u00fcsche,", "tokens": ["Je\u00b7ne", "Fel\u00b7sen", ",", "je\u00b7ne", "B\u00fc\u00b7sche", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jener Schatten Einsamkeit,", "tokens": ["Je\u00b7ner", "Schat\u00b7ten", "Ein\u00b7sam\u00b7keit", ","],