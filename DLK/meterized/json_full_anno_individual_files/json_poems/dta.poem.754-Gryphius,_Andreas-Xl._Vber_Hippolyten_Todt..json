{"dta.poem.754": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "Xl.  Vber  Hippolyten  Todt.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Dein Adel/ deine trew/ vnd vnverletzt gewissen ", "tokens": ["Dein", "A\u00b7del", "/", "dei\u00b7ne", "trew", "/", "vnd", "vn\u00b7ver\u00b7letzt", "ge\u00b7wis\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$(", "PPOSAT", "ADJD", "$(", "KON", "ADJD", "VAPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vnd Tugendt vnd verstandt/ die Blume deiner ", "tokens": ["Vnd", "Tu\u00b7gendt", "vnd", "ver\u00b7standt", "/", "die", "Blu\u00b7me", "dei\u00b7ner"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "NN", "KON", "VVPP", "$(", "ART", "NN", "PPOSAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "O Blume deiner zeit ruht auff der schwartzen bahr", "tokens": ["O", "Blu\u00b7me", "dei\u00b7ner", "zeit", "ruht", "auff", "der", "schwart\u00b7zen", "bahr"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "PPOSAT", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Kan dieser enge ", "tokens": ["Kan", "die\u00b7ser", "en\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["VMFIN", "PDAT", "ADJA"], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Ach nein der wehrte Geist ward von vns weg-gerissen/", "tokens": ["Ach", "nein", "der", "wehr\u00b7te", "Geist", "ward", "von", "vns", "weg\u00b7ge\u00b7ris\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKANT", "ART", "ADJA", "NN", "VAFIN", "APPR", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Zu dem/ der seine lust/ in wollust vnd gefahr.", "tokens": ["Zu", "dem", "/", "der", "sei\u00b7ne", "lust", "/", "in", "wol\u00b7lust", "vnd", "ge\u00b7fahr", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$(", "ART", "PPOSAT", "NN", "$(", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Feber schnelle macht/ der rawen Seuchen schar. ", "tokens": ["Der", "Fe\u00b7ber", "schnel\u00b7le", "macht", "/", "der", "ra\u00b7wen", "Seu\u00b7chen", "schar", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVFIN", "$(", "ART", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Hat dir den engen weg/ zum ", "tokens": ["Hat", "dir", "den", "en\u00b7gen", "weg", "/", "zum"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "PTKVZ", "$(", "APPRART"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Du scheidest von der Welt; die welt war dein nicht wehrt!", "tokens": ["Du", "schei\u00b7dest", "von", "der", "Welt", ";", "die", "welt", "war", "dein", "nicht", "wehrt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$.", "ART", "NN", "VAFIN", "PPOSAT", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sehr jung/ die Ewigkeit ist dir nunmehr beschert;", "tokens": ["Sehr", "jung", "/", "die", "E\u00b7wig\u00b7keit", "ist", "dir", "nun\u00b7mehr", "be\u00b7schert", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$(", "ART", "NN", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Von vns! wir werden dich ", "tokens": ["Von", "vns", "!", "wir", "wer\u00b7den", "dich"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPER", "$.", "PPER", "VAFIN", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Von mir: ich folge nach/ geschwinde: doch bereit", "tokens": ["Von", "mir", ":", "ich", "fol\u00b7ge", "nach", "/", "ge\u00b7schwin\u00b7de", ":", "doch", "be\u00b7reit"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["APPR", "PPER", "$.", "PPER", "VVFIN", "APPR", "$(", "ADJA", "$.", "ADV", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Durch schmertzen: die nunmehr verkehrt in Fr\u00f6ligkeit/", "tokens": ["Durch", "schmert\u00b7zen", ":", "die", "nun\u00b7mehr", "ver\u00b7kehrt", "in", "Fr\u00f6\u00b7lig\u00b7keit", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVINF", "$.", "ART", "ADV", "VVPP", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Hier gibst du gutte nacht/ vmb oben mich zu gr\u00fcssen.", "tokens": ["Hier", "gibst", "du", "gut\u00b7te", "nacht", "/", "vmb", "o\u00b7ben", "mich", "zu", "gr\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJA", "NN", "$(", "APPR", "ADV", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}