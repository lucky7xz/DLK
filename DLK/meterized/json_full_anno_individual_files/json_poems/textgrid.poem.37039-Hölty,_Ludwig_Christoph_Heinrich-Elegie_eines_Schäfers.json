 "pos": ["KOUS", "PPER", "APPRART", "NN", "VVFIN", "$,", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Von Abendwinden um uns rauschte.", "tokens": ["Von", "Ab\u00b7end\u00b7win\u00b7den", "um", "uns", "rauschte", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "Dort ist der Rasensitz, wo ich an ihrer Hand", "tokens": ["Dort", "ist", "der", "Ra\u00b7sen\u00b7sitz", ",", "wo", "ich", "an", "ih\u00b7rer", "Hand"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "$,", "PWAV", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mich dem Gef\u00fchl der Freude weyhte,", "tokens": ["Mich", "dem", "Ge\u00b7f\u00fchl", "der", "Freu\u00b7de", "weyh\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn sich der Mond erhob, und durch des Hayns Gewand", "tokens": ["Wenn", "sich", "der", "Mond", "er\u00b7hob", ",", "und", "durch", "des", "Hayns", "Ge\u00b7wand"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "ART", "NN", "VVFIN", "$,", "KON", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Hellblitzend Silber streute.", "tokens": ["Hell\u00b7blit\u00b7zend", "Sil\u00b7ber", "streu\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Durch jene Blumen, die mit Regenbogenglanz", "t