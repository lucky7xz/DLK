{"textgrid.poem.37039": {"metadata": {"author": {"name": "H\u00f6lty, Ludwig Christoph Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Elegie eines Sch\u00e4fers", "genre": "verse", "period": "N.A.", "pub_year": 1769, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr Linden, die ihr meiner H\u00fctte K\u00fchlung gebt,", "tokens": ["Ihr", "Lin\u00b7den", ",", "die", "ihr", "mei\u00b7ner", "H\u00fct\u00b7te", "K\u00fch\u00b7lung", "gebt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PPER", "PPOSAT", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Rauscht Klagen, und ihr milden Weste,", "tokens": ["Rauscht", "Kla\u00b7gen", ",", "und", "ihr", "mil\u00b7den", "Wes\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "KON", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die ihr von Zweig zu Zweig auf leichten Fl\u00fcgeln schwebt,", "tokens": ["Die", "ihr", "von", "Zweig", "zu", "Zweig", "auf", "leich\u00b7ten", "Fl\u00fc\u00b7geln", "schwebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "APPR", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Schl\u00fcpft traurig durch das Laub der \u00c4ste.", "tokens": ["Schl\u00fcpft", "trau\u00b7rig", "durch", "das", "Laub", "der", "\u00c4s\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Verbreite weit umher, o Nachhall, meine Quaal!", "tokens": ["Ver\u00b7brei\u00b7te", "weit", "um\u00b7her", ",", "o", "Nach\u00b7hall", ",", "mei\u00b7ne", "Qua\u00b7al", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADJD", "PTKVZ", "$,", "FM", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Rollt eure krausen Silberwellen", "tokens": ["Rollt", "eu\u00b7re", "krau\u00b7sen", "Sil\u00b7ber\u00b7wel\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Mit hohlen Murmeln durch das bunte Veilchenthal,", "tokens": ["Mit", "hoh\u00b7len", "Mur\u00b7meln", "durch", "das", "bun\u00b7te", "Veil\u00b7chen\u00b7thal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ihr Wiesenb\u00e4che, und ihr Quellen. \u2013", "tokens": ["Ihr", "Wie\u00b7sen\u00b7b\u00e4\u00b7che", ",", "und", "ihr", "Quel\u00b7len", ".", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "$,", "KON", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Melinde ist nicht mehr! Die Sch\u00f6ne liegt erbla\u00dft,", "tokens": ["Me\u00b7lin\u00b7de", "ist", "nicht", "mehr", "!", "Die", "Sch\u00f6\u00b7ne", "liegt", "er\u00b7bla\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PTKNEG", "ADV", "$.", "ART", "NN", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Rings um sie herrschet Todesstille.", "tokens": ["Rings", "um", "sie", "herr\u00b7schet", "To\u00b7des\u00b7stil\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ein finstres Grab, dort wo die Linde winkt, umfa\u00dft", "tokens": ["Ein", "finst\u00b7res", "Grab", ",", "dort", "wo", "die", "Lin\u00b7de", "winkt", ",", "um\u00b7fa\u00dft"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ADV", "PWAV", "ART", "NE", "VVFIN", "$,", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der sch\u00f6nsten Seele sch\u00f6nste H\u00fclle.", "tokens": ["Der", "sch\u00f6ns\u00b7ten", "See\u00b7le", "sch\u00f6ns\u00b7te", "H\u00fcl\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Mit ihr ist meine Ruh, und die Zufriedenheit,", "tokens": ["Mit", "ihr", "ist", "mei\u00b7ne", "Ruh", ",", "und", "die", "Zu\u00b7frie\u00b7den\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "PPOSAT", "NN", "$,", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die mir die guten G\u00f6tter gaben,", "tokens": ["Die", "mir", "die", "gu\u00b7ten", "G\u00f6t\u00b7ter", "ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und die mein Leben mir mit Rosen \u00fcberstreut,", "tokens": ["Und", "die", "mein", "Le\u00b7ben", "mir", "mit", "Ro\u00b7sen", "\u00fc\u00b7bers\u00b7treut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PPOSAT", "NN", "PPER", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Tief in die Nacht der Gruft begraben.", "tokens": ["Tief", "in", "die", "Nacht", "der", "Gruft", "be\u00b7gra\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Nun kleidet sich der Lenz f\u00fcr mich in Trauerflor,", "tokens": ["Nun", "klei\u00b7det", "sich", "der", "Lenz", "f\u00fcr", "mich", "in", "Trau\u00b7er\u00b7flor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "APPR", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den ich so oft mit ihr belauschte,", "tokens": ["Den", "ich", "so", "oft", "mit", "ihr", "be\u00b7lauschte", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADV", "ADV", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn er im Tulpenkranz erschien, inde\u00df ein Chor", "tokens": ["Wenn", "er", "im", "Tul\u00b7pen\u00b7kranz", "er\u00b7schien", ",", "in\u00b7de\u00df", "ein", "Chor"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVFIN", "$,", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Von Abendwinden um uns rauschte.", "tokens": ["Von", "Ab\u00b7end\u00b7win\u00b7den", "um", "uns", "rauschte", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Dort ist der Rasensitz, wo ich an ihrer Hand", "tokens": ["Dort", "ist", "der", "Ra\u00b7sen\u00b7sitz", ",", "wo", "ich", "an", "ih\u00b7rer", "Hand"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "$,", "PWAV", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mich dem Gef\u00fchl der Freude weyhte,", "tokens": ["Mich", "dem", "Ge\u00b7f\u00fchl", "der", "Freu\u00b7de", "weyh\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn sich der Mond erhob, und durch des Hayns Gewand", "tokens": ["Wenn", "sich", "der", "Mond", "er\u00b7hob", ",", "und", "durch", "des", "Hayns", "Ge\u00b7wand"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "ART", "NN", "VVFIN", "$,", "KON", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Hellblitzend Silber streute.", "tokens": ["Hell\u00b7blit\u00b7zend", "Sil\u00b7ber", "streu\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Durch jene Blumen, die mit Regenbogenglanz", "tokens": ["Durch", "je\u00b7ne", "Blu\u00b7men", ",", "die", "mit", "Re\u00b7gen\u00b7bo\u00b7gen\u00b7glanz"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "$,", "PRELS", "APPR", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Stolzieren, schl\u00fcpften ihre F\u00fc\u00dfe", "tokens": ["Stol\u00b7zie\u00b7ren", ",", "schl\u00fcpf\u00b7ten", "ih\u00b7re", "F\u00fc\u00b7\u00dfe"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So oft, dort wand sie mir den sch\u00f6nsten Blumenkranz", "tokens": ["So", "oft", ",", "dort", "wand", "sie", "mir", "den", "sch\u00f6ns\u00b7ten", "Blu\u00b7men\u00b7kranz"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "$,", "ADV", "VVFIN", "PPER", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Um meine Stirn, und gab mir K\u00fc\u00dfe.", "tokens": ["Um", "mei\u00b7ne", "Stirn", ",", "und", "gab", "mir", "K\u00fc\u00b7\u00dfe", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "$,", "KON", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Sie waren doch so s\u00fc\u00df! Noch s\u00fc\u00dfer als der Duft,", "tokens": ["Sie", "wa\u00b7ren", "doch", "so", "s\u00fc\u00df", "!", "Noch", "s\u00fc\u00b7\u00dfer", "als", "der", "Duft", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ADJD", "$.", "ADV", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der aus der Rosenknospe quillet.", "tokens": ["Der", "aus", "der", "Ro\u00b7sen\u00b7knos\u00b7pe", "quil\u00b7let", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und allen diesen Reiz verschlie\u00dfet jene Gruft,", "tokens": ["Und", "al\u00b7len", "die\u00b7sen", "Reiz", "ver\u00b7schlie\u00b7\u00dfet", "je\u00b7ne", "Gruft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "PDAT", "NN", "VVFIN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die itzt der May in Blumen h\u00fcllet!", "tokens": ["Die", "itzt", "der", "May", "in", "Blu\u00b7men", "h\u00fcl\u00b7let", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Ihr Linden, die ihr meiner H\u00fctte K\u00fchlung gebt,", "tokens": ["Ihr", "Lin\u00b7den", ",", "die", "ihr", "mei\u00b7ner", "H\u00fct\u00b7te", "K\u00fch\u00b7lung", "gebt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PPER", "PPOSAT", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Rauscht Klagen, und ihr milden Weste,", "tokens": ["Rauscht", "Kla\u00b7gen", ",", "und", "ihr", "mil\u00b7den", "Wes\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "KON", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die ihr von Zweig zu Zweig auf leichten Fl\u00fcgeln schwebt,", "tokens": ["Die", "ihr", "von", "Zweig", "zu", "Zweig", "auf", "leich\u00b7ten", "Fl\u00fc\u00b7geln", "schwebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "APPR", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Schl\u00fcpft traurig durch das Laub d