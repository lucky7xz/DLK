{"dta.poem.16160": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "214.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Meister hat gesagt: Es st\u00e4nden unsre Sachen", "tokens": ["Der", "Meis\u00b7ter", "hat", "ge\u00b7sagt", ":", "Es", "st\u00e4n\u00b7den", "uns\u00b7re", "Sa\u00b7chen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$.", "PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Viel besser, k\u00f6nnte man nur alles zweimal machen.", "tokens": ["Viel", "bes\u00b7ser", ",", "k\u00f6nn\u00b7te", "man", "nur", "al\u00b7les", "zwei\u00b7mal", "ma\u00b7chen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "VMFIN", "PIS", "ADV", "PIS", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Im Kleinen magst du das am Einzelnen probieren,", "tokens": ["Im", "Klei\u00b7nen", "magst", "du", "das", "am", "Ein\u00b7zel\u00b7nen", "pro\u00b7bie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VMFIN", "PPER", "PDS", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Im Gro\u00dfen geht es nicht, du wirst die Zeit verlieren.", "tokens": ["Im", "Gro\u00b7\u00dfen", "geht", "es", "nicht", ",", "du", "wirst", "die", "Zeit", "ver\u00b7lie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "PTKNEG", "$,", "PPER", "VAFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Was hilft im Einzelnen des Zweimalmachens Qual?", "tokens": ["Was", "hilft", "im", "Ein\u00b7zel\u00b7nen", "des", "Zwei\u00b7mal\u00b7ma\u00b7chens", "Qual", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPRART", "NN", "ART", "NN", "NN", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Das ganze Leben doch man lebt es nur einmal.", "tokens": ["Das", "gan\u00b7ze", "Le\u00b7ben", "doch", "man", "lebt", "es", "nur", "ein\u00b7mal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "PIS", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}