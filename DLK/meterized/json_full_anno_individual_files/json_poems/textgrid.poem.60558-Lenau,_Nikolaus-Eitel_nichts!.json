{"textgrid.poem.60558": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Eitel nichts!", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "'s ist eitel nichts, wohin mein Aug ich hefte!", "tokens": ["'s", "ist", "ei\u00b7tel", "nichts", ",", "wo\u00b7hin", "mein", "Aug", "ich", "hef\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "PIS", "$,", "PWAV", "PPOSAT", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Das Leben ist ein vielbesagtes Wandern,", "tokens": ["Das", "Le\u00b7ben", "ist", "ein", "viel\u00b7be\u00b7sag\u00b7tes", "Wan\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein w\u00fcstes Jagen ists von dem zum andern,", "tokens": ["Ein", "w\u00fcs\u00b7tes", "Ja\u00b7gen", "ists", "von", "dem", "zum", "an\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "APPR", "ART", "APPRART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und unterwegs verlieren wir die Kr\u00e4fte.", "tokens": ["Und", "un\u00b7ter\u00b7wegs", "ver\u00b7lie\u00b7ren", "wir", "die", "Kr\u00e4f\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ja, k\u00f6nnte man zum letzten Erdenziele", "tokens": ["Ja", ",", "k\u00f6nn\u00b7te", "man", "zum", "letz\u00b7ten", "Er\u00b7den\u00b7zie\u00b7le"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "VMFIN", "PIS", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Noch als derselbe frische Bursche kommen,", "tokens": ["Noch", "als", "der\u00b7sel\u00b7be", "fri\u00b7sche", "Bur\u00b7sche", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PDAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Wie man den ersten Anlauf hat genommen,", "tokens": ["Wie", "man", "den", "ers\u00b7ten", "An\u00b7lauf", "hat", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ART", "ADJA", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "So m\u00f6chte man noch lachen zu dem Spiele.", "tokens": ["So", "m\u00f6ch\u00b7te", "man", "noch", "la\u00b7chen", "zu", "dem", "Spie\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "ADV", "VVINF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Doch tr\u00e4gt uns eine Macht von Stund zu Stund,", "tokens": ["Doch", "tr\u00e4gt", "uns", "ei\u00b7ne", "Macht", "von", "Stund", "zu", "Stund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "APPR", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Wie's Kr\u00fcglein, das am Brunnenstein zersprang,", "tokens": ["Wie's", "Kr\u00fcg\u00b7lein", ",", "das", "am", "Brun\u00b7nen\u00b7stein", "zer\u00b7sprang", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Und dessen Inhalt sickert auf den Grund,", "tokens": ["Und", "des\u00b7sen", "In\u00b7halt", "si\u00b7ckert", "auf", "den", "Grund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRELAT", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "So weit es ging, den ganzen Weg entlang.", "tokens": ["So", "weit", "es", "ging", ",", "den", "gan\u00b7zen", "Weg", "ent\u00b7lang", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "VVFIN", "$,", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Nun ist es leer; wer mag daraus noch trinken?", "tokens": ["Nun", "ist", "es", "leer", ";", "wer", "mag", "da\u00b7raus", "noch", "trin\u00b7ken", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "$.", "PWS", "VMFIN", "PAV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Und zu den andern Scherben mu\u00df es sinken.", "tokens": ["Und", "zu", "den", "an\u00b7dern", "Scher\u00b7ben", "mu\u00df", "es", "sin\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "'s ist eitel nichts, wohin mein Aug ich hefte!", "tokens": ["'s", "ist", "ei\u00b7tel", "nichts", ",", "wo\u00b7hin", "mein", "Aug", "ich", "hef\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD",