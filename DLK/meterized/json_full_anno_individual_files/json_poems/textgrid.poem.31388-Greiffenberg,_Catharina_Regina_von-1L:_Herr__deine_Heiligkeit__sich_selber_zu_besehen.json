{"textgrid.poem.31388": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Herr / deine Heiligkeit / sich selber zu besehen", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herr / deine Heiligkeit / sich selber zu besehen", "tokens": ["Herr", "/", "dei\u00b7ne", "Hei\u00b7lig\u00b7keit", "/", "sich", "sel\u00b7ber", "zu", "be\u00b7se\u00b7hen"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$(", "PPOSAT", "NN", "$(", "PRF", "ADV", "PTKZU", "VVINF"], "meter": "++-+-+-+-+-+-", "measure": "unknown.measure.septa"}, "line.2": {"text": "hatt' eine Gottes-Lust. die Allheit fund' in ihr", "tokens": ["hatt'", "ei\u00b7ne", "Got\u00b7tes\u00b7Lust", ".", "die", "All\u00b7heit", "fund'", "in", "ihr"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "$.", "ART", "NN", "VVFIN", "APPR", "PPOSAT"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "ein reichs Erg\u00f6tzungs-Feld / betrachtend ihre Zier.", "tokens": ["ein", "reichs", "Er\u00b7g\u00f6t\u00b7zungs\u00b7Feld", "/", "be\u00b7trach\u00b7tend", "ih\u00b7re", "Zier", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sie konte / S\u00fc\u00dfheit satt / auf Wollust-Weiden gehen.", "tokens": ["Sie", "kon\u00b7te", "/", "S\u00fc\u00df\u00b7heit", "satt", "/", "auf", "Wol\u00b7lust\u00b7Wei\u00b7den", "ge\u00b7hen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$(", "NN", "ADJD", "$(", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Verstand / hatt tiefen Sinn' / sich selber zu verstehen.", "tokens": ["Ver\u00b7stand", "/", "hatt", "tie\u00b7fen", "Sinn'", "/", "sich", "sel\u00b7ber", "zu", "ver\u00b7ste\u00b7hen", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "VAFIN", "ADJA", "NN", "$(", "PRF", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ihr' Allvergn\u00fcgung fand' in dir auch / die Begier.", "tokens": ["Ih\u00b7r'", "All\u00b7ver\u00b7gn\u00fc\u00b7gung", "fand'", "in", "dir", "auch", "/", "die", "Be\u00b7gier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "PPER", "ADV", "$(", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.7": {"text": "der Wille hatt gesch\u00f6pfft sein wollen nur aus dir.", "tokens": ["der", "Wil\u00b7le", "hatt", "ge\u00b7sch\u00f6pfft", "sein", "wol\u00b7len", "nur", "aus", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "VAINF", "VMFIN", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "da\u00df du uns schuffst / geschah allein uns zuerh\u00f6hen.", "tokens": ["da\u00df", "du", "uns", "schuffst", "/", "ge\u00b7schah", "al\u00b7lein", "uns", "zu\u00b7er\u00b7h\u00f6\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "$(", "VVFIN", "ADV", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ach Abgrund-guter Gott! Ach wesentliche Gnad /", "tokens": ["Ach", "Ab\u00b7grun