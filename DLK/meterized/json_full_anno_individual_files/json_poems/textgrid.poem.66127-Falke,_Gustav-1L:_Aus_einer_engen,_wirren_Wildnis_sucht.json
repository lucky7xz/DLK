{"textgrid.poem.66127": {"metadata": {"author": {"name": "Falke, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "1L: Aus einer engen, wirren Wildnis sucht", "genre": "verse", "period": "N.A.", "pub_year": 1884, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aus einer engen, wirren Wildnis sucht", "tokens": ["Aus", "ei\u00b7ner", "en\u00b7gen", ",", "wir\u00b7ren", "Wild\u00b7nis", "sucht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ich einen Ausweg, doch am Ende stand", "tokens": ["Ich", "ei\u00b7nen", "Aus\u00b7weg", ",", "doch", "am", "En\u00b7de", "stand"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "ART", "NN", "$,", "ADV", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mit einer wehrenden Geberde: Ich.", "tokens": ["Mit", "ei\u00b7ner", "weh\u00b7ren\u00b7den", "Ge\u00b7ber\u00b7de", ":", "Ich", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$.", "PPER", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Nicht heftig, aber zwingend. Also ruhig", "tokens": ["Nicht", "hef\u00b7tig", ",", "a\u00b7ber", "zwin\u00b7gend", ".", "Al\u00b7so", "ru\u00b7hig"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PTKNEG", "ADJD", "$,", "ADV", "ADJD", "$.", "ADV", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und forschend richtete ich meinen Blick", "tokens": ["Und", "for\u00b7schend", "rich\u00b7te\u00b7te", "ich", "mei\u00b7nen", "Blick"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Auf mich, da\u00df ich betroffen r\u00fcckw\u00e4rts trat", "tokens": ["Auf", "mich", ",", "da\u00df", "ich", "be\u00b7trof\u00b7fen", "r\u00fcck\u00b7w\u00e4rts", "trat"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "$,", "KOUS", "PPER", "VVFIN", "ADV", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und langsam und von Graun gepackt entwich.", "tokens": ["Und", "lang\u00b7sam", "und", "von", "Graun", "ge\u00b7packt", "ent\u00b7wich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KON", "APPR", "NN", "VVPP", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ich suchte einen andern Weg und fand", "tokens": ["Ich", "such\u00b7te", "ei\u00b7nen", "an\u00b7dern", "Weg", "und", "fand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "KON", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Am Ausgang mich und ging zur\u00fcck, und immer", "tokens": ["Am", "Aus\u00b7gang", "mich", "und", "ging", "zu\u00b7r\u00fcck", ",", "und", "im\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPRART", "NN", "PPER", "KON", "VVFIN", "PTKVZ", "$,", "KON", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Fand ich am Ende eines jeden Wegs", "tokens": ["Fand", "ich", "am", "En\u00b7de", "ei\u00b7nes", "je\u00b7den", "Wegs"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "ART", "PIAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Denselben W\u00e4chter, da\u00df ich schaudernd floh.", "tokens": ["Den\u00b7sel\u00b7ben", "W\u00e4ch\u00b7ter", ",", "da\u00df", "ich", "schau\u00b7dernd", "floh", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "KOUS", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Stolz, Ehrgeiz, L\u00fcge, Wollust, Ha\u00df und Neid,", "tokens": ["Stolz", ",", "Ehr\u00b7geiz", ",", "L\u00fc\u00b7ge", ",", "Wol\u00b7lust", ",", "Ha\u00df", "und", "Neid", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Jedwede Leidenschaft trug meine Z\u00fcge", "tokens": ["Jed\u00b7we\u00b7de", "Lei\u00b7den\u00b7schaft", "trug", "mei\u00b7ne", "Z\u00fc\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "+--+-+++-+-", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Und schreckte mich. Auch Gram und Liebesleid,", "tokens": ["Und", "schreck\u00b7te", "mich", ".", "Auch", "Gram", "und", "Lie\u00b7be\u00b7sleid", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$.", "ADV", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Auch blasse Reue mit verqu\u00e4lten Seufzern,", "tokens": ["Auch", "blas\u00b7se", "Reu\u00b7e", "mit", "ver\u00b7qu\u00e4l\u00b7ten", "Seuf\u00b7zern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und Wahnsinn mit erloschnen bl\u00f6den Augen.", "tokens": ["Und", "Wahn\u00b7sinn", "mit", "er\u00b7lo\u00b7schnen", "bl\u00f6\u00b7den", "Au\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "So viel der Tore, so viel H\u00fcter grinsten", "tokens": ["So", "viel", "der", "To\u00b7re", ",", "so", "viel", "H\u00fc\u00b7ter", "grins\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "NN", "$,", "ADV", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Mit einem lautlosen Zur\u00fcck mich an.", "tokens": ["Mit", "ei\u00b7nem", "laut\u00b7lo\u00b7sen", "Zu\u00b7r\u00fcck", "mich", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-++--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Dann aber fand ich einen scheuen Jungen,", "tokens": ["Dann", "a\u00b7ber", "fand", "ich", "ei\u00b7nen", "scheu\u00b7en", "Jun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dem deckte Scham die weichen Wangen, als er", "tokens": ["Dem", "deck\u00b7te", "Scham", "die", "wei\u00b7chen", "Wan\u00b7gen", ",", "als", "er"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN", "$,", "KOUS", "PPER"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mich kommen sah, und seine Miene sprach", "tokens": ["Mich", "kom\u00b7men", "sah", ",", "und", "sei\u00b7ne", "Mie\u00b7ne", "sprach"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVINF", "VVFIN", "$,", "KON", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Fast demutvoll: Verzeih, da\u00df ich hier stehe.", "tokens": ["Fast", "de\u00b7mut\u00b7voll", ":", "Ver\u00b7zeih", ",", "da\u00df", "ich", "hier", "ste\u00b7he", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$.", "NN", "$,", "KOUS", "PPER", "ADV", "VVFIN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.5": {"line.1": {"text": "Da fa\u00dfte Wut mich: Fratze, bist du ich?", "tokens": ["Da", "fa\u00df\u00b7te", "Wut", "mich", ":", "Frat\u00b7ze", ",", "bist", "du", "ich", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "PPER", "$.", "NN", "$,", "VAFIN", "PPER", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Feigling, Erb\u00e4rmling, gib mir Raum. Und j\u00e4h", "tokens": ["Feig\u00b7ling", ",", "Er\u00b7b\u00e4rm\u00b7ling", ",", "gib", "mir", "Raum", ".", "Und", "j\u00e4h"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "VVIMP", "PPER", "NN", "$.", "KON", "ADJD"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Scho\u00df tieferer Purpur \u00fcber seine Schl\u00e4fen,", "tokens": ["Scho\u00df", "tie\u00b7fe\u00b7rer", "Pur\u00b7pur", "\u00fc\u00b7ber", "sei\u00b7ne", "Schl\u00e4\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "++--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Und seine Augen hob er meinem Schimpf", "tokens": ["Und", "sei\u00b7ne", "Au\u00b7gen", "hob", "er", "mei\u00b7nem", "Schimpf"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Verwirrt entgegen. Und ich hob die Faust.", "tokens": ["Ver\u00b7wirrt", "ent\u00b7ge\u00b7gen", ".", "Und", "ich", "hob", "die", "Faust", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$.", "KON", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Da fiel er totenbla\u00df mir in den Arm,", "tokens": ["Da", "fiel", "er", "to\u00b7ten\u00b7bla\u00df", "mir", "in", "den", "Arm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und seine Augen riefen, schrien: Schlag nicht!", "tokens": ["Und", "sei\u00b7ne", "Au\u00b7gen", "rie\u00b7fen", ",", "schri\u00b7en", ":", "Schlag", "nicht", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "$,", "VVFIN", "$.", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Ich aber, zornig, rang mit ihm, und rang", "tokens": ["Ich", "a\u00b7ber", ",", "zor\u00b7nig", ",", "rang", "mit", "ihm", ",", "und", "rang"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "ADV", "$,", "ADJD", "$,", "VVFIN", "APPR", "PPER", "$,", "KON", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Drei Tage und drei N\u00e4chte lang, und warf", "tokens": ["Drei", "Ta\u00b7ge", "und", "drei", "N\u00e4ch\u00b7te", "lang", ",", "und", "warf"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["CARD", "NN", "KON", "CARD", "NN", "ADJD", "$,", "KON", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Ihn nicht, und lie\u00df ersch\u00f6pft von ihm, und wich", "tokens": ["Ihn", "nicht", ",", "und", "lie\u00df", "er\u00b7sch\u00f6pft", "von", "ihm", ",", "und", "wich"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "PTKNEG", "$,", "KON", "VVFIN", "VVPP", "APPR", "PPER", "$,", "KON", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Schrittweis und Blick in Blick. Der seine war", "tokens": ["Schritt\u00b7weis", "und", "Blick", "in", "Blick", ".", "Der", "sei\u00b7ne", "war"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "KON", "NN", "APPR", "NN", "$.", "ART", "PPOSAT", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Voll stillen Vorwurfs und verst\u00f6rter Scham.", "tokens": ["Voll", "stil\u00b7len", "Vor\u00b7wurfs", "und", "ver\u00b7st\u00f6r\u00b7ter", "Scham", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Und ich verkroch mich unter einen Busch,", "tokens": ["Und", "ich", "ver\u00b7kroch", "mich", "un\u00b7ter", "ei\u00b7nen", "Busch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und meine W\u00e4chter folgten mir und sp\u00e4hten", "tokens": ["Und", "mei\u00b7ne", "W\u00e4ch\u00b7ter", "folg\u00b7ten", "mir", "und", "sp\u00e4h\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PPER", "KON", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Aufdringlich durch das schwarze Laub", "tokens": ["Auf\u00b7dring\u00b7lich", "durch", "das", "schwar\u00b7ze", "Laub"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und qu\u00e4lten mich.", "tokens": ["Und", "qu\u00e4l\u00b7ten", "mich", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "Aus einer engen, wirren Wildnis sucht", "tokens": ["Aus", "ei\u00b7ner", "en\u00b7gen", ",", "wir\u00b7ren", "Wild\u00b7nis", "sucht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ich einen Ausweg, doch am Ende stand", "tokens": ["Ich", "ei\u00b7nen", "Aus\u00b7weg", ",", "doch", "am", "En\u00b7de", "stand"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "ART", "NN", "$,", "ADV", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mit einer wehrenden Geberde: Ich.", "tokens": ["Mit", "ei\u00b7ner", "weh\u00b7ren\u00b7den", "Ge\u00b7ber\u00b7de", ":", "Ich", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$.", "PPER", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Nicht heftig, aber zwingend. Also ruhig", "tokens": ["Nicht", "hef\u00b7tig", ",", "a\u00b7ber", "zwin\u00b7gend", ".", "Al\u00b7so", "ru\u00b7hig"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PTKNEG", "ADJD", "$,", "ADV", "ADJD", "$.", "ADV", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und forschend richtete ich meinen Blick", "tokens": ["Un