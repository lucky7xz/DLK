r": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.14": {"text": "Und in des Aufflugs Durst sang Er den", "tokens": ["Und", "in", "des", "Auf\u00b7flugs", "Durst", "sang", "Er", "den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "NN", "VVFIN", "PPER", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Seinen ", "tokens": ["Sei\u00b7nen"], "token_info": ["word"], "pos": ["PPOSAT"], "meter": "+-", "measure": "trochaic.single"}, "line.16": {"text": "Fern von der Erde Staub, kreisend in t\u00f6nendem Flug. \u2013", "tokens": ["Fern", "von", "der", "Er\u00b7de", "Staub", ",", "krei\u00b7send", "in", "t\u00f6\u00b7nen\u00b7dem", "Flug", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$,", "ADJD", "APPR", "ADJA", "NN", "$.", "$("], "meter": "+--+-++--+--+", "measure": "iambic.hexa.invert"}}, "stanza.9": {"line.1": {"text": "Ach, es ist uns entschwunden der Herrliche! Flammen der Inbrunst", "tokens": ["Ach", ",", "es", "ist", "uns", "ent\u00b7schwun\u00b7den", "der", "Herr\u00b7li\u00b7che", "!", "Flam\u00b7men", "der", "In\u00b7brunst"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "$,", "PPER", "VAFIN", "PPER", "VVFIN", "ART", "NN", "$.", "NN", "ART", "NN"], "meter": "+-+--+--+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Hatten das irrdische Band, \u00f6ffnend den Kerker, verzehrt.", "tokens": ["Hat\u00b7ten", "das", "irr\u00b7di\u00b7sche", "Band", ",", "\u00f6ff\u00b7nend", "den", "Ker\u00b7ker", ",", "ver\u00b7zehrt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$,", "ADJD", "ART", "NN", "$,", "VVPP", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.3": {"text": "Bruder, Du starbst den Tod des Gerechten! \u2013 Dir, eh' erlosch das", "tokens": ["Bru\u00b7der", ",", "Du", "starbst", "den", "Tod", "des", "Ge\u00b7rech\u00b7ten", "!", "\u2013", "Dir", ",", "eh'", "er\u00b7losch", "das"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PPER", "VVFIN", "ART", "NN", "ART", "NN", "$.", "$(", "PPER", "$,", "KOUS", "ADJD", "ART"], "meter": "+--+-+--+--+-+-", "measure": "iambic.hexa.invert"}, "line.4": {"text": "Aug', aus der H\u00f6h' erschien tagend des Aufganges Glanz;", "tokens": ["Aug'", ",", "aus", "der", "H\u00f6h'", "er\u00b7schien", "ta\u00b7gend", "des", "Auf\u00b7gan\u00b7ges", "Glanz", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "ART", "NN", "VVFIN", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.5": {"text": "Dir gesandt vom Allbarmherzigen, da\u00df zu des Lenzes", "tokens": ["Dir", "ge\u00b7sandt", "vom", "All\u00b7barm\u00b7her\u00b7zi\u00b7gen", ",", "da\u00df", "zu", "des", "Len\u00b7zes"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVPP", "APPRART", "NN", "$,", "KOUS", "APPR", "ART", "NN"], "meter": "+-+-+-+--++-+-", "measure": "trochaic.septa.relaxed"}, "line.6": {"text": "Morgen Dir w\u00fcrde die Nacht jenes umschatteten Thals.", "tokens": ["Mor\u00b7gen", "Dir", "w\u00fcr\u00b7de", "die", "Nacht", "je\u00b7nes", "um\u00b7schat\u00b7te\u00b7ten", "Thals", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "VAFIN", "ART", "NN", "PDAT", "ADJA", "NN", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}}, "stanza.10": {"line.1": {"text": "Jammernde Klage verstumm'! \u2013 Ich liebt', o liebte wie viel mehr", "tokens": ["Jam\u00b7mern\u00b7de", "Kla\u00b7ge", "ver\u00b7stumm'", "!", "\u2013", "Ich", "liebt'", ",", "o", "lieb\u00b7te", "wie", "viel", "mehr"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "$.", "$(", "PPER", "VVFIN", "$,", "FM", "VVFIN", "KOKOM", "ADV", "ADV"], "meter": "+--+--+-+-+-+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Als mein eigenes Selbst \u2013 Bruder, Du wei\u00dft es ja! \u2013 Dich.", "tokens": ["Als", "mein", "ei\u00b7ge\u00b7nes", "Selbst", "\u2013", "Bru\u00b7der", ",", "Du", "wei\u00dft", "es", "ja", "!", "\u2013", "Dich", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "$(", "NN", "$,", "PPER", "VVFIN", "PPER", "ADV", "$.", "$(", "PPER", "$."], "meter": "--+---+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Dennoch th\u00e4ten sich auf vor meiner Stimme die Gr\u00e4ber,", "tokens": ["Den\u00b7noch", "th\u00e4\u00b7ten", "sich", "auf", "vor", "mei\u00b7ner", "Stim\u00b7me", "die", "Gr\u00e4\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "APPR", "APPR", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Wahrlich, ich hielte den Ruf, der Dich erflehte, zur\u00fcck!", "tokens": ["Wahr\u00b7lich", ",", "ich", "hiel\u00b7te", "den", "Ruf", ",", "der", "Dich", "er\u00b7fleh\u00b7te", ",", "zu\u00b7r\u00fcck", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "$,", "PPER", "VVFIN", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "PTKVZ", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Deines Himmels erfreu' ich mit reiner Liebe mich, Deines \u2013", "tokens": ["Dei\u00b7nes", "Him\u00b7mels", "er\u00b7freu'", "ich", "mit", "rei\u00b7ner", "Lie\u00b7be", "mich", ",", "Dei\u00b7nes", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "APPR", "ADJA", "NN", "PPER", "$,", "PPOSAT", "$("], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.6": {"text": "Heiliger Sehnsucht Lohn! \u2013 Sch\u00f6pfens und Sonnens, o dort,", "tokens": ["Hei\u00b7li\u00b7ger", "Sehn\u00b7sucht", "Lohn", "!", "\u2013", "Sch\u00f6p\u00b7fens", "und", "Son\u00b7nens", ",", "o", "dort", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$.", "$(", "NN", "KON", "NN", "$,", "FM", "ADV", "$,"], "meter": "+--+-++--+--+", "measure": "iambic.hexa.invert"}, "line.7": {"text": "Dort aus dem Urquell, dort an dem Urstrahl ewiger Liebe,", "tokens": ["Dort", "aus", "dem", "Ur\u00b7quell", ",", "dort", "an", "dem", "Ur\u00b7strahl", "e\u00b7wi\u00b7ger", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,", "ADV", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-++-+-+--+-", "measure": "iambic.septa.relaxed"}, "line.8": {"text": "Deren Abglanz schon hier Odem des Lebens Dir war! \u2013", "tokens": ["De\u00b7ren", "Ab\u00b7glanz", "schon", "hier", "O\u00b7dem", "des", "Le\u00b7bens", "Dir", "war", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "NN", "ADV", "ADV", "NN", "ART", "NN", "PPER", "VAFIN", "$.", "$("], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}}, "stanza.11": {"line.1": {"text": "Ach, Sein Odem! \u2013 Nun athmet aus bebender Lipp' er in s\u00fc\u00dfer", "tokens": ["Ach", ",", "Sein", "O\u00b7dem", "!", "\u2013", "Nun", "ath\u00b7met", "aus", "be\u00b7ben\u00b7der", "Lipp'", "er", "in", "s\u00fc\u00b7\u00dfer"], "token_info": ["word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PPOSAT", "NN", "$.", "$(", "ADV", "VVFIN", "APPR", "ADJA", "NN", "PPER", "APPR", "ADJA"], "meter": "+-+--+-+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Stunde des Wiedersehns, mir an der Wange nicht mehr!", "tokens": ["Stun\u00b7de", "des", "Wie\u00b7der\u00b7sehns", ",", "mir", "an", "der", "Wan\u00b7ge", "nicht", "mehr", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "PPER", "APPR", "ART", "NN", "PTKNEG", "ADV", "$."], "meter": "+--+-+-+-+--+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Einsam ist's und \u00f6de dem Schmachtenden! \u2013 H\u00fcter, o sag' ob", "tokens": ["Ein\u00b7sam", "ist's", "und", "\u00f6\u00b7de", "dem", "Schmach\u00b7ten\u00b7den", "!", "\u2013", "H\u00fc\u00b7ter", ",", "o", "sag'", "ob"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "KON", "ADJD", "ART", "NN", "$.", "$(", "NN", "$,", "FM", "FM", "KOUS"], "meter": "+-+-+--+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Schier sich neige die Nacht, d\u00e4mm're die Scheitel des Bergs? \u2013", "tokens": ["Schier", "sich", "nei\u00b7ge", "die", "Nacht", ",", "d\u00e4m\u00b7m'\u00b7re", "die", "Schei\u00b7tel", "des", "Bergs", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "PRF", "VVFIN", "ART", "NN", "$,", "VVFIN", "ART", "NN", "ART", "NN", "$.", "$("], "meter": "+-+--+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "Herr, Dein Wille gescheh'! Es leuchte mir fr\u00fcher, mir sp\u00e4ter,", "tokens": ["Herr", ",", "Dein", "Wil\u00b7le", "ge\u00b7scheh'", "!", "Es", "leuch\u00b7te", "mir", "fr\u00fc\u00b7her", ",", "mir", "sp\u00e4\u00b7ter", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "VVPP", "$.", "PPER", "VVFIN", "PPER", "ADJD", "$,", "PPER", "ADJD", "$,"], "meter": "+-+--+-+--++-+-", "measure": "trochaic.septa.relaxed"}, "line.6": {"text": "Jenes Wiedersehns, Deiner Erbarmungen, Tag!", "tokens": ["Je\u00b7nes", "Wie\u00b7der\u00b7sehns", ",", "Dei\u00b7ner", "Er\u00b7bar\u00b7mun\u00b7gen", ",", "Tag", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PDAT", "NN", "$,", "PPOSAT", "NN", "$,", "NN", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.7": {"text": "Wenn es genannt darf werden ein Wiedersehn, wenn in Seiner", "tokens": ["Wenn", "es", "ge\u00b7nannt", "darf", "wer\u00b7den", "ein", "Wie\u00b7der\u00b7sehn", ",", "wenn", "in", "Sei\u00b7ner"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVPP", "VMFIN", "VAFIN", "ART", "NN", "$,", "KOUS", "APPR", "PPOSAT"], "meter": "-+-+-+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Herrlichkeit dort ich Ihn staunend erblick' und entz\u00fcckt.", "tokens": ["Herr\u00b7lich\u00b7keit", "dort", "ich", "Ihn", "stau\u00b7nend", "er\u00b7blick'", "und", "ent\u00b7z\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPER", "PPER", "ADJD", "VVFIN", "KON", "VVPP", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}}, "stanza.12": {"line.1": {"text": "Mildr', o mildere, Bruder, den Strahl, wenn im Tode mein Auge", "tokens": ["Mildr'", ",", "o", "mil\u00b7de\u00b7re", ",", "Bru\u00b7der", ",", "den", "Strahl", ",", "wenn", "im", "To\u00b7de", "mein", "Au\u00b7ge"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "FM", "FM", "$,", "NN", "$,", "ART", "NN", "$,", "KOUS", "APPRART", "NN", "PPOSAT", "NN"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Bricht, wenn der Liebe Drang Dich zu dem Sterbenden zieht,", "tokens": ["Bricht", ",", "wenn", "der", "Lie\u00b7be", "Drang", "Dich", "zu", "dem", "Ster\u00b7ben\u00b7den", "zieht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "ART", "NN", "NN", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+--+-+-+-+--+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Dann, o Bote des Himmels, den Strahl! da\u00df wiedererkenne", "tokens": ["Dann", ",", "o", "Bo\u00b7te", "des", "Him\u00b7mels", ",", "den", "Strahl", "!", "da\u00df", "wie\u00b7der\u00b7er\u00b7ken\u00b7ne"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADV", "$,", "FM", "NN", "ART", "NN", "$,", "ART", "NN", "$.", "KOUS", "NN"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Dich im Erscheinenden, Dich segne mein Erstlings-Gef\u00fchl!", "tokens": ["Dich", "im", "Er\u00b7schei\u00b7nen\u00b7den", ",", "Dich", "seg\u00b7ne", "mein", "Erst\u00b7lings\u00b7Ge\u00b7f\u00fchl", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+--+---+--+--+", "measure": "dactylic.di.plus"}}}}}