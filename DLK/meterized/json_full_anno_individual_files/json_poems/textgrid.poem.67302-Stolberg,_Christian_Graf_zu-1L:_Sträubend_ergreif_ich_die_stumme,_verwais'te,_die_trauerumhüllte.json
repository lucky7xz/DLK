{"textgrid.poem.67302": {"metadata": {"author": {"name": "Stolberg, Christian Graf zu", "birth": "N.A.", "death": "N.A."}, "title": "1L: Str\u00e4ubend ergreif ich die stumme, verwais'te, die trauerumh\u00fcllte", "genre": "verse", "period": "N.A.", "pub_year": 1784, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Str\u00e4ubend ergreif ich die stumme, verwais'te, die trauerumh\u00fcllte", "tokens": ["Str\u00e4u\u00b7bend", "er\u00b7greif", "ich", "die", "stum\u00b7me", ",", "ver\u00b7wais'\u00b7te", ",", "die", "trau\u00b7er\u00b7um\u00b7h\u00fcll\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "ART", "ADJA", "$,", "VVFIN", "$,", "PRELS", "VVFIN"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Zwillings-Leyer, die ach, ihre Genossinn verlor!", "tokens": ["Zwil\u00b7lings\u00b7Ley\u00b7er", ",", "die", "ach", ",", "ih\u00b7re", "Ge\u00b7nos\u00b7sinn", "ver\u00b7lor", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "XY", "$,", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+---+--+--+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Ihre hohe Schwester, der nun sich die Irrdische schmachtend", "tokens": ["Ih\u00b7re", "ho\u00b7he", "Schwes\u00b7ter", ",", "der", "nun", "sich", "die", "Irr\u00b7di\u00b7sche", "schmach\u00b7tend"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PRELS", "ADV", "PRF", "ART", "NN", "ADJD"], "meter": "+-+-+---+-+--+-", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Nachsehnt, lauschend umsonst ihrem vertrauten Get\u00f6n.", "tokens": ["Nach\u00b7sehnt", ",", "lau\u00b7schend", "um\u00b7sonst", "ih\u00b7rem", "ver\u00b7trau\u00b7ten", "Ge\u00b7t\u00f6n", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "ADJD", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "J\u00fcngst noch klangen die Saiten in zauberndem Silbergelispel,", "tokens": ["J\u00fcngst", "noch", "klan\u00b7gen", "die", "Sai\u00b7ten", "in", "zau\u00b7bern\u00b7dem", "Sil\u00b7ber\u00b7ge\u00b7lis\u00b7pel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.2": {"text": "Wallend ums\u00e4uselten rings heilige Schauer ihr Spiel;", "tokens": ["Wal\u00b7lend", "um\u00b7s\u00e4u\u00b7sel\u00b7ten", "rings", "hei\u00b7li\u00b7ge", "Schau\u00b7er", "ihr", "Spiel", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Stunde der Weihe, du warst es! der himmlischen Weihe, da scholl der", "tokens": ["Stun\u00b7de", "der", "Wei\u00b7he", ",", "du", "warst", "es", "!", "der", "himm\u00b7li\u00b7schen", "Wei\u00b7he", ",", "da", "scholl", "der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ART", "NN", "$,", "PPER", "VAFIN", "PPER", "$.", "ART", "ADJA", "NN", "$,", "KOUS", "ADJD", "ART"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Feier-Hymnus im Schwung, den die Begeistrung gebot,", "tokens": ["Fei\u00b7er\u00b7Hym\u00b7nus", "im", "Schwung", ",", "den", "die", "Be\u00b7geis\u00b7trung", "ge\u00b7bot", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "Sie nicht die H\u00e4usliche! T\u00f6chter aus Eden hatten geg\u00fcrtet", "tokens": ["Sie", "nicht", "die", "H\u00e4us\u00b7li\u00b7che", "!", "T\u00f6ch\u00b7ter", "aus", "E\u00b7den", "hat\u00b7ten", "ge\u00b7g\u00fcr\u00b7tet"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "PTKNEG", "ART", "NN", "$.", "NN", "APPR", "NN", "VAFIN", "VVPP"], "meter": "-+-+--+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Sich mit dem Bogen des Bunds, strahlend die Heitre durchschwebt;", "tokens": ["Sich", "mit", "dem", "Bo\u00b7gen", "des", "Bunds", ",", "strah\u00b7lend", "die", "Heit\u00b7re", "durch\u00b7schwebt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "ART", "NN", "$,", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.7": {"text": "Ihnen, aus fliegenden Locken, von schimmernden Fittigen, trof der", "tokens": ["Ih\u00b7nen", ",", "aus", "flie\u00b7gen\u00b7den", "Lo\u00b7cken", ",", "von", "schim\u00b7mern\u00b7den", "Fit\u00b7ti\u00b7gen", ",", "trof", "der"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "$,", "APPR", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "$,", "VVFIN", "ART"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.8": {"text": "Heimath balsamischer Thau, hauchte der Lauben Ged\u00fcft.", "tokens": ["Hei\u00b7math", "bal\u00b7sa\u00b7mi\u00b7scher", "Thau", ",", "hauch\u00b7te", "der", "Lau\u00b7ben", "Ge\u00b7d\u00fcft", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+---+--+", "measure": "iambic.hexa.chol"}, "line.9": {"text": "Kundig wallte der Seher entgegen der hohen Entz\u00fcckung,", "tokens": ["Kun\u00b7dig", "wall\u00b7te", "der", "Se\u00b7her", "ent\u00b7ge\u00b7gen", "der", "ho\u00b7hen", "Ent\u00b7z\u00fc\u00b7ckung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.10": {"text": "Und zu der Sph\u00e4ren Tanz schwang der Gesang sich empor.", "tokens": ["Und", "zu", "der", "Sph\u00e4\u00b7ren", "Tanz", "schwang", "der", "Ge\u00b7sang", "sich", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "VVFIN", "ART", "NN", "PRF", "PTKVZ", "$."], "meter": "-+-+-+---+--+", "measure": "iambic.penta.chol"}, "line.11": {"text": "Sehnsucht athmeten ein Ihm der Himmlischen weihende Ch\u00f6re,", "tokens": ["Sehn\u00b7sucht", "ath\u00b7me\u00b7ten", "ein", "Ihm", "der", "Himm\u00b7li\u00b7schen", "wei\u00b7hen\u00b7de", "Ch\u00f6\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "PPER", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+--++-+--+--+-", "measure": "trochaic.septa.relaxed"}, "line.12": {"text": "Sehnsucht entflammte die Brust, str\u00f6mte die Saiten hinab.", "tokens": ["Sehn\u00b7sucht", "ent\u00b7flamm\u00b7te", "die", "Brust", ",", "str\u00f6m\u00b7te", "die", "Sai\u00b7ten", "hin\u00b7ab", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.13": {"text": "Anklang des Sions-Halleluja fl\u00fcsterte fernher,", "tokens": ["An\u00b7klang", "des", "Si\u00b7ons\u00b7Hal\u00b7le\u00b7lu\u00b7ja", "fl\u00fcs\u00b7ter\u00b7te", "fern\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NE", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.14": {"text": "Und in des Aufflugs Durst sang Er den", "tokens": ["Und", "in", "des", "Auf\u00b7flugs", "Durst", "sang", "Er", "den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "NN", "VVFIN", "PPER", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Seinen ", "tokens": ["Sei\u00b7nen"], "token_info": ["word"], "pos": ["PPOSAT"], "meter": "+-", "measure": "trochaic.single"}, "line.16": {"text": "Fern von der Erde Staub, kreisend in t\u00f6nendem Flug. \u2013", "tokens": ["Fern", "von", "der", "Er\u00b7de", "Staub", ",", "krei\u00b7send", "in", "t\u00f6\u00b7nen\u00b7dem", "Flug", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$,", "ADJD", "APPR", "ADJA", "NN", "$.", "$("], "meter": "+--+-++--+--+", "measure": "iambic.hexa.invert"}}, "stanza.3": {"line.1": {"text": "Ach, es ist uns entschwunden der Herrliche! Flammen der Inbrunst", "tokens": ["Ach", ",", "es", "ist", "uns", "ent\u00b7schwun\u00b7den", "der", "Herr\u00b7li\u00b7che", "!", "Flam\u00b7men", "der", "In\u00b7brunst"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "$,", "PPER", "VAFIN", "PPER", "VVFIN", "ART", "NN", "$.", "NN", "ART", "NN"], "meter": "+-+--+--+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Hatten das irrdische Band, \u00f6ffnend den Kerker, verzehrt.", "tokens": ["Hat\u00b7ten", "das", "irr\u00b7di\u00b7sche", "Band", ",", "\u00f6ff\u00b7nend", "den", "Ker\u00b7ker", ",", "ver\u00b7zehrt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$,", "ADJD", "ART", "NN", "$,", "VVPP", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.3": {"text": "Bruder, Du starbst den Tod des Gerechten! \u2013 Dir, eh' erlosch das", "tokens": ["Bru\u00b7der", ",", "Du", "starbst", "den", "Tod", "des", "Ge\u00b7rech\u00b7ten", "!", "\u2013", "Dir", ",", "eh'", "er\u00b7losch", "das"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PPER", "VVFIN", "ART", "NN", "ART", "NN", "$.", "$(", "PPER", "$,", "KOUS", "ADJD", "ART"], "meter": "+--+-+--+--+-+-", "measure": "iambic.hexa.invert"}, "line.4": {"text": "Aug', aus der H\u00f6h' erschien tagend des Aufganges Glanz;", "tokens": ["Aug'", ",", "aus", "der", "H\u00f6h'", "er\u00b7schien", "ta\u00b7gend", "des", "Auf\u00b7gan\u00b7ges", "Glanz", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "ART", "NN", "VVFIN", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.5": {"text": "Dir gesandt vom Allbarmherzigen, da\u00df zu des Lenzes", "tokens": ["Dir", "ge\u00b7sandt", "vom", "All\u00b7barm\u00b7her\u00b7zi\u00b7gen", ",", "da\u00df", "zu", "des", "Len\u00b7zes"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVPP", "APPRART", "NN", "$,", "KOUS", "APPR", "ART", "NN"], "meter": "+-+-+-+--++-+-", "measure": "trochaic.septa.relaxed"}, "line.6": {"text": "Morgen Dir w\u00fcrde die Nacht jenes umschatteten Thals.", "tokens": ["Mor\u00b7gen", "Dir", "w\u00fcr\u00b7de", "die", "Nacht", "je\u00b7nes", "um\u00b7schat\u00b7te\u00b7ten", "Thals", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "VAFIN", "ART", "NN", "PDAT", "ADJA", "NN", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "Jammernde Klage verstumm'! \u2013 Ich liebt', o liebte wie viel mehr", "tokens": ["Jam\u00b7mern\u00b7de", "Kla\u00b7ge", "ver\u00b7stumm'", "!", "\u2013", "Ich", "liebt'", ",", "o", "lieb\u00b7te", "wie", "viel", "mehr"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "$.", "$(", "PPER", "VVFIN", "$,", "FM", "VVFIN", "KOKOM", "ADV", "ADV"], "meter": "+--+--+-+-+-+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Als mein eigenes Selbst \u2013 Bruder, Du wei\u00dft es ja! \u2013 Dich.", "tokens": ["Als", "mein", "ei\u00b7ge\u00b7nes", "Selbst", "\u2013", "Bru\u00b7der", ",", "Du", "wei\u00dft", "es", "ja", "!", "\u2013", "Dich", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "$(", "NN", "$,", "PPER", "VVFIN", "PPER", "ADV", "$.", "$(", "PPER", "$."], "meter": "--+---+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Dennoch th\u00e4ten sich auf vor meiner Stimme die Gr\u00e4ber,", "tokens": ["Den\u00b7noch", "th\u00e4\u00b7ten", "sich", "auf", "vor", "mei\u00b7ner", "Stim\u00b7me", "die", "Gr\u00e4\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "APPR", "APPR", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Wahrlich, ich hielte den Ruf, der Dich erflehte, zur\u00fcck!", "tokens": ["Wahr\u00b7lich", ",", "ich", "hiel\u00b7te", "den", "Ruf", ",", "der", "Dich", "er\u00b7fleh\u00b7te", ",", "zu\u00b7r\u00fcck", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "$,", "PPER", "VVFIN", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "PTKVZ", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Deines Himmels erfreu' ich mit reiner Liebe mich, Deines \u2013", "tokens": ["Dei\u00b7nes", "Him\u00b7mels", "er\u00b7freu'", "ich", "mit", "rei\u00b7ner", "Lie\u00b7be", "mich", ",", "Dei\u00b7nes", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "APPR", "ADJA", "NN", "PPER", "$,", "PPOSAT", "$("], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.6": {"text": "Heiliger Sehnsucht Lohn! \u2013 Sch\u00f6pfens und Sonnens, o dort,", "tokens": ["Hei\u00b7li\u00b7ger", "Sehn\u00b7sucht", "Lohn", "!", "\u2013", "Sch\u00f6p\u00b7fens", "und", "Son\u00b7nens", ",", "o", "dort", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$.", "$(", "NN", "KON", "NN", "$,", "FM", "ADV", "$,"], "meter": "+--+-++--+--+", "measure": "iambic.hexa.invert"}, "line.7": {"text": "Dort aus dem Urquell, dort an dem Urstrahl ewiger Liebe,", "tokens": ["Dort", "aus", "dem", "Ur\u00b7quell", ",", "dort", "an", "dem", "Ur\u00b7strahl", "e\u00b7wi\u00b7ger", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,", "ADV", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-++-+-+--+-", "measure": "iambic.septa.relaxed"}, "line.8": {"text": "Deren Abglanz schon hier Odem des Lebens Dir war! \u2013", "tokens": ["De\u00b7ren", "Ab\u00b7glanz", "schon", "hier", "O\u00b7dem", "des", "Le\u00b7bens", "Dir", "war", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "NN", "ADV", "ADV", "NN", "ART", "NN", "PPER", "VAFIN", "$.", "$("], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}}, "stanza.5": {"line.1": {"text": "Ach, Sein Odem! \u2013 Nun athmet aus bebender Lipp' er in s\u00fc\u00dfer", "tokens": ["Ach", ",", "Sein", "O\u00b7dem", "!", "\u2013", "Nun", "ath\u00b7met", "aus", "be\u00b7ben\u00b7der", "Lipp'", "er", "in", "s\u00fc\u00b7\u00dfer"], "token_info": ["word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PPOSAT", "NN", "$.", "$(", "ADV", "VVFIN", "APPR", "ADJA", "NN", "PPER", "APPR", "ADJA"], "meter": "+-+--+-+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Stunde des Wiedersehns, mir an der Wange nicht mehr!", "tokens": ["Stun\u00b7de", "des", "Wie\u00b7der\u00b7sehns", ",", "mir", "an", "der", "Wan\u00b7ge", "nicht", "mehr", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "PPER", "APPR", "ART", "NN", "PTKNEG", "ADV", "$."], "meter": "+--+-+-+-+--+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Einsam ist's und \u00f6de dem Schmachtenden! \u2013 H\u00fcter, o sag' ob", "tokens": ["Ein\u00b7sam", "ist's", "und", "\u00f6\u00b7de", "dem", "Schmach\u00b7ten\u00b7den", "!", "\u2013", "H\u00fc\u00b7ter", ",", "o", "sag'", "ob"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "KON", "ADJD", "ART", "NN", "$.", "$(", "NN", "$,", "FM", "FM", "KOUS"], "meter": "+-+-+--+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Schier sich neige die Nacht, d\u00e4mm're die Scheitel des Bergs? \u2013", "tokens": ["Schier", "sich", "nei\u00b7ge", "die", "Nacht", ",", "d\u00e4m\u00b7m'\u00b7re", "die", "Schei\u00b7tel", "des", "Bergs", "?", "\u2013"], "token_info": 