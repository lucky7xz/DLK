{"dta.poem.12086": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "8.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Am H\u00fcgel sa\u00df ich Nachts, und war dem Thal entronnen,", "tokens": ["Am", "H\u00fc\u00b7gel", "sa\u00df", "ich", "Nachts", ",", "und", "war", "dem", "Thal", "ent\u00b7ron\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ADV", "$,", "KON", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von dem mir aufwerts klang ged\u00e4mpfter Schall der Wonnen,", "tokens": ["Von", "dem", "mir", "auf\u00b7werts", "klang", "ge\u00b7d\u00e4mpf\u00b7ter", "Schall", "der", "Won\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADV", "VVFIN", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Der lauten Weltlichkeit, die mich von sich gescheucht,", "tokens": ["Der", "lau\u00b7ten", "Welt\u00b7lich\u00b7keit", ",", "die", "mich", "von", "sich", "ge\u00b7scheucht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PRF", "APPR", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und selig f\u00fchlt' ich mich im Dunkel warm und feucht.", "tokens": ["Und", "se\u00b7lig", "f\u00fchlt'", "ich", "mich", "im", "Dun\u00b7kel", "warm", "und", "feucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "PRF", "APPRART", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Doch \u00fcber eine Schlucht zur Seit' her\u00fcber drang", "tokens": ["Doch", "\u00fc\u00b7ber", "ei\u00b7ne", "Schlucht", "zur", "Seit'", "her\u00b7\u00fc\u00b7ber", "drang"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "APPRART", "NN", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dein Schlummerr\u00f6cheln, o Natur, und macht mir bang.", "tokens": ["Dein", "Schlum\u00b7mer\u00b7r\u00f6\u00b7cheln", ",", "o", "Na\u00b7tur", ",", "und", "macht", "mir", "bang", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "FM", "NN", "$,", "KON", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Ein fl\u00fcsterndes Get\u00f6n im Laub der alten R\u00fcstern,", "tokens": ["Ein", "fl\u00fcs\u00b7tern\u00b7des", "Ge\u00b7t\u00f6n", "im", "Laub", "der", "al\u00b7ten", "R\u00fcs\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein d\u00fcsterndes Gest\u00f6hn, Geschnaub aus welchen N\u00fcstern?", "tokens": ["Ein", "d\u00fcs\u00b7tern\u00b7des", "Ge\u00b7st\u00f6hn", ",", "Ge\u00b7schnaub", "aus", "wel\u00b7chen", "N\u00fcs\u00b7tern", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "NN", "APPR", "PWAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Und die unheimlichen Nachtgeister trieben wieder", "tokens": ["Und", "die", "un\u00b7heim\u00b7li\u00b7chen", "Nacht\u00b7geis\u00b7ter", "trie\u00b7ben", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "ADV"], "meter": "-+-+--++-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Mich zu der Welt Get\u00f6s, dem ich entflohn war, nieder.", "tokens": ["Mich", "zu", "der", "Welt", "Ge\u00b7t\u00f6s", ",", "dem", "ich", "ent\u00b7flohn", "war", ",", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "NN", "$,", "PRELS", "PPER", "VVPP", "VAFIN", "$,", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "O Herz, das zwischen Welt und der Natur du schwebst,", "tokens": ["O", "Herz", ",", "das", "zwi\u00b7schen", "Welt", "und", "der", "Na\u00b7tur", "du", "schwebst", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PRELS", "APPR", "NN", "KON", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der einen scheu entstrebst, und vor der andern bebst!", "tokens": ["Der", "ei\u00b7nen", "scheu", "ent\u00b7strebst", ",", "und", "vor", "der", "an\u00b7dern", "bebst", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJD", "VVFIN", "$,", "KON", "APPR", "ART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}