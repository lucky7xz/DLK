{"dta.poem.3152": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "In der Neujahrsnacht fuhr ich durch verschneite Flur,", "tokens": ["In", "der", "Neu\u00b7jahrs\u00b7nacht", "fuhr", "ich", "durch", "ver\u00b7schnei\u00b7te", "Flur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Vom Jahreswechsel war im Schnee da keine Spur.", "tokens": ["Vom", "Jah\u00b7res\u00b7wech\u00b7sel", "war", "im", "Schnee", "da", "kei\u00b7ne", "Spur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "APPRART", "NN", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die zw\u00f6lfte Stunde nur schlug meine Taschenuhr,", "tokens": ["Die", "zw\u00f6lf\u00b7te", "Stun\u00b7de", "nur", "schlug", "mei\u00b7ne", "Ta\u00b7sche\u00b7nuhr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+++-+-+", "measure": "unknown.measure.septa"}, "line.2": {"text": "Doch rings blieb theilnahmlos die schweigende Natur.", "tokens": ["Doch", "rings", "blieb", "theil\u00b7nahm\u00b7los", "die", "schwei\u00b7gen\u00b7de", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Die Zeitabschnitte sind vom Menschen nur erdacht,", "tokens": ["Die", "Zeit\u00b7ab\u00b7schnit\u00b7te", "sind", "vom", "Men\u00b7schen", "nur", "er\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPRART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ununterbrochen geht die Weltuhr Tag und Nacht.", "tokens": ["Un\u00b7un\u00b7ter\u00b7bro\u00b7chen", "geht", "die", "Wel\u00b7tuhr", "Tag", "und", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Jahrstunden rufet Lerch' und Schwalb' und Kukuk aus,", "tokens": ["Jahr\u00b7stun\u00b7den", "ru\u00b7fet", "Ler\u00b7ch'", "und", "Schwalb'", "und", "Ku\u00b7kuk", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NN", "KON", "NN", "KON", "NN", "PTKVZ", "$,"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und Perpendikelgang ist Sturm und Wogenbraus.", "tokens": ["Und", "Per\u00b7pen\u00b7di\u00b7kel\u00b7gang", "ist", "Sturm", "und", "Wo\u00b7gen\u00b7braus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Der Sommer macht dem Herbst, der Winter Platz dem Lenze,", "tokens": ["Der", "Som\u00b7mer", "macht", "dem", "Herbst", ",", "der", "Win\u00b7ter", "Platz", "dem", "Len\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,", "ART", "NN", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch nirgends abgesteckt ist sichtbar eine Grenze.", "tokens": ["Doch", "nir\u00b7gends", "ab\u00b7ge\u00b7steckt", "ist", "sicht\u00b7bar", "ei\u00b7ne", "Gren\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVPP", "VAFIN", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Der Zeiten Wagen rollt gleich \u00fcber Au'n und Haiden,", "tokens": ["Der", "Zei\u00b7ten", "Wa\u00b7gen", "rollt", "gleich", "\u00fc\u00b7ber", "Au'n", "und", "Hai\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ohn' Ansto\u00df \u00fcber Jahr' auch und Jahrhundertscheiden.", "tokens": ["Ohn'", "An\u00b7sto\u00df", "\u00fc\u00b7ber", "Jahr'", "auch", "und", "Jahr\u00b7hun\u00b7dert\u00b7schei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "ADV", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "So rollet mein Gesang mit mir die Welt entlang,", "tokens": ["So", "rol\u00b7let", "mein", "Ge\u00b7sang", "mit", "mir", "die", "Welt", "ent\u00b7lang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "APPR", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den Zeitenwechsel durch, mit immer gleichem Klang.", "tokens": ["Den", "Zei\u00b7ten\u00b7wech\u00b7sel", "durch", ",", "mit", "im\u00b7mer", "glei\u00b7chem", "Klang", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "APPR", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Von Lebensstation zu Station begleitend,", "tokens": ["Von", "Le\u00b7bens\u00b7sta\u00b7ti\u00b7on", "zu", "Sta\u00b7ti\u00b7on", "be\u00b7glei\u00b7tend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Himmelssonne gleich, durch alle Zeichen schreitend.", "tokens": ["Der", "Him\u00b7mels\u00b7son\u00b7ne", "gleich", ",", "durch", "al\u00b7le", "Zei\u00b7chen", "schrei\u00b7tend", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "APPR", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}