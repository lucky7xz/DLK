{"textgrid.poem.33676": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sitzt ein V\u00f6glein unter meinem Dach, es singt,", "tokens": ["Sitzt", "ein", "V\u00f6\u00b7glein", "un\u00b7ter", "mei\u00b7nem", "Dach", ",", "es", "singt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Wenn ich Morgens noch so fr\u00fch erwach', es singt,", "tokens": ["Wenn", "ich", "Mor\u00b7gens", "noch", "so", "fr\u00fch", "er\u00b7wach'", ",", "es", "singt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ADV", "ADJD", "VVFIN", "$,", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Und am Abend, wenn die Blumen alle", "tokens": ["Und", "am", "A\u00b7bend", ",", "wenn", "die", "Blu\u00b7men", "al\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "$,", "KOUS", "ART", "NN", "PIAT"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Sich zum Schlummer legten m\u00fcd am Bach, es singt.", "tokens": ["Sich", "zum", "Schlum\u00b7mer", "leg\u00b7ten", "m\u00fcd", "am", "Bach", ",", "es", "singt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PRF", "APPRART", "NN", "VVFIN", "ADJD", "APPRART", "NN", "$,", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.5": {"text": "Wenn es still wird, glaub' ich's noch zu h\u00f6ren,", "tokens": ["Wenn", "es", "still", "wird", ",", "glaub'", "ich's", "noch", "zu", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VAFIN", "$,", "VVFIN", "PIS", "ADV", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Weil mein Leid und all mein Ach es singt.", "tokens": ["Weil", "mein", "Leid", "und", "all", "mein", "Ach", "es", "singt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "KON", "PIAT", "PPOSAT", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Sitzt ein V\u00f6glein unter meinem Dach, es singt,", "tokens": ["Sitzt", "ein", "V\u00f6\u00b7glein", "un\u00b7ter", "mei\u00b7nem", "Dach", ",", "es", "singt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Wenn ich Morgens noch so fr\u00fch erwach', es singt,", "tokens": ["Wenn", "ich", "Mor\u00b7gens", "noch", "so", "fr\u00fch", "er\u00b7wach'", ",", "es", "singt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ADV", "ADJD", "VVFIN", "$,", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Und am Abend, wenn die Blumen alle", "tokens": ["Und", "am", "A\u00b7bend", ",", "wenn", "die", "Blu\u00b7men", "al\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "$,", "KOUS", "ART", "NN", "PIAT"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text"