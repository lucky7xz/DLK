{"dta.poem.2995": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "41.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es gibt ein Jenseit, das herein ins Disseit reicht;", "tokens": ["Es", "gibt", "ein", "Jen\u00b7seit", ",", "das", "her\u00b7ein", "ins", "Dis\u00b7seit", "reicht", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PRELS", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Kein Herz ist, das davon nicht ein Gef\u00fchl beschleicht.", "tokens": ["Kein", "Herz", "ist", ",", "das", "da\u00b7von", "nicht", "ein", "Ge\u00b7f\u00fchl", "be\u00b7schleicht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "$,", "PRELS", "PAV", "PTKNEG", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Umschlungen h\u00e4lt es dich, umrungen und durchdrungen;", "tokens": ["Um\u00b7schlun\u00b7gen", "h\u00e4lt", "es", "dich", ",", "um\u00b7run\u00b7gen", "und", "durch\u00b7drun\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PRF", "$,", "VVPP", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du f\u00fchlst, es ist nicht dir, du selbst bist ihm entsprungen.", "tokens": ["Du", "f\u00fchlst", ",", "es", "ist", "nicht", "dir", ",", "du", "selbst", "bist", "ihm", "ent\u00b7sprun\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VAFIN", "PTKNEG", "PPER", "$,", "PPER", "ADV", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Du wei\u00dft nicht, was es ist, doch h\u00f6rst du da\u00df es spricht,", "tokens": ["Du", "wei\u00dft", "nicht", ",", "was", "es", "ist", ",", "doch", "h\u00f6rst", "du", "da\u00df", "es", "spricht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWS", "PPER", "VAFIN", "$,", "ADV", "VVFIN", "PPER", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Lieb' ist es und nicht Ha\u00df, nicht Finster, sondern Licht.", "tokens": ["Lieb'", "ist", "es", "und", "nicht", "Ha\u00df", ",", "nicht", "Fins\u00b7ter", ",", "son\u00b7dern", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "KON", "PTKNEG", "NN", "$,", "PTKNEG", "NN", "$,", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Es ist das Wirkliche, das Wahrheit in dir wirkt,", "tokens": ["Es", "ist", "das", "Wirk\u00b7li\u00b7che", ",", "das", "Wahr\u00b7heit", "in", "dir", "wirkt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "ART", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das Unerkl\u00e4rliche, des Klarheit dich umzirkt.", "tokens": ["Das", "Un\u00b7er\u00b7kl\u00e4r\u00b7li\u00b7che", ",", "des", "Klar\u00b7heit", "dich", "um\u00b7zirkt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Du kannst den Mittelpunkt der Seele dir nicht rauben,", "tokens": ["Du", "kannst", "den", "Mit\u00b7tel\u00b7punkt", "der", "See\u00b7le", "dir", "nicht", "rau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "ART", "NN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und mu\u00dft dem innern Sinn, wie deinen \u00e4u\u00dfern, glauben.", "tokens": ["Und", "mu\u00dft", "dem", "in\u00b7nern", "Sinn", ",", "wie", "dei\u00b7nen", "\u00e4u\u00b7\u00dfern", ",", "glau\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "ADJA", "NN", "$,", "PWAV", "PPOSAT", "ADJA", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Siehst du daf\u00fcr dich um nach Zeugnis der Erfahrung,", "tokens": ["Siehst", "du", "da\u00b7f\u00fcr", "dich", "um", "nach", "Zeug\u00b7nis", "der", "Er\u00b7fah\u00b7rung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PAV", "PRF", "APPR", "APPR", "NN", "ART", "NN", "$,"], "meter": "+-+-++-+-+-+-", "measure": "unknown.measure.septa"}, "line.2": {"text": "So nennst du, was damit einstimmet, Offenbarung.", "tokens": ["So", "nennst", "du", ",", "was", "da\u00b7mit", "ein\u00b7stim\u00b7met", ",", "Of\u00b7fen\u00b7ba\u00b7rung", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PRELS", "PAV", "VVFIN", "$,", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Nichts wird dir offenbart, wo du nicht offen bist;", "tokens": ["Nichts", "wird", "dir", "of\u00b7fen\u00b7bart", ",", "wo", "du", "nicht", "of\u00b7fen", "bist", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPER", "VVFIN", "$,", "PWAV", "PPER", "PTKNEG", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und au\u00dfen siehst du nichts, was dir nicht innen ist.", "tokens": ["Und", "au\u00b7\u00dfen", "siehst", "du", "nichts", ",", "was", "dir", "nicht", "in\u00b7nen", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PIS", "$,", "PWS", "PPER", "PTKNEG", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Das Aeu\u00dfre dient dir nur, dein Innres zu entfalten,", "tokens": ["Das", "A\u00b7e\u00b7u\u00df\u00b7re", "dient", "dir", "nur", ",", "dein", "Inn\u00b7res", "zu", "ent\u00b7fal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "$,", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Dein Innres, weiter dann das Aeu\u00dfre zu gestalten.", "tokens": ["Dein", "Inn\u00b7res", ",", "wei\u00b7ter", "dann", "das", "A\u00b7e\u00b7u\u00df\u00b7re", "zu", "ge\u00b7stal\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADV", "ADV", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}, "stanza.9": {"line.1": {"text": "Dann siehst du ausgemalt aus deinem Farbenschatze", "tokens": ["Dann", "siehst", "du", "aus\u00b7ge\u00b7malt", "aus", "dei\u00b7nem", "Far\u00b7ben\u00b7schat\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dein Jenseit leibhaft als Verkl\u00e4rung oder Fratze.", "tokens": ["Dein", "Jen\u00b7seit", "leib\u00b7haft", "als", "Ver\u00b7kl\u00e4\u00b7rung", "o\u00b7der", "Frat\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "KOKOM", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}