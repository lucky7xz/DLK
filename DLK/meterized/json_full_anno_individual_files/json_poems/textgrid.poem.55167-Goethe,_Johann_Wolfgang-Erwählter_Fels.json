{"textgrid.poem.55167": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Erw\u00e4hlter Fels", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hier im stillen gedachte der Liebende seiner Geliebten;", "tokens": ["Hier", "im", "stil\u00b7len", "ge\u00b7dach\u00b7te", "der", "Lie\u00b7ben\u00b7de", "sei\u00b7ner", "Ge\u00b7lieb\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.2": {"text": "Heiter sprach er zu mir: \u00bbWerde mir Zeuge, du Stein!", "tokens": ["Hei\u00b7ter", "sprach", "er", "zu", "mir", ":", "\u00bb", "Wer\u00b7de", "mir", "Zeu\u00b7ge", ",", "du", "Stein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "PPER", "$.", "$(", "VAFIN", "PPER", "NN", "$,", "PPER", "NN", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Doch erhebe dich nicht, du hast noch viele Gesellen;", "tokens": ["Doch", "er\u00b7he\u00b7be", "dich", "nicht", ",", "du", "hast", "noch", "vie\u00b7le", "Ge\u00b7sel\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "$,", "PPER", "VAFIN", "ADV", "PIAT", "NN", "$."], "meter": "-+-----+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Jedem Felsen der Flur, die mich, den Gl\u00fccklichen, n\u00e4hrt,", "tokens": ["Je\u00b7dem", "Fel\u00b7sen", "der", "Flur", ",", "die", "mich", ",", "den", "Gl\u00fcck\u00b7li\u00b7chen", ",", "n\u00e4hrt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PIAT", "NN", "ART", "NN", "$,", "PRELS", "PPER", "$,", "ART", "NN", "$,", "VVFIN", "$,"], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "Jedem Baume des Walds, um den ich wandernd mich schlinge:", "tokens": ["Je\u00b7dem", "Bau\u00b7me", "des", "Walds", ",", "um", "den", "ich", "wan\u00b7dernd", "mich", "schlin\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "NN", "$,", "KOUI", "ART", "PPER", "VVFIN", "PPER", "VVFIN", "$."], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.6": {"text": "\u203adenkmal bleibe des Gl\u00fccks!\u2039 ruf ich ihm weihend und froh.", "tokens": ["\u203a", "denk\u00b7mal", "blei\u00b7be", "des", "Gl\u00fccks", "!", "\u2039", "ruf", "ich", "ihm", "wei\u00b7hend", "und", "froh", "."], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "ART", "NN", "$.", "$(", "NN", "PPER", "PPER", "ADJD", "KON", "ADJD", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.7": {"text": "Doch die Stimme verleih ich nur dir, wie unter der Menge", "tokens": ["Doch", "die", "Stim\u00b7me", "ver\u00b7leih", "ich", "nur", "dir", ",", "wie", "un\u00b7ter", "der", "Men\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "ADV", "PPER", "$,", "PWAV", "APPR", "ART", "NN"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.8": {"text": "Einen die Muse sich w\u00e4hlt, freundlich die Lippen ihm k\u00fc\u00dft.\u00ab", "tokens": ["Ei\u00b7nen", "die", "Mu\u00b7se", "sich", "w\u00e4hlt", ",", "freund\u00b7lich", "die", "Lip\u00b7pen", "ihm", "k\u00fc\u00