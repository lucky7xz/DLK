{"textgrid.poem.39310": {"metadata": {"author": {"name": "Strachwitz, Moritz von", "birth": "N.A.", "death": "N.A."}, "title": "[der alte Gondolier h\u00f6rt auf zu plaudern]", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der alte Gondolier h\u00f6rt auf zu plaudern,", "tokens": ["Der", "al\u00b7te", "Gon\u00b7do\u00b7lier", "h\u00f6rt", "auf", "zu", "plau\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Aus seinen Falten scheint es leis' zu rinnen,", "tokens": ["Aus", "sei\u00b7nen", "Fal\u00b7ten", "scheint", "es", "leis'", "zu", "rin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Durch ganz Venedig weht geheimes Schaudern.", "tokens": ["Durch", "ganz", "Ve\u00b7ne\u00b7dig", "weht", "ge\u00b7hei\u00b7mes", "Schau\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJD", "VVFIN", "ADJA", "NN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "So ist's! \u2013 Du wardst entfernt und gingst von hinnen,", "tokens": ["So", "ist's", "!", "\u2013", "Du", "wardst", "ent\u00b7fernt", "und", "gingst", "von", "hin\u00b7nen", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "$.", "$(", "PPER", "VAFIN", "ADJD", "KON", "VVFIN", "APPR", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch \u00e4ngstlich kehrs