{"textgrid.poem.52577": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Stille Thr\u00e4nen flie\u00dfen,", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Stille Thr\u00e4nen flie\u00dfen,", "tokens": ["Stil\u00b7le", "Thr\u00e4\u00b7nen", "flie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Wenn das Herz erkrankt,", "tokens": ["Wenn", "das", "Herz", "er\u00b7krankt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Bleiche Engel gr\u00fc\u00dfen,", "tokens": ["Blei\u00b7che", "En\u00b7gel", "gr\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Wo die Liebe wankt.", "tokens": ["Wo", "die", "Lie\u00b7be", "wankt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Furcht und Wehmuth schleichen her,", "tokens": ["Furcht", "und", "Weh\u00b7muth", "schlei\u00b7chen", "her", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Welche nimmer weichen mehr,", "tokens": ["Wel\u00b7che", "nim\u00b7mer", "wei\u00b7chen", "mehr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Bis wir sterben m\u00fcssen.", "tokens": ["Bis", "wir", "ster\u00