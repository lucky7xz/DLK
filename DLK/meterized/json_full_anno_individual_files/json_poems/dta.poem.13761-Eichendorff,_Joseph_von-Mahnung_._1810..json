{"dta.poem.13761": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Mahnung .  \n 1810.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Es soll im Kampf der rechte Schmerz sich adeln,", "tokens": ["Es", "soll", "im", "Kampf", "der", "rech\u00b7te", "Schmerz", "sich", "a\u00b7deln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPRART", "NN", "ART", "ADJA", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den deutschen Ruhm aus der Verw\u00fcstung heben,", "tokens": ["Den", "deut\u00b7schen", "Ruhm", "aus", "der", "Ver\u00b7w\u00fcs\u00b7tung", "he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das will der alte Gott von seinen S\u00f6hnen!\u201c", "tokens": ["Das", "will", "der", "al\u00b7te", "Gott", "von", "sei\u00b7nen", "S\u00f6h\u00b7nen", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VMFIN", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}