{"dta.poem.10414": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "XxXVIII.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Philister in Sonntagsr\u00f6cklein", "tokens": ["Phi\u00b7lis\u00b7ter", "in", "Sonn\u00b7tags\u00b7r\u00f6ck\u00b7lein"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPR", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Spazieren durch Wald und Flur;", "tokens": ["Spa\u00b7zie\u00b7ren", "durch", "Wald", "und", "Flur", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Sie jauchzen, sie h\u00fcpfen wie B\u00f6cklein,", "tokens": ["Sie", "jauch\u00b7zen", ",", "sie", "h\u00fcp\u00b7fen", "wie", "B\u00f6c\u00b7klein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "$,", "PPER", "VVFIN", "KOKOM", "NN", "$,"], "meter": "-+--+-+++", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Begr\u00fc\u00dfen die sch\u00f6ne Natur.", "tokens": ["Be\u00b7gr\u00fc\u00b7\u00dfen", "die", "sch\u00f6\u00b7ne", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "Betrachten mit blinzelnden Augen,", "tokens": ["Be\u00b7trach\u00b7ten", "mit", "blin\u00b7zeln\u00b7den", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Wie Alles romantisch bl\u00fcht;", "tokens": ["Wie", "Al\u00b7les", "ro\u00b7man\u00b7tisch", "bl\u00fcht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADJD", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Mit langen Ohren saugen", "tokens": ["Mit", "lan\u00b7gen", "Oh\u00b7ren", "sau\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sie ein der Spatzen Lied.", "tokens": ["Sie", "ein", "der", "Spat\u00b7zen", "Lied", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ich aber verh\u00e4nge die Fenster", "tokens": ["Ich", "a\u00b7ber", "ver\u00b7h\u00e4n\u00b7ge", "die", "Fens\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Des ", "tokens": ["Des"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Es machen mir meine Gespenster", "tokens": ["Es", "ma\u00b7chen", "mir", "mei\u00b7ne", "Ge\u00b7spens\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Sogar einen Tagesbesuch.", "tokens": ["So\u00b7gar", "ei\u00b7nen", "Ta\u00b7ges\u00b7be\u00b7such", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.4": {"line.1": {"text": "Die alte Liebe erscheinet,", "tokens": ["Die", "al\u00b7te", "Lie\u00b7be", "er\u00b7schei\u00b7net", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Sie stieg aus dem Todtenreich,", "tokens": ["Sie", "stieg", "aus", "dem", "Tod\u00b7ten\u00b7reich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Sie setzt sich zu mir und weinet,", "tokens": ["Sie", "setzt", "sich", "zu", "mir", "und", "wei\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PPER", "KON", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und macht das Herz mir weich.", "tokens": ["Und", "macht", "das", "Herz", "mir", "weich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PPER", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}