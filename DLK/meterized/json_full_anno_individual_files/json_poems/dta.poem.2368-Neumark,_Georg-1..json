{"dta.poem.2368": {"metadata": {"author": {"name": "Neumark, Georg", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1652", "urn": "urn:nbn:de:kobv:b4-20428-0", "language": ["de:0.99"], "booktitle": "Neumark, Georg: Poetisch- und Musikalisches Lustw\u00e4ldchen. Hamburg, 1652."}, "poem": {"stanza.1": {"line.1": {"text": "Sey gegr\u00fcsset sch\u00f6ne G\u00f6ttinn/ Tugenl fatkel dieser", "tokens": ["Sey", "ge\u00b7gr\u00fcs\u00b7set", "sch\u00f6\u00b7ne", "G\u00f6t\u00b7tinn", "/", "Tu\u00b7genl", "fat\u00b7kel", "die\u00b7ser"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "VVPP", "ADJA", "NN", "$(", "NN", "ADJD", "PDAT"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.2": {"text": "Stadt", "tokens": ["Stadt"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Von Dem/ welcher deine\u0303 Nahmen allzeit hochgeehret hat/", "tokens": ["Von", "Dem", "/", "wel\u00b7cher", "dein\u1ebd", "Nah\u00b7men", "all\u00b7zeit", "hoch\u00b7geeh\u00b7ret", "hat", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$(", "PWAT", "PPOSAT", "NN", "ADV", "VVPP", "VAFIN", "$("], "meter": "+-+--+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Vernim was Er dir itzo bringt/", "tokens": ["Ver\u00b7nim", "was", "Er", "dir", "it\u00b7zo", "bringt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PWS", "PPER", "PPER", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Was Er von deinen Gaben singt.", "tokens": ["Was", "Er", "von", "dei\u00b7nen", "Ga\u00b7ben", "singt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Kallibelle keusche Seele vierde Charis dieser Zeit/", "tokens": ["Kal\u00b7li\u00b7bel\u00b7le", "keu\u00b7sche", "See\u00b7le", "vier\u00b7de", "Cha\u00b7ris", "die\u00b7ser", "Zeit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "PIAT", "NN", "PDAT", "NN", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Zehnde G\u00f6ttinn unsrer K\u00fcnste deine hohe Trefligkeit", "tokens": ["Zehn\u00b7de", "G\u00f6t\u00b7tinn", "uns\u00b7rer", "K\u00fcns\u00b7te", "dei\u00b7ne", "ho\u00b7he", "Tre\u00b7flig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "PPOSAT", "NN", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Und deine Himmelswerthe Pracht/", "tokens": ["Und", "dei\u00b7ne", "Him\u00b7mels\u00b7wert\u00b7he", "Pracht", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dieh Sch\u00f6neste zur G\u00f6ttinn macht.", "tokens": ["Dieh", "Sch\u00f6\u00b7nes\u00b7te", "zur", "G\u00f6t\u00b7tinn", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Deiner hellen Augenstrahlen flinkern gleich dem Sternen-", "tokens": ["Dei\u00b7ner", "hel\u00b7len", "Au\u00b7gen\u00b7strah\u00b7len", "flin\u00b7kern", "gleich", "dem", "Ster\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ADV", "ART", "TRUNC"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.2": {"text": "Licht'/", "tokens": ["Licht'", "/"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Ich wil glauben da\u00df dem F\u00f6bus solcher sch\u00f6ne Glantz ge-", "tokens": ["Ich", "wil", "glau\u00b7ben", "da\u00df", "dem", "F\u00f6\u00b7bus", "sol\u00b7cher", "sch\u00f6\u00b7ne", "Glantz", "ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "VVINF", "KOUS", "ART", "NN", "PIAT", "ADJA", "NN", "TRUNC"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.4": {"text": "bricht/", "tokens": ["bricht", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "-", "measure": "single.down"}, "line.5": {"text": "Der Liebholdinnen Majest\u00e4t/", "tokens": ["Der", "Lieb\u00b7hol\u00b7din\u00b7nen", "Ma\u00b7jes\u00b7t\u00e4t", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In deinen Liebesaugen steht.", "tokens": ["In", "dei\u00b7nen", "Lie\u00b7be\u00b7sau\u00b7gen", "steht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Deiner Wangen Liebesfelder scheinen lauter Milch und", "tokens": ["Dei\u00b7ner", "Wan\u00b7gen", "Lie\u00b7bes\u00b7fel\u00b7der", "schei\u00b7nen", "lau\u00b7ter", "Milch", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "NN", "VVFIN", "PIAT", "NN", "KON"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.2": {"text": "Blut/", "tokens": ["Blut", "/"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Deiner Lippe\u0303 zukkerhonig ziehet nach sich Hertz und Muht/", "tokens": ["Dei\u00b7ner", "Lipp\u1ebd", "zuk\u00b7ker\u00b7ho\u00b7nig", "zie\u00b7het", "nach", "sich", "Hertz", "und", "Muht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "VVFIN", "APPR", "PRF", "NN", "KON", "NN", "$("], "meter": "+--+-+-+-+-+-+", "measure": "iambic.septa.invert"}, "line.4": {"text": "Kein weisser Schnee/ kein Heiffenbein/", "tokens": ["Kein", "weis\u00b7ser", "Schnee", "/", "kein", "Heif\u00b7fen\u00b7bein", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$(", "PIAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Kan deinen H\u00e4ndchen gleicher sein.", "tokens": ["Kan", "dei\u00b7nen", "H\u00e4nd\u00b7chen", "glei\u00b7cher", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Wenn du deine Seiten r\u00fchrest wird ein mattes Hertz er-", "tokens": ["Wenn", "du", "dei\u00b7ne", "Sei\u00b7ten", "r\u00fch\u00b7rest", "wird", "ein", "mat\u00b7tes", "Hertz", "er"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "VAFIN", "ART", "ADJA", "NN", "TRUNC"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.2": {"text": "quikkt/", "tokens": ["quikkt", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "-", "measure": "single.down"}, "line.3": {"text": "H\u00f6ret man denn deine Stimme stehet man als wie entz\u00fckkt!", "tokens": ["H\u00f6\u00b7ret", "man", "denn", "dei\u00b7ne", "Stim\u00b7me", "ste\u00b7het", "man", "als", "wie", "ent\u00b7z\u00fckkt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "PPOSAT", "NN", "VVFIN", "PIS", "KOKOM", "KOKOM", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Kalliope wird selbst beth\u00f6rt/", "tokens": ["Kal\u00b7li\u00b7o\u00b7pe", "wird", "selbst", "be\u00b7th\u00f6rt", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADV", "VVFIN", "$("], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Wann sie dich/ Sch\u00f6nste/ spielen h\u00f6rt.", "tokens": ["Wann", "sie", "dich", "/", "Sch\u00f6ns\u00b7te", "/", "spie\u00b7len", "h\u00f6rt", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "$(", "NN", "$(", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Jhr Athenschen Junggesellen stellt nur an ein Frenden-", "tokens": ["Ihr", "A\u00b7then\u00b7schen", "Jung\u00b7ge\u00b7sel\u00b7len", "stellt", "nur", "an", "ein", "Fren\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ADV", "APPR", "ART", "TRUNC"], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "spiet/", "tokens": ["spiet", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Lasset Agaristen bleiben hier ist eures Spieles ziehl.", "tokens": ["Las\u00b7set", "A\u00b7gar\u00b7is\u00b7ten", "blei\u00b7ben", "hier", "ist", "eu\u00b7res", "Spie\u00b7les", "ziehl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "VVFIN", "ADV", "VAFIN", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Denn Kallibellen Zierligkeit/", "tokens": ["Denn", "Kal\u00b7li\u00b7bel\u00b7len", "Zier\u00b7lig\u00b7keit", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Geht \u00fcber Agaristen weit.", "tokens": ["Geht", "\u00fc\u00b7ber", "A\u00b7gar\u00b7is\u00b7ten", "weit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Hettestu nur Kallibellen in Beotien ersehn/", "tokens": ["Het\u00b7te\u00b7stu", "nur", "Kal\u00b7li\u00b7bel\u00b7len", "in", "Beo\u00b7ti\u00b7en", "er\u00b7sehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "NN", "APPR", "NN", "VVINF", "$("], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Grosser Juppiter du hettest dein\u2019 Eginen lassen stehn/", "tokens": ["Gros\u00b7ser", "Jup\u00b7pi\u00b7ter", "du", "het\u00b7test", "dein'", "E\u00b7gi\u00b7nen", "las\u00b7sen", "stehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPER", "VAFIN", "PPOSAT", "NN", "VVINF", "VVINF", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Eginen/ derer s\u00fcsse Gunst/", "tokens": ["E\u00b7gi\u00b7nen", "/", "de\u00b7rer", "s\u00fcs\u00b7se", "Gunst", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$(", "PDS", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dich hat versetzt in Feuersbrunst.", "tokens": ["Dich", "hat", "ver\u00b7setzt", "in", "Feu\u00b7ers\u00b7brunst", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}