{"dta.poem.394": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Als sie sich badete.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Wie steht ihr so entr\u00fcst ihr schwimmenden Najaden,", "tokens": ["Wie", "steht", "ihr", "so", "ent\u00b7r\u00fcst", "ihr", "schwim\u00b7men\u00b7den", "Na\u00b7ja\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und scheint in Thr\u00e4nen mehr als in der Fluth zu baden?", "tokens": ["Und", "scheint", "in", "Thr\u00e4\u00b7nen", "mehr", "als", "in", "der", "Fluth", "zu", "ba\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "PIAT", "KOKOM", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch still/ es kr\u00e4ncket euch/ da\u00df sich ein Engels-Kind/", "tokens": ["Doch", "still", "/", "es", "kr\u00e4n\u00b7cket", "euch", "/", "da\u00df", "sich", "ein", "En\u00b7gels\u00b7Kind", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$(", "PPER", "VVFIN", "PPER", "$(", "KOUS", "PRF", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So eure Pracht weg sticht/ in denen Fluhten findt.", "tokens": ["So", "eu\u00b7re", "Pracht", "weg", "sticht", "/", "in", "de\u00b7nen", "Fluh\u00b7ten", "findt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "ADV", "VVFIN", "$(", "APPR", "PRELS", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}