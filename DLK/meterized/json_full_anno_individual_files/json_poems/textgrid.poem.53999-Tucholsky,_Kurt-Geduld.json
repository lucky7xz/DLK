{"textgrid.poem.53999": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Geduld", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die gro\u00dfen, sanften Augen der Bauernpferde, die", "tokens": ["Die", "gro\u00b7\u00dfen", ",", "sanf\u00b7ten", "Au\u00b7gen", "der", "Bau\u00b7ern\u00b7pfer\u00b7de", ",", "die"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "ART", "NN", "$,", "PRELS"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "still trottend ziehn; auf den Augenr\u00e4ndern", "tokens": ["still", "trot\u00b7tend", "ziehn", ";", "auf", "den", "Au\u00b7gen\u00b7r\u00e4n\u00b7dern"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "ADJD", "VVINF", "$.", "APPR", "ART", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "und in den Augenwinkeln sitzt es schwarz vor Fliegen . . .", "tokens": ["und", "in", "den", "Au\u00b7gen\u00b7win\u00b7keln", "sitzt", "es", "schwarz", "vor", "Flie\u00b7gen", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PPER", "ADJD", "APPR", "NN", "$.", "$.", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Geduld \u2013", "tokens": ["Ge\u00b7duld", "\u2013"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "-+", "measure": "iambic.single"}}, "stanza.2": {"line.1": {"text": "Der wie ein Paket geschn\u00fcrte Hund, dem der Professor", "tokens": ["Der", "wie", "ein", "Pa\u00b7ket", "ge\u00b7schn\u00fcr\u00b7te", "Hund", ",", "dem", "der", "Pro\u00b7fes\u00b7sor"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "KOKOM", "ART", "NN", "ADJA", "NN", "$,", "PRELS", "ART", "NN"], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Curare eingespritzt hat, nun kann er sich nicht bewegen, nur noch f\u00fchlen;", "tokens": ["Cu\u00b7ra\u00b7re", "ein\u00b7ge\u00b7spritzt", "hat", ",", "nun", "kann", "er", "sich", "nicht", "be\u00b7we\u00b7gen", ",", "nur", "noch", "f\u00fch\u00b7len", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVPP", "VAFIN", "$,", "ADV", "VMFIN", "PPER", "PRF", "PTKNEG", "VVINF", "$,", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-++--+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}, "line.3": {"text": "sie haben ihm die Harnleiter durchgeschnitten, da liegt er.", "tokens": ["sie", "ha\u00b7ben", "ihm", "die", "Harn\u00b7lei\u00b7ter", "durch\u00b7ge\u00b7schnit\u00b7ten", ",", "da", "liegt", "er", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ART", "NN", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "$."], "meter": "-+--+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Studenten umgeben das pr\u00e4chtige Bild . . .", "tokens": ["Stu\u00b7den\u00b7ten", "um\u00b7ge\u00b7ben", "das", "pr\u00e4ch\u00b7ti\u00b7ge", "Bild", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["NN", "VVPP", "ART", "ADJA", "NN", "$.", "$.", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Geduld \u2013", "tokens": ["Ge\u00b7duld", "\u2013"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "-+", "measure": "iambic.single"}}, "stanza.3": {"line.1": {"text": "Der verheiratete Angestellte, der vor dem brummigen Chef steht,", "tokens": ["Der", "ver\u00b7hei\u00b7ra\u00b7te\u00b7te", "An\u00b7ge\u00b7stell\u00b7te", ",", "der", "vor", "dem", "brum\u00b7mi\u00b7gen", "Chef", "steht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+--+-+--++", "measure": "trochaic.octa.plus.relaxed"}, "line.2": {"text": "zitternd, die K\u00fcndigung an den Kopf geworfen zu bekommen;", "tokens": ["zit\u00b7ternd", ",", "die", "K\u00fcn\u00b7di\u00b7gung", "an", "den", "Kopf", "ge\u00b7wor\u00b7fen", "zu", "be\u00b7kom\u00b7men", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "ART", "NN", "APPR", "ART", "NN", "VVPP", "PTKZU", "VVINF", "$."], "meter": "+--+--+-+-+-+-+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "der Mann hinter dem Schreibtisch f\u00fchlt sich: er hat auf einmal", "tokens": ["der", "Mann", "hin\u00b7ter", "dem", "Schreib\u00b7tisch", "f\u00fchlt", "sich", ":", "er", "hat", "auf", "ein\u00b7mal"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "PRF", "$.", "PPER", "VAFIN", "APPR", "ADV"], "meter": "-++--+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "zwei Leben: das eigne und das des andern . . .", "tokens": ["zwei", "Le\u00b7ben", ":", "das", "eig\u00b7ne", "und", "das", "des", "an\u00b7dern", ".", ".", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["CARD", "NN", "$.", "ART", "ADJA", "KON", "PDS", "ART", "ADJA", "$.", "$.", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Geduld \u2013", "tokens": ["Ge\u00b7duld", "\u2013"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "-+", "measure": "iambic.single"}}, "stanza.4": {"line.1": {"text": "Der Proletarier im Holzschrein vor Gericht, wo unaufh\u00f6rlich die", "tokens": ["Der", "Pro\u00b7le\u00b7ta\u00b7rier", "im", "Holz\u00b7schrein", "vor", "Ge\u00b7richt", ",", "wo", "un\u00b7auf\u00b7h\u00f6r\u00b7lich", "die"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN", "APPR", "NN", "$,", "PWAV", "ADJD", "ART"], "meter": "-+--+-+-+-+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "dreisten Ermahnungen des Richters kalt-sp\u00f6ttisch auf ihn", "tokens": ["dreis\u00b7ten", "Er\u00b7mah\u00b7nun\u00b7gen", "des", "Rich\u00b7ters", "kal\u00b7tsp\u00f6t\u00b7tisch", "auf", "ihn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "ART", "NN", "ADJD", "APPR", "PPER"], "meter": "+--+-+-+-++-+-", "measure": "iambic.septa.invert"}, "line.3": {"text": "heruntersausen . . .", "tokens": ["her\u00b7un\u00b7ter\u00b7sau\u00b7sen", ".", ".", "."], "token_info": ["word", "punct", "punct", "punct"], "pos": ["VVPP", "$.", "$.", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Geduld \u2013", "tokens": ["Ge\u00b7duld", "\u2013"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "-+", "measure": "iambic.single"}}, "stanza.5": {"line.1": {"text": "Das F\u00fcrsorgekind, das einer verwitweten Meg\u00e4re in die Anstaltsfinger", "tokens": ["Das", "F\u00fcr\u00b7sor\u00b7ge\u00b7kind", ",", "das", "ei\u00b7ner", "ver\u00b7wit\u00b7we\u00b7ten", "Me\u00b7g\u00e4\u00b7re", "in", "die", "An\u00b7stalts\u00b7fin\u00b7ger"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "ADJA", "NN", "APPR", "ART", "NN"], "meter": "-+--+-+--++--+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "gefallen ist; die braucht bei M\u00e4nnern keine Lust zu suchen,", "tokens": ["ge\u00b7fal\u00b7len", "ist", ";", "die", "braucht", "bei", "M\u00e4n\u00b7nern", "kei\u00b7ne", "Lust", "zu", "su\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "$.", "ART", "VVFIN", "APPR", "NN", "PIAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.3": {"text": "sie hat die Kinder . . .", "tokens": ["sie", "hat", "die", "Kin\u00b7der", ".", ".", "."], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$.", "$.", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Geduld \u2013", "tokens": ["Ge\u00b7duld", "\u2013"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "-+", "measure": "iambic.single"}}, "stanza.6": {"line.1": {"text": "L\u00e4utert Leiden? Welchen Sinn hat es?", "tokens": ["L\u00e4u\u00b7tert", "Lei\u00b7den", "?", "Wel\u00b7chen", "Sinn", "hat", "es", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "PWAT", "NN", "VAFIN", "PPER", "$."], "meter": "+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Was haben sie getan, mein Gott: das Pferd, der Hund,", "tokens": ["Was", "ha\u00b7ben", "sie", "ge\u00b7tan", ",", "mein", "Gott", ":", "das", "Pferd", ",", "der", "Hund", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "VVPP", "$,", "PPOSAT", "NN", "$.", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "der Angestellte, der Proletarier, das F\u00fcrsorgekind \u2013?", "tokens": ["der", "An\u00b7ge\u00b7stell\u00b7te", ",", "der", "Pro\u00b7le\u00b7ta\u00b7rier", ",", "das", "F\u00fcr\u00b7sor\u00b7ge\u00b7kind", "\u2013", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$(", "$."], "meter": "-+-+-+---+-+--+", "measure": "iambic.hexa.chol"}}, "stanza.7": {"line.1": {"text": "Sind sie schuld?", "tokens": ["Sind", "sie", "schuld", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Woran sind sie schuld?", "tokens": ["Wo\u00b7ran", "sind", "sie", "schuld", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Nimm ihnen die Geduld!", "tokens": ["Nimm", "ih\u00b7nen", "die", "Ge\u00b7duld", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Nimm ihnen die Geduld!", "tokens": ["Nimm", "ih\u00b7nen", "die", "Ge\u00b7duld", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Nimm ihnen die Geduld \u2013!", "tokens": ["Nimm", "ih\u00b7nen", "die", "Ge\u00b7duld", "\u2013", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "PPER", "ART", "NN", "$(", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Die gro\u00dfen, sanften Augen der Bauernpferde, die", "tokens": ["Die", "gro\u00b7\u00dfen", ",", "sanf\u00b7ten", "Au\u00b7gen", "der", "Bau\u00b7ern\u00b7pfer\u00b7de", ",", "die"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "ART", "NN", "$,", "PRELS"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "still trottend ziehn; auf den Auge