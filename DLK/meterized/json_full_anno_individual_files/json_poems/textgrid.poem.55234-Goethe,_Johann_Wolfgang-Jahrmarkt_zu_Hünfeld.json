{"textgrid.poem.55234": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Jahrmarkt zu H\u00fcnfeld", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich ging, mit stolzem Geistsvertrauen,", "tokens": ["Ich", "ging", ",", "mit", "stol\u00b7zem", "Geists\u00b7ver\u00b7trau\u00b7en", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Auf dem Jahrmarkt mich umzuschauen,", "tokens": ["Auf", "dem", "Jahr\u00b7markt", "mich", "um\u00b7zu\u00b7schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "VVIZU", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die K\u00e4ufer zu sehn an der H\u00e4ndler Ger\u00fcste,", "tokens": ["Die", "K\u00e4u\u00b7fer", "zu", "sehn", "an", "der", "H\u00e4nd\u00b7ler", "Ge\u00b7r\u00fcs\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Zu pr\u00fcfen, ob ich noch etwas w\u00fc\u00dfte,", "tokens": ["Zu", "pr\u00fc\u00b7fen", ",", "ob", "ich", "noch", "et\u00b7was", "w\u00fc\u00df\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "KOUS", "PPER", "ADV", "PIS", "VVFIN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Wie mir's Lavater, vor alter Zeit,", "tokens": ["Wie", "mir's", "La\u00b7va\u00b7ter", ",", "vor", "al\u00b7ter", "Zeit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "NE", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Traulich \u00fcberliefert \u2013 das ging sehr weit!", "tokens": ["Trau\u00b7lich", "\u00fc\u00b7berl\u00b7ie\u00b7fert", "\u2013", "das", "ging", "sehr", "weit", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "$(", "PDS", "VVFIN", "ADV", "ADJD", "$."], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.7": {"text": "Da sah ich denn zuerst Soldaten,", "tokens": ["Da", "sah", "ich", "denn", "zu\u00b7erst", "Sol\u00b7da\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Denen w\u00e4r's eben zum besten geraten:", "tokens": ["De\u00b7nen", "w\u00e4r's", "e\u00b7ben", "zum", "bes\u00b7ten", "ge\u00b7ra\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "APPRART", "ADJA", "VVPP", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.9": {"text": "Die Tat und Qual, sie war geschehn,", "tokens": ["Die", "Tat", "und", "Qual", ",", "sie", "war", "ge\u00b7schehn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "$,", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Wollten sich nicht gleich einer neuen versehn;", "tokens": ["Woll\u00b7ten", "sich", "nicht", "gleich", "ei\u00b7ner", "neu\u00b7en", "ver\u00b7sehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "PTKNEG", "ADV", "ART", "ADJA", "VVINF", "$."], "meter": "+--+-+-+--+", "measure": "iambic.penta.invert"}}, "stanza.2": {"line.1": {"text": "Der Rock war schon der Dirne genug,", "tokens": ["Der", "Rock", "war", "schon", "der", "Dir\u00b7ne", "ge\u00b7nug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Da\u00df sie ihm derb in die H\u00e4nde schlug.", "tokens": ["Da\u00df", "sie", "ihm", "derb", "in", "die", "H\u00e4n\u00b7de", "schlug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADJD", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Bauer und B\u00fcrger, die schienen stumm,", "tokens": ["Bau\u00b7er", "und", "B\u00fcr\u00b7ger", ",", "die", "schie\u00b7nen", "stumm", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "PRELS", "VVFIN", "ADJD", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Die guten Knaben beinahe dumm.", "tokens": ["Die", "gu\u00b7ten", "Kna\u00b7ben", "bei\u00b7na\u00b7he", "dumm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "ADJD", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Beutel und Scheune war gefegt,", "tokens": ["Beu\u00b7tel", "und", "Scheu\u00b7ne", "war", "ge\u00b7fegt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VAFIN", "VVPP", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Und hatten keine Ehre eingelegt.", "tokens": ["Und", "hat\u00b7ten", "kei\u00b7ne", "Eh\u00b7re", "ein\u00b7ge\u00b7legt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Erwart'ten alle, was da k\u00e4me,", "tokens": ["Er\u00b7wart'\u00b7ten", "al\u00b7le", ",", "was", "da", "k\u00e4\u00b7me", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$,", "PRELS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Wahrscheinlich auch nicht sehr bequeme.", "tokens": ["Wahr\u00b7schein\u00b7lich", "auch", "nicht", "sehr", "be\u00b7que\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PTKNEG", "ADV", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Frauen und M\u00e4gdlein, in guter Ruh,", "tokens": ["Frau\u00b7en", "und", "M\u00e4gd\u00b7lein", ",", "in", "gu\u00b7ter", "Ruh", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.10": {"text": "Probierten an die h\u00f6lzernen Schuh';", "tokens": ["Pro\u00b7bier\u00b7ten", "an", "die", "h\u00f6l\u00b7zer\u00b7nen", "Schuh'", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.11": {"text": "Man sah an Mienen und Geb\u00e4rden:", "tokens": ["Man", "sah", "an", "Mie\u00b7nen", "und", "Ge\u00b7b\u00e4r\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Sie ist guter Hoffnung oder will es werden.", "tokens": ["Sie", "ist", "gu\u00b7ter", "Hoff\u00b7nung", "o\u00b7der", "will", "es", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJA", 