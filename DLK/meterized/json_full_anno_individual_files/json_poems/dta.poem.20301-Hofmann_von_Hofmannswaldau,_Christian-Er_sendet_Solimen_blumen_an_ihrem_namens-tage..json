{"dta.poem.20301": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Er sendet Solimen blumen an ihrem  \n namens-tage.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nimm diesen blumen-strau\u00df von meinen schlechten h\u00e4nden/", "tokens": ["Nimm", "die\u00b7sen", "blu\u00b7men\u00b7strau\u00df", "von", "mei\u00b7nen", "schlech\u00b7ten", "h\u00e4n\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PDAT", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und schau in gnaden an/ du blume dieser zeit/", "tokens": ["Und", "schau", "in", "gna\u00b7den", "an", "/", "du", "blu\u00b7me", "die\u00b7ser", "zeit", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "NN", "PTKVZ", "$(", "PPER", "VVFIN", "PDAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Was himmel und natur und meine wenigkeit", "tokens": ["Was", "him\u00b7mel", "und", "na\u00b7tur", "und", "mei\u00b7ne", "we\u00b7nig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "NN", "KON", "NN", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Auff deinen namens-tag dir zum geschencke senden.", "tokens": ["Auff", "dei\u00b7nen", "na\u00b7mens\u00b7tag", "dir", "zum", "ge\u00b7schen\u00b7cke", "sen\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Dein auge/ das noch mehr als sonnen kan verblenden/", "tokens": ["Dein", "au\u00b7ge", "/", "das", "noch", "mehr", "als", "son\u00b7nen", "kan", "ver\u00b7blen\u00b7den", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "PDS", "ADV", "PIAT", "KOKOM", "ADV", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Schaut hier an farb und licht zwar manchen unterscheid/", "tokens": ["Schaut", "hier", "an", "farb", "und", "licht", "zwar", "man\u00b7chen", "un\u00b7ter\u00b7scheid", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "NN", "KON", "NN", "ADV", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Doch nicht an meiner treu/ die ewig schon bereit", "tokens": ["Doch", "nicht", "an", "mei\u00b7ner", "treu", "/", "die", "e\u00b7wig", "schon", "be\u00b7reit"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PTKNEG", "APPR", "PPOSAT", "ADJD", "$(", "ART", "ADJD", "ADV", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Nach dir/ als sonne/ sich/ wo du nur bist/ zu wenden.", "tokens": ["Nach", "dir", "/", "als", "son\u00b7ne", "/", "sich", "/", "wo", "du", "nur", "bist", "/", "zu", "wen\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPER", "$(", "KOUS", "NN", "$(", "PRF", "$(", "PWAV", "PPER", "ADV", "VAFIN", "$(", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ach! aber siehstu nicht die gantz verliebte pracht/", "tokens": ["Ach", "!", "a\u00b7ber", "sieh\u00b7stu", "nicht", "die", "gantz", "ver\u00b7lieb\u00b7te", "pracht", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ADV", "PPER", "PTKNEG", "ART", "ADV", "VVFIN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Die diese blumen schier zu todten leichen macht?", "tokens": ["Die", "die\u00b7se", "blu\u00b7men", "schier", "zu", "tod\u00b7ten", "lei\u00b7chen", "macht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PDAT", "NN", "ADJD", "PTKZU", "VVINF", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Sie seynd ein ebenbild von meinem krancken hertzen/", "tokens": ["Sie", "seynd", "ein", "e\u00b7ben\u00b7bild", "von", "mei\u00b7nem", "kran\u00b7cken", "hert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADV", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Das durch dein abseyn wird in solchen stand gesetzt;", "tokens": ["Das", "durch", "dein", "ab\u00b7seyn", "wird", "in", "sol\u00b7chen", "stand", "ge\u00b7setzt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "PPOSAT", "NN", "VAFIN", "APPR", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "O k\u00f6nt ich seyn/ wie sie/ durch deinen blick ergetzt/", "tokens": ["O", "k\u00f6nt", "ich", "seyn", "/", "wie", "sie", "/", "durch", "dei\u00b7nen", "blick", "er\u00b7getzt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPER", "VAINF", "$(", "PWAV", "PPER", "$(", "APPR", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Wie pl\u00f6tzlich w\u00fcrden sich verlieren meine schmertzen!", "tokens": ["Wie", "pl\u00f6tz\u00b7lich", "w\u00fcr\u00b7den", "sich", "ver\u00b7lie\u00b7ren", "mei\u00b7ne", "schmert\u00b7zen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PRF", "VVINF", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}