{"textgrid.poem.46588": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[nun ist das Licht im Steigen]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun ist das Licht im Steigen,", "tokens": ["Nun", "ist", "das", "Licht", "im", "Stei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Es geht ins neue Jahr.", "tokens": ["Es", "geht", "ins", "neu\u00b7e", "Jahr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "La\u00df deinen Muth nicht neigen,", "tokens": ["La\u00df", "dei\u00b7nen", "Muth", "nicht", "nei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Es bleibt nicht, wie es war.", "tokens": ["Es", "bleibt", "nicht", ",", "wie", "es", "war", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWAV", "PPER", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "So schwer zu sein, ist eigen", "tokens": ["So", "schwer", "zu", "sein", ",", "ist", "ei\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ADJD", "PTKZU", "VAINF", "$,", "VAFIN", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Im Anfang immerdar,", "tokens": ["Im", "An\u00b7fang", "im\u00b7mer\u00b7dar", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Am Ende wird sich's zeigen,", "tokens": ["Am", "En\u00b7de", "wird", "sich's", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wozu das Ganze war.", "tokens": ["Wo\u00b7zu", "das", "Gan\u00b7ze", "war", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Nicht zage gleich dem Feigen", "tokens": ["Nicht", "za\u00b7ge", "gleich", "dem", "Fei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKNEG", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Und klag' in der Gefahr!", "tokens": ["Und", "klag'", "in", "der", "Ge\u00b7fahr", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Schwing auf zum Sonnenreigen", "tokens": ["Schwing", "auf", "zum", "Son\u00b7nen\u00b7rei\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Dich schweigend wie der Aar!", "tokens": ["Dich", "schwei\u00b7gend", "wie", "der", "Aar", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Und wenn du kannst nicht schweigen,", "tokens": ["Und", "wenn", "du", "kannst", "nicht", "schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.1