{"dta.poem.5418": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Spuren der GOttheit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Alle Ding' in der Natur,", "tokens": ["Al\u00b7le", "Ding'", "in", "der", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Die wir schmecken, h\u00f6ren, sehen,", "tokens": ["Die", "wir", "schme\u00b7cken", ",", "h\u00f6\u00b7ren", ",", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$,", "VVINF", "$,", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Deuten kl\u00e4rlich an, gestehen,", "tokens": ["Deu\u00b7ten", "kl\u00e4r\u00b7lich", "an", ",", "ge\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "ADJD", "PTKVZ", "$,", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und erinnern uns nicht nur,", "tokens": ["Und", "e\u00b7rin\u00b7nern", "uns", "nicht", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Da\u00df ein GOtt, ein Sch\u00f6pfer sey;", "tokens": ["Da\u00df", "ein", "Gott", ",", "ein", "Sch\u00f6p\u00b7fer", "sey", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "ART", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Sondern, wenn wir redlich h\u00f6ren,", "tokens": ["Son\u00b7dern", ",", "wenn", "wir", "red\u00b7lich", "h\u00f6\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPER", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "H\u00f6ren wir die weisen Lehren:", "tokens": ["H\u00f6\u00b7ren", "wir", "die", "wei\u00b7sen", "Leh\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Da\u00df wir seiner GOttheit Schein,", "tokens": ["Da\u00df", "wir", "sei\u00b7ner", "Got\u00b7theit", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Der so hell, als allgemein,", "tokens": ["Der", "so", "hell", ",", "als", "all\u00b7ge\u00b7mein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "$,", "KOUS", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Zu bewundern zu verehren,", "tokens": ["Zu", "be\u00b7wun\u00b7dern", "zu", "ver\u00b7eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PTKZU", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Pflichtig und verbunden seyn.", "tokens": ["Pflich\u00b7tig", "und", "ver\u00b7bun\u00b7den", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "VVPP", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Aller Sternen helle Heere,", "tokens": ["Al\u00b7ler", "Ster\u00b7nen", "hel\u00b7le", "Hee\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Die im Boden-losen Meere,", "tokens": ["Die", "im", "Bo\u00b7den\u00b7lo\u00b7sen", "Mee\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "In den Tieffen ohne Gr\u00e4ntzen,", "tokens": ["In", "den", "Tief\u00b7fen", "oh\u00b7ne", "Gr\u00e4nt\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Allenthalben um uns gl\u00e4ntzen,", "tokens": ["Al\u00b7len\u00b7thal\u00b7ben", "um", "uns", "gl\u00e4nt\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Zeigen, bey entw\u00f6lckter Nacht,", "tokens": ["Zei\u00b7gen", ",", "bey", "ent\u00b7w\u00f6lck\u00b7ter", "Nacht", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.17": {"text": "Von der Wercke Wunder-Pracht,", "tokens": ["Von", "der", "Wer\u00b7cke", "Wun\u00b7der\u00b7Pracht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.18": {"text": "Zeigen von des Sch\u00f6pfers Macht,", "tokens": ["Zei\u00b7gen", "von", "des", "Sch\u00f6p\u00b7fers", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.19": {"text": "In der wirckenden Natur,", "tokens": ["In", "der", "wir\u00b7cken\u00b7den", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "--+---+", "measure": "anapaest.init"}, "line.20": {"text": "Uns die allerkl\u00e4rste Spur;", "tokens": ["Uns", "die", "al\u00b7ler\u00b7kl\u00e4rs\u00b7te", "Spur", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.21": {"text": "Sah\u2019, am unbekandten Strande,", "tokens": ["Sah'", ",", "am", "un\u00b7be\u00b7kand\u00b7ten", "Stran\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Dorten Bias in dem Sande", "tokens": ["Dor\u00b7ten", "Bias", "in", "dem", "San\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "APPR", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.23": {"text": "Mathematische Figuren;", "tokens": ["Ma\u00b7the\u00b7ma\u00b7ti\u00b7sche", "Fi\u00b7gu\u00b7ren", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Sprach er: ", "tokens": ["Sprach", "er", ":"], "token_info": ["word", "word", "punct"], "pos": ["NN", "PPER", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.25": {"text": "Wie viel mehr kann man in Sternen", "tokens": ["Wie", "viel", "mehr", "kann", "man", "in", "Ster\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ADV", "VMFIN", "PIS", "APPR", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.26": {"text": "Der Sapphirnen Himmels-H\u00f6h\u2019n", "tokens": ["Der", "Sap\u00b7phir\u00b7nen", "Him\u00b7mels\u00b7H\u00f6h'n"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.27": {"text": "Spuren einer GOttheit sehn,", "tokens": ["Spu\u00b7ren", "ei\u00b7ner", "Got\u00b7theit", "sehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.28": {"text": "Diese grosse Wahrheit lernen:", "tokens": ["Die\u00b7se", "gros\u00b7se", "Wahr\u00b7heit", "ler\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}