{"textgrid.poem.36939": {"metadata": {"author": {"name": "Gla\u00dfbrenner, Adolf", "birth": "N.A.", "death": "N.A."}, "title": "1L: Da nun die Welt die Freiheit liebt,", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da nun die Welt die Freiheit liebt,", "tokens": ["Da", "nun", "die", "Welt", "die", "Frei\u00b7heit", "liebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die ganze, weite Welt!", "tokens": ["Die", "gan\u00b7ze", ",", "wei\u00b7te", "Welt", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da muthig sich das Volk erhebt,", "tokens": ["Da", "mut\u00b7hig", "sich", "das", "Volk", "er\u00b7hebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und Kett' auf Kette f\u00e4llt;", "tokens": ["Und", "Kett'", "auf", "Ket\u00b7te", "f\u00e4llt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da die verfluchte Sclaverei", "tokens": ["Da", "die", "ver\u00b7fluch\u00b7te", "Scla\u00b7ve\u00b7rei"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Auf immerdar verschwand,", "tokens": ["Auf", "im\u00b7mer\u00b7dar", "ver\u00b7schwand", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und, frei vom kalten Leichentuch,", "tokens": ["Und", ",", "frei", "vom", "kal\u00b7ten", "Lei\u00b7chen\u00b7tuch", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADJD", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Es gr\u00fcnt und bl\u00fcht im Land:", "tokens": ["Es", "gr\u00fcnt", "und", "bl\u00fcht", "im", "Land", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "So sind nicht Wir,", "tokens": ["So", "sind", "nicht", "Wir", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PTKNEG", "PPER", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.10": {"text": "So seid es Ihr,", "tokens": ["So", "seid", "es", "Ihr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "Gew\u00fcrm am Thron,", "tokens": ["Ge\u00b7w\u00fcrm", "am", "Thron", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.12": {"text": "Ihr seid Opposition!", "tokens": ["Ihr", "seid", "Op\u00b7po\u00b7si\u00b7ti\u00b7on", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Da jedes gro\u00dfe, sch\u00f6ne Herz", "tokens": ["Da", "je\u00b7des", "gro\u00b7\u00dfe", ",", "sch\u00f6\u00b7ne", "Herz"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "PIAT", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "F\u00fcr's Wohl des Volkes schl\u00e4gt,", "tokens": ["F\u00fcr's", "Wohl", "des", "Vol\u00b7kes", "schl\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und schale Mittelm\u00e4\u00dfigkeit", "tokens": ["Und", "scha\u00b7le", "Mit\u00b7tel\u00b7m\u00e4\u00b7\u00dfig\u00b7keit"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nur Eure Fahne tr\u00e4gt;", "tokens": ["Nur", "Eu\u00b7re", "Fah\u00b7ne", "tr\u00e4gt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da \u00fcberall, durch Stadt und Land,", "tokens": ["Da", "\u00fc\u00b7be\u00b7rall", ",", "durch", "Stadt", "und", "Land", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Geist der Freiheit zieht,", "tokens": ["Der", "Geist", "der", "Frei\u00b7heit", "zieht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und das Gespenst der Tyrannei", "tokens": ["Und", "das", "Ge\u00b7spenst", "der", "Ty\u00b7ran\u00b7nei"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Vor seinem Schwerte flieht:", "tokens": ["Vor", "sei\u00b7nem", "Schwer\u00b7te", "flieht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "So sind nicht Wir,", "tokens": ["So", "sind", "nicht", "Wir", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PTKNEG", "PPER", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.10": {"text": "So seid es Ihr,", "tokens": ["So", "seid", "es", "Ihr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "Gew\u00fcrm am Thron,", "tokens": ["Ge\u00b7w\u00fcrm", "am", "Thron", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.12": {"text": "Ihr seid Opposition!", "tokens": ["Ihr", "seid", "Op\u00b7po\u00b7si\u00b7ti\u00b7on", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Da aus der Asche der Gewalt", "tokens": ["Da", "aus", "der", "A\u00b7sche", "der", "Ge\u00b7walt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Ph\u00f6nix Geist erstand,", "tokens": ["Der", "Ph\u00f6\u00b7nix", "Geist", "er\u00b7stand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nach seinem k\u00fchnen Sonnenflug", "tokens": ["Nach", "sei\u00b7nem", "k\u00fch\u00b7nen", "Son\u00b7nen\u00b7flug"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sich Aller Blick gewandt;", "tokens": ["Sich", "Al\u00b7ler", "Blick", "ge\u00b7wandt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da Poesie, Humor und Kunst", "tokens": ["Da", "Poe\u00b7sie", ",", "Hu\u00b7mor", "und", "Kunst"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Euch h\u00f6hnen, wenn Ihr kr\u00e4chzt;", "tokens": ["Euch", "h\u00f6h\u00b7nen", ",", "wenn", "Ihr", "kr\u00e4chzt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Als Lerchen jubiliren, da", "tokens": ["Als", "Ler\u00b7chen", "ju\u00b7bi\u00b7li\u00b7ren", ",", "da"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["KOUS", "NN", "VVINF", "$,", "KOUS"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der kranke Adler \u00e4chzt:", "tokens": ["Der", "kran\u00b7ke", "Ad\u00b7ler", "\u00e4chzt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "So sind nicht Wir,", "tokens": ["So", "sind", "nicht", "Wir", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PTKNEG", "PPER", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.10": {"text": "So seid es Ihr,", "tokens": ["So", "seid", "es", "Ihr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "Gew\u00fcrm am Thron,", "tokens": ["Ge\u00b7w\u00fcrm", "am", "Thron", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.12": {"text": "Ihr seid Opposition!", "tokens": ["Ihr", "seid", "Op\u00b7po\u00b7si\u00b7ti\u00b7on", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Da nun der Mensch zum Menschen ward", "tokens": ["Da", "nun", "der", "Mensch", "zum", "Men\u00b7schen", "ward"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "NN", "APPRART", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Aus schn\u00f6dem F\u00fcrstenknecht;", "tokens": ["Aus", "schn\u00f6\u00b7dem", "F\u00fcrs\u00b7ten\u00b7knecht", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da stolzer er nun tr\u00e4gt das Haupt,", "tokens": ["Da", "stol\u00b7zer", "er", "nun", "tr\u00e4gt", "das", "Haupt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und fordert keck sein Recht;", "tokens": ["Und", "for\u00b7dert", "keck", "sein", "Recht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da's nun so ist, so wie es ist,", "tokens": ["Da's", "nun", "so", "ist", ",", "so", "wie", "es", "ist", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADV", "VAFIN", "$,", "ADV", "KOKOM", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wie's \u00e4ndert kein Geschw\u00e4tz:", "tokens": ["Wie's", "\u00e4n\u00b7dert", "kein", "Ge\u00b7schw\u00e4tz", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "So sind die Frei'n loyal, loyal", "tokens": ["So", "sind", "die", "Frei'n", "lo\u00b7yal", ",", "lo\u00b7yal"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "PTKVZ", "$,", "XY"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Dem g\u00f6ttlichen Gesetz!", "tokens": ["Dem", "g\u00f6tt\u00b7li\u00b7chen", "Ge\u00b7setz", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "So sind ", "tokens": ["So", "sind"], "token_info": ["word", "word"], "pos": ["ADV", "VAFIN"], "meter": "+-", "measure": "trochaic.single"}, "line.10": {"text": "So seid es Ihr,", "tokens": ["So", "seid", "es", "Ihr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "Gew\u00fcrm am Thron,", "tokens": ["Ge\u00b7w\u00fcrm", "am", "Thron", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.12": {"text": "Ihr seid Opposition!", "tokens": ["Ihr", "seid", "Op\u00b7po\u00b7si\u00b7ti\u00b7on", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Da nun die Welt die Freiheit liebt,", "tokens": ["Da", "nun", "die", "Welt", "die", "Frei\u00b7heit", "liebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die ganze, weite Welt!", "tokens": ["Die", "gan\u00b7ze", ",", "wei\u00b7te", "Welt", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+", "me