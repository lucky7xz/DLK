{"dta.poem.2406": {"metadata": {"author": {"name": "Neumark, Georg", "birth": "N.A.", "death": "N.A."}, "title": "An einen zwar reichen und vornehmen/ doch  \n aber sehr unverst\u00e4ndigen/ groben und hohnspot-  \n tenden Zuh\u00f6rer/ des Violdigammen-  \n spiels.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1652", "urn": "urn:nbn:de:kobv:b4-20428-0", "language": ["de:0.99"], "booktitle": "Neumark, Georg: Poetisch- und Musikalisches Lustw\u00e4ldchen. Hamburg, 1652."}, "poem": {"stanza.1": {"line.1": {"text": "Ich hett' es nimmermehr bey mir gedenken k\u00f6nnen", "tokens": ["Ich", "hett'", "es", "nim\u00b7mer\u00b7mehr", "bey", "mir", "ge\u00b7den\u00b7ken", "k\u00f6n\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "APPR", "PPER", "VVINF", "VMINF"], "meter": "-+-+-++--+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Da\u00df solch ein Unverstand und solche grobe Sinnen/", "tokens": ["Da\u00df", "solch", "ein", "Un\u00b7ver\u00b7stand", "und", "sol\u00b7che", "gro\u00b7be", "Sin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "ART", "NN", "KON", "PIAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Bey Menschen k\u00f6nten sein als ich bey dir versp\u00fchrt", "tokens": ["Bey", "Men\u00b7schen", "k\u00f6n\u00b7ten", "sein", "als", "ich", "bey", "dir", "ver\u00b7sp\u00fchrt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VMFIN", "PPOSAT", "KOUS", "PPER", "APPR", "PPER", "VVPP"], "meter": "-+-+-+-+++-+", "measure": "unknown.measure.septa"}, "line.4": {"text": "Du grober Eselskopff. Du hast dich zwar geziehrt/", "tokens": ["Du", "gro\u00b7ber", "E\u00b7sels\u00b7kopff", ".", "Du", "hast", "dich", "zwar", "ge\u00b7ziehrt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$.", "PPER", "VAFIN", "PPER", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Mit Seiden und mit Sammt mit breiten g\u00f6lnen Spitzen", "tokens": ["Mit", "Sei\u00b7den", "und", "mit", "Sammt", "mit", "brei\u00b7ten", "g\u00f6l\u00b7nen", "Spit\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "APPR", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Hast aber auch bey dir den gr\u00f6sten Unflad sitzen/", "tokens": ["Hast", "a\u00b7ber", "auch", "bey", "dir", "den", "gr\u00f6s\u00b7ten", "Un\u00b7flad", "sit\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "APPR", "PPER", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-++--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Ich meine T\u00f6lpeley/ pfuy Schl\u00fcngel/ Grobian/", "tokens": ["Ich", "mei\u00b7ne", "T\u00f6l\u00b7pe\u00b7ley", "/", "pfuy", "Schl\u00fcn\u00b7gel", "/", "Gro\u00b7bi\u00b7an", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "$(", "NE", "NN", "$(", "NE", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Versilberter Klau\u00dfnarr/ geputzter Pafian!", "tokens": ["Ver\u00b7sil\u00b7ber\u00b7ter", "Klau\u00df\u00b7narr", "/", "ge\u00b7putz\u00b7ter", "Pa\u00b7fi\u00b7an", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NE", "$(", "ADJA", "NN", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.9": {"text": "Ist die Violdigam dir Midaskopff zu wieder/", "tokens": ["Ist", "die", "Vi\u00b7ol\u00b7di\u00b7gam", "dir", "Mi\u00b7das\u00b7kopff", "zu", "wie\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PPER", "NN", "APPR", "ADV", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und h\u00f6rest auch nicht gern die wolgesetzten Lieder/", "tokens": ["Und", "h\u00f6\u00b7rest", "auch", "nicht", "gern", "die", "wol\u00b7ge\u00b7setz\u00b7ten", "Lie\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PTKNEG", "ADV", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "So pakke dich hinau\u00df und stelle dich dort ein", "tokens": ["So", "pak\u00b7ke", "dich", "hin\u00b7au\u00df", "und", "stel\u00b7le", "dich", "dort", "ein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "KON", "VVFIN", "PPER", "ADV", "ART"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Wo wilde Thrazier und grobe Bauren seyn.", "tokens": ["Wo", "wil\u00b7de", "Thra\u00b7zier", "und", "gro\u00b7be", "Bau\u00b7ren", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "KON", "ADJA", "NN", "VAINF", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "Denn dieses s\u00fcsse Spiel und sein beliebter Meister/", "tokens": ["Denn", "die\u00b7ses", "s\u00fcs\u00b7se", "Spiel", "und", "sein", "be\u00b7lieb\u00b7ter", "Meis\u00b7ter", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "ADJA", "NN", "KON", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Geh\u00f6ren nur allein vor hoh\u2019 und \u00e4dle Geister/", "tokens": ["Ge\u00b7h\u00f6\u00b7ren", "nur", "al\u00b7lein", "vor", "hoh'", "und", "\u00e4d\u00b7le", "Geis\u00b7ter", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "APPR", "ADJD", "KON", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Und nicht vor solche Narrn. Was sag\u2019 ich aber viel?", "tokens": ["Und", "nicht", "vor", "sol\u00b7che", "Narrn", ".", "Was", "sag'", "ich", "a\u00b7ber", "viel", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "APPR", "PIAT", "NN", "$.", "PWS", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Vor dich Herr Urian geh\u00f6rt ein Leyerspiel.", "tokens": ["Vor", "dich", "Herr", "U\u00b7ri\u00b7an", "ge\u00b7h\u00f6rt", "ein", "Ley\u00b7er\u00b7spiel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "NN", "NE", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}