{"textgrid.poem.54537": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "26.", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Gott! was rauhe Noth/ wie schaumt die schwartze See", "tokens": ["O", "Gott", "!", "was", "rau\u00b7he", "Noth", "/", "wie", "schaumt", "die", "schwart\u00b7ze", "See"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "$.", "PWS", "ADJA", "NN", "$(", "PWAV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und spr\u00fctzt ihr gr\u00fcnes Saltz/ wie reist der Zorn die Wellen", "tokens": ["Und", "spr\u00fctzt", "ihr", "gr\u00fc\u00b7nes", "Saltz", "/", "wie", "reist", "der", "Zorn", "die", "Wel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJA", "NN", "$(", "PWAV", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Durch Nebel volle Lufft wie heult das w\u00fcste Bellen", "tokens": ["Durch", "Ne\u00b7bel", "vol\u00b7le", "Lufft", "wie", "heult", "das", "w\u00fcs\u00b7te", "Bel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "ADJA", "NN", "KOKOM", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der tollen St\u00fcrm uns an. Die Klippe kracht von Weh/", "tokens": ["Der", "tol\u00b7len", "St\u00fcrm", "uns", "an", ".", "Die", "Klip\u00b7pe", "kracht", "von", "Weh", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA",