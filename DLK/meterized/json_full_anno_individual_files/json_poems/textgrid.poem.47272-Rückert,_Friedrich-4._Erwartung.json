{"textgrid.poem.47272": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "4. Erwartung", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn ich gegen Tages Mitte", "tokens": ["Wenn", "ich", "ge\u00b7gen", "Ta\u00b7ges", "Mit\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Setz' ins Zimmer meine Schritte,", "tokens": ["Setz'", "ins", "Zim\u00b7mer", "mei\u00b7ne", "Schrit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die auf Schnee und Eis geweilt;", "tokens": ["Die", "auf", "Schnee", "und", "Eis", "ge\u00b7weilt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Denk' ich, auf dem Wust des Tisches", "tokens": ["Denk'", "ich", ",", "auf", "dem", "Wust", "des", "Ti\u00b7sches"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "$,", "APPR", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Liegen m\u00fcss' ein Blatt, ein frisches", "tokens": ["Lie\u00b7gen", "m\u00fcss'", "ein", "Blatt", ",", "ein", "fri\u00b7sches"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VMFIN", "ART", "NN", "$,", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Das vom Freunde Kund' erteilt,", "tokens": ["Das", "vom", "Freun\u00b7de", "Kund'", "er\u00b7teilt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPRART", "NN", "NE", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wie die r\u00f6tlich angegl\u00fchte", "tokens": ["Wie", "die", "r\u00f6t\u00b7lich", "an\u00b7ge\u00b7gl\u00fch\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJD", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Mandelbl\u00fcte,", "tokens": ["Man\u00b7del\u00b7bl\u00fc\u00b7te", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "Die voraus dem Fr\u00fchling eilt.", "tokens": ["Die", "vo\u00b7raus", "dem", "Fr\u00fch\u00b7ling", "eilt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "----+-+", "measure": "unknown.measure.di"}}, "stanza.2": {"line.1": {"text": "Oder wann ich lange tr\u00e4umte,", "tokens": ["O\u00b7der", "wann", "ich", "lan\u00b7ge", "tr\u00e4um\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie er nun so lange s\u00e4umte,", "tokens": ["Wie", "er", "nun", "so", "lan\u00b7ge", "s\u00e4um\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADV", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sich zu melden; kommt mir's vor,", "tokens": ["Sich", "zu", "mel\u00b7den", ";", "kommt", "mir's", "vor", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PRF", "PTKZU", "VVINF", "$.", "VVFIN", "NE", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Selber m\u00fcss' er aus den Ecken", "tokens": ["Sel\u00b7ber", "m\u00fcs