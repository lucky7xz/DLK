{"dta.poem.19922": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Der Pilger .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905193111", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Auf d\u00fcrrer Haide geht", "tokens": ["Auf", "d\u00fcr\u00b7rer", "Hai\u00b7de", "geht"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NE", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Ein armer Wandersmann,", "tokens": ["Ein", "ar\u00b7mer", "Wan\u00b7ders\u00b7mann", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Kein k\u00fchlend L\u00fcftchen weht,", "tokens": ["Kein", "k\u00fch\u00b7lend", "L\u00fcft\u00b7chen", "weht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJD", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Das ihn erquicken kann.", "tokens": ["Das", "ihn", "er\u00b7qui\u00b7cken", "kann", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Er schaut Land ein, Land aus,", "tokens": ["Er", "schaut", "Land", "ein", ",", "Land", "aus", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "PTKVZ", "$,", "NN", "PTKVZ", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Horcht, keine Quelle flie\u00dft,", "tokens": ["Horcht", ",", "kei\u00b7ne", "Quel\u00b7le", "flie\u00dft", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Blickt, sieht nicht Wald, noch Haus,", "tokens": ["Blickt", ",", "sieht", "nicht", "Wald", ",", "noch", "Haus", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "PTKNEG", "NN", "$,", "ADV", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "So schattend ihn umschlie\u00dft.", "tokens": ["So", "schat\u00b7tend", "ihn", "um\u00b7schlie\u00dft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Er kann nicht weiter gehn,", "tokens": ["Er", "kann", "nicht", "wei\u00b7ter", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Er sinkt auf's d\u00fcrre Moos, \u2014", "tokens": ["Er", "sinkt", "auf's", "d\u00fcr\u00b7re", "Moos", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN", "$,", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch sieh! auf Bergesh\u00f6hn", "tokens": ["Doch", "sieh", "!", "auf", "Ber\u00b7ges\u00b7h\u00f6hn"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["KON", "VVIMP", "$.", "APPR", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Erblickt er jezt ein Schlo\u00df.", "tokens": ["Er\u00b7blickt", "er", "jezt", "ein", "Schlo\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "\u201eo Kranker! freue dich!", "tokens": ["\u201e", "o", "Kran\u00b7ker", "!", "freu\u00b7e", "dich", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "FM", "NN", "$.", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "\u201edas nimmt dich gastlich auf!\u201c", "tokens": ["\u201e", "das", "nimmt", "dich", "gast\u00b7lich", "auf", "!", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PDS", "VVFIN", "PRF", "ADJD", "PTKVZ", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er rafft zusammen sich,", "tokens": ["Er", "rafft", "zu\u00b7sam\u00b7men", "sich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PRF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Er eilt den Berg hinauf.", "tokens": ["Er", "eilt", "den", "Berg", "hin\u00b7auf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und als er auf den H\u00f6h'n \u2014", "tokens": ["Und", "als", "er", "auf", "den", "H\u00f6h'n"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "ART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Kein Schlo\u00df ersieht er mehr,", "tokens": ["Kein", "Schlo\u00df", "er\u00b7sieht", "er", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sieht eine Wolke stehn,", "tokens": ["Sieht", "ei\u00b7ne", "Wol\u00b7ke", "stehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die bald erstirbt, wie er.", "tokens": ["Die", "bald", "er\u00b7stirbt", ",", "wie", "er", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "$,", "PWAV", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}