{"textgrid.poem.48787": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "12. Er redet der Liebsten Augen an, die er umfinge", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr seid es, die ihr mir die meinen machet blind,", "tokens": ["Ihr", "seid", "es", ",", "die", "ihr", "mir", "die", "mei\u00b7nen", "ma\u00b7chet", "blind", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "$,", "PRELS", "PPER", "PRF", "ART", "PPOSAT", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "ihr lichten Spiegel ihr, da ich die ganzen Schmerzen", "tokens": ["ihr", "lich\u00b7ten", "Spie\u00b7gel", "ihr", ",", "da", "ich", "die", "gan\u00b7zen", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "PPER", "$,", "KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "leibhaftig kan besehn von mein und ihrem Herzen.", "tokens": ["leib\u00b7haf\u00b7tig", "kan", "be\u00b7sehn", "von", "mein", "und", "ih\u00b7rem", "Her\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VMFIN", "VVINF", "APPR", "PPOSAT", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ihr Werkstat, da die Gunst die g\u00fcldnen F\u00e4den spinnt,", "tokens": ["Ihr", "Werk\u00b7stat", ",", "da", "die", "Gunst", "die", "g\u00fcld\u00b7nen", "F\u00e4\u00b7den", "spinnt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "KOUS", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "dar\u00fcber Meister ist das kluge Venus-Kind,", "tokens": ["da\u00b7r\u00fc\u00b7ber", "Meis\u00b7ter", "ist", "das", "klu\u00b7ge", "Ve\u00b7nus\u00b7Kind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "ihr, meine Sonn' und Mon, ihr irdnen Himmelskerzen,", "tokens": ["ihr", ",", "mei\u00b7ne", "Sonn'", "und", "Mon", ",", "ihr", "ird\u00b7nen", "Him\u00b7mels\u00b7ker\u00b7zen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PPOSAT", "NN", "KON", "NE", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "in welchen Lust mit Zier und Schein mit Glanze scherzen,", "tokens": ["in", "wel\u00b7chen", "Lust", "mit", "Zier", "und", "Schein", "mit", "Glan\u00b7ze", "scher\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "APPR", "NN", "KON", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "ihr seid es, die mir mehr als alle Sch\u00e4tze sind!", "tokens": ["ihr", "seid", "es", ",", "die", "mir", "mehr", "als", "al\u00b7le", "Sch\u00e4t\u00b7ze", "sind", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "$,", "PRELS", "PPER", "PIAT", "KOKOM", "PIAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ihr R\u00e4uber meines Lichts und dessen Wiedergeber,", "tokens": ["Ihr", "R\u00e4u\u00b7ber", "mei\u00b7nes", "Lichts", "und", "des\u00b7sen", "Wie\u00b7der\u00b7ge\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPOSAT", "NN", "KON", "PRELAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "von euch zeucht Amor ein und aus in meine Leber,", "tokens": ["von", "euch", "zeucht", "A\u00b7mor", "ein", "und", "aus", "in", "mei\u00b7ne", "Le\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "NE", "PTKVZ", "KON", "APPR", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "als sein nat\u00fcrlichs Haus. Ihr beide tut mir kund,", "tokens": ["als", "sein", "na\u00b7t\u00fcr\u00b7lichs", "Haus", ".", "Ihr", "bei\u00b7de", "tut", "mir", "kund", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "$.", "PPER", "PIS", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "ihr stummen Redner ihr, worvon ich itzt noch sunge:", "tokens": ["ihr", "stum\u00b7men", "Red\u00b7ner", "ihr", ",", "wor\u00b7von", "ich", "itzt", "noch", "sun\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PPER", "$,", "PWAV", "PPER", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "verschweigets gleich ihr Mund und sagts nicht ihre Zunge,", "tokens": ["ver\u00b7schwei\u00b7gets", "gleich", "ihr", "Mund", "und", "sagts", "nicht", "ih\u00b7re", "Zun\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word",