{"textgrid.poem.57360": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie sich der Liebende freut, wenn nun die Geliebte, der hohen", "genre": "verse", "period": "N.A.", "pub_year": 1793, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie sich der Liebende freut, wenn nun die Geliebte, der hohen", "tokens": ["Wie", "sich", "der", "Lie\u00b7ben\u00b7de", "freut", ",", "wenn", "nun", "die", "Ge\u00b7lieb\u00b7te", ",", "der", "ho\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "PRF", "ART", "NN", "VVFIN", "$,", "KOUS", "ADV", "ART", "NN", "$,", "ART", "ADJA"], "meter": "-+-+--+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Todeswog' entflohn, wieder das Ufer betritt;", "tokens": ["To\u00b7des\u00b7wo\u00b7g'", "ent\u00b7flohn", ",", "wie\u00b7der", "das", "U\u00b7fer", "be\u00b7tritt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Oft schon hatt' er hinunter geschaut an dem Marmor des Strandes,", "tokens": ["Oft", "schon", "hatt'", "er", "hin\u00b7un\u00b7ter", "ge\u00b7schaut", "an", "dem", "Mar\u00b7mor", "des", "Stran\u00b7des", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "ADV", "VVPP", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.4": {"text": "Immer, neuen Gram, Scheiter und Leichen gesehn;", "tokens": ["Im\u00b7mer", ",", "neu\u00b7en", "Gram", ",", "Schei\u00b7ter", "und", "Lei\u00b7chen", "ge\u00b7sehn", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADJA", "NN", "$,", "NN", "KON", "NN", "VVPP", "$."], "meter": "+-+-++--+--+", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "Endlich sinket sie ihm aus einem Nachen, der antreibt,", "tokens": ["End\u00b7lich", "sin\u00b7ket", "sie", "ihm", "aus", "ei\u00b7nem", "Na\u00b7chen", ",", "der", "an\u00b7treibt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "APPR", "ART", "NN", "$,", "PRELS", "VVFIN", "$,"], "meter": "+-+--+-+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.6": {"text": "An das schlagende Herz, siehet den lebenden! lebt!", "tokens": ["An", "das", "schla\u00b7gen\u00b7de", "Herz", ",", "sie\u00b7het", "den", "le\u00b7ben\u00b7den", "!", "lebt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "VVFIN", "ART", "ADJA", "$.", "VVFIN", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.7": {"text": "Oder wie die Mutter, die harrend und stumm an dem Thor lag", "tokens": ["O\u00b7der", "wie", "die", "Mut\u00b7ter", ",", "die", "har\u00b7rend", "und", "stumm", "an", "dem", "Thor", "lag"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "$,", "PRELS", "ADJD", "KON", "ADJD", "APPR", "ART", "NN", "VVFIN"], "meter": "+-+-+--+--+--+-", "measure": "hexameter"}, "line.8": {"text": "Einer durchpesteten Stadt, welche den einzigen Sohn", "tokens": ["Ei\u00b7ner", "durch\u00b7pes\u00b7te\u00b7ten", "Stadt", ",", "wel\u00b7che", "den", "ein\u00b7zi\u00b7gen", "Sohn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ART", "ADJA", "NN"], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.9": {"text": "Mit zahllosen Sterbenden ihr, und Begrabenen einschloss,", "tokens": ["Mit", "zahl\u00b7lo\u00b7sen", "Ster\u00b7ben\u00b7den", "ihr", ",", "und", "Be\u00b7gra\u00b7be\u00b7nen", "ein\u00b7schloss", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "$,", "KON", "NN", "VVFIN", "$,"], "meter": "-+--+--++-+-+-+", "measure": "iambic.septa.relaxed"}, "line.10": {"text": "Und in der noch stets klagte das Todtengel\u00e4ut,", "tokens": ["Und", "in", "der", "noch", "stets", "klag\u00b7te", "das", "Tod\u00b7ten\u00b7ge\u00b7l\u00e4ut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADV", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+---+--+--+", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Wie sie sich freuet, wenn nun der rufende J\u00fcngling herausst\u00fcrzt,", "tokens": ["Wie", "sie", "sich", "freu\u00b7et", ",", "wenn", "nun", "der", "ru\u00b7fen\u00b7de", "J\u00fcng\u00b7ling", "her\u00b7aus\u00b7st\u00fcrzt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "VVFIN", "$,", "KOUS", "ADV", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+--+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.12": {"text": "Und die Botschaft selbst, dass er entronnen sey, bringt.", "tokens": ["Und", "die", "Bot\u00b7schaft", "selbst", ",", "dass", "er", "ent\u00b7ron\u00b7nen", "sey", ",", "bringt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "$,", "KOUS", "PPER", "VVPP", "VAFIN", "$,", "VVFIN", "$."], "meter": "--+-++--+--+", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "Wie der tr\u00fcbe, bange, der tiefersch\u00fctterte Zweifler,", "tokens": ["Wie", "der", "tr\u00fc\u00b7be", ",", "ban\u00b7ge", ",", "der", "tie\u00b7fer\u00b7sch\u00fct\u00b7ter\u00b7te", "Zweif\u00b7ler", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "$,", "ADV", "$,", "ART", "ADJA", "NN", "$,"], "meter": "--+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "(lastende Jahre lang trof ihm die Wunde schon fort)", "tokens": ["(", "las\u00b7ten\u00b7de", "Jah\u00b7re", "lang", "trof", "ihm", "die", "Wun\u00b7de", "schon", "fort", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJA", "NN", "ADJD", "VVFIN", "PPER", "ART", "NN", "ADV", "PTKVZ", "$("], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.15": {"text": "Bey noch Einmal ergrifner, itzt festgehaltener Wagschal,", "tokens": ["Bey", "noch", "Ein\u00b7mal", "er\u00b7grif\u00b7ner", ",", "itzt", "fest\u00b7ge\u00b7hal\u00b7te\u00b7ner", "Wag\u00b7schal", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADV", "ADJD", "$,", "ADV", "ADJA", "NN", "$,"], "meter": "--+--+--+-+---+", "measure": "anapaest.tri.plus"}, "line.16": {"text": "Sehend das \u00dcbergewicht, sich der Unsterblichkeit freut!", "tokens": ["Se\u00b7hend", "das", "\u00dc\u00b7ber\u00b7ge\u00b7wicht", ",", "sich", "der", "U\u00b7nsterb\u00b7lich\u00b7keit", "freut", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "$,", "PRF", "ART", "NN", "VVFIN", "$."], "meter": "+--+--+--+-+-+", "measure": "dactylic.tri.plus"}, "line.17": {"text": "Also freut' ich mich, dass ein grosses, m\u00e4chtiges Volk sich", "tokens": ["Al\u00b7so", "freut'", "ich", "mich", ",", "dass", "ein", "gros\u00b7ses", ",", "m\u00e4ch\u00b7ti\u00b7ges", "Volk", "sich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "$,", "KOUS", "ART", "ADJA", "$,", "ADJA", "NN", "PRF"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.18": {"text": "Nie Eroberungskrieg wieder zu kriegen entschloss;", "tokens": ["Nie", "Er\u00b7o\u00b7be\u00b7rungs\u00b7krieg", "wie\u00b7der", "zu", "krie\u00b7gen", "ent\u00b7schloss", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "ADV", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.19": {"text": "Und dass dieser Donner, durch sein Verstummen, den Donnern", "tokens": ["Und", "dass", "die\u00b7ser", "Don\u00b7ner", ",", "durch", "sein", "Ver\u00b7stum\u00b7men", ",", "den", "Don\u00b7nern"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "KOUS", "PDAT", "NN", "$,", "APPR", "PPOSAT", "NN", "$,", "ART", "NN"], "meter": "-+--+--+-+--+-", "measure": "amphibrach.tri.plus"}, "line.20": {"text": "Anderer V\u00f6lker, dereinst auch zu verstummen, gebot.", "tokens": ["An\u00b7de\u00b7rer", "V\u00f6l\u00b7ker", ",", "de\u00b7reinst", "auch", "zu", "ver\u00b7stum\u00b7men", ",", "ge\u00b7bot", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADV", "ADV", "PTKZU", "VVINF", "$,", "VVFIN", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.21": {"text": "Jetzo lag an der Kette das Ungeheuer, der Greuel", "tokens": ["Jet\u00b7zo", "lag", "an", "der", "Ket\u00b7te", "das", "Un\u00b7ge\u00b7heu\u00b7er", ",", "der", "Greu\u00b7el"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$,", "ART", "NN"], "meter": "-+-+-+--+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.22": {"text": "Greuel! itzt war der Mensch \u00fcber sich selber erh\u00f6ht!", "tokens": ["Greu\u00b7el", "!", "itzt", "war", "der", "Mensch", "\u00fc\u00b7ber", "sich", "sel\u00b7ber", "er\u00b7h\u00f6ht", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ADV", "VAFIN", "ART", "NN", "APPR", "PRF", "ADV", "VVPP", "$."], "meter": "---+--+--+--+", "measure": "iambic.tetra.relaxed"}, "line.23": {"text": "Aber, weh uns! sie selbst, die das Unthier z\u00e4hmten, vernichten", "tokens": ["A\u00b7ber", ",", "weh", "uns", "!", "sie", "selbst", ",", "die", "das", "Un\u00b7thier", "z\u00e4hm\u00b7ten", ",", "ver\u00b7nich\u00b7ten"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "$,", "VVFIN", "PPER", "$.", "PPER", "ADV", "$,", "PRELS", "ART", "NN", "VVFIN", "$,", "VVFIN"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.24": {"text": "Ihr hochheilig Gesetz, schlagen Erobererschlacht.", "tokens": ["Ihr", "hoch\u00b7hei\u00b7lig", "Ge\u00b7setz", ",", "schla\u00b7gen", "Er\u00b7o\u00b7be\u00b7rer\u00b7schlacht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "-++--+---+--+", "measure": "iambic.penta.relaxed"}, "line.25": {"text": "Hast du Verw\u00fcnschung, allein wie du nie vernahmst, so verw\u00fcnsche!", "tokens": ["Hast", "du", "Ver\u00b7w\u00fcn\u00b7schung", ",", "al\u00b7lein", "wie", "du", "nie", "ver\u00b7nahmst", ",", "so", "ver\u00b7w\u00fcn\u00b7sche", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "$,", "ADV", "KOKOM", "PPER", "ADV", "VVFIN", "$,", "ADV", "ADJA", "$."], "meter": "-+-+-+-+-+-+--+-", "measure": "iambic.septa.relaxed"}, "line.26": {"text": "Diesem Gesetz glich keins! aber es sey auch kein Fluch", "tokens": ["Die\u00b7sem", "Ge\u00b7setz", "glich", "keins", "!", "a\u00b7ber", "es", "sey", "auch", "kein", "Fluch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PDAT", "NN", "VVFIN", "PIAT", "$.", "KON", "PPER", "VAFIN", "ADV", "PIAT", "NN"], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.27": {"text": "Gleich dem schrecklichen, der die Hochverr\u00e4ther der Menschheit,", "tokens": ["Gleich", "dem", "schreck\u00b7li\u00b7chen", ",", "der", "die", "Hoch\u00b7ver\u00b7r\u00e4\u00b7ther", "der", "Menschheit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "$,", "PRELS", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+--+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.28": {"text": "Welche das hehre Gesetz \u00fcbertraten, verflucht.", "tokens": ["Wel\u00b7che", "das", "heh\u00b7re", "Ge\u00b7setz", "\u00fc\u00b7bert\u00b7ra\u00b7ten", ",", "ver\u00b7flucht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "ART", "ADJA", "NN", "VVFIN", "$,", "VVPP", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.29": {"text": "Sprechet den Fluch mit aus, ihr blutigen Thr\u00e4nen, die jetzo", "tokens": ["Spre\u00b7chet", "den", "Fluch", "mit", "aus", ",", "ihr", "blu\u00b7ti\u00b7gen", "Thr\u00e4\u00b7nen", ",", "die", "jet\u00b7zo"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "ART", "NN", "APPR", "PTKVZ", "$,", "PPOSAT", "ADJA", "NN", "$,", "PRELS", "ADV"], "meter": "+--+-+-+--+-+-+", "measure": "iambic.septa.invert"}, "line.30": {"text": "Weint, wer voraussieht; einst, wen das Gesehene trift.", "tokens": ["Weint", ",", "wer", "vor\u00b7aus\u00b7sieht", ";", "einst", ",", "wen", "das", "Ge\u00b7se\u00b7he\u00b7ne", "trift", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWS", "VVFIN", "$.", "ADV", "$,", "PWS", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-----+", "measure": "unknown.measure.penta"}, "line.31": {"text": "Wir lebt nun die Geliebte nicht mehr: der einzige Sohn nicht!", "tokens": ["Wir", "lebt", "nun", "die", "Ge\u00b7lieb\u00b7te", "nicht", "mehr", ":", "der", "ein\u00b7zi\u00b7ge", "Sohn", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "PTKNEG", "ADV", "$.", "ART", "ADJA", "NN", "PTKNEG", "$."], "meter": "-+-+-+--+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.32": {"text": "Und der Zweifler glaubt mir die Unsterblichkeit nicht!", "tokens": ["Und", "der", "Zweif\u00b7ler", "glaubt", "mir", "die", "U\u00b7nsterb\u00b7lich\u00b7keit", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "ART", "NN", "PTKNEG", "$."], "meter": "--+-+--+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Wie sich der Liebende freut, wenn nun die Geliebte, der hohen", "tokens": ["Wie", "sich", "der", "Lie\u00b7ben\u00b7de", "freut", ",", "wenn", "nun", "die", "Ge\u00b7lieb\u00b7te", ",", "der", "ho\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "PRF", "ART", "NN", "VVFIN", "$,", "KOUS", "ADV", "ART", "NN", "$,", "ART", "ADJA"], "meter": "-+-+--+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Todeswog' entflohn, wieder das Ufer betritt;", "tokens": ["To\u00b7des\u00b7wo\u00b7g'", "ent\u00b7flohn", ",", "wie\u00b7der", "das", "U\u00b7fer", "be\u00b7tritt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Oft schon hatt' er hinunter geschaut an dem Marmor des Strandes,", "tokens": ["Oft", "schon", "hatt'", "er", "hin\u00b7un\u00b7ter", "ge\u00b7schaut", "an", "dem", "Mar\u00b7mor", "des", "Stran\u00b7des", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "ADV", "VVPP", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.4": {"text": "Immer, neuen Gram, Scheiter und Leichen gesehn;", "tokens": ["Im\u00b7mer", ",", "neu\u00b7en", "Gram", ",", "Schei\u00b7ter", "und", "Lei\u00b7chen", "ge\u00b7sehn", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADJA", "NN", "$,", "NN", "KON", "NN", "VVPP", "$."], "meter": "+-+-++--+--+", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "Endlich sinket sie ihm aus einem Nachen, der antreibt,", "tokens": ["End\u00b7lich", "sin\u00b7ket", "sie", "ihm", "aus", "ei\u00b7nem", "Na\u00b7chen", ",", "der", "an\u00b7treibt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "APPR", "ART", "NN", "$,", "PRELS", "VVFIN", "$,"], "meter": "+-+--+-+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.6": {"text": "An das schlagende Herz, siehet den lebenden! lebt!", "tokens": ["An", "das", "schla\u00b7gen\u00b7de", "Herz", ",", "sie\u00b7het", "den", "le\u00b7ben\u00b7den", "!", "lebt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "VVFIN", "ART", "ADJA", "$.", "VVFIN", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.7": {"text": "Oder wie die Mutter, die harrend und stumm an dem Thor lag", "tokens": ["O\u00b7der", "wie", "die", "Mut\u00b7ter", ",", "die", "har\u00b7rend", "und", "stumm", "an", "dem", "Thor", "lag"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "$,", "PRELS", "ADJD", "KON", "ADJD", "APPR", "ART", "NN", "VVFIN"], "meter": "+-+-+--+--+--+-", "measure": "hexameter"}, "line.8": {"text": "Einer durchpesteten Stadt, welche den einzigen Sohn", "tokens": ["Ei\u00b7ner", "durch\u00b7pes\u00b7te\u00b7ten", "Stadt", ",", "wel\u00b7che", "den", "ein\u00b7zi\u00b7gen", "Sohn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ART", "ADJA", "NN"], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.9": {"text": "Mit zahllosen Sterbenden ihr, und Begrabenen einschloss,", "tokens": ["Mit", "zahl\u00b7lo\u00b7sen", "Ster\u00b7ben\u00b7den", "ihr", ",", "und", "Be\u00b7gra\u00b7be\u00b7nen", "ein\u00b7schloss", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "$,", "KON", "NN", "VVFIN", "$,"], "meter": "-+--+--++-+-+-+", "measure": "iambic.septa.relaxed"}, "line.10": {"text": "Und in der noch stets klagte das Todtengel\u00e4ut,", "tokens": ["Und", "in", "der", "noch", "stets", "klag\u00b7te", "das", "Tod\u00b7ten\u00b7ge\u00b7l\u00e4ut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADV", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+---+--+--+", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Wie sie sich freuet, wenn nun der rufende J\u00fcngling herausst\u00fcrzt,", "tokens": ["Wie", "sie", "sich", "freu\u00b7et", ",", "wenn", "nun", "der", "ru\u0