{"textgrid.poem.63310": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "Berliner Ballade", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie hing wie eine Latte", "tokens": ["Sie", "hing", "wie", "ei\u00b7ne", "Lat\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KOKOM", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vom Schranke steif und stumm.", "tokens": ["Vom", "Schran\u00b7ke", "steif", "und", "stumm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Am Morgen sah's ihr Gatte,", "tokens": ["Am", "Mor\u00b7gen", "sah's", "ihr", "Gat\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Lief nach dem Polizeipr\u00e4sidium.", "tokens": ["Lief", "nach", "dem", "Po\u00b7li\u00b7zei\u00b7pr\u00e4\u00b7si\u00b7di\u00b7um", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.2": {"line.1": {"text": "\u00bbmeine Frau\u00ab, so schrie er, \u00bbist verschieden...\u00ab", "tokens": ["\u00bb", "mei\u00b7ne", "Frau", "\u00ab", ",", "so", "schrie", "er", ",", "\u00bb", "ist", "ver\u00b7schie\u00b7den", "...", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "PPOSAT", "NN", "$(", "$,", "ADV", "VVFIN", "PPER", "$,", "$(", "VAFIN", "VVPP", "$(", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Doch der Polizeiwachtmeister Schmidt,", "tokens": ["Doch", "der", "Po\u00b7li\u00b7zei\u00b7wacht\u00b7meis\u00b7ter", "Schmidt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NE", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Rollte blutig seine Augen:", "tokens": ["Roll\u00b7te", "blu\u00b7tig", "sei\u00b7ne", "Au\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "\u00bbwie denn, ha'm Sie den Jeburtsschein mit?\u00ab", "tokens": ["\u00bb", "wie", "denn", ",", "ha'm", "Sie", "den", "Je\u00b7burts\u00b7schein", "mit", "?", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "KOKOM", "ADV", "$,", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$.", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Dieses hatte er mitnichten,", "tokens": ["Die\u00b7ses", "hat\u00b7te", "er", "mit\u00b7nich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und er setzte sich in Trab,", "tokens": ["Und", "er", "setz\u00b7te", "sich", "in", "Trab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PRF", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sp\u00e4t entsann er sich der ehelichen Pflichten, \u2013", "tokens": ["Sp\u00e4t", "ent\u00b7sann", "er", "sich", "der", "e\u00b7he\u00b7li\u00b7chen", "Pflich\u00b7ten", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PRF", "ART", "ADJA", "NN", "$,", "$("], "meter": 