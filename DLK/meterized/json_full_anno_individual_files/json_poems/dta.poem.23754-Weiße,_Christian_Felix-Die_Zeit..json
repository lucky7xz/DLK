{"dta.poem.23754": {"metadata": {"author": {"name": "Wei\u00dfe, Christian Felix", "birth": "N.A.", "death": "N.A."}, "title": "Die Zeit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1767", "urn": "urn:nbn:de:kobv:b4-20643-5", "language": ["de:0.99"], "booktitle": "Wei\u00dfe, Christian Felix: Lieder f\u00fcr Kinder. Leipzig, 1767."}, "poem": {"stanza.1": {"line.1": {"text": "So wie ein Tropfen in dem Bach,", "tokens": ["So", "wie", "ein", "Trop\u00b7fen", "in", "dem", "Bach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Folgt in der Zeit", "tokens": ["Folgt", "in", "der", "Zeit"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Ein Augenblick dem andern nach", "tokens": ["Ein", "Au\u00b7gen\u00b7blick", "dem", "an\u00b7dern", "nach"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "ADJA", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ins Meer der Ewigkeit.", "tokens": ["Ins", "Meer", "der", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der jetzt noch gegenw\u00e4rtig war,", "tokens": ["Der", "jetzt", "noch", "ge\u00b7gen\u00b7w\u00e4r\u00b7tig", "war", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "(schon jetzt nicht mehr!)", "tokens": ["(", "schon", "jetzt", "nicht", "mehr", "!", ")"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "ADV", "PTKNEG", "ADV", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Entflieht f\u00fcr mich auf immerdar", "tokens": ["Ent\u00b7flieht", "f\u00fcr", "mich", "auf", "im\u00b7mer\u00b7dar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PPER", "APPR", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ohn alle Wiederkehr.", "tokens": ["Ohn", "al\u00b7le", "Wie\u00b7der\u00b7kehr", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wie mu\u00df mir jeder Augenblick", "tokens": ["Wie", "mu\u00df", "mir", "je\u00b7der", "Au\u00b7gen\u00b7blick"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "PPER", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Unsch\u00e4tzbar seyn!", "tokens": ["Un\u00b7sch\u00e4tz\u00b7bar", "seyn", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VAINF", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Leg ich ihn ungen\u00fctzt zur\u00fcck,", "tokens": ["Leg", "ich", "ihn", "un\u00b7ge\u00b7n\u00fctzt", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So bring ich nie ihn ein.", "tokens": ["So", "bring", "ich", "nie", "ihn", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}