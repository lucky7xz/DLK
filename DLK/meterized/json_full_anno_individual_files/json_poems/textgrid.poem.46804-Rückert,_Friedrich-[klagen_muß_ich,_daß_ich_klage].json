{"textgrid.poem.46804": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[klagen mu\u00df ich, da\u00df ich klage]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Klagen mu\u00df ich, da\u00df ich klage,", "tokens": ["Kla\u00b7gen", "mu\u00df", "ich", ",", "da\u00df", "ich", "kla\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mich anklagen mu\u00df ich, da\u00df,", "tokens": ["Mich", "an\u00b7kla\u00b7gen", "mu\u00df", "ich", ",", "da\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVINF", "VMFIN", "PPER", "$,", "KOUS", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie ich Leid um zweie trage,", "tokens": ["Wie", "ich", "Leid", "um", "zwei\u00b7e", "tra\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "NN", "APPR", "CARD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ich die \u00fcbrigen verga\u00df.", "tokens": ["Ich", "die", "\u00fcb\u00b7ri\u00b7gen", "ver\u00b7ga\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Als sei hinweg das beste,", "tokens": ["Als", "sei", "hin\u00b7weg", "das", "bes\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "ADV", "ART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Hat keine Freud' am Reste", "tokens": ["Hat", "kei\u00b7ne", "Freud'", "am", "Res\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PIAT", "NN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Das Auge thr\u00e4nenna\u00df.", "tokens": ["Das", "Au\u00b7ge", "thr\u00e4\u00b7nen\u00b7na\u00df", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Alle glaubt' ich lieb zu haben,", "tokens": ["Al\u00b7le", "glaubt'", "ich", "lieb", "zu", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADJD", "PTKZU", "VAINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Alle lieb mit gleichem Trieb;", "tokens": ["Al\u00b7le", "lieb", "mit", "glei\u00b7chem", "Trieb", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber nun, die ich begraben,", "tokens": ["A\u00b7ber", "nun", ",", "die", "ich", "be\u00b7gra\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hab' ich doppelt, dreifach lieb,", "tokens": ["Hab'", "ich", "dop\u00b7pelt", ",", "drei\u00b7fach", "lieb", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "$,", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Lieb, weil ich sie geboren,", "tokens": ["Lieb", ",", "weil", "ich", "sie", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "PPER", "VVPP", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.6": {"text": "Lieb, weil ich sie verloren,", "tokens": ["Lieb", ",", "weil", "ich", "sie", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "PPER", "VVPP", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.7": {"text": "Lieb, weil nur das mir blieb.", "tokens": ["Lieb", ",", "weil", "nur", "das", "mir", "blieb", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "ADV", "ART", "PPER", "VVFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.3": {"line.1": {"text": "Meine Gr\u00f6\u00dfren sind die Sorgen,", "tokens": ["Mei\u00b7ne", "Gr\u00f6\u00df\u00b7ren", "sind", "die", "Sor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Meinen Sorgen bleib' ich treu;", "tokens": ["Mei\u00b7nen", "Sor\u00b7gen", "bleib'", "ich", "treu", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Meine Kleinen, jeden Morgen", "tokens": ["Mei\u00b7ne", "Klei\u00b7nen", ",", "je\u00b7den", "Mor\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Waren sie mein Spielwerk neu.", "tokens": ["Wa\u00b7ren", "sie", "mein", "Spiel\u00b7werk", "neu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPOSAT", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Die Sorgen sind geblieben,", "tokens": ["Die", "Sor\u00b7gen", "sind", "ge\u00b7blie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der Tod nahm nur die lieben", "tokens": ["Der", "Tod", "nahm", "nur", "die", "lie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Spielp\u00fcppchen ohne Scheu.", "tokens": ["Spiel\u00b7p\u00fcpp\u00b7chen", "oh\u00b7ne", "Scheu", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "\u00bbgr\u00f6\u00dfer w\u00fcrden sie geworden", "tokens": ["\u00bb", "gr\u00f6\u00b7\u00dfer", "w\u00fcr\u00b7den", "sie", "ge\u00b7wor\u00b7den"], "token_info": ["punct", "word", "word", "word", "word"], "pos": ["$(", "ADJD", "VAFIN", "PPER", "VAPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und dem Spiel entwachsen sein.\u00ab", "tokens": ["Und", "dem", "Spiel", "ent\u00b7wach\u00b7sen", "sein", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ART", "NN", "VVPP", "VAINF", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "O wie frostig weht von Norden", "tokens": ["O", "wie", "fros\u00b7tig", "weht", "von", "Nor\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "KOKOM", "ADJD", "VVFIN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mir der Trost ins Herz hinein!", "tokens": ["Mir", "der", "Trost", "ins", "Herz", "hin\u00b7ein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "APPRART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ich hofft' es zu erleben,", "tokens": ["Ich", "hofft'", "es", "zu", "er\u00b7le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Da\u00df gro\u00df sie w\u00fcrden eben,", "tokens": ["Da\u00df", "gro\u00df", "sie", "w\u00fcr\u00b7den", "e\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "PPER", "VAFIN", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Nun waren sie noch klein.", "tokens": ["Nun", "wa\u00b7ren", "sie", "noch", "klein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Heut sprang einer von den Jungen", "tokens": ["Heut", "sprang", "ei\u00b7ner", "von", "den", "Jun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Grade so ans Herz mir her,", "tokens": ["Gra\u00b7de", "so", "ans", "Herz", "mir", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPRART", "NN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie mein M\u00e4dchen sonst gesprungen,", "tokens": ["Wie", "mein", "M\u00e4d\u00b7chen", "sonst", "ge\u00b7sprun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ach, und nun nicht springet mehr;", "tokens": ["Ach", ",", "und", "nun", "nicht", "sprin\u00b7get", "mehr", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "KON", "ADV", "PTKNEG", "VVFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ich woll' ihn zu mir heben,", "tokens": ["Ich", "woll'", "ihn", "zu", "mir", "he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die Arme f\u00fchlt' ich beben,", "tokens": ["Die", "Ar\u00b7me", "f\u00fchlt'", "ich", "be\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Er war zu gro\u00df und schwer.", "tokens": ["Er", "war", "zu", "gro\u00df", "und", "schwer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKA", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Klagen mu\u00df ich, da\u00df ich klage,", "tokens": ["Kla\u00b7gen", "mu\u00df", "ich", ",", "da\u00df", "ich", "kla\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mich anklagen mu\u00df ich, da\u00df,", "tokens": ["Mich", "an\u00b7kla\u00b7gen", "mu\u00df", "ich", ",", "da\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVINF", "VMFIN", "PPER", "$,", "KOUS", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie ich Leid um zweie trage,", "tokens": ["Wie", "ich", "Leid", "um", "zwei\u00b7e", "tra\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "NN", "APPR", "CARD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ich die \u00fcbrigen 