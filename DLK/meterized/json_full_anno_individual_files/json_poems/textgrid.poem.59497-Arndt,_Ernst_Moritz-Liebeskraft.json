{"textgrid.poem.59497": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "Liebeskraft", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wem flammet der Begeistrung heil'ges Feuer?", "tokens": ["Wem", "flam\u00b7met", "der", "Be\u00b7geis\u00b7trung", "heil'\u00b7ges", "Feu\u00b7er", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wem pocht des Busens ungest\u00fcmer Drang?", "tokens": ["Wem", "pocht", "des", "Bu\u00b7sens", "un\u00b7ge\u00b7st\u00fc\u00b7mer", "Drang", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wem braust der Strom durch meine goldne Leier", "tokens": ["Wem", "braust", "der", "Strom", "durch", "mei\u00b7ne", "gold\u00b7ne", "Lei\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So freudig wie der Fr\u00fchlingsb\u00e4che Klang?", "tokens": ["So", "freu\u00b7dig", "wie", "der", "Fr\u00fch\u00b7lings\u00b7b\u00e4\u00b7che", "Klang", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "O dir, die an dem weiten Flammenbusen", "tokens": ["O", "dir", ",", "die", "an", "dem", "wei\u00b7ten", "Flam\u00b7men\u00b7bu\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "PPER", "$,", "PRELS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Pulse aller Wesen liebend w\u00e4rmt", "tokens": ["Die", "Pul\u00b7se", "al\u00b7ler", "We\u00b7sen", "lie\u00b7bend", "w\u00e4rmt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PIAT", "NN", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und allgewaltig um den Born der Musen", "tokens": ["Und", "all\u00b7ge\u00b7wal\u00b7tig", "um", "den", "Born", "der", "Mu\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und um des Helden Tatentr\u00e4ume schw\u00e4rmt.", "tokens": ["Und", "um", "des", "Hel\u00b7den", "Ta\u00b7ten\u00b7tr\u00e4u\u00b7me", "schw\u00e4rmt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Dir, Himmlische, schlag' ich die goldnen Saiten,", "tokens": ["Dir", ",", "Himm\u00b7li\u00b7sche", ",", "schlag'", "ich", "die", "gold\u00b7nen", "Sai\u00b7ten", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "$,", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dir rieselt meines Liedes Silberton,", "tokens": ["Dir", "rie\u00b7selt", "mei\u00b7nes", "Lie\u00b7des", "Sil\u00b7ber\u00b7ton", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Urania; schon in den Bl\u00fctenzeiten", "tokens": ["U\u00b7ra\u00b7nia", ";", "schon", "in", "den", "Bl\u00fc\u00b7ten\u00b7zei\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$.", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Der Vorwelt funkelte dein Feuerthron;", "tokens": ["Der", "Vor\u00b7welt", "fun\u00b7kel\u00b7te", "dein", "Feu\u00b7er\u00b7thron", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Du schlugest in das Chaos Lebensfunken,", "tokens": ["Du", "schlu\u00b7gest", "in", "das", "Chaos", "Le\u00b7bens\u00b7fun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+---++-+-", "measure": "dactylic.init"}, "line.2": {"text": "Und Welten sprangen jubelnd aus dem Nichts,", "tokens": ["Und", "Wel\u00b7ten", "spran\u00b7gen", "ju\u00b7belnd", "aus", "dem", "Nichts", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Es kreisten Monde, Sonnen flogen, trunken", "tokens": ["Es", "kreis\u00b7ten", "Mon\u00b7de", ",", "Son\u00b7nen", "flo\u00b7gen", ",", "trun\u00b7ken"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "NE", "$,", "NN", "VVFIN", "$,", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Des neuen Seins, die goldne Bahn des Lichts;", "tokens": ["Des", "neu\u00b7en", "Seins", ",", "die", "gold\u00b7ne", "Bahn", "des", "Lichts", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Von deinem Odem sprudelte die Quelle,", "tokens": ["Von", "dei\u00b7nem", "O\u00b7dem", "spru\u00b7del\u00b7te", "die", "Quel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Blume \u00f6ffnete den duft'gen Scho\u00df,", "tokens": ["Die", "Blu\u00b7me", "\u00f6ff\u00b7ne\u00b7te", "den", "duft'\u00b7gen", "Scho\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Fisch durchschl\u00fcpfte seine Silberwelle", "tokens": ["Der", "Fisch", "durch\u00b7schl\u00fcpf\u00b7te", "sei\u00b7ne", "Sil\u00b7ber\u00b7wel\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und W\u00fcrmchen liebten auf dem Erdenklo\u00df.", "tokens": ["Und", "W\u00fcrm\u00b7chen", "lieb\u00b7ten", "auf", "dem", "Er\u00b7den\u00b7klo\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Wes ist das Land, das Menschen aneinander", "tokens": ["Wes", "ist", "das", "Land", ",", "das", "Men\u00b7schen", "an\u00b7ein\u00b7an\u00b7der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VAFIN", "ART", "NN", "$,", "ART", "NN", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit losen Schlingen unaufl\u00f6slich schn\u00fcrt", "tokens": ["Mit", "lo\u00b7sen", "Schlin\u00b7gen", "un\u00b7auf\u00b7l\u00f6s\u00b7lich", "schn\u00fcrt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und freundlich seinen schl\u00e4ngelnden M\u00e4ander", "tokens": ["Und", "freund\u00b7lich", "sei\u00b7nen", "schl\u00e4n\u00b7geln\u00b7den", "M\u00e4\u00b7an\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Des Lebens leichten Schatten niederf\u00fchrt?", "tokens": ["Des", "Le\u00b7bens", "leich\u00b7ten", "Schat\u00b7ten", "nie\u00b7der\u00b7f\u00fchrt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Von wessen Odem weht die heil'ge Flamme,", "tokens": ["Von", "wes\u00b7sen", "O\u00b7dem", "weht", "die", "heil'\u00b7ge", "Flam\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Purpur auf des M\u00e4dchens Wange haucht", "tokens": ["Die", "Pur\u00b7pur", "auf", "des", "M\u00e4d\u00b7chens", "Wan\u00b7ge", "haucht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und, aufzudringen zu dem G\u00f6tterstamme,", "tokens": ["Und", ",", "auf\u00b7zu\u00b7drin\u00b7gen", "zu", "dem", "G\u00f6t\u00b7ter\u00b7stam\u00b7me", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Im Styx der Kraft den J\u00fcngling untertaucht?", "tokens": ["Im", "Styx", "der", "Kraft", "den", "J\u00fcng\u00b7ling", "un\u00b7ter\u00b7taucht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Was f\u00e4rbt dem Morgenrot die Rosenwangen?", "tokens": ["Was", "f\u00e4rbt", "dem", "Mor\u00b7gen\u00b7rot", "die", "Ro\u00b7sen\u00b7wan\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Was lehrt die Erden und die Sonne fliehn?", "tokens": ["Was", "lehrt", "die", "Er\u00b7den", "und", "die", "Son\u00b7ne", "fliehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "KON", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Bl\u00fctenzweige fl\u00fcsternd sich umfangen?", "tokens": ["Die", "Bl\u00fc\u00b7ten\u00b7zwei\u00b7ge", "fl\u00fcs\u00b7ternd", "sich", "um\u00b7fan\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Blumen an der Blumen Lippen gl\u00fchn?", "tokens": ["Die", "Blu\u00b7men", "an", "der", "Blu\u00b7men", "Lip\u00b7pen", "gl\u00fchn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "O la\u00df auch mich an deinen Busen fallen! \u2013", "tokens": ["O", "la\u00df", "auch", "mich", "an", "dei\u00b7nen", "Bu\u00b7sen", "fal\u00b7len", "!", "\u2013"], "token_in