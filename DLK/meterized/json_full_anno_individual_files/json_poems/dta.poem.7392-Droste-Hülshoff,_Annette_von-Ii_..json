{"dta.poem.7392": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Ii .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-20090519994", "language": ["de:0.99"], "booktitle": "Droste-H\u00fclshoff, Annette von: Gedichte. Stuttgart u. a., 1844."}, "poem": {"stanza.1": {"line.1": {"text": "Das nenn' ich eine Winternacht! das eine Jahresleiche!", "tokens": ["Das", "nenn'", "ich", "ei\u00b7ne", "Win\u00b7ter\u00b7nacht", "!", "das", "ei\u00b7ne", "Jah\u00b7res\u00b7lei\u00b7che", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ART", "NN", "$.", "ART", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Gnade", "tokens": ["Gna\u00b7de"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Der Himmel Jedem den die Noth treibt \u00fcber diese blanken", "tokens": ["Der", "Him\u00b7mel", "Je\u00b7dem", "den", "die", "Noth", "treibt", "\u00fc\u00b7ber", "die\u00b7se", "blan\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PIAT", "ART", "ART", "NN", "VVFIN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.4": {"text": "Pfade!", "tokens": ["Pfa\u00b7de", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Sie glitzern auf, der Schlange gleich im wei\u00dfen Pyramiden-", "tokens": ["Sie", "glit\u00b7zern", "auf", ",", "der", "Schlan\u00b7ge", "gleich", "im", "wei\u00b7\u00dfen", "Py\u00b7ra\u00b7mi\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "ART", "NN", "ADV", "APPRART", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.6": {"text": "sande,", "tokens": ["san\u00b7de", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.7": {"text": "Und dr\u00fcber h\u00e4ngt, ein Todtenlicht, der Mond an unsicht-", "tokens": ["Und", "dr\u00fc\u00b7ber", "h\u00e4ngt", ",", "ein", "Tod\u00b7ten\u00b7licht", ",", "der", "Mond", "an", "un\u00b7sicht"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PAV", "VVFIN", "$,", "ART", "NN", "$,", "ART", "NN", "APPR", "TRUNC"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "barem Bande,", "tokens": ["ba\u00b7rem", "Ban\u00b7de", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "--+-", "measure": "anapaest.init"}, "line.9": {"text": "Mit F\u00fcnkchen ist die Luft gef\u00fcllt,", "tokens": ["Mit", "F\u00fcnk\u00b7chen", "ist", "die", "Luft", "ge\u00b7f\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Die Sterbeseufzer zieht und quillt.", "tokens": ["Die", "Ster\u00b7be\u00b7seuf\u00b7zer", "zieht", "und", "quillt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Nie hat, seit Menschendenken, sich Sylvesternacht so scharf", "tokens": ["Nie", "hat", ",", "seit", "Men\u00b7schen\u00b7den\u00b7ken", ",", "sich", "Syl\u00b7ves\u00b7ter\u00b7nacht", "so", "scharf"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "$,", "APPR", "NN", "$,", "PRF", "NN", "ADV", "ADJD"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "ergossen,", "tokens": ["er\u00b7gos\u00b7sen", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Der Tag hat Flocken ausgestreut, der Abend sie mit Glas", "tokens": ["Der", "Tag", "hat", "Flo\u00b7cken", "aus\u00b7ge\u00b7streut", ",", "der", "A\u00b7bend", "sie", "mit", "Glas"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "NN", "VVPP", "$,", "ART", "NN", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "umschlossen;", "tokens": ["um\u00b7schlos\u00b7sen", ";"], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "In den Geh\u00f6ften Taub' und Huhn auf ihrer Stange \u00e4chzend", "tokens": ["In", "den", "Ge\u00b7h\u00f6f\u00b7ten", "Taub'", "und", "Huhn", "auf", "ih\u00b7rer", "Stan\u00b7ge", "\u00e4ch\u00b7zend"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "KON", "NN", "APPR", "PPOSAT", "NN", "VVPP"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.6": {"text": "ducken,", "tokens": ["du\u00b7cken", ","], "token_info": ["word", "punct"], "pos": ["VVINF", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.7": {"text": "Der Hund in seinem Schober heult und f\u00fchlt den Wurm", "tokens": ["Der", "Hund", "in", "sei\u00b7nem", "Scho\u00b7ber", "heult", "und", "f\u00fchlt", "den", "Wurm"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "KON", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "im Hirne zucken;", "tokens": ["im", "Hir\u00b7ne", "zu\u00b7cken", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "Zwei Spannen hat in dieser Nacht", "tokens": ["Zwei", "Span\u00b7nen", "hat", "in", "die\u00b7ser", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["CARD", "NN", "VAFIN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Das Eis dem Strome zugebracht.", "tokens": ["Das", "Eis", "dem", "Stro\u00b7me", "zu\u00b7ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Verklommen steht am Thor die Wach' und haucht in die er-", "tokens": ["Ver\u00b7klom\u00b7men", "steht", "am", "Thor", "die", "Wach'", "und", "haucht", "in", "die", "er"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPRART", "NN", "ART", "NN", "KON", "VVFIN", "APPR", "ART", "TRUNC"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "starrten H\u00e4nde,", "tokens": ["starr\u00b7ten", "H\u00e4n\u00b7de", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "\u201ewer da!\u201c \u201eein Freund!\u201c und hastig stampft es l\u00e4ngs der", "tokens": ["\u201e", "wer", "da", "!", "\u201c", "\u201e", "ein", "Freund", "!", "\u201c", "und", "has\u00b7tig", "stampft", "es", "l\u00e4ngs", "der"], "token_info": ["punct", "word", "word", "punct", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "PWS", "ADV", "$.", "$(", "$(", "ART", "NN", "$.", "$(", "KON", "ADJD", "VVFIN", "PPER", "APPR", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Br\u00fccke Steingel\u00e4nde;", "tokens": ["Br\u00fc\u00b7cke", "Stein\u00b7ge\u00b7l\u00e4n\u00b7de", ";"], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Betroffen sieht ihn der Rekrut wie einen Mast am Strome", "tokens": ["Be\u00b7trof\u00b7fen", "sieht", "ihn", "der", "Re\u00b7krut", "wie", "ei\u00b7nen", "Mast", "am", "Stro\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "ART", "NN", "KOKOM", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.6": {"text": "schwanken:", "tokens": ["schwan\u00b7ken", ":"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.7": {"text": "\u201eder ist betrunken oder irr!\u201c er steht ein Weilchen in Ge-", "tokens": ["\u201e", "der", "ist", "be\u00b7trun\u00b7ken", "o\u00b7der", "irr", "!", "\u201c", "er", "steht", "ein", "Weil\u00b7chen", "in", "Ge"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "PDS", "VAFIN", "ADJD", "KON", "NN", "$.", "$(", "PPER", "VVFIN", "ART", "NN", "APPR", "TRUNC"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.8": {"text": "danken,", "tokens": ["dan\u00b7ken", ","], "token_info": ["word", "punct"], "pos": ["VVINF", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.9": {"text": "Bekreuzt sich, zieht die Uhr heraus,", "tokens": ["Be\u00b7kreuzt", "sich", ",", "zieht", "die", "Uhr", "he\u00b7raus", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "$,", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Und lehnt sich an sein Schilderhaus.", "tokens": ["Und", "lehnt", "sich", "an", "sein", "Schil\u00b7der\u00b7haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "In's offne Land der T\u00e4uscher tritt, er athmet auf und schaut", "tokens": ["In's", "off\u00b7ne", "Land", "der", "T\u00e4u\u00b7scher", "tritt", ",", "er", "ath\u00b7met", "auf", "und", "schaut"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "VVFIN", "$,", "PPER", "VVFIN", "PTKVZ", "KON", "VVFIN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "nach oben;", "tokens": ["nach", "o\u00b7ben", ";"], "token_info": ["word", "word", "punct"], "pos": ["APPR", "ADV", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Kein W\u00f6lkchen h\u00e4ngt am Riesenbau der dunklen Saphir-", "tokens": ["Kein", "W\u00f6lk\u00b7chen", "h\u00e4ngt", "am", "Rie\u00b7sen\u00b7bau", "der", "dunk\u00b7len", "Sa\u00b7phir"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "APPRART", "NN", "ART", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "kuppel droben,", "tokens": ["kup\u00b7pel", "dro\u00b7ben", ","], "token_info": ["word", "word", "punct"], "pos": ["VVIMP", "ADV", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Er wendet sich, und sieht die Stadt wie eine Nebelmasse", "tokens": ["Er", "wen\u00b7det", "sich", ",", "und", "sieht", "die", "Stadt", "wie", "ei\u00b7ne", "Ne\u00b7bel\u00b7mas\u00b7se"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "$,", "KON", "VVFIN", "ART", "NN", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.6": {"text": "liegen,", "tokens": ["lie\u00b7gen", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.7": {"text": "Und dr\u00fcber, auf Sankt Thomas Thurm, das Wetterkreuz", "tokens": ["Und", "dr\u00fc\u00b7ber", ",", "auf", "Sankt", "Tho\u00b7mas", "Thurm", ",", "das", "Wet\u00b7ter\u00b7kreuz"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PAV", "$,", "APPR", "VVFIN", "NE", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "sich schimmernd wiegen,", "tokens": ["sich", "schim\u00b7mernd", "wie\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "Den Mantel zieht er an's Gesicht", "tokens": ["Den", "Man\u00b7tel", "zieht", "er", "an's", "Ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Und schreitet fort im Mondenlicht.", "tokens": ["Und", "schrei\u00b7tet", "fort", "im", "Mon\u00b7den\u00b7licht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Was liegt dort \u00fcber'm Weg? \u2014 ein Mensch, ein Mann in", "tokens": ["Was", "liegt", "dort", "\u00fc\u00b7ber'm", "Weg", "?", "ein", "Mensch", ",", "ein", "Mann", "in"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ADV", "APPRART", "NN", "$.", "$(", "ART", "NN", "$,", "ART", "NN", "APPR"], "meter": "-+-+-++-+-", "measure": "unknown.measure.penta"}, "line.2": {"text": "d\u00fcnnem Zwillichrocke, \u2014", "tokens": ["d\u00fcn\u00b7nem", "Zwil\u00b7li\u00b7chro\u00b7cke", ","], "token_info": ["word", "word", "punct", "punct"], "pos": ["ADJA", "NN", "$,", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Der T\u00e4uscher zuckt, doch zaudert nicht; wohl sieht des", "tokens": ["Der", "T\u00e4u\u00b7scher", "zuckt", ",", "doch", "zau\u00b7dert", "nicht", ";", "wohl", "sieht", "des"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "ADV", "VVFIN", "PTKNEG", "$.", "ADV", "VVFIN", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Greisen d\u00fcnne Locke,", "tokens": ["Grei\u00b7sen", "d\u00fcn\u00b7ne", "Lo\u00b7cke", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Die Glatze, leuchtend aus dem Schnee, er sieht sie im Vor-", "tokens": ["Die", "Glat\u00b7ze", ",", "leuch\u00b7tend", "aus", "dem", "Schnee", ",", "er", "sieht", "sie", "im", "Vor"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "VVPP", "APPR", "ART", "NN", "$,", "PPER", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.6": {"text": "\u00fcberschreiten,", "tokens": ["\u00fc\u00b7bersc\u00b7hrei\u00b7ten", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Und wie mit tausend Stricken zieht es nieder, nieder ihn,", "tokens": ["Und", "wie", "mit", "tau\u00b7send", "Stri\u00b7cken", "zieht", "es", "nie\u00b7der", ",", "nie\u00b7der", "ihn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PWAV", "APPR", "CARD", "NN", "VVFIN", "PPER", "PTKVZ", "$,", "PTKVZ", "PPER", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.8": {"text": "zur Seiten;", "tokens": ["zur", "Sei\u00b7ten", ";"], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.9": {"text": "An's Herz hat er die Faust geballt,", "tokens": ["An's", "Herz", "hat", "er", "die", "Faust", "ge\u00b7ballt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Und weiter, weiter sonder Halt!", "tokens": ["Und", "wei\u00b7ter", ",", "wei\u00b7ter", "son\u00b7der", "Halt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Die Scholle unterm Fu\u00dfe kracht, und scheint ihn wimmernd", "tokens": ["Die", "Schol\u00b7le", "un\u00b7term", "Fu\u00b7\u00dfe", "kracht", ",", "und", "scheint", "ihn", "wim\u00b7mernd"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "$,", "KON", "VVFIN", "PPER", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "anzuklagen,", "tokens": ["an\u00b7zu\u00b7kla\u00b7gen", ","], "token_info": ["word", "punct"], "pos": ["VVIZU", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Die Luft mit ihrem leisern Hauch ihm Sterber\u00f6cheln zuzu-", "tokens": ["Die", "Luft", "mit", "ih\u00b7rem", "lei\u00b7sern", "Hauch", "ihm", "Ster\u00b7be\u00b7r\u00f6\u00b7cheln", "zu\u00b7zu"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPOSAT", "ADJA", "NN", "PPER", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+--+", "me