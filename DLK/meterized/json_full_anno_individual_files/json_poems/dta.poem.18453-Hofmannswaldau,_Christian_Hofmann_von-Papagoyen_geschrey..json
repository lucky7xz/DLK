{"dta.poem.18453": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Papagoyen geschrey.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1703", "urn": "urn:nbn:de:kobv:b4-200905199360", "language": ["de:0.71", "en:0.28"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Papagoy rief aus was ihn Corinna lehrte/", "tokens": ["Ein", "Pa\u00b7pa\u00b7goy", "rief", "aus", "was", "ihn", "Co\u00b7rin\u00b7na", "lehr\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PRELS", "PPER", "NE", "VVFIN", "$("], "meter": "-+-+-+--++-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Al\u00df nun ein artzt da ging/ und hahnrey rufen h\u00f6rte/", "tokens": ["Al\u00df", "nun", "ein", "artzt", "da", "ging", "/", "und", "hahn\u00b7rey", "ru\u00b7fen", "h\u00f6r\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJD", "ADV", "VVFIN", "$(", "KON", "NN", "VVINF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sprach er: dem Papagoy mus etwas seyn geschehn/", "tokens": ["Sprach", "er", ":", "dem", "Pa\u00b7pa\u00b7goy", "mus", "et\u00b7was", "seyn", "ge\u00b7schehn", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$.", "ART", "NN", "VMFIN", "PIS", "VAINF", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df er vor seinen herrn mich jetzt hat angesehn.", "tokens": ["Da\u00df", "er", "vor", "sei\u00b7nen", "herrn", "mich", "jetzt", "hat", "an\u00b7ge\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "PPER", "ADV", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}