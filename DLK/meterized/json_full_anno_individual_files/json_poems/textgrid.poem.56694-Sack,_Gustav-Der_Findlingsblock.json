{"textgrid.poem.56694": {"metadata": {"author": {"name": "Sack, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Der Findlingsblock", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In weiter Heide auf den H\u00fcgelwellen", "tokens": ["In", "wei\u00b7ter", "Hei\u00b7de", "auf", "den", "H\u00fc\u00b7gel\u00b7wel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "NE", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "vom Meer der Vorzeit d\u00fcnend aufgeschlagen,", "tokens": ["vom", "Meer", "der", "Vor\u00b7zeit", "d\u00fc\u00b7nend", "auf\u00b7ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "liegt einer der granitenen Gesellen,", "tokens": ["liegt", "ei\u00b7ner", "der", "gra\u00b7ni\u00b7te\u00b7nen", "Ge\u00b7sel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "die einst der Gletscher Strom ins Land getragen.", "tokens": ["die", "einst", "der", "Glet\u00b7scher", "Strom", "ins", "Land", "ge\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "ADJA", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und ob der Himmel ihn mit Schlo\u00dfen schl\u00e4gt,", "tokens": ["Und", "ob", "der", "Him\u00b7mel", "ihn", "mit", "Schlo\u00b7\u00dfen", "schl\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "