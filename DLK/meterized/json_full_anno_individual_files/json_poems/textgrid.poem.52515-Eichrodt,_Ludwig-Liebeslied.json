{"textgrid.poem.52515": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Liebeslied", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es ist so gut und leicht gesagt,", "tokens": ["Es", "ist", "so", "gut", "und", "leicht", "ge\u00b7sagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich liebe, liebe dich,", "tokens": ["Ich", "lie\u00b7be", ",", "lie\u00b7be", "dich", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Man hat so schnell sich eingeliebt,", "tokens": ["Man", "hat", "so", "schnell", "sich", "ein\u00b7ge\u00b7liebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ADV", "ADJD", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So ganz herzinniglich.", "tokens": ["So", "ganz", "her\u00b7zin\u00b7nig\u00b7lich", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Man f\u00e4llt sich um den Hals und k\u00fc\u00dft,", "tokens": ["Man", "f\u00e4llt", "sich", "um", "den", "Hals", "und", "k\u00fc\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PRF", "APPR", "ART", "NN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Bis man vor Liebe trunken ist;", "tokens": ["Bis", "man", "vor", "Lie\u00b7be", "trun\u00b7ken", "ist", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "APPR", "NN", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und kann sein Gl\u00fcck nicht fassen,", "tokens": ["Und", "kann", "sein", "Gl\u00fcck", "nicht", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPOSAT", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und will sein Gl\u00fcck nicht lassen.", "tokens": ["Und", "will", "sein", "Gl\u00fcck", "nicht", "las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPOSAT", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und wenn man einmal Abschied nimmt,", "tokens": ["Und", "wenn", "man", "ein\u00b7mal", "Ab\u00b7schied", "nimmt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "ADV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ist man zum Tod betr\u00fcbt;", "tokens": ["Ist", "man", "zum", "Tod", "be\u00b7tr\u00fcbt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da f\u00fchlt man erst, da wei\u00df man erst,", "tokens": ["Da", "f\u00fchlt", "man", "erst", ",", "da", "wei\u00df", "man", "erst", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "$,", "ADV", "VVFIN", "PIS", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie sehr man sich geliebt.", "tokens": ["Wie", "sehr", "man", "sich", "ge\u00b7liebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PIS", "PRF", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Man k\u00fc\u00dft sich fort und bleibt allein,", "tokens": ["Man", "k\u00fc\u00dft", "sich", "fort", "und", "bleibt", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PRF", "PTKVZ", "KON", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Man weint sich aus und schickt sich drein,", "tokens": ["Man", "weint", "sich", "aus", "und", "schickt", "sich", "drein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PRF", "PTKVZ", "KON", "VVFIN", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und tr\u00e4umet unterdessen,", "tokens": ["Und", "tr\u00e4u\u00b7met", "un\u00b7ter\u00b7des\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und kann sich nicht vergessen.", "tokens": ["Und", "kann", "sich", "nicht", "ver\u00b7ges\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PRF", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und s\u00fc\u00df ist auch, wenn aus der Fern", "tokens": ["Und", "s\u00fc\u00df", "ist", "auch", ",", "wenn", "aus", "der", "Fern"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VAFIN", "ADV", "$,", "KOUS", "APPR", "ART", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Die Gr\u00fc\u00dfe kommen, gehn \u2013", "tokens": ["Die", "Gr\u00fc\u00b7\u00dfe", "kom\u00b7men", ",", "gehn", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "V