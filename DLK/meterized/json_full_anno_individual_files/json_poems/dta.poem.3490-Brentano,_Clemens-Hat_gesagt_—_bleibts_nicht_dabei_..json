{"dta.poem.3490": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Hat gesagt  \u2014  bleibts nicht dabei .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519172", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Vater hat gesagt,               ", "tokens": ["Mein", "Va\u00b7ter", "hat", "ge\u00b7sagt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Ich soll das Kindlein wiegen,", "tokens": ["Ich", "soll", "das", "Kin\u00b7dlein", "wie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er will mir auf den Abend", "tokens": ["Er", "will", "mir", "auf", "den", "A\u00b7bend"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Drey Gaggeleyer sieden;", "tokens": ["Drey", "Gag\u00b7ge\u00b7ley\u00b7er", "sie\u00b7den", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Siedt er mir drei,", "tokens": ["Siedt", "er", "mir", "drei", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "PPER", "CARD", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "I\u00dft er mir zwei,", "tokens": ["I\u00dft", "er", "mir", "zwei", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "CARD", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Und ich mag nicht wiegen,", "tokens": ["Und", "ich", "mag", "nicht", "wie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PTKNEG", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Um ein einziges Ey.", "tokens": ["Um", "ein", "ein\u00b7zi\u00b7ges", "Ey", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Mein Mutter hat gesagt,", "tokens": ["Mein", "Mut\u00b7ter", "hat", "ge\u00b7sagt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Ich soll die M\u00e4gdlein verrathen,", "tokens": ["Ich", "soll", "die", "M\u00e4gd\u00b7lein", "ver\u00b7ra\u00b7then", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Sie wollt mir auf den Abend", "tokens": ["Sie", "wollt", "mir", "auf", "den", "A\u00b7bend"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Drei V\u00f6gelein braten;", "tokens": ["Drei", "V\u00f6\u00b7ge\u00b7lein", "bra\u00b7ten", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Br\u00e4t sie mir drei,", "tokens": ["Br\u00e4t", "sie", "mir", "drei", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "CARD", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "I\u00dft sie mir zwei,", "tokens": ["I\u00dft", "sie", "mir", "zwei", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "CARD", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Um ein einziges V\u00f6glein", "tokens": ["Um", "ein", "ein\u00b7zi\u00b7ges", "V\u00f6\u00b7glein"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUI", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Treib ich kein Verr\u00e4therei.", "tokens": ["Treib", "ich", "kein", "Ver\u00b7r\u00e4\u00b7the\u00b7rei", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Mein Sch\u00e4tzlein hat gesagt,", "tokens": ["Mein", "Sch\u00e4tz\u00b7lein", "hat", "ge\u00b7sagt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Ich soll sein gedenken,", "tokens": ["Ich", "soll", "sein", "ge\u00b7den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Er w\u00f6llt mir auf den Abend", "tokens": ["Er", "w\u00f6llt", "mir", "auf", "den", "A\u00b7bend"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Drei K\u00fc\u00dflein auch schenken;", "tokens": ["Drei", "K\u00fc\u00df\u00b7lein", "auch", "schen\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "ADV", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Schenkt er mir drei,", "tokens": ["Schenkt", "er", "mir", "drei", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "CARD", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Bleibts nicht dabei,", "tokens": ["Bleibts", "nicht", "da\u00b7bei", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "PAV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Was k\u00fcmmert michs V\u00f6glein,", "tokens": ["Was", "k\u00fcm\u00b7mert", "michs", "V\u00f6\u00b7glein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Was schiert mich das Ey.", "tokens": ["Was", "schiert", "mich", "das", "Ey", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}