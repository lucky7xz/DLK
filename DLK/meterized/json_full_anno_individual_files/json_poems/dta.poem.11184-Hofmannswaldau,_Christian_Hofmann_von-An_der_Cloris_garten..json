{"dta.poem.11184": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "An der Cloris garten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dein garten ist galant; Doch hat mir unter allen,", "tokens": ["Dein", "gar\u00b7ten", "ist", "ga\u00b7lant", ";", "Doch", "hat", "mir", "un\u00b7ter", "al\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "VAFIN", "ADJD", "$.", "KON", "VAFIN", "PPER", "APPR", "PIS", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was seine sch\u00f6nheit giebt, nur di\u00df allein gefallen,", "tokens": ["Was", "sei\u00b7ne", "sch\u00f6n\u00b7heit", "giebt", ",", "nur", "di\u00df", "al\u00b7lein", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "VVFIN", "$,", "ADV", "PDS", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da\u00df man die \u00fcberschrifft an jeder blume liest:", "tokens": ["Da\u00df", "man", "die", "\u00fc\u00b7bersc\u00b7hrifft", "an", "je\u00b7der", "blu\u00b7me", "liest", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "VVFIN", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wir sind noch lange nicht so sch\u00f6n, als Cloris ist.", "tokens": ["Wir", "sind", "noch", "lan\u00b7ge", "nicht", "so", "sch\u00f6n", ",", "als", "Clo\u00b7ris", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "PTKNEG", "ADV", "ADJD", "$,", "KOUS", "NE", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}