{"textgrid.poem.24846": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Glaub nicht, da\u00df ich dem lauten Tage", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Glaub nicht, da\u00df ich dem lauten Tage", "tokens": ["Glaub", "nicht", ",", "da\u00df", "ich", "dem", "lau\u00b7ten", "Ta\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "PTKNEG", "$,", "KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Verrate, was du mir vertraust,", "tokens": ["Ver\u00b7ra\u00b7te", ",", "was", "du", "mir", "ver\u00b7traust", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn mir vorbei mit fl\u00fccht'gem Schritte", "tokens": ["Wenn", "mir", "vor\u00b7bei", "mit", "fl\u00fccht'\u00b7gem", "Schrit\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Du wandelst in der Deinen Mitte", "tokens": ["Du", "wan\u00b7delst", "in", "der", "Dei\u00b7nen", "Mit\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und mit dem Blick, halb k\u00fchn, halb zage,", "tokens": ["Und", "mit", "dem", "Blick", ",", "halb", "k\u00fchn", ",", "halb", "za\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "ADJD", "ADJD", "$,", "ADJD", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Verhei\u00dfend mir ins Antlitz schaust.", "tokens": ["Ver\u00b7hei\u00b7\u00dfend", "mir", "ins", "Ant\u00b7litz", "schaust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Berauscht vom Zauber deiner N\u00e4he", "tokens": ["Be\u00b7rauscht", "vom", "Zau\u00b7ber", "dei\u00b7ner", "N\u00e4\u00b7he"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPRART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dann seh' ich lang dir staunend nach,", "tokens": ["Dann", "seh'", "ich", "lang", "dir", "stau\u00b7nend", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und m\u00e4hlich erst, indem ich sinne,", "tokens": ["Und", "m\u00e4h\u00b7lich", "erst", ",", "in\u00b7dem", "ich", "sin\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Werd' ich des eignen Gl\u00fcckes inne,", "tokens": ["Werd'", "ich", "des", "eig\u00b7nen", "Gl\u00fc\u00b7ckes", "in\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wenn ich die Rede ganz verstehe,", "tokens": ["Wenn", "ich", "die", "Re\u00b7de", "ganz", "ver\u00b7ste\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die stumme, die dein Auge sprach.", "tokens": ["Die", "stum\u00b7me", ",", "die", "dein", "Au\u00b7ge", "sprach", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Die Abendschatten werden tr\u00fcber,", "tokens": ["Die", "A\u00b7bend\u00b7schat\u00b7ten", "wer\u00b7den", "tr\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "L\u00e4ngst in die Ferne schwandest du;", "tokens": ["L\u00e4ngst", "in", "die", "Fer\u00b7ne", "schwan\u00b7dest", "du", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und, wie den Tropfen Tau die Blume", "tokens": ["Und", ",", "wie", "den", "Trop\u00b7fen", "Tau", "die", "Blu\u00b7me"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "$,", "PWAV", "ART", "NN", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Birgt in des Kelches Heiligtume,", "tokens": ["Birgt", "in", "des", "Kel\u00b7ches", "Hei\u00b7lig\u00b7tu\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Schlie\u00dft meine Seele still sich \u00fcber", "tokens": ["Schlie\u00dft", "mei\u00b7ne", "See\u00b7le", "still", "sich", "\u00fc\u00b7ber"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "ADJD", "PRF", "APPR"], "meter": "-+