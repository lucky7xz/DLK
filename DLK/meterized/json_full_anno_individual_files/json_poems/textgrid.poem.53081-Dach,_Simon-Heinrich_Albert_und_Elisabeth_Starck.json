"Du", "bist", "hier", "vnd", "bleibst", "auch", "schon", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "KON", "VVFIN", "ADV", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Vnd verschaffe, das dein Thon", "tokens": ["Vnd", "ver\u00b7schaf\u00b7fe", ",", "das", "dein", "Thon"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "PRELS", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mag beseelen vnsre Lieder,", "tokens": ["Mag", "be\u00b7see\u00b7len", "vns\u00b7re", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Die ohn dich, O Phebus Kind,", "tokens": ["Die", "ohn", "dich", ",", "O", "Phe\u00b7bus", "Kind", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "$,", "NE", "NE", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Warlich sonder Seele sind.", "tokens": ["War\u00b7lich", "son\u00b7der", "See\u00b7le", "sind", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.18": {"line.1": {"text": "Ach mit was f\u00fcr sch\u00f6nen Dingen,", "tokens": ["Ach", "mit", "was", "f\u00fcr", "sch\u00f6\u00b7nen", "Din\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PRELS", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was f\u00fcr Lust vnd Fr\u00f6ligkeit", "tokens": ["Was", "f\u00fcr", "Lust", "vnd", "Fr\u00f6\u00b7lig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hoffen wir die LiebeZeit", "tokens": ["Hof\u00b7fen", "wir", "die", "Lie\u00b7be", "Zeit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "(da es Gott wil) zuzubringen,", "tokens": ["(", "da", "es", "Gott", "wil", ")", "zu\u00b7zu\u00b7brin\u00b7gen", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "NN", "VMFIN", "$(", "VVIZU", "$,"], "meter": "+-++--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Wann vorau\u00df der Frost erliegt", "tokens": ["Wann", "vor\u00b7au\u00df", "der", "Frost", "er\u00b7liegt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ART", "NN", "VVFIN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Vnd der Lentz die Herrschafft kriegt,", "tokens": ["Vnd", "der", "Lentz", "die", "Herr\u00b7schafft", "kriegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.19": {"line.1": {"text": "Wann wir auff begr\u00fcnter Heyden", "tokens": ["Wann", "wir", "auff", "be\u00b7gr\u00fcn\u00b7ter", "Hey\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hingestreckt ins feuchte Gra\u00df", "tokens": ["Hin\u00b7ge\u00b7streckt", "ins", "feuch\u00b7te", "Gra\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bey den B\u00e4chen, die wie Gla\u00df", "tokens": ["Bey", "den", "B\u00e4\u00b7chen", ",", "die", "wie", "Gla\u00df"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "KOKOM", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Vor sich rauschen, sollen weiden,", "tokens": ["Vor", "sich", "rau\u00b7schen", ",", "sol\u00b7len", "wei\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PRF", "VVINF", "$,", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wann die Lerch vnd Nachtigal", "tokens": ["Wann", "die", "Lerch", "vnd", "Nach\u00b7ti\u00b7gal"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "An wird stimmen Berg vnd Thal.", "tokens": ["An", "wird", "stim\u00b7men", "Berg", "vnd", "Thal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VAFIN", "ADJA", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.20": {"line.1": {"text": "Celadon, vor welches singen", "tokens": ["Ce\u00b7la\u00b7don", ",", "vor", "wel\u00b7ches", "sin\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "APPR", "PWAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Meine Geige sich entf\u00e4rbt,", "tokens": ["Mei\u00b7ne", "Gei\u00b7ge", "sich", "ent\u00b7f\u00e4rbt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Der sein Spiel von dem ererbt,", "tokens": ["Der", "sein", "Spiel", "von", "dem", "er\u00b7erbt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "APPR", "ART", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "So den Acheron kan zwingen,", "tokens": ["So", "den", "A\u00b7che\u00b7ron", "kan", "zwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Geht mit seiner Kunst voran,", "tokens": ["Geht", "mit", "sei\u00b7ner", "Kunst", "vo\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Dann sing' ich so gut ich kan.", "tokens": ["Dann", "sing'", "ich", "so", "gut", "ich", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADJD", "PPER", "VMFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.21": {"line.1": {"text": "Mein Berrintho wird mir sagen,", "tokens": ["Mein", "Ber\u00b7rin\u00b7tho", "wird", "mir", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Wo mir etwa Flei\u00df gebricht,", "tokens": ["Wo", "mir", "et\u00b7wa", "Flei\u00df", "ge\u00b7bricht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Vnd durch gutten Vnterricht", "tokens": ["Vnd", "durch", "gut\u00b7ten", "Vn\u00b7ter\u00b7richt"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Eine gute R\u00f6ht' abjagen,", "tokens": ["Ei\u00b7ne", "gu\u00b7te", "R\u00f6ht'", "ab\u00b7ja\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Mein Berrintho, der mich trieb,", "tokens": ["Mein", "Ber\u00b7rin\u00b7tho", ",", "der", "mich", "trieb", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Da\u00df ich dieses Lied auch schrieb.", "tokens": ["Da\u00df", "ich", "die\u00b7ses", "Lied", "auch", "schrieb", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDAT", "NN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.22": {"line.1": {"text": "Also wollen wir geniessen", "tokens": ["Al\u00b7so", "wol\u00b7len", "wir", "ge\u00b7nies\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Vnsers Lebens, weil es wehrt,", "tokens": ["Vn\u00b7sers", "Le\u00b7bens", ",", "weil", "es", "wehrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Vnd ob schon der Geist entfehrt,", "tokens": ["Vnd", "ob", "schon", "der", "Geist", "ent\u00b7fehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "----+-+", "measure": "unknown.measure.di"}, "line.4": {"text": "Augen vnd Geh\u00f6r sich schliessen,", "tokens": ["Au\u00b7gen", "vnd", "Ge\u00b7h\u00f6r", "sich", "schlies\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Werden wir doch, wie ich mein',", "tokens": ["Wer\u00b7den", "wir", "doch", ",", "wie", "ich", "mein'", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Vmb ein gut theil vbrig sein.", "tokens": ["Vmb", "ein", "gut", "theil", "vb\u00b7rig", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJD", "ADJD", "ADJD", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.23": {"line.1": {"text": "Unsrer Freundschafft, vnsrer Seiten", "tokens": ["Uns\u00b7rer", "Freund\u00b7schafft", ",", "vns\u00b7rer", "Sei\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wird ob Gott wil noch gedacht,", "tokens": ["Wird", "ob", "Gott", "wil", "noch", "ge\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KOUS", "NN", "VMFIN", "ADV", "VVPP", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Solte man vns zu der Nacht", "tokens": ["Sol\u00b7te", "man", "vns", "zu", "der", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PIS", "PRF", "APPR", "ART", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.4": {"text": "Auch vmb Morgen schon begleiten:", "tokens": ["Auch", "vmb", "Mor\u00b7gen", "schon", "be\u00b7glei\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Vnd der edlen Tichter Geist", "tokens": ["Vnd", "der", "ed\u00b7len", "Tich\u00b7ter", "Geist"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Lebt im Tod erst allermeist.", "tokens": ["Lebt", "im", "Tod", "erst", "al\u00b7ler\u00b7meist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "ADV", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.24": {"line.1": {"text": "Damon auff, vnd la\u00df vns leben,", "tokens": ["Da\u00b7mon", "auff", ",", "vnd", "la\u00df", "vns", "le\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PTKVZ", "$,", "KON", "VVIMP", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "La\u00df vns auff den Koht der Welt,", "tokens": ["La\u00df", "vns", "auff", "den", "Koht", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Der von vns ein Urtheil fellt,", "tokens": ["Der", "von", "vns", "ein", "Ur\u00b7theil", "fellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "So nicht taug, nicht so viel geben!", "tokens": ["So", "nicht", "taug", ",", "nicht", "so", "viel", "ge\u00b7ben", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "VVFIN", "$,", "PTKNEG", "ADV", "ADV", "VVINF", "$."], "meter": "--+--+--", "measure": "anapaest.di.plus"}, "line.5": {"text": "Muhtig sein vnd recht gethan", "tokens": ["Muh\u00b7tig", "sein", "vnd", "recht", "ge\u00b7than"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VAINF", "KON", "ADJD", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Bricht durch allen Neid die Bahn.", "tokens": ["Bricht", "durch", "al\u00b7len", "Neid", "die", "Bahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PIAT", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.25": {"line.1": {"text": "Die\u00df nur wil ich einig bitten,", "tokens": ["Die\u00df", "nur", "wil", "ich", "ei\u00b7nig", "bit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "VMFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df mir k\u00fcnfftig frey mag stehn,", "tokens": ["Da\u00df", "mir", "k\u00fcnff\u00b7tig", "frey", "mag", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "ADJD", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bey dir au\u00df vnd ein zu gehn", "tokens": ["Bey", "dir", "au\u00df", "vnd", "ein", "zu", "gehn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "PTKVZ", "KON", "ART", "PTKZU", "VVINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Nach der alten Freundschafft Sitten.", "tokens": ["Nach", "der", "al\u00b7ten", "Freund\u00b7schafft", "Sit\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ach wie wol ist meinem Sinn,", "tokens": ["Ach", "wie", "wol", "ist", "mei\u00b7nem", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ADV", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wenn ich, Damon, vmb dich bin.", "tokens": ["Wenn", "ich", ",", "Da\u00b7mon", ",", "vmb", "dich", "bin", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "NE", "$,", "KOUI", "PPER", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.26": {"line.1": {"text": "Andre m\u00f6gen von dir halten,", "tokens": ["And\u00b7re", "m\u00f6\u00b7gen", "von", "dir", "hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "APPR", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Von dir reden die\u00df vnd das,", "tokens": ["Von", "dir", "re\u00b7den", "die\u00df", "vnd", "das", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PDS", "KON", "PDS", "$,"], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.3": {"text": "Ich begehre durch das Gla\u00df,", "tokens": ["Ich", "be\u00b7geh\u00b7re", "durch", "das", "Gla\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "So ich trincke, zu erkalten,", "tokens": ["So", "ich", "trin\u00b7cke", ",", "zu", "er\u00b7kal\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "$,", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wo mein Hertz mit Trug vnd List", "tokens": ["Wo", "mein", "Hertz", "mit", "Trug", "vnd", "List"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPOSAT", "NN", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Gegen dich verf\u00e4lschet ist.", "tokens": ["Ge\u00b7gen", "dich", "ver\u00b7f\u00e4l\u00b7schet", "ist", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": [