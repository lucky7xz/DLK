["APPR", "PIS", "ADV", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Lenkern und den Leitern,", "tokens": ["Den", "Len\u00b7kern", "und", "den", "Lei\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sei du das Vorgebirg von Erz,", "tokens": ["Sei", "du", "das", "Vor\u00b7ge\u00b7birg", "von", "Erz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dran ihre \u00c4ngste scheitern;", "tokens": ["Dran", "ih\u00b7re", "\u00c4ngs\u00b7te", "schei\u00b7tern", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ruf ihnen zu: \u00bb", "tokens": ["Ruf", "ih\u00b7nen", "zu", ":", "\u00bb"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["NN", "PPER", "PTKVZ", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Dies Preu\u00dfen aufzurichten,", "tokens": ["Dies", "Preu\u00b7\u00dfen", "auf\u00b7zu\u00b7rich\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "NN", "VVIZU", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Es tut nicht gut, es tut nicht gut", "tokens": ["Es", "tut", "nicht", "gut", ",", "es", "tut", "nicht", "gut"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADJD", "$,", "PPER", "VVFIN", "PTKNEG", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Solch Zagen und Verzichten.", "tokens": ["Solch", "Za\u00b7gen", "und", "Ver\u00b7zich\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Wohl, angesichts von meinem Schlo\u00df,", "tokens": ["Wohl", ",", "an\u00b7ge\u00b7sichts", "von", "mei\u00b7nem", "Schlo\u00df", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mag ich hier droben wohnen,", "tokens": ["Mag", "ich", "hier", "dro\u00b7ben", "woh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Einschmelzt mich zu Kanonen;", "tokens": ["Ein\u00b7schmelzt", "mich", "zu", "Ka\u00b7no\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wohl thron' ich hier auf sichrem Sitz,", "tokens": ["Wohl", "thron'", "ich", "hier", "auf", "sich\u00b7rem", "Sitz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mein Schimmel selbst ward erzen,", "tokens": ["Mein", "Schim\u00b7mel", "selbst", "ward", "er\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VAFIN", "VVINF", "$,"], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.6": {"text": "Doch sichrer thront der alte Fritz", "tokens": ["Doch", "sich\u00b7rer", "thront", "der", "al\u00b7te", "Fritz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "ART", "ADJA", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "In alten Pr