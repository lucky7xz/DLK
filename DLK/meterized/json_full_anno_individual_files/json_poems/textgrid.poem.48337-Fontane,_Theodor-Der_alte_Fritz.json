{"textgrid.poem.48337": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Der alte Fritz", "genre": "verse", "period": "N.A.", "pub_year": 1858, "urn": "N.A.", "language": ["de:0.85", "sv:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bist endlich da! Gott sei's geklagt,", "tokens": ["Bist", "end\u00b7lich", "da", "!", "Gott", "sei's", "ge\u00b7klagt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "$.", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hast lange warten lassen;", "tokens": ["Hast", "lan\u00b7ge", "war\u00b7ten", "las\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "VVINF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Nun lehr' uns wieder, unverzagt", "tokens": ["Nun", "lehr'", "uns", "wie\u00b7der", ",", "un\u00b7ver\u00b7zagt"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Feind beim Schopfe fassen,", "tokens": ["Den", "Feind", "beim", "Schop\u00b7fe", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Den Feind in Ost, den Feind in West,", "tokens": ["Den", "Feind", "in", "Ost", ",", "den", "Feind", "in", "West", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "$,", "ART", "NN", "APPR", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Feinde drau\u00df und drinnen,", "tokens": ["Die", "Fein\u00b7de", "drau\u00df", "und", "drin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "KON", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Zerrei\u00df die Netze dicht und fest,", "tokens": ["Zer\u00b7rei\u00df", "die", "Net\u00b7ze", "dicht", "und", "fest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Womit sie uns umspinnen.", "tokens": ["Wo\u00b7mit", "sie", "uns", "um\u00b7spin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Blitz' nur herab von deiner Wacht,", "tokens": ["Blitz'", "nur", "her\u00b7ab", "von", "dei\u00b7ner", "Wacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Solch W\u00e4chter mag uns taugen:", "tokens": ["Solch", "W\u00e4ch\u00b7ter", "mag", "uns", "tau\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wir brauchen wieder, Tag und Nacht,", "tokens": ["Wir", "brau\u00b7chen", "wie\u00b7der", ",", "Tag", "und", "Nacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Alten-Fritzen-Augen;", "tokens": ["Die", "Al\u00b7ten\u00b7Frit\u00b7zen\u00b7Au\u00b7gen", ";"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Blitz' nur herab! und wenn im Nu", "tokens": ["Blitz'", "nur", "her\u00b7ab", "!", "und", "wenn", "im", "Nu"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ADV", "$.", "KON", "KOUS", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Schleicher du erraten,", "tokens": ["Die", "Schlei\u00b7cher", "du", "er\u00b7ra\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Dann heb den Stock und droh: \u00bbDu, du!\u00ab", "tokens": ["Dann", "heb", "den", "Stock", "und", "droh", ":", "\u00bb", "Du", ",", "du", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "word", "punct", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "KON", "ADJD", "$.", "$(", "PPER", "$,", "PPER", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wie weiland dem Kroaten.", "tokens": ["Wie", "wei\u00b7land", "dem", "Kroa\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Blitz' nur herab von deiner Wacht;", "tokens": ["Blitz'", "nur", "her\u00b7ab", "von", "dei\u00b7ner", "Wacht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und wenn uns Feinde spotten,", "tokens": ["Und", "wenn", "uns", "Fein\u00b7de", "spot\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Pandurentum und Slawenmacht", "tokens": ["Pan\u00b7du\u