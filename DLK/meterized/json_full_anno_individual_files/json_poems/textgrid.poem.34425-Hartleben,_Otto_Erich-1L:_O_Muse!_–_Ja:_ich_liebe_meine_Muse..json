{"textgrid.poem.34425": {"metadata": {"author": {"name": "Hartleben, Otto Erich", "birth": "N.A.", "death": "N.A."}, "title": "1L: O Muse! \u2013 Ja: ich liebe meine Muse.", "genre": "verse", "period": "N.A.", "pub_year": 1885, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Muse! \u2013 Ja: ich liebe meine Muse.", "tokens": ["O", "Mu\u00b7se", "!", "\u2013", "Ja", ":", "ich", "lie\u00b7be", "mei\u00b7ne", "Mu\u00b7se", "."], "token_info": ["word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "$(", "PTKANT", "$.", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es ist ein sch\u00f6nes Weib und jung an Jahren!", "tokens": ["Es", "ist", "ein", "sch\u00f6\u00b7nes", "Weib", "und", "jung", "an", "Jah\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "KON", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nicht allegorisch und abstract confuse,", "tokens": ["Nicht", "al\u00b7le\u00b7go\u00b7risch", "und", "abs\u00b7tract", "con\u00b7fu\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "KON", "FM", "FM", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "sie schaut mich an mit Augen braun und klaren.", "tokens": ["sie", "schaut", "mich", "an", "mit", "Au\u00b7gen", "braun", "und", "kla\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "APPR", "NN", "ADJD", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Sie redet zu den M\u00e4nnern in der Blouse,", "tokens": ["Sie", "re\u00b7det", "zu", "den", "M\u00e4n\u00b7nern", "in", "der", "Blou\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "wie auch zu denen, die auf Gummi fahren,", "tokens": ["wie", "auch", "zu", "de\u00b7nen", ",", "die", "auf", "Gum\u00b7mi", "fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "APPR", "PRELS", "$,", "PRELS", "APPR", "NE", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "und tr\u00e4gt nicht blaue Str\u00fcmpfe, sondern keine,", "tokens": ["und", "tr\u00e4gt", "nicht", "blau\u00b7e", "Str\u00fcmp\u00b7fe", ",", "son\u00b7dern", "kei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "ADJA", "NN", "$,", "KON", "PIAT", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "denn sie ist stolz auf ihre weissen Beine.", "tokens": ["denn", "sie", "ist", "stolz", "auf", "ih\u00b7re", "weis\u00b7sen", "Bei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADJD", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und doch ist sie von altem, echtem Stamme,", "tokens": ["Und", "doch", "ist", "sie", "von", "al\u00b7tem", ",", "ech\u00b7tem", "Stam\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "APPR", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "echt ihr Cost\u00fcm wie eine Butzenscheibe!", "tokens": ["echt", "ihr", "Cos\u00b7t\u00fcm", "wie", "ei\u00b7ne", "But\u00b7zen\u00b7schei\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Joniens Sonnenluft war ihre Amme,", "tokens": ["Jo\u00b7ni\u00b7ens", "Son\u00b7nen\u00b7luft", "war", "ih\u00b7re", "Am\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "die sie erzog zum sonnensch\u00f6nen Weibe:", "tokens": ["die", "sie", "er\u00b7zog", "zum", "son\u00b7nen\u00b7sch\u00f6\u00b7nen", "Wei\u00b7be", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRELS", "PPER", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "auf dass sie meine Brust zum Lied entflamme,", "tokens": ["auf", "dass", "sie", "mei\u00b7ne", "Brust", "zum", "Lied", "ent\u00b7flam\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "PPOSAT", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "dass immerdar ich ihr ein Sclave bleibe,", "tokens": ["dass", "im\u00b7mer\u00b7dar", "ich", "ihr", "ein", "Scla\u00b7ve", "blei\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPER", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "sch\u00f6nheitsgebannt, erfasst vom dunklen Sehnen", "tokens": ["sch\u00f6n\u00b7heits\u00b7ge\u00b7bannt", ",", "er\u00b7fasst", "vom", "dunk\u00b7len", "Seh\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.8": {"text": "nach euch, ihr G\u00f6tterhaine der Hellenen! \u2013", "tokens": ["nach", "euch", ",", "ihr", "G\u00f6t\u00b7ter\u00b7hai\u00b7ne", "der", "Hel\u00b7le\u00b7nen", "!", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPER", "$,", "PPOSAT", "NN", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Noch immer dieser Griechenschwarm von neulich", "tokens": ["Noch", "im\u00b7mer", "die\u00b7ser", "Grie\u00b7chen\u00b7schwarm", "von", "neu\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PDAT", "NN", "APPR", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "vor hundert Jahren? Endet man denn nie,", "tokens": ["vor", "hun\u00b7dert", "Jah\u00b7ren", "?", "En\u00b7det", "man", "denn", "nie", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "$.", "VVFIN", "PIS", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "dies h\u00f6chst frivole Volk zu preisen? Greulich!", "tokens": ["dies", "h\u00f6chst", "fri\u00b7vo\u00b7le", "Volk", "zu", "prei\u00b7sen", "?", "Greu\u00b7lich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "ADV", "ADJA", "NN", "PTKZU", "VVINF", "$.", "NE", "$."], "meter": "-+---+-+-+-", "measure": "dactylic.init"}, "line.4": {"text": "Und jeder weiss doch, wie P\u00e4derastie,", "tokens": ["Und", "je\u00b7der", "weiss", "doch", ",", "wie", "P\u00e4\u00b7de\u00b7ras\u00b7tie", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "ADV", "$,", "PWAV", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Knechtschaft der Frauen, Sclaverei \u2013 abscheulich!", "tokens": ["Knecht\u00b7schaft", "der", "Frau\u00b7en", ",", "Scla\u00b7ve\u00b7rei", "\u2013", "ab\u00b7scheu\u00b7lich", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "NN", "$(", "ADJD", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.6": {"text": "Sogar die G\u00f6tter lebten wie das Vieh!", "tokens": ["So\u00b7gar", "die", "G\u00f6t\u00b7ter", "leb\u00b7ten", "wie", "das", "Vieh", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "War da der Untergang nicht unausbleiblich?", "tokens": ["War", "da", "der", "Un\u00b7ter\u00b7gang", "nicht", "un\u00b7aus\u00b7bleib\u00b7lich", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Selbst im Olymp war die Bedienung weiblich!", "tokens": ["Selbst", "im", "O\u00b7lymp", "war", "die", "Be\u00b7die\u00b7nung", "weib\u00b7lich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VAFIN", "ART", "NN", "ADJD", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.4": {"line.1": {"text": "Da lob ich mir Berliner Sittlichkeit,", "tokens": ["Da", "lob", "ich", "mir", "Ber\u00b7li\u00b7ner", "Sitt\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "fest garantiert von Polizeicolonnen!", "tokens": ["fest", "ga\u00b7ran\u00b7tiert", "von", "Po\u00b7li\u00b7zei\u00b7co\u00b7lon\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Revolver tragen sie seit kurzer Zeit,", "tokens": ["Re\u00b7vol\u00b7ver", "tra\u00b7gen", "sie", "seit", "kur\u00b7zer", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "sind h\u00f6flich gegen Jedermann gesonnen,", "tokens": ["sind", "h\u00f6f\u00b7lich", "ge\u00b7gen", "Je\u00b7der\u00b7mann", "ge\u00b7son\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "APPR", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "die besten Christen in der Christenheit \u2013", "tokens": ["die", "bes\u00b7ten", "Chris\u00b7ten", "in", "der", "Chris\u00b7ten\u00b7heit", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "gar einen hab ich herzlich lieb gewonnen,", "tokens": ["gar", "ei\u00b7nen", "hab", "ich", "herz\u00b7lich", "lieb", "ge\u00b7won\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VAFIN", "PPER", "ADJD", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "das war der W\u00e4chter, der mir morgens schloss,", "tokens": ["das", "war", "der", "W\u00e4ch\u00b7ter", ",", "der", "mir", "mor\u00b7gens", "schloss", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NE", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "und dessen Gunst ich oft und gern genoss.", "tokens": ["und", "des\u00b7sen", "Gunst", "ich", "oft", "und", "gern", "ge\u00b7noss", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRELAT", "NN", "PPER", "ADV", "KON", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Die Socialisten und Prostituierten", "tokens": ["Die", "So\u00b7ci\u00b7a\u00b7lis\u00b7ten", "und", "Pros\u00b7ti\u00b7tu\u00b7ier\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "behandeln sie mit stillbewegtem Fleiss,", "tokens": ["be\u00b7han\u00b7deln", "sie", "mit", "still\u00b7be\u00b7weg\u00b7tem", "Fleiss", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "da die den braven B\u00fcrger sonst genierten", "tokens": ["da", "die", "den", "bra\u00b7ven", "B\u00fcr\u00b7ger", "sonst", "ge\u00b7nier\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ART", "ADJA", "NN", "ADV", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "und seinen sandgezognen Lebenskreis", "tokens": ["und", "sei\u00b7nen", "sand\u00b7ge\u00b7zog\u00b7nen", "Le\u00b7bens\u00b7kreis"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "durch unbequemes Toben alterierten.", "tokens": ["durch", "un\u00b7be\u00b7que\u00b7mes", "To\u00b7ben", "al\u00b7te\u00b7rier\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Was keiner sieht, das macht auch keinen heiss,", "tokens": ["Was", "kei\u00b7ner", "sieht", ",", "das", "macht", "auch", "kei\u00b7nen", "heiss", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "VVFIN", "$,", "PDS", "VVFIN", "ADV", "PIAT", "ADJA", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "und also regle man \u2013 das Strassenleben,", "tokens": ["und", "al\u00b7so", "re\u00b7gle", "man", "\u2013", "das", "Stras\u00b7sen\u00b7le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PIS", "$(", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "mags auch im Innern tiefere Wunden geben.", "tokens": ["mags", "auch", "im", "In\u00b7nern", "tie\u00b7fe\u00b7re", "Wun\u00b7den", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "APPRART", "NN", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.6": {"line.1": {"text": "Die Socialisten sieht man bei publiquen", "tokens": ["Die", "So\u00b7ci\u00b7a\u00b7lis\u00b7ten", "sieht", "man", "bei", "pu\u00b7bli\u00b7quen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PIS", "APPR", "NE"], "meter": "--+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Begr\u00e4bnisfeiern nur in \u2013 schwarzem Kreppe ...", "tokens": ["Be\u00b7gr\u00e4b\u00b7nis\u00b7fei\u00b7ern", "nur", "in", "\u2013", "schwar\u00b7zem", "Krep\u00b7pe", "..."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "$(", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Herrschaft hat mit ihren Domestiquen", "tokens": ["Die", "Herr\u00b7schaft", "hat", "mit", "ih\u00b7ren", "Do\u00b7mes\u00b7ti\u00b7quen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "im Haus nicht mal gemein \u2013 dieselbe Treppe ...", "tokens": ["im", "Haus", "nicht", "mal", "ge\u00b7mein", "\u2013", "die\u00b7sel\u00b7be", "Trep\u00b7pe", "..."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKNEG", "ADV", "ADJD", "$(", "PDAT", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Nicht zu err\u00f6then brauchen die Pudiquen,", "tokens": ["Nicht", "zu", "er\u00b7r\u00f6\u00b7then", "brau\u00b7chen", "die", "Pu\u00b7di\u00b7quen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PTKZU", "VVINF", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "da auf der Wilhelmstrasse keine \u2013 Prostituierte.", "tokens": ["da", "auf", "der", "Wil\u00b7helms\u00b7tras\u00b7se", "kei\u00b7ne", "\u2013", "Pros\u00b7ti\u00b7tu\u00b7ier\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "PIAT", "$(", "NN", "$."], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Kurz wie ein friedlich rieselnd B\u00e4chlein fliesst", "tokens": ["Kurz", "wie", "ein", "fried\u00b7lich", "rie\u00b7selnd", "B\u00e4ch\u00b7lein", "fliesst"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "KOKOM", "ART", "ADJD", "VVPP", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "das Leben dem hin, ders mit Mass geniesst.", "tokens": ["das", "Le\u00b7ben", "dem", "hin", ",", "ders", "mit", "Mass",