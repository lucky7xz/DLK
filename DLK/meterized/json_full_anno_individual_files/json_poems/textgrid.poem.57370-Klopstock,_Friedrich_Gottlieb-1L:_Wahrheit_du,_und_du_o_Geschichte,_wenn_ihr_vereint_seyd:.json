{"textgrid.poem.57370": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wahrheit du, und du o Geschichte, wenn ihr vereint seyd:", "genre": "verse", "period": "N.A.", "pub_year": 1794, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wahrheit du, und du o Geschichte, wenn ihr vereint seyd:", "tokens": ["Wahr\u00b7heit", "du", ",", "und", "du", "o", "Ge\u00b7schich\u00b7te", ",", "wenn", "ihr", "ver\u00b7eint", "seyd", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "KON", "PPER", "FM", "NN", "$,", "KOUS", "PPER", "VVPP", "VAFIN", "$."], "meter": "+-+-+--+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Schreibet Flammen der Griffel, mit welchem ihr zeugt von erh\u00f6hten", "tokens": ["Schrei\u00b7bet", "Flam\u00b7men", "der", "Grif\u00b7fel", ",", "mit", "wel\u00b7chem", "ihr", "zeugt", "von", "er\u00b7h\u00f6h\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "NN", "ART", "NN", "$,", "APPR", "PRELS", "PPER", "VVFIN", "APPR", "ADJA"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.3": {"text": "Buben; und die Stimme, mit der ihr das Zeugniss aussprecht,", "tokens": ["Bu\u00b7ben", ";", "und", "die", "Stim\u00b7me", ",", "mit", "der", "ihr", "das", "Zeug\u00b7niss", "aus\u00b7sprecht", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "KON", "ART", "NN", "$,", "APPR", "PRELS", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.4": {"text": "Spricht, ihr r\u00e4chenden! Donner aus.", "tokens": ["Spricht", ",", "ihr", "r\u00e4\u00b7chen\u00b7den", "!", "Don\u00b7ner", "aus", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPER", "VVINF", "$.", "NN", "PTKVZ", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.2": {"line.1": {"text": "R\u00e4chet sie jetzt, die Menschheit, an Frankreichs Oligokraten,", "tokens": ["R\u00e4\u00b7chet", "sie", "jetzt", ",", "die", "Menschheit", ",", "an", "Fran\u00b7kreichs", "O\u00b7li\u00b7go\u00b7kra\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "ART", "NN", "$,", "APPR", "NE", "NE", "$,"], "meter": "+--+-+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Ernste Vergelterinnen! Zu schonend r\u00fcgt der Verbrecher", "tokens": ["Erns\u00b7te", "Ver\u00b7gel\u00b7te\u00b7rin\u00b7nen", "!", "Zu", "scho\u00b7nend", "r\u00fcgt", "der", "Ver\u00b7bre\u00b7cher"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "PTKA", "ADJD", "VVFIN", "ART", "NN"], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.3": {"text": "Tod; Europa will das warnende Schandmal, will die", "tokens": ["Tod", ";", "Eu\u00b7ro\u00b7pa", "will", "das", "war\u00b7nen\u00b7de", "Schand\u00b7mal", ",", "will", "die"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "$.", "NE", "VMFIN", "ART", "ADJA", "NN", "$,", "VMFIN", "ART"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Ewige Piramide sehn!", "tokens": ["E\u00b7wi\u00b7ge", "Pi\u00b7ra\u00b7mi\u00b7de", "sehn", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Nie noch hat die Geschichte so ganz enth\u00fcllet der Wahrheit", "tokens": ["Nie", "noch", "hat", "die", "Ge\u00b7schich\u00b7te", "so", "ganz", "ent\u00b7h\u00fcl\u00b7let", "der", "Wahr\u00b7heit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VAFIN", "ART", "NN", "ADV", "ADV", "VVFIN", "ART", "NN"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Antlitz erblickt; es verschm\u00e4hte den Schleyer der Handelnden Unscham.", "tokens": ["Ant\u00b7litz", "er\u00b7blickt", ";", "es", "ver\u00b7schm\u00e4h\u00b7te", "den", "Schle\u00b7yer", "der", "Han\u00b7deln\u00b7den", "Un\u00b7scham", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVPP", "$.", "PPER", "VVFIN", "ART", "NN", "ART", "NN", "NE", "$."], "meter": "+--+--+-+-+-+--+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "Eilet denn, thut die Folg' uns kund der Vereinung! lindert,", "tokens": ["Ei\u00b7let", "denn", ",", "thut", "die", "Fol\u00b7g'", "uns", "kund", "der", "Ver\u00b7ei\u00b7nung", "!", "lin\u00b7dert", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "VVFIN", "ART", "NN", "PPER", "PTKVZ", "ART", "NN", "$.", "VVFIN", "$,"], "meter": "+--+-+--+--+-+-", "measure": "iambic.hexa.invert"}, "line.4": {"text": "L\u00f6schet der harrenden heissen Durst.", "tokens": ["L\u00f6\u00b7schet", "der", "har\u00b7ren\u00b7den", "heis\u00b7sen", "Durst", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "J\u00fcnglinge dulden's noch wohl, das Erwarten; wir Greise verabscheun's.", "tokens": ["J\u00fcng\u00b7lin\u00b7ge", "dul\u00b7den's", "noch", "wohl", ",", "das", "Er\u00b7war\u00b7ten", ";", "wir", "Grei\u00b7se", "ver\u00b7ab\u00b7scheun'", "s."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "abbreviation"], "pos": ["NN", "NE", "ADV", "ADV", "$,", "ART", "NN", "$.", "PPER", "NN", "VVFIN", "NE"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Auf denn, r\u00e4chet die Menschheit, und bald! Nicht s\u00fcss nur, auch edel", "tokens": ["Auf", "denn", ",", "r\u00e4\u00b7chet", "die", "Menschheit", ",", "und", "bald", "!", "Nicht", "s\u00fcss", "nur", ",", "auch", "e\u00b7del"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ADV", "$,", "VVFIN", "ART", "NN", "$,", "KON", "ADV", "$.", "PTKNEG", "ADJD", "ADV", "$,", "ADV", "ADJD"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.3": {"text": "Ist die Rache, um die wir flehen; o reicht aus voller,", "tokens": ["Ist", "die", "Ra\u00b7che", ",", "um", "die", "wir", "fle\u00b7hen", ";", "o", "reicht", "aus", "vol\u00b7ler", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$,", "APPR", "PRELS", "PPER", "VVINF", "$.", "FM", "VVFIN", "APPR", "ADJA", "$,"], "meter": "--+--+-+--+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "K\u00fchlender Schale den Labetrunk.", "tokens": ["K\u00fch\u00b7len\u00b7der", "Scha\u00b7le", "den", "La\u00b7be\u00b7trunk", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.5": {"line.1": {"text": "Sollten vielleicht dem Eroberer nur Schandmale den lauten", "tokens": ["Soll\u00b7ten", "viel\u00b7leicht", "dem", "Er\u00b7o\u00b7be\u00b7rer", "nur", "Schand\u00b7ma\u00b7le", "den", "lau\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "ART", "NN", "ADV", "NN", "ART", "ADJA"], "meter": "+--+--+-+-+-+-+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Namen ewigen? nicht dem Hochverr\u00e4ther der Menschheit,", "tokens": ["Na\u00b7men", "e\u00b7wi\u00b7gen", "?", "nicht", "dem", "Hoch\u00b7ver\u00b7r\u00e4\u00b7ther", "der", "Menschheit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "$.", "PTKNEG", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Nicht dem Scheusal, dem Heuchler auch, so der Freyheit opfernd,", "tokens": ["Nicht", "dem", "Scheu\u00b7sal", ",", "dem", "Heuch\u00b7ler", "auch", ",", "so", "der", "Frey\u00b7heit", "op\u00b7fernd", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "$,", "ART", "NN", "ADV", "$,", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "+-+--+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Kettenumrasselte Freye w\u00fcrgt?", "tokens": ["Ket\u00b7ten\u00b7um\u00b7ras\u00b7sel\u00b7te", "Frey\u00b7e", "w\u00fcrgt", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.6": {"line.1": {"text": "Nein, so w\u00e4hlet ihr nicht, vergesset eh die Erobrer,", "tokens": ["Nein", ",", "so", "w\u00e4h\u00b7let", "ihr", "nicht", ",", "ver\u00b7ges\u00b7set", "eh", "die", "E\u00b7rob\u00b7rer", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "VVFIN", "PPER", "PTKNEG", "$,", "VVFIN", "KOUS", "ART", "NN", "$,"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Als dass ihr nicht der Freyheit get\u00fcnchte Verg\u00f6tterer hinstellt,", "tokens": ["Als", "dass", "ihr", "nicht", "der", "Frey\u00b7heit", "ge\u00b7t\u00fcnch\u00b7te", "Ver\u00b7g\u00f6t\u00b7te\u00b7rer", "hin\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "PTKNEG", "ART", "NN", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+--+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "Wie sie waren. Mich deucht, ich sehe die Flammenschrift schon!", "tokens": ["Wie", "sie", "wa\u00b7ren", ".", "Mich", "deucht", ",", "ich", "se\u00b7he", "die", "Flam\u00b7men\u00b7schrift", "schon", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VAFIN", "$.", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "--+--+-+--+--+", "measure": "anapaest.di.plus"}, "line.4": {"text": "H\u00f6re der redenden Dounerton!", "tokens": ["H\u00f6\u00b7re", "der", "re\u00b7den\u00b7den", "Dou\u00b7ner\u00b7ton", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.7": {"line.1": {"text": "Wenn ich, erlebend, wirklich das seh', es wirklich, mein Ohr h\u00f6rt;", "tokens": ["Wenn", "ich", ",", "er\u00b7le\u00b7bend", ",", "wirk\u00b7lich", "das", "seh'", ",", "es", "wirk\u00b7lich", ",", "mein", "Ohr", "h\u00f6rt", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "VVPP", "$,", "ADJD", "PDS", "VVFIN", "$,", "PPER", "ADJD", "$,", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+--++", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Feyr' ich ein Fest, bekr\u00e4nze mit Eichenlaube das Haupt mir,", "tokens": ["Fey\u00b7r'", "ich", "ein", "Fest", ",", "be\u00b7kr\u00e4n\u00b7ze", "mit", "Ei\u00b7chen\u00b7lau\u00b7be", "das", "Haupt", "mir", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "$,", "VVFIN", "APPR", "NN", "ART", "NN", "PPER", "$,"], "meter": "+-+-+-+--+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Lade Freund' ein, sp\u00fcle den hellsten Kristall im reinsten", "tokens": ["La\u00b7de", "Freund'", "ein", ",", "sp\u00fc\u00b7le", "den", "hells\u00b7ten", "Kris\u00b7tall", "im", "reins\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "NN", "PTKVZ", "$,", "VVFIN", "ART", "ADJA", "NN", "APPRART", "ADJA"], "meter": "+-+-+--+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Bache, f\u00fcll' ihn mit Wein, der Greis", "tokens": ["Ba\u00b7che", ",", "f\u00fcll'", "ihn", "mit", "Wein", ",", "der", "Greis"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "$,", "VVFIN", "PPER", "APPR", "NN", "$,", "ART", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.8": {"line.1": {"text": "Wurde, wie ich. Im Kristall versiegt's nicht selten. Das Waldhorn", "tokens": ["Wur\u00b7de", ",", "wie", "ich", ".", "Im", "Kris\u00b7tall", "ver\u00b7siegt's", "nicht", "sel\u00b7ten", ".", "Das", "Wald\u00b7horn"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "$,", "PWAV", "PPER", "$.", "APPRART", "NN", "VVFIN", "PTKNEG", "ADJD", "$.", "ART", "NN"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Hallet; wer singen kann, singt. Wir freuen uns innig! Ich werde", "tokens": ["Hal\u00b7let", ";", "wer", "sin\u00b7gen", "kann", ",", "singt", ".", "Wir", "freu\u00b7en", "uns", "in\u00b7nig", "!", "Ich", "wer\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "$.", "PWS", "VVINF", "VMFIN", "$,", "VVFIN", "$.", "PPER", "VVFIN", "PPER", "ADJD", "$.", "PPER", "VAFIN"], "meter": "+--+--+-+-+-+-+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "Hundert Monde verj\u00fcngt! Wenn Rache, wie die vollbracht ist;", "tokens": ["Hun\u00b7dert", "Mon\u00b7de", "ver\u00b7j\u00fcngt", "!", "Wenn", "Ra\u00b7che", ",", "wie", "die", "voll\u00b7bracht", "ist", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVPP", "$.", "KOUS", "NN", "$,", "PWAV", "ART", "VVPP", "VAFIN", "$."], "meter": "+-+--+-+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Darf sich taumelnd die Freude freun.", "tokens": ["Darf", "sich", "tau\u00b7melnd", "die", "Freu\u00b7de", "freun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "ADJD", "ART", "NN", "VVINF", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.9": {"line.1": {"text": "Wahrheit du, und du o Geschichte, wenn ihr vereint seyd:", "tokens": ["Wahr\u00b7heit", "du", ",", "und", "du", "o", "Ge\u00b7schich\u00b7te", ",", "wenn", "ihr", "ver\u00b7eint", "seyd", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "KON", "PPER", "FM", "NN", "$,", "KOUS", "PPER", "VVPP", "VAFIN", "$."], "meter": "+-+-+--+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Schreibet Flammen der Griffel, mit welchem ihr zeugt von erh\u00f6hten", "tokens": ["Schrei\u00b7bet", "Flam\u00b7men", "der", "Grif\u00b7fel", ",", "mit", "wel\u00b7chem", "ihr", "zeugt", "von", "er\u00b7h\u00f6h\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "NN", "ART", "NN", "$,", "APPR", "PRELS", "PPER", "VVFIN", "APPR", "ADJA"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "