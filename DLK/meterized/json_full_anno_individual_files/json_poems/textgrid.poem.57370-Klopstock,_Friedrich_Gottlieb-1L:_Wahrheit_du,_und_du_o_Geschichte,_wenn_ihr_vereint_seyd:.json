d", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "$.", "NE", "VMFIN", "ART", "ADJA", "NN", "$,", "VMFIN", "ART"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Ewige Piramide sehn!", "tokens": ["E\u00b7wi\u00b7ge", "Pi\u00b7ra\u00b7mi\u00b7de", "sehn", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Nie noch hat die Geschichte so ganz enth\u00fcllet der Wahrheit", "tokens": ["Nie", "noch", "hat", "die", "Ge\u00b7schich\u00b7te", "so", "ganz", "ent\u00b7h\u00fcl\u00b7let", "der", "Wahr\u00b7heit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VAFIN", "ART", "NN", "ADV", "ADV", "VVFIN", "ART", "NN"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Antlitz erblickt; es verschm\u00e4hte den Schleyer der Handelnden Unscham.", "tokens": ["Ant\u00b7litz", "er\u00b7blickt", ";", "es", "ver\u00b7schm\u00e4h\u00b7te", "den", "Schle\u00b7yer", "der", "Han\u00b7deln\u00b7den", "Un\u00b7scham", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVPP", "$.", "PPER", "VVFIN", "ART", "NN", "ART", "NN", "NE", "$."], "meter": "+--+--+-+-+-+--+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "Eilet denn, thut die Folg' uns kund der Vereinung! lindert,", "tokens": ["Ei\u00b7let", "denn", ",", "thut", "die", "Fol\u00b7g'", "uns", "kund", "der", "Ver\u00b7ei\u00b7nung", "!", "lin\u00b7dert", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "VVFIN", "ART", "NN", "PPER", "PTKVZ", "ART", "NN", "$.", "VVFIN", "$,"], "meter": "+--+-+--+--+-+-", "measure": "iambic.hexa.invert"}, "line.4": {"text": "L\u00f6schet der harrenden heissen Durst.", "tokens": ["L\u00f6\u00b7schet", "der", "har\u00b7ren\u00b7den", "heis\u00b7sen", "Durst", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.12": {"line.1": {"text": "J\u00fcnglinge dulden's noch wohl, das Erwarten; wir Greise verabscheun's.", "tokens": ["J\u00fcng\u00b7lin\u00b7ge", "dul\u00b7den's", "noch", "wohl", ",", "das", "Er\u00b7war\u00b7ten", ";", "wir", "Grei\u00b7se", "ver\u00b7ab\u00b7scheun'", "s."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "abbreviation"], "pos": ["NN", "NE", "ADV", "ADV", "$,", "ART", "NN", "$.", "PPER", "NN", "VVFIN", "NE"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Auf denn, r\u00e4chet die Menschheit, und bald! Nicht s\u00fcss nur, auch edel", "tokens": ["Auf", "denn", ",", "r\u00e4\u00b7chet", "die", "Menschheit", ",", "und", "bald", "!", "Nicht", "s\u00fcss", "nur", ",", "auch", "e\u00b7del"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ADV", "$,", "VVFIN", "ART", "NN", "$,", "KON", "ADV", "$.", "PTKNEG", "ADJD", "ADV", "$,", "ADV", "ADJD"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.3": {"text": "Ist die Rache, um die wir flehen; o reicht aus voller,", "tokens": ["Ist", "die", "Ra\u00b7che", ",", "um", "die", "wir", "fle\u00b7hen", ";", "o", "reicht", "aus", "vol\u00b7ler", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$,", "APPR", "PRELS", "PPER", "VVINF", "$.", "FM", "VVFIN", "APPR", "ADJA", "$,"], "meter": "--+--+-+--+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "K\u00fchlender Schale den Labetrunk.", "tokens": ["K\u00fch\u00b7len\u00b7der", "Scha\u00b7le", "den", "La\u00b7be\u00b7trunk", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.13": {"line.1": {"text": "Sollten vielleicht dem Eroberer nur Schandmale den lauten", "tokens": ["Soll\u00b7ten", "viel\u00b7leicht", "dem", "Er\u00b7o\u00b7be\u00b7rer", "nur", "Schand\u00b7ma\u00b7le", "den", "lau\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "ART", "NN", "ADV", "NN", "ART", "ADJA"], "meter": "+--+--+-+-+-+-+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Namen ewigen? nicht dem Hochverr\u00e4ther der Menschheit,", "tokens": ["Na\u00b7men", "e\u00b7wi\u00b7gen", "?", "nicht", "dem", "Hoch\u00b7ver\u00b7r\u00e4\u00b7ther", "der", "Menschheit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "$.", "PTKNEG", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Nicht dem Scheusal, dem Heuchler auch, so der Freyheit opfernd,", "tokens": ["Nicht", "dem", "Scheu\u00b7sal", ",", "dem", "Heuch\u00b7ler", "auch", ",", "so", "der", "Frey\u00b7heit", "op\u00b7fernd", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "$,", "ART", "NN", "ADV", "$,", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "+-+--+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Kettenumrasselte Freye w\u00fcrgt?", "tokens": ["Ket\u00b7ten\u00b7um\u00b7ras\u00b7sel\u00b7te", "Frey\u00b7e", "w\u00fcrgt", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.14": {"line.1": {"text": "Nein, so w\u