{"dta.poem.7205": {"metadata": {"author": {"name": "Zesen, Philipp von", "birth": "N.A.", "death": "N.A."}, "title": "V or rede.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1640", "urn": "urn:nbn:de:kobv:b4-20685-2", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nun m\u00f6cht jhr klugen Griechen", "tokens": ["Nun", "m\u00f6cht", "jhr", "klu\u00b7gen", "Grie\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vnd jhr aus Latien euch ingesambt verkriechen/", "tokens": ["Vnd", "jhr", "aus", "La\u00b7ti\u00b7en", "euch", "in\u00b7ge\u00b7sambt", "ver\u00b7krie\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "NE", "PPER", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Es geht euch allen vor die de\u00fctsche Nachtigall/", "tokens": ["Es", "geht", "euch", "al\u00b7len", "vor", "die", "de\u00fct\u00b7sche", "Nach\u00b7ti\u00b7gall", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIS", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die nun so lieblich singt/ da\u00df auch jhr s\u00fc\u00dfer schall", "tokens": ["Die", "nun", "so", "lieb\u00b7lich", "singt", "/", "da\u00df", "auch", "jhr", "s\u00fc\u00b7\u00dfer", "schall"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ADV", "ADJD", "VVFIN", "$(", "KOUS", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die andern \u00fcbertrift: Sie sitzet auff den Rosen/", "tokens": ["Die", "an\u00b7dern", "\u00fc\u00b7bert\u00b7rift", ":", "Sie", "sit\u00b7zet", "auff", "den", "Ro\u00b7sen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "$.", "PPER", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Vnd modulirt mit macht; Die pr\u00e4chtigen Frantzosen", "tokens": ["Vnd", "mo\u00b7du\u00b7lirt", "mit", "macht", ";", "Die", "pr\u00e4ch\u00b7ti\u00b7gen", "Frant\u00b7zo\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "VVFIN", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Vermeinten gantz vnd gar/ vnd sagten ohne sche\u00fc", "tokens": ["Ver\u00b7mein\u00b7ten", "gantz", "vnd", "gar", "/", "vnd", "sag\u00b7ten", "oh\u00b7ne", "sche\u00fc"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "ADV", "KON", "ADV", "$(", "KON", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Da\u00df keine Zierligkeit in de\u00fctscher Sprache sey", "tokens": ["Da\u00df", "kei\u00b7ne", "Zier\u00b7lig\u00b7keit", "in", "de\u00fct\u00b7scher", "Spra\u00b7che", "sey"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIAT", "NN", "APPR", "ADJA", "NN", "VAFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Zur edlen Poesie: bi\u00df endlich sie gerochen", "tokens": ["Zur", "ed\u00b7len", "Poe\u00b7sie", ":", "bi\u00df", "end\u00b7lich", "sie", "ge\u00b7ro\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "$.", "KON", "ADV", "PPER", "VVPP"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Opitz der Edlen Ruhm/ vnd jhr die bahn gebrochen", "tokens": ["O\u00b7pitz", "der", "Ed\u00b7len", "Ruhm", "/", "vnd", "jhr", "die", "bahn", "ge\u00b7bro\u00b7chen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "ART", "ADJA", "NN", "$(", "KON", "PPER", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Durch G\u00f6ttlichen verstandt: da\u00df sie in kurtzer zeit", "tokens": ["Durch", "G\u00f6tt\u00b7li\u00b7chen", "ver\u00b7standt", ":", "da\u00df", "sie", "in", "kurt\u00b7zer", "zeit"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVPP", "$.", "KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Die andern \u00fcbertraff an Zier vnd Liebligkeit & c.", "tokens": ["Die", "an\u00b7dern", "\u00fc\u00b7bert\u00b7raff", "an", "Zier", "vnd", "Lieb\u00b7lig\u00b7keit", "&", "c."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "abbreviation"], "pos": ["ART", "ADJA", "VVFIN", "APPR", "NN", "KON", "NN", "$(", "NE"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}