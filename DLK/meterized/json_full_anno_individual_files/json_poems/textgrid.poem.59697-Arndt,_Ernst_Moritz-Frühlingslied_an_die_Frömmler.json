{"textgrid.poem.59697": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "Fr\u00fchlingslied an die Fr\u00f6mmler", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schm\u00e4lt mir nicht die alten Heiden,", "tokens": ["Schm\u00e4lt", "mir", "nicht", "die", "al\u00b7ten", "Hei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Denn ein Heide bin ich auch,", "tokens": ["Denn", "ein", "Hei\u00b7de", "bin", "ich", "auch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wann ich's Bl\u00fcmlein schau' der Heiden,", "tokens": ["Wann", "ich's", "Bl\u00fcm\u00b7lein", "schau'", "der", "Hei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wann ich's V\u00f6glein h\u00f6r' im Strauch.", "tokens": ["Wann", "ich's", "V\u00f6\u00b7glein", "h\u00f6r'", "im", "Strauch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Weg mit euren dunklen Listen!", "tokens": ["Weg", "mit", "eu\u00b7ren", "dunk\u00b7len", "Lis\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Weg mit eurer tr\u00fcben Kunst!", "tokens": ["Weg", "mit", "eu\u00b7rer", "tr\u00fc\u00b7ben", "Kunst", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn dem freien, frohen Christen", "tokens": ["Denn", "dem", "frei\u00b7en", ",", "fro\u00b7hen", "Chris\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Werden solche Schmerzen Dunst.", "tokens": ["Wer\u00b7den", "sol\u00b7che", "Schmer\u00b7zen", "Dunst", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ihr, die uns das Licht verd\u00fcstert,", "tokens": ["Ihr", ",", "die", "uns", "das", "Licht", "ver\u00b7d\u00fcs\u00b7tert", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schreckt die Freude bla\u00df und bleich,", "tokens": ["Schreckt", "die", "Freu\u00b7de", "bla\u00df", "und", "bleich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wi\u00dft, was unter Rosen fl\u00fcstert',", "tokens": ["Wi\u00dft", ",", "was", "un\u00b7ter", "Ro\u00b7sen", "fl\u00fcs\u00b7tert'", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PRELS", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hat auch Weg zum Himmelreich.", "tokens": ["Hat", "auch", "Weg", "zum", "Him\u00b7mel\u00b7reich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Blumen gab der Herr der Imme,", "tokens": ["Blu\u00b7men", "gab", "der", "Herr", "der", "Im\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Liebesklang der Nachtigall", "tokens": ["Lie\u00b7bes\u00b7klang", "der", "Nach\u00b7ti\u00b7gall"], "token_info": ["word", "word", "word"], "pos": ["NN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und dem Menschen eine Stimme", "tokens": ["Und", "dem", "Men\u00b7schen", "ei\u00b7ne", "Stim\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Tiefer Brust f\u00fcr Freudenschall.", "tokens": ["Tie\u00b7fer", "Brust", "f\u00fcr", "Freu\u00b7den\u00b7schall", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Bleibe Gott und Gottes Ehre", "tokens": ["Blei\u00b7be", "Gott", "und", "Got\u00b7tes", "Eh\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "NN", "KON", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In der ewigen Natur!", "tokens": ["In", "der", "e\u00b7wi\u00b7gen", "Na\u00b7tur", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sophoklesse und Homere", "tokens": ["So\u00b7pho\u00b7kles\u00b7se", "und", "Ho\u00b7me\u00b7re"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sangen seines Geistes nur.", "tokens": ["San\u00b7gen", "sei\u00b7nes", "Geis\u00b7tes", "nur", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Schm\u00e4lt mir Goethen nicht und Schiller,", "tokens": ["Schm\u00e4lt", "mir", "Goe\u00b7then", "nicht", "und", "Schil\u00b7ler", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NE", "PTKNEG", "KON", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ihr, des engen Eifers hei\u00df,", "tokens": ["Ihr", ",", "des", "en\u00b7gen", "Ei\u00b7fers", "hei\u00df", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ART", "ADJA", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Alle eure Jammertriller", "tokens": ["Al\u00b7le", "eu\u00b7re", "Jam\u00b7mer\u00b7tril\u00b7ler"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Geb' ich gern f\u00fcr solche preis.", "tokens": ["Geb'", "ich", "gern", "f\u00fcr", "sol\u00b7che", "preis", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Denn mein Heiland und Befreier", "tokens": ["Denn", "mein", "Hei\u00b7land", "und", "Be\u00b7frei\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Fuhr herab ins S\u00fcndenland,", "tokens": ["Fuhr", "her\u00b7ab", "ins", "S\u00fcn\u00b7den\u00b7land", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Der die h\u00f6chste Sternenleier", "tokens": ["Der", "die", "h\u00f6chs\u00b7te", "Ster\u00b7nen\u00b7lei\u00b7er"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hat f\u00fcr Lust und Leid gespannt.", "tokens": ["Hat", "f\u00fcr", "Lust", "und", "Leid", "ge\u00b7spannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Der mit ersten Morgenr\u00f6ten", "tokens": ["Der", "mit", "ers\u00b7ten", "Mor\u00b7gen\u00b7r\u00f6\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sang der Welten Urgesang,", "tokens": ["Sang", "der", "Wel\u00b7ten", "Ur\u00b7ge\u00b7sang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "G\u00f6nnet auch den Erdenfl\u00f6ten", "tokens": ["G\u00f6n\u00b7net", "auch", "den", "Er\u00b7den\u00b7fl\u00f6\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihren kurzen Freudenklang.", "tokens": ["Ih\u00b7ren", "kur\u00b7zen", "Freu\u00b7den\u00b7klang", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Denn besiegt hat er die L\u00fcste", "tokens": ["Denn", "be\u00b7siegt", "hat", "er", "die", "L\u00fcs\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVPP", "VAFIN", "PPER", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und den L\u00fcstensatan nur,", "tokens": ["Und", "den", "L\u00fcs\u00b7ten\u00b7sa\u00b7tan", "nur", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Damit jeder fr\u00f6hlich w\u00fc\u00dfte,", "tokens": ["Da\u00b7mit", "je\u00b7der", "fr\u00f6h\u00b7lich", "w\u00fc\u00df\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "PIAT", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gottes Kl\u00e4nge klingt Natur;", "tokens": ["Got\u00b7tes", "Kl\u00e4n\u00b7ge", "klingt", "Na\u00b7tur", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Denn gebracht hat hellre Lichter", "tokens": ["Denn", "ge\u00b7bracht", "hat", "hell\u00b7re", "Lich\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVPP", "VAFIN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Darum er dem Erdengraun,", "tokens": ["Da\u00b7rum", "er", "dem", "Er\u00b7den\u00b7graun", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "ART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Da\u00df die Menschenangesichter", "tokens": ["Da\u00df", "die", "Men\u00b7schen\u00b7an\u00b7ge\u00b7sich\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Heller sollten um sich schaun.", "tokens": ["Hel\u00b7ler", "soll\u00b7ten", "um", "sich", "schaun", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "APPR", "PRF", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Ha! Die Fr\u00fchlingsb\u00e4ume st\u00e4uben", "tokens": ["Ha", "!", "Die", "Fr\u00fch\u00b7lings\u00b7b\u00e4u\u00b7me", "st\u00e4u\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ITJ", "$.", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Duft'gen Bl\u00fctenschnee umher,", "tokens": ["Duft'\u00b7gen", "Bl\u00fc\u00b7ten\u00b7schnee", "um\u00b7her", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mich beleben, mich beleiben", "tokens": ["Mich", "be\u00b7le\u00b7ben", ",", "mich", "be\u00b7lei\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPER", "VVINF", "$,", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Will ich voll im Wonnemeer.", "tokens": ["Will", "ich", "voll", "im", "Won\u00b7ne\u00b7meer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJD", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Alles Heitre bl\u00fch' und Sch\u00f6ne!", "tokens": ["Al\u00b7les", "Heit\u00b7re", "bl\u00fch'", "und", "Sch\u00f6\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Spiele, s\u00fc\u00dfer Sonnenstrahl!", "tokens": ["Spie\u00b7le", ",", "s\u00fc\u00b7\u00dfer", "Son\u00b7nen\u00b7strahl", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "V\u00f6glein, singe deine T\u00f6ne!", "tokens": ["V\u00f6\u00b7glein", ",", "sin\u00b7ge", "dei\u00b7ne", "T\u00f6\u00b7ne", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "B\u00e4chlein, klinge hell zu Tal!", "tokens": ["B\u00e4ch\u00b7lein", ",", "klin\u00b7ge", "hell", "zu", "Tal", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ADJD", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Schm\u00e4lt mir nicht die alten Heiden,", "tokens": ["Schm\u00e4lt", "mir", "nicht", "die", "al\u00b7ten", "Hei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Denn ein Heide bin ich auch,", "tokens": ["Denn", "ein", "Hei\u00b7de", "bin", "ich", "auch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wann ich's Bl\u00fcmlein schau' der Heiden,", "tokens": ["Wann", "ich's", "Bl\u00fcm\u00b7lein", "schau'", "der", "Hei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wann ich's V\u00f6glein h\u00f6r' im Strauch.", "tokens": ["Wann", "ich's", "V\u00f6\u00b7glein", "h\u00f6r'", "im", "Strauch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.14": {"lin