{"dta.poem.20440": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Grabschrifft  \n  Mari\u00e6 de Medices.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Florentiner schlo\u00df gab mir die wiegen ein/", "tokens": ["Der", "Flo\u00b7ren\u00b7ti\u00b7ner", "schlo\u00df", "gab", "mir", "die", "wie\u00b7gen", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVFIN", "PPER", "ART", "VVFIN", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und der Pariser burg lie\u00df meinen ruhm erheben/", "tokens": ["Und", "der", "Pa\u00b7ri\u00b7ser", "burg", "lie\u00df", "mei\u00b7nen", "ruhm", "er\u00b7he\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mein liebster und sein ruhm wird in den wolcken schweben/", "tokens": ["Mein", "liebs\u00b7ter", "und", "sein", "ruhm", "wird", "in", "den", "wol\u00b7cken", "schwe\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "KON", "PPOSAT", "NN", "VAFIN", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Durch aller zeiten lauff mit des gestirnes schein.", "tokens": ["Durch", "al\u00b7ler", "zei\u00b7ten", "lauff", "mit", "des", "ge\u00b7stir\u00b7nes", "schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Zwey meiner eydame sah\u2019 ich gekr\u00f6net seyn/", "tokens": ["Zwey", "mei\u00b7ner", "ey\u00b7da\u00b7me", "sah'", "ich", "ge\u00b7kr\u00f6\u00b7net", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "PPOSAT", "NN", "VVFIN", "PPER", "VVPP", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und meinen \u00e4ltsten sohn in den geschichten leben/", "tokens": ["Und", "mei\u00b7nen", "\u00e4lts\u00b7ten", "sohn", "in", "den", "ge\u00b7schich\u00b7ten", "le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "APPR", "ART", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "In solcher w\u00fcrdigkeit (wer wird mir beyfall geben?)", "tokens": ["In", "sol\u00b7cher", "w\u00fcr\u00b7dig\u00b7keit", "(", "wer", "wird", "mir", "bey\u00b7fall", "ge\u00b7ben", "?", ")"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PIAT", "NN", "$(", "PWS", "VAFIN", "PPER", "ADV", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Starb ich in schn\u00f6der flucht: C\u00f6lln wird mein grabestein.", "tokens": ["Starb", "ich", "in", "schn\u00f6\u00b7der", "flucht", ":", "C\u00f6lln", "wird", "mein", "gra\u00b7be\u00b7stein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "VVFIN", "$.", "NE", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.9": {"text": "C\u00f6lln aller st\u00e4dte licht/ so sich in Deutschland finden;", "tokens": ["C\u00f6lln", "al\u00b7ler", "st\u00e4d\u00b7te", "licht", "/", "so", "sich", "in", "Deutschland", "fin\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PIAT", "ADJA", "NN", "$(", "ADV", "PRF", "APPR", "NE", "VVINF", "$."], "meter": "-+-+-+-+-++-", "measure": "unknown.measure.hexa"}, "line.10": {"text": "Steh\u2019 stille/ wandersmann; wo iemand will ergr\u00fcnden", "tokens": ["Steh'", "stil\u00b7le", "/", "wan\u00b7ders\u00b7mann", ";", "wo", "ie\u00b7mand", "will", "er\u00b7gr\u00fcn\u00b7den"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "$(", "PWAV", "$.", "PWAV", "PIS", "VMFIN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Den traurigen verlauff der noth/ so mich ber\u00fchrt/", "tokens": ["Den", "trau\u00b7ri\u00b7gen", "ver\u00b7lauff", "der", "noth", "/", "so", "mich", "be\u00b7r\u00fchrt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$(", "ADV", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Der wisse/ da\u00df das grab/ mu\u00df ein beh\u00e4ltni\u00df werden/", "tokens": ["Der", "wis\u00b7se", "/", "da\u00df", "das", "grab", "/", "mu\u00df", "ein", "be\u00b7h\u00e4lt\u00b7ni\u00df", "wer\u00b7den", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "KOUS", "ART", "NN", "$(", "VMFIN", "ART", "NN", "VAINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Der F\u00fcrstin/ derer blut die gantze welt regiert/", "tokens": ["Der", "F\u00fcrs\u00b7tin", "/", "de\u00b7rer", "blut", "die", "gant\u00b7ze", "welt", "re\u00b7giert", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PDS", "NN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und endlich nicht besa\u00df auch einer hand voll erden.", "tokens": ["Und", "end\u00b7lich", "nicht", "be\u00b7sa\u00df", "auch", "ei\u00b7ner", "hand", "voll", "er\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PTKNEG", "VVFIN", "ADV", "ART", "NN", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}