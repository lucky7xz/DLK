{"textgrid.poem.47278": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Das M\u00e4nnlein in der Gans", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das M\u00e4nnlein ging spazieren einmal", "tokens": ["Das", "M\u00e4nn\u00b7lein", "ging", "spa\u00b7zie\u00b7ren", "ein\u00b7mal"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "VVFIN", "ADV"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Auf dem Dach, ei seht doch!", "tokens": ["Auf", "dem", "Dach", ",", "ei", "seht", "doch", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "NE", "VVFIN", "ADV", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Das M\u00e4nnlein ist hurtig, das Dach ist schmal,", "tokens": ["Das", "M\u00e4nn\u00b7lein", "ist", "hur\u00b7tig", ",", "das", "Dach", "ist", "schmal", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Gib acht, es f\u00e4llt noch.", "tokens": ["Gib", "acht", ",", "es", "f\u00e4llt", "noch", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "CARD", "$,", "PPER", "VVFIN", "ADV", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Eh' sich's versieht, f\u00e4llt's vom Dach herunter", "tokens": ["Eh'", "sich's", "ver\u00b7sieht", ",", "f\u00e4llt's", "vom", "Dach", "her\u00b7un\u00b7ter"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "VVFIN", "$,", "VVFIN", "APPRART", "NN", "APZR"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.6": {"text": "Und bricht den Hals nicht, das ist ein Wunder.", "tokens": ["Und", "bricht", "den", "Hals", "nicht", ",", "das", "ist", "ein", "Wun\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKNEG", "$,", "PDS", "VAFIN", "ART", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Unter dem Dach steht ein Wasserzuber,", "tokens": ["Un\u00b7ter", "dem", "Dach", "steht", "ein", "Was\u00b7ser\u00b7zu\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Hineinf\u00e4llt's nicht schlecht;", "tokens": ["Hin\u00b7ein\u00b7f\u00e4llt's", "nicht", "schlecht", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "ADJD", "$."], "meter": "--+-+", "measure": "anapaest.init"}, "line.3": {"text": "Da wird es na\u00df \u00fcber und \u00fcber,", "tokens": ["Da", "wird", "es", "na\u00df", "\u00fc\u00b7ber", "und", "\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "APPR", "KON", "APPR", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Ei, das geschieht ihm recht.", "tokens": ["Ei", ",", "das", "ge\u00b7schieht", "ihm", "recht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PDS", "VVFIN", "PPER", "ADJD", "$."], "meter": "---+-+", "measure": "unknown.measure.di"}, "line.5": {"text": "Da kommt die Gans gelaufen,", "tokens": ["Da", "kommt", "die", "Gans", "ge\u00b7lau\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die wird's M\u00e4nnlein saufen.", "tokens": ["Die", "wird's", "M\u00e4nn\u00b7lein", "sau\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Die Gans hat's M\u00e4nnlein 'nuntergeschluckt,", "tokens": ["Die", "Gans", "hat's", "M\u00e4nn\u00b7lein", "'n\u00b7un\u00b7ter\u00b7ge\u00b7schluckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "NN", "VVFIN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sie hat einen guten Magen;", "tokens": ["Sie", "hat", "ei\u00b7nen", "gu\u00b7ten", "Ma\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber das M\u00e4nnlein hat sie doch gedruckt,", "tokens": ["A\u00b7ber", "das", "M\u00e4nn\u00b7lein", "hat", "sie", "doch", "ge\u00b7druckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "Das wollt' ich sagen.", "tokens": ["Das", "wollt'", "ich", "sa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Da schreit die Gans ganz j\u00e4mmerlich;", "tokens": ["Da", "schreit", "die", "Gans", "ganz", "j\u00e4m\u00b7mer\u00b7lich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das ist der K\u00f6chin \u00e4rgerlich.", "tokens": ["Das", "ist", "der", "K\u00f6\u00b7chin", "\u00e4r\u00b7ger\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Die K\u00f6chin wetzt das Messer,", "tokens": ["Die", "K\u00f6\u00b7chin", "wetzt", "das", "Mes\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sonst schneidt's ja nicht:", "tokens": ["Sonst", "schneidt's", "ja", "nicht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PTKNEG", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Die Gans schreit so, es ist nicht besser,", "tokens": ["Die", "Gans", "schreit", "so", ",", "es", "ist", "nicht", "bes\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$,", "PPER", "VAFIN", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Als da\u00df man sie sticht;", "tokens": ["Als", "da\u00df", "man", "sie", "sticht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PIS", "PPER", "VVFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Wir wollen sie nehmen und schlachten", "tokens": ["Wir", "wol\u00b7len", "sie", "neh\u00b7men", "und", "schlach\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "VVINF", "KON", "VVFIN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Zum Braten auf Weihnachten.", "tokens": ["Zum", "Bra\u00b7ten", "auf", "Weih\u00b7nach\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Sie rupft die Gans und nimmt sie aus", "tokens": ["Sie", "rupft", "die", "Gans", "und", "nimmt", "sie", "aus"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "KON", "VVFIN", "PPER", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und br\u00e4t sie,", "tokens": ["Und", "br\u00e4t", "sie", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Aber das M\u00e4nnlein darf nicht 'raus,", "tokens": ["A\u00b7ber", "das", "M\u00e4nn\u00b7lein", "darf", "nicht", "'raus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VMFIN", "PTKNEG", "NE", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Versteht sich.", "tokens": ["Ver\u00b7steht", "sich", "."], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PRF", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Die Gans wird eben gebraten;", "tokens": ["Die", "Gans", "wird", "e\u00b7ben", "ge\u00b7bra\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Was kann's dem M\u00e4nnlein schaden?", "tokens": ["Was", "kann's", "dem", "M\u00e4nn\u00b7lein", "scha\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Weihnachten kommt die Gans auf den Tisch", "tokens": ["Weih\u00b7nach\u00b7ten", "kommt", "die", "Gans", "auf", "den", "Tisch"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Im Pf\u00e4nnlein;", "tokens": ["Im", "Pf\u00e4nn\u00b7lein", ";"], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Der Vater thut sie 'raus und zerschneid't sie frisch.", "tokens": ["Der", "Va\u00b7ter", "thut", "sie", "'raus", "und", "zer\u00b7schneid't", "sie", "frisch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "NE", "KON", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Und das M\u00e4nnlein?", "tokens": ["Und", "das", "M\u00e4nn\u00b7lein", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Wie die Gans ist zerschnitten,", "tokens": ["Wie", "die", "Gans", "ist", "zer\u00b7schnit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Kriecht's M\u00e4nnlein aus der Mitten.", "tokens": ["Kriecht's", "M\u00e4nn\u00b7lein", "aus", "der", "Mit\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Da springt der Vater vom Tisch auf,", "tokens": ["Da", "springt", "der", "Va\u00b7ter", "vom", "Tisch", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Da wird der Stuhl leer;", "tokens": ["Da", "wird", "der", "Stuhl", "leer", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Da setzt das M\u00e4nnlein sich drauf", "tokens": ["Da", "setzt", "das", "M\u00e4nn\u00b7lein", "sich", "drauf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "PRF", "PAV"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Und macht sich \u00fcber die Gans her.", "tokens": ["Und", "macht", "sich", "\u00fc\u00b7ber", "die", "Gans", "her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Es sagt: \u00bbDu hast mich gefressen,", "tokens": ["Es", "sagt", ":", "\u00bb", "Du", "hast", "mich", "ge\u00b7fres\u00b7sen", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "$(", "PPER", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Jetzt will ich daf\u00fcr dich essen.\u00ab", "tokens": ["Jetzt", "will", "ich", "da\u00b7f\u00fcr", "dich", "es\u00b7sen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PAV", "PPER", "VVINF", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Da i\u00dft das M\u00e4nnlein gewaltig drauf los,", "tokens": ["Da", "i\u00dft", "das", "M\u00e4nn\u00b7lein", "ge\u00b7wal\u00b7tig", "drauf", "los", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJD", "PAV", "PTKVZ", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Als w\u00e4ren's seiner sieben;", "tokens": ["Als", "w\u00e4\u00b7ren's", "sei\u00b7ner", "sie\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPOSAT", "CARD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da essen wir alle dem M\u00e4nnlein zum Trotz,", "tokens": ["Da", "es\u00b7sen", "wir", "al\u00b7le", "dem", "M\u00e4nn\u00b7lein", "zum", "Trotz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIS", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Da ist nichts \u00fcbergeblieben", "tokens": ["Da", "ist", "nichts", "\u00fc\u00b7ber\u00b7ge\u00b7blie\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PIS", "VVINF"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Von der ganzen Gans, als ein T\u00e4tzlein,", "tokens": ["Von", "der", "gan\u00b7zen", "Gans", ",", "als", "ein", "T\u00e4tz\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "KOUS", "ART", "NN", "$,"], "meter": "+-+-+-++-", "measure": "unknown.measure.penta"}, "line.6": {"text": "Das kriegen dort hinten die K\u00e4tzlein.", "tokens": ["Das", "krie\u00b7gen", "dort", "hin\u00b7ten", "die", "K\u00e4tz\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+--+-+++", "measure": "iambic.penta.relaxed"}}, "stanza.9": {"line.1": {"text": "Nichts kriegt die Maus,", "tokens": ["Nichts", "kriegt", "die", "Maus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Das M\u00e4rlein ist aus.", "tokens": ["Das", "M\u00e4r\u00b7lein", "ist", "aus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Was ist denn das?", "tokens": ["Was", "ist", "denn", "das", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "PDS", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Ein Weihnachts-Spa\u00df;", "tokens": ["Ein", "Weih\u00b7nachts\u00b7Spa\u00df", ";"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Aufs Neujahr lernst", "tokens": ["Aufs", "Neu\u00b7jahr", "lernst"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Du, was?", "tokens": ["Du", ",", "was", "?"], "token_info": ["word", "punct", "word", "punct"], "pos": ["PPER", "$,", "PWS", "$."], "meter": "-+", "measure": "iambic.single"}, "line.7": {"text": "Den Ernst.", "tokens": ["Den", "Ernst", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.10": {"line.1": {"text": "Das M\u00e4nnlein ging spazieren einmal", "tokens": ["Das", "M\u00e4nn\u00b7lein", "ging", "spa\u00b7zie\u00b7ren", "ein\u00b7mal"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "VVFIN", "ADV"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Auf dem Dach, ei seht doch!", "tokens": ["Auf", "dem", "Dach", ",", "ei", "seht", "doch", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "NE", "VVFIN", "ADV", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Das M\u00e4nnlein ist hurtig, das Dach ist schmal,", "tokens": ["Das", "M\u00e4nn\u00b7lein", "ist", "hur\u00b