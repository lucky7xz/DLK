{"dta.poem.21194": {"metadata": {"author": {"name": "Zachariae, Justus Friedrich Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-20676-2", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Wehmuth weint der Menschlichkeit zu", "tokens": ["Die", "Weh\u00b7muth", "weint", "der", "Menschlich\u00b7keit", "zu"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PTKZU"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ehren\nAuch in der Ferne bittre Z\u00e4hren,\nWenn sie den Tod des Freundes h\u00f6rt:\nAllein wie wird ihr Schmerz vermehrt,\nWenn sie sich selber auf sein Grabmaal lehnet,\nUnd dessen Todtenstaub bethr\u00e4net,\nDen sie noch jenseit des Grabes verehrt.", "tokens": ["Eh\u00b7ren", "Auch", "in", "der", "Fer\u00b7ne", "bitt\u00b7re", "Z\u00e4h\u00b7ren", ",", "Wenn", "sie", "den", "Tod", "des", "Freun\u00b7des", "h\u00f6rt", ":", "Al\u00b7lein", "wie", "wird", "ihr", "Schmerz", "ver\u00b7mehrt", ",", "Wenn", "sie", "sich", "sel\u00b7ber", "auf", "sein", "Grab\u00b7maal", "leh\u00b7net", ",", "Und", "des\u00b7sen", "Tod\u00b7ten\u00b7staub", "be\u00b7th\u00b7r\u00e4\u00b7net", ",", "Den", "sie", "noch", "jen\u00b7seit", "des", "Gra\u00b7bes", "ver\u00b7ehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "ART", "NN", "ADJA", "NN", "$,", "KOUS", "PPER", "ART", "NN", "ART", "NN", "VVFIN", "$.", "ADV", "KOKOM", "VAFIN", "PPOSAT", "NN", "ADJD", "$,", "KOUS", "PPER", "PRF", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$,", "KON", "PRELAT", "NN", "VVFIN", "$,", "ART", "PPER", "ADV", "APPR", "ART", "NN", "VVPP", "$."], "meter": "+--+-+-+-+--+-+-+-+-+-+-+-+-+-+-+-+-+-+--+-+-+--+--+--+--+", "measure": "iambic.octa.plus.invert"}, "line.3": {"text": "So traurt der Christ mit bangem Herzen,", "tokens": ["So", "traurt", "der", "Christ", "mit", "ban\u00b7gem", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn er, Messias, deine Schmerzen", "tokens": ["Wenn", "er", ",", "Mes\u00b7si\u00b7as", ",", "dei\u00b7ne", "Schmer\u00b7zen"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "$,", "NE", "$,", "PPOSAT", "NN"], "meter": "+-+-++-+-", "measure": "unknown.measure.penta"}, "line.5": {"text": "In heiligen Geschichten h\u00f6rt:", "tokens": ["In", "hei\u00b7li\u00b7gen", "Ge\u00b7schich\u00b7ten", "h\u00f6rt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Allein, wie wird die Andacht nicht vermehrt,", "tokens": ["Al\u00b7lein", ",", "wie", "wird", "die", "An\u00b7dacht", "nicht", "ver\u00b7mehrt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "VAFIN", "ART", "NN", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Wenn Golgatha sich selbst ihm zeiget,", "tokens": ["Wenn", "Gol\u00b7ga\u00b7tha", "sich", "selbst", "ihm", "zei\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "PRF", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Er selbst hinab zu deinem Grabe steiget,", "tokens": ["Er", "selbst", "hin\u00b7ab", "zu", "dei\u00b7nem", "Gra\u00b7be", "stei\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Und deinen Tod darinnen verehrt!", "tokens": ["Und", "dei\u00b7nen", "Tod", "da\u00b7rin\u00b7nen", "ver\u00b7ehrt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}}}}