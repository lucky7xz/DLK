{"textgrid.poem.55468": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "49", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Will sich's wohl ziemen, dir zum zweiten Male", "tokens": ["Will", "sich's", "wohl", "zie\u00b7men", ",", "dir", "zum", "zwei\u00b7ten", "Ma\u00b7le"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VMFIN", "PIS", "ADV", "VVINF", "$,", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dieselbe Gabe festlich darzubringen?", "tokens": ["Die\u00b7sel\u00b7be", "Ga\u00b7be", "fest\u00b7lich", "dar\u00b7zu\u00b7brin\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ADJD", "PAV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Den Dichtertrank in deiner eignen Schale,", "tokens": ["Den", "Dich\u00b7ter\u00b7trank", "in", "dei\u00b7ner", "eig\u00b7nen", "Scha\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und nur dazu das alte Lied zu singen?", "tokens": ["Und", "nur", "da\u00b7zu", "das", "al\u00b7te", "Lied", "zu", "sin\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PAV", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "So sei es denn! \u2013 es bleiben alte Lieder", "tokens": ["So", "sei", "es", "denn", "!", "\u2013", "es", "blei\u00b7ben", "al\u00b7te", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "$.", "$(", "PPER", "VVINF", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Den Christgemeinden wie gewohnt erbaulich;", "tokens": ["Den", "Christ\u00b7ge\u00b7mein\u00b7den", "wie", "ge\u00b7wohnt", "er\u00b7bau\u00b7lich", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KOKOM", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und h\u00f6rt er Freundes Wunsch und Segen wieder,", "tokens": ["Und", "h\u00f6rt", "er", "Freun\u00b7des", "Wunsch", "und", "Se\u00b7gen", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "NN", "NN", "KON", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Er findet sie wie immer lieb und traulich.", "tokens": ["Er", "fin\u00b7det", "sie", "wie", "im\u00b7mer", "lieb", "und", "trau\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "KOKOM", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Will sich's wohl ziemen, dir zum zweiten Male", "tokens": ["Will", "sich's", "wohl", "zie\u00b7men", ",", "dir", "zum", "zwei\u00b7ten", "Ma\u00b7le"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VMFIN", "PIS", "ADV", "VVINF", "$,", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dieselbe Gabe festlich darzubringen?", "tokens": ["Die\u00b7sel\u00b7be", "Ga\u00b7be", "fest\u00b7lich", "dar\u00b7zu\u00b7brin\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ADJD", "PAV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text