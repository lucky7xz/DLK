{"textgrid.poem.67276": {"metadata": {"author": {"name": "Stolberg, Christian Graf zu", "birth": "N.A.", "death": "N.A."}, "title": "1L: Schwarzburgs f\u00fcrstliche Tochter, ich gr\u00fc\u00dfe Dich! Freundliches Auges", "genre": "verse", "period": "N.A.", "pub_year": 1784, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schwarzburgs f\u00fcrstliche Tochter, ich gr\u00fc\u00dfe Dich! Freundliches Auges", "tokens": ["Schwarz\u00b7burgs", "f\u00fcrst\u00b7li\u00b7che", "Toch\u00b7ter", ",", "ich", "gr\u00fc\u00b7\u00dfe", "Dich", "!", "Freund\u00b7li\u00b7ches", "Au\u00b7ges"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "ADJA", "NN", "$,", "PPER", "VVFIN", "PPER", "$.", "ADJA", "NN"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Nimm aus der Hand mir den Strau\u00df, den ich Dir pfl\u00fcckte zum Fest.", "tokens": ["Nimm", "aus", "der", "Hand", "mir", "den", "Strau\u00df", ",", "den", "ich", "Dir", "pfl\u00fcck\u00b7te", "zum", "Fest", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "PPER", "ART", "NE", "$,", "PRELS", "PPER", "PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Liebliche Blume du Selbst, sei hold den Blumen der Muse,", "tokens": ["Lieb\u00b7li\u00b7che", "Blu\u00b7me", "du", "Selbst", ",", "sei", "hold", "den", "Blu\u00b7men", "der", "Mu\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPER", "ADV", "$,", "VAFIN", "ADJD", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Hold dem G\u00e4rtner noch mehr, ihm, der sie w\u00e4hlte f\u00fcr Dich!", "tokens": ["Hold", "dem", "G\u00e4rt\u00b7ner", "noch", "mehr", ",", "ihm", ",", "der", "sie", "w\u00e4hl\u00b7te", "f\u00fcr", "Dich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "ADV", "ADV", "$,", "PPER", "$,", "PRELS", "PPER", "VVFIN", "APPR", "PPER", "$."], "meter": "+-+------+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Wisse, nicht fremd' ist er Dir, seit alterth\u00fcmlicher Urzeit", "tokens": ["Wis\u00b7se", ",", "nicht", "fremd'", "ist", "er", "Dir", ",", "seit", "al\u00b7tert\u00b7h\u00fcm\u00b7li\u00b7cher", "Ur\u00b7zeit"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PTKNEG", "ADJD", "VAFIN", "PPER", "PPER", "$,", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.6": {"text": "Einigte Sippschaftsband unser verb\u00fcndet Geschlecht! \u2013", "tokens": ["Ei\u00b7nig\u00b7te", "Sipp\u00b7schafts\u00b7band", "un\u00b7ser", "ver\u00b7b\u00fcn\u00b7det", "Ge\u00b7schlecht", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJA", "NN", "PPOSAT", "VVFIN", "NN", "$.", "$("], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.7": {"text": "G\u00fcntherin, la\u00df welken die Blumen! Trockne die Thr\u00e4nen", "tokens": ["G\u00fcn\u00b7the\u00b7rin", ",", "la\u00df", "wel\u00b7ken", "die", "Blu\u00b7men", "!", "Trock\u00b7ne", "die", "Thr\u00e4\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "VVIMP", "NN", "ART", "NN", "$.", "NN", "ART", "NN"], "meter": "+---+--+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.8": {"text": "Ihr, die des Herzens, des Geists Pflegerinn, Mutter Dir ist!", "tokens": ["Ihr", ",", "die", "des", "Her\u00b7zens", ",", "des", "Geists", "Pfle\u00b7ge\u00b7rinn", ",", "Mut\u00b7ter", "Dir", "ist", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "ART", "NN", "$,", "ART", "NN", "NN", "$,", "NN", "PPER", "VAFIN", "$."], "meter": "-+-+---+--+--+", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Alles vermag ja die Liebe, die tr\u00f6stende! Starreder Jammer", "tokens": ["Al\u00b7les", "ver\u00b7mag", "ja", "die", "Lie\u00b7be", ",", "die", "tr\u00f6s\u00b7ten\u00b7de", "!", "Star\u00b7re\u00b7der", "Jam\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PIS", "VVFIN", "ADV", "ART", "NN", "$,", "ART", "ADJA", "$.", "NN", "NN"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.10": {"text": "Thauet, geschmeidigt von ihr, sanfter in Wehmuth herab.", "tokens": ["Thau\u00b7et", ",", "ge\u00b7schmei\u00b7digt", "von", "ihr", ",", "sanf\u00b7ter", "in", "Weh\u00b7muth", "her\u00b7ab", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVPP", "APPR", "PPER", "$,", "ADJD", "APPR", "NN", "ADV", "$."], "meter": "+--+-+-+--++-+", "measure": "iambic.septa.invert"}, "line.11": {"text": "Ach, wie brennt mir im Herzen die Z\u00e4hre der Edlen! Doch Sie, die", "tokens": ["Ach", ",", "wie", "brennt", "mir", "im", "Her\u00b7zen", "die", "Z\u00e4h\u00b7re", "der", "Ed\u00b7len", "!", "Doch", "Sie", ",", "die"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ITJ", "$,", "PWAV", "VVFIN", "PPER", "APPRART", "NN", "ART", "NN", "ART", "NN", "$.", "KON", "PPER", "$,", "PRELS"], "meter": "--+--+--+--+-+--", "measure": "anapaest.tetra.plus"}, "line.12": {"text": "Tr\u00f6sten wir wollen, sie sch\u00f6pft selbst ja au