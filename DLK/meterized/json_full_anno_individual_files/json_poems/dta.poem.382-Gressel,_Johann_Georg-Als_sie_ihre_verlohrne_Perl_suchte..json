{"dta.poem.382": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Als sie ihre verlohrne Perl suchte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn ihr/ Cassandra, gleich die schlechte Perl verliehret", "tokens": ["Wenn", "ihr", "/", "Cas\u00b7sand\u00b7ra", ",", "gleich", "die", "schlech\u00b7te", "Perl", "ver\u00b7lieh\u00b7ret"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "$(", "NE", "$,", "ADV", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So bleibt doch eurer Pracht", "tokens": ["So", "bleibt", "doch", "eu\u00b7rer", "Pracht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Als wie dem Mond bey Nacht", "tokens": ["Als", "wie", "dem", "Mond", "bey", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "KOKOM", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die Wonne und der Prei\u00df/ ihr seyd sehr sch\u00f6n geziehret.", "tokens": ["Die", "Won\u00b7ne", "und", "der", "Prei\u00df", "/", "ihr", "seyd", "sehr", "sch\u00f6n", "ge\u00b7zieh\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$(", "PPER", "VAFIN", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die Augen sind Crystall/ Corall gl\u00fcht auf den Wangen/", "tokens": ["Die", "Au\u00b7gen", "sind", "Crys\u00b7tall", "/", "Co\u00b7rall", "gl\u00fcht", "auf", "den", "Wan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "NN", "$(", "NE", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die Perle deckt den Hals", "tokens": ["Die", "Per\u00b7le", "deckt", "den", "Hals"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Die Br\u00fcste ebenfals/", "tokens": ["Die", "Br\u00fcs\u00b7te", "e\u00b7ben\u00b7fals", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Weil ihr nun so geschm\u00fcckt/ was wolt ihr mehr verlangen?", "tokens": ["Weil", "ihr", "nun", "so", "ge\u00b7schm\u00fcckt", "/", "was", "wolt", "ihr", "mehr", "ver\u00b7lan\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "VVPP", "$(", "PWS", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}