{"textgrid.poem.37182": {"metadata": {"author": {"name": "Boie, Heinrich Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nicht Menschen nur, Du lenkst auch G\u00f6tter,", "genre": "verse", "period": "N.A.", "pub_year": 1780, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nicht Menschen nur, Du lenkst auch G\u00f6tter,", "tokens": ["Nicht", "Men\u00b7schen", "nur", ",", "Du", "lenkst", "auch", "G\u00f6t\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "ADV", "$,", "PPER", "VVFIN", "ADV", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "O Julia, und Wind und Wetter!", "tokens": ["O", "Ju\u00b7lia", ",", "und", "Wind", "und", "Wet\u00b7ter", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "KON", "NN", "KON", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Mit holdem Zauberlicht umgo\u00dfen", "tokens": ["Mit", "hol\u00b7dem", "Zau\u00b7ber\u00b7licht", "um\u00b7go\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "War schon ein Mond bei Dir verflo\u00dfen.", "tokens": ["War", "schon", "ein", "Mond", "bei", "Dir", "ver\u00b7flo\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Du l\u00e4cheltest: Hain, Meer, Gefilde", "tokens": ["Du", "l\u00e4\u00b7chel\u00b7test", ":", "Hain", ",", "Meer", ",", "Ge\u00b7fil\u00b7de"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "$.", "NE", "$,", "NN", "$,", "NN"], "meter": "-+--++-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ward mir ein Abglanz Deiner Milde,", "tokens": ["Ward", "mir", "ein", "Ab\u00b7glanz", "Dei\u00b7ner", "Mil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und was Dich liebet, was Dich ehret,", "tokens": ["Und", "was", "Dich", "lie\u00b7bet", ",", "was", "Dich", "eh\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVFIN", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Schien freundlich auch zu mir gekehret.", "tokens": ["Schien", "freund\u00b7lich", "auch", "zu", "mir", "ge\u00b7keh\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ADV", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Mein Geist erhub sich wonnetrunken;", "tokens": ["Mein", "Geist", "er\u00b7hub", "sich", "won\u00b7ne\u00b7trun\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Doch bald zum eignen Werth gesunken,", "tokens": ["Doch", "bald", "zum", "eig\u00b7nen", "Werth", "ge\u00b7sun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Entsagt' er jener hohen Freude", "tokens": ["Ent\u00b7sagt'", "er", "je\u00b7ner", "ho\u00b7hen", "Freu\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Und rief mir: \u00bbFa\u00dfe Mut und scheide!\u00ab", "tokens": ["Und", "rief", "mir", ":", "\u00bb", "Fa\u00b7\u00dfe", "Mut", "und", "schei\u00b7de", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "PPER", "$.", "$(", "ADJA", "NN", "KON", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Schon sah ich mich getrennt von allen", "tokens": ["Schon", "sah", "ich", "mich", "ge\u00b7trennt", "von", "al\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "VVPP", "APPR", "PIAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und still am krummen Ufer wallen,", "tokens": ["Und", "still", "am", "krum\u00b7men", "U\u00b7fer", "wal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPRART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Bald schwebend auf der blauen W\u00fcste,", "tokens": ["Bald", "schwe\u00b7bend", "auf", "der", "blau\u00b7en", "W\u00fcs\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gelandet bald an Holsteins K\u00fcste,", "tokens": ["Ge\u00b7lan\u00b7det", "bald", "an", "Hol\u00b7steins", "K\u00fcs\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "NE", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die heimisch zwar und traulich winket,", "tokens": ["Die", "hei\u00b7misch", "zwar", "und", "trau\u00b7lich", "win\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADV", "KON", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Doch minder Heimat mir jezt d\u00fcnket.", "tokens": ["Doch", "min\u00b7der", "Hei\u00b7mat", "mir", "jezt", "d\u00fcn\u00b7ket", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "NN", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Da l\u00e4chelst Du dem Gott der Winde \u2013", "tokens": ["Da", "l\u00e4\u00b7chelst", "Du", "dem", "Gott", "der", "Win\u00b7de", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und folgsam gleich Cytherens Kinde,", "tokens": ["Und", "folg\u00b7sam", "gleich", "Cy\u00b7the\u00b7rens", "Kin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "NE", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Das, seit es Deiner Macht gehuldigt,", "tokens": ["Das", ",", "seit", "es", "Dei\u00b7ner", "Macht", "ge\u00b7hul\u00b7digt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "KOUS", "PPER", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Kein Mensch der Unrat mehr beschuldigt,", "tokens": ["Kein", "Mensch", "der", "Un\u00b7rat", "mehr", "be\u00b7schul\u00b7digt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Hemmt er den Nord, dem schon die Wogen", "tokens": ["Hemmt", "er", "den", "Nord", ",", "dem", "schon", "die", "Wo\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "PRELS", "ADV", "ART", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.12": {"text": "Vor Moens Geklipp vor\u00fcber flogen,", "tokens": ["Vor", "Moens", "Ge\u00b7klipp", "vor\u00b7\u00fc\u00b7ber", "flo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NE", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Und hei\u00dft mit sanftem Wellenkr\u00e4useln", "tokens": ["Und", "hei\u00dft", "mit", "sanf\u00b7tem", "Wel\u00b7len\u00b7kr\u00e4u\u00b7seln"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Den S\u00fcdwind mir entgegen s\u00e4useln.", "tokens": ["Den", "S\u00fcd\u00b7wind", "mir", "ent\u00b7ge\u00b7gen", "s\u00e4u\u00b7seln", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "PTKVZ", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "\u00bbwarum, o Zauberin, erneuern", "tokens": ["\u00bb", "wa\u00b7rum", ",", "o", "Zau\u00b7be\u00b7rin", ",", "er\u00b7neu\u00b7ern"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word"], "pos": ["$(", "PWAV", "$,", "FM", "NN", "$,", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den Schmerz der Trennung?\u00ab \u2013", "tokens": ["Den", "Schmerz", "der", "Tren\u00b7nung", "?", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "NN", "ART", "NN", "$.", "$(", "$("], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "\u00bbkomm wir feiern,\u00ab", "tokens": ["\u00bb", "komm", "wir", "fei\u00b7ern", ",", "\u00ab"], "token_info": ["punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "PPER", "VVINF", "$,", "$("], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Antwortest Du mit s\u00fc\u00dfem Tone,", "tokens": ["Ant\u00b7wor\u00b7test", "Du", "mit", "s\u00fc\u00b7\u00dfem", "To\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbden Tag, da Ring und Myrthenkrone", "tokens": ["\u00bb", "den", "Tag", ",", "da", "Ring", "und", "Myr\u00b7then\u00b7kro\u00b7ne"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "ART", "NN", "$,", "KOUS", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mir segnend gab der frohe Hymen.", "tokens": ["Mir", "seg\u00b7nend", "gab", "der", "fro\u00b7he", "Hy\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Komm, Freund, Du sollst mit uns ihn r\u00fchmen.\u00ab", "tokens": ["Komm", ",", "Freund", ",", "Du", "sollst", "mit", "uns", "ihn", "r\u00fch\u00b7men", ".", "\u00ab"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "PPER", "VMFIN", "APPR", "PPER", "PPER", "VVINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Wohlan, mir heilig, drei mal heilig", "tokens": ["Wo\u00b7hlan", ",", "mir", "hei\u00b7lig", ",", "drei", "mal", "hei\u00b7lig"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "PPER", "ADJD", "$,", "CARD", "ADV", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sei dieser Tag! Auf! eilig eilig,", "tokens": ["Sei", "die\u00b7ser", "Tag", "!", "Auf", "!", "ei\u00b7lig", "ei\u00b7lig", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PDAT", "NN", "$.", "APPR", "$.", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wer je mit uns in goldnen Stunden", "tokens": ["Wer", "je", "mit", "uns", "in", "gold\u00b7nen", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "APPR", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Lieb und Freundschaft Gl\u00fcck empfunden!", "tokens": ["Der", "Lieb", "und", "Freund\u00b7schaft", "Gl\u00fcck", "emp\u00b7fun\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Herbei zum Tag des Gl\u00e4serklanges,", "tokens": ["Her\u00b7bei", "zum", "Tag", "des", "Gl\u00e4\u00b7ser\u00b7klan\u00b7ges", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Des Tanzes und des Brautgesanges!", "tokens": ["Des", "Tan\u00b7zes", "und", "des", "Braut\u00b7ge\u00b7san\u00b7ges", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Auf, la\u00dft uns singen, la\u00dft uns singen,", "tokens": ["Auf", ",", "la\u00dft", "uns", "sin\u00b7gen", ",", "la\u00dft", "uns", "sin\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "VVIMP", "PPER", "VVFIN", "$,", "VVIMP", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Indess die vollen Gl\u00e4ser klingen:", "tokens": ["In\u00b7dess", "die", "vol\u00b7len", "Gl\u00e4\u00b7ser", "klin\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Noch oft umarm an solchem Feste", "tokens": ["Noch", "oft", "um\u00b7arm", "an", "sol\u00b7chem", "Fes\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADJD", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Der beste Mann der Weiber beste!", "tokens": ["Der", "bes\u00b7te", "Mann", "der", "Wei\u00b7ber", "bes\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Doch Freundin! soll mit leichterm Herzen", "tokens": ["Doch", "Freun\u00b7din", "!", "soll", "mit", "leich\u00b7term", "Her\u00b7zen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "NN", "$.", "VMFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ich froher unter frohen scherzen,", "tokens": ["Ich", "fro\u00b7her", "un\u00b7ter", "fro\u00b7hen", "scher\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPR", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So la\u00df mich aus dem Zauberkreise", "tokens": ["So", "la\u00df", "mich", "aus", "dem", "Zau\u00b7ber\u00b7krei\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVIMP", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und gi