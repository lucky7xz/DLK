{"dta.poem.19886": {"metadata": {"author": {"name": "B\u00fcrger, Gottfried August", "birth": "N.A.", "death": "N.A."}, "title": "Das  \n  Neue Leben.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1778", "urn": "urn:nbn:de:kobv:b4-20090519672", "language": ["de:0.57", "en:0.42"], "booktitle": "B\u00fcrger, Gottfried August: Gedichte. G\u00f6ttingen, 1778."}, "poem": {"stanza.1": {"line.1": {"text": "Eia! Wie so wach und froh,               ", "tokens": ["Ei\u00b7a", "!", "Wie", "so", "wach", "und", "froh", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PWAV", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Froh und wach sind meine Sinnen!", "tokens": ["Froh", "und", "wach", "sind", "mei\u00b7ne", "Sin\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "O vor welcher Sonne floh", "tokens": ["O", "vor", "wel\u00b7cher", "Son\u00b7ne", "floh"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "PWAT", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Meines Lebens Nacht von hinnen?", "tokens": ["Mei\u00b7nes", "Le\u00b7bens", "Nacht", "von", "hin\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "APPR", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie so holden Grus entbot", "tokens": ["Wie", "so", "hol\u00b7den", "Grus", "ent\u00b7bot"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Mir das neue Morgenrot!", "tokens": ["Mir", "das", "neu\u00b7e", "Mor\u00b7gen\u00b7rot", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Mein erheitertes Gesicht", "tokens": ["Mein", "er\u00b7hei\u00b7ter\u00b7tes", "Ge\u00b7sicht"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Siehet Paradiese bl\u00fchen.", "tokens": ["Sie\u00b7het", "Pa\u00b7ra\u00b7die\u00b7se", "bl\u00fc\u00b7hen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Welche T\u00f6ne! H\u00f6r\u2019 ich nicht", "tokens": ["Wel\u00b7che", "T\u00f6\u00b7ne", "!", "H\u00f6r'", "ich", "nicht"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PWAT", "NN", "$.", "VVIMP", "PPER", "PTKNEG"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Aller Himmel Melodieen?", "tokens": ["Al\u00b7ler", "Him\u00b7mel", "Me\u00b7lo\u00b7di\u00b7e\u00b7en", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "O wie s\u00fcs erf\u00fclt die Luft", "tokens": ["O", "wie", "s\u00fcs", "er\u00b7f\u00fclt", "die", "Luft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "KOKOM", "NE", "VVFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Edens Amarantenduft!", "tokens": ["E\u00b7dens", "A\u00b7ma\u00b7ran\u00b7ten\u00b7duft", "!"], "token_info": ["word", "word", "punct"], "pos": ["NE", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Weingott, bist du mir so nah,", "tokens": ["Wein\u00b7gott", ",", "bist", "du", "mir", "so", "nah", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VAFIN", "PPER", "PPER", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Mir so nah bei jedem Male?", "tokens": ["Mir", "so", "nah", "bei", "je\u00b7dem", "Ma\u00b7le", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJD", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Kehrst du in Ambrosia", "tokens": ["Kehrst", "du", "in", "A\u00b7mbro\u00b7sia"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und in Nektar diese Schaale?", "tokens": ["Und", "in", "Nek\u00b7tar", "die\u00b7se", "Schaa\u00b7le", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "PDAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Geber der Ambrosia", "tokens": ["Ge\u00b7ber", "der", "A\u00b7mbro\u00b7sia"], "token_info": ["word", "word", "word"], "pos": ["NN", "ART", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.6": {"text": "Und des Nektars, mir so nah?", "tokens": ["Und", "des", "Nek\u00b7tars", ",", "mir", "so", "nah", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PPER", "ADV", "ADJD", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.4": {"line.1": {"text": "Liebe, deine Wunderkraft", "tokens": ["Lie\u00b7be", ",", "dei\u00b7ne", "Wun\u00b7der\u00b7kraft"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Hat mein Leben neu geboren,", "tokens": ["Hat", "mein", "Le\u00b7ben", "neu", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Hat zum Gl\u00fck der G\u00f6tterschaft", "tokens": ["Hat", "zum", "Gl\u00fck", "der", "G\u00f6t\u00b7ter\u00b7schaft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPRART", "NN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mich hienieden schon erkoren.", "tokens": ["Mich", "hien\u00b7ie\u00b7den", "schon", "er\u00b7ko\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ohne Wandel! ewig so!", "tokens": ["Oh\u00b7ne", "Wan\u00b7del", "!", "e\u00b7wig", "so", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$.", "ADJD", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ewig jung und ewig froh!", "tokens": ["E\u00b7wig", "jung", "und", "e\u00b7wig", "froh", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}