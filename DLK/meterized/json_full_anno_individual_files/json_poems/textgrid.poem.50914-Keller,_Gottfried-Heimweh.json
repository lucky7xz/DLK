{"textgrid.poem.50914": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Heimweh", "genre": "verse", "period": "N.A.", "pub_year": 1845, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "An den sch\u00f6nen Limmatborden,", "tokens": ["An", "den", "sch\u00f6\u00b7nen", "Lim\u00b7mat\u00b7bor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die so gr\u00fcn ins Wasser hangen,", "tokens": ["Die", "so", "gr\u00fcn", "ins", "Was\u00b7ser", "han\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Bin ich manches Mal gegangen,", "tokens": ["Bin", "ich", "man\u00b7ches", "Mal", "ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PIAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn die Erde jung geworden", "tokens": ["Wenn", "die", "Er\u00b7de", "jung", "ge\u00b7wor\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADJD", "VAPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und den Fr\u00fchlingsmantel wob,", "tokens": ["Und", "den", "Fr\u00fch\u00b7lings\u00b7man\u00b7tel", "wob", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wenn die Wasser voller klangen", "tokens": ["Wenn", "die", "Was\u00b7ser", "vol\u00b7ler", "klan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADJA", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und bis vor die F\u00fc\u00dfe drangen,", "tokens": ["Und", "bis", "vor", "die", "F\u00fc\u00b7\u00dfe", "dran\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Da\u00df der Pfad sich schwellend hob.", "tokens": ["Da\u00df", "der", "Pfad", "sich", "schwel\u00b7lend", "hob", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PRF", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wenn die Welle singend flieht,", "tokens": ["Wenn", "die", "Wel\u00b7le", "sin\u00b7gend", "flieht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist's, als h\u00f6re man Geschichten,", "tokens": ["Ist's", ",", "als", "h\u00f6\u00b7re", "man", "Ge\u00b7schich\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "VVFIN", "PIS", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Was im Oberland geschieht,", "tokens": ["Was", "im", "O\u00b7ber\u00b7land", "ge\u00b7schieht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Weit ins Niederland berichten;", "tokens": ["Weit", "ins", "Nie\u00b7der\u00b7land", "be\u00b7rich\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und wenn man stromaufw\u00e4rts sieht,", "tokens": ["Und", "wenn", "man", "strom\u00b7auf\u00b7w\u00e4rts", "sieht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "ADV", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Will es scheinen, da\u00df die ganze", "tokens": ["Will", "es", "schei\u00b7nen", ",", "da\u00df", "die", "gan\u00b7ze"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VMFIN", "PPER", "VVFIN", "$,", "KOUS", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Innre Schweiz im Firnenglanze", "tokens": ["Inn\u00b7re", "Schweiz", "im", "Fir\u00b7nen\u00b7glan\u00b7ze"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "NE", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Auf der Flut herniederzieht.", "tokens": ["Auf", "der", "Flut", "her\u00b7nie\u00b7der\u00b7zieht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ausgespannte Netze schimmern", "tokens": ["Aus\u00b7ge\u00b7spann\u00b7te", "Net\u00b7ze", "schim\u00b7mern"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zwischen bl\u00fctenwei\u00dfen B\u00e4umen,", "tokens": ["Zwi\u00b7schen", "bl\u00fc\u00b7ten\u00b7wei\u00b7\u00dfen", "B\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Perlend in der Sonne flimmern", "tokens": ["Per\u00b7lend", "in", "der", "Son\u00b7ne", "flim\u00b7mern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sie von feuchten Wassersch\u00e4umen.", "tokens": ["Sie", "von", "feuch\u00b7ten", "Was\u00b7ser\u00b7sch\u00e4u\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und ein Kn\u00e4blein schl\u00e4ft im Kahn,", "tokens": ["Und", "ein", "Kn\u00e4\u00b7blein", "schl\u00e4ft", "im", "Kahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.6": {"text": "Wiegend sich in jungen Tr\u00e4umen;", "tokens": ["Wie\u00b7gend", "sich", "in", "jun\u00b7gen", "Tr\u00e4u\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Ohne Hast und ohne S\u00e4umen", "tokens": ["Oh\u00b7ne", "Hast", "und", "oh\u00b7ne", "S\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "KON", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Schafft der Vater nebenan.", "tokens": ["Schafft", "der", "Va\u00b7ter", "ne\u00b7be\u00b7nan", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Ja, mit ruhig festem Schritte", "tokens": ["Ja", ",", "mit", "ru\u00b7hig", "fes\u00b7tem", "Schrit\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "APPR", "ADJD", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schreiten dort die M\u00e4nner hin!", "tokens": ["Schrei\u00b7ten", "dort", "die", "M\u00e4n\u00b7ner", "hin", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Klar und einfach ist die Sitte,", "tokens": ["Klar", "und", "ein\u00b7fach", "ist", "die", "Sit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Klug und ernst der freie Sinn.", "tokens": ["Klug", "und", "ernst", "der", "frei\u00b7e", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und in ihrer sichern Mitte", "tokens": ["Und", "in", "ih\u00b7rer", "si\u00b7chern", "Mit\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wuchsen Recht und Freiheit gro\u00df;", "tokens": ["Wuch\u00b7sen", "Recht", "und", "Frei\u00b7heit", "gro\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Das Gesetz schm\u00fcckt jede H\u00fctte,", "tokens": ["Das", "Ge\u00b7setz", "schm\u00fcckt", "je\u00b7de", "H\u00fct\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Jeden Herd ziert ein Gescho\u00df.", "tokens": ["Je\u00b7den", "Herd", "ziert", "ein", "Ge\u00b7scho\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Etwas Wein auch pflanzt der Bauer", "tokens": ["Et\u00b7was", "Wein", "auch", "pflanzt", "der", "Bau\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "ADV", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "An der Berge gr\u00fcnen F\u00fc\u00dfen,", "tokens": ["An", "der", "Ber\u00b7ge", "gr\u00fc\u00b7nen", "F\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn auch manchmal etwas sauer:", "tokens": ["Wenn", "auch", "manch\u00b7mal", "et\u00b7was", "sau\u00b7er", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADV", "ADV", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Arbeit wei\u00df ihn zu vers\u00fc\u00dfen.", "tokens": ["Ar\u00b7beit", "wei\u00df", "ihn", "zu", "ver\u00b7s\u00fc\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "L\u00e4ngst schon wohnt an jenen Fl\u00fcssen", "tokens": ["L\u00e4ngst", "schon", "wohnt", "an", "je\u00b7nen", "Fl\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "APPR", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Rasche Tat, entschlo\u00dfnes Handeln,", "tokens": ["Ra\u00b7sche", "Tat", ",", "ent\u00b7schlo\u00df\u00b7nes", "Han\u00b7deln", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Da\u00df vor ihrem heitren Wandeln", "tokens": ["Da\u00df", "vor", "ih\u00b7rem", "hei\u00b7tren", "Wan\u00b7deln"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Gram und Sorge schwinden m\u00fcssen.", "tokens": ["Gram", "und", "Sor\u00b7ge", "schwin\u00b7den", "m\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "VVINF", "VMINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Hier, an diesem fremden Strand,", "tokens": ["Hier", ",", "an", "die\u00b7sem", "frem\u00b7den", "Strand", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sind die Weine stark und s\u00fc\u00df,", "tokens": ["Sind", "die", "Wei\u00b7ne", "stark", "und", "s\u00fc\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und es gleicht das edle Land", "tokens": ["Und", "es", "gleicht", "das", "ed\u00b7le", "Land"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Auch wohl einem Paradies;", "tokens": ["Auch", "wohl", "ei\u00b7nem", "Pa\u00b7ra\u00b7dies", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Aber dumpf und ungewi\u00df", "tokens": ["A\u00b7ber", "dumpf", "und", "un\u00b7ge\u00b7wi\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Sind die Herzen und die Blicke,", "tokens": ["Sind", "die", "Her\u00b7zen", "und", "die", "Bli\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und verworrene Geschicke", "tokens": ["Und", "ver\u00b7wor\u00b7re\u00b7ne", "Ge\u00b7schi\u00b7cke"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Walten in der Finsternis!", "tokens": ["Wal\u00b7ten", "in", "der", "Fins\u00b7ter\u00b7nis", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "An den sch\u00f6nen Limmatborden,", "tokens": ["An", "den", "sch\u00f6\u00b7nen", "Lim\u00b7mat\u00b7bor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die so gr\u00fcn ins Wasser hangen,", "tokens": ["Die", "so", "gr\u00fcn", "ins", "Was\u00b7ser", "han\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Bin ich manches Mal gegangen,", "tokens": ["Bin", "ich", "man\u00b7ches", "Mal", "ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PIAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn die Erde jung geworden", "tokens": ["Wenn", "die", "Er\u00b7de", "jung", "ge\u00b7wor\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADJD", "VAPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und den Fr\u00fchlingsmantel wob,", "tokens": [