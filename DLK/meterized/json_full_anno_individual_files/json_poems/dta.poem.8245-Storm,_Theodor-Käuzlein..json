{"dta.poem.8245": {"metadata": {"author": {"name": "Storm, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "K\u00e4uzlein.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1852", "urn": "urn:nbn:de:kobv:b4-200905196098", "language": ["de:0.99"], "booktitle": "Storm, Theodor: Gedichte. Kiel, 1852."}, "poem": {"stanza.1": {"line.1": {"text": "Da sitzt der Kauz im Ulmenbaum,", "tokens": ["Da", "sitzt", "der", "Kauz", "im", "Ul\u00b7men\u00b7baum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und heult und heult im Ulmenbaum.", "tokens": ["Und", "heult", "und", "heult", "im", "Ul\u00b7men\u00b7baum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Welt hat f\u00fcr uns beide Raum!", "tokens": ["Die", "Welt", "hat", "f\u00fcr", "uns", "bei\u00b7de", "Raum", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Was heult der Kauz im Ulmenbaum", "tokens": ["Was", "heult", "der", "Kauz", "im", "Ul\u00b7men\u00b7baum"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Von Sterben und von Sterben?", "tokens": ["Von", "Ster\u00b7ben", "und", "von", "Ster\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und \u00fcber'n Weg die Nachtigall,", "tokens": ["Und", "\u00fc\u00b7ber'n", "Weg", "die", "Nach\u00b7ti\u00b7gall", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gen\u00fcber pfeift die Nachtigall.", "tokens": ["Ge\u00b7n\u00fc\u00b7ber", "pfeift", "die", "Nach\u00b7ti\u00b7gall", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "O weh, die Lieb' ist gangen all!", "tokens": ["O", "weh", ",", "die", "Lieb'", "ist", "gan\u00b7gen", "all", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "ART", "NN", "VAFIN", "VVPP", "PIAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Was pfeift so s\u00fc\u00df die Nachtigall", "tokens": ["Was", "pfeift", "so", "s\u00fc\u00df", "die", "Nach\u00b7ti\u00b7gall"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ADV", "ADJD", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Von Liebe und von Liebe?", "tokens": ["Von", "Lie\u00b7be", "und", "von", "Lie\u00b7be", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Zur Rechten hell ein Liebeslied,", "tokens": ["Zur", "Rech\u00b7ten", "hell", "ein", "Lie\u00b7bes\u00b7lied", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zur Linken grell ein Sterbelied!", "tokens": ["Zur", "Lin\u00b7ken", "grell", "ein", "Ster\u00b7be\u00b7lied", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ach, bleibt denn nichts, wenn Liebe schied,", "tokens": ["Ach", ",", "bleibt", "denn", "nichts", ",", "wenn", "Lie\u00b7be", "schied", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVFIN", "ADV", "PIS", "$,", "KOUS", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Denn nichts, als nur ein Sterbelied", "tokens": ["Denn", "nichts", ",", "als", "nur", "ein", "Ster\u00b7be\u00b7lied"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PIS", "$,", "KOUS", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Kaum wegbreit noch hin\u00fcber?", "tokens": ["Kaum", "weg\u00b7breit", "noch", "hin\u00b7\u00fc\u00b7ber", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADV", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}