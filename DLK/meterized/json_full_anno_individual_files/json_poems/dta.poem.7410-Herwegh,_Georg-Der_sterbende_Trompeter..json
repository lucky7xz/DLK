{"dta.poem.7410": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Der sterbende Trompeter.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192430", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Teufel, da\u00df ich daniedersank!", "tokens": ["Der", "Teu\u00b7fel", ",", "da\u00df", "ich", "da\u00b7nie\u00b7der\u00b7sank", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wie werden die polnischen Lanzen,", "tokens": ["Wie", "wer\u00b7den", "die", "pol\u00b7ni\u00b7schen", "Lan\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Wie werden die Schwerter bei anderem Klang", "tokens": ["Wie", "wer\u00b7den", "die", "Schwer\u00b7ter", "bei", "an\u00b7de\u00b7rem", "Klang"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Den Schlachtenreigen nun tanzen!", "tokens": ["Den", "Schlach\u00b7ten\u00b7rei\u00b7gen", "nun", "tan\u00b7zen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Wohl stand ich so oft, wohl stand ich so oft,", "tokens": ["Wohl", "stand", "ich", "so", "oft", ",", "wohl", "stand", "ich", "so", "oft", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "$,", "ADV", "VVFIN", "PPER", "ADV", "ADV", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Umbraust von grimmigen Wettern,", "tokens": ["Um\u00b7braust", "von", "grim\u00b7mi\u00b7gen", "Wet\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und habe gehofft, und habe gehofft,", "tokens": ["Und", "ha\u00b7be", "ge\u00b7hofft", ",", "und", "ha\u00b7be", "ge\u00b7hofft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "VVPP", "$,", "KON", "VAFIN", "VVPP", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "In befreiete L\u00fcfte zu schmettern;", "tokens": ["In", "be\u00b7frei\u00b7e\u00b7te", "L\u00fcf\u00b7te", "zu", "schmet\u00b7tern", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Ich habe gehofft, wenn der blutige Tod", "tokens": ["Ich", "ha\u00b7be", "ge\u00b7hofft", ",", "wenn", "der", "blu\u00b7ti\u00b7ge", "Tod"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "KOUS", "ART", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Auf sausenden Kugeln geflogen,", "tokens": ["Auf", "sau\u00b7sen\u00b7den", "Ku\u00b7geln", "ge\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Gehofft, wenn er donnernd um mich gedroht,", "tokens": ["Ge\u00b7hofft", ",", "wenn", "er", "don\u00b7nernd", "um", "mich", "ge\u00b7droht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "ADJD", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Gehofft, und hab' mich betrogen.", "tokens": ["Ge\u00b7hofft", ",", "und", "hab'", "mich", "be\u00b7tro\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Da\u00df die Seele leichter von hinnen zieht,", "tokens": ["Da\u00df", "die", "See\u00b7le", "leich\u00b7ter", "von", "hin\u00b7nen", "zieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJD", "APPR", "ADV", "VVFIN", "$,"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Kameraden, seid jetzo beschworen!", "tokens": ["Ka\u00b7me\u00b7ra\u00b7den", ",", "seid", "jet\u00b7zo", "be\u00b7schwo\u00b7ren", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VAFIN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Nehmt meine Trompete und blast mir das Lied:", "tokens": ["Nehmt", "mei\u00b7ne", "Trom\u00b7pe\u00b7te", "und", "blast", "mir", "das", "Lied", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "KON", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "\u201enoch ist Polen nicht verloren!\u201c", "tokens": ["\u201e", "noch", "ist", "Po\u00b7len", "nicht", "ver\u00b7lo\u00b7ren", "!", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "VAFIN", "NE", "PTKNEG", "VVPP", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Und blast mir das Lied, sonst Nichts, sonst Nichts,", "tokens": ["Und", "blast", "mir", "das", "Lied", ",", "sonst", "Nichts", ",", "sonst", "Nichts", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "$,", "ADV", "PIS", "$,", "ADV", "PIS", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und la\u00dft es mich sterbend noch hauchen!", "tokens": ["Und", "la\u00dft", "es", "mich", "ster\u00b7bend", "noch", "hau\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "PRF", "ADJD", "ADV", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Dann gebt sie mir wieder; am Tag des Gerichts", "tokens": ["Dann", "gebt", "sie", "mir", "wie\u00b7der", ";", "am", "Tag", "des", "Ge\u00b7richts"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADV", "$.", "APPRART", "NN", "ART", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Werd' ich die Trompete brauchen.", "tokens": ["Werd'", "ich", "die", "Trom\u00b7pe\u00b7te", "brau\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Denn wenn Gott den Toten auf Erden ruft,", "tokens": ["Denn", "wenn", "Gott", "den", "To\u00b7ten", "auf", "Er\u00b7den", "ruft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "NN", "ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wenn er will aus den Gr\u00e4bern sie schrecken,", "tokens": ["Wenn", "er", "will", "aus", "den", "Gr\u00e4\u00b7bern", "sie", "schre\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VMFIN", "APPR", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Da mu\u00df er zuerst aus ihrer Gruft", "tokens": ["Da", "mu\u00df", "er", "zu\u00b7erst", "aus", "ih\u00b7rer", "Gruft"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Doch die Trompeter erwecken.", "tokens": ["Doch", "die", "Trom\u00b7pe\u00b7ter", "er\u00b7we\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Das wird ein Tag der Freude, juchhei!", "tokens": ["Das", "wird", "ein", "Tag", "der", "Freu\u00b7de", ",", "juch\u00b7hei", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ART", "NN", "$,", "ITJ", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Wie spreng' ich den dr\u00fcckenden Rasen,", "tokens": ["Wie", "spreng'", "ich", "den", "dr\u00fc\u00b7cken\u00b7den", "Ra\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Um allen V\u00f6lkern der Erde herbei", "tokens": ["Um", "al\u00b7len", "V\u00f6l\u00b7kern", "der", "Er\u00b7de", "her\u00b7bei"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "PIAT", "NN", "ART", "NN", "PTKVZ"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Dann gegen die Russen zu blasen!", "tokens": ["Dann", "ge\u00b7gen", "die", "Rus\u00b7sen", "zu", "bla\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}}}}