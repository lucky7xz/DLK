", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "NN", "NE", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.85": {"text": "Mein Bluth streicht dein Geschlecht mit neuen Farben", "tokens": ["Mein", "Bluth", "streicht", "dein", "Ge\u00b7schlecht", "mit", "neu\u00b7en", "Far\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.86": {"text": "an/", "tokens": ["an", "/"], "token_info": ["word", "punct"], "pos": ["APPR", "$("], "meter": "+", "measure": "single.up"}, "line.87": {"text": "Soll denn alleine dich der Ruhm nicht beugen k\u00f6nnen/", "tokens": ["Soll", "denn", "al\u00b7lei\u00b7ne", "dich", "der", "Ruhm", "nicht", "beu\u00b7gen", "k\u00f6n\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ADV", "PPER", "ART", "NN", "PTKNEG", "VVINF", "VMINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.88": {"text": "Der sonst das Frauen Volck so leicht bezaubern kan?", "tokens": ["Der", "sonst", "das", "Frau\u00b7en", "Volck", "so", "leicht", "be\u00b7zau\u00b7bern", "kan", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "NN", "ADV", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.89": {"text": "Ich zeige keine Gunst die nur will heute wehren/", "tokens": ["Ich", "zei\u00b7ge", "kei\u00b7ne", "Gunst", "die", "nur", "will", "heu\u00b7te", "weh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "ART", "ADV", "VMFIN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.90": {"text": "Und wie manch Fligen-Wurm sich nur zw\u00f6lf Stunden", "tokens": ["Und", "wie", "manch", "Fli\u00b7gen\u00b7Wurm", "sich", "nur", "zw\u00f6lf", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PIAT", "NN", "PRF", "ADV", "CARD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.91": {"text": "speist/", "tokens": ["speist", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+", "measure": "single.up"}, "line.92": {"text": "Der Faden meiner Treu/ der l\u00e4st sich nicht verzehren/", "tokens": ["Der", "Fa\u00b7den", "mei\u00b7ner", "Treu", "/", "der", "l\u00e4st", "sich", "nicht", "ver\u00b7zeh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$(", "ART", "VVFIN", "PRF", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.93": {"text": "Und wilst du mehr als di\u00df/ dir soll was ehlich heist;", "tokens": ["Und", "wilst", "du", "mehr", "als", "di\u00df", "/", "dir", "soll", "was", "eh\u00b7lich", "heist", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PIAT", "KOKOM", "PDS", "$(", "PPER", "VMFIN", "PIS", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.94": {"text": "Du solst in meiner Scho\u00df nicht mit Verachtung sitzen/", "tokens": ["Du", "solst", "in", "mei\u00b7ner", "Scho\u00df", "nicht", "mit", "Ver\u00b7ach\u00b7tung", "sit\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PPOSAT", "NN", "PTKNEG", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.95": {"text": "Mein