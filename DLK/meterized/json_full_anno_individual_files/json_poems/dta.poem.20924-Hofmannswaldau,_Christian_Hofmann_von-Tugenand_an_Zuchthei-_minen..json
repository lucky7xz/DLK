{"dta.poem.20924": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Tugenand an Zuchthei-  \n minen.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "La\u00df Zuchtheimine dich mein Siegel nicht er-", "tokens": ["La\u00df", "Zucht\u00b7hei\u00b7mi\u00b7ne", "dich", "mein", "Sie\u00b7gel", "nicht", "er"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "NN", "PPER", "PPOSAT", "NN", "PTKNEG", "TRUNC"], "meter": "-+-+-+-+-++", "measure": "unknown.measure.hexa"}, "line.2": {"text": "schrecken/", "tokens": ["schre\u00b7cken", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Und nenne meine Hand nicht einen frembden", "tokens": ["Und", "nen\u00b7ne", "mei\u00b7ne", "Hand", "nicht", "ei\u00b7nen", "fremb\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "PTKNEG", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Gast/", "tokens": ["Gast", "/"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Der Adler/ den du schaust/ der kan dich nicht beflecken/", "tokens": ["Der", "Ad\u00b7ler", "/", "den", "du", "schaust", "/", "der", "kan", "dich", "nicht", "be\u00b7fle\u00b7cken", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "PPER", "VVFIN", "$(", "ART", "VMFIN", "PPER", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Er wird dir unterthan/ weil du sein Hertze hast.", "tokens": ["Er", "wird", "dir", "un\u00b7ter\u00b7than", "/", "weil", "du", "sein", "Hert\u00b7ze", "hast", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "$(", "KOUS", "PPER", "VAINF", "VVFIN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Er will dich wo er kan/ der Sonne gleiche f\u00fchren/", "tokens": ["Er", "will", "dich", "wo", "er", "kan", "/", "der", "Son\u00b7ne", "glei\u00b7che", "f\u00fch\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "PWAV", "PPER", "VMFIN", "$(", "ART", "NN", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Mein Fl\u00fcgel soll ein Schild f\u00fcr deinen Feinden seyn/", "tokens": ["Mein", "Fl\u00fc\u00b7gel", "soll", "ein", "Schild", "f\u00fcr", "dei\u00b7nen", "Fein\u00b7den", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Er will dein sch\u00f6nes Haubt mit einer Crone ziehren/", "tokens": ["Er", "will", "dein", "sch\u00f6\u00b7nes", "Haubt", "mit", "ei\u00b7ner", "Cro\u00b7ne", "zieh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "ADJA", "NN", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "F\u00fcr der sich scheuen soll der goldnen Sternen Schein.", "tokens": ["F\u00fcr", "der", "sich", "scheu\u00b7en", "soll", "der", "gold\u00b7nen", "Ster\u00b7nen", "Schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PRF", "VVINF", "VMFIN", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ich kenne deine Zucht/ und mache mir Gedancken/", "tokens": ["Ich", "ken\u00b7ne", "dei\u00b7ne", "Zucht", "/", "und", "ma\u00b7che", "mir", "Ge\u00b7dan\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$(", "KON", "VVFIN", "PPER", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Da\u00df Zuchtheimine nicht wird ohne Schrecken stehn/", "tokens": ["Da\u00df", "Zucht\u00b7hei\u00b7mi\u00b7ne", "nicht", "wird", "oh\u00b7ne", "Schre\u00b7cken", "stehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PTKNEG", "VAFIN", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Sie ist so sehr vertieft in ihrer Keuschheit Schrancken/", "tokens": ["Sie", "ist", "so", "sehr", "ver\u00b7tieft", "in", "ih\u00b7rer", "Keuschheit", "Schran\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "VVFIN", "APPR", "PPOSAT", "NN", "NN", "$("], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "Und will auf dieser Bahn mehr als behutsam gehn.", "tokens": ["Und", "will", "auf", "die\u00b7ser", "Bahn", "mehr", "als", "be\u00b7hut\u00b7sam", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "APPR", "PDAT", "NN", "PIAT", "KOKOM", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Sie nennet Buhlerey den Fall Strick zarter Jugendt/", "tokens": ["Sie", "nen\u00b7net", "Buh\u00b7le\u00b7rey", "den", "Fall", "Strick", "zar\u00b7ter", "Ju\u00b7gendt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "ART", "NN", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Sie schaut ein geiles Aug\u2019 als einen Jrrwisch an/", "tokens": ["Sie", "schaut", "ein", "gei\u00b7les", "Aug'", "als", "ei\u00b7nen", "Jrr\u00b7wisch", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "KOKOM", "ART", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Sie h\u00e4lt ein freches Wort vor R\u00e4uber wahrer Tu-", "tokens": ["Sie", "h\u00e4lt", "ein", "fre\u00b7ches", "Wort", "vor", "R\u00e4u\u00b7ber", "wah\u00b7rer", "Tu"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "APPR", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "gendt/", "tokens": ["gendt", "/"], "token_info": ["word", "punct"], "pos": ["VVPP", "$("], "meter": "-", "measure": "single.down"}, "line.19": {"text": "Und meinet da\u00df ein Traum sie auch beflecken kan.", "tokens": ["Und", "mei\u00b7net", "da\u00df", "ein", "Traum", "sie", "auch", "be\u00b7fle\u00b7cken", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KOUS", "ART", "NN", "PPER", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Die Bluhme so von Lieb und brennen wird genennet/", "tokens": ["Die", "Bluh\u00b7me", "so", "von", "Lieb", "und", "bren\u00b7nen", "wird", "ge\u00b7nen\u00b7net", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "NN", "KON", "VVINF", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Ist ihr den Dornen gleich und nicht vor ihren Krantz/", "tokens": ["Ist", "ihr", "den", "Dor\u00b7nen", "gleich", "und", "nicht", "vor", "ih\u00b7ren", "Krantz", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "ADV", "KON", "PTKNEG", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Sie meint das Liebest\u00fcck als eine Nessel brennet/", "tokens": ["Sie", "meint", "das", "Lie\u00b7be\u00b7st\u00fcck", "als", "ei\u00b7ne", "Nes\u00b7sel", "bren\u00b7net", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "KOKOM", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Und hei\u00dft den Venus Stern die Fackel ohne Glantz.", "tokens": ["Und", "hei\u00dft", "den", "Ve\u00b7nus", "Stern", "die", "Fa\u00b7ckel", "oh\u00b7ne", "Glantz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "NN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Doch Zuchtheimine la\u00df Verdacht und Kummer fah-", "tokens": ["Doch", "Zucht\u00b7hei\u00b7mi\u00b7ne", "la\u00df", "Ver\u00b7dacht", "und", "Kum\u00b7mer", "fah"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VVIMP", "NN", "KON", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "ren/", "tokens": ["ren", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "-", "measure": "single.down"}, "line.26": {"text": "Die Flamme die mich treibt/ ist reine gleich wie du/", "tokens": ["Die", "Flam\u00b7me", "die", "mich", "treibt", "/", "ist", "rei\u00b7ne", "gleich", "wie", "du", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "PPER", "VVFIN", "$(", "VAFIN", "ADJA", "ADV", "KOKOM", "PPER", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Nicht pr\u00fcfe so genau/ hier seyn nicht falsche Wahren/", "tokens": ["Nicht", "pr\u00fc\u00b7fe", "so", "ge\u00b7nau", "/", "hier", "seyn", "nicht", "fal\u00b7sche", "Wah\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "ADV", "ADJD", "$(", "ADV", "VAINF", "PTKNEG", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Es leget der Betrug hier kein Gewichte zu.", "tokens": ["Es", "le\u00b7get", "der", "Be\u00b7trug", "hier", "kein", "Ge\u00b7wich\u00b7te", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Es soll mir mehr an dir als Fleisch und Blut behagen/", "tokens": ["Es", "soll", "mir", "mehr", "an", "dir", "als", "Fleisch", "und", "Blut", "be\u00b7ha\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "APPR", "PPER", "KOUS", "NN", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Ich wei\u00df die Sch\u00f6nheit ist ein Gauckelspiel der Zeit/", "tokens": ["Ich", "wei\u00df", "die", "Sch\u00f6n\u00b7heit", "ist", "ein", "Gau\u00b7ckel\u00b7spiel", "der", "Zeit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "VAFIN", "ART", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Wir schauen sie vor uns fast stets zu Grabe tragen/", "tokens": ["Wir", "schau\u00b7en", "sie", "vor", "uns", "fast", "stets", "zu", "Gra\u00b7be", "tra\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PPER", "ADV", "ADV", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Und machen uns zur Gruft derselben Eitelkeit.", "tokens": ["Und", "ma\u00b7chen", "uns", "zur", "Gruft", "der\u00b7sel\u00b7ben", "Ei\u00b7tel\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "NN", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Ein ungeschm\u00fcckter Schmuck/ die Gleichheit der Ge-", "tokens": ["Ein", "un\u00b7ge\u00b7schm\u00fcck\u00b7ter", "Schmuck", "/", "die", "Gleich\u00b7heit", "der", "Ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "NN", "ART", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.34": {"text": "berden/", "tokens": ["ber\u00b7den", "/"], "token_info": ["word", "punct"], "pos": ["VVPP", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.35": {"text": "Ein Firnisloses Werck/ mit Amber unvermengt/", "tokens": ["Ein", "Fir\u00b7nis\u00b7lo\u00b7ses", "Werck", "/", "mit", "Am\u00b7ber", "un\u00b7ver\u00b7mengt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$(", "APPR", "NN", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Ein Blick/ der niemahls will durch Kunst verbessert", "tokens": ["Ein", "Blick", "/", "der", "nie\u00b7mahls", "will", "durch", "Kunst", "ver\u00b7bes\u00b7sert"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$(", "ART", "ADV", "VMFIN", "APPR", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.37": {"text": "werden/", "tokens": ["wer\u00b7den", "/"], "token_info": ["word", "punct"], "pos": ["VAINF", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.38": {"text": "An dem die Einfalt noch mit beyden Armen h\u00e4ngt.", "tokens": ["An", "dem", "die", "Ein\u00b7falt", "noch", "mit", "bey\u00b7den", "Ar\u00b7men", "h\u00e4ngt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "ADV", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Ein Purpur/ welchen Scham/ nicht Kunst hat ange-", "tokens": ["Ein", "Pur\u00b7pur", "/", "wel\u00b7chen", "Scham", "/", "nicht", "Kunst", "hat", "an\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$(", "PWAT", "NN", "$(", "PTKNEG", "NN", "VAFIN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.40": {"text": "strichen/", "tokens": ["stri\u00b7chen", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.41": {"text": "Ein Schnee der feurig ist und keine H\u00fclffe kennt/", "tokens": ["Ein", "Schnee", "der", "feu\u00b7rig", "ist", "und", "kei\u00b7ne", "H\u00fclf\u00b7fe", "kennt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJD", "VAFIN", "KON", "PIAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Ein Gang von welchem nicht die Sitsamkeit gewichen/", "tokens": ["Ein", "Gang", "von", "wel\u00b7chem", "nicht", "die", "Sit\u00b7sam\u00b7keit", "ge\u00b7wi\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PRELS", "PTKNEG", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Ein Auge so von Scham und nicht vor Liebe brennt.", "tokens": ["Ein", "Au\u00b7ge", "so", "von", "Scham", "und", "nicht", "vor", "Lie\u00b7be", "brennt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "NN", "KON", "PTKNEG", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Ein himmelreiner Geist/ wiewohl mit Zucht verm\u00e4h-", "tokens": ["Ein", "him\u00b7mel\u00b7rei\u00b7ner", "Geist", "/", "wie\u00b7wohl", "mit", "Zucht", "ver\u00b7m\u00e4h"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$(", "KOUS", "APPR", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "let/", "tokens": ["let", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "-", "measure": "single.down"}, "line.46": {"text": "Ein keusches Freundlich sein/ darau\u00df die Tugend lacht/", "tokens": ["Ein", "keu\u00b7sches", "Freund\u00b7lich", "sein", "/", "dar\u00b7au\u00df", "die", "Tu\u00b7gend", "lacht", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAINF", "$(", "PAV", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Ein Sinn so vor den Witz nur seine Fehler zehlet/", "tokens": ["Ein", "Sinn", "so", "vor", "den", "Witz", "nur", "sei\u00b7ne", "Feh\u00b7ler", "zeh\u00b7let", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "ART", "NN", "ADV", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Und sich durch dieses auch zu einem Engel macht.", "tokens": ["Und", "sich", "durch", "die\u00b7ses", "auch", "zu", "ei\u00b7nem", "En\u00b7gel", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "APPR", "PDAT", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.49": {"text": "Die\u00df ist der feste Grund von meinem reinen Feuer/", "tokens": ["Die\u00df", "ist", "der", "fes\u00b7te", "Grund", "von", "mei\u00b7nem", "rei\u00b7nen", "Feu\u00b7er", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.50": {"text": "Kein geiler Schwefel hat denselben mir erweckt/", "tokens": ["Kein", "gei\u00b7ler", "Schwe\u00b7fel", "hat", "den\u00b7sel\u00b7ben", "mir", "er\u00b7weckt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VAFIN", "PDS", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.51": {"text": "Du hast (sag ich zuviel?) O sch\u00f6nes Ungeheuer/", "tokens": ["Du", "hast", "(", "sag", "ich", "zu\u00b7viel", "?", ")", "O", "sch\u00f6\