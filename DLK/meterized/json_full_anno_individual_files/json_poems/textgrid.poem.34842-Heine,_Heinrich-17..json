{"textgrid.poem.34842": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "17.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit Br\u00fcnetten hat's eine Ende!", "tokens": ["Mit", "Br\u00fc\u00b7net\u00b7ten", "hat's", "ei\u00b7ne", "En\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+--++-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ich gerate dieses Jahr", "tokens": ["Ich", "ge\u00b7ra\u00b7te", "die\u00b7ses", "Jahr"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PDAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wieder in die blauen Augen,", "tokens": ["Wie\u00b7der", "in", "die", "blau\u00b7en", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Wieder in das blonde Haar.", "tokens": ["Wie\u00b7der", "in", "das", "blon\u00b7de", "Haar", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Die Blondine, die ich liebe,", "tokens": ["Die", "Blon\u00b7di\u00b7ne", ",", "die", "ich", "lie\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ist so fromm, so sanft, so mild!", "tokens": ["Ist", "so", "fromm", ",", "so", "sanft", ",", "so", "mild", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos