{"dta.poem.19482": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Erntegewitter.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "Ein j\u00e4her Blitz. Der Erntewagen schwankt.", "tokens": ["Ein", "j\u00e4\u00b7her", "Blitz", ".", "Der", "Ern\u00b7te\u00b7wa\u00b7gen", "schwankt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Aus seinen Garben fahren Dirnen auf", "tokens": ["Aus", "sei\u00b7nen", "Gar\u00b7ben", "fah\u00b7ren", "Dir\u00b7nen", "auf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "APPR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und springen schreiend in die Nacht hinab.", "tokens": ["Und", "sprin\u00b7gen", "schrei\u00b7end", "in", "die", "Nacht", "hin\u00b7ab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ein Blitz. Auf einer goldnen Garbe thront", "tokens": ["Ein", "Blitz", ".", "Auf", "ei\u00b7ner", "gold\u00b7nen", "Gar\u00b7be", "thront"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$.", "APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Noch unvertrieben eine frevle Maid,", "tokens": ["Noch", "un\u00b7ver\u00b7trie\u00b7ben", "ei\u00b7ne", "frev\u00b7le", "Maid", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Der das gel\u00f6ste Haar den Nacken peitscht.", "tokens": ["Der", "das", "ge\u00b7l\u00f6s\u00b7te", "Haar", "den", "Na\u00b7cken", "peitscht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Sie hebt das volle Glas mit nacktem Arm,", "tokens": ["Sie", "hebt", "das", "vol\u00b7le", "Glas", "mit", "nack\u00b7tem", "Arm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Als br\u00e4chte sie's der Gluth die sie umflammt,", "tokens": ["Als", "br\u00e4ch\u00b7te", "sie's", "der", "Gluth", "die", "sie", "um\u00b7flammt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "ART", "NN", "ART", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Und leert's auf einen Zug. Ins Dunkel wirft", "tokens": ["Und", "leert's", "auf", "ei\u00b7nen", "Zug", ".", "Ins", "Dun\u00b7kel", "wirft"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$.", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Sie's weit und gleitet ihrem Becher nach.", "tokens": ["Sie's", "weit", "und", "glei\u00b7tet", "ih\u00b7rem", "Be\u00b7cher", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "KON", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Ein Blitz. Zwei schwarze Rosse b\u00e4umen sich.", "tokens": ["Ein", "Blitz", ".", "Zwei", "schwar\u00b7ze", "Ros\u00b7se", "b\u00e4u\u00b7men", "sich", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "CARD", "ADJA", "NN", "VVFIN", "PRF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Die Peitsche knallt. Sie ziehen an. Vorbei.", "tokens": ["Die", "Peit\u00b7sche", "knallt", ".", "Sie", "zie\u00b7hen", "an", ".", "Vor\u00b7bei", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "PPER", "VVFIN", "PTKVZ", "$.", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}