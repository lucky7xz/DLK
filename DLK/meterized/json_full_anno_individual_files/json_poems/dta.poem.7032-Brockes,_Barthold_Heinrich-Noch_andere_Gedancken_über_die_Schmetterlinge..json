{"dta.poem.7032": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Noch andere Gedancken \u00fcber  \n die Schmetterlinge.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "J\u00fcngst sah' ich abermal, mit innigem Vergn\u00fcgen,", "tokens": ["J\u00fcngst", "sah'", "ich", "a\u00b7ber\u00b7mal", ",", "mit", "in\u00b7ni\u00b7gem", "Ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-++-+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Verschiedne Schmetterling in meinem Garten fliegen.", "tokens": ["Ver\u00b7schied\u00b7ne", "Schmet\u00b7ter\u00b7ling", "in", "mei\u00b7nem", "Gar\u00b7ten", "flie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Es schien die stille Lufft durch sie belebt,", "tokens": ["Es", "schien", "die", "stil\u00b7le", "Lufft", "durch", "sie", "be\u00b7lebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ja wenn bald hie, bald dort, die bunte Menge schwebt,", "tokens": ["Ja", "wenn", "bald", "hie", ",", "bald", "dort", ",", "die", "bun\u00b7te", "Men\u00b7ge", "schwebt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "KOUS", "ADV", "ADV", "$,", "ADV", "ADV", "$,", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So schien so gar die Lufft sich gleichsam zu bem\u00fchn,", "tokens": ["So", "schien", "so", "gar", "die", "Lufft", "sich", "gleich\u00b7sam", "zu", "be\u00b7m\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "ART", "NN", "PRF", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Nicht minder, als das Land den Blick auf sich zu ziehn,", "tokens": ["Nicht", "min\u00b7der", ",", "als", "das", "Land", "den", "Blick", "auf", "sich", "zu", "ziehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "$,", "KOUS", "ART", "NN", "ART", "NN", "APPR", "PRF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Nicht minder, als die Erd in bunter Pracht zu bl\u00fchn.", "tokens": ["Nicht", "min\u00b7der", ",", "als", "die", "Erd", "in", "bun\u00b7ter", "Pracht", "zu", "bl\u00fchn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "$,", "KOUS", "ART", "NN", "APPR", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Denn jeder Schmetterling, an sch\u00f6nen Farben reich,", "tokens": ["Denn", "je\u00b7der", "Schmet\u00b7ter\u00b7ling", ",", "an", "sch\u00f6\u00b7nen", "Far\u00b7ben", "reich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$,", "APPR", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Sieht einer fliegenden lebend\u2019gen Bluhmen gleich.", "tokens": ["Sieht", "ei\u00b7ner", "flie\u00b7gen\u00b7den", "le\u00b7ben\u00b7d'\u00b7gen", "Bluh\u00b7men", "gleich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "ADJA", "NN", "ADV", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Noch nicht genung, ich werd\u2019 noch mehr erg\u00f6tzt,", "tokens": ["Noch", "nicht", "ge\u00b7nung", ",", "ich", "werd'", "noch", "mehr", "er\u00b7g\u00f6tzt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "ADV", "$,", "PPER", "VAFIN", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Wenn manche sich auf bunte Bluhmen setzt;", "tokens": ["Wenn", "man\u00b7che", "sich", "auf", "bun\u00b7te", "Bluh\u00b7men", "setzt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PRF", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Es schien, ob wollen sie durch ihre Bluhmen-Pracht,", "tokens": ["Es", "schien", ",", "ob", "wol\u00b7len", "sie", "durch", "ih\u00b7re", "Bluh\u00b7men\u00b7Pracht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "VMFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Die die Natur so sch\u00f6n gemacht,", "tokens": ["Die", "die", "Na\u00b7tur", "so", "sch\u00f6n", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Mit Bluhmen um den Vorzug streiten.", "tokens": ["Mit", "Bluh\u00b7men", "um", "den", "Vor\u00b7zug", "strei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}