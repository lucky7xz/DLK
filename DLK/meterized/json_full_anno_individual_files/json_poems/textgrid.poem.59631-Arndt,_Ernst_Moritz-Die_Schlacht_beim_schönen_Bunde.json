{"textgrid.poem.59631": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "Die Schlacht beim sch\u00f6nen Bunde", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf Viktoria! Auf Viktoria!", "tokens": ["Auf", "Vik\u00b7to\u00b7ria", "!", "Auf", "Vik\u00b7to\u00b7ria", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NE", "$.", "APPR", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Welch ein Klang aus Niederland!", "tokens": ["Welch", "ein", "Klang", "aus", "Nie\u00b7der\u00b7land", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "NN", "APPR", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00dcber Strom und Berg geklungen,", "tokens": ["\u00dc\u00b7ber", "Strom", "und", "Berg", "ge\u00b7klun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Tausendstimmig nachgesungen", "tokens": ["Tau\u00b7sends\u00b7tim\u00b7mig", "nach\u00b7ge\u00b7sun\u00b7gen"], "token_info": ["word", "word"], "pos": ["ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Rollet er die Welt entlang.", "tokens": ["Rol\u00b7let", "er", "die", "Welt", "ent\u00b7lang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Alter Bl\u00fccher! Alter Bl\u00fccher!", "tokens": ["Al\u00b7ter", "Bl\u00fc\u00b7cher", "!", "Al\u00b7ter", "Bl\u00fc\u00b7cher", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NE", "$.", "NN", "NE", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "J\u00fcngling mit dem wei\u00dfen Haar!", "tokens": ["J\u00fcng\u00b7ling", "mit", "dem", "wei\u00b7\u00dfen", "Haar", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Der wie Mars zu Rosse sitzet,", "tokens": ["Der", "wie", "Mars", "zu", "Ros\u00b7se", "sit\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KOKOM", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der wie Gottes Wetter blitzet,", "tokens": ["Der", "wie", "Got\u00b7tes", "Wet\u00b7ter", "blit\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KOKOM", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Machst den Schwur du wieder wahr?", "tokens": ["Machst", "den", "Schwur", "du", "wie\u00b7der", "wahr", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "VVFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Jenen Schwur, den du geschworen", "tokens": ["Je\u00b7nen", "Schwur", ",", "den", "du", "ge\u00b7schwo\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VVFIN", "$,", "PRELS", "PPER", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Einst an Gott und Vaterland,", "tokens": ["Einst", "an", "Gott", "und", "Va\u00b7ter\u00b7land", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Deinen Degen zu zerbrechen", "tokens": ["Dei\u00b7nen", "De\u00b7gen", "zu", "zer\u00b7bre\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Oder Deutschlands Schmach zu r\u00e4chen", "tokens": ["O\u00b7der", "Deutschlands", "Schmach", "zu", "r\u00e4\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NE", "NN", "PTKZU", "VVINF"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.5": {"text": "An dem welschen Bubentand?", "tokens": ["An", "dem", "wel\u00b7schen", "Bu\u00b7ben\u00b7tand", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Alter Bl\u00fccher! Alter Bl\u00fccher!", "tokens": ["Al\u00b7ter", "Bl\u00fc\u00b7cher", "!", "Al\u00b7ter", "Bl\u00fc\u00b7cher", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NE", "$.", "NN", "NE", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mahnst du das Banditenheer", "tokens": ["Mahnst", "du", "das", "Ban\u00b7di\u00b7ten\u00b7heer"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ART", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "An der Katzbach nasse Tiefen", "tokens": ["An", "der", "Katz\u00b7bach", "nas\u00b7se", "Tie\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und an Leipzig, wo sie liefen?", "tokens": ["Und", "an", "Leip\u00b7zig", ",", "wo", "sie", "lie\u00b7fen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NE", "$,", "PWAV", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "An Brienne, Laon, La F\u00e8re?", "tokens": ["An", "Bri\u00b7en\u00b7ne", ",", "Laon", ",", "La", "F\u00e8\u00b7re", "?"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "NE", "$,", "NE", "NE", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Auf Viktoria! Auf Viktoria!", "tokens": ["Auf", "Vik\u00b7to\u00b7ria", "!", "Auf", "Vik\u00b7to\u00b7ria", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NE", "$.", "APPR", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dreimal hoch Viktoria!", "tokens": ["Drei\u00b7mal", "hoch", "Vik\u00b7to\u00b7ria", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "NE", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Wer in Spanien ist gewesen,", "tokens": ["Wer", "in", "Spa\u00b7ni\u00b7en", "ist", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "NE", "VAFIN", "VAPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Kennt den Namen auserlesen,", "tokens": ["Kennt", "den", "Na\u00b7men", "au\u00b7ser\u00b7le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Kennt das Feld Vittoria.", "tokens": ["Kennt", "das", "Feld", "Vit\u00b7to\u00b7ria", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NE", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Talavera, Salamanka", "tokens": ["Ta\u00b7la\u00b7ve\u00b7ra", ",", "Sa\u00b7la\u00b7man\u00b7ka"], "token_info": ["word", "punct", "word"], "pos": ["NE", "$,", "NE"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Und Vittoria dreimal hoch!", "tokens": ["Und", "Vit\u00b7to\u00b7ria", "drei\u00b7mal", "hoch", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "ADJD", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Auch ein Klang klingt von Tolose,", "tokens": ["Auch", "ein", "Klang", "klingt", "von", "To\u00b7lo\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "APPR", "NE", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Und dir bebt das Herz, Franzose \u2013", "tokens": ["Und", "dir", "bebt", "das", "Herz", ",", "Fran\u00b7zo\u00b7se", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$,", "NE", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wellington, der lebet noch.", "tokens": ["Wel\u00b7ling\u00b7ton", ",", "der", "le\u00b7bet", "noch", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "VVFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Auf Viktoria! Auf Viktoria!", "tokens": ["Auf", "Vik\u00b7to\u00b7ria", "!", "Auf", "Vik\u00b7to\u00b7ria", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NE", "$.", "APPR", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bl\u00fccher, Wellington und Gott,", "tokens": ["Bl\u00fc\u00b7cher", ",", "Wel\u00b7ling\u00b7ton", "und", "Gott", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Diese drei sind fest verbunden,", "tokens": ["Die\u00b7se", "drei", "sind", "fest", "ver\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "CARD", "VAFIN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und der Feind ist hingeschwunden,", "tokens": ["Und", "der", "Feind", "ist", "hin\u00b7ge\u00b7schwun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.5": {"text": "Und sein Dr\u00e4un ist Kinderspott.", "tokens": ["Und", "sein", "Dr\u00e4un", "ist", "Kin\u00b7der\u00b7spott", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Bei La belle Alliance \u2013", "tokens": ["Bei", "La", "bel\u00b7le", "Al\u00b7li\u00b7an\u00b7ce", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADJA", "NN", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Hei\u00dft auf Deutsch der Sch\u00f6ne Bund \u2013", "tokens": ["Hei\u00dft", "auf", "Deutsch", "der", "Sch\u00f6\u00b7ne", "Bund", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hielt der gro\u00dfe Himmelsrichter", "tokens": ["Hielt", "der", "gro\u00b7\u00dfe", "Him\u00b7mels\u00b7rich\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Das Gericht der B\u00f6sewichter,", "tokens": ["Das", "Ge\u00b7richt", "der", "B\u00f6\u00b7se\u00b7wich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ihres Trotzes letzte Stund'.", "tokens": ["Ih\u00b7res", "Trot\u00b7zes", "letz\u00b7te", "Stund'", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Auch Viktoria, auch Viktoria", "tokens": ["Auch", "Vik\u00b7to\u00b7ria", ",", "auch", "Vik\u00b7to\u00b7ria"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADV", "NE", "$,", "ADV", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Euch, ihr Tapfre, die ihr ruht!", "tokens": ["Euch", ",", "ihr", "Tapf\u00b7re", ",", "die", "ihr", "ruht", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PPOSAT", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die kein Schlachtruf mehr erwecket,", "tokens": ["Die", "kein", "Schlach\u00b7truf", "mehr", "er\u00b7we\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die des Todes Nacht bedecket,", "tokens": ["Die", "des", "To\u00b7des", "Nacht", "be\u00b7de\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Freiheit bl\u00fcht aus eurem Blut.", "tokens": ["Frei\u00b7heit", "bl\u00fcht", "aus", "eu\u00b7rem", "Blut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Nun nach Frankreich! Nun nach Frankreich!", "tokens": ["Nun", "nach", "Fran\u00b7kreich", "!", "Nun", "nach", "Fran\u00b7kreich", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NE", "$.", "ADV", "APPR", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Klinget dort Viktoria!", "tokens": ["Klin\u00b7get", "dort", "Vik\u00b7to\u00b7ria", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "NE", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Da\u00df die B\u00fcberei sich sch\u00e4me,", "tokens": ["Da\u00df", "die", "B\u00fc\u00b7be\u00b7rei", "sich", "sch\u00e4\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df die Eitelkeit sich gr\u00e4me,", "tokens": ["Da\u00df", "die", "Ei\u00b7tel\u00b7keit", "sich", "gr\u00e4\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Klinget hell Viktoria!", "tokens": ["Klin\u00b7get", "hell", "Vik\u00b7to\u00b7ria", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "NE", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.11": {"line.1": {"text": "Nun nach Frankreich! Nun nach Frankreich!", "tokens": ["Nun", "nach", "Fran\u00b7kreich", "!", "Nun", "nach", "Fran\u00b7kreich", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NE", "$.", "ADV", "APPR", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gie\u00dft den Strom der M\u00e4nner aus!", "tokens": ["Gie\u00dft", "den", "Strom", "der", "M\u00e4n\u00b7ner", "aus", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "La\u00dft sie sehen, la\u00dft sie f\u00fchlen,", "tokens": ["La\u00dft", "sie", "se\u00b7hen", ",", "la\u00dft", "sie", "f\u00fch\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVINF", "$,", "VVIMP", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Was es hei\u00dft, mit Eiden spielen;", "tokens": ["Was", "es", "hei\u00dft", ",", "mit", "Ei\u00b7den", "spie\u00b7len", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Kehrt die Brut der H\u00f6lle aus!", "tokens": ["Kehrt", "die", "Brut", "der", "H\u00f6l\u00b7le", "aus", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Nun nach Frankreich! Nun nach Frankreich!", "tokens": ["Nun", "nach", "Fran\u00b7kreich", "!", "Nun", "nach", "Fran\u00b7kreich", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NE", "$.", "ADV", "APPR", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ins Franzosenparadies!", "tokens": ["Ins", "Fran\u00b7zo\u00b7sen\u00b7pa\u00b7ra\u00b7dies", "!"], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Straft das Land der b\u00f6sen Heiden,", "tokens": ["Straft", "das", "Land", "der", "b\u00f6\u00b7sen", "Hei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Das uns zwanzig Jahr' an 