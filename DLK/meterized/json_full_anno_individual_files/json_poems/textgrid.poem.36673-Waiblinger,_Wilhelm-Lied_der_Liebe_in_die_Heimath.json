{"textgrid.poem.36673": {"metadata": {"author": {"name": "Waiblinger, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Lied der Liebe in die Heimath", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach warum in dieser Ferne,", "tokens": ["Ach", "wa\u00b7rum", "in", "die\u00b7ser", "Fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PWAV", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "S\u00fc\u00dfes Herz, so weit von dir?", "tokens": ["S\u00fc\u00b7\u00dfes", "Herz", ",", "so", "weit", "von", "dir", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADV", "ADJD", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Alle Sonnen, alle Sterne,", "tokens": ["Al\u00b7le", "Son\u00b7nen", ",", "al\u00b7le", "Ster\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Oeffnen ihre Augen mir,", "tokens": ["Oeff\u00b7nen", "ih\u00b7re", "Au\u00b7gen", "mir", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Nur die sch\u00f6nsten blauen Strahlen,", "tokens": ["Nur", "die", "sch\u00f6ns\u00b7ten", "blau\u00b7en", "Strah\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Nur das reinste tiefste Licht,", "tokens": ["Nur", "das", "reins\u00b7te", "tiefs\u00b7te", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Drin sich Erd' und Himmel malen,", "tokens": ["Drin", "sich", "Erd'", "und", "Him\u00b7mel", "ma\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PRF", "NN", "KON", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Nur dein treues Auge nicht.", "tokens": ["Nur", "dein", "treu\u00b7es", "Au\u00b7ge", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ja, ich seh' in wilden Lauben,", "tokens": ["Ja", ",", "ich", "seh'", "in", "wil\u00b7den", "Lau\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ueber Bergen, \u00fcber Seen,", "tokens": ["Ue\u00b7ber", "Ber\u00b7gen", ",", "\u00fc\u00b7ber", "Seen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Kind voll Unschuld und voll Glauben,", "tokens": ["Kind", "voll", "Un\u00b7schuld", "und", "voll", "Glau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "NN", "KON", "ADJD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dich in frommer Stille gehn.", "tokens": ["Dich", "in", "from\u00b7mer", "Stil\u00b7le", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Um die bleichen feuchten Wangen", "tokens": ["Um", "die", "blei\u00b7chen", "feuch\u00b7ten", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUI", "ART", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Spielt die frische Abendluft,", "tokens": ["Spielt", "die", "fri\u00b7sche", "A\u00b7ben\u00b7dluft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und es steigt dein zart Verlangen", "tokens": ["Und", "es", "steigt", "dein", "zart", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "ADJD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Himmelw\u00e4rts wie Blumenduft.", "tokens": ["Him\u00b7mel\u00b7w\u00e4rts", "wie", "Blu\u00b7men\u00b7duft", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Thr\u00e4n' an Thr\u00e4ne seh' ich rinnen", "tokens": ["Thr\u00e4n'", "an", "Thr\u00e4\u00b7ne", "seh'", "ich", "rin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "VVFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tief aus deines Auges Nacht,", "tokens": ["Tief", "aus", "dei\u00b7nes", "Au\u00b7ges", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und mit gl\u00fchend hei\u00dfen Sinnen", "tokens": ["Und", "mit", "gl\u00fc\u00b7hend", "hei\u00b7\u00dfen", "Sin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJD", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "H\u00e4ngst du an der Sterne Pracht \u2013", "tokens": ["H\u00e4ngst", "du", "an", "der", "Ster\u00b7ne", "Pracht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "O mein Kind, in jenen R\u00e4umen", "tokens": ["O", "mein", "Kind", ",", "in", "je\u00b7nen", "R\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "PPOSAT", "NN", "$,", "APPR", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Suchst du den Geliebten schon,", "tokens": ["Suchst", "du", "den", "Ge\u00b7lieb\u00b7ten", "schon", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADV", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Und so fr\u00fch den sch\u00f6nen Tr\u00e4umen", "tokens": ["Und", "so", "fr\u00fch", "den", "sch\u00f6\u00b7nen", "Tr\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADJD", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Spr\u00e4che das Verh\u00e4ngni\u00df Hohn?", "tokens": ["Spr\u00e4\u00b7che", "das", "Ver\u00b7h\u00e4ng\u00b7ni\u00df", "Hohn", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Nein, dem liebenden Gem\u00fcthe", "tokens": ["Nein", ",", "dem", "lie\u00b7ben\u00b7den", "Ge\u00b7m\u00fc\u00b7the"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$,", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sind sie schmerzlich sanfter Trost!", "tokens": ["Sind", "sie", "schmerz\u00b7lich", "sanf\u00b7ter", "Trost", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nach dem Winter kommt die Bl\u00fcthe,", "tokens": ["Nach", "dem", "Win\u00b7ter", "kommt", "die", "Bl\u00fc\u00b7the", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die ein neuer West umkost.", "tokens": ["Die", "ein", "neu\u00b7er", "West", "um\u00b7kost", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Bei den heimathlichen Auen,", "tokens": ["Bei", "den", "hei\u00b7math\u00b7li\u00b7chen", "Au\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Bei der Burgruine Bild,", "tokens": ["Bei", "der", "Bur\u00b7gru\u00b7i\u00b7ne", "Bild", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Da, wo Aug' und Blumen thanen,", "tokens": ["Da", ",", "wo", "Aug'", "und", "Blu\u00b7men", "tha\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "M\u00e4dchen, sei dein Weh gestillt.", "tokens": ["M\u00e4d\u00b7chen", ",", "sei", "dein", "Weh", "ge\u00b7stillt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VAFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Was du weinend mir gegeben,", "tokens": ["Was", "du", "wei\u00b7nend", "mir", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "All dein himmlisch Heiligthum,", "tokens": ["All", "dein", "himm\u00b7lisch", "Hei\u00b7lig\u00b7thum", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "ADJD", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "War ein Ku\u00df f\u00fcrs Erdenleben,", "tokens": ["War", "ein", "Ku\u00df", "f\u00fcrs", "Er\u00b7den\u00b7le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "War es f\u00fcr Elysium.", "tokens": ["War", "es", "f\u00fcr", "E\u00b7ly\u00b7si\u00b7um", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Mein ist dein versch\u00e4mtes Zagen,", "tokens": ["Mein", "ist", "dein", "ver\u00b7sch\u00e4m\u00b7tes", "Za\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VAFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mein die jungfr\u00e4uliche Scheu,", "tokens": ["Mein", "die", "jung\u00b7fr\u00e4u\u00b7li\u00b7che", "Scheu", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.7": {"text": "Konntest du so muthig wagen,", "tokens": ["Konn\u00b7test", "du", "so", "mut\u00b7hig", "wa\u00b7gen", ","], 