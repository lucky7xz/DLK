{"dta.poem.23373": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "H\u00f6fische Poesie.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn man euren Glanz will schauen,", "tokens": ["Wenn", "man", "eu\u00b7ren", "Glanz", "will", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPOSAT", "NN", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wirft man euch ein K\u00f6rnlein vor,", "tokens": ["Wirft", "man", "euch", "ein", "K\u00f6rn\u00b7lein", "vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und man lockt euch wie die Pfauen", "tokens": ["Und", "man", "lockt", "euch", "wie", "die", "Pfau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "VVFIN", "PPER", "KOKOM", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Drau\u00dfen vor des Schlo\u00dfhofs Thor.", "tokens": ["Drau\u00b7\u00dfen", "vor", "des", "Schlo\u00df\u00b7hofs", "Thor", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "T\u00fcrksche Enten, H\u00e4hn' und Hennen", "tokens": ["T\u00fcrk\u00b7sche", "En\u00b7ten", ",", "H\u00e4hn'", "und", "Hen\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sind hoff\u00e4hig nur allein,", "tokens": ["Sind", "hof\u00b7f\u00e4\u00b7hig", "nur", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ADV", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "D\u00fcrfen nicht nach Futter rennen,", "tokens": ["D\u00fcr\u00b7fen", "nicht", "nach", "Fut\u00b7ter", "ren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "D\u00fcrfen nicht nach Futter schrei'n.", "tokens": ["D\u00fcr\u00b7fen", "nicht", "nach", "Fut\u00b7ter", "schrei'", "n."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["NE", "PTKNEG", "APPR", "NN", "VVFIN", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Merkt es euch, ihr K\u00f6nigsdichter!", "tokens": ["Merkt", "es", "euch", ",", "ihr", "K\u00f6\u00b7nigs\u00b7dich\u00b7ter", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPER", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn ihr schlagt das sch\u00f6nste Rad,", "tokens": ["Wenn", "ihr", "schlagt", "das", "sch\u00f6ns\u00b7te", "Rad", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Fri\u00dft sich andres Hofgelichter", "tokens": ["Fri\u00dft", "sich", "and\u00b7res", "Hof\u00b7ge\u00b7lich\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ganz bequem im Schlo\u00dfhof satt.", "tokens": ["Ganz", "be\u00b7quem", "im", "Schlo\u00df\u00b7hof", "satt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPRART", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}