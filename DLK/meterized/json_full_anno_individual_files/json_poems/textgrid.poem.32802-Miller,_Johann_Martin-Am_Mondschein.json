{"textgrid.poem.32802": {"metadata": {"author": {"name": "Miller, Johann Martin", "birth": "N.A.", "death": "N.A."}, "title": "Am Mondschein", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da liegt, vom lieben Mond erhellt,", "tokens": ["Da", "liegt", ",", "vom", "lie\u00b7ben", "Mond", "er\u00b7hellt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In stiller Ruh' umher die Welt;", "tokens": ["In", "stil\u00b7ler", "Ruh'", "um\u00b7her", "die", "Welt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nur Grillen zirpen noch um mich,", "tokens": ["Nur", "Gril\u00b7len", "zir\u00b7pen", "noch", "um", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "ADV", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und freuen ihres Daseins sich.", "tokens": ["Und", "freu\u00b7en", "ih\u00b7res", "Da\u00b7seins", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "PRF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ihr K\u00e4fer und ihr V\u00f6gelein,", "tokens": ["Ihr", "K\u00e4\u00b7fer", "und", "ihr", "V\u00f6\u00b7ge\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auch ich noch kann mit euch mich freun!", "tokens": ["Auch", "ich", "noch", "kann"