{"dta.poem.18619": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "G rabschrifft eines erh\u00e4ngten  \n M\u00fcnchs.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1702", "urn": "urn:nbn:de:kobv:b4-200905197766", "language": ["de:0.99"], "booktitle": "Hunold, Christian Friedrich: Die Edle Bem\u00fchung m\u00fcssiger Stunden. Hamburg, 1702."}, "poem": {"stanza.1": {"line.1": {"text": "Das Closter ist best\u00fcrtzt/ die Nonnen klagen sehr/", "tokens": ["Das", "Clos\u00b7ter", "ist", "be\u00b7st\u00fcrtzt", "/", "die", "Non\u00b7nen", "kla\u00b7gen", "sehr", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$(", "ART", "NN", "VVFIN", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Bruder schimpft das Recht/ das wir den M\u00fcnchen", "tokens": ["Der", "Bru\u00b7der", "schimpft", "das", "Recht", "/", "das", "wir", "den", "M\u00fcn\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$(", "PRELS", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "gaben:", "tokens": ["ga\u00b7ben", ":"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Er hengt sich selber auf und denckt daran nicht mehr/", "tokens": ["Er", "hengt", "sich", "sel\u00b7ber", "auf", "und", "denckt", "da\u00b7ran", "nicht", "mehr", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "PTKVZ", "KON", "VVFIN", "PAV", "PTKNEG", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Da\u00df er von uns darzu mu\u00df das Gerichte haben.", "tokens": [