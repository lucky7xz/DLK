{"dta.poem.15732": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "65.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Von allen Thieren hat den Menschen Gott zuletzt", "tokens": ["Von", "al\u00b7len", "Thie\u00b7ren", "hat", "den", "Men\u00b7schen", "Gott", "zu\u00b7letzt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "VAFIN", "ART", "NN", "NN", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Erschaffen, und so ists noch in der Sch\u00f6pfung jetzt.", "tokens": ["Er\u00b7schaf\u00b7fen", ",", "und", "so", "ists", "noch", "in", "der", "Sch\u00f6p\u00b7fung", "jetzt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "KON", "ADV", "VAFIN", "ADV", "APPR", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Von allem wird der Mensch im Menschen reif zuletzt,", "tokens": ["Von", "al\u00b7lem", "wird", "der", "Mensch", "im", "Men\u00b7schen", "reif", "zu\u00b7letzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VAFIN", "ART", "NN", "APPRART", "NN", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nachdem er sich aus dem in jenes umgesetzt.", "tokens": ["Nach\u00b7dem", "er", "sich", "aus", "dem", "in", "je\u00b7nes", "um\u00b7ge\u00b7setzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPR", "ART", "APPR", "PDAT", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ein Pflanzenleben ist der Mensch zuerst berufen", "tokens": ["Ein", "Pflan\u00b7zen\u00b7le\u00b7ben", "ist", "der", "Mensch", "zu\u00b7erst", "be\u00b7ru\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "ADV", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu leben, dann lebt er durchs Thier in vielen Stufen.", "tokens": ["Zu", "le\u00b7ben", ",", "dann", "lebt", "er", "durchs", "Thier", "in", "vie\u00b7len", "Stu\u00b7fen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "ADV", "VVFIN", "PPER", "APPRART", "NN", "APPR", "PIAT", "NN", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.4": {"line.1": {"text": "Wie viele sind, die auf den niedern Stufen bleiben,", "tokens": ["Wie", "vie\u00b7le", "sind", ",", "die", "auf", "den", "nie\u00b7dern", "Stu\u00b7fen", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VAFIN", "$,", "PRELS", "APPR", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie wenige, die ganz empor zur h\u00f6chsten treiben!", "tokens": ["Wie", "we\u00b7ni\u00b7ge", ",", "die", "ganz", "em\u00b7por", "zur", "h\u00f6chs\u00b7ten", "trei\u00b7ben", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "$,", "PRELS", "ADV", "PTKVZ", "APPRART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Wie manche, die zur\u00fcck zur Tiefe wieder sinken,", "tokens": ["Wie", "man\u00b7che", ",", "die", "zu\u00b7r\u00fcck", "zur", "Tie\u00b7fe", "wie\u00b7der", "sin\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "$,", "PRELS", "ADV", "APPRART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und zeigen uns das Thier, wo wir dem Menschen winken.", "tokens": ["Und", "zei\u00b7gen", "uns", "das", "Thier", ",", "wo", "wir", "dem", "Men\u00b7schen", "win\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "$,", "PWAV", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}