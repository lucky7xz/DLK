{"dta.poem.4282": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Das kalte  \n und doch gesegnete Jahr 1740.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "In diesem sonderbaren Jahr, da man, nach siebenzehn", "tokens": ["In", "die\u00b7sem", "son\u00b7der\u00b7ba\u00b7ren", "Jahr", ",", "da", "man", ",", "nach", "sie\u00b7ben\u00b7zehn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,", "KOUS", "PIS", "$,", "APPR", "CARD"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "hundert Jahren,", "tokens": ["hun\u00b7dert", "Jah\u00b7ren", ","], "token_info": ["word", "word", "punct"], "pos": ["CARD", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Noch vierzig schrieb, hat unsre Fl\u00e4ch\u2019 der Welt was sonder-", "tokens": ["Noch", "vier\u00b7zig", "schrieb", ",", "hat", "uns\u00b7re", "Fl\u00e4ch'", "der", "Welt", "was", "son\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "CARD", "VVFIN", "$,", "VAFIN", "PPOSAT", "NN", "ART", "NN", "PWS", "TRUNC"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "lichs erfahren.", "tokens": ["lichs", "er\u00b7fah\u00b7ren", "."], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "DerWinter war entsetzlich kalt. Wie man sich nicht erinnern", "tokens": ["Der", "Win\u00b7ter", "war", "ent\u00b7setz\u00b7lich", "kalt", ".", "Wie", "man", "sich", "nicht", "e\u00b7rin\u00b7nern"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADJD", "ADJD", "$.", "PWAV", "PIS", "PRF", "PTKNEG", "VVINF"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.6": {"text": "kann,", "tokens": ["kann", ","], "token_info": ["word", "punct"], "pos": ["VMFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.7": {"text": "Da\u00df solches ehedem geschehen; fing er schon im October an,", "tokens": ["Da\u00df", "sol\u00b7ches", "e\u00b7he\u00b7dem", "ge\u00b7sche\u00b7hen", ";", "fing", "er", "schon", "im", "Oc\u00b7to\u00b7ber", "an", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "VVPP", "$.", "VVFIN", "PPER", "ADV", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-+--+", "measure": "iambic.octa.plus.octa.plus.chol"}, "line.8": {"text": "Und w\u00e4hrte bis im Junius; da sah man zwar kein Eis nicht", "tokens": ["Und", "w\u00e4hr\u00b7te", "bis", "im", "Ju\u00b7nius", ";", "da", "sah", "man", "zwar", "kein", "Eis", "nicht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "APPRART", "NN", "$.", "ADV", "VVFIN", "PIS", "ADV", "PIAT", "NN", "PTKNEG"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "mehr,", "tokens": ["mehr", ","], "token_info": ["word", "punct"], "pos": ["ADV", "$,"], "meter": "-", "measure": "single.down"}, "line.10": {"text": "Doch war so gar im Julius die Luft noch nicht von K\u00e4lte", "tokens": ["Doch", "war", "so", "gar", "im", "Ju\u00b7lius", "die", "Luft", "noch", "nicht", "von", "K\u00e4l\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ADV", "ADV", "APPRART", "NE", "ART", "NN", "ADV", "PTKNEG", "APPR", "NN"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "leer.", "tokens": ["leer", "."], "token_info": ["word", "punct"], "pos": ["ADJD", "$."], "meter": "-", "measure": "single.down"}, "line.12": {"text": "Kein Regen fiel, kein Gras erschien. So wie die K\u00e4lte erst", "tokens": ["Kein", "Re\u00b7gen", "fiel", ",", "kein", "Gras", "er\u00b7schien", ".", "So", "wie", "die", "K\u00e4l\u00b7te", "erst"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "$,", "PIAT", "NN", "VVFIN", "$.", "ADV", "KOKOM", "ART", "NN", "ADV"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.13": {"text": "gethan,", "tokens": ["ge\u00b7than", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.14": {"text": "So hinderte die strenge D\u00fcrre, betr\u00fcbt genug, nun auch", "tokens": ["So", "hin\u00b7der\u00b7te", "die", "stren\u00b7ge", "D\u00fcr\u00b7re", ",", "be\u00b7tr\u00fcbt", "ge\u00b7nug", ",", "nun", "auch"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,", "VVFIN", "ADV", "$,", "ADV", "ADV"], "meter": "-+---+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.15": {"text": "daran.", "tokens": ["da\u00b7ran", "."], "token_info": ["word", "punct"], "pos": ["PAV", "$."], "meter": "-+", "measure": "iambic.single"}, "line.16": {"text": "Wodurch das Vieh, so fast verkam, annoch in \u00e4usserster", "tokens": ["Wo\u00b7durch", "das", "Vieh", ",", "so", "fast", "ver\u00b7kam", ",", "an\u00b7noch", "in", "\u00e4us\u00b7sers\u00b7ter"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "$,", "ADV", "ADV", "VVFIN", "$,", "ADV", "APPR", "ADJA"], "meter": "-+-+-+-+-+-+--", "measure": "unknown.measure.hexa"}, "line.17": {"text": "Gefahr,", "tokens": ["Ge\u00b7fahr", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.18": {"text": "Vor Hunger umzukommen, war.", "tokens": ["Vor", "Hun\u00b7ger", "um\u00b7zu\u00b7kom\u00b7men", ",", "war", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "VVIZU", "$,", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Ein nie erlebter Sommer folgt dem nie erlebten Winter", "tokens": ["Ein", "nie", "er\u00b7leb\u00b7ter", "Som\u00b7mer", "folgt", "dem", "nie", "er\u00b7leb\u00b7ten", "Win\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ADJA", "NN", "VVFIN", "ART", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.20": {"text": "nach,", "tokens": ["nach", ","], "token_info": ["word", "punct"], "pos": ["APPR", "$,"], "meter": "+", "measure": "single.up"}, "line.21": {"text": "Da noch die ganz besondre K\u00e4lte sich selten nur verringert", "tokens": ["Da", "noch", "die", "ganz", "be\u00b7sond\u00b7re", "K\u00e4l\u00b7te", "sich", "sel\u00b7ten", "nur", "ver\u00b7rin\u00b7gert"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "ADV", "ADJA", "NN", "PRF", "ADJD", "ADV", "VVPP"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.22": {"text": "hat.", "tokens": ["hat", "."], "token_info": ["word", "punct"], "pos": ["VAFIN", "$."], "meter": "+", "measure": "single.up"}, "line.23": {"text": "Heut ist Bartholom\u00e4us Tag,", "tokens": ["Heut", "ist", "Bar\u00b7tho\u00b7lo\u00b7m\u00e4us", "Tag", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NE", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.24": {"text": "Und dennoch steht im Wetter-Glase die K\u00e4lt\u2019 auf drey und", "tokens": ["Und", "den\u00b7noch", "steht", "im", "Wet\u00b7ter\u00b7Gla\u00b7se", "die", "K\u00e4lt'", "auf", "drey", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "APPRART", "NN", "ART", "NN", "APPR", "CARD", "KON"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.25": {"text": "drei\u00dfig Grad.", "tokens": ["drei\u00b7\u00dfig", "Grad", "."], "token_info": ["word", "word", "punct"], "pos": ["CARD", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.26": {"text": "Den Fr\u00fchling f\u00fchlt\u2019 und sah man nicht. Es ist kein Som-", "tokens": ["Den", "Fr\u00fch\u00b7ling", "f\u00fchlt'", "und", "sah", "man", "nicht", ".", "Es", "ist", "kein", "Som"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "PIS", "PTKNEG", "$.", "PPER", "VAFIN", "PIAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "mer fast zu sp\u00fchren.", "tokens": ["mer", "fast", "zu", "sp\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.28": {"text": "Es scheint in diesem Jahre fast der Herbst den Winter zu", "tokens": ["Es", "scheint", "in", "die\u00b7sem", "Jah\u00b7re", "fast", "der", "Herbst", "den", "Win\u00b7ter", "zu"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PDAT", "NN", "ADV", "ART", "NN", "ART", "NN", "PTKZU"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.29": {"text": "ber\u00fchren,", "tokens": ["be\u00b7r\u00fch\u00b7ren", ","], "token_info": ["word", "punct"], "pos": ["VVINF", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.30": {"text": "Ohn\u2019 Zwischen-Stand vom Lenz und Sommer. Er k\u00f6mmt", "tokens": ["Ohn'", "Zwi\u00b7schen\u00b7Stand", "vom", "Lenz", "und", "Som\u00b7mer", ".", "Er", "k\u00f6mmt"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "APPRART", "NN", "KON", "NN", "$.", "PPER", "VVFIN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.31": {"text": "schon allgemach herbey.", "tokens": ["schon", "all\u00b7ge\u00b7mach", "her\u00b7bey", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.32": {"text": "(gOtt gebe, da\u00df die Witterung im selben angenehmer sey!)", "tokens": ["(", "gOtt", "ge\u00b7be", ",", "da\u00df", "die", "Wit\u00b7te\u00b7rung", "im", "sel\u00b7ben", "an\u00b7ge\u00b7neh\u00b7mer", "sey", "!", ")"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "NN", "VVFIN", "$,", "KOUS", "ART", "NN", "APPRART", "ADJA", "NN", "VAFIN", "$.", "$("], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.33": {"text": "Und dennoch, ob der Ackers-Mann fast alle Winter-Saat", "tokens": ["Und", "den\u00b7noch", ",", "ob", "der", "A\u00b7cker\u00b7sMann", "fast", "al\u00b7le", "Win\u00b7ter\u00b7Saat"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "$,", "KOUS", "ART", "NN", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.34": {"text": "verlohren,", "tokens": ["ver\u00b7loh\u00b7ren", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.35": {"text": "Ob ihm so wohl die Winter-Gerst, als auch sein Weizen meist", "tokens": ["Ob", "ihm", "so", "wohl", "die", "Win\u00b7ter\u00b7Gerst", ",", "als", "auch", "sein", "Wei\u00b7zen", "meist"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ART", "NN", "$,", "KOUS", "ADV", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.36": {"text": "erfroren;", "tokens": ["er\u00b7fro\u00b7ren", ";"], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.37": {"text": "So sieht man doch (ein wirklichs Wunder!) den Rocken, auch", "tokens": ["So", "sieht", "man", "doch", "(", "ein", "wirk\u00b7lichs", "Wun\u00b7der", "!", ")", "den", "Ro\u00b7cken", ",", "auch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct", "word", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "$(", "ART", "ADJA", "NN", "$.", "$(", "ART", "NN", "$,", "ADV"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.38": {"text": "die Sommer-Saat,", "tokens": ["die", "Som\u00b7mer\u00b7Saat", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.39": {"text": "Nebst allen Fr\u00fcchten in dem Garten, zusammt den Fr\u00fcchten", "tokens": ["Nebst", "al\u00b7len", "Fr\u00fcch\u00b7ten", "in", "dem", "Gar\u00b7ten", ",", "zu\u00b7sammt", "den", "Fr\u00fcch\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "APPR", "ART", "NN", "$,", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.40": {"text": "auf dem Lande,", "tokens": ["auf", "dem", "Lan\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.41": {"text": "In einem solchen Ueberflu\u00df, in einem solchen guten Stande,", "tokens": ["In", "ei\u00b7nem", "sol\u00b7chen", "Ue\u00b7berf\u00b7lu\u00df", ",", "in", "ei\u00b7nem", "sol\u00b7chen", "gu\u00b7ten", "Stan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PIAT", "NN", "$,", "APPR", "ART", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.42": {"text": "Da\u00df man, in vielen Jahren fast, dergleichen kaum gesehen", "tokens": ["Da\u00df", "man", ",", "in", "vie\u00b7len", "Jah\u00b7ren", "fast", ",", "derg\u00b7lei\u00b7chen", "kaum", "ge\u00b7se\u00b7hen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PIS", "$,", "APPR", "PIAT", "NN", "ADV", "$,", "PIS", "ADV", "VVPP"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.43": {"text": "hat.", "tokens": ["hat", "."], "token_info": ["word", "punct"], "pos": ["VAFIN", "$."], "meter": "+", "measure": "single.up"}, "line.44": {"text": "Ich kann von meinen eignen Fr\u00fcchten, hier mit Bewunde-", "tokens": ["Ich", "kann", "von", "mei\u00b7nen", "eig\u00b7nen", "Fr\u00fcch\u00b7ten", ",", "hier", "mit", "Be\u00b7wun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,", "ADV", "APPR", "TRUNC"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.45": {"text": "rung zu sprechen,", "tokens": ["rung", "zu", "spre\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PTKZU", "VVINF", "$,"], "meter": "--+-", "measure": "anapaest.init"}, "line.46": {"text": "Zu k\u00fcnftiger Erinnerung, mit innrer Lust, mich nicht ent-", "tokens": ["Zu", "k\u00fcnf\u00b7ti\u00b7ger", "E\u00b7rin\u00b7ne\u00b7rung", ",", "mit", "inn\u00b7rer", "Lust", ",", "mich", "nicht", "ent"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "$,", "PPER", "PTKNEG", "TRUNC"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.47": {"text": "brechen.", "tokens": ["bre\u00b7chen", "."], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.48": {"text": "Ich seh nicht nur auf meinem Acker den Rocken dicht, gerad\u2019", "tokens": ["Ich", "seh", "nicht", "nur", "auf", "mei\u00b7nem", "A\u00b7cker", "den", "Ro\u00b7cken", "dicht", ",", "ge\u00b7rad'"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "APPR", "PPOSAT", "NN", "ART", "NN", "ADJD", "$,", "ADV"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.49": {"text": "und sch\u00f6n,", "tokens": ["und", "sch\u00f6n", ","], "token_info": ["word", "word", "punct"], "pos": ["KON", "ADJD", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.50": {"text": "Die Aehren von besondrer Gr\u00f6sse, gef\u00fcllt mit groben K\u00f6r-", "tokens": ["Die", "A\u00b7eh\u00b7ren", "von", "be\u00b7sond\u00b7rer", "Gr\u00f6s\u00b7se", ",", "ge\u00b7f\u00fcllt", "mit", "gro\u00b7ben", "K\u00f6r"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,", "VVPP", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+--+-+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.51": {"text": "nern, steh'n;", "tokens": ["nern", ",", "steh'n", ";"], "token_info": ["word", "punct", "word", "punct"], "pos": ["VVINF", "$,", "VVFIN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.52": {"text": "Ich habe gar, da sonst die Aehre des Rockens nur vier Reihen", "tokens": ["Ich", "ha\u00b7be", "gar", ",", "da", "sonst", "die", "A\u00b7eh\u00b7re", "des", "Ro\u00b7ckens", "nur", "vier", "Rei\u00b7hen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "$,", "KOUS", "ADV", "ART", "NN", "ART", "NN", "ADV", "CARD", "NN"], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.53": {"text": "n\u00e4hret,", "tokens": ["n\u00e4h\u00b7ret", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.54": {"text": "An ihrer vielen sechs gez\u00e4hlt; so fast vom Rocken nicht", "tokens": ["An", "ih\u00b7rer", "vie\u00b7len", "sechs", "ge\u00b7z\u00e4hlt", ";", "so", "fast", "vom", "Ro\u00b7cken", "nicht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "PIAT", "CARD", "VVPP", "$.", "ADV", "ADV", "APPRART", "NN", "PTKNEG"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.55": {"text": "erh\u00f6ret.", "tokens": ["er\u00b7h\u00f6\u00b7ret", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.56": {"text": "Wie denn von Gersten, Rocken, Bohnen fast \u00fcberall das", "tokens": ["Wie", "denn", "von", "Gers\u00b7ten", ",", "Ro\u00b7cken", ",", "Boh\u00b7nen", "fast", "\u00fc\u00b7be\u00b7rall", "das"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "APPR", "NN", "$,", "NN", "$,", "NN", "ADV", "ADV", "ART"], "meter": "-+-+-+-+--+---", "measure": "iambic.penta.relaxed"}, "line.57": {"text": "ganze Land,", "tokens": ["gan\u00b7ze", "Land", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.58": {"text": "In recht besonderm Ueberflu\u00df, bedecket und gesegnet stand.", "tokens": ["In", "recht", "be\u00b7son\u00b7derm", "Ue\u00b7berf\u00b7lu\u00df", ",", "be\u00b7de\u00b7cket", "und", "ge\u00b7seg\u00b7net", "stand", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$,", "VVFIN", "KON", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.59": {"text": "Nun konnt\u2019 ich, und ich kann auch noch hievon die Ursach\u2019", "tokens": ["Nun", "konnt'", "ich", ",", "und", "ich", "kann", "auch", "noch", "hie\u00b7von", "die", "Ur\u00b7sach'"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "$,", "KON", "PPER", "VMFIN", "ADV", "ADV", "PAV", "ART", "NN"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.60": {"text": "nicht begreifen,", "tokens": ["nicht", "be\u00b7grei\u00b7fen", ","], "token_info": ["word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.61": {"text": "Wie doch, bey einer steten K\u00e4lte, die Fr\u00fcchte sich so k\u00f6nnen", "tokens": ["Wie", "doch", ",", "bey", "ei\u00b7ner", "ste\u00b7ten", "K\u00e4l\u00b7te", ",", "die", "Fr\u00fcch\u00b7te", "sich", "so", "k\u00f6n\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "$,", "APPR", "ART", "ADJA", "NN", "$,", "ART", "NN", "PRF", "ADV", "VMFIN"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.62": {"text": "h\u00e4ufen;", "tokens": ["h\u00e4u\u00b7fen", ";"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.63": {"text": "Da sonst, so wie die Menschen meynen, die W\u00e4rme meist der", "tokens": ["Da", "sonst", ",", "so", "wie", "die", "Men\u00b7schen", "mey\u00b7nen", ",", "die", "W\u00e4r\u00b7me", "meist", "der"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "$,", "ADV", "KOKOM", "ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "ART"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.64": {"text": "Fr\u00fcchte Leben.", "tokens": ["Fr\u00fcch\u00b7te", "Le\u00b7ben", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.65": {"text": "Nunmehr ist unser aller Wunsch: Der grosse Geber wolle", "tokens": ["Nun\u00b7mehr", "ist", "un\u00b7ser", "al\u00b7ler", "Wunsch", ":", "Der", "gros\u00b7se", "Ge\u00b7ber", "wol\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPOSAT", "PIAT", "NN", "$.", "ART", "ADJA", "NN", "VMFIN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.66": {"text": "geben,", "tokens": ["ge\u00b7ben", ","], "token_info": ["word", "punct"], "pos": ["VVINF", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.67": {"text": "Da\u00df, da die Erndte-Zeit vorhanden, die sch\u00f6nen Fr\u00fccht\u2019", "tokens": ["Da\u00df", ",", "da", "die", "Ernd\u00b7te\u00b7Zeit", "vor\u00b7han\u00b7den", ",", "die", "sch\u00f6\u00b7nen", "Fr\u00fccht'"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "$,", "KOUS", "ART", "NN", "ADJD", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.68": {"text": "auch m\u00f6gen reifen,", "tokens": ["auch", "m\u00f6\u00b7gen", "rei\u00b7fen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.69": {"text": "Da\u00df der nunmehr so \u00f6ftre Regen den Schatz der Fr\u00fcchte", "tokens": ["Da\u00df", "der", "nun\u00b7mehr", "so", "\u00f6ft\u00b7re", "Re\u00b7gen", "den", "Schatz", "der", "Fr\u00fcch\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADV", "ADV", "ADJA", "NN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.70": {"text": "nicht vermindre,", "tokens": ["nicht", "ver\u00b7mind\u00b7re", ","], "token_info": ["word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.71": {"text": "Noch, durch zu viele Feuchtigkeit, die frohe Erndte nicht", "tokens": ["Noch", ",", "durch", "zu", "vie\u00b7le", "Feuch\u00b7tig\u00b7keit", ",", "die", "fro\u00b7he", "Ernd\u00b7te", "nicht"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "APPR", "APPR", "PIAT", "NN", "$,", "ART", "ADJA", "NN", "PTKNEG"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.72": {"text": "verhindre,", "tokens": ["ver\u00b7hind\u00b7re", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.73": {"text": "Damit man den gezeugten Segen bey gutem Wetter m\u00f6ge", "tokens": ["Da\u00b7mit", "man", "den", "ge\u00b7zeug\u00b7ten", "Se\u00b7gen", "bey", "gu\u00b7tem", "Wet\u00b7ter", "m\u00f6\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VMFIN"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.74": {"text": "heben,", "tokens": ["he\u00b7ben", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.75": {"text": "Und da\u00df wir, wenn wir ihn bekommen, auch GOtt zu danken", "tokens": ["Und", "da\u00df", "wir", ",", "wenn", "wir", "ihn", "be\u00b7kom\u00b7men", ",", "auch", "Gott", "zu", "dan\u00b7ken"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "$,", "KOUS", "PPER", "PPER", "VVINF", "$,", "ADV", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.76": {"text": "uns bestreben;", "tokens": ["uns", "be\u00b7stre\u00b7ben", ";"], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.77": {"text": "Doch auch zugleich dabey erwegen, da\u00df, da wir, trotz der", "tokens": ["Doch", "auch", "zu\u00b7gleich", "da\u00b7bey", "er\u00b7we\u00b7gen", ",", "da\u00df", ",", "da", "wir", ",", "trotz", "der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADV", "ADV", "PAV", "VVFIN", "$,", "KOUS", "$,", "KOUS", "PPER", "$,", "APPR", "ART"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.78": {"text": "d\u00fcrren, kalten", "tokens": ["d\u00fcr\u00b7ren", ",", "kal\u00b7ten"], "token_info": ["word", "punct", "word"], "pos": ["VVFIN", "$,", "ADJA"], "meter": "+-+-", "measure": "trochaic.di"}, "line.79": {"text": "Und pftmahls nassen Witterung, doch so viel Heu und Korn", "tokens": ["Und", "pft\u00b7mahls", "nas\u00b7sen", "Wit\u00b7te\u00b7rung", ",", "doch", "so", "viel", "Heu", "und", "Korn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADJA", "NN", "$,", "ADV", "ADV", "PIAT", "NN", "KON", "NN"], "meter": "-+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.80": {"text": "erhalten,", "tokens": ["er\u00b7hal\u00b7ten", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.81": {"text": "Es fast ein doppelt Wunder sey; in welchem man zugleich", "tokens": ["Es", "fast", "ein", "dop\u00b7pelt", "Wun\u00b7der", "sey", ";", "in", "wel\u00b7chem", "man", "zu\u00b7gleich"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "ART", "ADJD", "NN", "VAFIN", "$.", "APPR", "PRELS", "PIS", "ADV"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.82": {"text": "entdeckt,", "tokens": ["ent\u00b7deckt", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.83": {"text": "Da\u00df in dem Wege der Natur ein\u2019 offenbahre Warnung", "tokens": ["Da\u00df", "in", "dem", "We\u00b7ge", "der", "Na\u00b7tur", "ein'", "of\u00b7fen\u00b7bah\u00b7re", "War\u00b7nung"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.84": {"text": "steckt,", "tokens": ["steckt", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.85": {"text": "Da\u00df wir nicht k\u00fcnftig, wie vorhin, bey unserer Gewohnheit", "tokens": ["Da\u00df", "wir", "nicht", "k\u00fcnf\u00b7tig", ",", "wie", "vor\u00b7hin", ",", "bey", "un\u00b7se\u00b7rer", "Ge\u00b7wohn\u00b7heit"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "PTKNEG", "ADJD", "$,", "PWAV", "ADV", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.86": {"text": "bleiben,", "tokens": ["blei\u00b7ben", ","], "token_info": ["word", "punct"], "pos": ["VVINF", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.87": {"text": "Der Erndte Segen unserm Flei\u00df und unsrer Arbeit zuzu-", "tokens": ["Der", "Ernd\u00b7te", "Se\u00b7gen", "un\u00b7serm", "Flei\u00df", "und", "uns\u00b7rer", "Ar\u00b7beit", "zu\u00b7zu"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.88": {"text": "schreiben,", "tokens": ["schrei\u00b7ben", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.89": {"text": "Und denken, ohne da\u00df wir uns der Huld des grossen Gebers", "tokens": ["Und", "den\u00b7ken", ",", "oh\u00b7ne", "da\u00df", "wir", "uns", "der", "Huld", "des", "gros\u00b7sen", "Ge\u00b7bers"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVINF", "$,", "KOUI", "KOUS", "PPER", "PRF", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.90": {"text": "freuen,", "tokens": ["freu\u00b7en", ","], "token_info": ["word", "punct"], "pos": ["VVINF", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.91": {"text": "Es sey des Himmels Schuldigkeit, die Erndte m\u00fcsse wohl", "tokens": ["Es", "sey", "des", "Him\u00b7mels", "Schul\u00b7dig\u00b7keit", ",", "die", "Ernd\u00b7te", "m\u00fcs\u00b7se", "wohl"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "NN", "$,", "ART", "NN", "VMFIN", "ADV"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.92": {"text": "gedeyen.", "tokens": ["ge\u00b7de\u00b7yen", "."], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.93": {"text": "Auch la\u00dft uns sonderlich hiebey zu \u00fcberlegen nicht vergessen,", "tokens": ["Auch", "la\u00dft", "uns", "son\u00b7der\u00b7lich", "hie\u00b7bey", "zu", "\u00fc\u00b7berl\u00b7e\u00b7gen", "nicht", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "PTKZU", "VVINF", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.94": {"text": "Was vor ein Schatz im Grase stecke, und seinen grossen", "tokens": ["Was", "vor", "ein", "Schatz", "im", "Gra\u00b7se", "ste\u00b7cke", ",", "