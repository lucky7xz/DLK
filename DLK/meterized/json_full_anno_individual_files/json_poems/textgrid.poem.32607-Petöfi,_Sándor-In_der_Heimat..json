{"textgrid.poem.32607": {"metadata": {"author": {"name": "Pet\u00f6fi, S\u00e1ndor", "birth": "N.A.", "death": "N.A."}, "title": "In der Heimat.", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du Ebene im goldnen \u00c4hrenschmuck,", "tokens": ["Du", "E\u00b7be\u00b7ne", "im", "gold\u00b7nen", "\u00c4h\u00b7ren\u00b7schmuck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wo D\u00e9lib\u00e1b", "tokens": ["Wo", "D\u00e9\u00b7lib\u00e1b"], "token_info": ["word", "word"], "pos": ["PWAV", "NE"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Uns neckt, wenn hei\u00dfe Strahlen niederlohn,", "tokens": ["Uns", "neckt", ",", "wenn", "hei\u00b7\u00dfe", "Strah\u00b7len", "nie\u00b7der\u00b7lohn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Kennst du mich noch? Erkenne deinen Sohn!", "tokens": ["Kennst", "du", "mich", "noch", "?", "Er\u00b7ken\u00b7ne", "dei\u00b7nen", "Sohn", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "$.", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Lang ist es her, da ich, ein junges Blut,", "tokens": ["Lang", "ist", "es", "her", ",", "da", "ich", ",", "ein", "jun\u00b7ges", "Blut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "PTKVZ", "$,", "KOUS", "PPER", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Im Schatten deiner Pappeln hier geruht,", "tokens": ["Im", "Schat\u00b7ten", "dei\u00b7ner", "Pap\u00b7peln", "hier", "ge\u00b7ruht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und \u00fcber meinem Haupt im wilden Flug", "tokens": ["Und", "\u00fc\u00b7ber", "mei\u00b7nem", "Haupt", "im", "wil\u00b7den", "Flug"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die Luft durchschnitt der V\u00f6gel Wanderzug.", "tokens": ["Die", "Luft", "durch\u00b7schnitt", "der", "V\u00f6\u00b7gel", "Wan\u00b7der\u00b7zug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Da in mein Vaterhaus zuletzt ich kam,", "tokens": ["Da", "in", "mein", "Va\u00b7ter\u00b7haus", "zu\u00b7letzt", "ich", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und dann, gebrochnen Herzens, Abschied nahm,", "tokens": ["Und", "dann", ",", "ge\u00b7broch\u00b7nen", "Her\u00b7zens", ",", "Ab\u00b7schied", "nahm", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "ADJA", "NN", "$,", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die gute Mutter ihre einz'ge Hab',", "tokens": ["Die", "gu\u00b7te", "Mut\u00b7ter", "ih\u00b7re", "einz'\u00b7ge", "Hab'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ein segnend Wort, mir zum Geleite gab!", "tokens": ["Ein", "seg\u00b7nend", "Wort", ",", "mir", "zum", "Ge\u00b7lei\u00b7te", "gab", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$,", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Geboren ward seitdem so manches Jahr,", "tokens": ["Ge\u00b7bo\u00b7ren", "ward", "seit\u00b7dem", "so", "man\u00b7ches", "Jahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PAV", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und manches starb dahin, das traurig war;", "tokens": ["Und", "man\u00b7ches", "starb", "da\u00b7hin", ",", "das", "trau\u00b7rig", "war", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PAV", "$,", "PRELS", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mein Schicksalswagen f\u00fchrt' mich hin und her,", "tokens": ["Mein", "Schick\u00b7sals\u00b7wa\u00b7gen", "f\u00fchrt'", "mich", "hin", "und", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die weite Welt durchzog ich kreuz und quer.", "tokens": ["Die", "wei\u00b7te", "Welt", "durch\u00b7zog", "ich", "kreuz", "und", "quer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "NN", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Des Lebens Schule ist die fremde Welt,", "tokens": ["Des", "Le\u00b7bens", "Schu\u00b7le", "ist", "die", "frem\u00b7de", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Es kostet Schwei\u00df, da\u00df man sich da erh\u00e4lt,", "tokens": ["Es", "kos\u00b7tet", "Schwei\u00df", ",", "da\u00df", "man", "sich", "da", "er\u00b7h\u00e4lt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,", "KOUS", "PIS", "PRF", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Weg ist hart und holprig auch dabei,", "tokens": ["Der", "Weg", "ist", "hart", "und", "holp\u00b7rig", "auch", "da\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "ADJD", "ADV", "PAV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Er f\u00fchrt uns oft in \u00f6de W\u00fcstenei.", "tokens": ["Er", "f\u00fchrt", "uns", "oft", "in", "\u00f6\u00b7de", "W\u00fcs\u00b7te\u00b7nei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Das kenn' ich wie's kein andrer kennt, f\u00fcrwahr,", "tokens": ["Das", "kenn'", "ich", "wie's", "kein", "an\u00b7drer", "kennt", ",", "f\u00fcr\u00b7wahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "VVFIN", "PIAT", "PIS", "VVFIN", "$,", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mein Los bot mir den, Wermutbecher dar,", "tokens": ["Mein", "Los", "bot", "mir", "den", ",", "Wer\u00b7mut\u00b7be\u00b7cher", "dar", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ART", "$,", "NN", "PTKVZ", "$,"], "meter": "-+-+-+---+", "measure": "zehnsilber"}, "line.3": {"text": "Ich trank ihn leer, \u2013 nur Bitternis und Pein,", "tokens": ["Ich", "trank", "ihn", "leer", ",", "\u2013", "nur", "Bit\u00b7ter\u00b7nis", "und", "Pein", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "$,", "$(", "ADV", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Gewi\u00df, der Tod kann auch nicht bittrer sein!", "tokens": ["Ge\u00b7wi\u00df", ",", "der", "Tod", "kann", "auch", "nicht", "bit\u00b7trer", "sein", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ART", "NN", "VMFIN", "ADV", "PTKNEG", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Jetzt aber, was an Leid mich \u00fcberkam,", "tokens": ["Jetzt", "a\u00b7ber", ",", "was", "an", "Leid", "mich", "\u00fc\u00b7ber\u00b7kam", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "PRELS", "APPR", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und was mein Herz so arg erf\u00fcllt mit Gram,", "tokens": ["Und", "was", "mein", "Herz", "so", "arg", "er\u00b7f\u00fcllt", "mit", "Gram", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPOSAT", "NN", "ADV", "ADJD", "VVPP", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dies alles sp\u00fclt auf diesem Erdenfleck", "tokens": ["Dies", "al\u00b7les", "sp\u00fclt", "auf", "die\u00b7sem", "Er\u00b7den\u00b7fleck"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "PIS", "VVFIN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die Tr\u00e4ne einer heil'gen Freude weg!", "tokens": ["Die", "Tr\u00e4\u00b7ne", "ei\u00b7ner", "heil'\u00b7gen", "Freu\u00b7de", "weg", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Hier wiegte Mutter mich, hier ist mein Heim,", "tokens": ["Hier", "wieg\u00b7te", "Mut\u00b7ter", "mich", ",", "hier", "ist", "mein", "Heim", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "PPER", "$,", "ADV", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Hier saugt' an ihrer Brust ich Honigseim,", "tokens": ["Hier", "saugt'", "an", "ih\u00b7rer", "Brust", "ich", "Ho\u00b7ni\u00b7gseim", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN", "PPER", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und heiter l\u00e4chelt hier die Sonne schon", "tokens": ["Und", "hei\u00b7ter", "l\u00e4\u00b7chelt", "hier", "die", "Son\u00b7ne", "schon"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "ADV", "ART", "NN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Du sch\u00f6ne Heimat, deinem treuen Sohn!", "tokens": ["Du", "sch\u00f6\u00b7ne", "Hei\u00b7mat", ",", "dei\u00b7nem", "treu\u00b7en", "Sohn", "!"], "token_info