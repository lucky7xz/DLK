{"textgrid.poem.39899": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "1L: Trommel schallt,", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Trommel schallt,", "tokens": ["Trom\u00b7mel", "schallt", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Lustig wallt", "tokens": ["Lus\u00b7tig", "wallt"], "token_info": ["word", "word"], "pos": ["NE", "VVFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Auf dem Weg Turenne's Heer,", "tokens": ["Auf", "dem", "Weg", "Tu\u00b7ren\u00b7ne's", "Heer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.4": {"text": "Wie die Flut im blauen Meer,", "tokens": ["Wie", "die", "Flut", "im", "blau\u00b7en", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Alles zieht", "tokens": ["Al\u00b7les", "zieht"], "token_info": ["word", "word"], "pos": ["PIS", "VVFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "In geschlossnem Glied.", "tokens": ["In", "ge\u00b7schloss\u00b7nem", "Glied", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Einer nur", "tokens": ["Ei\u00b7ner", "nur"], "token_info": ["word", "word"], "pos": ["PIS", "ADV"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Durch die Flur", "tokens": ["Durch", "die", "Flur"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Schlendert seitw\u00e4rts von der Schar,", "tokens": ["Schlen\u00b7dert", "seit\u00b7w\u00e4rts", "von", "der", "Schar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Nimmt der strengen Zucht nicht wahr,", "tokens": ["Nimmt", "der", "stren\u00b7gen", "Zucht", "nicht", "wahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PTKNEG", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Lehnt am Baum", "tokens": ["Lehnt", "am", "Baum"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "APPRART", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Im Gedankentraum.", "tokens": ["Im", "Ge\u00b7dan\u00b7ken\u00b7traum", "."], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Bald erwacht", "tokens": ["Bald", "er\u00b7wacht"], "token_info": ["word", "word"], "pos": ["ADV", "VVPP"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Er nicht sacht.", "tokens": ["Er", "nicht", "sacht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Denn der Marschall stie\u00df mit Zorn", "tokens": ["Denn", "der", "Mar\u00b7schall", "stie\u00df", "mit", "Zorn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "NN"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "In sein schnelles Ro\u00df den Sporn,", "tokens": ["In", "sein", "schnel\u00b7les", "Ro\u00df", "den", "Sporn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Jenem gab", "tokens": ["Je\u00b7nem", "gab"], "token_info": ["word", "word"], "pos": ["PDS", "VVFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Grimmen Streich sein Stab.", "tokens": ["Grim\u00b7men", "Streich", "sein", "Stab", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "PPOSAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "\u00bbfort von hier,", "tokens": ["\u00bb", "fort", "von", "hier", ","], "token_info": ["punct", "word", "word", "word", "punct"], "pos": ["$(", "PTKVZ", "APPR", "ADV", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Musketier!", "tokens": ["Mus\u00b7ke\u00b7tier", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Willst du schnell in Reih und Glied?\u00ab", "tokens": ["Willst", "du", "schnell", "in", "Reih", "und", "Glied", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "PPER", "ADJD", "APPR", "NN", "KON", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Doch ein dunkles Auge sieht", "tokens": ["Doch", "ein", "dunk\u00b7les", "Au\u00b7ge", "sieht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Unterm Hut", "tokens": ["Un\u00b7term", "Hut"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Zu ihm auf in Glut.", "tokens": ["Zu", "ihm", "auf", "in", "Glut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "APPR", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Herr! Euch reut,", "tokens": ["Herr", "!", "Euch", "reut", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Da\u00df Ihr heut", "tokens": ["Da\u00df", "Ihr", "heut"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPER", "ADV"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Einen Braven unbefugt", "tokens": ["Ei\u00b7nen", "Bra\u00b7ven", "un\u00b7be\u00b7fugt"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Um des kleinen Fehles schlugt!\u00ab \u2013", "tokens": ["Um", "des", "klei\u00b7nen", "Feh\u00b7les", "schlugt", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "VVFIN", "$.", "$(", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Murrt's in Bart", "tokens": ["Murrt's", "in", "Bart"], "token_info": ["word", "word", "word"], "pos": ["NE", "APPR", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Nach Soldatenart.", "tokens": ["Nach", "Sol\u00b7da\u00b7ten\u00b7art", "."], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NE", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Trommel schallt,", "tokens": ["Trom\u00b7mel", "schallt", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Feld und Wald", "tokens": ["Feld", "und", "Wald"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Zieht das stolze Heer entlang:", "tokens": ["Zieht", "das", "stol\u00b7ze", "Heer", "ent\u00b7lang", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Hoch vom Felsen\u00fcberhang", "tokens": ["Hoch", "vom", "Fel\u00b7sen\u00b7\u00fc\u00b7ber\u00b7hang"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Aus dem Moos", "tokens": ["Aus", "dem", "Moos"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "--+", "measure": "anapaest.init"}, "line.6": {"text": "Ragt es riesengro\u00df.", "tokens": ["Ragt", "es", "rie\u00b7sen\u00b7gro\u00df", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "Finster liegt", "tokens": ["Fins\u00b7ter", "liegt"], "token_info": ["word", "word"], "pos": ["NN", "VVFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Nie besiegt,", "tokens": ["Nie", "be\u00b7siegt", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVPP", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Nicht vom Hunger, nicht vom Sturm,", "tokens": ["Nicht", "vom", "Hun\u00b7ger", ",", "nicht", "vom", "Sturm", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPRART", "NN", "$,", "PTKNEG", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Dort die Veste Thurm an Thurm.", "tokens": ["Dort", "die", "Ves\u00b7te", "Thurm", "an", "Thurm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "\u00bbauf zum Wall!\u00ab", "tokens": ["\u00bb", "auf", "zum", "Wall", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "APPR", "APPRART", "NN", "$.", "$("], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Ruft der Feldmarschall.", "tokens": ["Ruft", "der", "Feld\u00b7mar\u00b7schall", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.8": {"line.1": {"text": "Z\u00f6gernd sieht", "tokens": ["Z\u00f6\u00b7gernd", "sieht"], "token_info": ["word", "word"], "pos": ["VVPP", "VVFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Glied um Glied", "tokens": ["Glied", "um", "Glied"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPR", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "An dem steilen Stein empor,", "tokens": ["An", "dem", "stei\u00b7len", "Stein", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Endlich treten zwanzig vor:", "tokens": ["End\u00b7lich", "tre\u00b7ten", "zwan\u00b7zig", "vor", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "CARD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "\u00bbfolget mir!\u00ab", "tokens": ["\u00bb", "fol\u00b7get", "mir", "!", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "PPER", "$.", "$("], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Ruft ein Musketier.", "tokens": ["Ruft", "ein", "Mus\u00b7ke\u00b7tier", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.9": {"line.1": {"text": "Pulverdampf,", "tokens": ["Pul\u00b7ver\u00b7dampf", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Sturm und Kampf;", "tokens": ["Sturm", "und", "Kampf", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Von der Leiter st\u00fcrzen viel!", "tokens": ["Von", "der", "Lei\u00b7ter", "st\u00fcr\u00b7zen", "viel", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Jetzund oder nie an's Ziel!", "tokens": ["Je\u00b7tzund", "o\u00b7der", "nie", "an's", "Ziel", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADV", "APPRART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Einer steht", "tokens": ["Ei\u00b7ner", "steht"], "token_info": ["word", "word"], "pos": ["PIS", "VVFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Von der Fahn' umweht.", "tokens": ["Von", "der", "Fahn'", "um\u00b7weht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.10": {"line.1": {"text": "Jubelschall", "tokens": ["Ju\u00b7bel\u00b7schall"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "T\u00f6nt vom Wall.", "tokens": ["T\u00f6nt", "vom", "Wall", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NE", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "\u00bbsagt, wer drang so k\u00fchn empor,", "tokens": ["\u00bb", "sagt", ",", "wer", "drang", "so", "k\u00fchn", "em\u00b7por", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$,", "PWS", "VVFIN", "ADV", "ADJD", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sagt, wer \u00f6ffnet uns das Thor?\u00ab \u2013", "tokens": ["