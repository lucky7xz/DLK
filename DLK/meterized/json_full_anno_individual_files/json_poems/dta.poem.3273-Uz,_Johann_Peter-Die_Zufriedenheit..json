{"dta.poem.3273": {"metadata": {"author": {"name": "Uz, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "Die Zufriedenheit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1755", "urn": "urn:nbn:de:kobv:b4-20614-6", "language": ["de:0.99"], "booktitle": "Uz, Johann Peter: Lyrische und andere Gedichte. 2. Aufl. Ansbach, 1755."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Geist, der sich zu keiner Zeit", "tokens": ["Ein", "Geist", ",", "der", "sich", "zu", "kei\u00b7ner", "Zeit"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PRF", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In feiger Ungeduld verlieret,", "tokens": ["In", "fei\u00b7ger", "Un\u00b7ge\u00b7duld", "ver\u00b7lie\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und stets die Weisheit h\u00f6rt, die, wie das Gl\u00fcck", "tokens": ["Und", "stets", "die", "Weis\u00b7heit", "h\u00f6rt", ",", "die", ",", "wie", "das", "Gl\u00fcck"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "VVFIN", "$,", "PRELS", "$,", "PWAV", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "uns s\u00fchret,", "tokens": ["uns", "s\u00fch\u00b7ret", ","], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Mit Rosen ieden Pfad bestreut:", "tokens": ["Mit", "Ro\u00b7sen", "ie\u00b7den", "Pfad", "be\u00b7streut", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Freund! ein wahrhaftig weiser Geist", "tokens": ["Freund", "!", "ein", "wahr\u00b7haf\u00b7tig", "wei\u00b7ser", "Geist"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "ART", "ADJD", "ADJA", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "F\u00fchlt kaum die halbe Last der Plagen,", "tokens": ["F\u00fchlt", "kaum", "die", "hal\u00b7be", "Last", "der", "Pla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und lacht bey tr\u00fcber Luft in angenehmern Tagen,", "tokens": ["Und", "lacht", "bey", "tr\u00fc\u00b7ber", "Luft", "in", "an\u00b7ge\u00b7neh\u00b7mern", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Als Thoren, die man gl\u00fccklich preist.", "tokens": ["Als", "Tho\u00b7ren", ",", "die", "man", "gl\u00fcck\u00b7lich", "preist", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "PRELS", "PIS", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Schilt nicht des Himmels Tyranney,", "tokens": ["Schilt", "nicht", "des", "Him\u00b7mels", "Ty\u00b7ran\u00b7ney", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von ihm kommt unser wenigst Leiden.", "tokens": ["Von", "ihm", "kommt", "un\u00b7ser", "we\u00b7nigst", "Lei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PPOSAT", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Kein Zustand ist so hart: ein Chor der stillen Freuden", "tokens": ["Kein", "Zu\u00b7stand", "ist", "so", "hart", ":", "ein", "Chor", "der", "stil\u00b7len", "Freu\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VAFIN", "ADV", "ADJD", "$.", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Gesellt sich ihm mitleidig bey.", "tokens": ["Ge\u00b7sellt", "sich", "ihm", "mit\u00b7lei\u00b7dig", "bey", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wir fr\u00f6hnen th\u00f6richter Begier,", "tokens": ["Wir", "fr\u00f6h\u00b7nen", "th\u00f6\u00b7rich\u00b7ter", "Be\u00b7gier", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Die auch bey nahen Quellen schmachtet.", "tokens": ["Die", "auch", "bey", "na\u00b7hen", "Quel\u00b7len", "schmach\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Vergn\u00fcgen beut sich an: umsonst! es wird verachtet;", "tokens": ["Ver\u00b7gn\u00fc\u00b7gen", "beut", "sich", "an", ":", "um\u00b7sonst", "!", "es", "wird", "ver\u00b7ach\u00b7tet", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "PTKVZ", "$.", "ADV", "$.", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nur was uns flieht, verfolgen wir.", "tokens": ["Nur", "was", "uns", "flieht", ",", "ver\u00b7fol\u00b7gen", "wir", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PWS", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Zu ekel sind wir, uns zur Pein:", "tokens": ["Zu", "e\u00b7kel", "sind", "wir", ",", "uns", "zur", "Pein", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "VAFIN", "PPER", "$,", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wir lassen West und Sommer weichen,", "tokens": ["Wir", "las\u00b7sen", "West", "und", "Som\u00b7mer", "wei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und wollen, wann sie fliehn, in schattigten Gestr\u00e4uchen,", "tokens": ["Und", "wol\u00b7len", ",", "wann", "sie", "fliehn", ",", "in", "schat\u00b7tig\u00b7ten", "Ge\u00b7str\u00e4u\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "$,", "PWAV", "PPER", "VVINF", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Um murmelnd Wasser fr\u00f6hlig seyn.", "tokens": ["Um", "mur\u00b7melnd", "Was\u00b7ser", "fr\u00f6h\u00b7lig", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADJD", "NN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Der warme Fr\u00fchling kommt zur\u00fcck:", "tokens": ["Der", "war\u00b7me", "Fr\u00fch\u00b7ling", "kommt", "zu\u00b7r\u00fcck", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da braucht ein Weiser ihn beyzeiten.", "tokens": ["Da", "braucht", "ein", "Wei\u00b7ser", "ihn", "bey\u00b7zei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Er l\u00e4\u00dft Vernunft allein die blinden W\u00fcnsche leiten,", "tokens": ["Er", "l\u00e4\u00dft", "Ver\u00b7nunft", "al\u00b7lein", "die", "blin\u00b7den", "W\u00fcn\u00b7sche", "lei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "ADV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und w\u00fcnscht kein schimmerreiches Gl\u00fcck.", "tokens": ["Und", "w\u00fcnscht", "kein", "schim\u00b7mer\u00b7rei\u00b7ches", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Kein stolzer Schein beth\u00f6rt sein Herz:", "tokens": ["Kein", "stol\u00b7zer", "Schein", "be\u00b7th\u00f6rt", "sein", "Herz", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er sch\u00e4tzt nicht blo\u00df ein theures Lachen;", "tokens": ["Er", "sch\u00e4tzt", "nicht", "blo\u00df", "ein", "theu\u00b7res", "La\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und kan des P\u00f6bels Wahn durch sich zu schanden machen,", "tokens": ["Und", "kan", "des", "P\u00f6\u00b7bels", "Wahn", "durch", "sich", "zu", "schan\u00b7den", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "NN", "NN", "APPR", "PRF", "PTKZU", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ob fl\u00f6h uns Arme Lust und Scherz.", "tokens": ["Ob", "fl\u00f6h", "uns", "Ar\u00b7me", "Lust", "und", "Scherz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Weil ich nicht pr\u00e4chtig schmausen kann,", "tokens": ["Weil", "ich", "nicht", "pr\u00e4ch\u00b7tig", "schmau\u00b7sen", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADJD", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Soll ich nicht fr\u00f6hlig schmausen k\u00f6nnen?", "tokens": ["Soll", "ich", "nicht", "fr\u00f6h\u00b7lig", "schmau\u00b7sen", "k\u00f6n\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "ADJD", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Will Flora, f\u00fcr mein Haar, mir holde Rosen g\u00f6nnen;", "tokens": ["Will", "Flo\u00b7ra", ",", "f\u00fcr", "mein", "Haar", ",", "mir", "hol\u00b7de", "Ro\u00b7sen", "g\u00f6n\u00b7nen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "$,", "APPR", "PPOSAT", "NN", "$,", "PPER", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Was geht der F\u00fcrsten Pracht mich an?", "tokens": ["Was", "geht", "der", "F\u00fcrs\u00b7ten", "Pracht", "mich", "an", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Was hilfts zur Lust, wann ihre Wand", "tokens": ["Was", "hilfts", "zur", "Lust", ",", "wann", "ih\u00b7re", "Wand"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "VVFIN", "APPRART", "NN", "$,", "PWAV", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sich in gew\u00fcrktes Gold verh\u00fcllet,", "tokens": ["Sich", "in", "ge\u00b7w\u00fcrk\u00b7tes", "Gold", "ver\u00b7h\u00fcl\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und ein Bedienten-Schwarm die Marmor-S\u00e4le f\u00fcllet,", "tokens": ["Und", "ein", "Be\u00b7dien\u00b7ten\u00b7Schwarm", "die", "Mar\u00b7mor\u00b7S\u00e4\u00b7le", "f\u00fcl\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mit g\u00fcldnen Sch\u00fcsseln in der Hand?", "tokens": ["Mit", "g\u00fcld\u00b7nen", "Sch\u00fcs\u00b7seln", "in", "der", "Hand", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Sieh hin, wo keine Pracht gebricht!", "tokens": ["Sieh", "hin", ",", "wo", "kei\u00b7ne", "Pracht", "ge\u00b7bricht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "PWAV", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Man g\u00e4hnt auch mitten im Gepr\u00e4nge;", "tokens": ["Man", "g\u00e4hnt", "auch", "mit\u00b7ten", "im", "Ge\u00b7pr\u00e4n\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der Necktar Jupiters, der Speisen ekle Menge,", "tokens": ["Der", "Neck\u00b7tar", "Ju\u00b7pi\u00b7ters", ",", "der", "Spei\u00b7sen", "ek\u00b7le", "Men\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die fesseln, ach! die Freude nicht.", "tokens": ["Die", "fes\u00b7seln", ",", "ach", "!", "die", "Freu\u00b7de", "nicht", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ITJ", "$.", "ART", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Die Freude, des Ly\u00e4us Kind,", "tokens": ["Die", "Freu\u00b7de", ",", "des", "Ly\u00b7\u00e4us", "Kind", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NE", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Entflieht unruhigen Pal\u00e4sten,", "tokens": ["Ent\u00b7flieht", "un\u00b7ru\u00b7hi\u00b7gen", "Pa\u00b7l\u00e4s\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Und schw\u00e4rmt zu H\u00fctten hin, die nur gew\u00e4hlten G\u00e4sten,", "tokens": ["Und", "schw\u00e4rmt", "zu", "H\u00fct\u00b7ten", "hin", ",", "die", "nur", "ge\u00b7w\u00e4hl\u00b7ten", "G\u00e4s\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "PTKVZ", "$,", "PRELS", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nur dir, o Freundschaft! heilig sind.", "tokens": ["Nur", "dir", ",", "o", "Freund\u00b7schaft", "!", "hei\u00b7lig", "sind", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PPER", "$,", "FM", "NN", "$.", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Fleu\u00dft nicht f\u00fcr sie der Reben Blut,", "tokens": ["Fleu\u00dft", "nicht", "f\u00fcr", "sie", "der", "Re\u00b7ben", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "APPR", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Chios edle Berge schw\u00e4rzen?", "tokens": ["Die", "Chios", "ed\u00b7le", "Ber\u00b7ge", "schw\u00e4r\u00b7zen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Auch Bacchus unsers Rheins fl\u00f6\u00dft in zufriedne Herzen", "tokens": ["Auch", "Bac\u00b7chus", "un\u00b7sers", "Rheins", "fl\u00f6\u00dft", "in", "zu\u00b7fried\u00b7ne", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "NE", "NE", "NE", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Vertraulichkeit und guten Muth.", "tokens": ["Ver\u00b7trau\u00b7lich\u00b7keit", "und", "gu\u00b7ten", "Muth", ".