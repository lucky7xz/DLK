{"textgrid.poem.32800": {"metadata": {"author": {"name": "Miller, Johann Martin", "birth": "N.A.", "death": "N.A."}, "title": "An den Mond", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Meine Seele lebt nicht hier!", "tokens": ["Mei\u00b7ne", "See\u00b7le", "lebt", "nicht", "hier", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKNEG", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sie ist hingewandelt zu der Trauten,", "tokens": ["Sie", "ist", "hin\u00b7ge\u00b7wan\u00b7delt", "zu", "der", "Trau\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Die nun ewig mein ist!", "tokens": ["Die", "nun", "e\u00b7wig", "mein", "ist", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "PPOSAT", "VAFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Sag, o Hauch des Abends, mir", "tokens": ["Sag", ",", "o", "Hauch", "des", "A\u00b7bends", ",", "mir"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["NN", "$,", "FM", "NN", "ART", "NN", "$,", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "(du umwehtest sie mit deinen Schwingen),", "tokens": ["(", "du", "um\u00b7weh\u00b7test", "sie", "mit", "dei\u00b7nen", "Schwin\u00b7gen", ")", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$(", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Wo sie jetzo wandelt?", "tokens": ["Wo", "sie", "jet\u00b7zo", "wan\u00b7delt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Stark liebt ihre Seel', und treu!", "tokens": ["Stark", "liebt", "ih\u00b7re", "Seel'", ",", "und", "treu", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPOSAT", "NN", "$,", "KON", "ADJD", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Weint ihr Aug' jetzt, da\u00df ihr Lieber fern ist?", "tokens": ["Weint", "ihr", "Aug'", "jetzt", ",", "da\u00df", "ihr", "Lie\u00b7ber", "fern", "ist", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADV", "$,", "KOUS", "PPER", "ADJD", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+-++", "measure": "unknown.measure.hexa"}, "line.3": {"text": "Sag mir's, Hauch des Abends!", "tokens": ["Sag", "mir's", ",", "Hauch", "des", "A\u00b7bends", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "NN", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Sieh, da tritt der Mond hervor;", "tokens": ["Sieh", ",", "da", "tritt", "der", "Mond", "her\u00b7vor", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Bleich ist sein Gesicht, und melancholisch,", "tokens": ["Bleich", "ist", "sein", "Ge\u00b7sicht", ",", "und", "me\u00b7lan\u00b7cho\u00b7lisch", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPOSAT", "NN", "$,", "KON", "ADJD", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text