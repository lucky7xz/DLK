{"textgrid.poem.42929": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "\u00dcber meinen gestrigen Traum", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie kam ich gerade auf ein Gestirn?", "tokens": ["Wie", "kam", "ich", "ge\u00b7ra\u00b7de", "auf", "ein", "Ge\u00b7stirn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Du sagst: Ich st\u00f6hnte tr\u00e4umend ganz laut.", "tokens": ["Du", "sagst", ":", "Ich", "st\u00f6hn\u00b7te", "tr\u00e4u\u00b7mend", "ganz", "laut", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VVFIN", "ADJD", "ADV", "ADJD", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Vielleicht steigt die Phantasie ins Hirn,", "tokens": ["Viel\u00b7leicht", "steigt", "die", "Phan\u00b7ta\u00b7sie", "ins", "Hirn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Wenn der Magen verdaut.", "tokens": ["Wenn", "der", "Ma\u00b7gen", "ver\u00b7daut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVPP", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Man sollte kurz vorm Schlafengehen", "tokens": ["Man", "soll\u00b7te", "kurz", "vorm", "Schla\u00b7fen\u00b7ge\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VMFIN", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nichts essen. Auch war ich gestern bezecht.", "tokens": ["Nichts", "es\u00b7sen", ".", "Auch", "war", "ich", "ge\u00b7stern", "be\u00b7zecht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVINF", "$.", "ADV", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+---+-+", "measure": "zehnsilber"}, "line.3": {"text": "Doch warum tr\u00e4ume ich immer nur schlecht,", "tokens": ["Doch", "wa\u00b7rum", "tr\u00e4u\u00b7me", "ich", "im\u00b7mer", "nur", "schlecht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "VVFIN", "PPER", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Nie gut. Das kann ich nicht verstehen.", "tokens": ["Nie", "gut", ".", "Das", "kann", "ich", "nicht", "ver\u00b7ste\u00b7hen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$.", "PDS", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ob auf der Seite, ob auf dem R\u00fccken", "tokens": ["Ob", "auf", "der", "Sei\u00b7te", ",", "ob", "auf", "dem", "R\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "$,", "KOUS", "APPR", "ART", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Oder auch auf dem Bauch \u2013 \u2013", "tokens": ["O\u00b7der", "auch", "auf", "dem", "Bauch", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "$(", "$("], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Immer nur Schlimmes. \u00bbAlpdr\u00fccken.\u00ab", "tokens": ["Im\u00b7mer", "nur", "Schlim\u00b7mes", ".", "\u00bb", "Alp\u00b7dr\u00fc\u00b7cken", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "punct"], "pos": ["ADV", "ADV", "NN", "$.", "$(", "NN", "$.", "$("], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Aber Name ist Schall und Rauch.", "tokens": ["A\u00b7ber", "Na\u00b7me", "ist", "Schall", "und", "Rauch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "NN", "KON", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.4": {"line.1": {"text": "Meist von der Schule und vom Milit\u00e4r \u2013 \u2013", "tokens": ["Meist", "von", "der", "Schu\u00b7le", "und", "vom", "Mi\u00b7li\u00b7t\u00e4r", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "KON", "APPRART", "NN", "$(", "$("], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Als ob ich schuldbeladen w\u00e4r \u2013 \u2013", "tokens": ["Als", "ob", "ich", "schuld\u00b7be\u00b7la\u00b7den", "w\u00e4r", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "KOUS", "PPER", "VVINF", "VAFIN", "$(", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und wenn ich aufwache, schwitze ich,", "tokens": ["Und", "wenn", "ich", "auf\u00b7wa\u00b7che", ",", "schwit\u00b7ze", "ich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Und manchmal kniee ich oder sitze ich,", "tokens": ["Und", "manch\u00b7mal", "kni\u00b7ee", "ich", "o\u00b7der", "sit\u00b7ze", "ich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "KON", "VVFIN", "PPER", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Du wei\u00dft ja, wie neulich!", "tokens": ["Du", "wei\u00dft", "ja", ",", "wie", "neu\u00b7lich", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PWAV", "ADV", "$."], "meter": "--+-+-", "measure": "anapaest.init"}, "line.6": {"text": "O, es ist greulich.", "tokens": ["O", ",", "es", "ist", "greu\u00b7lich", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VAFIN", "ADJD", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.5": {"line.1": {"text": "Warum man das \u00fcberhaupt weitererz\u00e4hlt?", "tokens": ["Wa\u00b7rum", "man", "das", "\u00fc\u00b7ber\u00b7haupt", "wei\u00b7te\u00b7rer\u00b7z\u00e4hlt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ART", "ADV", "VVFIN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Hat doch niemand Vergn\u00fcgen daran,", "tokens": ["Hat", "doch", "nie\u00b7mand", "Ver\u00b7gn\u00fc\u00b7gen", "da\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PIS", "NN", "PAV", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Weil man da frei heraus l\u00fcgen kann. \u2013", "tokens": ["Weil", "man", "da", "frei", "he\u00b7raus", "l\u00fc\u00b7gen", "kann", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PIS", "ADV", "ADJD", "PTKVZ", "VVFIN", "VMFIN", "$.", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Aber so ein Traum qu\u00e4lt.", "tokens": ["A\u00b7ber", "so", "ein", "Traum", "qu\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Gestern hab ich noch anders getr\u00e4umt:", "tokens": ["Ge\u00b7stern", "hab", "ich", "noch", "an\u00b7ders", "ge\u00b7tr\u00e4umt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Da waren etwa hundert Personen.", "tokens": ["Da", "wa\u00b7ren", "et\u00b7wa", "hun\u00b7dert", "Per\u00b7so\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "CARD", "NN", "$."], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Die haben die Dachwohnung ausger\u00e4umt,", "tokens": ["Die", "ha\u00b7ben", "die", "Dach\u00b7woh\u00b7nung", "aus\u00b7ge\u00b7r\u00e4umt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+--++-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Wo die Buchbinders wohnen.", "tokens": ["Wo", "die", "Buch\u00b7bin\u00b7ders", "woh\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVINF", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}}, "stanza.7": {"line.1": {"text": "Dann haben wir auf dem Dachsims getanzt.", "tokens": ["Dann", "ha\u00b7ben", "wir", "auf", "dem", "Dach\u00b7sims", "ge\u00b7tanzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Dann hast du mich, sagst du, aufgeweckt,", "tokens": ["Dann", "hast", "du", "mich", ",", "sagst", "du", ",", "auf\u00b7ge\u00b7weckt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "$,", "VVFIN", "PPER", "$,", "VVPP", "$,"], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Und ich, sagst du, sagte noch tr\u00e4umend erschreckt:", "tokens": ["Und", "ich", ",", "sagst", "du", ",", "sag\u00b7te", "noch", "tr\u00e4u\u00b7mend", "er\u00b7schreckt", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "VVFIN", "PPER", "$,", "VVFIN", "ADV", "ADJD", "VVPP", "$."], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "\u00bbich habe ein Sternschn\u00fcppchen gepflanzt.\u00ab", "tokens": ["\u00bb", "ich", "ha\u00b7be", "ein", "Stern\u00b7schn\u00fcpp\u00b7chen", "ge\u00b7pflanzt", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VAFIN", "ART", "NN", "VVPP", "$.", "$("], "meter": "-+--++--+", "measure": "iambic.tetra.relaxed"}}, "stanza.8": {"line.1": {"text": "Ich wei\u00df nur noch: Ich war vom Dach", "tokens": ["Ich", "wei\u00df", "nur", "noch", ":", "Ich", "war", "vom", "Dach"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "$.", "PPER", "VAFIN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Pl\u00f6tzlich fort und bei dir und war wach.", "tokens": ["Pl\u00f6tz\u00b7lich", "fort", "und", "bei", "dir", "und", "war", "wach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "KON", "APPR", "PPER", "KON", "VAFIN", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und du streicheltest mich wie ein P\u00fcppchen", "tokens": ["Und", "du", "strei\u00b7chel\u00b7test", "mich", "wie", "ein", "P\u00fcpp\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPER", "KOKOM", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und fragtest mich \u2013 ach, so r\u00fchrend war das \u2013", "tokens": ["Und", "frag\u00b7test", "mich", "\u2013", "ach", ",", "so", "r\u00fch\u00b7rend", "war", "das", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$(", "XY", "$,", "ADV", "VVPP", "VAFIN", "ART", "$("], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Fragtest mich immer wieder: \u00bbWas", "tokens": ["Frag\u00b7test", "mich", "im\u00b7mer", "wie\u00b7der", ":", "\u00bb", "Was"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "$.", "$(", "PWS"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Hast du gepflanzt!? Ein Sternschn\u00fcppchen?\u00ab", "tokens": ["Hast", "du", "ge\u00b7pflanzt", "!?", "Ein", "Stern\u00b7schn\u00fcpp\u00b7chen", "?", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "$.", "ART", "NN", "$.", "$("], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}}, "stanza.9": {"line.1": {"text": "Wie kam ich gerade auf ein Gestirn?", "tokens": ["Wie", "kam", "ich", "ge\u00b7ra\u00b7de", "auf", "ein", "Ge\u00b7stirn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Du sagst: Ich st\u00f6hnte tr\u00e4umend ganz laut.", "tokens": ["Du", "sagst", ":", "Ich", "st\u00f6hn\u00b7te", "tr\u00e4u\u00b7mend", "ganz", "laut", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VVFIN", "ADJD", "ADV", "ADJD", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}