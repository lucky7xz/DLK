{"dta.poem.1142": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "28. Die Sch\u00f6ne ohn Geist und Bewegung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Du fremdem Sinne nach zu unbesinntes Kind/ ", "tokens": ["Du", "frem\u00b7dem", "Sin\u00b7ne", "nach", "zu", "un\u00b7be\u00b7sinn\u00b7tes", "Kind", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "APPR", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Kind aber/ dessen Glantz der Sonne beyzusetzen/", "tokens": ["Kind", "a\u00b7ber", "/", "des\u00b7sen", "Glantz", "der", "Son\u00b7ne", "bey\u00b7zu\u00b7set\u00b7zen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$(", "PRELAT", "NN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Weil man nicht lauter Geist und Feuer bey dir findt/", "tokens": ["Weil", "man", "nicht", "lau\u00b7ter", "Geist", "und", "Feu\u00b7er", "bey", "dir", "findt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PTKNEG", "PIAT", "NN", "KON", "NN", "APPR", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Soll deine Sch\u00f6nheit drum geringer seyn zu sch\u00e4tzen?", "tokens": ["Soll", "dei\u00b7ne", "Sch\u00f6n\u00b7heit", "drum", "ge\u00b7rin\u00b7ger", "seyn", "zu", "sch\u00e4t\u00b7zen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "PAV", "ADJD", "VAINF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wenn mich der Wangen Glutt/ der Haare Gold macht", "tokens": ["Wenn", "mich", "der", "Wan\u00b7gen", "Glutt", "/", "der", "Haa\u00b7re", "Gold", "macht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "$(", "ART", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "blind/ ", "tokens": ["blind", "/"], "token_info": ["word", "punct"], "pos": ["ADJD", "$("], "meter": "+", "measure": "single.up"}, "line.7": {"text": "Wenn mich die Lilien schneeweisser Scho\u00df ergetzen/", "tokens": ["Wenn", "mich", "die", "Li\u00b7li\u00b7en", "schnee\u00b7weis\u00b7ser", "Scho\u00df", "er\u00b7get\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Seh ich ein Wunderwerck/ das meinen Sinn verbindt/", "tokens": ["Seh", "ich", "ein", "Wun\u00b7der\u00b7werck", "/", "das", "mei\u00b7nen", "Sinn", "ver\u00b7bindt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$(", "ART", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Dich/ sch\u00f6ner Stein/ zu ehrn als einen Wunder-G\u00f6tzen.", "tokens": ["Dich", "/", "sch\u00f6\u00b7ner", "Stein", "/", "zu", "ehrn", "als", "ei\u00b7nen", "Wun\u00b7der\u00b7G\u00f6t\u00b7zen", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$(", "ADJA", "NN", "$(", "PTKZU", "VVINF", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Der Himmel schnizte selbst di\u00df sch\u00f6ne Menschen-Bild/", "tokens": ["Der", "Him\u00b7mel", "schniz\u00b7te", "selbst", "di\u00df", "sch\u00f6\u00b7ne", "Men\u00b7schen\u00b7Bild", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "PDS", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "(ein Bild/ das aber lebt/ und sich bewegen kan/", "tokens": ["(", "ein", "Bild", "/", "das", "a\u00b7ber", "lebt", "/", "und", "sich", "be\u00b7we\u00b7gen", "kan", "/"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "$(", "PDS", "ADV", "VVFIN", "$(", "KON", "PRF", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Der hat die zarte Brust mit Marmol ausgef\u00fcllt.", "tokens": ["Der", "hat", "die", "zar\u00b7te", "Brust", "mit", "Mar\u00b7mol", "aus\u00b7ge\u00b7f\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "APPR", "NE", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Es sah dich die Natur von aller ihrer Zier", "tokens": ["Es", "sah", "dich", "die", "Na\u00b7tur", "von", "al\u00b7ler", "ih\u00b7rer", "Zier"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN", "APPR", "PIAT", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Bereichert/ selbst entsezt und mit Erstarren an:", "tokens": ["Be\u00b7rei\u00b7chert", "/", "selbst", "ent\u00b7sezt", "und", "mit", "Er\u00b7star\u00b7ren", "an", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "ADV", "VVPP", "KON", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Du schl\u00e4ffst in ihr noch heut/ und jene schl\u00e4fft in dir.", "tokens": ["Du", "schl\u00e4ffst", "in", "ihr", "noch", "heut", "/", "und", "je\u00b7ne", "schl\u00e4fft", "in", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "ADV", "ADV", "$(", "KON", "PDS", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}