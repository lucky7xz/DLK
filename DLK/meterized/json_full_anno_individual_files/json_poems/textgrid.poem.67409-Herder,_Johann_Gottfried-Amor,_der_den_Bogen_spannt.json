{"textgrid.poem.67409": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Amor, der den Bogen spannt", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf wen, o Knabe, r\u00fcstest Du den Bogen,", "tokens": ["Auf", "wen", ",", "o", "Kna\u00b7be", ",", "r\u00fcs\u00b7test", "Du", "den", "Bo\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWS", "$,", "FM", "NN", "$,", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den m\u00e4chtigen? Entr\u00fcstet pr\u00fcfst Du ihn.", "tokens": ["Den", "m\u00e4ch\u00b7ti\u00b7gen", "?", "Ent\u00b7r\u00fcs\u00b7tet", "pr\u00fcfst", "Du", "ihn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$.", "VVFIN", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wer hat zum Kampfe Dich herausgezogen,", "tokens": ["Wer", "hat", "zum", "Kamp\u00b7fe", "Dich", "her\u00b7aus\u00b7ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "APPRART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dich, den die Tapfersten vorsichtig fliehn?", "tokens": ["Dich", ",", "den", "die", "Tap\u00b7fers\u00b7ten", "vor\u00b7sich\u00b7tig", "fliehn", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "+--+---+-+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Der Th\u00f6richte! ihn hat sein Herz betrogen;", "tokens": ["Der", "Th\u00f6\u00b7rich\u00b7te", "!", "ihn", "hat", "sein", "Herz", "be\u00b7tro\u00b7gen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "PPER", "VAFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Das schw\u00e4chste Herz ist oft am Meisten k\u00fchn.", "tokens": ["Das", "schw\u00e4chs\u00b7te", "Herz", "ist", "oft", "am", "Meis\u00b7ten", "k\u00fchn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "APPRART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Bald wird er, seinen sichern Stolz zu b\u00fc\u00dfen,", "tokens": ["Bald", "wird", "er", ",", "sei\u00b7nen", "si\u00b7chern", "Stolz", "zu", "b\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "PPOSAT", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Den zweiten Pfeil von Dir erflehen m\u00fcssen.", "tokens": ["Den", "zwei\u00b7ten", "Pfeil", "von", "Dir", "er\u00b7fle\u00b7hen", "m\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPER", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wolan, o Knabe, zeuch hinaus zum Siege!", "tokens": ["Wo\u00b7lan", ",", "o", "Kna\u00b7be", ",", "zeuch", "hin\u00b7aus", "zum", "Sie\u00b7ge", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "FM", "NN", "$,", "VVIMP", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den schlausten Heuchelnden besiege Du;", "tokens": ["Den", "schlaus\u00b7ten", "Heu\u00b7cheln\u00b7den", "be\u00b7sie\u00b7ge", "Du", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dann w\u00e4hl, ich bitte, statt der losen Kriege", "tokens": ["Dann", "w\u00e4hl", ",", "ich", "bit\u00b7te", ",", "statt", "der", "lo\u00b7sen", "Krie\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "PPER", "ADV", "$,", "KOUI", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein ander Spiel und g\u00f6nn uns unsre Ruh!", "tokens": ["Ein", "an\u00b7der", "Spiel", "und", "g\u00f6nn", "uns", "uns\u00b7re", "Ruh", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Damit die Lieb' in holde Tr\u00e4um' uns wiege,", "tokens": ["Da\u00b7mit", "die", "Lieb'", "in", "hol\u00b7de", "Tr\u00e4um'", "uns", "wie\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "APPR", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Bind Du der Psyche Augen liebreich zu,", "tokens": ["Bind", "Du", "der", "Psy\u00b7che", "Au\u00b7gen", "lieb\u00b7reich", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Da\u00df wir, vergessend uns, wie Du erblinden", "tokens": ["Da\u00df", "wir", ",", "ver\u00b7ges\u00b7send", "uns", ",", "wie", "Du", "er\u00b7blin\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "$,", "VVPP", "PPER", "$,", "PWAV", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und sch\u00f6ner uns in Andern wiederfinden!", "tokens": ["Und", "sch\u00f6\u00b7ner", "uns", "in", "An\u00b7dern", "wie\u00b7der\u00b7fin\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "PPER", "APPR", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Auf wen, o Knabe, r\u00fcstest Du den Bogen,", "tokens": ["Auf", "wen", ",", "o", "Kna\u00b7be", ",", "r\u00fcs\u00b7test", "Du", "den", "Bo\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWS", "$,", "FM", "NN", "$,", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den m\u00e4chtigen? Entr\u00fcstet pr\u00fcfst Du ihn.", "tokens": ["Den", "m\u00e4ch\u00b7ti\u00b7gen", "?", "Ent\u00b7r\u00fcs\u00b7tet", "pr\u00fcfst", "Du", "ihn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$.", "VVFIN", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wer hat zum Kampfe Dich herausgezogen,", "tokens": ["Wer", "hat", "zum", "Kamp\u00b7fe", "Dich", "her\u00b7aus\u00b7ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "APPRART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dich, den die Tapfersten vorsichtig fliehn?", "tokens": ["Dich", ",", "den", "die", "Tap\u00b7fers\u00b7ten", "vor\u00b7sich\u00b7tig", "fliehn", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "ART