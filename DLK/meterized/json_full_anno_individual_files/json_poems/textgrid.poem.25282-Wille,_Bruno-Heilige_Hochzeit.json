meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und meine Seele will gesunden;", "tokens": ["Und", "mei\u00b7ne", "See\u00b7le", "will", "ge\u00b7sun\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VMFIN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Vergessen ist der morsche Leib.", "tokens": ["Ver\u00b7ges\u00b7sen", "ist", "der", "mor\u00b7sche", "Leib", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So hab ich endlich dich gefunden,", "tokens": ["So", "hab", "ich", "end\u00b7lich", "dich", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ersehnte Braut, mein Sonnenweib!", "tokens": ["Er\u00b7sehn\u00b7te", "Braut", ",", "mein", "Son\u00b7nen\u00b7weib", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Der Gram entflieht; ein letztes Sorgen", "tokens": ["Der", "Gram", "ent\u00b7flieht", ";", "ein", "letz\u00b7tes", "Sor\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Umschleicht mich: da\u00df ich w\u00fcst getr\u00e4umt", "tokens": ["Um\u00b7schleicht", "mich", ":", "da\u00df", "ich", "w\u00fcst", "ge\u00b7tr\u00e4umt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$.", "KOUS", "PPER", "VVFIN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und diesen hochzeitlichen Morgen", "tokens": ["Und", "die\u00b7sen", "hoch\u00b7zeit\u00b7li\u00b7chen", "Mor\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PDAT", "ADJA", "NN"], "meter": "-+-+---+-", "measure": "unknown.measure.tri"}, "line.4": {"text": "Im Jugendwahne lang vers\u00e4umt!", "tokens": ["Im", "Ju\u00b7gend\u00b7wah\u00b7ne", "lang", "ver\u00b7s\u00e4umt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch still! Ein Trost ist mir geblieben:", "tokens": ["Doch", "still", "!", "Ein", "Trost", "ist", "mir", "ge\u00b7blie\u00b7ben", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$.", "ART", "NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Im Tod zu minnen, ward mein Loos!", "tokens": ["Im", "Tod", "zu", "min\u00b7nen", ",", "ward", "mein", "Loos", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKZU", "VVINF", "$,", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ein Augenblick, erf\u00fcllt mit Lieben,", "tokens": ["Ein", "Au\u00b7gen\u00b7blick", ",", "er\u00b7f\u00fcllt", "mit", "Lie\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "APPR", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ist wie der Himmel tief und gro\u00df.", "tokens": ["Ist", "wie", "der", "Him\u00b7mel", "tief", "und", "gro\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KOKOM", "ART", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.te