, "Him\u00b7mel", "tief", "und", "gro\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KOKOM", "ART", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Komm, Sonnenmund, du Hochzeitsbecher,", "tokens": ["Komm", ",", "Son\u00b7nen\u00b7mund", ",", "du", "Hoch\u00b7zeits\u00b7be\u00b7cher", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "PPER", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zum Abendmahle mir geweiht!", "tokens": ["Zum", "A\u00b7bend\u00b7mah\u00b7le", "mir", "ge\u00b7weiht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Im Kusse sterbend saugt der Zecher", "tokens": ["Im", "Kus\u00b7se", "ster\u00b7bend", "saugt", "der", "Ze\u00b7cher"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADJD", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das Feuerblut der Ewigkeit.", "tokens": ["Das", "Feu\u00b7er\u00b7blut", "der", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "La\u00df trinken, trinken deinen Gatten/", "tokens": ["La\u00df", "trin\u00b7ken", ",", "trin\u00b7ken", "dei\u00b7nen", "Gat\u00b7ten", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "VVINF", "$,", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Bis ihm die Seele feierstill,", "tokens": ["Bis", "ihm", "die", "See\u00b7le", "fei\u00b7er\u00b7still", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ein Himmel ohne Wolkenschatten,", "tokens": ["Ein", "Him\u00b7mel", "oh\u00b7ne", "Wol\u00b7ken\u00b7schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ein Sonntag, so nicht enden will.", "tokens": ["Ein", "Sonn\u00b7tag", ",", "so", "nicht", "en\u00b7den", "will", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}