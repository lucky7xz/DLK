{"textgrid.poem.25282": {"metadata": {"author": {"name": "Wille, Bruno", "birth": "N.A.", "death": "N.A."}, "title": "Heilige Hochzeit", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O schw\u00fcler Traum von Lust und Minne!", "tokens": ["O", "schw\u00fc\u00b7ler", "Traum", "von", "Lust", "und", "Min\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ich wallte suchend durch das Land,", "tokens": ["Ich", "wall\u00b7te", "su\u00b7chend", "durch", "das", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da hat die sch\u00f6ne Teufelinne", "tokens": ["Da", "hat", "die", "sch\u00f6\u00b7ne", "Teu\u00b7fe\u00b7lin\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Schlangenblicken mich gebannt.", "tokens": ["Mit", "Schlan\u00b7gen\u00b7bli\u00b7cken", "mich", "ge\u00b7bannt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein Irrwisch, hat sie mich verblendet", "tokens": ["Ein", "Irr\u00b7wisch", ",", "hat", "sie", "mich", "ver\u00b7blen\u00b7det"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "VAFIN", "PPER", "PRF", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und hingeschleppt durch Nacht und Sumpf,", "tokens": ["Und", "hin\u00b7ge\u00b7schleppt", "durch", "N