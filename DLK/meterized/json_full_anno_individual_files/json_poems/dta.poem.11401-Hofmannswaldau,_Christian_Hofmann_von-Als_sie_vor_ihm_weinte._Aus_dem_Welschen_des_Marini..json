{"dta.poem.11401": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Als sie vor ihm weinte.  \n Aus dem Welschen des  Marini.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es ist wohl wahr, Lisette weinet;", "tokens": ["Es", "ist", "wohl", "wahr", ",", "Li\u00b7set\u00b7te", "wei\u00b7net", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Doch darum wunder ich mich nicht,", "tokens": ["Doch", "da\u00b7rum", "wun\u00b7der", "ich", "mich", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "NN", "PPER", "PRF", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Weil die gelinde bach, so aus den augen bricht,", "tokens": ["Weil", "die", "ge\u00b7lin\u00b7de", "bach", ",", "so", "aus", "den", "au\u00b7gen", "bricht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "VVFIN", "$,", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ein fremdes quell zu haben scheinet.", "tokens": ["Ein", "frem\u00b7des", "quell", "zu", "ha\u00b7ben", "schei\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VAINF", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Drum bilde dir nicht ein: Leander sey betrogen;", "tokens": ["Drum", "bil\u00b7de", "dir", "nicht", "ein", ":", "Le\u00b7an\u00b7der", "sey", "be\u00b7tro\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PTKNEG", "PTKVZ", "$.", "NE", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Er kennt die sonnen wohl, womit Lisette blitzt:", "tokens": ["Er", "kennt", "die", "son\u00b7nen", "wohl", ",", "wo\u00b7mit", "Li\u00b7set\u00b7te", "blitzt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "ADV", "$,", "PWAV", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die sind es, so die fluth, so meine seele schwitzt,", "tokens": ["Die", "sind", "es", ",", "so", "die", "fluth", ",", "so", "mei\u00b7ne", "see\u00b7le", "schwitzt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "$,", "ADV", "ART", "NN", "$,", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Aus meiner augen brunn in den crystall gezogen.", "tokens": ["Aus", "mei\u00b7ner", "au\u00b7gen", "brunn", "in", "den", "crys\u00b7tall", "ge\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Und also ist es falsch, da\u00df mich Lisette liebt;", "tokens": ["Und", "al\u00b7so", "ist", "es", "falsch", ",", "da\u00df", "mich", "Li\u00b7set\u00b7te", "liebt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ADJD", "$,", "KOUS", "PPER", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ist sie gleich gegen mich mitleidend und betr\u00fcbt,", "tokens": ["Ist", "sie", "gleich", "ge\u00b7gen", "mich", "mit\u00b7lei\u00b7dend", "und", "be\u00b7tr\u00fcbt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "APPR", "PPER", "VVPP", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "So ist sie es doch nur zum scheine:", "tokens": ["So", "ist", "sie", "es", "doch", "nur", "zum", "schei\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "ADV", "ADV", "APPRART", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Denn ihre thr\u00e4nen sind nicht ihre, sondern meine.", "tokens": ["Denn", "ih\u00b7re", "thr\u00e4\u00b7nen", "sind", "nicht", "ih\u00b7re", ",", "son\u00b7dern", "mei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "PTKNEG", "PPOSAT", "$,", "KON", "PPOSAT", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}