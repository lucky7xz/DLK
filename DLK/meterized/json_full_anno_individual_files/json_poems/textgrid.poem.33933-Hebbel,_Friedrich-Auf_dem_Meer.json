{"textgrid.poem.33933": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Auf dem Meer", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Allheilig Meer! Es donnern deine Kl\u00e4nge", "tokens": ["All\u00b7hei\u00b7lig", "Meer", "!", "Es", "don\u00b7nern", "dei\u00b7ne", "Kl\u00e4n\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "NN", "$.", "PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mir so gewaltig in's erschreckte Ohr,", "tokens": ["Mir", "so", "ge\u00b7wal\u00b7tig", "in's", "er\u00b7schreck\u00b7te", "Ohr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJD", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Als br\u00e4chen die verhalt'nen Fluchges\u00e4nge", "tokens": ["Als", "br\u00e4\u00b7chen", "die", "ver\u00b7halt'\u00b7nen", "Fluch\u00b7ge\u00b7s\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Begrabener Titanen draus hervor.", "tokens": ["Be\u00b7gra\u00b7be\u00b7ner", "Ti\u00b7ta\u00b7nen", "draus", "her\u00b7vor", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PAV", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Sie st\u00fcrzten sich hinab in deine Wogen,", "tokens": ["Sie", "st\u00fcrz\u00b7ten", "sich", "hin\u00b7ab", "in", "dei\u00b7ne", "Wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sie wollten sterben; aber um den Tod", "tokens": ["Sie", "woll\u00b7ten", "ster\u00b7ben", ";", "a\u00b7ber", "um", "den", "Tod"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "VVINF", "$.", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Hat deine falsche Tiefe sie betrogen,", "tokens": ["Hat", "dei\u00b7ne", "fal\u00b7sche", "Tie\u00b7fe", "sie", "be\u00b7tro\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sie tragen noch des Lebens \u00f6de Noth.", "tokens": ["Sie", "tra\u00b7gen", "noch", "des", "Le\u00b7bens", "\u00f6\u00b7de", "Noth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Sie wissen's jetzt: man kann nicht einzeln sterben;", "tokens": ["Sie", "wis\u00b7sen's", "jetzt", ":", "man", "kann", "nicht", "ein\u00b7zeln", "ster\u00b7ben", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$.", "PIS", "VMFIN", "PTKNEG", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So lange noch ein Zwerg auf Erden lebt,", "tokens": ["So", "lan\u00b7ge", "noch", "ein", "Zwerg", "auf", "Er\u00b7den", "lebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wird sich kein Gott den ganzen Tod erwerben,", "tokens": ["Wird", "sich", "kein", "Gott", "den", "gan\u00b7zen", "Tod", "er\u00b7wer\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PRF", "PIAT", "NN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ob er im Meer, im Aetna sich begr\u00e4bt.", "tokens": ["Ob", "er", "im", "Meer", ",", "im", "A\u00b7et\u00b7na", "sich", "be\u00b7gr\u00e4bt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "$,", "APPRART", "NE", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.4": {"line.1": {"text": "Sie sehen jetzt die bl\u00f6den Menschen kauern", "tokens": ["Sie", "se\u00b7hen", "jetzt", "die", "bl\u00f6\u00b7den", "Men\u00b7schen", "kau\u00b7ern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Um ihres gro\u00dfen Daseins Aschenrest;", "tokens": ["Um", "ih\u00b7res", "gro\u00b7\u00dfen", "Da\u00b7seins", "A\u00b7schen\u00b7rest", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "ADJA", "NN", "NE", "$."], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Da grollen sie: soll das denn ewig dauern?", "tokens": ["Da", "grol\u00b7len", "sie", ":", "soll", "das", "denn", "e\u00b7wig", "dau\u00b7ern", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "VMFIN", "PDS", "ADV", "ADJD", "VVINF", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Wie lange h\u00e4lt der Wurm die W\u00e4rme fest!", "tokens": ["Wie", "lan\u00b7ge", "h\u00e4lt", "der", "Wurm", "die", "W\u00e4r\u00b7me", "fest", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Uns kreis'te doch das Ganze in den Adern,", "tokens": ["Uns", "kreis'\u00b7te", "doch", "das", "Gan\u00b7ze", "in", "den", "A\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das jetzt zu Tropfen tausendfach zerrann;", "tokens": ["Das", "jetzt", "zu", "Trop\u00b7fen", "tau\u00b7send\u00b7fach", "zer\u00b7rann", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "APPR", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wir mu\u00dften dennoch mit den G\u00f6ttern hadern,", "tokens": ["Wir", "mu\u00df\u00b7ten", "den\u00b7noch", "mit", "den", "G\u00f6t\u00b7tern", "ha\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Jetzt haben Legionen g'nug daran!", "tokens": ["Jetzt", "ha\u00b7ben", "Le\u00b7gi\u00b7o\u00b7nen", "g'\u00b7nug", "da\u00b7ran", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "ADV", "PAV", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.6": {"line.1": {"text": "So grollen sie im Aetna und im Grunde", "tokens": ["So", "grol\u00b7len", "sie", "im", "A\u00b7et\u00b7na", "und", "im", "Grun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NE", "KON", "APPRART", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Des Meers, und nicken langsam wieder ein;", "tokens": ["Des", "Meers", ",", "und", "ni\u00b7cken", "lang\u00b7sam", "wie\u00b7der", "ein", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KON", "VVFIN", "ADJD", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch nach Jahrhunderten ruft eine Stunde", "tokens": ["Doch", "nach", "Jahr\u00b7hun\u00b7der\u00b7ten", "ruft", "ei\u00b7ne", "Stun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "VVFIN", "ART", "NN"], "meter": "---+--+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Sie abermals zur\u00fcck in's \u00f6de Sein.", "tokens": ["Sie", "a\u00b7ber\u00b7mals", "zu\u00b7r\u00fcck", "in's", "\u00f6\u00b7de", "Sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PTKVZ", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Dann w\u00e4hnen sie: nun ist die Welt am Ende,", "tokens": ["Dann", "w\u00e4h\u00b7nen", "sie", ":", "nun", "ist", "die", "Welt", "am", "En\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "ADV", "VAFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und dies Erwachen ist das letzte Weh!", "tokens": ["Und", "dies", "Er\u00b7wa\u00b7chen", "ist", "das", "letz\u00b7te", "Weh", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "NN", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dann wirft der Eine seine Feuerbr\u00e4nde,", "tokens": ["Dann", "wirft", "der", "Ei\u00b7ne", "sei\u00b7ne", "Feu\u00b7er\u00b7br\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ART", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dann ras't der And're in dem Schoo\u00df der See.", "tokens": ["Dann", "ras't", "der", "An\u00b7d'\u00b7re", "in", "dem", "Schoo\u00df", "der", "See", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.8": {"line.1": {"text": "Ich ahnt' es l\u00e4ngst! Die grollenden Titanen", "tokens": ["Ich", "ahnt'", "es", "l\u00e4ngst", "!", "Die", "grol\u00b7len\u00b7den", "Ti\u00b7ta\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sind aus dem Schlummer wieder aufgest\u00f6rt,", "tokens": ["Sind", "aus", "dem", "Schlum\u00b7mer", "wie\u00b7der", "auf\u00b7ge\u00b7st\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und haben, an die alte Nacht zu mahnen,", "tokens": ["Und", "ha\u00b7ben", ",", "an", "die", "al\u00b7te", "Nacht", "zu", "mah\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "$,", "APPR", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Jedwedes Element der Welt emp\u00f6rt.", "tokens": ["Jed\u00b7we\u00b7des", "E\u00b7le\u00b7ment", "der", "Welt", "em\u00b7p\u00f6rt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "War's Empedokles, der die Stadt der Elbe", "tokens": ["Wa\u00b7r's", "Em\u00b7pe\u00b7do\u00b7kles", ",", "der", "die", "Stadt", "der", "El\u00b7be"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "$,", "PRELS", "ART", "NN", "ART", "NE"], "meter": "+----+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Mit seiner Aetnafackel angesteckt?", "tokens": ["Mit", "sei\u00b7ner", "A\u00b7et\u00b7na\u00b7fac\u00b7kel", "an\u00b7ge\u00b7steckt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und ist's ein And'rer, oder ist's derselbe,", "tokens": ["Und", "ist's", "ein", "An\u00b7d'\u00b7rer", ",", "o\u00b7der", "ist's", "der\u00b7sel\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "$,", "KON", "VAFIN", "PDAT", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Der z\u00fcrnend jetzt den alten Meergeist weckt?", "tokens": ["Der", "z\u00fcr\u00b7nend", "jetzt", "den", "al\u00b7ten", "Meer\u00b7geist", "weckt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Wohlauf! Zur\u00fcckgeschlagen sind die Flammen!", "tokens": ["Wohl\u00b7auf", "!", "Zu\u00b7r\u00fcck\u00b7ge\u00b7schla\u00b7gen", "sind", "die", "Flam\u00b7men", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Schwellt denn in Eins, ihr Meere, fern und nah',", "tokens": ["Schwellt", "denn", "in", "Eins", ",", "ihr", "Mee\u00b7re", ",", "fern", "und", "nah'", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "NN", "$,", "PPOSAT", "NN", "$,", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Kn\u00fcpft Wogentanz und Sternentanz zusammen,", "tokens": ["Kn\u00fcpft", "Wo\u00b7gen\u00b7tanz", "und", "Ster\u00b7nen\u00b7tanz", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie Aeschylos es im Prometheus sah.", "tokens": ["Wie", "A\u00b7esc\u00b7hy\u00b7los", "es", "im", "Pro\u00b7me\u00b7theus", "sah", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.11": {"line.1": {"text": "Allheilig Meer! Es donnern deine Kl\u00e4nge", "tokens": ["All\u00b7hei\u00b7lig", "Meer", "!", "Es", "don\u00b7nern", "dei\u00b7ne", "Kl\u00e4n\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "NN", "$.", "PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mir so gewaltig in's erschreckte Ohr,", "tokens": ["Mir", "so", "ge\u00b7wal\u00b7tig", "in's", "er\u00b7schreck\u00b7te", "Ohr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJD", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Als br\u00e4chen die verhalt'nen Fluchg