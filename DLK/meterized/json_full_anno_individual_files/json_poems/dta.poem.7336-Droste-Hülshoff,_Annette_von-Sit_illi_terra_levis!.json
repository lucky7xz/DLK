{"dta.poem.7336": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Sit illi terra levis!", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-20090519994", "language": ["de:0.99"], "booktitle": "Droste-H\u00fclshoff, Annette von: Gedichte. Stuttgart u. a., 1844."}, "poem": {"stanza.1": {"line.1": {"text": "So sonder Arg hast du in diesem Leben", "tokens": ["So", "son\u00b7der", "Arg", "hast", "du", "in", "die\u00b7sem", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "VAFIN", "PPER", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mich deinen allerbesten Freund genannt,", "tokens": ["Mich", "dei\u00b7nen", "al\u00b7ler\u00b7bes\u00b7ten", "Freund", "ge\u00b7nannt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Hast mir so oft gereicht die hagre Hand, \u2014", "tokens": ["Hast", "mir", "so", "oft", "ge\u00b7reicht", "die", "hag\u00b7re", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "VVPP", "ART", "ADJA", "NN", "$,", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Hab' ich gel\u00e4chelt, mag mir Gott vergeben.", "tokens": ["Hab'", "ich", "ge\u00b7l\u00e4\u00b7chelt", ",", "mag", "mir", "Gott", "ver\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVPP", "$,", "VMFIN", "PPER", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Die Schlange wacht in jedes Menschen Brust,", "tokens": ["Die", "Schlan\u00b7ge", "wacht", "in", "je\u00b7des", "Men\u00b7schen", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PIAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Was ich dir bot, es war doch treue Gabe,", "tokens": ["Was", "ich", "dir", "bot", ",", "es", "war", "doch", "treu\u00b7e", "Ga\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PPER", "VVFIN", "$,", "PPER", "VAFIN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und hier bekenn' ich es, an deinem Grabe,", "tokens": ["Und", "hier", "be\u00b7kenn'", "ich", "es", ",", "an", "dei\u00b7nem", "Gra\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PPER", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Du warst mir lieber als ich es gewu\u00dft.", "tokens": ["Du", "warst", "mir", "lie\u00b7ber", "als", "ich", "es", "ge\u00b7wu\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "KOUS", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ob ich auch nie zu jenen mich gesellte,", "tokens": ["Ob", "ich", "auch", "nie", "zu", "je\u00b7nen", "mich", "ge\u00b7sell\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "APPR", "PDAT", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die lachend deine Einfalt angeschaut;", "tokens": ["Die", "la\u00b7chend", "dei\u00b7ne", "Ein\u00b7falt", "an\u00b7ge\u00b7schaut", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Des Hauptes, das in Ehren war ergraut,", "tokens": ["Des", "Haup\u00b7tes", ",", "das", "in", "Eh\u00b7ren", "war", "er\u00b7graut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Verh\u00f6hnung immer mir die Adern schwellte;", "tokens": ["Ver\u00b7h\u00f6h\u00b7nung", "im\u00b7mer", "mir", "die", "A\u00b7dern", "schwell\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Doch erst wo aller Menschen Witz versiegt,", "tokens": ["Doch", "erst", "wo", "al\u00b7ler", "Men\u00b7schen", "Witz", "ver\u00b7siegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PWAV", "PIAT", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ein armer Tropfen in Egyptens Sande,", "tokens": ["Ein", "ar\u00b7mer", "Trop\u00b7fen", "in", "E\u00b7gyp\u00b7tens", "San\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NE", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Hier erst erkenn' ich, an der Seelen Brande,", "tokens": ["Hier", "erst", "er\u00b7kenn'", "ich", ",", "an", "der", "See\u00b7len", "Bran\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "$,", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Wie schwer des Auges warme Thr\u00e4ne wiegt.", "tokens": ["Wie", "schwer", "des", "Au\u00b7ges", "war\u00b7me", "Thr\u00e4\u00b7ne", "wiegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Sah ich sie nicht an deine Wimper steigen,", "tokens": ["Sah", "ich", "sie", "nicht", "an", "dei\u00b7ne", "Wim\u00b7per", "stei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "PTKNEG", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn du dem fremden Leide dich geeint?", "tokens": ["Wenn", "du", "dem", "frem\u00b7den", "Lei\u00b7de", "dich", "ge\u00b7eint", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Hast du nicht meinen Todten nachgeweint,", "tokens": ["Hast", "du", "nicht", "mei\u00b7nen", "Tod\u00b7ten", "nach\u00b7ge\u00b7weint", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "So hei\u00df wie deines eignen Blutes Zweigen?", "tokens": ["So", "hei\u00df", "wie", "dei\u00b7nes", "eig\u00b7nen", "Blu\u00b7tes", "Zwei\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "PPOSAT", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "O! wenn ich in der Freude de\u00df verga\u00df,", "tokens": ["O", "!", "wenn", "ich", "in", "der", "Freu\u00b7de", "de\u00df", "ver\u00b7ga\u00df", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "KOUS", "PPER", "APPR", "ART", "NN", "ART", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Mit bitterm Herzen mu\u00df ich es beklagen,", "tokens": ["Mit", "bit\u00b7term", "Her\u00b7zen", "mu\u00df", "ich", "es", "be\u00b7kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Denn von des Schicksals harter Hand geschlagen,", "tokens": ["Denn", "von", "des", "Schick\u00b7sals", "har\u00b7ter", "Hand", "ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Wie gern ich dann in deinem Auge las!", "tokens": ["Wie", "gern", "ich", "dann", "in", "dei\u00b7nem", "Au\u00b7ge", "las", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPER", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Noch seh ich dich im Hauch des Winterbrodems", "tokens": ["Noch", "seh", "ich", "dich", "im", "Hauch", "des", "Win\u00b7ter\u00b7bro\u00b7dems"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Herstapfen, wie den irren Haidegeist,", "tokens": ["Hers\u00b7tap\u00b7fen", ",", "wie", "den", "ir\u00b7ren", "Hai\u00b7de\u00b7geist", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wenn Tropf' an Tropfen deiner Stirn entfleu\u00dft,", "tokens": ["Wenn", "Tropf'", "an", "Trop\u00b7fen", "dei\u00b7ner", "Stirn", "ent\u00b7fleu\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "NN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "H\u00f6r noch das Keuchen deines armen Odems.", "tokens": ["H\u00f6r", "noch", "das", "Keu\u00b7chen", "dei\u00b7nes", "ar\u00b7men", "O\u00b7dems", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "NN", "PPOSAT", "ADJA", "NE", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.5": {"text": "Es waren schlimme Wege, rauh und weit,", "tokens": ["Es", "wa\u00b7ren", "schlim\u00b7me", "We\u00b7ge", ",", "rauh", "und", "weit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJA", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Die du gewandelt manche Winterwende,", "tokens": ["Die", "du", "ge\u00b7wan\u00b7delt", "man\u00b7che", "Win\u00b7ter\u00b7wen\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Um des Altares heil'ge Gnadenspende", "tokens": ["Um", "des", "Al\u00b7ta\u00b7res", "heil'\u00b7ge", "Gna\u00b7den\u00b7spen\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUI", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Zu tragen mir in meine Einsamkeit.", "tokens": ["Zu", "tra\u00b7gen", "mir", "in", "mei\u00b7ne", "Ein\u00b7sam\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "O manchem Sp\u00f6tter gabst du ernst Gedenken,", "tokens": ["O", "man\u00b7chem", "Sp\u00f6t\u00b7ter", "gabst", "du", "ernst", "Ge\u00b7den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PIAT", "NN", "VVFIN", "PPER", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn h\u00f6hnend deine kleine Hab' er prie\u00df,", "tokens": ["Wenn", "h\u00f6h\u00b7nend", "dei\u00b7ne", "klei\u00b7ne", "Hab'", "er", "prie\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "PPOSAT", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "F\u00fcr schlechtes Ding dir Tausende verhie\u00df,", "tokens": ["F\u00fcr", "schlech\u00b7tes", "Ding", "dir", "Tau\u00b7sen\u00b7de", "ver\u00b7hie\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und du nur gl\u00fccklich warst ihn zu beschenken!", "tokens": ["Und", "du", "nur", "gl\u00fcck\u00b7lich", "warst", "ihn", "zu", "be\u00b7schen\u00b7ken", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "ADJD", "VAFIN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "So werth war dir kein Gut, so ehrenreich,", "tokens": ["So", "werth", "war", "dir", "kein", "Gut", ",", "so", "eh\u00b7ren\u00b7reich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PPER", "PIAT", "NN", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Da\u00df du es nicht mit Freuden hingegeben,", "tokens": ["Da\u00df", "du", "es", "nicht", "mit", "Freu\u00b7den", "hin\u00b7ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PTKNEG", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Dann sah man deine Lippen freundlich beben,", "tokens": ["Dann", "sah", "man", "dei\u00b7ne", "Lip\u00b7pen", "freund\u00b7lich", "be\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PPOSAT", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und zucken wie das D\u00e4mmerlicht im Teich.", "tokens": ["Und", "zu\u00b7cken", "wie", "das", "D\u00e4m\u00b7mer\u00b7licht", "im", "Teich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KOKOM", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "An deinem Kleide, schwarz und Fadenscheinend,", "tokens": ["An", "dei\u00b7nem", "Klei\u00b7de", ",", "schwarz", "und", "Fa\u00b7den\u00b7schei\u00b7nend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "ADJD", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "War jeder Fleck ein heimlich Ehrenmaal,", "tokens": ["War", "je\u00b7der", "Fleck", "ein", "heim\u00b7lich", "Eh\u00b7ren\u00b7maal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Du frommer Dieb am Eignen! ohne Wahl", "tokens": ["Du", "from\u00b7mer", "Dieb", "am", "Eig\u00b7nen", "!", "oh\u00b7ne", "Wahl"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "ADJA", "NN", "APPRART", "NN", "$.", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Das Schlechteste dir noch genugsam meinend.", "tokens": ["Das", "Schlech\u00b7tes\u00b7te", "dir", "noch", "ge\u00b7nug\u00b7sam", "mei\u00b7nend", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Mann ohne Falsch und mit der offnen Hand,", "tokens": ["Mann", "oh\u00b7ne", "Falsch", "und", "mit", "der", "off\u00b7nen", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Drin wie Demant der Wittwe Heller blinken,", "tokens": ["Drin", "wie", "De\u00b7mant", "der", "Witt\u00b7we", "Hel\u00b7ler", "blin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "NN", "ART", "NN", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Sanft soll der Thau auf deinen H\u00fcgel sinken,", "tokens": ["Sanft", "soll", "der", "Thau", "auf", "dei\u00b7nen", "H\u00fc\u00b7gel", "sin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.8": {"text": "Und leicht, leicht sey dir das geweihte Land!", "tokens": ["Und", "leicht", ",", "leicht", "sey", "dir", "das", "ge\u00b7we