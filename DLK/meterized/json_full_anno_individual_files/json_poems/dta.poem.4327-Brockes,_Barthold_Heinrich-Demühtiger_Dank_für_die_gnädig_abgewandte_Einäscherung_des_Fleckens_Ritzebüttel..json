{"dta.poem.4327": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Dem\u00fchtiger Dank  \n f\u00fcr die  \n gn\u00e4dig abgewandte Ein\u00e4scherung  \n des  \n Fleckens Ritzeb\u00fcttel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die stille Nacht hatt' allbereits die Welt in Schatten", "tokens": ["Die", "stil\u00b7le", "Nacht", "hatt'", "all\u00b7be\u00b7reits", "die", "Welt", "in", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "eingeh\u00fcllet,", "tokens": ["ein\u00b7ge\u00b7h\u00fcl\u00b7let", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Und, wie mit Dunkelheit die Luft, mit Schlaf schon", "tokens": ["Und", ",", "wie", "mit", "Dun\u00b7kel\u00b7heit", "die", "Luft", ",", "mit", "Schlaf", "schon"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "$,", "PWAV", "APPR", "NN", "ART", "NN", "$,", "APPR", "NN", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "manches Aug' erf\u00fcllet,", "tokens": ["man\u00b7ches", "Aug'", "er\u00b7f\u00fcl\u00b7let", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Als pl\u00f6tzlich ein entsetzlich Licht den schwarzen Duft der", "tokens": ["Als", "pl\u00f6tz\u00b7lich", "ein", "ent\u00b7setz\u00b7lich", "Licht", "den", "schwar\u00b7zen", "Duft", "der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "ART", "ADJD", "NN", "ART", "ADJA", "NN", "ART"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Finsterni\u00df,", "tokens": ["Fins\u00b7ter\u00b7ni\u00df", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Und ein erb\u00e4rmliches Geschrey die Augen von einander", "tokens": ["Und", "ein", "er\u00b7b\u00e4rm\u00b7li\u00b7ches", "Ge\u00b7schrey", "die", "Au\u00b7gen", "von", "ein\u00b7an\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "ART", "NN", "APPR", "PRF"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.8": {"text": "ri\u00df.", "tokens": ["ri\u00df", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+", "measure": "single.up"}, "line.9": {"text": "Es nahm von einem schnellen Feuer ein rohter Schrecken-", "tokens": ["Es", "nahm", "von", "ei\u00b7nem", "schnel\u00b7len", "Feu\u00b7er", "ein", "roh\u00b7ter", "Schre\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "ART", "ADJA", "TRUNC"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "reicher Schein", "tokens": ["rei\u00b7cher", "Schein"], "token_info": ["word", "word"], "pos": ["ADJD", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.11": {"text": "Jm Augenblick die hohe Tiefe des Schatten-reichen Luft-", "tokens": ["Jm", "Au\u00b7gen\u00b7blick", "die", "ho\u00b7he", "Tie\u00b7fe", "des", "Schat\u00b7ten\u00b7rei\u00b7chen", "Luft"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.12": {"text": "Raums ein.", "tokens": ["Raums", "ein", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKVZ", "$."], "meter": "-+", "measure": "iambic.single"}, "line.13": {"text": "Ein gr\u00e4\u00dflich Licht beflo\u00df das Land, fiel an die H\u00f6h\u2019n,", "tokens": ["Ein", "gr\u00e4\u00df\u00b7lich", "Licht", "be\u00b7flo\u00df", "das", "Land", ",", "fiel", "an", "die", "H\u00f6h'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VVFIN", "ART", "NN", "$,", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "schwamm auf der Fluht,", "tokens": ["schwamm", "auf", "der", "Fluht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.15": {"text": "Es loderte mit lautem Prasseln die fre\u00dfige verzehrnde", "tokens": ["Es", "lo\u00b7der\u00b7te", "mit", "lau\u00b7tem", "Pras\u00b7seln", "die", "fre\u00b7\u00dfi\u00b7ge", "ver\u00b7zehrn\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.16": {"text": "Gluht,", "tokens": ["Gluht", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.17": {"text": "Es fuhr die gelblich rohte Loh mit regem Wallen, wie", "tokens": ["Es", "fuhr", "die", "gelb\u00b7lich", "roh\u00b7te", "Loh", "mit", "re\u00b7gem", "Wal\u00b7len", ",", "wie"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJD", "ADJA", "NN", "APPR", "ADJA", "NN", "$,", "PWAV"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.18": {"text": "ein Strahl,", "tokens": ["ein", "Strahl", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.19": {"text": "Aus zwey mit Stroh gedeckten D\u00e4chern, und aus zwo", "tokens": ["Aus", "zwey", "mit", "Stroh", "ge\u00b7deck\u00b7ten", "D\u00e4\u00b7chern", ",", "und", "aus", "zwo"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "CARD", "APPR", "NN", "ADJA", "NN", "$,", "KON", "APPR", "CARD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Scheunen auf einmahl.", "tokens": ["Scheu\u00b7nen", "auf", "ein\u00b7mahl", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Was mich bey diesem Ungl\u00fcck nun mehr, als das Un-", "tokens": ["Was", "mich", "bey", "die\u00b7sem", "Un\u00b7gl\u00fcck", "nun", "mehr", ",", "als", "das", "Un"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "PPER", "APPR", "PDAT", "NN", "ADV", "ADV", "$,", "KOUS", "ART", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "gl\u00fcck selber, schreckte,", "tokens": ["gl\u00fcck", "sel\u00b7ber", ",", "schreck\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADV", "$,", "VVFIN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Und allen Eingesessenen die allergr\u00f6\u00dfte Furcht erweckte,", "tokens": ["Und", "al\u00b7len", "Ein\u00b7ge\u00b7ses\u00b7se\u00b7nen", "die", "al\u00b7ler\u00b7gr\u00f6\u00df\u00b7te", "Furcht", "er\u00b7weck\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.4": {"text": "War ein gef\u00e4hrlich strenger Wind, der in die grosse Lohe", "tokens": ["War", "ein", "ge\u00b7f\u00e4hr\u00b7lich", "stren\u00b7ger", "Wind", ",", "der", "in", "die", "gros\u00b7se", "Lo\u00b7he"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "ADJD", "ADJA", "NN", "$,", "PRELS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.5": {"text": "blies,", "tokens": ["blies", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-", "measure": "single.down"}, "line.6": {"text": "Und gleichsam ihr geschwinde Wege, sich schleunig zu", "tokens": ["Und", "gleich\u00b7sam", "ihr", "ge\u00b7schwin\u00b7de", "We\u00b7ge", ",", "sich", "schleu\u00b7nig", "zu"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADJD", "PPOSAT", "ADJA", "NN", "$,", "PRF", "ADJD", "PTKZU"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "vergr\u00f6ssern, wies.", "tokens": ["ver\u00b7gr\u00f6s\u00b7sern", ",", "wies", "."], "token_info": ["word", "punct", "word", "punct"], "pos": ["VVINF", "$,", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Hier h\u00e4tte man ein \u00e4ngstlichs Schreyen, ein allgemeines", "tokens": ["Hier", "h\u00e4t\u00b7te", "man", "ein", "\u00e4ngst\u00b7lichs", "Schre\u00b7yen", ",", "ein", "all\u00b7ge\u00b7mei\u00b7nes"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VAFIN", "PIS", "ART", "ADJA", "NN", "$,", "ART", "ADJA"], "meter": "-+---+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Klag-Get\u00f6n,", "tokens": ["Klag\u00b7Ge\u00b7t\u00f6n", ","], "token_info": ["word", "punct"], "pos": ["NE", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Des Viehes j\u00e4mmerliches Br\u00fcllen, ein Wasser! Wasser!", "tokens": ["Des", "Vie\u00b7hes", "j\u00e4m\u00b7mer\u00b7li\u00b7ches", "Br\u00fcl\u00b7len", ",", "ein", "Was\u00b7ser", "!", "Was\u00b7ser", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,", "ART", "NN", "$.", "NN", "$."], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "rufen h\u00f6ren,", "tokens": ["ru\u00b7fen", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "punct"], "pos": ["VVINF", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Ein Lermen, Retten, Schleppen, W\u00fchlen, sich mit der", "tokens": ["Ein", "Ler\u00b7men", ",", "Ret\u00b7ten", ",", "Schlep\u00b7pen", ",", "W\u00fch\u00b7len", ",", "sich", "mit", "der"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,", "PRF", "APPR", "ART"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Gluht die Furcht vermehren,", "tokens": ["Gluht", "die", "Furcht", "ver\u00b7meh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Mehr, als man tragen konnte, tragen, die hellen Thr\u00e4nen", "tokens": ["Mehr", ",", "als", "man", "tra\u00b7gen", "konn\u00b7te", ",", "tra\u00b7gen", ",", "die", "hel\u00b7len", "Thr\u00e4\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PIS", "$,", "KOUS", "PIS", "VVINF", "VMFIN", "$,", "VVINF", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "fliessen seh'n,", "tokens": ["flies\u00b7sen", "seh'n", ","], "token_info": ["word", "word", "punct"], "pos": ["VVINF", "VVINF", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.9": {"text": "Und lauter Elend schauen sollen! Ich eilte, nebst der", "tokens": ["Und", "lau\u00b7ter", "E\u00b7lend", "schau\u00b7en", "sol\u00b7len", "!", "Ich", "eil\u00b7te", ",", "nebst", "der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KON", "PIAT", "NN", "VVINF", "VMFIN", "$.", "PPER", "VVFIN", "$,", "APPR", "ART"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Wasser-Spr\u00fctzen,", "tokens": ["Was\u00b7ser\u00b7Spr\u00fct\u00b7zen", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.11": {"text": "Den Flammen Widerstand zu thun, und das f\u00fcr ihrer", "tokens": ["Den", "Flam\u00b7men", "Wi\u00b7der\u00b7stand", "zu", "thun", ",", "und", "das", "f\u00fcr", "ih\u00b7rer"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "PTKZU", "VVINF", "$,", "KON", "PDS", "APPR", "PPOSAT"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Wut zu sch\u00fctzen,", "tokens": ["Wut", "zu", "sch\u00fct\u00b7zen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.13": {"text": "Was sich noch wollte retten lassen. Ich \u00fcberlegte, rief,", "tokens": ["Was", "sich", "noch", "woll\u00b7te", "ret\u00b7ten", "las\u00b7sen", ".", "Ich", "\u00fc\u00b7ber\u00b7leg\u00b7te", ",", "rief", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "PRF", "ADV", "VMFIN", "VVINF", "VVINF", "$.", "PPER", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.14": {"text": "befahl,", "tokens": ["be\u00b7fahl", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.15": {"text": "Bedrohte, bat, gebot, versprach, war bald an dem, bald", "tokens": ["Be\u00b7droh\u00b7te", ",", "bat", ",", "ge\u00b7bot", ",", "ver\u00b7sprach", ",", "war", "bald", "an", "dem", ",", "bald"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$,", "VAFIN", "ADV", "APPR", "ART", "$,", "ADV"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.16": {"text": "diesem Orte,", "tokens": ["die\u00b7sem", "Or\u00b7te", ","], "token_info": ["word", "word", "punct"], "pos": ["PDAT", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.17": {"text": "Ver\u00e4nderte Befehl und Anstalt, ver\u00e4nderte den Wasser-", "tokens": ["Ver\u00b7\u00e4n\u00b7der\u00b7te", "Be\u00b7fehl", "und", "An\u00b7stalt", ",", "ver\u00b7\u00e4n\u00b7der\u00b7te", "den", "Was\u00b7ser"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "NN", "$,", "VVFIN", "ART", "TRUNC"], "meter": "-+---+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.18": {"text": "Strahl", "tokens": ["Strahl"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.19": {"text": "Der unaufh\u00f6rlich regen Spr\u00fctze, nachdem ich es f\u00fcr", "tokens": ["Der", "un\u00b7auf\u00b7h\u00f6r\u00b7lich", "re\u00b7gen", "Spr\u00fct\u00b7ze", ",", "nach\u00b7dem", "ich", "es", "f\u00fcr"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "ADJA", "NN", "$,", "KOUS", "PPER", "PPER", "APPR"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.20": {"text": "n\u00f6htig hielt.", "tokens": ["n\u00f6h\u00b7tig", "hielt", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.4": {"line.1": {"text": "Doch alles w\u00e4r umsonst gewesen, wenn nicht des H\u00f6chsten", "tokens": ["Doch", "al\u00b7les", "w\u00e4r", "um\u00b7sonst", "ge\u00b7we\u00b7sen", ",", "wenn", "nicht", "des", "H\u00f6chs\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PIS", "VAFIN", "ADV", "VAPP", "$,", "KOUS", "PTKNEG", "ART", "NN"], "meter": "-+-+-+-+-++-+-", "measure": "unknown.measure.septa"}, "line.2": {"text": "Gnaden-Wille", "tokens": ["Gna\u00b7den\u00b7Wil\u00b7le"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Dem Sturm gebohten sich zu legen. Es ward ein\u2019 unver-", "tokens": ["Dem", "Sturm", "ge\u00b7boh\u00b7ten", "sich", "zu", "le\u00b7gen", ".", "Es", "ward", "ein'", "un\u00b7ver"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PRF", "PTKZU", "VVINF", "$.", "PPER", "VAFIN", "ART", "TRUNC"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "hoffte Stille,", "tokens": ["hoff\u00b7te", "Stil\u00b7le", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Nebst einer kleinen Aenderung des Windes, allgemach", "tokens": ["Nebst", "ei\u00b7ner", "klei\u00b7nen", "A\u00b7en\u00b7de\u00b7rung", "des", "Win\u00b7des", ",", "all\u00b7ge\u00b7mach"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "$,", "ADV"], "meter": "-+-+--+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.6": {"text": "gef\u00fchlt.", "tokens": ["ge\u00b7f\u00fchlt", "."], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-+", "measure": "iambic.single"}, "line.7": {"text": "Die Lohe stieg mehr \u00fcber sich, die anfangs imm