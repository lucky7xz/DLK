{"textgrid.poem.39957": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "1. Der Stein in Ketten", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In die nackten Jurarippen,", "tokens": ["In", "die", "nack\u00b7ten", "Ju\u00b7ra\u00b7rip\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zwischen Felsgekl\u00fcft und Klippen,", "tokens": ["Zwi\u00b7schen", "Fels\u00b7ge\u00b7kl\u00fcft", "und", "Klip\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "H\u00e4ngt ein Steinblock eingekeilt,", "tokens": ["H\u00e4ngt", "ein", "Stein\u00b7block", "ein\u00b7ge\u00b7keilt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Unter dem mit Lustentsetzen", "tokens": ["Un\u00b7ter", "dem", "mit", "Lus\u00b7tent\u00b7set\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und mit schaurigem Ergetzen", "tokens": ["Und", "mit", "schau\u00b7ri\u00b7gem", "Er\u00b7get\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "+-+---+-", "measure": "unknown.measure.tri"}, "line.6": {"text": "Auch der schnellste Wandrer weilt.", "tokens": ["Auch", "der", "schnells\u00b7te", "Wand\u00b7rer", "weilt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "--+-+-+", "measure": "anapaest.i