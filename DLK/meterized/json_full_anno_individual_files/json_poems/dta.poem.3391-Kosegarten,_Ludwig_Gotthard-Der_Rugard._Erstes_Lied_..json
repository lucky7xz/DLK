{"dta.poem.3391": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Der Rugard.  \n  Erstes Lied .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1798", "urn": "urn:nbn:de:kobv:b4-200905193292", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Auf deiner schroffen Felsenscheitel", "tokens": ["Auf", "dei\u00b7ner", "schrof\u00b7fen", "Fel\u00b7sen\u00b7schei\u00b7tel"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Empfange mich, alter Rugard.", "tokens": ["Emp\u00b7fan\u00b7ge", "mich", ",", "al\u00b7ter", "Ru\u00b7gard", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADJA", "NE", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Empfange mich, Hehrer!", "tokens": ["Emp\u00b7fan\u00b7ge", "mich", ",", "Heh\u00b7rer", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Mich l\u00fcstert, zu schauen,", "tokens": ["Mich", "l\u00fcs\u00b7tert", ",", "zu", "schau\u00b7en", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PTKZU", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Mich l\u00fcstert, zu fassen,", "tokens": ["Mich", "l\u00fcs\u00b7tert", ",", "zu", "fas\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PTKZU", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Mit Einem staunenden Blicke", "tokens": ["Mit", "Ei\u00b7nem", "stau\u00b7nen\u00b7den", "Bli\u00b7cke"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Die K\u00fcsten, die Inseln, und das donnernde Meer.", "tokens": ["Die", "K\u00fcs\u00b7ten", ",", "die", "In\u00b7seln", ",", "und", "das", "don\u00b7nern\u00b7de", "Meer", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "KON", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-+--+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Fern dringt,", "tokens": ["Fern", "dringt", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.2": {"text": "Rings rollt", "tokens": ["Rings", "rollt"], "token_info": ["word", "word"], "pos": ["NN", "VVFIN"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Mein rastlos schw\u00e4rmendes Auge.", "tokens": ["Mein", "rast\u00b7los", "schw\u00e4r\u00b7men\u00b7des", "Au\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Es heult der Sturm;", "tokens": ["Es", "heult", "der", "Sturm", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Es tosen die Ufer;", "tokens": ["Es", "to\u00b7sen", "die", "U\u00b7fer", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Der Riesengr\u00e4ber Gr\u00fcfte seufzen.", "tokens": ["Der", "Rie\u00b7sen\u00b7gr\u00e4\u00b7ber", "Gr\u00fcf\u00b7te", "seuf\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ich aber umfasse vom zackigen Felsen", "tokens": ["Ich", "a\u00b7ber", "um\u00b7fas\u00b7se", "vom", "za\u00b7cki\u00b7gen", "Fel\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Ufer und Inseln, und Festen und Meer.", "tokens": ["U\u00b7fer", "und", "In\u00b7seln", ",", "und", "Fes\u00b7ten", "und", "Meer", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "KON", "NN", "KON", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.4": {"line.1": {"text": "Rechts blickt,", "tokens": ["Rechts", "blickt", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "Links sp\u00e4ht", "tokens": ["Links", "sp\u00e4ht"], "token_info": ["word", "word"], "pos": ["ADV", "VVFIN"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Mein rastlos rollendes Auge.", "tokens": ["Mein", "rast\u00b7los", "rol\u00b7len\u00b7des", "Au\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Dort donnert das Ostmeer;", "tokens": ["Dort", "don\u00b7nert", "das", "Ost\u00b7meer", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Hier sauset der Raddas-Hayn.", "tokens": ["Hier", "sau\u00b7set", "der", "Rad\u00b7das\u00b7Hayn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Dort th\u00fcrmet die l\u00e4ndliche Tochter der Berge:", "tokens": ["Dort", "th\u00fcr\u00b7met", "die", "l\u00e4nd\u00b7li\u00b7che", "Toch\u00b7ter", "der", "Ber\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.7": {"text": "Rings zirkelt das wellengeschlagne Gestad'.", "tokens": ["Rings", "zir\u00b7kelt", "das", "wel\u00b7len\u00b7ge\u00b7schlag\u00b7ne", "Ge\u00b7stad'", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.5": {"line.1": {"text": "Wie tobt das Meer!", "tokens": ["Wie", "tobt", "das", "Meer", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Wie saus't der Hayn!", "tokens": ["Wie", "saus't", "der", "Hayn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "H\u00f6rst du den Oceandonner?", "tokens": ["H\u00f6rst", "du", "den", "O\u00b7ce\u00b7an\u00b7don\u00b7ner", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$."], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.4": {"text": "H\u00f6rst du das Sausen im Raddas-Hayn?", "tokens": ["H\u00f6rst", "du", "das", "Sau\u00b7sen", "im", "Rad\u00b7das\u00b7Hayn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Sch\u00f6n klingt dein Donner,", "tokens": ["Sch\u00f6n", "klingt", "dein", "Don\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "O Meer, den Ohren des J\u00fcnglings.", "tokens": ["O", "Meer", ",", "den", "Oh\u00b7ren", "des", "J\u00fcng\u00b7lings", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Dein Sausen, o Hayn,", "tokens": ["Dein", "Sau\u00b7sen", ",", "o", "Hayn", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "FM", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Weht lieblich dem Draussenwandler.", "tokens": ["Weht", "lieb\u00b7lich", "dem", "Draus\u00b7sen\u00b7wand\u00b7ler", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "Und du, o l\u00e4ndliche Tochter der Berge,", "tokens": ["Und", "du", ",", "o", "l\u00e4nd\u00b7li\u00b7che", "Toch\u00b7ter", "der", "Ber\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "FM", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ich liebe dich, Traute! Ich fand", "tokens": ["Ich", "lie\u00b7be", "dich", ",", "Trau\u00b7te", "!", "Ich", "fand"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "NN", "$.", "PPER", "VVFIN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "In dir der V\u00e4ter Biedermuth,", "tokens": ["In", "dir", "der", "V\u00e4\u00b7ter", "Bie\u00b7der\u00b7muth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In deinen T\u00f6chtern Milde,", "tokens": ["In", "dei\u00b7nen", "T\u00f6ch\u00b7tern", "Mil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "In deinen J\u00fcnglingen Geniusgluth,", "tokens": ["In", "dei\u00b7nen", "J\u00fcng\u00b7lin\u00b7gen", "Ge\u00b7nius\u00b7gluth", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "In deinen wirthlichen H\u00fctten", "tokens": ["In", "dei\u00b7nen", "wirth\u00b7li\u00b7chen", "H\u00fct\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Gastfreyheit winkend und hochgesch\u00fcrzt.", "tokens": ["Gast\u00b7frey\u00b7heit", "win\u00b7kend", "und", "hoch\u00b7ge\u00b7sch\u00fcrzt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "KON", "ADJD", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.8": {"line.1": {"text": "Auch segnet meine Seele,", "tokens": ["Auch", "seg\u00b7net", "mei\u00b7ne", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Bergtochter, dich, Sey immer", "tokens": ["Berg\u00b7toch\u00b7ter", ",", "dich", ",", "Sey", "im\u00b7mer"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["NN", "$,", "PPER", "$,", "VAFIN", "ADV"], "meter": "++--+-+", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Vom Thau des Himmels trunken!", "tokens": ["Vom", "Thau", "des", "Him\u00b7mels", "trun\u00b7ken", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sey immer satt vom Marke", "tokens": ["Sey", "im\u00b7mer", "satt", "vom", "Mar\u00b7ke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der gartenvollen Flur.", "tokens": ["Der", "gar\u00b7ten\u00b7vol\u00b7len", "Flur", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Wie heult der Sturm!", "tokens": ["Wie", "heult", "der", "Sturm", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Wie tobt das Meer!", "tokens": ["Wie", "tobt", "das", "Meer", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Tiefer erseufzen die Gr\u00e4ber der Helden;", "tokens": ["Tie\u00b7fer", "er\u00b7seuf\u00b7zen", "die", "Gr\u00e4\u00b7ber", "der", "Hel\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "Schauriger saust es in Raddas Hayn.", "tokens": ["Schau\u00b7ri\u00b7ger", "saust", "es", "in", "Rad\u00b7das", "Hayn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "NE", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.10": {"line.1": {"text": "Weit blinkt,", "tokens": ["Weit", "blinkt", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.2": {"text": "Fern dringt", "tokens": ["Fern", "dringt"], "token_info": ["word", "word"], "pos": ["NN", "VVFIN"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Mein rastlossp\u00e4hendes Auge. \u2014 \u2014", "tokens": ["Mein", "rast\u00b7loss\u00b7p\u00e4\u00b7hen\u00b7des", "Au\u00b7ge", "."], "token_info": ["word", "word", "word", "punct", "punct", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$.", "$(", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.11": {"line.1": {"text": "Wer ist sie, die in S\u00fcden,", "tokens": ["Wer", "ist", "sie", ",", "die", "in", "S\u00fc\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "$,", "PRELS", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In Mitte blauer Fluren,", "tokens": ["In", "Mit\u00b7te", "blau\u00b7er", "Flu\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Im Abendglanz der Sonne,", "tokens": ["Im", "A\u00b7bend\u00b7glanz", "der", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Zinnen ihrer Th\u00fcrme", "tokens": ["Die", "Zin\u00b7nen", "ih\u00b7rer", "Th\u00fcr\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Dem Nebelged\u00e4mmer erstreckt?", "tokens": ["Dem", "Ne\u00b7bel\u00b7ge\u00b7d\u00e4m\u00b7mer", "er\u00b7streckt", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.12": {"line.1": {"text": "Ist das nicht meine Traute,", "tokens": ["Ist", "das", "nicht", "mei\u00b7ne", "Trau\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "PTKNEG", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nicht meiner Jugend F\u00fchrerin,", "tokens": ["Nicht", "mei\u00b7ner", "Ju\u00b7gend", "F\u00fch\u00b7re\u00b7rin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nicht meiner Kr\u00e4fte Weckerin,", "tokens": ["Nicht", "mei\u00b7ner", "Kr\u00e4f\u00b7te", "We\u00b7cke\u00b7rin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nicht meiner Freuden Pflegerin? \u2014", "tokens": ["Nicht", "mei\u00b7ner", "Freu\u00b7den", "Pfle\u00b7ge\u00b7rin", "?"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PTKNEG", "PPOSAT", "NN", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ist's nicht mein Hyldathen ", "tokens": ["Ist's", "nicht", "mein", "Hyl\u00b7da\u00b7then"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "PTKNEG", "PPOSAT", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.13": {"line.1": {"text": "Es ists, es ists!", "tokens": ["Es", "ists", ",", "es", "ists", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "PPER", "VAFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Diess Brusterschwellen,", "tokens": ["Diess", "Brus\u00b7ter\u00b7schwel\u00b7len", ","], "token_info": ["word", "word", "punct"], "pos": ["PDAT", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Diess Wangergl\u00fchen,", "tokens": ["Diess", "Wan\u00b7ger\u00b7gl\u00fc\u00b7hen", ","], "