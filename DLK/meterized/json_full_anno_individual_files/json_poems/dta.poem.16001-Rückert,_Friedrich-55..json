{"dta.poem.16001": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "55.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das Mittelm\u00e4\u00dfige nur ist des Guten Feind,", "tokens": ["Das", "Mit\u00b7tel\u00b7m\u00e4\u00b7\u00dfi\u00b7ge", "nur", "ist", "des", "Gu\u00b7ten", "Feind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das Schlechte nicht, weil Schlecht und Gut sich nie vereint.", "tokens": ["Das", "Schlech\u00b7te", "nicht", ",", "weil", "Schlecht", "und", "Gut", "sich", "nie", "ver\u00b7eint", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "$,", "KOUS", "NN", "KON", "ADJD", "PRF", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Das Schlechte l\u00e4\u00dft sich nie dem Guten \u00e4hnlich drechseln,", "tokens": ["Das", "Schlech\u00b7te", "l\u00e4\u00dft", "sich", "nie", "dem", "Gu\u00b7ten", "\u00e4hn\u00b7lich", "drech\u00b7seln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ADV", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie sehn sich gar nicht gleich und sind nicht zu verwechseln.", "tokens": ["Sie", "sehn", "sich", "gar", "nicht", "gleich", "und", "sind", "nicht", "zu", "ver\u00b7wech\u00b7seln", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "PTKNEG", "ADV", "KON", "VAFIN", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Das Mittelm\u00e4\u00dfige dagegen, weil es zwischen", "tokens": ["Das", "Mit\u00b7tel\u00b7m\u00e4\u00b7\u00dfi\u00b7ge", "da\u00b7ge\u00b7gen", ",", "weil", "es", "zwi\u00b7schen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "PAV", "$,", "KOUS", "PPER", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gutem und Schlechtem liegt, droht beides zu vermischen.", "tokens": ["Gu\u00b7tem", "und", "Schlech\u00b7tem", "liegt", ",", "droht", "bei\u00b7des", "zu", "ver\u00b7mi\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "$,", "VVFIN", "PIS", "PTKZU", "VVINF", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}}}}}