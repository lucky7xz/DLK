{"textgrid.poem.67425": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "An Gleim", "genre": "verse", "period": "N.A.", "pub_year": 1795, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bl\u00e4tter, die Du in Deinem Busen bargest,", "tokens": ["Bl\u00e4t\u00b7ter", ",", "die", "Du", "in", "Dei\u00b7nem", "Bu\u00b7sen", "bar\u00b7gest", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Kommen Dir jetzt, ein Buch! Geliebter, br\u00e4chten", "tokens": ["Kom\u00b7men", "Dir", "jetzt", ",", "ein", "Buch", "!", "Ge\u00b7lieb\u00b7ter", ",", "br\u00e4ch\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word"], "pos": ["NN", "PPER", "ADV", "$,", "ART", "NN", "$.", "NN", "$,", "VVFIN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Dir sie zur\u00fcck die Stunden, da des H\u00fcttners liebliche T\u00f6ne,", "tokens": ["Dir", "sie", "zu\u00b7r\u00fcck", "die", "Stun\u00b7den", ",", "da", "des", "H\u00fctt\u00b7ners", "lieb\u00b7li\u00b7che", "T\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPER", "ADV", "ART", "NN", "$,", "KOUS", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+--+-", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "Fl\u00f6tent\u00f6ne, sich in den Hall der Tuba", "tokens": ["Fl\u00f6\u00b7ten\u00b7t\u00f6\u00b7ne", ",", "sich", "in", "den", "Hall", "der", "Tu\u00b7ba"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PRF", "APPR", "ART", "NN", "ART", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Mischten; wir sahn von der H\u00f6h hinunter", "tokens": ["Mischten", ";", "wir", "sahn", "von", "der", "H\u00f6h", "hin\u00b7un\u00b7ter"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "PPER", "VVFIN", "APPR", "ART", "NN", "APPO"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Und empfanden, o Gleim, das Gl\u00fcck harmonischen Lebens.", "tokens": ["Und", "emp\u00b7fan\u00b7den", ",", "o", "Gleim", ",", "das", "Gl\u00fcck", "har\u00b7mo\u00b7ni\u00b7schen", "Le\u00b7bens", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "FM", "NN", "$,", "ART", "NN", "ADJA", "NN", "$."], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Bl\u00e4tter, die Du in Deinem Busen bargest,", "tokens": ["Bl\u00e4t\u00b7ter", ",", "die", "Du", "in", "Dei\u00b7nem", "Bu\u00b7sen", "bar\u00b7gest", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Kommen Dir jetzt, ein Buch! Geliebter, br\u00e4chten", "tokens": ["Kom\u00b7men", "Dir", "jetzt", ",", "ein", "Buch", "!", "Ge\u00b7lieb\u00b7ter", ",", "br\u00e4ch\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word"], "pos": ["NN", "PPER", "ADV", "$,", "ART", "NN", "$.", "NN", "$,", "VVFIN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Dir sie zur\u00fcck die Stunden, da des H\u00fcttners liebliche T\u00f6ne,", "tokens": ["Dir", "sie", "zu\u00b7r\u00fcck", "die", "Stun\u00b7den", ",", "da", "des", "H\u00fctt\u00b7ners", "lieb\u00b7li\u00b7che", "T\u00f6\u00b7ne", ","], "