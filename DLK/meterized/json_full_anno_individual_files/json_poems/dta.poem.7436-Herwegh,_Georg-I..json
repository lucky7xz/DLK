{"dta.poem.7436": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "I.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192430", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Laut mit dem Schwall der Wogen ringend,", "tokens": ["Laut", "mit", "dem", "Schwall", "der", "Wo\u00b7gen", "rin\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Durchzieht den See der stolze D\u00e4mpfer,", "tokens": ["Durch\u00b7zieht", "den", "See", "der", "stol\u00b7ze", "D\u00e4mp\u00b7fer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und braust, das Schweizerbanner schwingend,", "tokens": ["Und", "braust", ",", "das", "Schwei\u00b7zer\u00b7ban\u00b7ner", "schwin\u00b7gend", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dahin, ein zornentbrannter K\u00e4mpfer.", "tokens": ["Da\u00b7hin", ",", "ein", "zor\u00b7nent\u00b7brann\u00b7ter", "K\u00e4mp\u00b7fer", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "\u201ewenn wir an Ulrich Huttens Grabe,", "tokens": ["\u201e", "wenn", "wir", "an", "Ul\u00b7rich", "Hut\u00b7tens", "Gra\u00b7be", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "APPR", "NE", "NE", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dort bei des Seees gr\u00f6\u00dfter Breitung,", "tokens": ["Dort", "bei", "des", "See\u00b7es", "gr\u00f6\u00df\u00b7ter", "Brei\u00b7tung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dann rufe mich, mein Schifferknabe!\u201c", "tokens": ["Dann", "ru\u00b7fe", "mich", ",", "mein", "Schif\u00b7fer\u00b7kna\u00b7be", "!", "\u201c"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und weiter tr\u00e4umt' ich in der Zeitung.", "tokens": ["Und", "wei\u00b7ter", "tr\u00e4umt'", "ich", "in", "der", "Zei\u00b7tung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Die Zeit, wie sich geb\u00fchrt, in Ehren,", "tokens": ["Die", "Zeit", ",", "wie", "sich", "ge\u00b7b\u00fchrt", ",", "in", "Eh\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "PRF", "VVPP", "$,", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Kann mich die Zeitung nie erfreuen;", "tokens": ["Kann", "mich", "die", "Zei\u00b7tung", "nie", "er\u00b7freu\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Doch mag der Teufel sie entbehren,", "tokens": ["Doch", "mag", "der", "Teu\u00b7fel", "sie", "ent\u00b7beh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Mensch will nun einmal vom Neuen!", "tokens": ["Der", "Mensch", "will", "nun", "ein\u00b7mal", "vom", "Neu\u00b7en", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Frankreich! Ha \u2014 was wird dort verhandelt?", "tokens": ["Fran\u00b7kreich", "!", "Ha", "was", "wird", "dort", "ver\u00b7han\u00b7delt", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "NE", "$(", "PWS", "VAFIN", "ADV", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Gift? Dolch? Emeuten? Carbonaris?", "tokens": ["Gift", "?", "Dolch", "?", "E\u00b7meu\u00b7ten", "?", "Car\u00b7bo\u00b7na\u00b7ris", "?"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$.", "NN", "$.", "NN", "$.", "NE", "$."], "meter": "-+-+-+---", "measure": "unknown.measure.tri"}, "line.3": {"text": "Die Scene wiederum verwandelt?", "tokens": ["Die", "Sce\u00b7ne", "wie\u00b7de\u00b7rum", "ver\u00b7wan\u00b7delt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das St\u00fcck hei\u00dft Helena und Paris!", "tokens": ["Das", "St\u00fcck", "hei\u00dft", "He\u00b7le\u00b7na", "und", "Pa\u00b7ris", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NE", "KON", "NE", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.5": {"line.1": {"text": "Sie haben ihren Unverge\u00dfnen", "tokens": ["Sie", "ha\u00b7ben", "ih\u00b7ren", "Un\u00b7ver\u00b7ge\u00df\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Geraubt dem Schoos krystallner Wogen,", "tokens": ["Ge\u00b7raubt", "dem", "Schoos", "krys\u00b7tall\u00b7ner", "Wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Den Helden aus dem Unerme\u00dfnen", "tokens": ["Den", "Hel\u00b7den", "aus", "dem", "Un\u00b7er\u00b7me\u00df\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In ihres Babels Koth gezogen.", "tokens": ["In", "ih\u00b7res", "Ba\u00b7bels", "Koth", "ge\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Sie kamen \u00fcber ihn im Schlafe,", "tokens": ["Sie", "ka\u00b7men", "\u00fc\u00b7ber", "ihn", "im", "Schla\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie \u00fcber Simson die Philister;", "tokens": ["Wie", "\u00fc\u00b7ber", "Sim\u00b7son", "die", "Phi\u00b7lis\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NE", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Es triumphirt der gro\u00dfe Sklave,", "tokens": ["Es", "tri\u00b7um\u00b7phirt", "der", "gro\u00b7\u00dfe", "Skla\u00b7ve", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und pfiffig l\u00e4chelt sein Minister.", "tokens": ["Und", "pfif\u00b7fig", "l\u00e4\u00b7chelt", "sein", "Mi\u00b7nis\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Was Albion heilig, wird man lesen,", "tokens": ["Was", "Al\u00b7bi\u00b7on", "hei\u00b7lig", ",", "wird", "man", "le\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "ADJD", "$,", "VAFIN", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Das hat der Franken Volk vernichtet;", "tokens": ["Das", "hat", "der", "Fran\u00b7ken", "Volk", "ver\u00b7nich\u00b7tet", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "England lie\u00df ruhig Ihn verwesen,", "tokens": ["En\u00b7gland", "lie\u00df", "ru\u00b7hig", "Ihn", "ver\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADJD", "PPER", "VVPP", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Wo Ihn der Weltgeist hingedichtet;", "tokens": ["Wo", "Ihn", "der", "Welt\u00b7geist", "hin\u00b7ge\u00b7dich\u00b7tet", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Wo Ihn des Meeres Flut umsch\u00e4umte,", "tokens": ["Wo", "Ihn", "des", "Mee\u00b7res", "Flut", "um\u00b7sch\u00e4um\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wo mit dem All Er im Vereine", "tokens": ["Wo", "mit", "dem", "All", "Er", "im", "Ver\u00b7ei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "PPER", "APPRART", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Wohl oft von jenem Gothen tr\u00e4umte,", "tokens": ["Wohl", "oft", "von", "je\u00b7nem", "Go\u00b7then", "tr\u00e4um\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "De\u00df Grab doch sichrer, als das seine.", "tokens": ["De\u00df", "Grab", "doch", "sich\u00b7rer", ",", "als", "das", "sei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJA", "$,", "KOUS", "ART", "PPOSAT", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "O Spott! es schleppt in ihre Mauern", "tokens": ["O", "Spott", "!", "es", "schleppt", "in", "ih\u00b7re", "Mau\u00b7ern"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "$.", "PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein H\u00e4nfling dieses Adlers Leiche;", "tokens": ["Ein", "H\u00e4nf\u00b7ling", "die\u00b7ses", "Ad\u00b7lers", "Lei\u00b7che", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Nicht Jubelschall, nur banges Trauern", "tokens": ["Nicht", "Ju\u00b7bel\u00b7schall", ",", "nur", "ban\u00b7ges", "Trau\u00b7ern"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PTKNEG", "NN", "$,", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sollt' herrschen in der Franken Reiche.", "tokens": ["Sollt'", "herr\u00b7schen", "in", "der", "Fran\u00b7ken", "Rei\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "APPR", "ART", "NN", "NE", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Das eigne Volk sa\u00df zu Gerichte,", "tokens": ["Das", "eig\u00b7ne", "Volk", "sa\u00df", "zu", "Ge\u00b7rich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Des Kaisers Zauber ist geschieden;", "tokens": ["Des", "Kai\u00b7sers", "Zau\u00b7ber", "ist", "ge\u00b7schie\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}