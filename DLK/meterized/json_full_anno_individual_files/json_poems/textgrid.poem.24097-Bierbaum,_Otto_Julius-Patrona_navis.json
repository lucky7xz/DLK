{"textgrid.poem.24097": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Patrona navis", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun darf ichs sagen, da\u00df ich viele Jahre", "tokens": ["Nun", "darf", "ichs", "sa\u00b7gen", ",", "da\u00df", "ich", "vie\u00b7le", "Jah\u00b7re"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PIS", "VVINF", "$,", "KOUS", "PPER", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Verworfen war und schlimm umhergetrieben", "tokens": ["Ver\u00b7wor\u00b7fen", "war", "und", "schlimm", "um\u00b7her\u00b7ge\u00b7trie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "KON", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In Tiefen, die voll Schlamm und Grauensind.", "tokens": ["In", "Tie\u00b7fen", ",", "die", "voll", "Schlamm", "und", "Grau\u00b7en\u00b7sind", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "ADJD", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Jetzt geht mein Weg ins hingebreitet Klare,", "tokens": ["Jetzt", "geht", "mein", "Weg", "ins", "hin\u00b7ge\u00b7brei\u00b7tet", "Kla\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "APPRART", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und kaum Erinnrung ist mir mehr geblieben", "tokens": ["Und", "kaum", "E\u00b7rinn\u00b7rung", "ist", "mir", "mehr", "ge\u00b7blie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "NN", "VAFIN", "PPER", "ADV", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "An jene Jahre w\u00fcst und leer und blind.", "tokens": ["An", "je\u00b7ne", "Jah\u00b7re", "w\u00fcst", "und", "leer", "und", "blind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "KON", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Gl\u00fcck hei\u00dft das Schiff, auf dem ich selig fahre,", "tokens": ["Gl\u00fcck", "hei\u00dft", "das", "Schiff", ",", "auf", "dem", "ich", "se\u00b7lig", "fah\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,", "APPR", "PRELS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Dein Name ist ihm golden aufgeschrieben,", "tokens": ["Dein", "Na\u00b7me", "ist", "ihm", "gol\u00b7den", "auf\u00b7ge\u00b7schrie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Und alle Segel sind voll gutem Wind.", "tokens": ["Und", "al\u00b7le", "Se\u00b7gel", "sind", "voll", "gu\u00b7tem", "Wind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Nun darf ichs sagen, da\u00df ich viele Jahre", "tokens": ["Nun", "darf", "ichs", "sa\u00b7gen", ",", "da\u00df", "ich", "vie\u00b7le", "Jah\u00b7re"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PIS", "VVINF", "$,", "KOUS", "PPER", "PIAT", "NN"],