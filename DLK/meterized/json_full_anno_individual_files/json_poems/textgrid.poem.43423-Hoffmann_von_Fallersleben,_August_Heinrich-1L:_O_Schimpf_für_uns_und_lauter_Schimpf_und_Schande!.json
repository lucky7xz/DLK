{"textgrid.poem.43423": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: O Schimpf f\u00fcr uns und lauter Schimpf und Schande!", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Schimpf f\u00fcr uns und lauter Schimpf und Schande!", "tokens": ["O", "Schimpf", "f\u00fcr", "uns", "und", "lau\u00b7ter", "Schimpf", "und", "Schan\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPR", "PPER", "KON", "PIAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und Gott sei es geklagt,", "tokens": ["Und", "Gott", "sei", "es", "ge\u00b7klagt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-++--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Da\u00df noch ein Feind im deutschen Vaterlande", "tokens": ["Da\u00df", "noch", "ein", "Feind", "im", "deut\u00b7schen", "Va\u00b7ter\u00b7lan\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Uns wie der Teufel plagt!", "tokens": ["Uns", "wie", "der", "Teu\u00b7fel", "plagt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KOKOM", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ein Feind, der unsers Lebens h\u00f6chste G\u00fcter", "tokens": ["Ein", "Feind", ",", "der", "un\u00b7sers", "Le\u00b7bens", "h\u00f6chs\u00b7te", "G\u00fc\u00b7ter"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "P