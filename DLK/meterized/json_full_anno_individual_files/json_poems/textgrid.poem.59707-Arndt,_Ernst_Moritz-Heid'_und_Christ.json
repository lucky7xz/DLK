{"textgrid.poem.59707": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "Heid' und Christ", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbgeh drunter durch und la\u00df es rollen!", "tokens": ["\u00bb", "geh", "drun\u00b7ter", "durch", "und", "la\u00df", "es", "rol\u00b7len", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PAV", "APPR", "KON", "VVIMP", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "La\u00df donnern laut, was Donner ist!", "tokens": ["La\u00df", "don\u00b7nern", "laut", ",", "was", "Don\u00b7ner", "ist", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ADJD", "$,", "PWS", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Denn was die H\u00f6chsten droben wollen,", "tokens": ["Denn", "was", "die", "H\u00f6chs\u00b7ten", "dro\u00b7ben", "wol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "ADV", "VMFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das mu\u00df geschehn zu jeder Frist.", "tokens": ["Das", "mu\u00df", "ge\u00b7schehn", "zu", "je\u00b7der", "Frist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "VVINF", "PTKZU", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Sprich: Trotz sei dir, o Himmelsk\u00f6nig!", "tokens": ["Sprich", ":", "Trotz", "sei", "dir", ",", "o", "Him\u00b7mels\u00b7k\u00f6\u00b7nig", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "$.", "NN", "VAFIN", "PPER", "$,", "FM", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Auch meine Stirne tr\u00e4gt dein Mal \u2013", "tokens": ["Auch", "mei\u00b7ne", "Stir\u00b7ne", "tr\u00e4gt", "dein", "Mal", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hui! Hoch und Niedrig, Gro\u00df und Wenig!", "tokens": ["Hui", "!", "Hoch", "und", "Nied\u00b7rig", ",", "Gro\u00df", "und", "We\u00b7nig", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["FM", "$.", "ADJD", "KON", "NE", "$,", "NE", "KON", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "K\u00fchn steh' ich deinem Wetterstrahl.", "tokens": ["K\u00fchn", "steh'", "ich", "dei\u00b7nem", "Wet\u00b7ter\u00b7strahl", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wie? Knien, wimmern mit den Wichten?", "tokens": ["Wie", "?", "Kni\u00b7en", ",", "wim\u00b7mern", "mit", "den", "Wich\u00b7ten", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "NN", "$,", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das hemmt kein Tr\u00f6pflein deiner Flut \u2013", "tokens": ["Das", "hemmt", "kein", "Tr\u00f6pf\u00b7lein", "dei\u00b7ner", "Flut", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PIAT", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zerschlagen kannst du, nicht vernichten:", "tokens": ["Zer\u00b7schla\u00b7gen", "kannst", "du", ",", "nicht", "ver\u00b7nich\u00b7ten", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "$,", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Auch meine Brust schwellt G\u00f6tterglut.", "tokens": ["Auch", "mei\u00b7ne", "Brust", "schwellt", "G\u00f6t\u00b7ter\u00b7glut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Reck' aus denn! Schleudre deine Flamme", "tokens": ["Reck'", "aus", "denn", "!", "Schleud\u00b7re", "dei\u00b7ne", "Flam\u00b7me"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "APPR", "ADV", "$.", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit deinen sch\u00e4rfsten Blitzen aus!", "tokens": ["Mit", "dei\u00b7nen", "sch\u00e4rfs\u00b7ten", "Blit\u00b7zen", "aus", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den Funken vom Prometheusstamme", "tokens": ["Den", "Fun\u00b7ken", "vom", "Pro\u00b7me\u00b7theus\u00b7stam\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ihn l\u00f6schet keine Allmacht aus.\u00ab", "tokens": ["Ihn", "l\u00f6\u00b7schet", "kei\u00b7ne", "All\u00b7macht", "aus", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "So klingt des alten Heiden Rede,", "tokens": ["So", "klingt", "des", "al\u00b7ten", "Hei\u00b7den", "Re\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So k\u00e4mpft er, gro\u00df im Weltensturz,", "tokens": ["So", "k\u00e4mpft", "er", ",", "gro\u00df", "im", "Wel\u00b7ten\u00b7sturz", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die ungeheure Geisterfehde,", "tokens": ["Die", "un\u00b7ge\u00b7heu\u00b7re", "Geis\u00b7ter\u00b7feh\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Doch wird ihm Lust und Atem kurz.", "tokens": ["Doch", "wird", "ihm", "Lust", "und", "A\u00b7tem", "kurz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "NN", "KON", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Was frommt ", "tokens": ["Was", "frommt"], "token_info": ["word", "word"], "pos": ["PWS", "VVFIN"], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "Was frommt der k\u00fchne Spruch dem Mann,", "tokens": ["Was", "frommt", "der", "k\u00fch\u00b7ne", "Spruch", "dem", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den von dem finstern Stolz erretten", "tokens": ["Den", "von", "dem", "fins\u00b7tern", "Stolz", "er\u00b7ret\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Kein Sonnenstrahl der Liebe kann?", "tokens": ["Kein", "Son\u00b7nen\u00b7strahl", "der", "Lie\u00b7be", "kann", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "NN", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Was frommt ihm Werfen in die Speichen", "tokens": ["Was", "frommt", "ihm", "Wer\u00b7fen", "in", "die", "Spei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Des Schicksalswagens frech die Faust,", "tokens": ["Des", "Schick\u00b7sals\u00b7wa\u00b7gens", "frech", "die", "Faust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der \u00fcber Tr\u00fcmmer fort und Leichen", "tokens": ["Der", "\u00fc\u00b7ber", "Tr\u00fcm\u00b7mer", "fort", "und", "Lei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "NN", "PTKVZ", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Unwiderstehlich weitersaust?", "tokens": ["Un\u00b7wi\u00b7der\u00b7steh\u00b7lich", "wei\u00b7ter\u00b7saust", "?"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Was frommt's, im bittern Gram vermodern,", "tokens": ["Was", "frommt's", ",", "im", "bit\u00b7tern", "Gram", "ver\u00b7mo\u00b7dern", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "APPRART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn auch der Funke nimmer stirbt?", "tokens": ["Wenn", "auch", "der", "Fun\u00b7ke", "nim\u00b7mer", "stirbt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In hei\u00dfer Flamm' unsterblich lodern,", "tokens": ["In", "hei\u00b7\u00dfer", "Flamm'", "uns\u00b7terb\u00b7lich", "lo\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn Lust und Mut im Trotz verdirbt?", "tokens": ["Wenn", "Lust", "und", "Mut", "im", "Trotz", "ver\u00b7dirbt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Mir horch'! Ich will dir Sch\u00f6nres weisen,", "tokens": ["Mir", "horch'", "!", "Ich", "will", "dir", "Sch\u00f6n\u00b7res", "wei\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "$.", "PPER", "VMFIN", "PPER", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den hellen, heitern, sanften Weg:", "tokens": ["Den", "hel\u00b7len", ",", "hei\u00b7tern", ",", "sanf\u00b7ten", "Weg", ":"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zu meinem Heiland sollst du reisen,", "tokens": ["Zu", "mei\u00b7nem", "Hei\u00b7land", "sollst", "du", "rei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ihn schaun, und Trotz und Zorn ist weg.", "tokens": ["Ihn", "schaun", ",", "und", "Trotz", "und", "Zorn", "ist", "weg", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "$,", "KON", "NN", "KON", "NN", "VAFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Prometheus auch hat er gehangen", "tokens": ["Pro\u00b7me\u00b7theus", "auch", "hat", "er", "ge\u00b7han\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "VAFIN", "PPER", "VVPP"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "An seinem blut'gen Kaukasus,", "tokens": ["An", "sei\u00b7nem", "blut'\u00b7gen", "Kau\u00b7ka\u00b7sus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Weil er das Sehnen und Verlangen", "tokens": ["Weil", "er", "das", "Seh\u00b7nen", "und", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der bangen Seelen stillen mu\u00df;", "tokens": ["Der", "ban\u00b7gen", "See\u00b7len", "stil\u00b7len", "mu\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Weil er das neue Licht getragen,", "tokens": ["Weil", "er", "das", "neu\u00b7e", "Licht", "ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Menschheit junges Morgenrot,", "tokens": ["Der", "Menschheit", "jun\u00b7ges", "Mor\u00b7gen\u00b7rot", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Ist er ans Schandenholz geschlagen,", "tokens": ["Ist", "er", "ans", "Schan\u00b7den\u00b7holz", "ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gestorben den Verbrechertod.", "tokens": ["Ge\u00b7stor\u00b7ben", "den", "Ver\u00b7bre\u00b7cher\u00b7tod", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Ihn schau', sein Kreuz und seine Wunden,", "tokens": ["Ihn", "schau'", ",", "sein", "Kreuz", "und", "sei\u00b7ne", "Wun\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dann geht dir auf ein himmlisch Licht,", "tokens": ["Dann", "geht", "dir", "auf", "ein", "himm\u00b7lisch", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und alle Schrecken sind verschwunden,", "tokens": ["Und", "al\u00b7le", "Schre\u00b7cken", "sind", "ver\u00b7schwun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und Stolz und Hochmut k\u00e4mpfen nicht.", "tokens": ["Und", "Stolz", "und", "Hoch\u00b7mut", "k\u00e4mp\u00b7fen", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Dann la\u00df nur alle Donner rollen,", "tokens": ["Dann", "la\u00df", "nur", "al\u00b7le", "Don\u00b7ner", "rol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "ADV", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Gar fr\u00f6hlich gehst du drunter durch:", "tokens": ["Gar", "fr\u00f6h\u00b7lich", "gehst", "du", "drun\u00b7ter", "durch", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PPER", "PAV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Denn wie dein Vater mu\u00df Gott wollen,", "tokens": ["Denn", "wie", "dein", "Va\u00b7ter", "mu\u00df", "Gott", "wol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPOSAT", "NN", "VMFIN", "NN", "VMFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und Liebe tr\u00e4gt die Himmelsburg.", "tokens": ["Und", "Lie\u00b7be", "tr\u00e4gt", "die", "Him\u00b7mels\u00b7burg", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "\u00bbgeh drunter durch und la\u00df es rollen!", "tokens": ["\u00bb", "geh", "drun\u00b7ter", "durch", "und", "la\u00df", "es", "rol\u00b7len", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PAV", "APPR", "KON", "VVIMP", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "La\u00df donnern laut, was Donner ist!", "tokens": ["La\u00df", "don\u00b7nern", "l