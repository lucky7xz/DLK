{"dta.poem.9257": {"metadata": {"author": {"name": "Weise, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Das Erste Gespr\u00e4ch.", "genre": "Lyrik; Drama; Prosa", "period": "N.A.", "pub_year": "1701", "urn": "urn:nbn:de:kobv:b4-25043-0", "language": ["de:0.99"], "booktitle": "Weise, Christian: \u00dcberfl\u00fc\u00dfige Gedancken Der gr\u00fcnenden jugend. Leipzig, 1701."}, "poem": {"stanza.1": {"line.1": {"text": "Astr\u00e4a bistu wol mit mir zu frieden", "tokens": ["A\u00b7str\u00e4a", "bis\u00b7tu", "wol", "mit", "mir", "zu", "frie\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "ADV", "APPR", "PPER", "APPR", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Da\u00df ich zur\u00fccke reisen kan/", "tokens": ["Da\u00df", "ich", "zu\u00b7r\u00fc\u00b7cke", "rei\u00b7sen", "kan", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nachdem ich dir mein hertz allhier beschieden", "tokens": ["Nach\u00b7dem", "ich", "dir", "mein", "hertz", "all\u00b7hier", "be\u00b7schie\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "PPOSAT", "NN", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und die versicherung gethan/", "tokens": ["Und", "die", "ver\u00b7si\u00b7che\u00b7rung", "ge\u00b7than", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da\u00df ich aus allen", "tokens": ["Da\u00df", "ich", "aus", "al\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PIAT"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Dir zu gefallen/", "tokens": ["Dir", "zu", "ge\u00b7fal\u00b7len", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Keine will lieben als eben dich.", "tokens": ["Kei\u00b7ne", "will", "lie\u00b7ben", "als", "e\u00b7ben", "dich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "VVINF", "KOKOM", "ADV", "PPER", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "2. Astr\u00e4a schaue nur auf mein gel\u00fccke", "tokens": ["A\u00b7str\u00e4a", "schau\u00b7e", "nur", "auf", "mein", "ge\u00b7l\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Dem ich noch immer folgen mu\u00df/", "tokens": ["Dem", "ich", "noch", "im\u00b7mer", "fol\u00b7gen", "mu\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADV", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dasselbe locket mich so weit zur\u00fccke", "tokens": ["Das\u00b7sel\u00b7be", "lo\u00b7cket", "mich", "so", "weit", "zu\u00b7r\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und hemt den zuckers\u00fcssen schlu\u00df", "tokens": ["Und", "hemt", "den", "zu\u00b7cker\u00b7s\u00fcs\u00b7sen", "schlu\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich bleib indessen", "tokens": ["Ich", "bleib", "in\u00b7des\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Dein unvergessen", "tokens": ["Dein", "un\u00b7ver\u00b7ges\u00b7sen"], "token_info": ["word", "word"], "pos": ["PPOSAT", "ADJA"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Edelstes seelgen und denck an dich.", "tokens": ["E\u00b7dels\u00b7tes", "seel\u00b7gen", "und", "denck", "an", "dich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "KON", "VVIMP", "APPR", "PPER", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "3. Die winde k\u00f6nnen zwar den leib verjagen/", "tokens": ["Die", "win\u00b7de", "k\u00f6n\u00b7nen", "zwar", "den", "leib", "ver\u00b7ja\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VMFIN", "ADV", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch die gedancken bleiben hier/", "tokens": ["Doch", "die", "ge\u00b7dan\u00b7cken", "blei\u00b7ben", "hier", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die werden dir allzeit ins ohre sagen:", "tokens": ["Die", "wer\u00b7den", "dir", "all\u00b7zeit", "ins", "oh\u00b7re", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ach dein Adonis lebt in dir/", "tokens": ["Ach", "dein", "A\u00b7do\u00b7nis", "lebt", "in", "dir", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PPOSAT", "NN", "VVFIN", "APPR", "PPER", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der ungebrochen", "tokens": ["Der", "un\u00b7ge\u00b7bro\u00b7chen"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Sich selbst versprochen/", "tokens": ["Sich", "selbst", "ver\u00b7spro\u00b7chen", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PRF", "ADV", "VVINF", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Liebste derhalben besinne dich.", "tokens": ["Liebs\u00b7te", "der\u00b7hal\u00b7ben", "be\u00b7sin\u00b7ne", "dich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDS", "VVFIN", "PPER", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "4. Wird ja der neid und ha\u00df mit wort und wercken", "tokens": ["Wird", "ja", "der", "neid", "und", "ha\u00df", "mit", "wort", "und", "wer\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ART", "NN", "KON", "VVIMP", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Uns mitler zeit entgegen gehn/", "tokens": ["Uns", "mit\u00b7ler", "zeit", "ent\u00b7ge\u00b7gen", "gehn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "PTKVZ", "VVINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So wollen wir uns doch in hoffnung st\u00e4rcken", "tokens": ["So", "wol\u00b7len", "wir", "uns", "doch", "in", "hoff\u00b7nung", "st\u00e4r\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "ADV", "APPR", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und der verfolgung widerstehn/", "tokens": ["Und", "der", "ver\u00b7fol\u00b7gung", "wi\u00b7der\u00b7stehn", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Bi\u00df unsre Sonne", "tokens": ["Bi\u00df", "uns\u00b7re", "Son\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Mit lust und wonne", "tokens": ["Mit", "lust", "und", "won\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "PWAV"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Scheinet vollkommen auff mich und dich.", "tokens": ["Schei\u00b7net", "voll\u00b7kom\u00b7men", "auff", "mich", "und", "dich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "PPER", "KON", "PPER", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.5": {"line.1": {"text": "5. Der himmel wird es schon mit ehsten f\u00fcgen/", "tokens": ["Der", "him\u00b7mel", "wird", "es", "schon", "mit", "ehs\u00b7ten", "f\u00fc\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "APPR", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wofern du nur zufrieden bist/", "tokens": ["Wo\u00b7fern", "du", "nur", "zu\u00b7frie\u00b7den", "bist", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VAFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df wir uns beyderseits also vergn\u00fcgen/", "tokens": ["Da\u00df", "wir", "uns", "bey\u00b7der\u00b7seits", "al\u00b7so", "ver\u00b7gn\u00fc\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Gleichwie der wunsch gewesen ist:", "tokens": ["Gleich\u00b7wie", "der", "wunsch", "ge\u00b7we\u00b7sen", "ist", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJD", "VAPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Jtzt hilfft kein klagen/", "tokens": ["Jtzt", "hilfft", "kein", "kla\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "VVINF", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Nur la\u00df mich sagen:", "tokens": ["Nur", "la\u00df", "mich", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Liebe mich Sch\u00f6nste/ so lieb ich dich.", "tokens": ["Lie\u00b7be", "mich", "Sch\u00f6ns\u00b7te", "/", "so", "lieb", "ich", "dich", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "NN", "$(", "ADV", "ADJD", "PPER", "PRF", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.6": {"line.1": {"text": "6. ", "tokens": [], "token_info": [], "pos": []}, "line.2": {"text": "Ein \u00fcbrigs trauren hilfft nicht viel/", "tokens": ["Ein", "\u00fcb\u00b7rigs", "trau\u00b7ren", "hilfft", "nicht", "viel", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVINF", "VVFIN", "PTKNEG", "ADV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Weil ich in kurtzen dich mit grosser freude", "tokens": ["Weil", "ich", "in", "kurt\u00b7zen", "dich", "mit", "gros\u00b7ser", "freu\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Gesund und froh umbfangen wil.", "tokens": ["Ge\u00b7sund", "und", "froh", "umb\u00b7fan\u00b7gen", "wil", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du bist mein leben", "tokens": ["Du", "bist", "mein", "le\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPOSAT", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Bleib mir ergeben", "tokens": ["Bleib", "mir", "er\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["NN", "PPER", "VVPP"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Liebste so leb ich vor mich und dich.", "tokens": ["Liebs\u00b7te", "so", "leb", "ich", "vor", "mich", "und", "dich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVFIN", "PPER", "APPR", "PPER", "KON", "PPER", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}}}}