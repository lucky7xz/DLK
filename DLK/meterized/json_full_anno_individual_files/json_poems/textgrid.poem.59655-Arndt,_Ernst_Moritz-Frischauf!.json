, "Sand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVPP", "KOKOM", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "Es gilt mit Gott hineinzufahren,", "tokens": ["Es", "gilt", "mit", "Gott", "hin\u00b7ein\u00b7zu\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Gott wird Unten Oben sein;", "tokens": ["Mit", "Gott", "wird", "Un\u00b7ten", "O\u00b7ben", "sein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "ADV", "ADV", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Denk' der Jahrtausende, die waren,", "tokens": ["Denk'", "der", "Jahr\u00b7tau\u00b7sen\u00b7de", ",", "die", "wa\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "$,", "PRELS", "VAFIN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Jahrtausende, die werden sein.", "tokens": ["Jahr\u00b7tau\u00b7sen\u00b7de", ",", "die", "wer\u00b7den", "sein", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "VAINF", "VAINF", "$."], "meter": "-+---+-+", "measure": "dactylic.init"}}}}}