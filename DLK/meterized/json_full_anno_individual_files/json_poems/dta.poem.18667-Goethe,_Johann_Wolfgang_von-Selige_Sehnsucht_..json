{"dta.poem.18667": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "Selige Sehnsucht .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Sagt es niemand, nur den Weisen,", "tokens": ["Sagt", "es", "nie\u00b7mand", ",", "nur", "den", "Wei\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIS", "$,", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Weil die Menge gleich verh\u00f6hnet,", "tokens": ["Weil", "die", "Men\u00b7ge", "gleich", "ver\u00b7h\u00f6h\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Das Lebend\u2019ge will ich preisen", "tokens": ["Das", "Le\u00b7ben\u00b7d'\u00b7ge", "will", "ich", "prei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das nach Flammentod sich sehnet.", "tokens": ["Das", "nach", "Flam\u00b7men\u00b7tod", "sich", "seh\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "NN", "PRF", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "In der Liebesn\u00e4chte K\u00fchlung,", "tokens": ["In", "der", "Lie\u00b7bes\u00b7n\u00e4ch\u00b7te", "K\u00fch\u00b7lung", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die dich zeugte, wo du zeugtest,", "tokens": ["Die", "dich", "zeug\u00b7te", ",", "wo", "du", "zeug\u00b7test", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ueberf\u00e4llt dich fremde F\u00fchlung", "tokens": ["Ue\u00b7ber\u00b7f\u00e4llt", "dich", "frem\u00b7de", "F\u00fch\u00b7lung"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn die stille Kerze leuchtet.", "tokens": ["Wenn", "die", "stil\u00b7le", "Ker\u00b7ze", "leuch\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Nicht mehr bleibest du umfangen", "tokens": ["Nicht", "mehr", "blei\u00b7best", "du", "um\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "VVFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In der Finsterniss Beschattung,", "tokens": ["In", "der", "Fins\u00b7ter\u00b7niss", "Be\u00b7schat\u00b7tung", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und dich reisset neu Verlangen", "tokens": ["Und", "dich", "reis\u00b7set", "neu", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADJD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf zu h\u00f6herer Begattung.", "tokens": ["Auf", "zu", "h\u00f6\u00b7he\u00b7rer", "Be\u00b7gat\u00b7tung", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Keine Ferne macht dich schwierig,", "tokens": ["Kei\u00b7ne", "Fer\u00b7ne", "macht", "dich", "schwie\u00b7rig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kommst geflogen und gebannt.", "tokens": ["Kommst", "ge\u00b7flo\u00b7gen", "und", "ge\u00b7bannt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "KON", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und zuletzt, des Lichts begierig,", "tokens": ["Und", "zu\u00b7letzt", ",", "des", "Lichts", "be\u00b7gie\u00b7rig", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bist du Schmetterling verbrannt,", "tokens": ["Bist", "du", "Schmet\u00b7ter\u00b7ling", "ver\u00b7brannt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Und so lang du das nicht hast,", "tokens": ["Und", "so", "lang", "du", "das", "nicht", "hast", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "PPER", "PDS", "PTKNEG", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Dieses: Stirb und werde!", "tokens": ["Die\u00b7ses", ":", "Stirb", "und", "wer\u00b7de", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PDAT", "$.", "VVIMP", "KON", "VAFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Bist du nur ein tr\u00fcber Gast", "tokens": ["Bist", "du", "nur", "ein", "tr\u00fc\u00b7ber", "Gast"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf der dunklen Erde.", "tokens": ["Auf", "der", "dunk\u00b7len", "Er\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}