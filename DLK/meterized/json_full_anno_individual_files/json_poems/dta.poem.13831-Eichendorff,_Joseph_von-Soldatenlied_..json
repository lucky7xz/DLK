{"dta.poem.13831": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Soldatenlied .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.85", "sv:0.14"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Trompeten nur h\u00f6r' ich werben", "tokens": ["Trom\u00b7pe\u00b7ten", "nur", "h\u00f6r'", "ich", "wer\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "VVFIN", "PPER", "VVINF"], "meter": "++--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "So hell durch die Fr\u00fchlingsluft,", "tokens": ["So", "hell", "durch", "die", "Fr\u00fch\u00b7lings\u00b7luft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Zur Hochzeit oder zum Sterben", "tokens": ["Zur", "Hoch\u00b7zeit", "o\u00b7der", "zum", "Ster\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "KON", "APPRART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "So \u00fcberm\u00e4chtig es ruft.", "tokens": ["So", "\u00fc\u00b7ber\u00b7m\u00e4ch\u00b7tig", "es", "ruft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}