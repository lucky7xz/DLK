{"textgrid.poem.53289": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die\u00df Pilger-Land l\u00e4sst keinen ruhig bleiben,", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die\u00df Pilger-Land l\u00e4sst keinen ruhig bleiben,", "tokens": ["Die\u00df", "Pil\u00b7ger\u00b7Land", "l\u00e4sst", "kei\u00b7nen", "ru\u00b7hig", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "VVFIN", "PIAT", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wir m\u00fcssen stets vmbher vns lassen treiben,", "tokens": ["Wir", "m\u00fcs\u00b7sen", "stets", "vm\u00b7bher", "vns", "las\u00b7sen", "trei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADJD", "PPER", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So schickt es Gott, damit wir vns bey zeiten,", "tokens": ["So", "schickt", "es", "Gott", ",", "da\u00b7mit", "wir", "vns", "bey", "zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "$,", "KOUS", "PPER", "PRF", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zur letzten Fahrt aus dieser Welt bereiten:", "tokens": ["Zur", "letz\u00b7ten", "Fahrt", "aus", "die\u00b7ser", "Welt", "be\u00b7rei\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "APPR", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Doch welcher inniglich", "tokens": ["Doch", "wel\u00b7cher", "in\u00b7nig\u00b7lich"], "token_info": ["word", "word", "word"], "pos": ["KON", "PWAT", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Mit zuvertrawen sich,", "tokens": ["Mit", "zu\u00b7ver\u00b7tra\u00b7wen", "sich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "PRF", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "Auff seinen Gott kan gr\u00fcnden,", "tokens": ["Auff", "sei\u00b7nen", "Gott", "kan", "gr\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ihm heimstellt Gl\u00fcck vnd Fall,", "tokens": ["Ihm", "heim\u00b7stellt", "Gl\u00fcck", "vnd", "Fall", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Der wird sich \u00fcberall", "tokens": ["Der", "wird", "sich", "\u00fc\u00b7be\u00b7rall"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "PRF", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Zu Haus' vnd wol befinden.", "tokens": ["Zu", "Haus'", "vnd", "wol", "be\u00b7fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich lasse mich durch mein Verh\u00e4ngni\u00df bringen,", "tokens": ["Ich", "las\u00b7se", "mich", "durch", "mein", "Ver\u00b7h\u00e4ng\u00b7ni\u00df", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wohin das Liecht der Sonnen nicht kan dringen,", "tokens": ["Wo\u00b7hin", "das", "Liecht", "der", "Son\u00b7nen", "nicht", "kan", "drin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "PTKNEG", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wil jrrig gehn im heissen Mohren-Sande,", "tokens": ["Wil", "jr\u00b7rig", "gehn", "im", "heis\u00b7sen", "Moh\u00b7ren\u00b7San\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Werd' vnbekant zu Wasser vnd zu Lande,", "tokens": ["Werd'", "vn\u00b7be\u00b7kant", "zu", "Was\u00b7ser", "vnd", "zu", "Lan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Hab ich nur f\u00fcr vnd f\u00fcr", "tokens": ["Hab", "ich", "nur", "f\u00fcr", "vnd", "f\u00fcr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ADV", "APPR", "KON", "APPR"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Gott, meinen Schutz, bey mir,", "tokens": ["Gott", ",", "mei\u00b7nen", "Schutz", ",", "bey", "mir", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "$,", "APPR", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "So wil ich selig leben,", "tokens": ["So", "wil", "ich", "se\u00b7lig", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Auch einen Lobgesang", "tokens": ["Auch", "ei\u00b7nen", "Lob\u00b7ge\u00b7sang"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Zu sonderlichem Danck", "tokens": ["Zu", "son\u00b7der\u00b7li\u00b7chem", "Danck"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Ihm noch dabey erheben.", "tokens": ["Ihm", "noch", "da\u00b7bey", "er\u00b7he\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PAV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "O Gott, der du die gantze Welt regierest,", "tokens": ["O", "Gott", ",", "der", "du", "die", "gant\u00b7ze", "Welt", "re\u00b7gie\u00b7rest", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PRELS", "PPER", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vnd vns, dein Volck, so wunderbarlich f\u00fchrest,", "tokens": ["Vnd", "vns", ",", "dein", "Volck", ",", "so", "wun\u00b7der\u00b7bar\u00b7lich", "f\u00fch\u00b7rest", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "PPOSAT", "NN", "$,", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Komm, steh vns bey auff allen vnsern Wegen", "tokens": ["Komm", ",", "steh", "vns", "bey", "auff", "al\u00b7len", "vn\u00b7sern", "We\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "VVFIN", "PPER", "APPR", "APPR", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mit deinem Schutz vnd gnadenreichen Segen,", "tokens": ["Mit", "dei\u00b7nem", "Schutz", "vnd", "gna\u00b7den\u00b7rei\u00b7chen", "Se\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Sey auch an diesem Ort',", "tokens": ["Sey", "auch", "an", "die\u00b7sem", "Ort'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Herr, vns ein Fel\u00df vnd Hort,", "tokens": ["Herr", ",", "vns", "ein", "Fel\u00df", "vnd", "Hort", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "ART", "NN", "KON", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "Auff Den wir m\u00f6gen bawen:", "tokens": ["Auff", "Den", "wir", "m\u00f6\u00b7gen", "ba\u00b7wen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.8": {"text": "Wend' alle Noth vnd Pein,", "tokens": ["Wend'", "al\u00b7le", "Noth", "vnd", "Pein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Zieh mit vns aus vnd ein,", "tokens": ["Zieh", "mit", "vns", "aus", "vnd", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Vns, die wir Dir vertrawen.", "tokens": ["Vns", ",", "die", "wir", "Dir", "ver\u00b7tra\u00b7wen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PPER", "PPER", "VVINF", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.4": {"line.1": {"text": "Vnd, wenn wir nun den letzten Au\u00dfzug halten", "tokens": ["Vnd", ",", "wenn", "wir", "nun", "den", "letz\u00b7ten", "Au\u00df\u00b7zug", "hal\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "$,", "KOUS", "PPER", "ADV", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Aus dieser Welt, vnd durch den Todt erkalten,", "tokens": ["Aus", "die\u00b7ser", "Welt", ",", "vnd", "durch", "den", "Todt", "er\u00b7kal\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "KON", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Hilff vns getrost des Leibes H\u00fctte r\u00e4umen,", "tokens": ["Hilff", "vns", "ge\u00b7trost", "des", "Lei\u00b7bes", "H\u00fct\u00b7te", "r\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVFIN", "ART", "NN", "NN", "VVINF", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Da\u00df wir vns nit aus schrecken selbst vers\u00e4umen,", "tokens": ["Da\u00df", "wir", "vns", "nit", "aus", "schre\u00b7cken", "selbst", "ver\u00b7s\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PTKNEG", "APPR", "VVFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Brich ab die\u00df Erden-hau\u00df,", "tokens": ["Brich", "ab", "die\u00df", "Er\u00b7den\u00b7hau\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "PDS", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Vnd f\u00fchr die Seel' heraus,", "tokens": ["Vnd", "f\u00fchr", "die", "Seel'", "he\u00b7raus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Entrei\u00df sie dem Get\u00fcmmel,", "tokens": ["Ent\u00b7rei\u00df", "sie", "dem", "Ge\u00b7t\u00fcm\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Bring sie zu wahrer Ruh,", "tokens": ["Bring", "sie", "zu", "wah\u00b7rer", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Vnd stell jhr wieder zu", "tokens": ["Vnd", "stell", "jhr", "wie\u00b7der", "zu"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "PPER", "ADV", "PTKZU"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Ihr Vaterland, den Himmel.", "tokens": ["Ihr", "Va\u00b7ter\u00b7land", ",", "den", "Him\u00b7mel", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Die\u00df Pilger-Land l\u00e4sst keinen ruhig bleiben,", "tokens": ["Die\u00df", "Pil\u00b7ger\u00b7Land", "l\u00e4sst", "kei\u00b7nen", "ru\u00b7hig", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "VVFIN", "PIAT", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wir m\u00fcssen stets vmbher vns lassen treiben,", "tokens": ["Wir", "m\u00fcs\u00b7sen", "stets", "vm\u00b7bher", "vns", "las\u00b7sen", "trei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADJD", "PPER", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-