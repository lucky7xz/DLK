{"textgrid.poem.49297": {"metadata": {"author": {"name": "Rist, Johann", "birth": "N.A.", "death": "N.A."}, "title": "1L: Schlechtes B\u00fcchlein, wilt du reisen", "genre": "verse", "period": "N.A.", "pub_year": 1637, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schlechtes B\u00fcchlein, wilt du reisen", "tokens": ["Schlech\u00b7tes", "B\u00fcch\u00b7lein", ",", "wilt", "du", "rei\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "VMFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nunmehr in die gro\u00dfe Welt,", "tokens": ["Nun\u00b7mehr", "in", "die", "gro\u00b7\u00dfe", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Da die Sachen schlecht bestellt,", "tokens": ["Da", "die", "Sa\u00b7chen", "schlecht", "be\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo man dich mit Schmach wird speisen,", "tokens": ["Wo", "man", "dich", "mit", "Schmach", "wird", "spei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PRF", "APPR", "NN", "VAFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ja dir sauren L\u00e4sterwein", "tokens": ["Ja", "dir", "sau\u00b7ren", "L\u00e4s\u00b7ter\u00b7wein"], "token_info": ["word", "word", "word", "word"], "pos": ["PTKANT", "PPER", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Aller Oerter schenken ein?", "tokens": ["Al\u00b7ler", "O\u00b7er\u00b7ter", "schen\u00b7ken", "ein", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PTKVZ", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.7": {"text": "Wei\u00dft du nicht, der dich geschrieben,", "tokens": ["Wei\u00dft", "du", "nicht", ",", "der", "dich", "ge\u00b7schrie\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Da\u00df er hat der Kl\u00e4ffer viel,", "tokens": ["Da\u00df", "er", "hat", "der", "Kl\u00e4f\u00b7fer", "viel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VAFIN", "ART", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Welcher Gott verha\u00dftes Ziel", "tokens": ["Wel\u00b7cher", "Gott", "ver\u00b7ha\u00df\u00b7tes", "Ziel"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAT", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Dieses ist, ihn zu betr\u00fcben?", "tokens": ["Die\u00b7ses", "ist", ",", "ihn", "zu", "be\u00b7tr\u00fc\u00b7ben", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "$,", "PPER", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Bleibe, B\u00fcchlein, wo du bist,", "tokens": ["Blei\u00b7be", ",", "B\u00fcch\u00b7lein", ",", "wo", "du", "bist", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "PWAV", "PPER", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Weil dein Herr noch fr\u00f6lich ist.", "tokens": ["Weil", "dein", "Herr", "noch", "fr\u00f6\u00b7lich", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Ach, du bist ja schlecht bekleidet,", "tokens": ["Ach", ",", "du", "bist", "ja", "schlecht", "be\u00b7klei\u00b7det", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VAFIN", "ADV", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Schlecht von Worten, schlecht von Kunst;", "tokens": ["Schlecht", "von", "Wor\u00b7ten", ",", "schlecht", "von", "Kunst", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,", "ADJD", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "Gleichwol hoffest du noch Gunst,", "tokens": ["Gleich\u00b7wol", "hof\u00b7fest", "du", "noch", "Gunst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.16": {"text": "Da doch manches wird beneidet,", "tokens": ["Da", "doch", "man\u00b7ches", "wird", "be\u00b7nei\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PIS", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Das ein solcher Geist gemacht,", "tokens": ["Das", "ein", "sol\u00b7cher", "Geist", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "PIAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.18": {"text": "Der sich trefflich hoch gebracht.", "tokens": ["Der", "sich", "treff\u00b7lich", "hoch", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ADJD", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.19": {"text": "Nein, da leben ander' Helden,", "tokens": ["Nein", ",", "da", "le\u00b7ben", "an\u00b7der'", "Hel\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "VVFIN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Und der edlen Tichter mehr,", "tokens": ["Und", "der", "ed\u00b7len", "Tich\u00b7ter", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.21": {"text": "Reich von Kunst und gro\u00df von Ehr'.", "tokens": ["Reich", "von", "Kunst", "und", "gro\u00df", "von", "Ehr'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "ADJD", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.22": {"text": "Etlich' hat der Tod geri\u00dfen", "tokens": ["Et\u00b7lich'", "hat", "der", "Tod", "ge\u00b7ri\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "In ein fest verschlo\u00dfnes Grab;", "tokens": ["In", "ein", "fest", "ver\u00b7schlo\u00df\u00b7nes", "Grab", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.24": {"text": "Er nahm bei sich selber ab,", "tokens": ["Er", "nahm", "bei", "sich", "sel\u00b7ber", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PRF", "ADV", "PTKVZ", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.25": {"text": "Da\u00df ihr gar zu gro\u00dfes Wi\u00dfen", "tokens": ["Da\u00df", "ihr", "gar", "zu", "gro\u00b7\u00dfes", "Wi\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "Sie noch w\u00fcrd in dieser Zeit", "tokens": ["Sie", "noch", "w\u00fcrd", "in", "die\u00b7ser", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VAFIN", "APPR", "PDAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.27": {"text": "Sch\u00fctzen vor der Sterblichkeit.", "tokens": ["Sch\u00fct\u00b7zen", "vor", "der", "Sterb\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.28": {"text": "Muste gar zu fr\u00fch davon;", "tokens": ["Mus\u00b7te", "gar", "zu", "fr\u00fch", "da\u00b7von", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PTKA", "ADJD", "PAV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.29": {"text": "Und der Grobheit Untertreter,", "tokens": ["Und", "der", "Grob\u00b7heit", "Un\u00b7ter\u00b7tre\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.30": {"text": "Ein so junger frischer Held,", "tokens": ["Ein", "so", "jun\u00b7ger", "fri\u00b7scher", "Held", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.31": {"text": "Lie\u00df ja gar zu schnell die Welt.", "tokens": ["Lie\u00df", "ja", "gar", "zu", "schnell", "die", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "PTKA", "ADJD", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.32": {"text": "Zwar sie schlafen nach dem Leibe,", "tokens": ["Zwar", "sie", "schla\u00b7fen", "nach", "dem", "Lei\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.33": {"text": "Wachen aber nach der Kunst;", "tokens": ["Wa\u00b7chen", "a\u00b7ber", "nach", "der", "Kunst", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.34": {"text": "M\u00f6glich ist es nicht \u00fcmsunst,", "tokens": ["M\u00f6g\u00b7lich", "ist", "es", "nicht", "\u00fcm\u00b7sunst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "PTKNEG", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.35": {"text": "Was auch ich zu Zeiten schreibe:", "tokens": ["Was", "auch", "ich", "zu", "Zei\u00b7ten", "schrei\u00b7be", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.36": {"text": "Nach dem Tode lebt man noch.", "tokens": ["Nach", "dem", "To\u00b7de", "lebt", "man", "noch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PIS", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.37": {"text": "Drum, mein B\u00fcchlein, gehe doch.", "tokens": ["Drum", ",", "mein", "B\u00fcch\u00b7lein", ",", "ge\u00b7he", "doch", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "$,", "PPOSAT", "NN", "$,", "VVFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.38": {"text": "Gehe, weil du ja wilt gehen;", "tokens": ["Ge\u00b7he", ",", "weil", "du", "ja", "wilt", "ge\u00b7hen", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "PPER", "ADV", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.39": {"text": "Glaube mir, da\u00df mancher Geist,", "tokens": ["Glau\u00b7be", "mir", ",", "da\u00df", "man\u00b7cher", "Geist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "KOUS", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.40": {"text": "Der nicht alles Thorheit hei\u00dft,", "tokens": ["Der", "nicht", "al\u00b7les", "Thor\u00b7heit", "hei\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.41": {"text": "Dich mit Freuden wird ansehen,", "tokens": ["Dich", "mit", "Freu\u00b7den", "wird", "an\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "VAFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.42": {"text": "Ja dich ehren ohne List,", "tokens": ["Ja", "dich", "eh\u00b7ren", "oh\u00b7ne", "List", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.43": {"text": "Mehr auch als du w\u00fcrdig bist.", "tokens": ["Mehr", "auch", "als", "du", "w\u00fcr\u00b7dig", "bist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KOUS", "PPER", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.44": {"text": "Sage nur den Hochgelahrten,", "tokens": ["Sa\u00b7ge", "nur", "den", "Hoch\u00b7ge\u00b7lahr\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.45": {"text": "Unsern Teutschen teutsch und frei,", "tokens": ["Un\u00b7sern", "Teut\u00b7schen", "teutsch", "und", "frei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.46": {"text": "Da\u00df ich stets ihr Diener sei,", "tokens": ["Da\u00df", "ich", "stets", "ihr", "Die\u00b7ner", "sei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.47": {"text": "Der in Demut woll' abwarten", "tokens": ["Der", "in", "De\u00b7mut", "woll'", "ab\u00b7war\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "NN", "VMFIN", "VVINF"], "meter": "--+--+--", "measure": "anapaest.di.plus"}, "line.48": {"text": "Ihre Meinung und Bericht,", "tokens": ["Ih\u00b7re", "Mei\u00b7nung", "und", "Be\u00b7richt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.49": {"text": "Ob du taugest, oder nicht.", "tokens": ["Ob", "du", "tau\u00b7gest", ",", "o\u00b7der", "nicht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "KON", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.50": {"text": "Werden sie dich n\u00fctzlich nennen,", "tokens": ["Wer\u00b7den", "sie", "dich", "n\u00fctz\u00b7lich", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PRF", "ADJD", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.51": {"text": "Hast du schon erreicht dein Ziel;", "tokens": ["Hast", "du", "schon", "er\u00b7reicht", "dein", "Ziel", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "VVPP", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.52": {"text": "Halten sie das Widerspiel,", "tokens": ["Hal\u00b7ten", "sie", "das", "Wi\u00b7der\u00b7spiel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.53": {"text": "Wahrlich, Buch, so must du brennen;", "tokens": ["Wahr\u00b7lich", ",", "Buch", ",", "so", "must", "du", "bren\u00b7nen", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "NN", "$,", "ADV", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.54": {"text": "Denn der klugen Seelen Schrei'n", "tokens": ["Denn", "der", "klu\u00b7gen", "See\u00b7len", "Schrei'n"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.55": {"text": "Soll und mu\u00df dein Richter sein.", "tokens": ["Soll", "und", "mu\u00df", "dein", "Rich\u00b7ter", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "KON", "VMFIN", "PPOSAT", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.56": {"text": "Wird dich aber einer tadlen,", "tokens": ["Wird", "dich", "a\u00b7ber", "ei\u00b7ner", "tad\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.57": {"text": "Der doch selber weinig wei\u00df,", "tokens": ["Der", "doch", "sel\u00b7ber", "wei\u00b7nig", "wei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.58": {"text": "Diesem sag', er soll' mit Flei\u00df", "tokens": ["Die\u00b7sem", "sag'", ",", "er", "soll'", "mit", "Flei\u00df"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "$,", "PPER", "VMFIN", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.59": {"text": "Seine scharfe L\u00e4sternadlen", "tokens": ["Sei\u00b7ne", "schar\u00b7fe", "L\u00e4s\u00b7ter\u00b7nad\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.60": {"text": "Stecken in sein falsches Herz.", "tokens": ["Ste\u00b7cken", "in", "sein", "fal\u00b7sches", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.61": {"text": "Solches bringt dem Neider Schmerz.", "tokens": ["Sol\u00b7ches", "bringt", "dem", "Nei\u00b7der", "Schmerz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.62": {"text": "Sprich: \u00bbWas hast du mirs zu sagen?", "tokens": ["Sprich", ":", "\u00bb", "Was", "hast", "du", "mirs", "zu", "sa\u00b7gen", "?"], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$.", "$(", "PWS", "VAFIN", "PPER", "NE", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.63": {"text": "Bin ich selber doch nicht mein;", "tokens": ["Bin", "ich", "sel\u00b7ber", "doch", "nicht", "mein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "PTKNEG", "PPOSAT", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.64": {"text": "Sol ich aber strafbar sein,", "tokens": ["Sol", "ich", "a\u00b7ber", "straf\u00b7bar", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADJD", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.65": {"text": "Magst du meinen Herren fragen.", "tokens": ["Magst", "du", "mei\u00b7nen", "Her\u00b7ren", "fra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.66": {"text": "Lebt der nur, so wird er dich", "tokens": ["Lebt", "der", "nur", ",", "so", "wird", "er", "dich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADV", "$,", "ADV", "VAFIN", "PPER", "PRF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.67": {"text": "Finden und wol sch\u00fctzen mich.\u00ab", "tokens": ["Fin\u00b7den", "und", "wol", "sch\u00fct\u00b7zen", "mich", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "KON", "ADV", "VVFIN", "PPER", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Schlechtes B\u00fcchlein, wilt du reisen", "tokens": ["Schlech\u00b7tes", "B\u00fcch\u00b7lein", ",", "wilt", "du", "rei\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "VMFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nunmehr in die gro\u00dfe Welt,", "tokens": ["Nun\u00b7mehr", "in", "die", "gro\u00b7\u00dfe", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Da die Sachen schlecht bestellt,", "tokens": ["Da", "die", "Sa\u00b7chen", "schlecht", "be\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo man dich mit Schmach wird speisen,", "tokens": ["Wo", "man", "dich", "mit", "Schmach", "wird", "spei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PRF", "APPR", "NN", "VAFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ja dir sauren L\u00e4sterwein", "tokens": ["Ja", "dir", "sau\u00b7ren", "L\u00e4s\u00b7ter\u00b7wein"], "token_info": ["word", "word", "word", "word"], "pos": ["PTKANT", "PPER", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Aller Oerter schenken ein?", "tokens": ["Al\u00b7ler", "O\u00b7er\u00b7ter", "schen\u00b7ken", "ein", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PTKVZ", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.7": {"text": "Wei\u00dft du nicht, der dich geschrieben,", "tokens": ["Wei\u00dft", "du", "nicht", ",", "der", "dich", "ge\u00b7schrie\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Da\u00df er hat der Kl\u00e4ffer viel,", "tokens": ["Da\u00df", "er", "hat", "der", "Kl\u00e4f\u00b7fer", "viel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VAFIN", "ART", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Welcher Gott verha\u00dftes Ziel", "tokens": ["Wel\u00b7cher", "Gott", "ver\u00b7ha\u00df\u00b7tes", "Ziel"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAT", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Dieses ist, ihn zu betr\u00fcben?", "tokens": ["Die\u00b7ses", "ist", ",", "ihn", "zu", "be\u00b7tr\u00fc\u00b7ben", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "$,", "PPER", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Bleibe, B\u00fcchlein, wo du bist,", "tokens": ["Blei\u00b7be", ",", "B\u00fcch\u00b7lein", ",", "wo", "du", "bist", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "PWAV", "PPER", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Weil dein Herr noch fr\u00f6lich ist.", "tokens": ["Weil", "dein", "Herr", "noch", "fr\u00f6\u00b7lich", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Ach, du bist ja schlecht bekleidet,", "tokens": ["Ach", ",", "du", "bist", "ja", "schlecht", "be\u00b7klei\u00b7det", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VAFIN", "ADV", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Schlecht von Worten, schlecht von Kunst;", "tokens": ["Schlecht", "von", "Wor\u00b7ten", ",", "schlecht", "von", "Kunst", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,", "ADJD", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "Gleichwol hoffest du noch Gunst,", "tokens": ["Gleich\u00b7wol", "hof\u00b7fest", "du", "noch", "Gunst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.16": {"text": "Da doch manches wird beneidet,", "tokens": ["Da", "doch", "man\u00b7ches", "wird", "be\u00b7nei\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PIS", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Das ein solcher Geist gemacht,", "tokens": ["Das", "ein", "sol\u00b7cher", "Geist", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "PIAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.18": {"text": "Der sich trefflich hoch gebracht.", "tokens": ["Der", "sich", "treff\u00b7lich", "hoch", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ADJD", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.19": {"text": "Nein, da leben ander' Helden,", "tokens": ["Nein", ",", "da", "le\u00b7ben", "an\u00b7der'", "Hel\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "VVFIN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Und der edlen Tichter mehr,", "tokens": ["Und", "der", "ed\u00b7len", "Tich\u00b7ter", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.21": {"text": "Reich von Kunst und gro\u00df von Ehr'.", "tokens": ["Reich", "von", "Kunst", "und", "gro\u00df", "von", "Ehr'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "ADJD", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.22": {"text": "Etlich' hat der Tod geri\u00dfen", "tokens": ["Et\u00b7lich'", "hat", "der", "Tod", "ge\u00b7ri\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "In ein fest verschlo\u00dfnes Grab;", "tokens": ["In", "ein", "fest", "ver\u00b7schlo\u00df\u00b7nes", "Grab", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.24": {"text": "Er nahm bei sich selber ab,", "tokens": ["Er", "nahm", "bei", "sich", "sel\u00b7ber", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PRF", "ADV", "PTKVZ", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.25": {"text": "Da\u00df ihr gar zu gro\u00dfes Wi\u00dfen", "tokens": ["Da\u00df", "ihr", "gar", "zu", "gro\u00b7\u00dfes", "Wi\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "Sie noch w\u00fcrd in dieser Zeit", "tokens": ["Sie", "noch", "w\u00fcrd", "in", "die\u00b7ser", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VAFIN", "APPR", "PDAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.27": {"text": "Sch\u00fctzen vor der Sterblichkeit.", "tokens": ["Sch\u00fct\u00b7zen", "vor", "der", "Sterb\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.28": {"text": "Muste gar zu fr\u00fch davon;", "tokens": ["Mus\u00b7te", "gar", "zu", "fr\u00fch", "da\u00b7von", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PTKA", "ADJD", "PAV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.29": {"text": "Und der Grobheit Untertreter,", "tokens": ["Und", "der", "Grob\u00b7heit", "Un\u00b7ter\u00b7tre\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.30": {"text": "Ein so junger frischer Held,", "tokens": ["Ein", "so", "jun\u00b7ger", "fri\u00b7scher", "Held", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.31": {"text": "Lie\u00df ja gar zu schnell die Welt.", "tokens": ["Lie\u00df", "ja", "gar", "zu", "schnell", "die", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "PTKA", "ADJD", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.32": {"text": "Zwar sie schlafen nach dem Leibe,", "tokens": ["Zwar", "sie", "schla\u00b7fen", "nach", "dem", "Lei\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.33": {"text": "Wachen aber nach der Kunst;", "tokens": ["Wa\u00b7chen", "a\u00b7ber", "nach", "der", "Kunst", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.34": {"text": "M\u00f6glich ist es nicht \u00fcmsunst,", "tokens": ["M\u00f6g\u00b7lich", "ist", "es", "nicht", "\u00fcm\u00b7sunst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "PTKNEG", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.35": {"text": "Was auch ich zu Zeiten schreibe:", "tokens": ["Was", "auch", "ich", "zu", "Zei\u00b7ten", "schrei\u00b7be", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.36": {"text": "Nach dem Tode lebt man noch.", "tokens": ["Nach", "dem", "To\u00b7de", "lebt", "man", "noch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PIS", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.37": {"text": "Drum, mein B\u00fcchlein, gehe doch.", "tokens": ["Drum", ",", "mein", "B\u00fcch\u00b7lein", ",", "ge\u00b7he", "doch", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "$,", "PPOSAT", "NN", "$,", "VVFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.38": {"text": "Gehe, weil du ja wilt gehen;", "tokens": ["Ge\u00b7he", ",", "weil", "du", "ja", "wilt", "ge\u00b7hen", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "PPER", "ADV", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.39": {"text": "Glaube mir, da\u00df mancher Geist,", "tokens": ["Glau\u00b7be", "mir", ",", "da\u00df", "man\u00b7cher", "Geist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "KOUS", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.40": {"text": "Der nicht alles Thorheit hei\u00dft,", "tokens": ["Der", "nicht", "al\u00b7les", "Thor\u00b7heit", "hei\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.41": {"text": "Dich mit Freuden wird ansehen,", "tokens": ["Dich", "mit", "Freu\u00b7den", "wird", "an\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "VAFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.42": {"text": "Ja dich ehren ohne List,", "tokens": ["Ja", "dich", "eh\u00b7ren", "oh\u00b7ne", "List", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.43": {"text": "Mehr auch als du w\u00fcrdig bist.", "tokens": ["Mehr", "auch", "als", "du", "w\u00fcr\u00b7dig", "bist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KOUS", "PPER", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.44": {"text": "Sage nur den Hochgelahrten,", "tokens": ["Sa\u00b7ge", "nur", "den", "Hoch\u00b7ge\u00b7lahr\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.45": {"text": "Unsern Teutschen teutsch und frei,", "tokens": ["Un\u00b7sern", "Teut\u00b7schen", "teutsch", "und", "frei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.46": {"text": "Da\u00df ich stets ihr Diener sei,", "tokens": ["Da\u00df", "ich", "stets", "ihr", "Die\u00b7ner", "sei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.47": {"text": "Der in Demut woll' abwarten", "tokens": ["Der", "in", "De\u00b7mut", "woll'", "ab\u00b7war\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "NN", "VMFIN", "VVINF"], "meter": "--+--+--", "measure": "anapaest.di.plus"}, "line.48": {"text": "Ihre Meinung und Bericht,", "tokens": ["Ih\u00b7re", "Mei\u00b7nung", "und", "Be\u00b7richt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.49": {"text": "Ob du taugest, oder nicht.", "tokens": ["Ob", "du", "tau\u00b7gest", ",", "o\u00b7der", "nicht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "KON", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.50": {"text": "Werden sie dich n\u00fctzlich nennen,", "tokens": ["Wer\u00b7den", "sie", "dich", "n\u00fctz\u00b7lich", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PRF", "ADJD", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.51": {"text": "Hast du schon erreicht dein Ziel;", "tokens": ["Hast", "du", "schon", "er\u00b7reicht", "dein", "Ziel", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "VVPP", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.52": {"text": "Halten sie das Widerspiel,", "tokens": ["Hal\u00b7ten", "sie", "das", "Wi\u00b7der\u00b7spiel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.53": {"text": "Wahrlich, Buch, so must du brennen;", "tokens": ["Wahr\u00b7lich", ",", "Buch", ",", "so", "must", "du", "bren\u00b7nen", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "NN", "$,", "ADV", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.54": {"text": "Denn der klugen Seelen Schrei'n", "tokens": ["Denn", "der", "klu\u00b7gen", "See\u00b7len", "Schrei'n"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.55": {"text": "Soll und mu\u00df dein Richter sein.", "tokens": ["Soll", "und", "mu\u00df", "dein", "Rich\u00b7ter", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "KON", "VMFIN", "PPOSAT", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.56": {"text": "Wird dich aber einer tadlen,", "tokens": ["Wird", "dich", "a\u00b7ber", "ei\u00b7ner", "tad\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.57": {"text": "Der doch selber weinig wei\u00df,", "tokens": ["Der", "doch", "sel\u00b7ber", "wei\u00b7nig", "wei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.58": {"text": "Diesem sag', er soll' mit Flei\u00df", "tokens": ["Die\u00b7sem", "sag'", ",", "er", "soll'", "mit", "Flei\u00df"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "$,", "PPER", "VMFIN", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.59": {"text": "Seine scharfe L\u00e4sternadlen", "tokens": ["Sei\u00b7ne", "schar\u00b7fe", "L\u00e4s\u00b7ter\u00b7nad\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.60": {"text": "Stecken in sein falsches Herz.", "tokens": ["Ste\u00b7cken", "in", "sein", "fal\u00b7sches", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.61": {"text": "Solches bringt dem Neider Schmerz.", "tokens": ["Sol\u00b7ches", "bringt", "dem", "Nei\u00b7der", "Schmerz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.62": {"text": "Sprich: \u00bbWas hast du mirs zu sagen?", "tokens": ["Sprich", ":", "\u00bb", "Was", "hast", "du", "mirs", "zu", "sa\u00b7gen", "?"], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$.", "$(", "PWS", "VAFIN", "PPER", "NE", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.63": {"text": "Bin ich selber doch nicht mein;", "tokens": ["Bin", "ich", "sel\u00b7ber", "doch", "nicht", "mein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "PTKNEG", "PPOSAT", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.64": {"text": "Sol ich aber strafbar sein,", "tokens": ["Sol", "ich", "a\u00b7ber", "straf\u00b7bar", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADJD", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.65": {"text": "Magst du meinen Herren fragen.", "tokens": ["Magst", "du", "mei\u00b7nen", "Her\u00b7ren", "fra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.66": {"text": "Lebt der nur, so wird er dich", "tokens": ["Lebt", "der", "nur", ",", "so", "wird", "er", "dich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADV", "$,", "ADV", "VAFIN", "PPER", "PRF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.67": {"text": "Finden und wol sch\u00fctzen mich.\u00ab", "tokens": ["Fin\u00b7den", "und", "wol", "sch\u00fct\u00b7zen", "mich", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "KON", "ADV", "VVFIN", "PPER", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}