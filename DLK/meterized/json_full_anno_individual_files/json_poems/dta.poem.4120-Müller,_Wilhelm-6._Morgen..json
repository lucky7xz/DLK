{"dta.poem.4120": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "6.  Morgen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1821", "urn": "urn:nbn:de:kobv:b4-200905194209", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Zieh' ich mit dem Morgenschein,", "tokens": ["Zieh'", "ich", "mit", "dem", "Mor\u00b7gen\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Abendlust und Abendleid", "tokens": ["A\u00b7bend\u00b7lust", "und", "A\u00b7ben\u00b7dleid"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hinter mir so weit, so weit!", "tokens": ["Hin\u00b7ter", "mir", "so", "weit", ",", "so", "weit", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "ADJD", "$,", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ei, wie roth deine Wangen sind,", "tokens": ["Ei", ",", "wie", "roth", "dei\u00b7ne", "Wan\u00b7gen", "sind", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ADJD", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Morgen, Morgen, s\u00fc\u00dfes Kind!", "tokens": ["Mor\u00b7gen", ",", "Mor\u00b7gen", ",", "s\u00fc\u00b7\u00dfes", "Kind", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bl\u00fcmlein weinten die ganze Nacht,", "tokens": ["Bl\u00fcm\u00b7lein", "wein\u00b7ten", "die", "gan\u00b7ze", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Weil man dich zu Bett gebracht;", "tokens": ["Weil", "man", "dich", "zu", "Bett", "ge\u00b7bracht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PRF", "APPR", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Mittag kam, der stolze Ritter,", "tokens": ["Mit\u00b7tag", "kam", ",", "der", "stol\u00b7ze", "Rit\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Abend kam, der m\u00fcde Schnitter,", "tokens": ["A\u00b7bend", "kam", ",", "der", "m\u00fc\u00b7de", "Schnit\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Keinen haben sie angeschaut,", "tokens": ["Kei\u00b7nen", "ha\u00b7ben", "sie", "an\u00b7ge\u00b7schaut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.8": {"text": "Haben still auf dich vertraut.", "tokens": ["Ha\u00b7ben", "still", "auf", "dich", "ver\u00b7traut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "APPR", "PPER", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Und nun bist du wieder da,", "tokens": ["Und", "nun", "bist", "du", "wie\u00b7der", "da", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ADV", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Bist so freundlich, bist so nah!", "tokens": ["Bist", "so", "freund\u00b7lich", ",", "bist", "so", "nah", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "$,", "VAFIN", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Und sie richten sich empor,", "tokens": ["Und", "sie", "rich\u00b7ten", "sich", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PRF", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Sch\u00fctteln ab der Tr\u00e4ume Flor:", "tokens": ["Sch\u00fct\u00b7teln", "ab", "der", "Tr\u00e4u\u00b7me", "Flor", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Wie sie wanken, wie sie beben,", "tokens": ["Wie", "sie", "wan\u00b7ken", ",", "wie", "sie", "be\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVINF", "$,", "PWAV", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Scheu die trunknen Blicke heben!", "tokens": ["Scheu", "die", "trunk\u00b7nen", "Bli\u00b7cke", "he\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "War's dein Ku\u00df, der sie erweckte?", "tokens": ["Wa\u00b7r's", "dein", "Ku\u00df", ",", "der", "sie", "er\u00b7weck\u00b7te", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "War's ein Zephyr, der sie neckte?", "tokens": ["Wa\u00b7r's", "ein", "Ze\u00b7phyr", ",", "der", "sie", "neck\u00b7te", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.17": {"text": "Welcher Schrecken, welche Lust,", "tokens": ["Wel\u00b7cher", "Schre\u00b7cken", ",", "wel\u00b7che", "Lust", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PWAT", "NN", "$,", "PWAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.18": {"text": "Mund an Mund, und Brust an Brust!", "tokens": ["Mund", "an", "Mund", ",", "und", "Brust", "an", "Brust", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,", "KON", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Guten Morgen, guten Morgen!", "tokens": ["Gu\u00b7ten", "Mor\u00b7gen", ",", "gu\u00b7ten", "Mor\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In die Winde alle Sorgen,", "tokens": ["In", "die", "Win\u00b7de", "al\u00b7le", "Sor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Alle Thr\u00e4nen von den Wangen,", "tokens": ["Al\u00b7le", "Thr\u00e4\u00b7nen", "von", "den", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aus dem Herzen alles Bangen,", "tokens": ["Aus", "dem", "Her\u00b7zen", "al\u00b7les", "Ban\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Alles froh und Alles frei,", "tokens": ["Al\u00b7les", "froh", "und", "Al\u00b7les", "frei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADJD", "KON", "PIS", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ob's der erste Welttag sei!", "tokens": ["Ob's", "der", "ers\u00b7te", "Welt\u00b7tag", "sei", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Auch die kleinen Waldv\u00f6gelein", "tokens": ["Auch", "die", "klei\u00b7nen", "Wald\u00b7v\u00f6\u00b7ge\u00b7lein"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Wollen bei dem Feste sein,", "tokens": ["Wol\u00b7len", "bei", "dem", "Fes\u00b7te", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Lassen ihre Stimmlein klingen,", "tokens": ["Las\u00b7sen", "ih\u00b7re", "Stimm\u00b7lein", "klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Einen Gru\u00df hinaufzusingen.", "tokens": ["Ei\u00b7nen", "Gru\u00df", "hin\u00b7auf\u00b7zu\u00b7sin\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wi\u00dft ihr, wer's am besten meint", "tokens": ["Wi\u00dft", "ihr", ",", "wer's", "am", "bes\u00b7ten", "meint"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "VAFIN", "PTKA", "ADJD", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Mit dem jungen Himmelsfreund?", "tokens": ["Mit", "dem", "jun\u00b7gen", "Him\u00b7mels\u00b7freund", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Lerche sich zum H\u00f6chsten schwingt,", "tokens": ["Ler\u00b7che", "sich", "zum", "H\u00f6chs\u00b7ten", "schwingt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Und ihm grad' an's Herze sinkt.", "tokens": ["Und", "ihm", "grad'", "an's", "Her\u00b7ze", "sinkt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Lerche, Lerche, einen Gru\u00df,", "tokens": ["Ler\u00b7che", ",", "Ler\u00b7che", ",", "ei\u00b7nen", "Gru\u00df", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Lerche, Lerche, Gru\u00df und Ku\u00df,", "tokens": ["Ler\u00b7che", ",", "Ler\u00b7che", ",", "Gru\u00df", "und", "Ku\u00df", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Nimm sie mit dir von uns Allen,", "tokens": ["Nimm", "sie", "mit", "dir", "von", "uns", "Al\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "PPER", "APPR", "PPER", "NE", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.12": {"text": "Und la\u00df deine Stimme schallen,", "tokens": ["Und", "la\u00df", "dei\u00b7ne", "Stim\u00b7me", "schal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.13": {"text": "Wenn wir dich nicht mehr ersehn,", "tokens": ["Wenn", "wir", "dich", "nicht", "mehr", "er\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "Aus den lieben blauen H\u00f6h'n!", "tokens": ["Aus", "den", "lie\u00b7ben", "blau\u00b7en", "H\u00f6h'n", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Fischlein, Fischlein in dem See,", "tokens": ["Fisc\u00b7hlein", ",", "Fisc\u00b7hlein", "in", "dem", "See", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wird's da unten euch zu weh?", "tokens": ["Wird's", "da", "un\u00b7ten", "euch", "zu", "weh", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "PPER", "PTKZU", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Drang sein helles Rosenlicht", "tokens": ["Drang", "sein", "hel\u00b7les", "Ro\u00b7sen\u00b7licht"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Noch in eure Tiefe nicht?", "tokens": ["Noch", "in", "eu\u00b7re", "Tie\u00b7fe", "nicht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ei, so springt einmal heraus", "tokens": ["Ei", ",", "so", "springt", "ein\u00b7mal", "he\u00b7raus"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "ADV", "VVFIN", "ADV", "PTKVZ"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Aus dem d\u00fcstern Wogenhaus,", "tokens": ["Aus", "dem", "d\u00fcs\u00b7tern", "Wo\u00b7gen\u00b7haus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.7": {"text": "Schnappt von seinen Aeugelein", "tokens": ["Schnappt", "von", "sei\u00b7nen", "A\u00b7e\u00b7u\u00b7ge\u00b7lein"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Einen Blick zu euch hinein,", "tokens": ["Ei\u00b7nen", "Blick", "zu", "euch", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Und die Lampen von Krystall", "tokens": ["Und", "die", "Lam\u00b7pen", "von", "Krys\u00b7tall"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "NN"], "meter": "--+--++", "measure": "anapaest.di.plus"}, "line.10": {"text": "Z\u00fcndet an mit seinem Strahl!", "tokens": ["Z\u00fcn\u00b7det", "an", "mit", "sei\u00b7nem", "Strahl", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Morgenstund hat Gold im Mund!", "tokens": ["Mor\u00b7gen\u00b7stund", "hat", "Gold", "im", "Mund", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Arme Wandrer, rings und rund,", "tokens": ["Ar\u00b7me", "Wand\u00b7rer", ",", "rings", "und", "rund", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADV", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Auf und fort im Morgenschein,", "tokens": ["Auf", "und", "fort", "im", "Mor\u00b7gen\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KON", "PTKVZ", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wollt ihr reiche Leute sein!", "tokens": ["Wollt", "ihr", "rei\u00b7che", "Leu\u00b7te", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "ADJA", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}