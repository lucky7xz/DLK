{"textgrid.poem.67530": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ihm, der zehntausend Sonnenheere", "genre": "verse", "period": "N.A.", "pub_year": 1765, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihm, der zehntausend Sonnenheere", "tokens": ["Ihm", ",", "der", "zehn\u00b7tau\u00b7send", "Son\u00b7nen\u00b7hee\u00b7re"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PPER", "$,", "PRELS", "CARD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Im Strahlenangesicht als Br\u00e4ute schuf,", "tokens": ["Im", "Strah\u00b7len\u00b7an\u00b7ge\u00b7sicht", "als", "Br\u00e4u\u00b7te", "schuf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KOUS", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dem jedes Jahr erklingt und jede Erde", "tokens": ["Dem", "je\u00b7des", "Jahr", "er\u00b7klingt", "und", "je\u00b7de", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "VVFIN", "KON", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "H\u00fcpft wie ein Elephant,", "tokens": ["H\u00fcpft", "wie", "ein", "E\u00b7le\u00b7phant", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "Dem tausend Fr\u00fchlingsch\u00f6re scherzen,", "tokens": ["Dem", "tau\u00b7send", "Fr\u00fch\u00b7ling\u00b7sch\u00f6\u00b7re", "scher\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "CARD", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Schnitter singen und das Waldheer br\u00fcllt:", "tokens": ["Die", "Schnit\u00b7ter", "sin\u00b7gen", "und", "das", "Wald\u00b7heer", "br\u00fcllt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "KON", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dem jauchz', o Leyer, himmelhohe Lieder,", "tokens": ["Dem", "jauch\u00b7z'", ",", "o", "Le\u00b7yer", ",", "him\u00b7mel\u00b7ho\u00b7he", "Lie\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "FM", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-++-+-+-+-+-", "measure": "unknown.measure.hexa"}, "line.4": {"text": "Stolz, da\u00df Dich Jovah h\u00f6rt!", "tokens": ["Stolz", ",", "da\u00df", "Dich", "Jo\u00b7vah", "h\u00f6rt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "NE", "VVFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.3": {"line.1": {"text": "Denn wenn die Morgensterne jauchzen", "tokens": ["Denn", "wenn", "die", "Mor\u00b7gens\u00b7ter\u00b7ne", "jauch\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und Erden h\u00fcpfen und die Jahrszeit singt,", "tokens": ["Und", "Er\u00b7den", "h\u00fcp\u00b7fen", "und", "die", "Jahrs\u00b7zeit", "singt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVINF", "KON", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "H\u00f6rt er im festlichen Concert der Sph\u00e4ren", "tokens": ["H\u00f6rt", "er", "im", "fest\u00b7li\u00b7chen", "Con\u00b7cert", "der", "Sph\u00e4\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPRART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Noch gern Dein wimmernd Lied.", "tokens": ["Noch", "gern", "Dein", "wim\u00b7mernd", "Lied", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPOSAT", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Drum t\u00f6n' am neuen goldnen Jahre", "tokens": ["Drum", "t\u00f6n'", "am", "neu\u00b7en", "gold\u00b7nen", "Jah\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "APPRART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von goldnen Saiten ihm ein neues Lied!", "tokens": ["Von", "gold\u00b7nen", "Sai\u00b7ten", "ihm", "ein", "neu\u00b7es", "Lied", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Er gab dem Jahr das Allmachtshorn der F\u00fclle", "tokens": ["Er", "gab", "dem", "Jahr", "das", "All\u00b7machts\u00b7horn", "der", "F\u00fcl\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und Balsam seinem Fu\u00df.", "tokens": ["Und", "Bal\u00b7sam", "sei\u00b7nem", "Fu\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Der hier ein Kriegsvolk satter Aehren", "tokens": ["Der", "hier", "ein", "Kriegs\u00b7volk", "sat\u00b7ter", "A\u00b7eh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und dort ein Blumenheer wie Jungfrau'n spro\u00dft,", "tokens": ["Und", "dort", "ein", "Blu\u00b7men\u00b7heer", "wie", "Jung\u00b7frau'n", "spro\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "KOKOM", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df Balsamwolken wandelten zum Himmel,", "tokens": ["Da\u00df", "Bal\u00b7sam\u00b7wol\u00b7ken", "wan\u00b7del\u00b7ten", "zum", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein Festgeruch dem Herrn:", "tokens": ["Ein", "Fest\u00b7ge\u00b7ruch", "dem", "Herrn", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Er kr\u00f6nte unser Jahr mit Palmen,", "tokens": ["Er", "kr\u00f6n\u00b7te", "un\u00b7ser", "Jahr", "mit", "Pal\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df Segensstr\u00f6me niederthaueten,", "tokens": ["Da\u00df", "Se\u00b7gens\u00b7str\u00f6\u00b7me", "nie\u00b7dert\u00b7hau\u00b7e\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Er schlo\u00df die Stadt zum Fels, hob unsre H\u00e4user", "tokens": ["Er", "schlo\u00df", "die", "Stadt", "zum", "Fels", ",", "hob", "uns\u00b7re", "H\u00e4u\u00b7ser"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "$,", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Hoch zu Pal\u00e4sten auf,", "tokens": ["Hoch", "zu", "Pa\u00b7l\u00e4s\u00b7ten", "auf", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "PTKVZ", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.7": {"line.1": {"text": "Umlagert' uns, statt Kriegesheeren,", "tokens": ["Um\u00b7la\u00b7gert'", "uns", ",", "statt", "Krie\u00b7ges\u00b7hee\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUI", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Schiffen \u2013 ja, rings um uns ward", "tokens": ["Mit", "Schif\u00b7fen", "\u2013", "ja", ",", "rings", "um", "uns", "ward"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "$(", "PTKANT", "$,", "ADV", "APPR", "PPER", "VAFIN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Die Flur ein Paradies, da die Monarchin", "tokens": ["Die", "Flur", "ein", "Pa\u00b7ra\u00b7dies", ",", "da", "die", "Mon\u00b7ar\u00b7chin"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Als G\u00f6ttin zu uns kam.", "tokens": ["Als", "G\u00f6t\u00b7tin", "zu", "uns", "kam", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Heil uns, wir sahn Sie, deren Adler,", "tokens": ["Heil", "uns", ",", "wir", "sahn", "Sie", ",", "de\u00b7ren", "Ad\u00b7ler", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "PPER", "VVFIN", "PPER", "$,", "PRELAT", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "So wie Aurorens goldne Fl\u00fcgel, Ruh", "tokens": ["So", "wie", "Au\u00b7ro\u00b7rens", "gold\u00b7ne", "Fl\u00fc\u00b7gel", ",", "Ruh"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "KOKOM", "NE", "ADJA", "NN", "$,", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Auf uns herabgie\u00dft \u2013 sahn Sie, deren Scepter", "tokens": ["Auf", "uns", "her\u00b7ab\u00b7gie\u00dft", "\u2013", "sahn", "Sie", ",", "de\u00b7ren", "Scep\u00b7ter"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPER", "VVFIN", "$(", "VVFIN", "PPER", "$,", "PRELAT", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Mit Weisheit Riga h\u00e4lt.", "tokens": ["Mit", "Weis\u00b7heit", "Ri\u00b7ga", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NE", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Drum jauchze, Land, dem Kronengeber,", "tokens": ["Drum", "jauch\u00b7ze", ",", "Land", ",", "dem", "Kro\u00b7nen\u00b7ge\u00b7ber", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "$,", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df er Sie Dir geschenkt, da\u00df Du Sie sahst!", "tokens": ["Da\u00df", "er", "Sie", "Dir", "ge\u00b7schenkt", ",", "da\u00df", "Du", "Sie", "sahst", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PPER", "VVPP", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sing, Landmann, wenn Du m\u00e4hst, Ihr Erntelieder,", "tokens": ["Sing", ",", "Land\u00b7mann", ",", "wenn", "Du", "m\u00e4hst", ",", "Ihr", "Ern\u00b7te\u00b7lie\u00b7der", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "KOUS", "PPER", "VVFIN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wo Sie als Ceres fuhr!", "tokens": ["Wo", "Sie", "als", "Ce\u00b7res", "fuhr", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "KOUS", "NE", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "\u00bbheil uns, wir streueten Ihr Kr\u00e4nze!\u00ab", "tokens": ["\u00bb", "heil", "uns", ",", "wir", "streu\u00b7e\u00b7ten", "Ihr", "Kr\u00e4n\u00b7ze", "!", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "KOUS", "PPER", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "$.", "$("], "meter": "+--+---+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "So singt, Jungfrauen, einst zum Hochzeitsreihn!", "tokens": ["So", "singt", ",", "Jung\u00b7frau\u00b7en", ",", "einst", "zum", "Hoch\u00b7zeits\u00b7reihn", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "NN", "$,", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und Euer Br\u00e4ut'gam sing': \u00bbVor Ihr, der Sonne,", "tokens": ["Und", "Eu\u00b7er", "Br\u00e4ut'\u00b7gam", "sing'", ":", "\u00bb", "Vor", "Ihr", ",", "der", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PTKVZ", "$.", "$(", "APPR", "PPER", "$,", "ART", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Bl\u00fcht' ich zum Manne auf!\u00ab", "tokens": ["Bl\u00fcht'", "ich", "zum", "Man\u00b7ne", "auf", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "PTKVZ", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "\u00bbzum Mann auch ich!\u00ab so h\u00fcpft der J\u00fcngling.", "tokens": ["\u00bb", "zum", "Mann", "auch", "ich", "!", "\u00ab", "so", "h\u00fcpft", "der", "J\u00fcng\u00b7ling", "."], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPRART", "NN", "ADV", "PPER", "$.", "$(", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbzum J\u00fcngling ich!\u00ab so lallt das Kind und dr\u00fcckt", "tokens": ["\u00bb", "zum", "J\u00fcng\u00b7ling", "ich", "!", "\u00ab", "so", "lallt", "das", "Kind", "und", "dr\u00fcckt"], "token_info": ["punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "APPRART", "NN", "PPER", "$.", "$(", "ADV", "VVFIN", "ART", "NN", "KON", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Mutter Brust. Die jauchzt; der Ungeborne", "tokens": ["Der", "Mut\u00b7ter", "Brust", ".", "Die", "jauchzt", ";", "der", "Un\u00b7ge\u00b7bor\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "NN", "$.", "ART", "VVFIN", "$.", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "H\u00fcpft froh in ihrem Schoo\u00df.", "tokens": ["H\u00fcpft", "froh", "in", "ih\u00b7rem", "Schoo\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Und sterbend hebt der Greis die H\u00e4nde", "tokens": ["Und", "ster\u00b7bend", "hebt", "der", "Greis", "die", "H\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und segnet Sie, zum letzten neuen Jahr:", "tokens": ["Und", "seg\u00b7net", "Sie", ",", "zum", "letz\u00b7ten", "neu\u00b7en", "Jahr", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "APPRART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "\u00bbseht lange, lange Sie, mein Sohn und Enkel!", "tokens": ["\u00bb", "seht", "lan\u00b7ge", ",", "lan\u00b7ge", "Sie", ",", "mein", "Sohn", "und", "En\u00b7kel", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "ADV", "$,", "ADV", "PPER", "$,", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ich aber geh' heut hin", "tokens": ["Ich", "a\u00b7ber", "geh'", "heut", "hin"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "ADV", "PTKVZ"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Zum Friedensheer des vor'gen Jahres", "tokens": ["Zum", "Frie\u00b7dens\u00b7heer", "des", "vor'\u00b7gen", "Jah\u00b7res"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und k\u00fcsse, Freude weinend, noch Ihr Bild!", "tokens": ["Und", "k\u00fcs\u00b7se", ",", "Freu\u00b7de", "wei\u00b7nend", ",", "noch", "Ihr", "Bild", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN"