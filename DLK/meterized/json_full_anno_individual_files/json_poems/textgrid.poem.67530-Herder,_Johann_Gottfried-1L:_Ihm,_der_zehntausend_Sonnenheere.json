word", "word", "punct"], "pos": ["PAV", "VVFIN", "$,", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df er Sie Dir geschenkt, da\u00df Du Sie sahst!", "tokens": ["Da\u00df", "er", "Sie", "Dir", "ge\u00b7schenkt", ",", "da\u00df", "Du", "Sie", "sahst", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PPER", "VVPP", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sing, Landmann, wenn Du m\u00e4hst, Ihr Erntelieder,", "tokens": ["Sing", ",", "Land\u00b7mann", ",", "wenn", "Du", "m\u00e4hst", ",", "Ihr", "Ern\u00b7te\u00b7lie\u00b7der", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "KOUS", "PPER", "VVFIN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wo Sie als Ceres fuhr!", "tokens": ["Wo", "Sie", "als", "Ce\u00b7res", "fuhr", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "KOUS", "NE", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.26": {"line.1": {"text": "\u00bbheil uns, wir streueten Ihr Kr\u00e4nze!\u00ab", "tokens": ["\u00bb", "heil", "uns", ",", "wir", "streu\u00b7e\u00b7ten", "Ihr", "Kr\u00e4n\u00b7ze", "!", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "KOUS", "PPER", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "$.", "$("], "meter": "+--+---+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "So singt, Jungfrauen, einst zum Hochzeitsreihn!", "tokens": ["So", "singt", ",", "Jung\u00b7frau\u00b7en", ",", "einst", "zum", "Hoch\u00b7zeits\u00b7reihn", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punc