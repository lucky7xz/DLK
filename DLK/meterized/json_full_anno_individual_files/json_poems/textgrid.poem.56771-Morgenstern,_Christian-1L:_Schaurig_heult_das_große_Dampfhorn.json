{"textgrid.poem.56771": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Schaurig heult das gro\u00dfe Dampfhorn", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schaurig heult das gro\u00dfe Dampfhorn", "tokens": ["Schau\u00b7rig", "heult", "das", "gro\u00b7\u00dfe", "Dampf\u00b7horn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "seine Warnung in den Nebel ...", "tokens": ["sei\u00b7ne", "War\u00b7nung", "in", "den", "Ne\u00b7bel", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Irgendwo antwortet schaurig,", "tokens": ["Ir\u00b7gend\u00b7wo", "ant\u00b7wor\u00b7tet", "schau\u00b7rig", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "leis bald, lauter bald, ein andres ...", "tokens": ["leis", "bald", ",", "lau\u00b7ter", "bald", ",", "ein", "and\u00b7res", "..."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "ADV", "$,", "ADJD", "ADV", "$,", "ART", "PIS", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Angstvoll stehn die Passagiere,", "tokens": ["Angst\u00b7voll", "stehn", "die", "Pas\u00b7sa\u00b7gie\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "jeden Nerv gespannt die Mannschaft ...", "tokens": ["je\u00b7den", "Nerv", "ge\u00b7spannt", "die", "Mann\u00b7schaft", ".