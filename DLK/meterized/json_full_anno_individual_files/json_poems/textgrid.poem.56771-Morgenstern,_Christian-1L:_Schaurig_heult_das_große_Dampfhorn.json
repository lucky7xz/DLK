word", "word", "punct"], "pos": ["NN", "ART", "NN", "PRF", "ADV", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Schaurig heult das gro\u00dfe Dampfhorn ...", "tokens": ["Schau\u00b7rig", "heult", "das", "gro\u00b7\u00dfe", "Dampf\u00b7horn", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Dumpf antwortet's aus dem Nebel ...", "tokens": ["Dumpf", "ant\u00b7wor\u00b7tet's", "aus", "dem", "Ne\u00b7bel", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.15": {"text": "In den Schiffsraum steigen Wachen,", "tokens": ["In", "den", "Schiffs\u00b7raum", "stei\u00b7gen", "Wa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "an den Luken, an den Booten", "tokens": ["an", "den", "Lu\u00b7ken", ",", "an", "den", "Boo\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "harrt Bemannung, von der Br\u00fccke", "tokens": ["harrt", "Be\u00b7man\u00b7nung", ",", "von", "der", "Br\u00fc\u00b7cke"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "NN", "$,", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "schallt des Kapit\u00e4ns Befehlsruf ...", "tokens": ["schallt", "des", "Ka\u00b7pi\u00b7t\u00e4ns", "Be\u00b7fehls\u00b7ruf", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, 