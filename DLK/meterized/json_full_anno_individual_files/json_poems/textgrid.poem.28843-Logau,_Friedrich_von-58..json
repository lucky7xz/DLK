{"textgrid.poem.28843": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "58.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr Jungfern, h\u00f6rt mir zu; doch fasset die Geberden", "tokens": ["Ihr", "Jung\u00b7fern", ",", "h\u00f6rt", "mir", "zu", ";", "doch", "fas\u00b7set", "die", "Ge\u00b7ber\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "PPER", "PTKVZ", "$.", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und meint durch meinen Ruhm nicht st\u00f6ltzer wo zu werden!", "tokens": ["Und", "meint", "durch", "mei\u00b7nen", "Ruhm", "nicht", "st\u00b7\u00f6lt\u00b7zer", "wo", "zu", "wer\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "PTKNEG", "ADJA", "PWAV", "PTKZU", "VAINF", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Die Jungfern sind ein Volck, sind unter uns gestellt", "tokens": ["Die", "Jung\u00b7fern", "sind", "ein", "Volck", ",", "sind", "un\u00b7ter", "uns", "ge\u00b7stellt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$,", "VAFIN", "APPR", "PPER", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Als Engel in der Zeit, als Wunder in der Welt;", "tokens": ["Als", "En\u00b7gel", "in", "der", "Zeit", ",", "als", "Wun\u00b7der", "in", "der", "Welt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "APPR", "ART", "NN", "$,", "KOUS", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Sie sind ein kurtz Begrieff von allen Zierligkeiten,", "tokens": ["Sie", "sind", "ein", "kurtz", "Be\u00b7grieff", "von", "al\u00b7len", "Zier\u00b7lig\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJD", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der Menschheit h\u00f6chster Schmuck, ein Vorbild jener Zeiten,", "tokens": ["Der", "Menschheit", "h\u00f6chs\u00b7ter", "Schmuck", ",", "ein", "Vor\u00b7bild", "je\u00b7ner", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,", "ART", "NN", "PDAT", "NN", "$,"], "meter": "--+-+-+-+-+-", "measure": "anapaest.init"}, "line.7": {"text": "Wo alles klar wird seyn, ein Muster erster Art,", "tokens": ["Wo", "al\u00b7les", "klar", "wird", "seyn", ",", "ein", "Mus\u00b7ter", "ers\u00b7ter", "Art", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADJD", "VAFIN", "VAINF", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Eh uns der S\u00fcnden Schmach in Eden erblich ward.", "tokens": ["Eh", "uns", "der", "S\u00fcn\u00b7den", "Schmach", "in", "E\u00b7den", "er\u00b7blich", "ward", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "APPR", "NN", "ADJD", "VAFIN", "$."], "meter": "+--+-+-+--+-", "measure": "iambic.penta.invert"}, "line.9": {"text": "Die Tugend hat sie lieb, l\u00e4st gern um sie sich finden;", "tokens": ["Die", "Tu\u00b7gend", "hat", "sie", "lieb", ",", "l\u00e4st", "gern", "um", "sie", "sich", "fin\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADJD", "$,", "VVFIN", "ADV", "APPR", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Die Ehre kr\u00f6n sie sch\u00f6n; ihr Ruhm bleibt nicht dahinden,", "tokens": ["Die", "Eh\u00b7re", "kr\u00f6n", "sie", "sch\u00f6n", ";", "ihr", "Ruhm", "bleibt", "nicht", "da\u00b7hin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "$.", "PPOSAT", "NN", "VVFIN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Geht mit dem Himmel um, r\u00fchrt bi\u00df an Himmel an;", "tokens": ["Geht", "mit", "dem", "Him\u00b7mel", "um", ",", "r\u00fchrt", "bi\u00df", "an", "Him\u00b7mel", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PTKVZ", "$,", "VVFIN", "ADV", "APPR", "NN", "PTKVZ", "$."], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.12": {"text": "Ein ieder preist sie hoch, wer preisen immer kan.", "tokens": ["Ein", "ie\u00b7der", "preist", "sie", "hoch", ",", "wer", "prei\u00b7sen", "im\u00b7mer", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "PPER", "ADJD", "$,", "PWS", "VVFIN", "ADV", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ich w\u00fcste nicht, wer der, und wannen er entsprossen,", "tokens": ["Ich", "w\u00fcs\u00b7te", "nicht", ",", "wer", "der", ",", "und", "wan\u00b7nen", "er", "ent\u00b7spros\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWS", "ART", "$,", "KON", "VVFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und was f\u00fcr wilde Milch sein erster Mund genossen,", "tokens": ["Und", "was", "f\u00fcr", "wil\u00b7de", "Milch", "sein", "ers\u00b7ter", "Mund", "ge\u00b7nos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "APPR", "ADJA", "NN", "PPOSAT", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Der ernstlich hier nur siht, der fr\u00f6hlich hier nicht lacht,", "tokens": ["Der", "ernst\u00b7lich", "hier", "nur", "siht", ",", "der", "fr\u00f6h\u00b7lich", "hier", "nicht", "lacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADV", "ADV", "VVFIN", "$,", "PRELS", "ADJD", "ADV", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Wann ihm de\u00df Himmels Gunst die Augen w\u00fcrdig macht", "tokens": ["Wann", "ihm", "de\u00df", "Him\u00b7mels", "Gunst", "die", "Au\u00b7gen", "w\u00fcr\u00b7dig", "macht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ART", "NN", "NN", "ART", "NN", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Zu schauen diesen Glantz, zu mercken diese Sonnen,", "tokens": ["Zu", "schau\u00b7en", "die\u00b7sen", "Glantz", ",", "zu", "mer\u00b7cken", "die\u00b7se", "Son\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PDAT", "NN", "$,", "PTKZU", "VVINF", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Wodurch der Menschheit Werth den h\u00f6chsten Stand gewunnen", "tokens": ["Wo\u00b7durch", "der", "Menschheit", "Werth", "den", "h\u00f6chs\u00b7ten", "Stand", "ge\u00b7wun\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "NN", "ART", "ADJA", "NN", "VVINF"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.19": {"text": "Und so erleuchtet ist. Er ist nicht werth so gar,", "tokens": ["Und", "so", "er\u00b7leuch\u00b7tet", "ist", ".", "Er", "ist", "nicht", "werth", "so", "gar", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVPP", "VAFIN", "$.", "PPER", "VAFIN", "PTKNEG", "ADJD", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Da\u00df seine Mutter selbst ie eine Jungfer war,", "tokens": ["Da\u00df", "sei\u00b7ne", "Mut\u00b7ter", "selbst", "ie", "ei\u00b7ne", "Jung\u00b7fer", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "ADV", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Der sein Geberde nicht zur Ehrerbitung neiget,", "tokens": ["Der", "sein", "Ge\u00b7ber\u00b7de", "nicht", "zur", "Ehr\u00b7er\u00b7bi\u00b7tung", "nei\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "PTKNEG", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Sein Haupt zum tieffsten b\u00fcckt, den Fu\u00df in Demut beuget", "tokens": ["Sein", "Haupt", "zum", "tieffs\u00b7ten", "b\u00fcckt", ",", "den", "Fu\u00df", "in", "De\u00b7mut", "beu\u00b7get"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPRART", "ADJA", "VVFIN", "$,", "ART", "NN", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Und gibt sich pflichtbar hin f\u00fcr einen eignen Knecht", "tokens": ["Und", "gibt", "sich", "pflicht\u00b7bar", "hin", "f\u00fcr", "ei\u00b7nen", "eig\u00b7nen", "Knecht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "ADJD", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "F\u00fcr ein so liebes Volck und himmlisches Geschlecht.", "tokens": ["F\u00fcr", "ein", "so", "lie\u00b7bes", "Volck", "und", "himm\u00b7li\u00b7sches", "Ge\u00b7schlecht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADV", "ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Iedoch merckt gleichwol drauff, ihr lieblichen Jungfrauen!", "tokens": ["Ie\u00b7doch", "merckt", "gleich\u00b7wol", "drauff", ",", "ihr", "lieb\u00b7li\u00b7chen", "Jung\u00b7frau\u00b7en", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PTKVZ", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.26": {"text": "Ich meine die, wo mehr auff That als Wort zu bauen;", "tokens": ["Ich", "mei\u00b7ne", "die", ",", "wo", "mehr", "auff", "That", "als", "Wort", "zu", "bau\u00b7en", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "$,", "PWAV", "ADV", "APPR", "NN", "KOUS", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Und habt mir nur f\u00fcr gut, ich mein auch meistens die,", "tokens": ["Und", "habt", "mir", "nur", "f\u00fcr", "gut", ",", "ich", "mein", "auch", "meis\u00b7tens", "die", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "APPR", "ADJD", "$,", "PPER", "PPOSAT", "ADV", "ADV", "ART", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Wo Winter nicht verbot, da\u00df Fr\u00fchling mehr nicht bl\u00fch.", "tokens": ["Wo", "Win\u00b7ter", "nicht", "ver\u00b7bot", ",", "da\u00df", "Fr\u00fch\u00b7ling", "mehr", "nicht", "bl\u00fch", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "PTKNEG", "VVFIN", "$,", "KOUS", "NN", "ADV", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ihr Jungfern, h\u00f6rt mir zu; doch fasset die Geberden", "tokens": ["Ihr", "Jung\u00b7fern", ",", "h\u00f6rt", "mir", "zu", ";", "doch", "fas\u00b7set", "die", "Ge\u00b7ber\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "PPER", "PTKVZ", "$.", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und meint durch meinen Ruhm nicht st\u00f6ltzer wo zu werden!", "tokens": ["Und", "meint", "durch", "mei\u00b7nen", "Ruhm", "nicht", "st\u00b7\u00f6lt\u00b7zer", "wo", "zu", "wer\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "PTKNEG", "ADJA", "PWAV", "PTKZU", "VAINF", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa