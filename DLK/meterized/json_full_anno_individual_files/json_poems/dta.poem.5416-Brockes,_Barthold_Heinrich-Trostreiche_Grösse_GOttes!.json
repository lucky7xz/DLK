{"dta.poem.5416": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Trostreiche Gr\u00f6sse GOttes!", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die unerm\u00e4\u00dfliche Beschaffenheit", "tokens": ["Die", "un\u00b7er\u00b7m\u00e4\u00df\u00b7li\u00b7che", "Be\u00b7schaf\u00b7fen\u00b7heit"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Von unsers Sch\u00f6pfers Gr\u00f6\u00df\u2019, die in die Ewigkeit,", "tokens": ["Von", "un\u00b7sers", "Sch\u00f6p\u00b7fers", "Gr\u00f6\u00df'", ",", "die", "in", "die", "E\u00b7wig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NE", "$,", "PRELS", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So sonder Ende, sich erstrecket,", "tokens": ["So", "son\u00b7der", "En\u00b7de", ",", "sich", "er\u00b7stre\u00b7cket", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und dadurch fast, da unser Geist so klein,", "tokens": ["Und", "da\u00b7durch", "fast", ",", "da", "un\u00b7ser", "Geist", "so", "klein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "ADV", "$,", "KOUS", "PPOSAT", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und wir fast gegen ihn f\u00fcr nichts zu rechnen seyn", "tokens": ["Und", "wir", "fast", "ge\u00b7gen", "ihn", "f\u00fcr", "nichts", "zu", "rech\u00b7nen", "seyn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "ADV", "APPR", "PPER", "APPR", "PIS", "PTKZU", "VVINF", "VAINF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Durch gar zu grosse Gr\u00f6\u00df\u2019 uns schrecket,", "tokens": ["Durch", "gar", "zu", "gros\u00b7se", "Gr\u00f6\u00df'", "uns", "schre\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "APPR", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Ist dennoch voller Trost; weil sie ja nicht allein", "tokens": ["Ist", "den\u00b7noch", "vol\u00b7ler", "Trost", ";", "weil", "sie", "ja", "nicht", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ADJA", "NN", "$.", "KOUS", "PPER", "ADV", "PTKNEG", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Sich in die Fern\u2019 und von uns abwerts sencket;", "tokens": ["Sich", "in", "die", "Fern'", "und", "von", "uns", "ab\u00b7werts", "sen\u00b7cket", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "KON", "APPR", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Nein, da sie alle Ding\u2019 erf\u00fcllt; ist die\u00df der Schlu\u00df,", "tokens": ["Nein", ",", "da", "sie", "al\u00b7le", "Ding'", "er\u00b7f\u00fcllt", ";", "ist", "die\u00df", "der", "Schlu\u00df", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "KOUS", "PPER", "PIAT", "NN", "VVPP", "$.", "VAFIN", "PDS", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Da\u00df sie sich gleichfals zu uns lencket,", "tokens": ["Da\u00df", "sie", "sich", "gleich\u00b7fals", "zu", "uns", "len\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Da\u00df sie uns gleichfals nah, ja uns ber\u00fchren mu\u00df.", "tokens": ["Da\u00df", "sie", "uns", "gleich\u00b7fals", "nah", ",", "ja", "uns", "be\u00b7r\u00fch\u00b7ren", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "ADJD", "$,", "ADV", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}