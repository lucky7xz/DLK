{"dta.poem.13097": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Achtes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201ebringt die Bildhauerinn ein Bildnis in die Frag,", "tokens": ["\u201e", "bringt", "die", "Bild\u00b7hau\u00b7e\u00b7rinn", "ein", "Bild\u00b7nis", "in", "die", "Frag", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "ART", "NN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "\u201eso lenckt sie schon das Ohr nach meinem Cither-Schlag;", "tokens": ["\u201e", "so", "lenckt", "sie", "schon", "das", "Ohr", "nach", "mei\u00b7nem", "Cither\u00b7Schlag", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "\u201eund was sie sich entschlie\u00dft in Marmel auszurunden,", "tokens": ["\u201e", "und", "was", "sie", "sich", "ent\u00b7schlie\u00dft", "in", "Mar\u00b7mel", "aus\u00b7zu\u00b7run\u00b7den", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "PWS", "PPER", "PRF", "VVFIN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u201edas wird durch meine Kunst bereitet und erfunden.", "tokens": ["\u201e", "das", "wird", "durch", "mei\u00b7ne", "Kunst", "be\u00b7rei\u00b7tet", "und", "er\u00b7fun\u00b7den", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PDS", "VAFIN", "APPR", "PPOSAT", "NN", "VVFIN", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "\u201eein Blinder sieht die Schild- und Mahlereyen nicht,", "tokens": ["\u201e", "ein", "Blin\u00b7der", "sieht", "die", "Schild", "und", "Mah\u00b7le\u00b7re\u00b7yen", "nicht", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "ART", "TRUNC", "KON", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "330\u201dDoch h\u00f6rt er was der Klang der edlen Cither spricht;", "tokens": ["\"", "Doch", "h\u00f6rt", "er", "was", "der", "Klang", "der", "ed\u00b7len", "Cit\u00b7her", "spricht", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "VVFIN", "PPER", "PIS", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "\u201edesselben Lieblichkeit ermuntert alle Sinnen,", "tokens": ["\u201e", "des\u00b7sel\u00b7ben", "Lieb\u00b7lich\u00b7keit", "er\u00b7mun\u00b7tert", "al\u00b7le", "Sin\u00b7nen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PDAT", "NN", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "\u201ewas fragt er um das Aug? er kann das Herz gewinnen.", "tokens": ["\u201e", "was", "fragt", "er", "um", "das", "Aug", "?", "er", "kann", "das", "Herz", "ge\u00b7win\u00b7nen", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VVFIN", "PPER", "APPR", "ART", "NN", "$.", "PPER", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}