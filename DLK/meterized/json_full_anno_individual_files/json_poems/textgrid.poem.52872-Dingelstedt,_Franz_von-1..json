{"textgrid.poem.52872": {"metadata": {"author": {"name": "Dingelstedt, Franz von", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1847, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Weib, gib mir Deckel, Spie\u00df und Mantel,", "tokens": ["Weib", ",", "gib", "mir", "De\u00b7ckel", ",", "Spie\u00df", "und", "Man\u00b7tel", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVIMP", "PPER", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Dienst geht los, ich mu\u00df hinaus.", "tokens": ["Der", "Dienst", "geht", "los", ",", "ich", "mu\u00df", "hin\u00b7aus", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,", "PPER", "VMFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Noch einen Schluck ... Adies Mariandel!", "tokens": ["Noch", "ei\u00b7nen", "Schluck", "...", "A\u00b7dies", "Ma\u00b7ri\u00b7an\u00b7del", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$(", "NE", "NE", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ich h\u00fct' die Stadt, h\u00fct' du das Haus!", "tokens": ["Ich", "h\u00fct'", "die", "Stadt", ",", "h\u00fct'", "du", "das", "Haus", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nun schrei' ich wieder wie besessen,", "tokens": ["Nun", "schrei'", "ich", "wie\u00b7der", "wie", "be\u00b7ses\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "KOKOM", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Was sie nicht zu verstehen wagen", "tokens": ["Was", "sie", "nicht", "zu", "ver\u00b7ste\u00b7hen", "wa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "PTKNEG", "PTKZU", "VVINF", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und was sie alle Tag vergessen:", "tokens": ["Und", "was", "sie", "al\u00b7le", "Tag", "ver\u00b7ges\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Schnarcht ruhig fort in Euren Nestern", "tokens": ["Schnarcht", "ru\u00b7hig", "fort", "in", "Eu\u00b7ren", "Nes\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADJD", "PTKVZ", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und habt auf mein Gekreisch nicht acht!", "tokens": ["Und", "habt", "auf", "mein", "Ge\u00b7kreisch", "nicht", "acht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "PPOSAT", "NN", "PTKNEG", "CARD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Welt ist akkurat wie gestern,", "tokens": ["Die", "Welt", "ist", "ak\u00b7ku\u00b7rat", "wie", "ge\u00b7stern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KOKOM", "ADV", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die Nacht so schwarz wie alle Nacht.", "tokens": ["Die", "Nacht", "so", "schwarz", "wie", "al\u00b7le", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "KOKOM", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Auch welche Zeit, will Niemand wissen,", "tokens": ["Auch", "wel\u00b7che", "Zeit", ",", "will", "Nie\u00b7mand", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PWAT", "NN", "$,", "VMFIN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "'s gibt keine Zeit in unsren Tagen,", "tokens": ["'s", "gibt", "kei\u00b7ne", "Zeit", "in", "un\u00b7sren", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Duckt Euch nur in die warmen Kissen,", "tokens": ["Duckt", "Euch", "nur", "in", "die", "war\u00b7men", "Kis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "La\u00df keiner sich im Schlaf ber\u00fccken", "tokens": ["La\u00df", "kei\u00b7ner", "sich", "im", "Schlaf", "be\u00b7r\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PIS", "PRF", "APPRART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vom (vulgo Zeitgeist) Antichrist,", "tokens": ["Vom", "(", "vul\u00b7go", "Zeit\u00b7geist", ")", "An\u00b7ti\u00b7ch\u00b7rist", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPRART", "$(", "NE", "NN", "$(", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Und sollte wen ein \u00c4lplein dr\u00fccken,", "tokens": ["Und", "soll\u00b7te", "wen", "ein", "\u00c4l\u00b7plein", "dr\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PWS", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dankt Gott, da\u00df es nichts \u00c4rgres ist.", "tokens": ["Dankt", "Gott", ",", "da\u00df", "es", "nichts", "\u00c4r\u00b7gres", "ist", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$,", "KOUS", "PPER", "PIS", "PIS", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das Murren, Meistern, Zerrn und Zanken,", "tokens": ["Das", "Mur\u00b7ren", ",", "Meis\u00b7tern", ",", "Zerrn", "und", "Zan\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Das Tr\u00e4umen tut es freilich nicht,", "tokens": ["Das", "Tr\u00e4u\u00b7men", "tut", "es", "frei\u00b7lich", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Drum schluckt sie runter, die Gedanken,", "tokens": ["Drum", "schluckt", "sie", "run\u00b7ter", ",", "die", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADJD", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Auch wackelt nicht im b\u00f6sen Willen", "tokens": ["Auch", "wa\u00b7ckelt", "nicht", "im", "b\u00f6\u00b7sen", "Wil\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PTKNEG", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "An Eurem Bett und r\u00e4kelt nicht,", "tokens": ["An", "Eu\u00b7rem", "Bett", "und", "r\u00e4\u00b7kelt", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "VVFIN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Zipfelm\u00fctze zieht im Stillen", "tokens": ["Die", "Zip\u00b7fel\u00b7m\u00fct\u00b7ze", "zieht", "im", "Stil\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zufrieden \u00fcbers Angesicht.", "tokens": ["Zu\u00b7frie\u00b7den", "\u00fc\u00b7bers", "An\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Hund im Stall, der Mann beim Weibe,", "tokens": ["Der", "Hund", "im", "Stall", ",", "der", "Mann", "beim", "Wei\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die Magd beim Knecht, wie Recht und Pflicht,", "tokens": ["Die", "Magd", "beim", "Knecht", ",", "wie", "Recht", "und", "Pflicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,", "PWAV", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So ruht und r\u00fchrt Euch nicht beileibe,", "tokens": ["So", "ruht", "und", "r\u00fchrt", "Euch", "nicht", "bei\u00b7lei\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "PPER", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Und wann die Nacht, wie alle N\u00e4chte,", "tokens": ["Und", "wann", "die", "Nacht", ",", "wie", "al\u00b7le", "N\u00e4ch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "$,", "PWAV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vollendet hat den tr\u00e4gen Lauf,", "tokens": ["Voll\u00b7en\u00b7det", "hat", "den", "tr\u00e4\u00b7gen", "Lauf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dann steigt, doch stets zuerst das rechte", "tokens": ["Dann", "steigt", ",", "doch", "stets", "zu\u00b7erst", "das", "rech\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "ADV", "ADV", "ADV", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bein aus den Federn, sittsam auf!", "tokens": ["Bein", "aus", "den", "Fe\u00b7dern", ",", "sitt\u00b7sam", "auf", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,", "ADJD", "PTKVZ", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Labt Euch an dem Zichorientranke", "tokens": ["Labt", "Euch", "an", "dem", "Zi\u00b7cho\u00b7ri\u00b7en\u00b7tran\u00b7ke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Und tretet Eure M\u00fchlen gern,", "tokens": ["Und", "tre\u00b7tet", "Eu\u00b7re", "M\u00fch\u00b7len", "gern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "li