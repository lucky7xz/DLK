{"dta.poem.21478": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Pallas lunatica.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Es war in einer blauen Sommernacht;", "tokens": ["Es", "war", "in", "ei\u00b7ner", "blau\u00b7en", "Som\u00b7mer\u00b7nacht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Vom Himmel schien, wie bl\u00fch\u2019nd in Lilienpracht,", "tokens": ["Vom", "Him\u00b7mel", "schien", ",", "wie", "bl\u00fch'nd", "in", "Li\u00b7lien\u00b7pracht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,", "PWAV", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Mond hernieder in mein K\u00e4mmerlein,", "tokens": ["Der", "Mond", "her\u00b7nie\u00b7der", "in", "mein", "K\u00e4m\u00b7mer\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "In dem ich sa\u00df am Fenster und allein.", "tokens": ["In", "dem", "ich", "sa\u00df", "am", "Fens\u00b7ter", "und", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "VVFIN", "APPRART", "NN", "KON", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ich sa\u00df allein, das Herz von Trauer schwer,", "tokens": ["Ich", "sa\u00df", "al\u00b7lein", ",", "das", "Herz", "von", "Trau\u00b7er", "schwer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "ART", "NN", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Inde\u00df aus einem fernen Garten her", "tokens": ["In\u00b7de\u00df", "aus", "ei\u00b7nem", "fer\u00b7nen", "Gar\u00b7ten", "her"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "APZR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Brummba\u00df und Geige durch die Stille klang", "tokens": ["Brumm\u00b7ba\u00df", "und", "Gei\u00b7ge", "durch", "die", "Stil\u00b7le", "klang"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Zu wildem Tanz und lustig tollem Sang.", "tokens": ["Zu", "wil\u00b7dem", "Tanz", "und", "lus\u00b7tig", "tol\u00b7lem", "Sang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Das Herz von Trauer schwer, sa\u00df ich allein,", "tokens": ["Das", "Herz", "von", "Trau\u00b7er", "schwer", ",", "sa\u00df", "ich", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "ADJD", "$,", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sah wie gebannt in diesen wei\u00dfen Schein", "tokens": ["Sah", "wie", "ge\u00b7bannt", "in", "die\u00b7sen", "wei\u00b7\u00dfen", "Schein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "KOKOM", "ADJD", "APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Des Mondenlichts, und geisterhaft und mild", "tokens": ["Des", "Mon\u00b7den\u00b7lichts", ",", "und", "geis\u00b7ter\u00b7haft", "und", "mild"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "KON", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Schien auf mich nieder, ach ein leuchtend\u2019 Bild.", "tokens": ["Schien", "auf", "mich", "nie\u00b7der", ",", "ach", "ein", "leuch\u00b7tend'", "Bild", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "PTKVZ", "$,", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ich kenne dich, o Antlitz, edelbleich,", "tokens": ["Ich", "ken\u00b7ne", "dich", ",", "o", "Ant\u00b7litz", ",", "e\u00b7del\u00b7bleich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "FM", "NN", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wer ri\u00df dich aufw\u00e4rts aus dem Schattenreich,", "tokens": ["Wer", "ri\u00df", "dich", "auf\u00b7w\u00e4rts", "aus", "dem", "Schat\u00b7ten\u00b7reich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wo du seit Jahren schliefest wonnestill,", "tokens": ["Wo", "du", "seit", "Jah\u00b7ren", "schlie\u00b7fest", "won\u00b7nes\u00b7till", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die blasse Stirn umkr\u00e4nzt von Asphodill?", "tokens": ["Die", "blas\u00b7se", "Stirn", "um\u00b7kr\u00e4nzt", "von", "As\u00b7pho\u00b7dill", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "APPR", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Was siehst du mich so gl\u00e4nzend schweigsam an?", "tokens": ["Was", "siehst", "du", "mich", "so", "gl\u00e4n\u00b7zend", "schweig\u00b7sam", "an", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PRF", "ADV", "ADJD", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ha, immer wirket noch dein Zauberbann,", "tokens": ["Ha", ",", "im\u00b7mer", "wir\u00b7ket", "noch", "dein", "Zau\u00b7ber\u00b7bann", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "VVFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der mich emportr\u00e4gt mit des Adlers Schwung \u2014", "tokens": ["Der", "mich", "em\u00b7por\u00b7tr\u00e4gt", "mit", "des", "Ad\u00b7lers", "Schwung"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "O Sehnsucht, Sehnsucht, o Beseligung!", "tokens": ["O", "Sehn\u00b7sucht", ",", "Sehn\u00b7sucht", ",", "o", "Be\u00b7se\u00b7li\u00b7gung", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "NN", "$,", "FM", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Vergessen wie ein Traum der Erde Leid!", "tokens": ["Ver\u00b7ges\u00b7sen", "wie", "ein", "Traum", "der", "Er\u00b7de", "Leid", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "NN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "O Himmelslust! O ", "tokens": ["O", "Him\u00b7mels\u00b7lust", "!", "O"], "token_info": ["word", "word", "punct", "word"], "pos": ["NE", "NN", "$.", "NE"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Ach, wei\u00dfes Antlitz, lotosblumensch\u00f6n,", "tokens": ["Ach", ",", "wei\u00b7\u00dfes", "Ant\u00b7litz", ",", "lo\u00b7tos\u00b7blu\u00b7men\u00b7sch\u00f6n", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ITJ", "$,", "ADJA", "NN", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Was blickst du sinnend auf zu ", "tokens": ["Was", "blickst", "du", "sin\u00b7nend", "auf", "zu"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "ADJD", "APPR", "PTKZU"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "\u201eschau\u2019st du dort oben jenes h\u00f6chste Licht\u201c \u2014", "tokens": ["\u201e", "schau'st", "du", "dort", "o\u00b7ben", "je\u00b7nes", "h\u00f6chs\u00b7te", "Licht", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "PPER", "ADV", "ADV", "PDAT", "ADJA", "NN", "$(", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "O frommes Kind, ich seh es nicht, ", "tokens": ["O", "from\u00b7mes", "Kind", ",", "ich", "seh", "es", "nicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "PPER", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich seh\u2019 nur, wie versinkt das Blau der Nacht", "tokens": ["Ich", "seh'", "nur", ",", "wie", "ver\u00b7sinkt", "das", "Blau", "der", "Nacht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PWAV", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "In immer d\u00fcst\u2019rer gl\u00e4nzend schwarze Pracht.", "tokens": ["In", "im\u00b7mer", "d\u00fcst'\u00b7rer", "gl\u00e4n\u00b7zend", "schwar\u00b7ze", "Pracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJD", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "\u201egeduld! wir sind von ihm nicht mehr so weit\u201c \u2014", "tokens": ["\u201e", "ge\u00b7duld", "!", "wir", "sind", "von", "ihm", "nicht", "mehr", "so", "weit", "\u201c"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADJD", "$.", "PPER", "VAFIN", "APPR", "PPER", "PTKNEG", "ADV", "ADV", "ADJD", "$(", "$("], "meter": "-+-++--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "O du mein Stern, umh\u00fcllt von Seligkeit,", "tokens": ["O", "du", "mein", "Stern", ",", "um\u00b7h\u00fcllt", "von", "Se\u00b7lig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "PPOSAT", "NN", "$,", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wag\u2019 ich zu ahnen kaum das h\u00f6chste Licht,", "tokens": ["Wag'", "ich", "zu", "ah\u00b7nen", "kaum", "das", "h\u00f6chs\u00b7te", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKZU", "VVINF", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Von dem dein blasser Mund verhei\u00dfend spricht.", "tokens": ["Von", "dem", "dein", "blas\u00b7ser", "Mund", "ver\u00b7hei\u00b7\u00dfend", "spricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PPOSAT", "ADJA", "NN", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "\u201eo weiter, weiter nur zum Flug hinan\u201c \u2014", "tokens": ["\u201e", "o", "wei\u00b7ter", ",", "wei\u00b7ter", "nur", "zum", "Flug", "hi\u00b7nan", "\u201c"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "FM", "ADV", "$,", "ADV", "ADV", "APPRART", "NN", "PTKVZ", "$(", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mein heilig Kind, ich bin ein kranker Mann,", "tokens": ["Mein", "hei\u00b7lig", "Kind", ",", "ich", "bin", "ein", "kran\u00b7ker", "Mann", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "$,", "PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Bin m\u00fcde, grenzenlos, schon sink\u2019 ich, weh \u2026", "tokens": ["Bin", "m\u00fc\u00b7de", ",", "gren\u00b7zen\u00b7los", ",", "schon", "sink'", "ich", ",", "weh", "\u2026"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "ADJD", "$,", "ADJD", "$,", "ADV", "VVFIN", "PPER", "$,", "ADV", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Von unten winkt ein nachtschwarz stummer See.", "tokens": ["Von", "un\u00b7ten", "winkt", "ein", "nacht\u00b7schwarz", "stum\u00b7mer", "See", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VVFIN", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "O du mein Stern, o wei\u00dfe Blume du,", "tokens": ["O", "du", "mein", "Stern", ",", "o", "wei\u00b7\u00dfe", "Blu\u00b7me", "du", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "PPOSAT", "NN", "$,", "FM", "ADJA", "NN", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mein Himmel, ach, ich sehne mich nach Ruh \u2014", "tokens": ["Mein", "Him\u00b7mel", ",", "ach", ",", "ich", "seh\u00b7ne", "mich", "nach", "Ruh"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ITJ", "$,", "PPER", "VVFIN", "PRF", "APPR", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "\u201egeliebter, siehst du nicht den ros\u2019gen Schein?\u201c \u2026", "tokens": ["\u201e", "ge\u00b7lieb\u00b7ter", ",", "siehst", "du", "nicht", "den", "ros'\u00b7gen", "Schein", "?", "\u201c", "\u2026"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "ADJA", "$,", "VVFIN", "PPER", "PTKNEG", "ART", "ADJA", "NN", "$.", "$(", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mein Muth zerbricht, weh\u2019 mir, flieg du allein!", "tokens": ["Mein", "Muth", "zer\u00b7bricht", ",", "weh'", "mir", ",", "flieg", "du", "al\u00b7lein", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Umathmet schon vom Hauch des ew\u2019gen Lichts,", "tokens": ["Um\u00b7ath\u00b7met", "schon", "vom", "Hauch", "des", "ew'\u00b7gen", "Lichts", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sank ich hinab ins bodenlose Nichts;", "tokens": ["Sank", "ich", "hin\u00b7ab", "ins", "bo\u00b7den\u00b7lo\u00b7se", "Nichts", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und w\u00e4hrend aus den Tiefen ich so nah", "tokens": ["Und", "w\u00e4h\u00b7rend", "aus", "den", "Tie\u00b7fen", "ich", "so", "nah"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "APPR", "ART", "NN", "PPER", "ADV", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Des ries\u2019gen D\u00e4mons h\u00f6hnisch\u2019 Grinsen sah,", "tokens": ["Des", "ries'\u00b7gen", "D\u00e4\u00b7mons", "h\u00f6h\u00b7nisch'", "Grin\u00b7sen", "sah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.12": {"line.1": {"text": "Da scholl es wild wie ein Verzweiflungsschrei", "tokens": ["Da", "scholl", "es", "wild", "wie", "ein", "Ver\u00b7zwei\u00b7flungs\u00b7schrei"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Von oben her in grauser Melodei:", "tokens": ["Von", "o\u00b7ben", "her", "in", "grau\u00b7ser", "Me\u00b7lo\u00b7dei", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "APZR", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "\u201egetrennt auf immerdar! Du ", "tokens": ["\u201e", "ge\u00b7trennt", "auf", "im\u00b7mer\u00b7dar", "!", "Du"], "token_info": 