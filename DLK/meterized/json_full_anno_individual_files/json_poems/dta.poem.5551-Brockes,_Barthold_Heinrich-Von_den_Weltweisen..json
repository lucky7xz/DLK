{"dta.poem.5551": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Weltweisen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Allein, wen werd ich mir zu einem F\u00fchrer wehlen?", "tokens": ["Al\u00b7lein", ",", "wen", "werd", "ich", "mir", "zu", "ei\u00b7nem", "F\u00fch\u00b7rer", "weh\u00b7len", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWS", "VAFIN", "PPER", "PRF", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wer von der Weisen Zunfft, aus den begrauten Zeiten,", "tokens": ["Wer", "von", "der", "Wei\u00b7sen", "Zunfft", ",", "aus", "den", "be\u00b7grau\u00b7ten", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "NN", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wird meinen bangen Fu\u00df, so da\u00df ich nicht mag fehlen,", "tokens": ["Wird", "mei\u00b7nen", "ban\u00b7gen", "Fu\u00df", ",", "so", "da\u00df", "ich", "nicht", "mag", "feh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN", "$,", "ADV", "KOUS", "PPER", "PTKNEG", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Zum Steig der Wahrheit leiten?", "tokens": ["Zum", "Steig", "der", "Wahr\u00b7heit", "lei\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ich \u00fcberlege die Geschichte", "tokens": ["Ich", "\u00fc\u00b7berl\u00b7e\u00b7ge", "die", "Ge\u00b7schich\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Von K\u00fcnsten und von Wissenschafften.", "tokens": ["Von", "K\u00fcns\u00b7ten", "und", "von", "Wis\u00b7sen\u00b7schaff\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Die Namen, welche das Ger\u00fcchte", "tokens": ["Die", "Na\u00b7men", ",", "wel\u00b7che", "das", "Ge\u00b7r\u00fcch\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Verewigt, stell ich mir", "tokens": ["Ve\u00b7re\u00b7wigt", ",", "stell", "ich", "mir"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVPP", "$,", "VVFIN", "PPER", "PPER"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.9": {"text": "Von neuen in Gedancken f\u00fcr.", "tokens": ["Von", "neu\u00b7en", "in", "Ge\u00b7dan\u00b7cken", "f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "APPR", "NN", "APPR", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Ich bringe von den alten Weisen", "tokens": ["Ich", "brin\u00b7ge", "von", "den", "al\u00b7ten", "Wei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Die ganze Zahl ber\u00fchmter Greisen,", "tokens": ["Die", "gan\u00b7ze", "Zahl", "be\u00b7r\u00fchm\u00b7ter", "Grei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Die von den Griechen stammen,", "tokens": ["Die", "von", "den", "Grie\u00b7chen", "stam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Auf einen Hauffen hier zusammen;", "tokens": ["Auf", "ei\u00b7nen", "Hauf\u00b7fen", "hier", "zu\u00b7sam\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Wie der Apelles unsrer Zeit,", "tokens": ["Wie", "der", "A\u00b7pel\u00b7les", "uns\u00b7rer", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NE", "PPOSAT", "NN", "$,"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.15": {"text": "In seiner Schul, ", "tokens": ["In", "sei\u00b7ner", "Schul", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.16": {"text": "Dieselbigen gewust uns darzustellen.", "tokens": ["Die\u00b7sel\u00b7bi\u00b7gen", "ge\u00b7wust", "uns", "dar\u00b7zu\u00b7stel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}