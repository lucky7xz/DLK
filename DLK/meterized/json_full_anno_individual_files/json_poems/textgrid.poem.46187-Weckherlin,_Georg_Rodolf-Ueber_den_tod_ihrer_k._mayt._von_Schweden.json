{"textgrid.poem.46187": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Ueber den tod ihrer k. mayt. von Schweden", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dein eigner mut, o held, weil gotsforcht, ehr und recht", "tokens": ["Dein", "eig\u00b7ner", "mut", ",", "o", "held", ",", "weil", "gots\u00b7forcht", ",", "ehr", "und", "recht"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "FM", "VVFIN", "$,", "KOUS", "ADJD", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "dein herz und schwert allein gest\u00e4rket und gewetzet,", "tokens": ["dein", "herz", "und", "schwert", "al\u00b7lein", "ge\u00b7st\u00e4r\u00b7ket", "und", "ge\u00b7wet\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "VVFIN", "ADV", "VVPP", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "weil auch der erdenkreis f\u00fcr dich zu eng und schlecht", "tokens": ["weil", "auch", "der", "er\u00b7den\u00b7kreis", "f\u00fcr", "dich", "zu", "eng", "und", "schlecht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "APPR", "PPER", "PTKA", "ADJD", "KON", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "hat in den himmel dich, zu fr\u00fch f\u00fcr uns, versetzet.", "tokens": ["hat", "in", "den", "him\u00b7mel", "dich", ",", "zu", "fr\u00fch", "f\u00fcr", "uns", ",", "ver\u00b7set\u00b7zet", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "PPER", "$,", "PTKA", "ADJD", "APPR", "PPER", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Dan gleichwie deine faust der gl\u00e4ubigen geschlecht,", "tokens": ["Dan", "gleich\u00b7wie", "dei\u00b7ne", "faust", "der", "gl\u00e4u\u00b7bi\u00b7gen", "ge\u00b7schlecht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KON", "PPOSAT", "NN", "ART", "ADJA", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "als es in h\u00f6chster not, errettet und ergetzet:", "tokens": ["als", "es", "in", "h\u00f6chs\u00b7ter", "not", ",", "er\u00b7ret\u00b7tet", "und", "er\u00b7get\u00b7zet", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "$,", "VVPP", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "also hat durch dein haupt die kugel, leider! recht", "tokens": ["al\u00b7so", "hat", "durch", "dein", "haupt", "die", "ku\u00b7gel", ",", "lei\u00b7der", "!", "recht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["ADV", "VAFIN", "APPR", "PPOSAT", "NN", "ART", "NN", "$,", "ADV", "$.", "ADJD"], "meter": "+----+-+-+-+", "measure": "dactylic.init"}, "line.8": {"text": "der teutschen freiheit herz und tugend haupt verletzet.", "tokens": ["der", "teut\u00b7schen", "frei\u00b7heit", "herz", "und", "tu\u00b7gend", "haupt", "ver\u00b7let\u00b7zet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "KON", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Sigreich und selig zwar hat dich, weil in der sc