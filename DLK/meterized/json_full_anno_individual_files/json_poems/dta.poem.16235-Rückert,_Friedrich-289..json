{"dta.poem.16235": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "289.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Einfacher Haushalt ist im Staate zu empfehlen;", "tokens": ["Ein\u00b7fa\u00b7cher", "Haus\u00b7halt", "ist", "im", "Staa\u00b7te", "zu", "emp\u00b7feh\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den sollst du, wie im Haus, auch im Gem\u00fcte w\u00e4hlen.", "tokens": ["Den", "sollst", "du", ",", "wie", "im", "Haus", ",", "auch", "im", "Ge\u00b7m\u00fc\u00b7te", "w\u00e4h\u00b7len", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPER", "$,", "PWAV", "APPRART", "NN", "$,", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ob enger sei der Leib, ob weiter sei der Bogen,", "tokens": ["Ob", "en\u00b7ger", "sei", "der", "Leib", ",", "ob", "wei\u00b7ter", "sei", "der", "Bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "VAFIN", "ART", "NN", "$,", "KOUS", "ADV", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Geschlossen sei er nur, so fest als rein gezogen.", "tokens": ["Ge\u00b7schlos\u00b7sen", "sei", "er", "nur", ",", "so", "fest", "als", "rein", "ge\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "ADV", "$,", "ADV", "ADJD", "KOKOM", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Was Fremdes tritt herein, anweis' ihm seine Stelle,", "tokens": ["Was", "Frem\u00b7des", "tritt", "her\u00b7ein", ",", "an\u00b7weis'", "ihm", "sei\u00b7ne", "Stel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VVFIN", "PTKVZ", "$,", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und was nur st\u00f6ren kann, abweis' es von der Schwelle.", "tokens": ["Und", "was", "nur", "st\u00f6\u00b7ren", "kann", ",", "ab\u00b7weis'", "es", "von", "der", "Schwel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "VVINF", "VMFIN", "$,", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Ein Manigfaltiges, ein Vielgestaltiges,", "tokens": ["Ein", "Ma\u00b7nig\u00b7fal\u00b7ti\u00b7ges", ",", "ein", "Viel\u00b7ge\u00b7stal\u00b7ti\u00b7ges", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zusammen sei's gefa\u00dft durch ein Gewaltiges,", "tokens": ["Zu\u00b7sam\u00b7men", "sei's", "ge\u00b7fa\u00dft", "durch", "ein", "Ge\u00b7wal\u00b7ti\u00b7ges", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Durch ein Gewaltiges, das in der Mitte steht", "tokens": ["Durch", "ein", "Ge\u00b7wal\u00b7ti\u00b7ges", ",", "das", "in", "der", "Mit\u00b7te", "steht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als Sonn', um die sich ein Planetenwirbel dreht.", "tokens": ["Als", "Sonn'", ",", "um", "die", "sich", "ein", "Pla\u00b7ne\u00b7ten\u00b7wir\u00b7bel", "dreht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "APPR", "PRELS", "PRF", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.6": {"line.1": {"text": "Den Mittelpunkt des Lichts, den Mittelpunkt der Ruh,", "tokens": ["Den", "Mit\u00b7tel\u00b7punkt", "des", "Lichts", ",", "den", "Mit\u00b7tel\u00b7punkt", "der", "Ruh", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Zieh- und Schwerkraft, hast, mu\u00dft haben in dir du.", "tokens": ["Der", "Zieh", "und", "Schwer\u00b7kraft", ",", "hast", ",", "mu\u00dft", "ha\u00b7ben", "in", "dir", "du", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "TRUNC", "KON", "NN", "$,", "VAFIN", "$,", "VMFIN", "VAFIN", "APPR", "PPER", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Verdunkle nur ihn nicht und bring ihn nicht ins Schwanken", "tokens": ["Ver\u00b7dunk\u00b7le", "nur", "ihn", "nicht", "und", "bring", "ihn", "nicht", "ins", "Schwan\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "PPER", "PTKNEG", "KON", "VVFIN", "PPER", "PTKNEG", "APPRART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Durch th\u00f6richte Begier und eitele Gedanken.", "tokens": ["Durch", "th\u00f6\u00b7rich\u00b7te", "Be\u00b7gier", "und", "ei\u00b7te\u00b7le", "Ge\u00b7dan\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}}, "stanza.8": {"line.1": {"text": "Du gibst den Dingen Werth, und mu\u00dft dich selbst verklagen,", "tokens": ["Du", "gibst", "den", "Din\u00b7gen", "Werth", ",", "und", "mu\u00dft", "dich", "selbst", "ver\u00b7kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$,", "KON", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn du, was du entbehrt, zu hoch hast angeschlagen.", "tokens": ["Wenn", "du", ",", "was", "du", "ent\u00b7behrt", ",", "zu", "hoch", "hast", "an\u00b7ge\u00b7schla\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "PWS", "PPER", "VVPP", "$,", "PTKA", "ADJD", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Mit Vielem h\u00e4lt man Haus, mit Wen'gem kommt man aus;", "tokens": ["Mit", "Vie\u00b7lem", "h\u00e4lt", "man", "Haus", ",", "mit", "Wen'\u00b7gem", "kommt", "man", "aus", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VVFIN", "PIS", "NN", "$,", "APPR", "PIS", "VVFIN", "PIS", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der schont den Magen, wem gen\u00fcgt ein Ohrenschmaus.", "tokens": ["Der", "schont", "den", "Ma\u00b7gen", ",", "wem", "ge\u00b7n\u00fcgt", "ein", "Oh\u00b7ren\u00b7schmaus", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "$,", "PWS", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Nimm nur, was dir sich beut, und thu was du vermagst,", "tokens": ["Nimm", "nur", ",", "was", "dir", "sich", "beut", ",", "und", "thu", "was", "du", "ver\u00b7magst", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "$,", "PWS", "PPER", "PRF", "VVFIN", "$,", "KON", "VVFIN", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So lebst du ohne da\u00df du dich noch andre plagst.", "tokens": ["So", "lebst", "du", "oh\u00b7ne", "da\u00df", "du", "dich", "noch", "and\u00b7re", "plagst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "KOUS", "PPER", "PRF", "ADV", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}