{"dta.poem.9983": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "92.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Alles volck ist unter mir/ hohe/ niedre/ m\u00e4nner/ frauen/", "tokens": ["Al\u00b7les", "volck", "ist", "un\u00b7ter", "mir", "/", "ho\u00b7he", "/", "nied\u00b7re", "/", "m\u00e4n\u00b7ner", "/", "frau\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PIS", "ADJD", "VAFIN", "APPR", "PPER", "$(", "ADJA", "$(", "PIS", "$(", "ADJA", "$(", "VVINF", "$("], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Allen bin ich gleich bedienet/ in des arm und reichen hau\u00df.", "tokens": ["Al\u00b7len", "bin", "ich", "gleich", "be\u00b7die\u00b7net", "/", "in", "des", "arm", "und", "rei\u00b7chen", "hau\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "ADV", "VVFIN", "$(", "APPR", "ART", "ADJD", "KON", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Manche sch\u00f6nheit seh ich blo\u00df/ die sonst keiner kan beschauen/", "tokens": ["Man\u00b7che", "sch\u00f6n\u00b7heit", "seh", "ich", "blo\u00df", "/", "die", "sonst", "kei\u00b7ner", "kan", "be\u00b7schau\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "ADV", "$(", "ART", "ADV", "PIS", "VMFIN", "VVINF", "$("], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Offters wird in meinem beyseyn liebes-glut geleschet aus.", "tokens": ["Off\u00b7ters", "wird", "in", "mei\u00b7nem", "bey\u00b7seyn", "lie\u00b7bes\u00b7glut", "ge\u00b7le\u00b7schet", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "PPOSAT", "PIAT", "NN", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}}}}