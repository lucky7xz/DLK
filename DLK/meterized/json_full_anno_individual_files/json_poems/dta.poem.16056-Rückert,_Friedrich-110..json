{"dta.poem.16056": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "110.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nicht darum sollst du dich verbunden halten, Kind,", "tokens": ["Nicht", "da\u00b7rum", "sollst", "du", "dich", "ver\u00b7bun\u00b7den", "hal\u00b7ten", ",", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PTKNEG", "PAV", "VMFIN", "PPER", "PRF", "VVPP", "VVINF", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu Handlungen, weil sie von Gott geboten sind.", "tokens": ["Zu", "Hand\u00b7lun\u00b7gen", ",", "weil", "sie", "von", "Gott", "ge\u00b7bo\u00b7ten", "sind", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "KOUS", "PPER", "APPR", "NN", "VVPP", "VAFIN", "$."], "meter": "-++--+-+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Vielmehr als g\u00f6ttliches Gebot sei das empfunden", "tokens": ["Viel\u00b7mehr", "als", "g\u00f6tt\u00b7li\u00b7ches", "Ge\u00b7bot", "sei", "das", "emp\u00b7fun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "ADJA", "NN", "VAFIN", "ART", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von dir, wozu du dich f\u00fchlst innerlich verbunden.", "tokens": ["Von", "dir", ",", "wo\u00b7zu", "du", "dich", "f\u00fchlst", "in\u00b7ner\u00b7lich", "ver\u00b7bun\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "PWAV", "PPER", "PRF", "VVFIN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Was ist der Unterschied? dort mu\u00dft du andern glauben,", "tokens": ["Was", "ist", "der", "Un\u00b7ter\u00b7schied", "?", "dort", "mu\u00dft", "du", "an\u00b7dern", "glau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "$.", "ADV", "VMFIN", "PPER", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hier glaube nur dir selbst, und nichts kann dich dir rauben.", "tokens": ["Hier", "glau\u00b7be", "nur", "dir", "selbst", ",", "und", "nichts", "kann", "dich", "dir", "rau\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPER", "ADV", "$,", "KON", "PIS", "VMFIN", "PPER", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}