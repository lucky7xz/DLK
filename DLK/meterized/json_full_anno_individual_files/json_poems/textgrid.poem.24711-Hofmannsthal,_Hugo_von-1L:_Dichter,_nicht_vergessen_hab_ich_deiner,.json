{"textgrid.poem.24711": {"metadata": {"author": {"name": "Hofmannsthal, Hugo von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Dichter, nicht vergessen hab ich deiner,", "genre": "verse", "period": "N.A.", "pub_year": 1893, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dichter, nicht vergessen hab ich deiner,", "tokens": ["Dich\u00b7ter", ",", "nicht", "ver\u00b7ges\u00b7sen", "hab", "ich", "dei\u00b7ner", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PTKNEG", "VVPP", "VAFIN", "PPER", "PPOSAT", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "W\u00e4hrend du die sch\u00f6nen Wege gingest,", "tokens": ["W\u00e4h\u00b7rend", "du", "die", "sch\u00f6\u00b7nen", "We\u00b7ge", "gin\u00b7gest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Goldene Lebensfr\u00fcchte", "tokens": ["Gol\u00b7de\u00b7ne", "Le\u00b7bens\u00b7fr\u00fcch\u00b7te"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Aus dunklem Laub zu pfl\u00fccken", "tokens": ["Aus", "dunk\u00b7lem", "Laub", "zu", "pfl\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und schauernde Gedanken", "tokens": ["Und", "schau\u00b7ern\u00b7de", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "-+---+-", "measure": "dactylic.init"}, "line.6": {"text": "Aus Nymphenh\u00e4nden.", "tokens": ["Aus", "Nym\u00b7phen\u00b7h\u00e4n\u00b7den", "."], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Oft gedacht ich deiner,", "tokens": ["Oft", "ge\u00b7dacht", "ich", "dei\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "PPER", "PPOSAT", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Aber ein Mal vor allen:", "tokens": ["A\u00b7ber", "ein", "Mal", "vor", "al\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "PIAT", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.9": {"text": "Da war mystischer Vollmond", "tokens": ["Da", "war", "mys\u00b7ti\u00b7scher", "Voll\u00b7mond"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Mir \u00fcber der Stirn,", "tokens": ["Mir", "\u00fc\u00b7ber", "der", "Stirn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.11": {"text": "Ein leuchtendes Ding, ein Land", "tokens": ["Ein", "leuch\u00b7ten\u00b7des", "Ding", ",", "ein", "Land"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.12": {"text": "Hoch im leeren Raum.", "tokens": ["Hoch", "im", "lee\u00b7ren", "Raum", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.13": {"text": "Ich schaut ihn an", "tokens": ["Ich", "schaut", "ihn", "an"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ"], "meter": "-+-+", "measure": "iambic.di"}, "line.14": {"text": "Und wuchs empor", "tokens": ["Und", "wuchs", "em\u00b7por"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVFIN", "PTKVZ"], "meter": "-+-+", "measure": "iambic.di"}, "line.15": {"text": "Und kam ihm n\u00e4her", "tokens": ["Und", "kam", "ihm", "n\u00e4\u00b7her"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADJD"], "meter": "-+-+-", "measure": "iambic.di"}, "line.16": {"text": "Und meint', er k\u00e4m zu mir,", "tokens": ["Und", "meint'", ",", "er", "k\u00e4m", "zu", "mir", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PPER", "VVFIN", "APPR", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.17": {"text": "\u2013 Wie einer \u00fcber des gleitenden Schiffes Bord gebeugt", "tokens": ["\u2013", "Wie", "ei\u00b7ner", "\u00fc\u00b7ber", "des", "glei\u00b7ten\u00b7den", "Schif\u00b7fes", "Bord", "ge\u00b7beugt"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "PWAV", "PIS", "APPR", "ART", "ADJA", "NN", "NN", "VVPP"], "meter": "-+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.18": {"text": "Auf leerem blauem schweigendem Meer", "tokens": ["Auf", "lee\u00b7rem", "blau\u00b7em", "schwei\u00b7gen\u00b7dem", "Meer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "ADJA", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.19": {"text": "Einer Insel entgegenstarrt", "tokens": ["Ei\u00b7ner", "In\u00b7sel", "ent\u00b7ge\u00b7gen\u00b7starrt"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.20": {"text": "Und meint, sie schwebt ihm entgegen,", "tokens": ["Und", "meint", ",", "sie", "schwebt", "ihm", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.21": {"text": "Die leuchtende, mit Blumenf\u00fc\u00dfen \u2013:", "tokens": ["Die", "leuch\u00b7ten\u00b7de", ",", "mit", "Blu\u00b7men\u00b7f\u00fc\u00b7\u00dfen", "\u2013", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "$,", "APPR", "NN", "$(", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "So wuchs ich auf,", "tokens": ["So", "wuchs", "ich", "auf", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.23": {"text": "Dem Mond entgegen riesengro\u00df,", "tokens": ["Dem", "Mond", "ent\u00b7ge\u00b7gen", "rie\u00b7sen\u00b7gro\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Vergessend meiner F\u00fc\u00dfe", "tokens": ["Ver\u00b7ges\u00b7send", "mei\u00b7ner", "F\u00fc\u00b7\u00dfe"], "token_info": ["word", "word", "word"], "pos": ["VVPP", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.25": {"text": "Und der dunklen Erde unter mir.", "tokens": ["Und", "der", "dunk\u00b7len", "Er\u00b7de", "un\u00b7ter", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "APPR", "PPER", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Ein solcher mu\u00df ich da geworden sein", "tokens": ["Ein", "sol\u00b7cher", "mu\u00df", "ich", "da", "ge\u00b7wor\u00b7den", "sein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PIS", "VMFIN", "PPER", "ADV", "VAPP", "VAINF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wie der Genius der Zeit,", "tokens": ["Wie", "der", "Ge\u00b7nius", "der", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Der Gebieter der Dinge,", "tokens": ["Der", "Ge\u00b7bie\u00b7ter", "der", "Din\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Stein\u00e4ugig, gewappnet,", "tokens": ["Stei\u00b7n\u00e4u\u00b7gig", ",", "ge\u00b7wapp\u00b7net", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$,", "VVPP", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Kolossalisch hinschreitend", "tokens": ["Ko\u00b7los\u00b7sa\u00b7lisch", "hin\u00b7schrei\u00b7tend"], "token_info": ["word", "word"], "pos": ["NN", "VVPP"], "meter": "-+---+-", "measure": "dactylic.init"}, "line.6": {"text": "\u00dcber die Reiche ...", "tokens": ["\u00dc\u00b7ber", "die", "Rei\u00b7che", "..."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NE", "$("], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Wenn seine Sohlen im Flu\u00dfbett wandeln,", "tokens": ["Wenn", "sei\u00b7ne", "Soh\u00b7len", "im", "Flu\u00df\u00b7bett", "wan\u00b7deln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Reichen der Pinien von Kreideklippen", "tokens": ["Rei\u00b7chen", "der", "Pi\u00b7ni\u00b7en", "von", "Krei\u00b7de\u00b7klip\u00b7pen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "APPR", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.9": {"text": "Des steilen Ufers emporgereckte", "tokens": ["Des", "stei\u00b7len", "U\u00b7fers", "em\u00b7por\u00b7ge\u00b7reck\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Schwarze Wipfel nicht auf,", "tokens": ["Schwar\u00b7ze", "Wip\u00b7fel", "nicht", "auf", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKNEG", "PTKVZ", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.11": {"text": "Lange nicht,", "tokens": ["Lan\u00b7ge", "nicht", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "PTKNEG", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.12": {"text": "An die mattsilberne Fratze der Gorgo,", "tokens": ["An", "die", "matt\u00b7sil\u00b7ber\u00b7ne", "Frat\u00b7ze", "der", "Gor\u00b7go", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NE", "$,"], "meter": "+-++--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.13": {"text": "Die ihm die Stirne des Knies umbindet,", "tokens": ["Die", "ihm", "die", "Stir\u00b7ne", "des", "Knies", "um\u00b7bin\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "Nur unten die Schienen der schreitenden Beine", "tokens": ["Nur", "un\u00b7ten", "die", "Schie\u00b7nen", "der", "schrei\u00b7ten\u00b7den", "Bei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.15": {"text": "Spiegeln beim Blitzschein", "tokens": ["Spie\u00b7geln", "beim", "Blitz\u00b7schein"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPRART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.16": {"text": "Der schwarzen Pinien sturmschaukelnde Wipfel.", "tokens": ["Der", "schwar\u00b7zen", "Pi\u00b7ni\u00b7en", "sturm\u00b7schau\u00b7keln\u00b7de", "Wip\u00b7fel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.17": {"text": "So schreit ich manchmal,", "tokens": ["So", "schreit", "ich", "manch\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.18": {"text": "Kan\u00e4le, G\u00e4rten, Ein\u00f6de, H\u00fcgel", "tokens": ["Ka\u00b7n\u00e4\u00b7le", ",", "G\u00e4r\u00b7ten", ",", "Ein\u00b7\u00f6\u00b7de", ",", "H\u00fc\u00b7gel"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.19": {"text": "Zwischen den Schritten,", "tokens": ["Zwi\u00b7schen", "den", "Schrit\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.20": {"text": "Hin \u00fcber die Welt,", "tokens": ["Hin", "\u00fc\u00b7ber", "die", "Welt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.21": {"text": "Darin nichts Fremdes ist", "tokens": ["Da\u00b7rin", "nichts", "Frem\u00b7des", "ist"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "PIAT", "NN", "VAFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.22": {"text": "In solchen Stunden ...:", "tokens": ["In", "sol\u00b7chen", "Stun\u00b7den", "...", ":"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["APPR", "PIAT", "NN", "$(", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.23": {"text": "All Gegenwart,", "tokens": ["All", "Ge\u00b7gen\u00b7wart", ","], "token_info": ["word", "word", "punct"], "pos": ["PIAT", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.24": {"text": "All Sinn, all wie im Traum.", "tokens": ["All", "Sinn", ",", "all", "wie", "im", "Traum", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "KOKOM", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Da sa\u00dfest auch du", "tokens": ["Da", "sa\u00b7\u00dfest", "auch", "du"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "PPER"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Irgendwo", "tokens": ["Ir\u00b7gend\u00b7wo"], "token_info": ["word"], "pos": ["ADV"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "In meiner Welt", "tokens": ["In", "mei\u00b7ner", "Welt"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "\u00dcber Bogen und korinthischem Geb\u00e4lk", "tokens": ["\u00dc\u00b7ber", "Bo\u00b7gen", "und", "ko\u00b7rin\u00b7thi\u00b7schem", "Ge\u00b7b\u00e4lk"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "ADJA", "NN"], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Einer r\u00f6mischen Ruine", "tokens": ["Ei\u00b7ner", "r\u00f6\u00b7mi\u00b7schen", "Ru\u00b7i\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "In einem Vogelnest,", "tokens": ["In", "ei\u00b7nem", "Vo\u00b7gel\u00b7nest", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Einem Nest aus wilden Rosen und Schlingkraut,", "tokens": ["Ei\u00b7nem", "Nest", "aus", "wil\u00b7den", "Ro\u00b7sen", "und", "Schling\u00b7kraut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.8": {"text": "Um dich die leere Luft,", "tokens": ["Um", "dich", "die", "lee\u00b7re", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Allein, ein Hirtengott, ein Pan,", "tokens": ["Al\u00b7lein", ",", "ein", "Hir\u00b7ten\u00b7gott", ",", "ein", "Pan", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Und leuchtend unter dir die Lebensflur.", "tokens": ["Und", "leuch\u00b7tend", "un\u00b7ter", "dir", "die", "Le\u00b7bens\u00b7flur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und jetzt bist du daheim, nicht mehr ein Gott,", "tokens": ["Und", "jetzt", "bist", "du", "da\u00b7heim", ",", "nicht", "mehr", "ein", "Gott", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ADV", "$,", "PTKNEG", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "\u00bbim Schattenland, ein Schattenmann,", "tokens": ["\u00bb", "im", "Schat\u00b7ten\u00b7land", ",", "ein", "Schat\u00b7ten\u00b7mann", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "APPRART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der grauen Heimat \u00f6de Schollen tretend \u2013\u00ab", "tokens": ["Der", "grau\u00b7en", "Hei\u00b7mat", "\u00f6\u00b7de", "Schol\u00b7len",