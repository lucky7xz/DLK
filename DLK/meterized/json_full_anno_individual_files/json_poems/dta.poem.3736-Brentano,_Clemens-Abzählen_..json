{"dta.poem.3736": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Abz\u00e4hlen .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519172", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Eins, zwey, drey,               ", "tokens": ["Eins", ",", "zwey", ",", "drey", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "CARD", "$,", "CARD", "$,"], "meter": "+--", "measure": "dactylic.init"}, "line.2": {"text": "Bicke, borne hey,", "tokens": ["Bi\u00b7cke", ",", "bor\u00b7ne", "hey", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Bicke borne Pfefferkoren,", "tokens": ["Bi\u00b7cke", "bor\u00b7ne", "Pfef\u00b7fer\u00b7ko\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der M\u00fcller hat seine Frau verloren,", "tokens": ["Der", "M\u00fcl\u00b7ler", "hat", "sei\u00b7ne", "Frau", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VAFIN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "H\u00e4nschen hat sie g'funden.", "tokens": ["H\u00e4n\u00b7schen", "hat", "sie", "g'\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "VVINF", "$."], "meter": "+-++-+-", "measure": "unknown.measure.tetra"}, "line.6": {"text": "D' Katzen schlagen d' Tromme,", "tokens": ["D'", "Kat\u00b7zen", "schla\u00b7gen", "d'", "Trom\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["$(", "NN", "VVFIN", "NE", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "D' Maus kehren d' Stuben aus,", "tokens": ["D'", "Maus", "keh\u00b7ren", "d'", "Stu\u00b7ben", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "NE", "NN", "PTKVZ", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.8": {"text": "D' Ratten tragen den Dreck hinaus:", "tokens": ["D'", "Rat\u00b7ten", "tra\u00b7gen", "den", "Dreck", "hin\u00b7aus", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "'s sitzt ein M\u00e4nnel unter dem Dach,", "tokens": ["'s", "sitzt", "ein", "M\u00e4n\u00b7nel", "un\u00b7ter", "dem", "Dach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.10": {"text": "Hat sich bald zu krank gelacht.", "tokens": ["Hat", "sich", "bald", "zu", "krank", "ge\u00b7lacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PRF", "ADV", "PTKA", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}