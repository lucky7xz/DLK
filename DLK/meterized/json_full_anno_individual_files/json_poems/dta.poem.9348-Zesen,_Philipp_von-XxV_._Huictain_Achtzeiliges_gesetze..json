{"dta.poem.9348": {"metadata": {"author": {"name": "Zesen, Philipp von", "birth": "N.A.", "death": "N.A."}, "title": "XxV .  \n Huictain/ Achtzeiliges gesetze.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-20684-8", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was ist doch wohl dem Menschen zuvergleichen?", "tokens": ["Was", "ist", "doch", "wohl", "dem", "Men\u00b7schen", "zu\u00b7ver\u00b7glei\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der kleinen Welt/ was \u00fcbertrifft er nicht", "tokens": ["Der", "klei\u00b7nen", "Welt", "/", "was", "\u00fc\u00b7bert\u00b7rifft", "er", "nicht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$(", "PWS", "VVFIN", "PPER", "PTKNEG"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "An zier und schmuck? Die Sonne selbst mu\u00df weichen/", "tokens": ["An", "zier", "und", "schmuck", "?", "Die", "Son\u00b7ne", "selbst", "mu\u00df", "wei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADJD", "$.", "ART", "NN", "ADV", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "wo sich nur zeigt sein sch\u00f6nes Angesicht.", "tokens": ["wo", "sich", "nur", "zeigt", "sein", "sch\u00f6\u00b7nes", "An\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die Rose mu\u00df vor seinen Lippen bleichen/", "tokens": ["Die", "Ro\u00b7se", "mu\u00df", "vor", "sei\u00b7nen", "Lip\u00b7pen", "blei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "wie ist er doch so artlich zugericht?", "tokens": ["wie", "ist", "er", "doch", "so", "art\u00b7lich", "zu\u00b7ge\u00b7richt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "ein Ebenbild des H\u00f6chsten; doch mu\u00df sterben", "tokens": ["ein", "E\u00b7ben\u00b7bild", "des", "H\u00f6chs\u00b7ten", ";", "doch", "mu\u00df", "ster\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "$.", "ADV", "VMFIN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "fein edler Leib und in der grufft verderben.", "tokens": ["fein", "ed\u00b7ler", "Leib", "und", "in", "der", "grufft", "ver\u00b7der\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "KON", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}