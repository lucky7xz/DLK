{"textgrid.poem.42431": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Das war beredet und besprochen,", "genre": "verse", "period": "N.A.", "pub_year": 1876, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das war beredet und besprochen,", "tokens": ["Das", "war", "be\u00b7re\u00b7det", "und", "be\u00b7spro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "VVPP", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie lange her, ich ahn' es nicht.", "tokens": ["Wie", "lan\u00b7ge", "her", ",", "ich", "ahn'", "es", "nicht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PTKVZ", "$,", "PPER", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Tag ist da, die Pulse pochen,", "tokens": ["Der", "Tag", "ist", "da", ",", "die", "Pul\u00b7se", "po\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "$,", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Flocken fallen tr\u00e4g und dicht.", "tokens": ["Die", "Flo\u00b7cken", "fal\u00b7len", "tr\u00e4g", "und", "dicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Im fremden Dorf, im fremden Saale,", "tokens": ["Im", "frem\u00b7den", "Dorf", ",", "im", "frem\u00b7den", "Saa\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Es kennt uns keiner, welche Lust,", "tokens": ["Es", "kennt", "uns", "kei\u00b7ner", ",", "wel\u00b7che", "Lust", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIS", "$,", "PWAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wir drehn uns unter'm Kerzenstrahle,", "tokens": ["Wir", "drehn", "uns", "un\u00b7ter'm", "Ker\u00b7zen\u00b7strah\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Wie schwei\u00dft die Liebe Brust an Brust.", "tokens": ["Wie", "schwei\u00dft", "die", "Lie\u00b7be", "Brust", "an", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Und eng gedr\u00e4ngt im regen Schleifer,", "tokens": ["Und", "eng", "ge\u00b7dr\u00e4ngt", "im", "re\u00b7gen", "Schlei\u00b7fer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVPP", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Entz\u00fcnden wir uns mehr und mehr,", "tokens": ["Ent\u00b7z\u00fcn\u00b7den", "wir", "uns", "mehr", "und", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "KON", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich f\u00fchl's, ich bin Besitzergreifer,", "tokens": ["Ich", "f\u00fchl's", ",", "ich", "bin", "Be\u00b7sit\u00b7zer\u00b7grei\u00b7fer", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ich wei\u00df auch, das ist dein Begehr.", "tokens": ["Ich", "wei\u00df", "auch", ",", "das", "ist", "dein", "Be\u00b7gehr", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PDS", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Geheimnisvoller Schatten breitet", "tokens": ["Ge\u00b7heim\u00b7nis\u00b7vol\u00b7ler", "Schat\u00b7ten", "brei\u00b7tet"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sich \u00fcber unser Stelldichein,", "tokens": ["Sich", "\u00fc\u00b7ber", "un\u00b7ser", "Stell\u00b7dich\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "O komm, ein Zimmer liegt bereitet,", "tokens": ["O", "komm", ",", "ein", "Zim\u00b7mer", "liegt", "be\u00b7rei\u00b7tet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "ART", "NN", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ein traut Gemach, wir sind allein.", "tokens": ["Ein", "traut", "Ge\u00b7mach", ",", "wir", "sind", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "VVFIN", "NN", "$,", "PPER", "VAFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Der Wirt, mit artigem Verneigen,", "tokens": ["Der", "Wirt", ",", "mit", "ar\u00b7ti\u00b7gem", "Ver\u00b7nei\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "L\u00e4\u00dft uns hinein, w\u00fcnscht gute Nacht,", "tokens": ["L\u00e4\u00dft", "uns", "hin\u00b7ein", ",", "w\u00fcnscht", "gu\u00b7te", "Nacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$,", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Kein Sp\u00e4her horcht, die Sterne schweigen,", "tokens": ["Kein", "Sp\u00e4\u00b7her", "horcht", ",", "die", "Ster\u00b7ne", "schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und stumm ist rings die Winterpracht.", "tokens": ["Und", "stumm", "ist", "rings", "die", "Win\u00b7ter\u00b7pracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und wie beim Fest die Hochzeitsg\u00e4ste", "tokens": ["Und", "wie", "beim", "Fest", "die", "Hoch\u00b7zeits\u00b7g\u00e4s\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Noch weiter jubeln bei Musik,", "tokens": ["Noch", "wei\u00b7ter", "ju\u00b7beln", "bei", "Mu\u00b7sik", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVINF", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Verklingt, verhallt in unserm Neste", "tokens": ["Ver\u00b7klingt", ",", "ver\u00b7hallt", "in", "un\u00b7serm", "Nes\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Gejauchz und Violingequiek.", "tokens": ["Ge\u00b7jauchz", "und", "Vi\u00b7o\u00b7lin\u00b7ge\u00b7qui\u00b7ek", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wie bin ich schnell bei Band und Schnallen,", "tokens": ["Wie", "bin", "ich", "schnell", "bei", "Band", "und", "Schnal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADJD", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sie wehrt sich, sie verweigert's mir,", "tokens": ["Sie", "wehrt", "sich", ",", "sie", "ver\u00b7wei\u00b7gert's", "mir", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "$,", "PPER", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und ist mir um den Hals gefallen,", "tokens": ["Und", "ist", "mir", "um", "den", "Hals", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Verwirrung schlo\u00df die Augen ihr.", "tokens": ["Ver\u00b7wir\u00b7rung", "schlo\u00df", "die", "Au\u00b7gen", "ihr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "PPOSAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Noch str\u00e4ubt sie sich, schon f\u00e4llt die H\u00fclle,", "tokens": ["Noch", "str\u00e4ubt", "sie", "sich", ",", "schon", "f\u00e4llt", "die", "H\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "$,", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sie will nicht und sie mu\u00df, sie mu\u00df,", "tokens": ["Sie", "will", "nicht", "und", "sie", "mu\u00df", ",", "sie", "mu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "KON", "PPER", "VMFIN", "$,", "PPER", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und bringt mir ihre s\u00fc\u00dfe F\u00fclle,", "tokens": ["Und", "bringt", "mir", "ih\u00b7re", "s\u00fc\u00b7\u00dfe", "F\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und bringt sie mir in Glut und Ku\u00df.", "tokens": ["Und", "bringt", "sie", "mir", "in", "Glut", "und", "Ku\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Der Morgen naht in tiefer Stille,", "tokens": ["Der", "Mor\u00b7gen", "naht", "in", "tie\u00b7fer", "Stil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sie schl\u00e4ft ersch\u00f6pft im weichen Flaum,", "tokens": ["Sie", "schl\u00e4ft", "er\u00b7sch\u00f6pft", "im", "wei\u00b7chen", "Flaum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VVPP", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Noch drang nicht durch die Ladenrille", "tokens": ["Noch", "drang", "nicht", "durch", "die", "La\u00b7den\u00b7ril\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PTKNEG", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das Fr\u00fchrot in den heiligen Raum.", "tokens": ["Das", "Fr\u00fch\u00b7rot", "in", "den", "hei\u00b7li\u00b7gen", "Raum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Die Ampel gie\u00dft in D\u00e4mmermilde", "tokens": ["Die", "Am\u00b7pel", "gie\u00dft", "in", "D\u00e4m\u00b7mer\u00b7mil\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ein Zartlicht ihr um Brust und Arm,", "tokens": ["Ein", "Zart\u00b7licht", "ihr", "um", "Brust", "und", "Arm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und auf das himmlische Gebilde", "tokens": ["Und", "auf", "das", "himm\u00b7li\u00b7sche", "Ge\u00b7bil\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Sah l\u00e4chelnd ich und liebewarm.", "tokens": ["Sah", "l\u00e4\u00b7chelnd", "ich", "und", "lie\u00b7be\u00b7warm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPER", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Und eh' die Sonne sich erhoben,", "tokens": ["Und", "eh'", "die", "Son\u00b7ne", "sich", "er\u00b7ho\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sind wir schon unterwegs im Schnee,", "tokens": ["Sind", "wir", "schon", "un\u00b7ter\u00b7wegs", "im", "Schnee", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da hab' ich sie emporgehoben,", "tokens": ["Da", "hab'", "ich", "sie", "em\u00b7por\u00b7ge\u00b7ho\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und trug sie, ein verzognes Reh.", "tokens": ["Und", "trug", "sie", ",", "ein", "ver\u00b7zog\u00b7nes", "Reh", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und trug sie bis an ihre Kammer,", "tokens": ["Und", "trug", "sie", "bis", "an", "ih\u00b7re", "Kam\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "An's Erdenende th\u00e4t ich's noch,", "tokens": ["An's", "Er\u00b7de\u00b7nen\u00b7de", "th\u00e4t", "ich's", "noch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PIS", "ADV", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.7": {"text": "Sie aber wollte kaum die Klammer", "tokens": ["Sie", "a\u00b7ber", "woll\u00b7te", "kaum", "die", "Klam\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VMFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Entl\u00f6sen meinem Nackenjoch.", "tokens": ["Ent\u00b7l\u00f6\u00b7sen", "mei\u00b7nem", "Na\u00b7cken\u00b7joch", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Die erste Kr\u00e4he l\u00e4\u00dft sich h\u00f6ren,", "tokens": ["Die", "ers\u00b7te", "Kr\u00e4\u00b7he", "l\u00e4\u00dft", "sich", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Leb' wohl, mein Schatz, auf Wiedersehn.", "tokens": ["Leb'", "wohl", ",", "mein", "Schatz", ",", "auf", "Wie\u00b7der\u00b7sehn", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$,", "PPOSAT", "NN", "$,", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und durch die hochbeschneiten F\u00f6hren", "tokens": ["Und", "durch", "die", "hoch\u00b7be\u00b7schnei\u00b7ten", "F\u00f6h\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mu\u00df nun den Weg allein ich gehn.", "tokens": ["Mu\u00df", "nun", "den", "Weg", "al\u00b7lein", "ich", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "ADV", "PPER", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Sonne steigt, und tausend Funken", "tokens": ["Die", "Son\u00b7ne", "steigt", ",", "und", "tau\u00b7send", "Fun\u00b7ken"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "KON", "CARD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Durchglitzern das beeiste Feld.", "tokens": ["Durch\u00b7glit\u00b7zern", "das", "be\u00b7eis\u00b7te", "Feld", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Von Gl\u00fcck und Liebe bin ich trunken,", "tokens": ["Von", "Gl\u00fcck", "und", "Lie\u00b7be", "bin", "ich", "trun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "O Gott, wie herrlich ist die Welt.", "tokens": ["O", "Gott", ",", "wie", "herr\u00b7lich", "ist", "die", "Welt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PWAV", "ADJD", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Das war beredet und besprochen,", "tokens": ["Das", "war", "be\u00b7re\u00b7det", "und", "be\u00b7spro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "VVPP", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie lange her, ich ahn' es nicht.", "tokens": ["Wie", "lan\u00b7ge", "her", ",", "ich", "ahn'", "es", "nicht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PTKVZ", "$,", "PPER", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Tag ist da, die Pulse pochen,", "tokens": ["Der", "Tag", "ist", "da", ",", "die", "Pul\u00b7se", "po\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "$,", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Flocken fallen tr\u00e4g und dicht.", "tokens": ["Die", "Flo\u00b7cken", "fal\u00b7len", "tr\u00e4g", "und", "dicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Im fremden Dorf, im fremden Saale,", "tokens": ["Im", "frem\u00b7den", "Dorf", ",", "im", "frem\u00b7den", "Saa\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Es kennt uns keiner, welche Lust,", "tokens": ["Es", "kennt", "uns", "kei\u00b7ner", ",", "wel\u00b7che", "Lust", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIS", "$,", "PWAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wir drehn uns unter'm Kerzenstrahle,", "tokens": ["Wir", "drehn", "uns", "un\u00b7ter'm", "Ker\u00b7zen\u00b7strah\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Wie schwei\u00dft die Liebe Brust an Brust.", "tokens": ["Wie", "schwei\u00dft", "die", "Lie\u00b7be", "Brust", "an", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Und eng gedr\u00e4ngt im regen Schleifer,", "tokens": ["Und", "eng", "ge\u00b7dr\u00e4ngt", "im", "re\u00b7gen", "Schlei\u00b7fer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVPP", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Entz\u00fcnden wir uns mehr und mehr,", "tokens": ["Ent\u00b7z\u00fcn\u00b7den", "wir", "uns", "mehr", "und", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "KON", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich f\u00fchl's, ich bin Besitzergreifer,", "tokens": ["Ich", "f\u00fchl's", ",", "ich", "bin", "Be\u00b7sit\u00b7zer\u00b7grei\u00b7fer", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ich wei\u00df auch, das ist dein Begehr.", "tokens": ["Ich", "wei\u00df", "auch", ",", "das", "ist", "dein", "Be\u00b7gehr", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PDS", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Geheimnisvoller Schatten breitet", "tokens": ["Ge\u00b7heim\u00b7nis\u00b7vol\u00b7ler", "Schat\u00b7ten", "brei\u00b7tet"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sich \u00fcber unser Stelldichein,", "tokens": ["Sich", "\u00fc\u00b7ber", "un\u00b7ser", "Stell\u00b7dich\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "O komm, ein Zimmer liegt bereitet,", "tokens": ["O", "komm", ",", "ein", "Zim\u00b7mer", "liegt", "be\u00b7rei\u00b7tet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "ART", "NN", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ein traut Gemach, wir sind allein.", "tokens": ["Ein", "traut", "Ge\u00b7mach", ",", "wir", "sind", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "VVFIN", "NN", "$,", "PPER", "VAFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Der Wirt, mit artigem Verneigen,", "tokens": ["Der", "Wirt", ",", "mit", "ar\u00b7ti\u00b7gem", "Ver\u00b7nei\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "L\u00e4\u00dft uns hinein, w\u00fcnscht gute Nacht,", "tokens": ["L\u00e4\u00dft", "uns", "hin\u00b7ein", ",", "w\u00fcnscht", "gu\u00b7te", "Nacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$,", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Kein Sp\u00e4her horcht, die Sterne schweigen,", "tokens": ["Kein", "Sp\u00e4\u00b7her", "horcht", ",", "die", "Ster\u00b7ne", "schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und stumm ist rings die Winterpracht.", "tokens": ["Und", "stumm", "ist", "rings", "die", "Win\u00b7ter\u00b7pracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und wie beim Fest die Hochzeitsg\u00e4ste", "tokens": ["Und", "wie", "beim", "Fest", "die", "Hoch\u00b7zeits\u00b7g\u00e4s\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Noch weiter jubeln bei Musik,", "tokens": ["Noch", "wei\u00b7ter", "ju\u00b7beln", "bei", "Mu\u00b7sik", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVINF", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Verklingt, verhallt in unserm Neste", "tokens": ["Ver\u00b7klingt", ",", "ver\u00b7hallt", "in", "un\u00b7serm", "Nes\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Gejauchz und Violingequiek.", "tokens": ["Ge\u00b7jauchz", "und", "Vi\u00b7o\u00b7lin\u00b7ge\u00b7qui\u00b7ek", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Wie bin ich schnell bei Band und Schnallen,", "tokens": ["Wie", "bin", "ich", "schnell", "bei", "Band", "und", "Schnal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADJD", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sie wehrt sich, sie verweigert's mir,", "tokens": ["Sie", "wehrt", "sich", ",", "sie", "ver\u00b7wei\u00b7gert's", "mir", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "$,", "PPER", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und ist mir um den Hals gefallen,", "tokens": ["Und", "ist", "mir", "um", "den", "Hals", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Verwirrung schlo\u00df die Augen ihr.", "tokens": ["Ver\u00b7wir\u00b7rung", "schlo\u00df", "die", "Au\u00b7gen", "ihr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "PPOSAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Noch str\u00e4ubt sie sich, schon f\u00e4llt die H\u00fclle,", "tokens": ["Noch", "str\u00e4ubt", "sie", "sich", ",", "schon", "f\u00e4llt", "die", "H\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "$,", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sie will nicht und sie mu\u00df, sie mu\u00df,", "tokens": ["Sie", "will", "nicht", "und", "sie", "mu\u00df", ",", "sie", "mu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "KON", "PPER", "VMFIN", "$,", "PPER", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und bringt mir ihre s\u00fc\u00dfe F\u00fclle,", "tokens": ["Und", "bringt", "mir", "ih\u00b7re", "s\u00fc\u00b7\u00dfe", "F\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und bringt sie mir in Glut und Ku\u00df.", "tokens": ["Und", "bringt", "sie", "mir", "in", "Glut", "und", "Ku\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Der Morgen naht in tiefer Stille,", "tokens": ["Der", "Mor\u00b7gen", "naht", "in", "tie\u00b7fer", "Stil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sie schl\u00e4ft ersch\u00f6pft im weichen Flaum,", "tokens": ["Sie", "schl\u00e4ft", "er\u00b7sch\u00f6pft", "im", "wei\u00b7chen", "Flaum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VVPP", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Noch drang nicht durch die Ladenrille", "tokens": ["Noch", "drang", "nicht", "durch", "die", "La\u00b7den\u00b7ril\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PTKNEG", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das Fr\u00fchrot in den heiligen Raum.", "tokens": ["Das", "Fr\u00fch\u00b7rot", "in", "den", "hei\u00b7li\u00b7gen", "Raum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Die Ampel gie\u00dft in D\u00e4mmermilde", "tokens": ["Die", "Am\u00b7pel", "gie\u00dft", "in", "D\u00e4m\u00b7mer\u00b7mil\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ein Zartlicht ihr um Brust und Arm,", "tokens": ["Ein", "Zart\u00b7licht", "ihr", "um", "Brust", "und", "Arm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und auf das himmlische Gebilde", "tokens": ["Und", "auf", "das", "himm\u00b7li\u00b7sche", "Ge\u00b7bil\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Sah l\u00e4chelnd ich und liebewarm.", "tokens": ["Sah", "l\u00e4\u00b7chelnd", "ich", "und", "lie\u00b7be\u00b7warm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPER", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Und eh' die Sonne sich erhoben,", "tokens": ["Und", "eh'", "die", "Son\u00b7ne", "sich", "er\u00b7ho\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sind wir schon unterwegs im Schnee,", "tokens": ["Sind", "wir", "schon", "un\u00b7ter\u00b7wegs", "im", "Schnee", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da hab' ich sie emporgehoben,", "tokens": ["Da", "hab'", "ich", "sie", "em\u00b7por\u00b7ge\u00b7ho\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und trug sie, ein verzognes Reh.", "tokens": ["Und", "trug", "sie", ",", "ein", "ver\u00b7zog\u00b7nes", "Reh", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und trug sie bis an ihre Kammer,", "tokens": ["Und", "trug", "sie", "bis", "an", "ih\u00b7re", "Kam\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "An's Erdenende th\u00e4t ich's noch,", "tokens": ["An's", "Er\u00b7de\u00b7nen\u00b7de", "th\u00e4t", "ich's", "noch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PIS", "ADV", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.7": {"text": "Sie aber wollte kaum die Klammer", "tokens": ["Sie", "a\u00b7ber", "woll\u00b7te", "kaum", "die", "Klam\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VMFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Entl\u00f6sen meinem Nackenjoch.", "tokens": ["Ent\u00b7l\u00f6\u00b7sen", "mei\u00b7nem", "Na\u00b7cken\u00b7joch", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "Die erste Kr\u00e4he l\u00e4\u00dft sich h\u00f6ren,", "tokens": ["Die", "ers\u00b7te", "Kr\u00e4\u00b7he", "l\u00e4\u00dft", "sich", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Leb' wohl, mein Schatz, auf Wiedersehn.", "tokens": ["Leb'", "wohl", ",", "mein", "Schatz", ",", "auf", "Wie\u00b7der\u00b7sehn", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$,", "PPOSAT", "NN", "$,", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und durch die hochbeschneiten F\u00f6hren", "tokens": ["Und", "durch", "die", "hoch\u00b7be\u00b7schnei\u00b7ten", "F\u00f6h\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mu\u00df nun den Weg allein ich gehn.", "tokens": ["Mu\u00df", "nun", "den", "Weg", "al\u00b7lein", "ich", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "ADV", "PPER", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Sonne steigt, und tausend Funken", "tokens": ["Die", "Son\u00b7ne", "steigt", ",", "und", "tau\u00b7send", "Fun\u00b7ken"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "KON", "CARD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Durchglitzern das beeiste Feld.", "tokens": ["Durch\u00b7glit\u00b7zern", "das", "be\u00b7eis\u00b7te", "Feld", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Von Gl\u00fcck und Liebe bin ich trunken,", "tokens": ["Von", "Gl\u00fcck", "und", "Lie\u00b7be", "bin", "ich", "trun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "O Gott, wie herrlich ist die Welt.", "tokens": ["O", "Gott", ",", "wie", "herr\u00b7lich", "ist", "die", "Welt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PWAV", "ADJD", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}