word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Den blauen, lichten Sternenraum,", "tokens": ["Den", "blau\u00b7en", ",", "lich\u00b7ten", "Ster\u00b7nen\u00b7raum", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Erde Blumenfeld \u2013", "tokens": ["Der", "Er\u00b7de", "Blu\u00b7men\u00b7feld", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Da war mein Jammer nur ein Traum,", "tokens": ["Da", "war", "mein", "Jam\u00b7mer", "nur", "ein", "Traum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die Welt die beste Welt.", "tokens": ["Die", "Welt", "die", "bes\u00b7te", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Drum dank' ich dem, der Wunder tut", "tokens": ["Drum", "dank'", "ich", "dem", ",", "der", "Wun\u00b7der", "tut"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "ART", "$,", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und G\u00fcte f\u00fcr und f\u00fcr,", "tokens": ["Und", "G\u00fc\u00b7te", "f\u00fcr", "und", "f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "KON", "APPR", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es rieselt jeder Tropfen Blut", "tokens": ["Es", "rie\u00b7selt", "je\u00b