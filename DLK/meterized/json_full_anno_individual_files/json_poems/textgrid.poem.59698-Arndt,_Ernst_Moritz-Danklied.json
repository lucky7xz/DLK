": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "L\u00e4\u00dft seine Treue nimmer los,", "tokens": ["L\u00e4\u00dft", "sei\u00b7ne", "Treu\u00b7e", "nim\u00b7mer", "los", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die alles selig tr\u00e4gt,", "tokens": ["Die", "al\u00b7les", "se\u00b7lig", "tr\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und seine Liebe lockt so s\u00fc\u00df,", "tokens": ["Und", "sei\u00b7ne", "Lie\u00b7be", "lockt", "so", "s\u00fc\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Was Liebe mag verstehn,", "tokens": ["Was", "Lie\u00b7be", "mag", "ver\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Da\u00df wir zu ihm ins Paradies", "tokens": ["Da\u00df", "wir", "zu", "ihm", "ins", "Pa\u00b7ra\u00b7dies"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der Lust und Unschuld gehn.", "tokens": ["Der", "Lust", "und", "Un\u00b7schuld", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}