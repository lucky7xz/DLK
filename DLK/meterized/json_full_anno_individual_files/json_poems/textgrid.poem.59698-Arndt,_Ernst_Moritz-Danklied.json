{"textgrid.poem.59698": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "Danklied", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Frischauf, mein Herz, und werde Klang!", "tokens": ["Frisc\u00b7hauf", ",", "mein", "Herz", ",", "und", "wer\u00b7de", "Klang", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "NN", "$,", "KON", "VAFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und, Seele, werde Lied!", "tokens": ["Und", ",", "See\u00b7le", ",", "wer\u00b7de", "Lied", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "$,", "NN", "$,", "VAFIN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und, Freude, t\u00f6ne Lobgesang,", "tokens": ["Und", ",", "Freu\u00b7de", ",", "t\u00f6\u00b7ne", "Lob\u00b7ge\u00b7sang", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "$,", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der mir im Busen gl\u00fcht!", "tokens": ["Der", "mir", "im", "Bu\u00b7sen", "gl\u00fcht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Denn er, der alle Himmel rollt", "tokens": ["Denn", "er", ",", "der", "al\u00b7le", "Him\u00b7mel", "rollt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "$,", "PRELS", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und z\u00e4hlt das Sternenheer,", "tokens": ["Und", "z\u00e4hlt", "das", "Ster\u00b7nen\u00b7heer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Denn Gott, der Vater fromm und hold,", "tokens": ["Denn", "Gott", ",", "der", "Va\u00b7ter", "fromm", "und", "hold", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "ART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Verl\u00e4\u00dft mich nimmermehr.", "tokens": ["Ver\u00b7l\u00e4\u00dft", "mich", "nim\u00b7mer\u00b7mehr", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich lag, umh\u00fcllt mit Finsternis,", "tokens": ["Ich", "lag", ",", "um\u00b7h\u00fcllt", "mit", "Fins\u00b7ter\u00b7nis", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die aus der H\u00f6lle kam,", "tokens": ["Die", "aus", "der", "H\u00f6l\u00b7le", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und durch die tiefste Seele ri\u00df", "tokens": ["Und", "durch", "die", "tiefs\u00b7te", "See\u00b7le", "ri\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Tigerklaun der Gram,", "tokens": ["Mit", "Ti\u00b7ger\u00b7klaun", "der", "Gram", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Gebrochen war mir alle Kraft,", "tokens": ["Ge\u00b7bro\u00b7chen", "war", "mir", "al\u00b7le", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Erloschen aller Mut,", "tokens": ["Er\u00b7lo\u00b7schen", "al\u00b7ler", "Mut", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Da rief ich dem, der alles schafft:", "tokens": ["Da", "rief", "ich", "dem", ",", "der", "al\u00b7les", "schafft", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "$,", "PRELS", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mach's, Vater, mach' es gut!", "tokens": ["Mach's", ",", "Va\u00b7ter", ",", "mach'", "es", "gut", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und pl\u00f6tzlich ward die Nacht zu Licht,", "tokens": ["Und", "pl\u00f6tz\u00b7lich", "ward", "die", "Nacht", "zu", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zur Wonne ward das Leid,", "tokens": ["Zur", "Won\u00b7ne", "ward", "das", "Leid", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und wieder schaut' ich aufgericht't", "tokens": ["Und", "wie\u00b7der", "schaut'", "ich", "auf\u00b7ge\u00b7richt't"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des Lebens Herrlichkeit,", "tokens": ["Des", "Le\u00b7bens", "Herr\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Den blauen, lichten Sternenraum,", "tokens": ["Den", "blau\u00b7en", ",", "lich\u00b7ten", "Ster\u00b7nen\u00b7raum", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Erde Blumenfeld \u2013", "tokens": ["Der", "Er\u00b7de", "Blu\u00b7men\u00b7feld", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Da war mein Jammer nur ein Traum,", "tokens": ["Da", "war", "mein", "Jam\u00b7mer", "nur", "ein", "Traum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die Welt die beste Welt.", "tokens": ["Die", "Welt", "die", "bes\u00b7te", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Drum dank' ich dem, der Wunder tut", "tokens": ["Drum", "dank'", "ich", "dem", ",", "der", "Wun\u00b7der", "tut"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "ART", "$,", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und G\u00fcte f\u00fcr und f\u00fcr,", "tokens": ["Und", "G\u00fc\u00b7te", "f\u00fcr", "und", "f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "KON", "APPR", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es rieselt jeder Tropfen Blut", "tokens": ["Es", "rie\u00b7selt", "je\u00b7der", "Trop\u00b7fen", "Blut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Lobgesang in mir,", "tokens": ["Den", "Lob\u00b7ge\u00b7sang", "in", "mir", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Es wird ein jeder Blick ein Strahl,", "tokens": ["Es", "wird", "ein", "je\u00b7der", "Blick", "ein", "Strahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "PIAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der auf gen Himmel dringt,", "tokens": ["Der", "auf", "gen", "Him\u00b7mel", "dringt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und tausend, tausend, tausend Mal", "tokens": ["Und", "tau\u00b7send", ",", "tau\u00b7send", ",", "tau\u00b7send", "Mal"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["KON", "ADJD", "$,", "VVPP", "$,", "CARD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Das ", "tokens": ["Das"], "token_info": ["word"], "pos": ["PDS"], "meter": "-", "measure": "single.down"}}, "stanza.5": {"line.1": {"text": "Denn wie die Kindlein in dem Scho\u00df", "tokens": ["Denn", "wie", "die", "Kin\u00b7dlein", "in", "dem", "Scho\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die treue Mutter hegt,", "tokens": ["Die", "treu\u00b7e", "Mut\u00b7ter", "hegt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "L\u00e4\u00dft seine Treue nimmer los,", "tokens": ["L\u00e4\u00dft", "sei\u00b7ne", "Treu\u00b7e", "nim\u00b7mer", "los", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die alles selig tr\u00e4gt,", "tokens": ["Die", "al\u00b7les", "se\u00b7lig", "tr\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und seine Liebe lockt so s\u00fc\u00df,", "tokens": ["Und", "sei\u00b7ne", "Lie\u00b7be", "lockt", "so", "s\u00fc\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Was Liebe mag verstehn,", "tokens": ["Was", "Lie\u00b7be", "mag", "ver\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Da\u00df wir zu ihm ins Paradies", "tokens": ["Da\u00df", "wir", "zu", "ihm", "ins", "Pa\u00b7ra\u00b7dies"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der Lust und Unschuld gehn.", "tokens": ["Der", "Lust", "und", "Un\u00b7schuld", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVINF