{"textgrid.poem.39962": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "3. Wie drei K\u00f6nige sich aufmachten, dem Sterne nachzuziehen", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Drei K\u00f6n'ge machten da sich auf,", "tokens": ["Drei", "K\u00f6n'\u00b7ge", "mach\u00b7ten", "da", "sich", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "ADV", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "(doch keiner wu\u00dfte von dem andern),", "tokens": ["(", "doch", "kei\u00b7ner", "wu\u00df\u00b7te", "von", "dem", "an\u00b7dern", ")", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "PIS", "VVFIN", "APPR", "ART", "ADJA", "$(", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die merkten auf des Sternes Lauf,", "tokens": ["Die", "merk\u00b7ten", "auf", "des", "Ster\u00b7nes", "Lauf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und huben an mit ihm zu wandern.", "tokens": ["Und", "hu\u00b7ben", "an", "mit", "ihm", "zu", "wan\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "APPR", "PPER", "PTKZU", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Schon lange harrten sie des Herrn,", "tokens": ["Schon", "lan\u00b7ge", "harr\u00b7ten", "sie", "des", "Herrn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den des Propheten Wort verk\u00fcndet,", "tokens": ["Den", "des", "Pro\u00b7phe\u00b7ten", "Wort", "ver\u00b7k\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "NN", "VVPP", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Der Sehnsucht Funken hat der Stern", "tokens": ["Der", "Sehn\u00b7sucht", "Fun\u00b7ken", "hat", "der", "Stern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Zur lichten Flamme jetzt entz\u00fcndet.", "tokens": ["Zur", "lich\u00b7ten", "Flam\u00b7me", "jetzt", "ent\u00b7z\u00fcn\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ein jeder nun bereitet sich", "tokens": ["Ein", "je\u00b7der", "nun", "be\u00b7rei\u00b7tet", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PIS", "ADV", "VVFIN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In den drei fern geschiednen Landen", "tokens": ["In", "den", "drei", "fern", "ge\u00b7schied\u00b7nen", "Lan\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "CARD", "ADJD", "ADJA", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Mit Opfern, Gaben, k\u00f6niglich,", "tokens": ["Mit", "Op\u00b7fern", ",", "Ga\u00b7ben", ",", "k\u00f6\u00b7nig\u00b7lich", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zierraten, k\u00f6stlichen Gewanden.", "tokens": ["Zier\u00b7ra\u00b7ten", ",", "k\u00f6st\u00b7li\u00b7chen", "Ge\u00b7wan\u00b7den", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJA", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Und M\u00e4uler und Kameele dr\u00fcckt", "tokens": ["Und", "M\u00e4u\u00b7ler", "und", "Ka\u00b7mee\u00b7le", "dr\u00fcckt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Last der aufgeladnen G\u00fcter,", "tokens": ["Die", "Last", "der", "auf\u00b7ge\u00b7lad\u00b7nen", "G\u00fc\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Manch gutes Saumro\u00df geht geb\u00fcckt,", "tokens": ["Manch", "gu\u00b7tes", "Saum\u00b7ro\u00df", "geht", "ge\u00b7b\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und nebenher die Schar der H\u00fcter.", "tokens": ["Und", "ne\u00b7ben\u00b7her", "die", "Schar", "der", "H\u00fc\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und jeder, neben andrem Gut,", "tokens": ["Und", "je\u00b7der", ",", "ne\u00b7ben", "an\u00b7drem", "Gut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nimmt seines Landes eigne Gaben;", "tokens": ["Nimmt", "sei\u00b7nes", "Lan\u00b7des", "eig\u00b7ne", "Ga\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Des Golds und der Gesteine Glut", "tokens": ["Des", "Golds", "und", "der", "Ge\u00b7stei\u00b7ne", "Glut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sucht aus der K\u00f6nig der ", "tokens": ["Sucht", "aus", "der", "K\u00f6\u00b7nig", "der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "ART"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Der Herr von ", "tokens": ["Der", "Herr", "von"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "APPR"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Des edlen Weihrauchs aus dem Baume,", "tokens": ["Des", "ed\u00b7len", "Weih\u00b7rauchs", "aus", "dem", "Bau\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Dem dunkeln Myrrhenkraut entrafft", "tokens": ["Dem", "dun\u00b7keln", "Myr\u00b7rhen\u00b7kraut", "en\u00b7trafft"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}}, "stanza.4": {"line.1": {"text": "Was zu des Leibes Notdurft frommt,", "tokens": ["Was", "zu", "des", "Lei\u00b7bes", "Not\u00b7durft", "frommt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "L\u00e4dt jeder auf, zur langen Reise;", "tokens": ["L\u00e4dt", "je\u00b7der", "auf", ",", "zur", "lan\u00b7gen", "Rei\u00b7se", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PTKVZ", "$,", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbvon ", "tokens": ["\u00bb", "von"], "token_info": ["punct", "word"], "pos": ["$(", "APPR"], "meter": "-", "measure": "single.down"}, "line.4": {"text": "Der Herr der Herren!\u00ab sprach der Weise.", "tokens": ["Der", "Herr", "der", "Her\u00b7ren", "!", "\u00ab", "sprach", "der", "Wei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$.", "$(", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Dorthin zieht sie das Sterngebild,", "tokens": ["Dor\u00b7thin", "zieht", "sie", "das", "Stern\u00b7ge\u00b7bild", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Doch wei\u00df es keiner von dem andern:", "tokens": ["Doch", "wei\u00df", "es", "kei\u00b7ner", "von", "dem", "an\u00b7dern", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PIS", "APPR", "ART", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Ein\u00f6de voll Gew\u00fcrm und Wild", "tokens": ["Ein\u00b7\u00f6\u00b7de", "voll", "Ge\u00b7w\u00fcrm", "und", "Wild"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADJD", "NN", "KON", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Trennt ihre Pfade, die sie wandern.", "tokens": ["Trennt", "ih\u00b7re", "Pfa\u00b7de", ",", "die", "sie", "wan\u00b7dern", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Sie r\u00fcsten gro\u00dfe Heeresmacht,", "tokens": ["Sie", "r\u00fcs\u00b7ten", "gro\u00b7\u00dfe", "Hee\u00b7res\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Neugebornen zu empfangen,", "tokens": ["Den", "Neu\u00b7ge\u00b7bor\u00b7nen", "zu", "emp\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sie sehn im Geiste schon die Pracht", "tokens": ["Sie", "sehn", "im", "Geis\u00b7te", "schon", "die", "Pracht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der k\u00f6niglichen Hofburg prangen;", "tokens": ["Der", "k\u00f6\u00b7nig\u00b7li\u00b7chen", "Hof\u00b7burg", "pran\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Sie baun im Geiste den Palast,", "tokens": ["Sie", "baun", "im", "Geis\u00b7te", "den", "Pa\u00b7last", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das Cedernthor, die Marmelstiege; \u2013", "tokens": ["Das", "Ce\u00b7dern\u00b7thor", ",", "die", "Mar\u00b7mel\u00b7stie\u00b7ge", ";", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und drinnen schl\u00e4ft in Duft und Glast", "tokens": ["Und", "drin\u00b7nen", "schl\u00e4ft", "in", "Duft", "und", "Glast"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der K\u00f6nigssohn in goldner Wiege.", "tokens": ["Der", "K\u00f6\u00b7nigs\u00b7sohn", "in", "gold\u00b7ner", "Wie\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Denn solch' und gr\u00f6\u00dfre Herrlichkeit", "tokens": ["Denn", "solch'", "und", "gr\u00f6\u00df\u00b7re", "Herr\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "KON", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Verspricht der Stern, der golden leuchtet,", "tokens": ["Ver\u00b7spricht", "der", "Stern", ",", "der", "gol\u00b7den", "leuch\u00b7tet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "ART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und all das funkelnde Geleit", "tokens": ["Und", "all", "das", "fun\u00b7keln\u00b7de", "Ge\u00b7leit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit seines Lichtes Thau befeuchtet;", "tokens": ["Mit", "sei\u00b7nes", "Lich\u00b7tes", "Thau", "be\u00b7feuch\u00b7tet", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wo s