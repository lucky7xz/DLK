{"dta.poem.5805": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Gesetzen der Bewegung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Durch diesen klaren Grund wird jeder \u00fcberf\u00fchret:", "tokens": ["Durch", "die\u00b7sen", "kla\u00b7ren", "Grund", "wird", "je\u00b7der", "\u00fc\u00b7berf\u00b7\u00fch\u00b7ret", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "VAFIN", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Kein C\u00f6rper finder sich, der von sich selbst sich", "tokens": ["Kein", "C\u00f6r\u00b7per", "fin\u00b7der", "sich", ",", "der", "von", "sich", "selbst", "sich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "PRF", "$,", "PRELS", "APPR", "PRF", "ADV", "PRF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "r\u00fchret.", "tokens": ["r\u00fch\u00b7ret", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "F\u00e4ngt einer nun sich zu bewegen", "tokens": ["F\u00e4ngt", "ei\u00b7ner", "nun", "sich", "zu", "be\u00b7we\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADV", "PRF", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Durch einen andern an; so mu\u00df man \u00fcberlegen,", "tokens": ["Durch", "ei\u00b7nen", "an\u00b7dern", "an", ";", "so", "mu\u00df", "man", "\u00fc\u00b7berl\u00b7e\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "PTKVZ", "$.", "ADV", "VMFIN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und Stuffenweis\u2019 erwegen,", "tokens": ["Und", "Stuf\u00b7fen\u00b7weis'", "er\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Woher der Erstere dasjenige bekommen,", "tokens": ["Wo\u00b7her", "der", "Ers\u00b7te\u00b7re", "das\u00b7je\u00b7ni\u00b7ge", "be\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PDS", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Was er dem andern gab. So wird durch den Verstand", "tokens": ["Was", "er", "dem", "an\u00b7dern", "gab", ".", "So", "wird", "durch", "den", "Ver\u00b7stand"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "ART", "ADJA", "VVFIN", "$.", "ADV", "VAFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ein Erstes Wesen gleich erkannt,", "tokens": ["Ein", "Ers\u00b7tes", "We\u00b7sen", "gleich", "er\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Von Welchem alle Ding\u2019 ihr Wesen hergenommen,", "tokens": ["Von", "Wel\u00b7chem", "al\u00b7le", "Ding'", "ihr", "We\u00b7sen", "her\u00b7ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PIAT", "NN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Durch Welches sie bestehn, Das die Natur gemacht,", "tokens": ["Durch", "Wel\u00b7ches", "sie", "be\u00b7stehn", ",", "Das", "die", "Na\u00b7tur", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWS", "PPER", "VVINF", "$,", "PDS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und Welches sie erh\u00e4lt.", "tokens": ["Und", "Wel\u00b7ches", "sie", "er\u00b7h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}