{"textgrid.poem.34980": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "O Hoffmann, deutscher Brutus", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Hoffmann, deutscher Brutus,", "tokens": ["O", "Hoff\u00b7mann", ",", "deut\u00b7scher", "Bru\u00b7tus", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PRELS", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie bist du mutig und k\u00fchn,", "tokens": ["Wie", "bist", "du", "mu\u00b7tig", "und", "k\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Du setzest L\u00e4use den F\u00fcrsten", "tokens": ["Du", "set\u00b7zest", "L\u00e4u\u00b7se", "den", "F\u00fcrs\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "In den Pelz, in den Hermelin.", "tokens": ["In", "den", "Pelz", ",", "in", "den", "Her\u00b7me\u00b7lin", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Und wen es juckt, der kratzt sich,", "tokens": ["Und", "wen", "es", "juckt", ",", "der", "kratzt", "sich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVFIN", "$,", "PRELS", "VVFIN", "PRF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sie kratzen sich endlich tot,", "tokens": ["Sie", "krat\u00b7zen", "sich", "en