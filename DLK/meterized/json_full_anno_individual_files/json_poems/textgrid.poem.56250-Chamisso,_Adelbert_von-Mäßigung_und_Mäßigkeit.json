{"textgrid.poem.56250": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "M\u00e4\u00dfigung und M\u00e4\u00dfigkeit", "genre": "verse", "period": "N.A.", "pub_year": 1809, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "La\u00dft das Wort uns geben heute,", "tokens": ["La\u00dft", "das", "Wort", "uns", "ge\u00b7ben", "heu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "PPER", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Uns vom Trunke zu entw\u00f6hnen;", "tokens": ["Uns", "vom", "Trun\u00b7ke", "zu", "ent\u00b7w\u00f6h\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ziemt sich's f\u00fcr gesetzte Leute,", "tokens": ["Ziemt", "sich's", "f\u00fcr", "ge\u00b7setz\u00b7te", "Leu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "W\u00fcster V\u00f6llerei zu fr\u00f6nen?", "tokens": ["W\u00fcs\u00b7ter", "V\u00f6l\u00b7le\u00b7rei", "zu", "fr\u00f6\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Nein, es ziemt sich Sittsamkeit.", "tokens": ["Nein", ",", "es", "ziemt", "sich", "Sitt\u00b7sam\u00b7keit", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "PRF", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Gutes Beispiel will ich geben:", "tokens": ["Gu\u00b7tes", "Bei\u00b7spiel", "will", "ich", "ge\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "M\u00e4\u00dfigung und M\u00e4\u00dfigkeit! \u2013", "tokens": ["M\u00e4\u00b7\u00dfi\u00b7gung", "und", "M\u00e4\u00b7\u00dfig\u00b7keit", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["NN", "KON", "NN", "$.", "$("], "meter": "----+-+", "measure": "unknown.measure.di"}, "line.8": {"text": "Sto\u00dfet an, sie sollen leben! \u2013", "tokens": ["Sto\u00b7\u00dfet", "an", ",", "sie", "sol\u00b7len", "le\u00b7ben", "!", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "PPER", "VMFIN", "VVINF", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "M\u00e4\u00dfigung und M\u00e4\u00dfigkeit! \u2013", "tokens": ["M\u00e4\u00b7\u00dfi\u00b7gung", "und", "M\u00e4\u00b7\u00dfig\u00b7keit", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["NN", "KON", "NN", "$.", "$("], "meter": "----+-+", "measure": "unknown.measure.di"}, "line.10": {"text": "Ma\u00df! Ma\u00df!", "tokens": ["Ma\u00df", "!", "Ma\u00df", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$.", "NN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.11": {"text": "Leert darauf das volle Glas!", "tokens": ["Leert", "da\u00b7rauf", "das", "vol\u00b7le", "Glas", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PAV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Seht, ein Glas ist Gottes Gabe,", "tokens": ["Seht", ",", "ein", "Glas", "ist", "Got\u00b7tes", "Ga\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ART", "NN", "VAFIN", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und das zweite stimmt uns lyrisch;", "tokens": ["Und", "das", "zwei\u00b7te", "stimmt", "uns", "ly\u00b7risch", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "VVFIN", "PPER", "ADJD", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Wenn ich gegen drei nichts habe,", "tokens": ["Wenn", "ich", "ge\u00b7gen", "drei", "nichts", "ha\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "CARD", "PIS", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Machen viele doch uns tierisch;", "tokens": ["Ma\u00b7chen", "vie\u00b7le", "doch", "uns", "tie\u00b7risch", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "PPER", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Trinket mehr nicht als genung!", "tokens": ["Trin\u00b7ket", "mehr", "nicht", "als", "ge\u00b7nung", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PTKNEG", "KOUS", "NN", "$."], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.6": {"text": "Und mein Lied will ich euch singen:", "tokens": ["Und", "mein", "Lied", "will", "ich", "euch", "sin\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VMFIN", "PPER", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "M\u00e4\u00dfigkeit und M\u00e4\u00dfigung! \u2013", "tokens": ["M\u00e4\u00b7\u00dfig\u00b7keit", "und", "M\u00e4\u00b7\u00dfi\u00b7gung", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["NN", "KON", "NN", "$.", "$("], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.8": {"text": "La\u00dft die vollen Gl\u00e4ser klingen! \u2013", "tokens": ["La\u00dft", "die", "vol\u00b7len", "Gl\u00e4\u00b7ser", "klin\u00b7gen", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVIMP", "ART", "ADJA", "NN", "VVINF", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "M\u00e4\u00dfigkeit und M\u00e4\u00dfigung!", "tokens": ["M\u00e4\u00b7\u00dfig\u00b7keit", "und", "M\u00e4\u00b7\u00dfi\u00b7gung", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.10": {"text": "Ma\u00df! Ma\u00df!", "tokens": ["Ma\u00df", "!", "Ma\u00df", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$.", "NN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.11": {"text": "Leert darauf das volle Glas!", "tokens": ["Leert", "da\u00b7rauf", "das", "vol\u00b7le", "Glas", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PAV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Seht den Trunkenbold in schr\u00e4gen", "tokens": ["Seht", "den", "Trun\u00b7ken\u00b7bold", "in", "schr\u00e4\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "APPR", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Linien durch die Gassen wanken;", "tokens": ["Li\u00b7ni\u00b7en", "durch", "die", "Gas\u00b7sen", "wan\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Kommt die Hausfrau ihm entgegen,", "tokens": ["Kommt", "die", "Haus\u00b7frau", "ihm", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "PTKVZ", "$,"], "meter": "+-++--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "H\u00f6rt sie keifen, h\u00f6rt sie zanken;", "tokens": ["H\u00f6rt", "sie", "kei\u00b7fen", ",", "h\u00f6rt", "sie", "zan\u00b7ken", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVINF", "$,", "VVFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Das verdient Beherzigung.", "tokens": ["Das", "ver\u00b7dient", "Be\u00b7her\u00b7zi\u00b7gung", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "NN", "$."], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.6": {"text": "La\u00dft uns an der Tugend haften:", "tokens": ["La\u00dft", "uns", "an", "der", "Tu\u00b7gend", "haf\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "M\u00e4\u00dfigkeit und M\u00e4\u00dfigung!", "tokens": ["M\u00e4\u00b7\u00dfig\u00b7keit", "und", "M\u00e4\u00b7\u00dfi\u00b7gung", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.8": {"text": "Pereant die Lasterhaften;", "tokens": ["Pe\u00b7re\u00b7ant", "die", "Las\u00b7ter\u00b7haf\u00b7ten", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "M\u00e4\u00dfigkeit und M\u00e4\u00dfigung!", "tokens": ["M\u00e4\u00b7\u00dfig\u00b7keit", "und", "M\u00e4\u00b7\u00dfi\u00b7gung", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.10": {"text": "Ma\u00df! Ma\u00df!", "tokens": ["Ma\u00df", "!", "Ma\u00df", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$.", "NN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.11": {"text": "Leert darauf das volle Glas!", "tokens": ["Leert", "da\u00b7rauf", "das", "vol\u00b7le", "Glas", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PAV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Was hast, Schlingel, du zu lachen?", "tokens": ["Was", "hast", ",", "Schlin\u00b7gel", ",", "du", "zu", "la\u00b7chen", "?"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "$,", "NN", "$,", "PPER", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Will das Lachen dir vertreiben;", "tokens": ["Will", "das", "La\u00b7chen", "dir", "ver\u00b7trei\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dich moralisch auch zu machen,", "tokens": ["Dich", "mo\u00b7ra\u00b7lisch", "auch", "zu", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dir die Ohren t\u00fcchtig reiben,", "tokens": ["Dir", "die", "Oh\u00b7ren", "t\u00fcch\u00b7tig", "rei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Pack dich fort bei guter Zeit!", "tokens": ["Pack", "dich", "fort", "bei", "gu\u00b7ter", "Zeit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PTKVZ", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Doch ich will mich nicht erbo\u00dfen:", "tokens": ["Doch", "ich", "will", "mich", "nicht", "er\u00b7bo\u00b7\u00dfen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "M\u00e4\u00dfigung und M\u00e4\u00dfigkeit! \u2013", "tokens": ["M\u00e4\u00b7\u00dfi\u00b7gung", "und", "M\u00e4\u00b7\u00dfig\u00b7keit", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["NN", "KON", "NN", "$.", "$("], "meter": "----+-+", "measure": "unknown.measure.di"}, "line.8": {"text": "Eingeschenkt und angesto\u00dfen! \u2013", "tokens": ["Ein\u00b7ge\u00b7schenkt", "und", "an\u00b7ge\u00b7sto\u00b7\u00dfen", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["VVPP", "KON", "VVPP", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "M\u00e4\u00dfigung und M\u00e4\u00dfigkeit!", "tokens": ["M\u00e4\u00b7\u00dfi\u00b7gung", "und", "M\u00e4\u00b7\u00dfig\u00b7keit", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "----+-+", "measure": "unknown.measure.di"}, "line.10": {"text": "Ma\u00df! Ma\u00df!", "tokens": ["Ma\u00df", "!", "Ma\u00df", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$.", "NN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.11": {"text": "Leert darauf das volle Glas!", "tokens": ["Leert", "da\u00b7rauf", "das", "vol\u00b7le", "Glas", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PAV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Modus, ut nos docuere,", "tokens": ["Mo\u00b7dus", ",", "ut", "nos", "do\u00b7cu\u00b7e\u00b7re", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Sit in rebus, sumus rati;", "tokens": ["Sit", "in", "re\u00b7bus", ",", "su\u00b7mus", "ra\u00b7ti", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$,", "FM.la", "FM.la", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Medium qui tenuere", "tokens": ["Me\u00b7di\u00b7um", "qui", "te\u00b7nu\u00b7e\u00b7re"], "token_info": ["word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Nominati sunt beati;", "tokens": ["No\u00b7mi\u00b7na\u00b7ti", "sunt", "be\u00b7a\u00b7ti", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "C'est le juste Milieu zur Zeit!", "tokens": ["C'\u00b7est", "le", "jus\u00b7te", "Mi\u00b7li\u00b7eu", "zur", "Zeit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ergo! Ergel! \u2013 Deutsch gesprochen:", "tokens": ["Er\u00b7go", "!", "Er\u00b7gel", "!", "\u2013", "Deutsch", "ge\u00b7spro\u00b7chen", ":"], "token_info": ["word", "punct", "word", "punct", "punct", "word", "word", "punct"], "pos": ["NE", "$.", "NN", "$.", "$(", "NN", "VVPP", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "M\u00e4\u00dfigung und M\u00e4\u00dfigkeit! \u2013", "tokens": ["M\u00e4\u00b7\u00dfi\u00b7gung", "und", "M\u00e4\u00b7\u00dfig\u00b7keit", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["NN", "KON", "NN", "$.", "$("], "meter": "----+-+", "measure": "unknown.measure.di"}, "line.8": {"text": "Frisch das Glas nur ausgestochen \u2013", "tokens": ["Frisch", "das", "Glas", "nur", "aus\u00b7ge\u00b7sto\u00b7chen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "ADV", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "M\u00e4\u00dfigung und M\u00e4\u00dfigkeit!", "tokens": ["M\u00e4\u00b7\u00dfi\u00b7gung", "und", "M\u00e4\u00b7\u00dfig\u00b7keit", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "----+-+", "measure": "unknown.measure.di"}, "line.10": {"text": "Ma\u00df! Ma\u00df!", "tokens": ["Ma\u00df", "!", "Ma\u00df", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$.", "NN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.11": {"text": "Leert darauf das volle Glas!", "tokens": ["Leert", "da\u00b7rauf", "das", "vol\u00b7le", "Glas", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PAV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "N\u00fcchtern bin ich, \u2013 Wein her! Wein her! \u2013", "tokens": ["N\u00fcch\u00b7tern", "bin", "ich", ",", "\u2013", "Wein", "her", "!", "Wein", "her", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["NN", "VAFIN", "PPER", "$,", "$(", "NN", "PTKVZ", "$.", "NN", "PTKVZ", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Immer n\u00fcchtern, \u2013 das versteht sich. \u2013", "tokens": ["Im\u00b7mer", "n\u00fcch\u00b7tern", ",", "\u2013", "das", "ver\u00b7steht", "sich", ".", "\u2013"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADJD", "$,", "$(", "PDS", "VVFIN", "PRF", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nur das Haus, der Boden, \u2013 Nein, Herr,", "tok