{"dta.poem.7968": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Schlu\u00dfsonett .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Wie, wenn man auch die Glocke nicht mehr ziehet,", "tokens": ["Wie", ",", "wenn", "man", "auch", "die", "Glo\u00b7cke", "nicht", "mehr", "zie\u00b7het", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "KOUS", "PIS", "ADV", "ART", "NN", "PTKNEG", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es lange dauert, bis sie ausgeklungen;", "tokens": ["Es", "lan\u00b7ge", "dau\u00b7ert", ",", "bis", "sie", "aus\u00b7ge\u00b7klun\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "$,", "KOUS", "PPER", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie, wer von einem Berge kam gesprungen,", "tokens": ["Wie", ",", "wer", "von", "ei\u00b7nem", "Ber\u00b7ge", "kam", "ge\u00b7sprun\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "PWS", "APPR", "ART", "NN", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Umsonst, den Lauf zu hemmen, sich bem\u00fchet;", "tokens": ["Um\u00b7sonst", ",", "den", "Lauf", "zu", "hem\u00b7men", ",", "sich", "be\u00b7m\u00fc\u00b7het", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "ART", "NN", "PTKZU", "VVINF", "$,", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wie oft aus Br\u00e4nden, welche l\u00e4ngst vergl\u00fchet,", "tokens": ["Wie", "oft", "aus", "Br\u00e4n\u00b7den", ",", "wel\u00b7che", "l\u00e4ngst", "ver\u00b7gl\u00fc\u00b7het", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "APPR", "NN", "$,", "PRELS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein Fl\u00e4mmchen unversehens sich geschwungen;", "tokens": ["Ein", "Fl\u00e4mm\u00b7chen", "un\u00b7ver\u00b7se\u00b7hens", "sich", "ge\u00b7schwun\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und sp\u00e4t noch eine Bl\u00fcthe vorgedrungen", "tokens": ["Und", "sp\u00e4t", "noch", "ei\u00b7ne", "Bl\u00fc\u00b7the", "vor\u00b7ge\u00b7drun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "ADV", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Aus Aesten, die sonst v\u00f6llig abgebl\u00fchet;", "tokens": ["Aus", "A\u00b7es\u00b7ten", ",", "die", "sonst", "v\u00f6l\u00b7lig", "ab\u00b7ge\u00b7bl\u00fc\u00b7het", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "ADV", "ADJD", "VVPP", "$."], "meter": "+---+-+-+-+-", "measure": "dactylic.init"}}, "stanza.3": {"line.1": {"text": "Wie den Gesang, den zu des Liebchens Preise", "tokens": ["Wie", "den", "Ge\u00b7sang", ",", "den", "zu", "des", "Lieb\u00b7chens", "Prei\u00b7se"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "NN"], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.2": {"text": "Der Sch\u00e4fer angestimmt aus voller Seele,", "tokens": ["Der", "Sch\u00e4\u00b7fer", "an\u00b7ge\u00b7stimmt", "aus", "vol\u00b7ler", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Gedankenlose Halle weiter treiben:", "tokens": ["Ge\u00b7dan\u00b7ken\u00b7lo\u00b7se", "Hal\u00b7le", "wei\u00b7ter", "trei\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "So geht es mir mit der Sonettenweise:", "tokens": ["So", "geht", "es", "mir", "mit", "der", "So\u00b7net\u00b7ten\u00b7wei\u00b7se", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ob mir\u2019s an Zweck und an Gedanken fehle,", "tokens": ["Ob", "mir's", "an", "Zweck", "und", "an", "Ge\u00b7dan\u00b7ken", "feh\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "APPR", "NN", "KON", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mu\u00df ich zum Schlusse dies Sonett doch schreiben.", "tokens": ["Mu\u00df", "ich", "zum", "Schlus\u00b7se", "dies", "So\u00b7nett", "doch", "schrei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPRART", "NN", "PDS", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}