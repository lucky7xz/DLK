{"textgrid.poem.46900": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[weil ich mich nirgends f\u00fcrchte zu verlieren]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Weil ich mich nirgends f\u00fcrchte zu verlieren,", "tokens": ["Weil", "ich", "mich", "nir\u00b7gends", "f\u00fcrch\u00b7te", "zu", "ver\u00b7lie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wohin mich, Herr, dein Machtgebot", "tokens": ["Wo\u00b7hin", "mich", ",", "Herr", ",", "dein", "Macht\u00b7ge\u00b7bot"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PWAV", "PPER", "$,", "NN", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Versetzen mag in deines Reichs Revieren,", "tokens": ["Ver\u00b7set\u00b7zen", "mag", "in", "dei\u00b7nes", "Reichs", "Re\u00b7vie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das deine Geister unter dir regieren;", "tokens": ["Das", "dei\u00b7ne", "Geis\u00b7ter", "un\u00b7ter", "dir", "re\u00b7gie\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "So f\u00fcrcht' ich nicht den Tod, der droht.", "tokens": ["So", "f\u00fcrcht'", "ich", "nicht", "den", "Tod", ",", "der", "droht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "ART", "NN", "$,", "PRELS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Und sollt' ich etwa meiner Lieben ihren", "tokens": ["Und", "sollt'", "ich", "et\u00b7wa", "mei\u00b7ner", "Lie\u00b7ben", "ih\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "ADV", "PPOSAT", "ADJA", "PPOSAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mehr f\u00fcrchten als den eignen Tod!", "tokens": ["Mehr", "f\u00fcrch\u00b7ten", "als", "den", "eig\u00b7nen", "Tod", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "KOKOM", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Weil ich sie aus den Augen mu\u00df verlieren,", "tokens": ["Weil", "ich", "sie", "aus", "den", "Au\u00b7gen", "mu\u00df", "ver\u00b7lie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "APPR", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wenn sie, um einen andern Platz zu zieren,", "tokens": ["Wenn", "sie", ",", "um", "ei\u00b7nen", "an\u00b7dern", "Platz", "zu", "zie\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "KOUI", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Von hier entr\u00fccket dein Gebot?", "tokens": ["Von", "hier", "ent\u00b7r\u00fc\u00b7cket", "dein", "Ge\u00b7bot", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Weil ich mich nirgends