{"dta.poem.14974": {"metadata": {"author": {"name": "K\u00f6rner, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Hoch lebe das Haus Oesterreich!", "genre": "Lyrik", "period": "N.A.", "pub_year": "1814", "urn": "urn:nbn:de:kobv:b4-200905193258", "language": ["de:0.99"], "booktitle": "K\u00f6rner, Theodor: Leyer und Schwerdt. Berlin, 1814."}, "poem": {"stanza.1": {"line.1": {"text": "Und bleich der Mond die Wolken s\u00e4umt. \u2014", "tokens": ["Und", "bleich", "der", "Mond", "die", "Wol\u00b7ken", "s\u00e4umt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADJD", "ART", "NN", "ART", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Was bist du Welt so still, so leer!", "tokens": ["Was", "bist", "du", "Welt", "so", "still", ",", "so", "leer", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "NN", "ADV", "ADJD", "$,", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was lauerst du wie ein falsches Meer? \u2014", "tokens": ["Was", "lau\u00b7erst", "du", "wie", "ein", "fal\u00b7sches", "Meer", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "VVFIN", "PPER", "KOKOM", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Es saust so \u00f6de durch dein Reich", "tokens": ["Es", "saust", "so", "\u00f6\u00b7de", "durch", "dein", "Reich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und Schauder fa\u00dft die Seele gleich,", "tokens": ["Und", "Schau\u00b7der", "fa\u00dft", "die", "See\u00b7le", "gleich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Als wolltest du mit leisem Beben", "tokens": ["Als", "woll\u00b7test", "du", "mit", "lei\u00b7sem", "Be\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "VMFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Des Morgens blut'gen Schleyer heben. \u2014", "tokens": ["Des", "Mor\u00b7gens", "blut'\u00b7gen", "Schle\u00b7yer", "he\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Noch schlummerts tief in Lagers Raum,", "tokens": ["Noch", "schlum\u00b7merts", "tief", "in", "La\u00b7gers", "Raum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die Sterne steigen, auf und nieder;", "tokens": ["Die", "Ster\u00b7ne", "stei\u00b7gen", ",", "auf", "und", "nie\u00b7der", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "APPR", "KON", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Die Todtenstille regt sich kaum! \u2014", "tokens": ["Die", "Tod\u00b7ten\u00b7stil\u00b7le", "regt", "sich", "kaum", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ADV", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "O la\u00df der Welt den sch\u00f6nen Traum,", "tokens": ["O", "la\u00df", "der", "Welt", "den", "sch\u00f6\u00b7nen", "Traum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Der nahe Tag verscheucht ihn wieder! \u2014", "tokens": ["Der", "na\u00b7he", "Tag", "ver\u00b7scheucht", "ihn", "wie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ADV", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "In Osten graut's, es sinkt die Nacht. \u2014", "tokens": ["In", "Os\u00b7ten", "graut's", ",", "es", "sinkt", "die", "Nacht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NE", "NE", "$,", "PPER", "VVFIN", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gottlob! der Morgen ist erwacht! \u2014", "tokens": ["Gott\u00b7lob", "!", "der", "Mor\u00b7gen", "ist", "er\u00b7wacht", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "$.", "ART", "NN", "VAFIN", "VVPP", "$.", "$("], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.4": {"line.1": {"text": "Gottlob, der neue Tag bricht an! \u2014", "tokens": ["Gott\u00b7lob", ",", "der", "neu\u00b7e", "Tag", "bricht", "an", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "$,", "ART", "ADJA", "NN", "VVFIN", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Seht euch nochmal die Sonne an.", "tokens": ["Seht", "euch", "noch\u00b7mal", "die", "Son\u00b7ne", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wohl viele, die jetzt r\u00fcstig stehn,", "tokens": ["Wohl", "vie\u00b7le", ",", "die", "jetzt", "r\u00fcs\u00b7tig", "stehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "$,", "PRELS", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sehn sie nie wieder untergehn.", "tokens": ["Sehn", "sie", "nie", "wie\u00b7der", "un\u00b7ter\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "VVINF", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "In manchen Herzen pocht das Blut,", "tokens": ["In", "man\u00b7chen", "Her\u00b7zen", "pocht", "das", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nach raschen Streites Uebermuth,", "tokens": ["Nach", "ra\u00b7schen", "Strei\u00b7tes", "Ue\u00b7ber\u00b7muth", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Und eh die n\u00e4chsten Stunden tagen,", "tokens": ["Und", "eh", "die", "n\u00e4chs\u00b7ten", "Stun\u00b7den", "ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Hat manches Herz schon ausgeschlagen!", "tokens": ["Hat", "man\u00b7ches", "Herz", "schon", "aus\u00b7ge\u00b7schla\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Die Sonne kommt, der Nebel rei\u00dft\u201a", "tokens": ["Die", "Son\u00b7ne", "kommt", ",", "der", "Ne\u00b7bel", "rei\u00dft", "\u201a"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein stumm Gebet den Vater prei\u00dft.", "tokens": ["Ein", "stumm", "Ge\u00b7bet", "den", "Va\u00b7ter", "prei\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Nun lebt und regt sich alle Welt\u201a", "tokens": ["Nun", "lebt", "und", "regt", "sich", "al\u00b7le", "Welt", "\u201a"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "PRF", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In blanken Waffen gl\u00e4nzt das Feld.", "tokens": ["In", "blan\u00b7ken", "Waf\u00b7fen", "gl\u00e4nzt", "das", "Feld", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der J\u00fcngling schreitet k\u00fchn hinaus\u201a", "tokens": ["Der", "J\u00fcng\u00b7ling", "schrei\u00b7tet", "k\u00fchn", "hin\u00b7aus", "\u201a"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er schaut hinauf ins Vaterhaus,", "tokens": ["Er", "schaut", "hin\u00b7auf", "ins", "Va\u00b7ter\u00b7haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und leise Ahndung f\u00fcllt sein Herz,", "tokens": ["Und", "lei\u00b7se", "Ahn\u00b7dung", "f\u00fcllt", "sein", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und zieht ihn d\u00e4mmernd himmelw\u00e4rts.", "tokens": ["Und", "zieht", "ihn", "d\u00e4m\u00b7mernd", "him\u00b7mel\u00b7w\u00e4rts", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da tr\u00e4gt der tiefbewegte Sinn", "tokens": ["Da", "tr\u00e4gt", "der", "tief\u00b7be\u00b7weg\u00b7te", "Sinn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die Tr\u00e4ume zu der Liebsten hin.", "tokens": ["Die", "Tr\u00e4u\u00b7me", "zu", "der", "Liebs\u00b7ten", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Sie weinte, als er scheiden mu\u00dft',", "tokens": ["Sie", "wein\u00b7te", ",", "als", "er", "schei\u00b7den", "mu\u00dft'", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Und Wehmuth haucht in seine Brust,", "tokens": ["Und", "Weh\u00b7muth", "haucht", "in", "sei\u00b7ne", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Und er gedenkt der sch\u00f6nen Zeiten! \u2014", "tokens": ["Und", "er", "ge\u00b7denkt", "der", "sch\u00f6\u00b7nen", "Zei\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Er f\u00fchlt's, es war ein ewig Scheiden! \u2014", "tokens": ["Er", "f\u00fchlt's", ",", "es", "war", "ein", "e\u00b7wig", "Schei\u00b7den", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VAFIN", "ART", "ADJD", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Die Sonne steigt, der L\u00e4rmschu\u00df kracht,", "tokens": ["Die", "Son\u00b7ne", "steigt", ",", "der", "L\u00e4rm\u00b7schu\u00df", "kracht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Laut jubelnd zieht das Heer zur Schlacht.", "tokens": ["Laut", "ju\u00b7belnd", "zieht", "das", "Heer", "zur", "Schlacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "\u201eseht ihr den Stephan her\u00fcberwinken,", "tokens": ["\u201e", "seht", "ihr", "den", "Ste\u00b7phan", "her\u00b7\u00fc\u00b7berw\u00b7in\u00b7ken", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "++-+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "\u201eund dort die Fr\u00e4nk'schen Adler blinken?", "tokens": ["\u201e", "und", "dort", "die", "Fr\u00e4nk'\u00b7schen", "Ad\u00b7ler", "blin\u00b7ken", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "ADV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "\u201eauf Br\u00fcder, st\u00fcrzt euch muthig drein',", "tokens": ["\u201e", "auf", "Br\u00fc\u00b7der", ",", "st\u00fcrzt", "euch", "mut\u00b7hig", "drein'", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "NN", "$,", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u201edie Adler m\u00fcssen unser seyn. \u2014", "tokens": ["\u201e", "die", "Ad\u00b7ler", "m\u00fcs\u00b7sen", "un\u00b7ser", "seyn", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "NN", "VMFIN", "PPOSAT", "VAINF", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "\u201elebt wohl, lebt wohl, ihr meine Lieben,", "tokens": ["\u201e", "lebt", "wohl", ",", "lebt", "wohl", ",", "ihr", "mei\u00b7ne", "Lie\u00b7ben", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "ADV", "$,", "VVFIN", "ADV", "$,", "PPER", "PPOSAT", "ADJA", "$,"], "meter": "+----+-+-", "measure": "dactylic.init"}, "line.6": {"text": "\u201eweint nicht, ich wollt euch nicht betr\u00fcben!\u201c", "tokens": ["\u201e", "weint", "nicht", ",", "ich", "wollt", "euch", "nicht", "be\u00b7tr\u00fc\u00b7ben", "!", "\u201c"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "PTKNEG", "$,", "PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$.", "$("], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}, "stanza.9": {"line.1": {"text": "Es wogt der Kampf, es br\u00fcllt der Tod,", "tokens": ["Es", "wogt", "der", "Kampf", ",", "es", "br\u00fcllt", "der", "Tod", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Wunden klaffen blutigroth! \u2014", "tokens": ["Die", "Wun\u00b7den", "klaf\u00b7fen", "blu\u00b7ti\u00b7groth", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "\u201emir nach! mir nach! dort ist der Ruhm,", "tokens": ["\u201e", "mir", "nach", "!", "mir", "nach", "!", "dort", "ist", "der", "Ruhm", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "PTKVZ", "$.", "PPER", "PTKVZ", "$.", "ADV", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u201eihr k\u00e4mpft f\u00fcr euer Heiligthum!\u201c", "tokens": ["\u201e", "ihr", "k\u00e4mpft", "f\u00fcr", "eu\u00b7er", "Hei\u00b7lig\u00b7thum", "!", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und neben ihm und unter ihm", "tokens": ["Und", "ne\u00b7ben", "ihm", "und", "un\u00b7ter", "ihm"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "KON", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "W\u00fcrgt rasch des Todes Ungest\u00fcm,", "tokens": ["W\u00fcrgt", "rasch", "des", "To\u00b7des", "Un\u00b7ge\u00b7st\u00fcm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Und Mann und Ro\u00df zusammenbrach", "tokens": ["Und", "Mann", "und", "Ro\u00df", "zu\u00b7sam\u