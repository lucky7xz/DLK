{"textgrid.poem.67223": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wenn aus der Tiefe kommt der Fr\u00fchling in das Leben,", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn aus der Tiefe kommt der Fr\u00fchling in das Leben,", "tokens": ["Wenn", "aus", "der", "Tie\u00b7fe", "kommt", "der", "Fr\u00fch\u00b7ling", "in", "das", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es wundert sich der Mensch, und neue Worte streben", "tokens": ["Es", "wun\u00b7dert", "sich", "der", "Mensch", ",", "und", "neu\u00b7e", "Wor\u00b7te", "stre\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN", "$,", "KON", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Aus Geistigkeit, die Freude kehret wieder", "tokens": ["Aus", "Geis\u00b7tig\u00b7keit", ",", "die", "Freu\u00b7de", "keh\u00b7ret", "wie\u00b7der"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "ART", "NN", "VVFIN", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und festlich machen sich Gesang und Lieder.", "tokens": ["Und", "fest\u00b7lich", "ma\u00b7chen", "sich", "Ge\u00b7sang", "und", "Lie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PRF", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Das Leben findet sich aus Harmonie der Zeiten,", "tokens": ["Das", "Le\u00b7ben", "fin\u00b7det", "sich", "aus", "Har\u00b7mo\u00b7nie", "der", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df immerdar den Sinn Natur und Geist geleiten,", "tokens": ["Da\u00df", "im\u00b7mer\u00b7dar", "den", "Sinn", "Na\u00b7tur", "und", "Geist", "ge\u00b7lei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und die Vollkommenheit ist Eines in dem Geiste,", "tokens": ["Und", "die", "Voll\u00b7kom\u00b7men\u00b7heit", "ist", "Ei\u00b7nes", "in", "dem", "Geis\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "PIS", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So findet vieles sich, und aus Natur das meiste.", "tokens": ["So", "fin\u00b7det", "vie\u00b7les", "sich", ",", "und", "aus", "Na\u00b7tur", "das", "meis\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PRF", "$,", "KON", "APPR", "NN", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wenn aus der Tiefe kommt der Fr\u00fchling in das Leben,", "tokens": ["Wenn", "aus", "der", "Tie\u00b7fe", "kommt", "der", "Fr\u00fch\u00b7ling", "in", "das", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KO