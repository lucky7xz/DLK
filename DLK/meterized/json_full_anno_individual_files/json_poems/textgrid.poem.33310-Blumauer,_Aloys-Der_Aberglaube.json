{"textgrid.poem.33310": {"metadata": {"author": {"name": "Blumauer, Aloys", "birth": "N.A.", "death": "N.A."}, "title": "Der Aberglaube", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Mann, dem eine Maus des Nachts den Schuh zerbi\u00df,", "tokens": ["Ein", "Mann", ",", "dem", "ei\u00b7ne", "Maus", "des", "Nachts", "den", "Schuh", "zer\u00b7bi\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "ART", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Erz\u00e4hlte einst dem weisen Kato dies,", "tokens": ["Er\u00b7z\u00e4hl\u00b7te", "einst", "dem", "wei\u00b7sen", "Ka\u00b7to", "dies", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NE", "PDS", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Als ein Ereigni\u00df, das ihn sehr be\u00e4ngstigt h\u00e4tte.", "tokens": ["Als", "ein", "Er\u00b7eig\u00b7ni\u00df", ",", "das", "ihn", "sehr", "be\u00b7\u00e4ngs\u00b7tigt", "h\u00e4t\u00b7te", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "PRELS", "PPER", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dar\u00fcber, Freund, legt ruhig euch zu Bette,", "tokens": ["Da\u00b7r\u00fc\u00b7ber", ",", "Freund", ",", "legt", "ru\u00b7hig", "euch", "zu", "Bet\u00b7te", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "NN", "$,", "VVFIN", "ADJD", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+---+-", "measure": "unknown.measure.tetra"}, "line.5": {"text": "Sprach Kato; denn das Wunder w\u00e4r'", "tokens": ["Sprach", "Ka\u00b7to", ";", "denn", "das", "Wun\u00b7der", "w\u00e4r'"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "NE", "$.", "KON", "ART", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Um zehnmal noch bedenklicher,", "tokens": ["Um", "zehn\u00b7mal", "noch", "be\u00b7denk\u00b7li\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.7": {"text": "Wenn euer Schuh die Maus gebissen h\u00e4tte.", "tokens": ["Wenn", "eu\u00b7er", "Schuh", "die", "Maus", "ge\u00b7bis\u00b7sen", "h\u00e4t\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ART", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ein Mann, dem eine Maus des Nachts den Schuh zerbi\u00df,", "tokens": ["Ein", "Mann", ",", "dem", "ei\u00b7ne", "Maus", "des", "Nachts", "den", "Schuh", "zer\u00b7bi\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "ART", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Erz\u00e4hlte einst dem weisen Kato dies,", "tokens": ["Er\u00b7z\u00e4hl\u00b7te", "einst", "dem", "wei\u00b7sen", "Ka\u00b7to", "dies", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NE", "PDS", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Als ein Ereigni\u00df, das ihn sehr be\u00e4ngstigt h\u00e4tte.", "tokens": ["Als", "ein", "Er\u00b7eig\u00b7ni\u00df", ",", "das", "ihn", "sehr", "be\u0