{"dta.poem.18517": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Der unvermuthete verlust Frauen  \n Sabina Menntzelin/ gebohrner Z\u00f6-  \n bin/ an ihren Hn. Sohn.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1703", "urn": "urn:nbn:de:kobv:b4-200905199360", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mein freund/ ich dachte zwar/ es solten meine zeilen", "tokens": ["Mein", "freund", "/", "ich", "dach\u00b7te", "zwar", "/", "es", "sol\u00b7ten", "mei\u00b7ne", "zei\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$(", "PPER", "VVFIN", "ADV", "$(", "PPER", "VMFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vor di\u00dfmahl ein begrif von tausend w\u00fcntschen seyn;", "tokens": ["Vor", "di\u00df\u00b7mahl", "ein", "be\u00b7grif", "von", "tau\u00b7send", "w\u00fcnt\u00b7schen", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ART", "NN", "APPR", "CARD", "VVFIN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So aber wird der brief zu lauter donner-keilen/", "tokens": ["So", "a\u00b7ber", "wird", "der", "brief", "zu", "lau\u00b7ter", "don\u00b7ner\u00b7kei\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "ART", "VVFIN", "APPR", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die feder tauchet sich in blut und thr\u00e4nen ein.", "tokens": ["Die", "fe\u00b7der", "tau\u00b7chet", "sich", "in", "blut", "und", "thr\u00e4\u00b7nen", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "NN", "KON", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich zittre dir ein wort/ ein hartes wort/ zu sagen/", "tokens": ["Ich", "zitt\u00b7re", "dir", "ein", "wort", "/", "ein", "har\u00b7tes", "wort", "/", "zu", "sa\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$(", "ART", "ADJA", "NN", "$(", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ach thr\u00e4nen/ nehmet doch das amt der zungen an!", "tokens": ["Ach", "thr\u00e4\u00b7nen", "/", "neh\u00b7met", "doch", "das", "amt", "der", "zun\u00b7gen", "an", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "$(", "VVFIN", "ADV", "ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der gantz verborgne GOtt hat an den knauff geschlagen/", "tokens": ["Der", "gantz", "ver\u00b7borg\u00b7ne", "Gott", "hat", "an", "den", "knauff", "ge\u00b7schla\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "VAFIN", "APPR", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und einen schweren ri\u00df in all dein heil gethan.", "tokens": ["Und", "ei\u00b7nen", "schwe\u00b7ren", "ri\u00df", "in", "all", "dein", "heil", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "VVFIN", "APPR", "PIAT", "PPOSAT", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Es liegt der mund erbla\u00dft/ der deinen zarten wangen/", "tokens": ["Es", "liegt", "der", "mund", "er\u00b7bla\u00dft", "/", "der", "dei\u00b7nen", "zar\u00b7ten", "wan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVPP", "$(", "ART", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Als dich das licht umfieng/ die ersten k\u00fcsse gab;", "tokens": ["Als", "dich", "das", "licht", "um\u00b7fi\u00b7eng", "/", "die", "ers\u00b7ten", "k\u00fcs\u00b7se", "gab", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$(", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "Die edle mutter ist uns aus den augen gangen/", "tokens": ["Die", "ed\u00b7le", "mut\u00b7ter", "ist", "uns", "aus", "den", "au\u00b7gen", "gan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "APPR", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und deinen besten trost umfa\u00dft ein todten-grab.", "tokens": ["Und", "dei\u00b7nen", "bes\u00b7ten", "trost", "um\u00b7fa\u00dft", "ein", "tod\u00b7ten\u00b7grab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ach unverhoffter fall/ der uns das blut der seelen/", "tokens": ["Ach", "un\u00b7ver\u00b7hoff\u00b7ter", "fall", "/", "der", "uns", "das", "blut", "der", "see\u00b7len", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADJA", "NN", "$(", "PRELS", "PPER", "ART", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Die grimme thr\u00e4nen-fluth/ aus hertz und augen prest!", "tokens": ["Die", "grim\u00b7me", "thr\u00e4\u00b7nen\u00b7fluth", "/", "aus", "hertz", "und", "au\u00b7gen", "prest", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Ich selber finde mich in so verworrnen h\u00f6len/", "tokens": ["Ich", "sel\u00b7ber", "fin\u00b7de", "mich", "in", "so", "ver\u00b7worr\u00b7nen", "h\u00f6\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PRF", "APPR", "ADV", "VVINF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "So da\u00df der bittre schmertz mich selbst nicht fassen l\u00e4st.", "tokens": ["So", "da\u00df", "der", "bitt\u00b7re", "schmertz", "mich", "selbst", "nicht", "fas\u00b7sen", "l\u00e4st", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ART", "ADJA", "NN", "PPER", "ADV", "PTKNEG", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Ein tod/ der als ein schlaf die augen \u00fcberzogen/", "tokens": ["Ein", "tod", "/", "der", "als", "ein", "schlaf", "die", "au\u00b7gen", "\u00fc\u00b7berz\u00b7o\u00b7gen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "KOUS", "ART", "NN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Nahm den entbundnen geist aus seiner wohnung hin;", "tokens": ["Nahm", "den", "ent\u00b7bund\u00b7nen", "geist", "aus", "sei\u00b7ner", "woh\u00b7nung", "hin", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.19": {"text": "Gleich als das licht der welt stieg an die himmels-bogen/", "tokens": ["Gleich", "als", "das", "licht", "der", "welt", "stieg", "an", "die", "him\u00b7mels\u00b7bo\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ART", "NN", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Umbgab die lange nacht den neuen Seraphin.", "tokens": ["U\u00b7mbgab", "die", "lan\u00b7ge", "nacht", "den", "neu\u00b7en", "Se\u00b7ra\u00b7phin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.21": {"text": "Der menschen letzter feind verlohr hier seinen nahmen/", "tokens": ["Der", "men\u00b7schen", "letz\u00b7ter", "feind", "ver\u00b7lohr", "hier", "sei\u00b7nen", "nah\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "VVFIN", "ADV", "PPOSAT", "VVFIN", "$("], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.22": {"text": "Er sah als wie ein freund und naher bruder aus.", "tokens": ["Er", "sah", "als", "wie", "ein", "freund", "und", "na\u00b7her", "bru\u00b7der", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOUS", "KOKOM", "ART", "NN", "KON", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Denn als aus Sions-burg die letzten wincke kamen/", "tokens": ["Denn", "als", "aus", "Si\u00b7ons\u00b7burg", "die", "letz\u00b7ten", "win\u00b7cke", "ka\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "APPR", "NE", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Brach er gantz unvermerckt der seelen bauges hau\u00df.", "tokens": ["Brach", "er", "gantz", "un\u00b7ver\u00b7merckt", "der", "see\u00b7len", "bau\u00b7ges", "hau\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJD", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Ein kurtzer augenblick gab sie an dessen seiten/", "tokens": ["Ein", "kurt\u00b7zer", "au\u00b7gen\u00b7blick", "gab", "sie", "an", "des\u00b7sen", "sei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "APPR", "PDS", "VVFIN", "$("], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.26": {"text": "Der seinen nahmen ihr zum letzten seustzer lieh;", "tokens": ["Der", "sei\u00b7nen", "nah\u00b7men", "ihr", "zum", "letz\u00b7ten", "seust\u00b7zer", "lieh", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Denn den sie nie verlie\u00df/ der muste sie begleiten", "tokens": ["Denn", "den", "sie", "nie", "ver\u00b7lie\u00df", "/", "der", "mus\u00b7te", "sie", "be\u00b7glei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ART", "PPER", "ADV", "VVFIN", "$(", "ART", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Dahin/ wo heil und trost verzuckert qval und m\u00fch.", "tokens": ["Da\u00b7hin", "/", "wo", "heil", "und", "trost", "ver\u00b7zu\u00b7ckert", "qval", "und", "m\u00fch", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$(", "PWAV", "ADJD", "KON", "NN", "VVFIN", "NE", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Die welt vermist an ihr ein muster vieler frauen/", "tokens": ["Die", "welt", "ver\u00b7mist", "an", "ihr", "ein", "mus\u00b7ter", "vie\u00b7ler", "frau\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPER", "ART", "NN", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Wo alle tugenden sich ihren sitz erkiest;", "tokens": ["Wo", "al\u00b7le", "tu\u00b7gen\u00b7den", "sich", "ih\u00b7ren", "sitz", "er\u00b7kiest", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "PRF", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Denn nichts verstelltes war an ihr durchaus zu schauen/", "tokens": ["Denn", "nichts", "ver\u00b7stell\u00b7tes", "war", "an", "ihr", "durc\u00b7haus", "zu", "schau\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "ADJA", "VAFIN", "APPR", "PPER", "ADV", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Was sonst der argen welt gewohnter zierath ist.", "tokens": ["Was", "sonst", "der", "ar\u00b7gen", "welt", "ge\u00b7wohn\u00b7ter", "zie\u00b7rath", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "ADJA", "NN", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Der kirchen fehlt ein glied von ungemeiner g\u00fcte/", "tokens": ["Der", "kir\u00b7chen", "fehlt", "ein", "glied", "von", "un\u00b7ge\u00b7mei\u00b7ner", "g\u00fc\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Ein glied das iederzeit des H\u00f6chsten tempel war:", "tokens": ["Ein", "glied", "das", "ie\u00b7der\u00b7zeit", "des", "H\u00f6chs\u00b7ten", "tem\u00b7pel", "war", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ART", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Der arme klagt die hand/ wir aber ihr gem\u00fcthe;", "tokens": ["Der", "ar\u00b7me", "klagt", "die", "hand", "/", "wir", "a\u00b7ber", "ihr", "ge\u00b7m\u00fc\u00b7the", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "ART", "NN", "$(", "PPER", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Kurtz: Unser kleinod liegt auff einer todten-baar.", "tokens": ["Kurtz", ":", "Un\u00b7ser", "klei\u00b7nod", "liegt", "auff", "ei\u00b7ner", "tod\u00b7ten\u00b7baar", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$.", "PPOSAT", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "So seufftzt die gantze stadt! drum schweigen meine w\u00f6rter/", "tokens": ["So", "seufftzt", "die", "gant\u00b7ze", "stadt", "!", "drum", "schwei\u00b7gen", "mei\u00b7ne", "w\u00f6r\u00b7ter", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$.", "PAV", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Dergleichen lebens-lauff besch\u00e4met allen ruhm;", "tokens": ["Derg\u00b7lei\u00b7chen", "le\u00b7bens\u00b7lauff", "be\u00b7sch\u00e4\u00b7met", "al\u00b7len", "ruhm", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADJD", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Zudem bewohnt der geist schon die bestirnten \u00f6rter/", "tokens": ["Zu\u00b7dem", "be\u00b7wohnt", "der", "geist", "schon", "die", "bes\u00b7tirn\u00b7ten", "\u00f6r\u00b7ter", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "NN", "ADV", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Und hat der engel lob zu seinem eigenthum.", "tokens": ["Und", "hat", "der", "en\u00b7gel", "lob", "zu", "sei\u00b7nem", "ei\u00b7gen\u00b7thum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Jhr schmertzlicher verlust ist hier zwar zu betrauren/", "tokens": ["Ihr", "schmertz\u00b7li\u00b7cher", "ver\u00b7lust", "ist", "hier", "zwar", "zu", "b