{"dta.poem.2608": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Schuldiges Mitleiden  \n  Bey Beerdigung Hn. J. G. Z. j\u00fcngsten  \n S\u00f6hnleins 22. April. 1665.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Mensch ist ja mit recht ein Gast der Welt zu sch\u00e4tz&#275;/", "tokens": ["Der", "Mensch", "ist", "ja", "mit", "recht", "ein", "Gast", "der", "Welt", "zu", "sch\u00e4tz", "&#275;", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "XML_entity", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "APPR", "ADV", "ART", "NN", "ART", "NN", "APPR", "NE", "$(", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Denn heute kehrt er ein/ und Morgen wieder aus:", "tokens": ["Denn", "heu\u00b7te", "kehrt", "er", "ein", "/", "und", "Mor\u00b7gen", "wie\u00b7der", "aus", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ART", "$(", "KON", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Trachten/ so die Zeit ihm pfleget f\u00fcrzusetzen/", "tokens": ["Die", "Trach\u00b7ten", "/", "so", "die", "Zeit", "ihm", "pfle\u00b7get", "f\u00fcr\u00b7zu\u00b7set\u00b7zen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ADV", "ART", "NN", "PPER", "VVFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sind balsamirtes Gifft und \u00fcberzuckert Grau\u00df.", "tokens": ["Sind", "bal\u00b7sa\u00b7mir\u00b7tes", "Gifft", "und", "\u00fc\u00b7ber\u00b7zu\u00b7ckert", "Grau\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "KON", "VVFIN", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Es sey die K\u00f6derung so lieblich als sie wolle/", "tokens": ["Es", "sey", "die", "K\u00f6\u00b7de\u00b7rung", "so", "lieb\u00b7lich", "als", "sie", "wol\u00b7le", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "ADJD", "KOKOM", "PPER", "VMFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die K\u00f6rner erster Lust sind D\u00f6rner letzter Pein/", "tokens": ["Die", "K\u00f6r\u00b7ner", "ers\u00b7ter", "Lust", "sind", "D\u00f6r\u00b7ner", "letz\u00b7ter", "Pein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VAFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Da\u00df nicht ein Aloe auf Julep folgen solle/", "tokens": ["Da\u00df", "nicht", "ein", "A\u00b7loe", "auf", "Ju\u00b7lep", "fol\u00b7gen", "sol\u00b7le", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "ART", "NN", "APPR", "NE", "VVINF", "VMFIN", "$("], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "W\u00e4r in dem grossen Saal der Erden ungemein.", "tokens": ["W\u00e4r", "in", "dem", "gros\u00b7sen", "Saal", "der", "Er\u00b7den", "un\u00b7ge\u00b7mein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "ADJA", "NN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Und unsre meiste Kost sind rasende Begierden/", "tokens": ["Und", "uns\u00b7re", "meis\u00b7te", "Kost", "sind", "ra\u00b7sen\u00b7de", "Be\u00b7gier\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VAFIN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Jetzt macht uns Hoffnung satt/ bald f\u00fcllt uns Schrecken an.", "tokens": ["Jetzt", "macht", "uns", "Hoff\u00b7nung", "satt", "/", "bald", "f\u00fcllt", "uns", "Schre\u00b7cken", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "ADJD", "$(", "ADV", "VVFIN", "PPER", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Den speiset h\u00f6chst vergn\u00fcgt ein Auge voller Zierden/", "tokens": ["Den", "spei\u00b7set", "h\u00f6chst", "ver\u00b7gn\u00fcgt", "ein", "Au\u00b7ge", "vol\u00b7ler", "Zier\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "VVFIN", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Wenn der aus Gelddurst nicht bey Nachte ruhen kan.", "tokens": ["Wenn", "der", "aus", "Geld\u00b7durst", "nicht", "bey", "Nach\u00b7te", "ru\u00b7hen", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "APPR", "NN", "PTKNEG", "APPR", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Wir lassen nicht der Zeit erst Raum uns zu verzehren/", "tokens": ["Wir", "las\u00b7sen", "nicht", "der", "Zeit", "erst", "Raum", "uns", "zu", "ver\u00b7zeh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ART", "NN", "ADV", "NN", "PPER", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Indem verg\u00e4lter Ha\u00df und d\u00fcrrer Neid uns frist/", "tokens": ["In\u00b7dem", "ver\u00b7g\u00e4l\u00b7ter", "Ha\u00df", "und", "d\u00fcr\u00b7rer", "Neid", "uns", "frist", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "KON", "ADJA", "NN", "PPER", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Denckt man f\u00fcrm Untergang durch Kr\u00e4uter sich zu wehren/", "tokens": ["Denckt", "man", "f\u00fcrm", "Un\u00b7ter\u00b7gang", "durch", "Kr\u00e4u\u00b7ter", "sich", "zu", "weh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPRART", "NN", "APPR", "NN", "PRF", "PTKZU", "VVINF", "$("], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.16": {"text": "So ist doch Fleisch und Blut das uns vom Tode list.", "tokens": ["So", "ist", "doch", "Fleisch", "und", "Blut", "das", "uns", "vom", "To\u00b7de", "list", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "NN", "KON", "NN", "ART", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Wir m\u00fcssen ingesammt die W\u00fcrme Schwestern nennen/", "tokens": ["Wir", "m\u00fcs\u00b7sen", "in\u00b7ge\u00b7sammt", "die", "W\u00fcr\u00b7me", "Schwes\u00b7tern", "nen\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Und die Verwesung nur als Mutter sehen an.", "tokens": ["Und", "die", "Ver\u00b7we\u00b7sung", "nur", "als", "Mut\u00b7ter", "se\u00b7hen", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "KOUS", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Der Schlu\u00df ist l\u00e4ngst gemacht/ da\u00df Seel und Leib sich trennen/", "tokens": ["Der", "Schlu\u00df", "ist", "l\u00e4ngst", "ge\u00b7macht", "/", "da\u00df", "Seel", "und", "Leib", "sich", "tren\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVPP", "$(", "KOUS", "NN", "KON", "NN", "PRF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Die wird dem Himmel/ der der Erden beygethan.", "tokens": ["Die", "wird", "dem", "Him\u00b7mel", "/", "der", "der", "Er\u00b7den", "bey\u00b7ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$(", "ART", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Weil Sterben und Geburt sich fest umbwickelt haben/", "tokens": ["Weil", "Ster\u00b7ben", "und", "Ge\u00b7burt", "sich", "fest", "um\u00b7bwi\u00b7ckelt", "ha\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "PRF", "ADJD", "VVPP", "VAINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Das Leben und der Todt in gleichen Ketten gehn;", "tokens": ["Das", "Le\u00b7ben", "und", "der", "Todt", "in", "glei\u00b7chen", "Ket\u00b7ten", "gehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "So ists kein Unterscheid/ da\u00df dieser fr\u00fch begraben/", "tokens": ["So", "ists", "kein", "Un\u00b7ter\u00b7scheid", "/", "da\u00df", "die\u00b7ser", "fr\u00fch", "be\u00b7gra\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIAT", "NN", "$(", "KOUS", "PDAT", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Ein ander sp\u00e4t hernach wird auff der Bahre stehn.", "tokens": ["Ein", "an\u00b7der", "sp\u00e4t", "her\u00b7nach", "wird", "auff", "der", "Bah\u00b7re", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJD", "ADV", "VAFIN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Betr\u00fcbtste/ da\u00df mein Wort von keiner Anmuth linde/", "tokens": ["Be\u00b7tr\u00b7\u00fcbts\u00b7te", "/", "da\u00df", "mein", "Wort", "von", "kei\u00b7ner", "An\u00b7muth", "lin\u00b7de", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "KOUS", "PPOSAT", "NN", "APPR", "PIAT", "NN", "ADJA", "$("], "meter": "-++-+-+-+-+-+-", "measure": "unknown.measure.septa"}, "line.26": {"text": "Und heissen Schmertzen auch noch mehr die Sporen giebt/", "tokens": ["Und", "heis\u00b7sen", "Schmert\u00b7zen", "auch", "noch", "mehr", "die", "Spo\u00b7ren", "giebt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ADV", "ADV", "ADV", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Erwecket der Verlust ob ihrem zarten Kinde/", "tokens": ["Er\u00b7we\u00b7cket", "der", "Ver\u00b7lust", "ob", "ih\u00b7rem", "zar\u00b7ten", "Kin\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "KOUS", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Das/ weils von Hertzen kam/ das Hertze tief betr\u00fcbt.", "tokens": ["Das", "/", "weils", "von", "Hert\u00b7zen", "kam", "/", "das", "Hert\u00b7ze", "tief", "be\u00b7tr\u00fcbt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$(", "ADV", "APPR", "NN", "VVFIN", "$(", "PDS", "VVFIN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Dem G\u00e4rtner bringt es Pein wenn seine Lust-Gefielder", "tokens": ["Dem", "G\u00e4rt\u00b7ner", "bringt", "es", "Pein", "wenn", "sei\u00b7ne", "Lust\u00b7Ge\u00b7fiel\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "NN", "KOUS", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Ein gifftig Reif bestreicht/ und alle M\u00fch verderbt.", "tokens": ["Ein", "giff\u00b7tig", "Reif", "be\u00b7streicht", "/", "und", "al\u00b7le", "M\u00fch", "ver\u00b7derbt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VVPP", "$(", "KON", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Wenn er ersticken sieht des Fr\u00fchlings sch\u00f6ne Bilder/", "tokens": ["Wenn", "er", "er\u00b7sti\u00b7cken", "sieht", "des", "Fr\u00fch\u00b7lings", "sch\u00f6\u00b7ne", "Bil\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVINF", "VVFIN", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Und vor den wachen Flei\u00df nur lehre Stoppeln erbt.", "tokens": ["Und", "vor", "den", "wa\u00b7chen", "Flei\u00df", "nur", "leh\u00b7re", "Stop\u00b7peln", "erbt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "ADV", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Der Garten eurer Eh\u2019 hat zweyfach Frucht getragen/", "tokens": ["Der", "Gar\u00b7ten", "eu\u00b7rer", "Eh'", "hat", "zwey\u00b7fach", "Frucht", "ge\u00b7tra\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VAFIN", "APPR", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Und mit der doppel-Lust das gantze Haus ergetzt;", "tokens": ["Und", "mit", "der", "dop\u00b7pel\u00b7Lust", "das", "gant\u00b7ze", "Haus", "er\u00b7getzt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Run eure Blume f\u00e4lt/ so ist sie werth zu klagen/", "tokens": ["Run", "eu\u00b7re", "Blu\u00b7me", "f\u00e4lt", "/", "so", "ist", "sie", "werth", "zu", "kla\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "VVFIN", "$(", "ADV", "VAFIN", "PPER", "ADJD", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Und da\u00df der Thr\u00e4nen Thau noch ihre Bl\u00e4tter netzt.", "tokens": ["Und", "da\u00df", "der", "Thr\u00e4\u00b7nen", "Thau", "noch", "ih\u00b7re", "Bl\u00e4t\u00b7ter", "netzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "NN", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Denn ob zwar die Vernunfft hier wil den Meister spielen/", "tokens": ["Denn", "ob", "zwar", "die", "Ver\u00b7nunfft", "hier", "wil", "den", "Meis\u00b7ter", "spie\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "ART", "NN", "ADV", "VMFIN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Indem sie schleust: Ein Sohn/ der Freuden Sammel-Platz/", "tokens": ["In\u00b7dem", "sie", "schleust", ":", "Ein", "Sohn", "/", "der", "Freu\u00b7den", "Sam\u00b7mel\u00b7Platz", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$.", "ART", "NN", "$(", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Der k\u00fcnftig Ehr und Ruhm kan dem Geschlecht erzielen/", "tokens": ["Der", "k\u00fcnf\u00b7tig", "Ehr", "und", "Ruhm", "kan", "dem", "Ge\u00b7schlecht", "er\u00b7zie\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "KON", "NN", "VMFIN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Des Vatern Ebenbild/ der Mutter Lust und Schatz/", "tokens": ["Des", "Va\u00b7tern", "E\u00b7ben\u00b7bild", "/", "der", "Mut\u00b7ter", "Lust", "und", "Schatz", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART