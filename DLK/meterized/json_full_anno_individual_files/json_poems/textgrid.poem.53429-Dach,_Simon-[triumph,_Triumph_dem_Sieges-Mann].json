{"textgrid.poem.53429": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "[triumph, Triumph dem Sieges-Mann]", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Triumph, Triumph dem Sieges-Mann", "tokens": ["Tri\u00b7umph", ",", "Tri\u00b7umph", "dem", "Sie\u00b7ges\u00b7Mann"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "ART", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Der S\u00fcnd und Todt bezwingen kan. Alleluja. Alleluja.", "tokens": ["Der", "S\u00fcnd", "und", "Todt", "be\u00b7zwin\u00b7gen", "kan", ".", "Al\u00b7le\u00b7lu\u00b7ja", ".", "Al\u00b7le\u00b7lu\u00b7ja", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVINF", "VMFIN", "$.", "NE", "$.", "NE", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.3": {"text": "Triumph dem Herren, der die Macht", "tokens": ["Tri\u00b7umph", "dem", "Her\u00b7ren", ",", "der", "die", "Macht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ART", "NN", "$,", "PRELS", "ART", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Der H\u00f6llen unter sich gebracht, Alleluja, Alleluja.", "tokens": ["Der", "H\u00f6l\u00b7len", "un\u00b7ter", "sich", "ge\u00b7bracht", ",", "Al\u00b7le\u00b7lu\u00b7ja", ",", "Al\u00b7le\u00b7lu\u00b7ja", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "APPR", "PRF", "VVPP", "$,", "ITJ", "$,", "NE", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}, "stanza.2": {"line.1": {"text": "Ihr Christen jauchzet allesambt,", "tokens": ["Ihr", "Chris\u00b7ten", "jauch\u00b7zet", "al\u00b7le\u00b7sambt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nunmehr ist nichts, was uns verdammt, Alleluja. Alleluja.", "tokens": ["Nun\u00b7mehr", "ist", "nichts", ",", "was", "uns", "ver\u00b7dammt", ",", "Al\u00b7le\u00b7lu\u00b7ja", ".", "Al\u00b7le\u00b7lu\u00b7ja", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "$,", "PRELS", "PPER", "VVPP", "$,", "NE", "$.", "NE", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.3": {"text": "Nun hat es seine Richtigkeit", "tokens": ["Nun", "hat", "es", "sei\u00b7ne", "Rich\u00b7tig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit unser Freud' und Sicherheit. Alleluja. Alleluja.", "tokens": ["Mit", "un\u00b7ser", "Freud'", "und", "Si\u00b7cher\u00b7heit", ".", "Al\u00b7le\u00b7lu\u00b7ja", ".", "Al\u00b7le\u00b7lu\u00b7ja", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "NN", "$.", "NE", "$.", "NE", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}, "stanza.3": {"line.1": {"text": "Der Herr ist herrlich und erh\u00f6ht,", "tokens": ["Der", "Herr", "ist", "herr\u00b7lich", "und", "er\u00b7h\u00f6ht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der von dem Todt jetzt auffersteht, Alleluja. Alleluja.", "tokens": ["Der", "von", "dem", "Todt", "jetzt", "auf\u00b7fer\u00b7steht", ",", "Al\u00b7le\u00b7lu\u00b7ja", ".", "Al\u00b7le\u00b7lu\u00b7ja", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ADV", "VVFIN", "$,", "NE", "$.", "NE", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.3": {"text": "Er ist es, der die H\u00f6ll umbschlo\u00df'", "tokens": ["Er", "ist", "es", ",", "der", "die", "H\u00f6ll", "umb\u00b7schlo\u00df'"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "$,", "PRELS", "ART", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und machet uns Gefangne lo\u00df. Alleluja. Alleluja.", "tokens": ["Und", "ma\u00b7chet", "uns", "Ge\u00b7fang\u00b7ne", "lo\u00df", ".", "Al\u00b7le\u00b7lu\u00b7ja", ".", "Al\u00b7le\u00b7lu\u00b7ja", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "NN", "PTKVZ", "$.", "NE", "$.", "NE", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}, "stanza.4": {"line.1": {"text": "Die Bande Josephs sind entzwey,", "tokens": ["Die", "Ban\u00b7de", "Jo\u00b7se\u00b7phs", "sind", "ent\u00b7zwey", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VAFIN", "PTKVZ", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Er und wir alle werden frey, Alleluja. Alleluja.", "tokens": ["Er", "und", "wir", "al\u00b7le", "wer\u00b7den", "frey", ",", "Al\u00b7le\u00b7lu\u00b7ja", ".", "Al\u00b7le\u00b7lu\u00b7ja", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "KON", "PPER", "PIS", "VAFIN", "ADJD", "$,", "NE", "$.", "NE", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.3": {"text": "Er hat den Himmel auffgethan", "tokens": ["Er", "hat", "den", "Him\u00b7mel", "auff\u00b7ge\u00b7than"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und ist hinein die Th\u00fcr und Bahn, Alleluja, Alleluja.", "tokens": ["Und", "ist", "hin\u00b7ein", "die", "Th\u00fcr", "und", "Bahn", ",", "Al\u00b7le\u00b7lu\u00b7ja", ",", "Al\u00b7le\u00b7lu\u00b7ja", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ART", "NN", "KON", "NN", "$,", "ITJ", "$,", "NE", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}, "stanza.5": {"line.1": {"text": "Die Lade Gottes unser Heyl,", "tokens": ["Die", "La\u00b7de", "Got\u00b7tes", "un\u00b7ser", "Heyl", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wird heute wieder uns zu theil, Alleluja. Alleluja.", "tokens": ["Wird", "heu\u00b7te", "wie\u00b7der", "uns", "zu", "theil", ",", "Al\u00b7le\u00b7lu\u00b7ja", ".", "Al\u00b7le\u00b7lu\u00b7ja", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "PPER", "PTKA", "ADJD", "$,", "NE", "$.", "NE", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.3": {"text": "Auch Jonas, der im Fische lag,", "tokens": ["Auch", "Jo\u00b7nas", ",", "der", "im", "Fi\u00b7sche", "lag", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kommt heute wieder an den Tag, Alleluja. Alleluja.", "tokens": ["Kommt", "heu\u00b7te", "wie\u00b7der", "an", "den", "Tag", ",", "Al\u00b7le\u00b7lu\u00b7ja", ".", "Al\u00b7le\u00b7lu\u00b7ja", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "APPR", "ART", "NN", "$,", "NE", "$.", "NE", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}, "stanza.6": {"line.1": {"text": "Ihr B\u00e4che Belials erschreckt", "tokens": ["Ihr", "B\u00e4\u00b7che", "Be\u00b7li\u00b7als", "er\u00b7schreckt"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "F\u00fcr Christo, den Gott aufferweckt, Alleluja. Alleluja.", "tokens": ["F\u00fcr", "Chris\u00b7to", ",", "den", "Gott", "auf\u00b7fer\u00b7weckt", ",", "Al\u00b7le\u00b7lu\u00b7ja", ".", "Al\u00b7le\u00b7lu\u00b7ja", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "NE", "$,", "ART", "NN", "VVPP", "$,", "NE", "$.", "NE", "$."], "meter": "--+--+-+-+-+-+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Es ist vergebens, was ihr schafft,", "tokens": ["Es", "ist", "ver\u00b7ge\u00b7bens", ",", "was", "ihr", "schafft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Denn euer Zorn ist ohne Krafft, Alleluja. Alleluja.", "tokens": ["Denn", "eu\u00b7er", "Zorn", "ist", "oh\u00b7ne", "Krafft", ",", "Al\u00b7le\u00b7lu\u00b7ja", ".", "Al\u00b7le\u00b7lu\u00b7ja", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "APPR", "NN", "$,", "NE", "$.", "NE", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}, "stanza.7": {"line.1": {"text": "Du alter Drache geh' anitzt", "tokens": ["Du", "al\u00b7ter", "Dra\u00b7che", "geh'", "a\u00b7nitzt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADJA", "NN", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ergrimmt und auf die Welt erhitzt, Alleluja. Alleluja.", "tokens": ["Er\u00b7grimmt", "und", "auf", "die", "Welt", "er\u00b7hitzt", ",", "Al\u00b7le\u00b7lu\u00b7ja", ".", "Al\u00b7le\u00b7lu\u00b7ja", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "KON", "APPR", "ART", "NN", "VVFIN", "$,", "NE", "$.", "NE", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.3": {"text": "Schau Christus, der zweyfache Held,", "tokens": ["Schau", "Chris\u00b7tus", ",", "der", "zwey\u00b7fa\u00b7che", "Held", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Ist der dich ritterlich gef\u00e4llt, Alleluja. Alleluja.", "tokens": ["Ist", "der", "dich", "rit\u00b7ter\u00b7lich", "ge\u00b7f\u00e4llt", ",", "Al\u00b7le\u00b7lu\u00b7ja", ".", "Al\u00b7le\u00b7lu\u00b7ja", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VAFIN", "ART", "PPER", "ADJD", "VVPP", "$,", "NE", "$.", "NE", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}, "stanza.8": {"line.1": {"text": "Du aber, den S\u00fcnd, H\u00f6ll undt Todt,", "tokens": ["Du", "a\u00b7ber", ",", "den", "S\u00fcnd", ",", "H\u00f6ll", "undt", "Todt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "ART", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.2": {"text": "Mu\u00df f\u00fcrchten als den wahren Gott, Alleluja. Alleluja.", "tokens": ["Mu\u00df", "f\u00fcrch\u00b7ten", "als", "den", "wah\u00b7ren", "Gott", ",", "Al\u00b7le\u00b7lu\u00b7ja", ".", "Al\u00b7le\u00b7lu\u00b7ja", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VMFIN", "VVINF", "KOKOM", "ART", "ADJA", "NN", "$,", "NE", "$.", "NE", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.3": {"text": "F\u00fcr den der Satan mit Verdru\u00df", "tokens": ["F\u00fcr", "den", "der", "Sa\u00b7tan", "mit", "Ver\u00b7dru\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Erbeben und erzittern mu\u00df, Alleluja. Alleluja.", "tokens": ["Er\u00b7be\u00b7ben", "und", "er\u00b7zit\u00b7tern", "mu\u00df", ",", "Al\u00b7le\u00b7lu\u00b7ja", ".", "Al\u00b7le\u00b7lu\u00b7ja", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "KON", "VVINF", "VMFIN", "$,", "NE", "$.", "NE", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}, "stanza.9": {"line.1": {"text": "Gib uns auf das erkante Leyd", "tokens": ["Gib", "uns", "auf", "das", "er\u00b7kan\u00b7te", "Leyd"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Dein Unschuld und Gerechtigkeit, Alleluja. Alleluja.", "tokens": ["Dein", "Un\u00b7schuld", "und", "Ge\u00b7rech\u00b7tig\u00b7keit", ",", "Al\u00b7le\u00b7lu\u00b7ja", ".", "Al\u00b7le\u00b7lu\u00b7ja", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "$,", "NE", "$.", "NE", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.3": {"text": "Damit auch wir so aufferstehn,", "tokens": ["Da\u00b7mit", "auch", "wir", "so", "auf\u00b7fer\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "word",