{"textgrid.poem.48712": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "13. Auf der durchl\u00e4uchtigsten Frauen, Frauen Augusten, F\u00fcrstl. Frau Mutter in Holstein, ihro F\u00fcrstl. Gnaden hochseliges Ableben", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die teure K\u00f6nigin der hochgef\u00fcrsten Frauen,", "tokens": ["Die", "teu\u00b7re", "K\u00f6\u00b7ni\u00b7gin", "der", "hoch\u00b7ge\u00b7f\u00fcrs\u00b7ten", "Frau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "des Landes Kraft und Trost, die durch des Himmels Gunst", "tokens": ["des", "Lan\u00b7des", "Kraft", "und", "Trost", ",", "die", "durch", "des", "Him\u00b7mels", "Gunst"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "KON", "NN", "$,", "PRELS", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "allein ihr ", "tokens": ["al\u00b7lein", "ihr"], "token_info": ["word", "word"], "pos": ["ADV", "PPOSAT"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "die nun ganz Deutschland fri\u00dft, der Armen ihr Vertrauen,", "tokens": ["die", "nun", "ganz", "Deutschland", "fri\u00dft", ",", "der", "Ar\u00b7men", "ihr", "Ver\u00b7trau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "NE", "VVFIN", "$,", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "der Frommen Schutz und Trost, der B\u00f6sen Furcht und Grauen,", "toke