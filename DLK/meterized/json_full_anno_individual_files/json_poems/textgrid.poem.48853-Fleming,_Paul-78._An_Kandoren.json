{"textgrid.poem.48853": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "78. An Kandoren", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Di\u00df wird vor dieses Mal wol sein der letzte Brief,", "tokens": ["Di\u00df", "wird", "vor", "die\u00b7ses", "Mal", "wol", "sein", "der", "letz\u00b7te", "Brief", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "PDAT", "NN", "ADV", "PPOSAT", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "den ich, ", "tokens": ["den", "ich", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "PPER", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "weil ich gesonnen bin mich anderweit zu wenden,", "tokens": ["weil", "ich", "ge\u00b7son\u00b7nen", "bin", "mich", "an\u00b7der\u00b7weit", "zu", "wen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "VAFIN", "PPER", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "dahin, noch eh' ich ward, mir mein Verh\u00e4ngn\u00fc\u00df rief.", "tokens": ["da\u00b7hin", ",", "noch", "eh'", "ich", "ward", ",", "mir", "mein", "Ver\u00b7h\u00e4ng\u00b7n\u00fc\u00df", "rief", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "ADV", "KOUS", "PPER", "VAFIN", "$,", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Mich wird der k\u00fchne Wind und ein verwognes Schiff", "tokens": ["Mich", "wird", "der", "k\u00fch\u00b7ne", "Wind", "und", "ein", "ver\u00b7wog\u00b7nes", "Schiff"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "weit f\u00fchren \u00fcber See, da hoff' ich an den Str\u00e4nden", "tokens": ["weit", "f\u00fch\u00b7ren", "\u00fc\u00b7ber", "See", ",", "da", "hoff'", "ich", "an", "den", "Str\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVINF", "APPR", "NN", "$,", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "des pr\u00e4chtigen ", "tokens": ["des", "pr\u00e4ch\u00b7ti\u00b7gen"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "wohin vor dieser Zeit kein deutsches Segel lief.", "tokens": ["wo\u00b7hin", "vor", "die\u00b7ser", "Zeit", "kein", "deut\u00b7sches", "Se\u00b7gel", "lief", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "PDAT", "NN", "PIAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ach! klagst du, wo wird nun ", "tokens": ["Ach", "!", "klagst", "du", ",", "wo", "wird", "nun"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "$.", "VVFIN", "PPER", "$,", "PWAV", "VAFIN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Nein, Licht, sei gutes Muts! Kan ich dir schon nit schreiben,", "tokens": ["Nein", ",", "Licht", ",", "sei", "gu\u00b7tes", "Muts", "!", "Kan", "ich", "dir", "schon", "nit", "schrei\u00b7ben", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "NN", "$,", "VAFIN", "ADJA", "NN", "$.", "VMFIN", "PPER", "PPER", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "so hat doch meine Post stets einen offnen Pa\u00df.", "tokens": ["so", "hat", "doch", "mei\u00b7ne", "Post", "stets", "ei\u00b7nen", "off\u00b7nen", "Pa\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PPOSAT", "NN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Erwachen soll kein Tag, kein Abend schlafen gehen,", "tokens": ["Er\u00b7wa\u00b7chen", "soll", "kein", "Tag", ",", "kein", "A\u00b7bend", "schla\u00b7fen", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PIAT", "NN", "$,", "PIAT", "NN", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "so soll der Westenwind dir in die Ohren wehen:", "tokens": ["so", "soll", "der", "Wes\u00b7ten\u00b7wind", "dir", "in", "die", "Oh\u00b7ren", "we\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "er lebt und denket dein, dein Freund, ohn' Unterla\u00df.", "tokens": ["er", "lebt", "und", "den\u00b7ket", "dein", ",", "dein", "Freund", ",", "ohn'", "Un\u00b7ter\u00b7la\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PPOSAT", "$,", "PPOSAT", "NN", "$,", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure"