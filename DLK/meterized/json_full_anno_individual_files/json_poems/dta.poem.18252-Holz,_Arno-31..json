": "iambic.tetra"}}, "stanza.28": {"line.1": {"text": "Du gabst mir einen wilden Rappen,", "tokens": ["Du", "gabst", "mir", "ei\u00b7nen", "wil\u00b7den", "Rap\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Umschn\u00fcrtest meine Brust mit Erz", "tokens": ["Um\u00b7schn\u00fcr\u00b7test", "mei\u00b7ne", "Brust", "mit", "Erz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und unter Thr\u00e4nen in mein Wappen", "tokens": ["Und", "un\u00b7ter", "Thr\u00e4\u00b7nen", "in", "mein", "Wap\u00b7pen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hast du gestickt ein blutend Herz!\u201c", "tokens": ["Hast", "du", "ge\u00b7stickt", "ein", "blu\u00b7tend", "Herz", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "ART", "ADJD", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}