{"dta.poem.18252": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "31.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1886", "urn": "urn:nbn:de:kobv:b4-200905192665", "language": ["de:0.99"], "booktitle": "Holz, Arno: Das Buch der Zeit. Lieder eines Modernen. Z\u00fcrich, 1886."}, "poem": {"stanza.1": {"line.1": {"text": "Und doch klingt's schon: \u201eEs war einmal!\u201c", "tokens": ["Und", "doch", "klingt's", "schon", ":", "\u201e", "Es", "war", "ein\u00b7mal", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADV", "VVFIN", "ADV", "$.", "$(", "PPER", "VAFIN", "ADV", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Wiege n\u00e4her als der Bahre,", "tokens": ["Der", "Wie\u00b7ge", "n\u00e4\u00b7her", "als", "der", "Bah\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Stieg ich tagt\u00e4glich ins Pennal.", "tokens": ["Stieg", "ich", "tag\u00b7t\u00e4g\u00b7lich", "ins", "Pen\u00b7nal", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "APPRART", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.2": {"line.1": {"text": "Ich war ein tr\u00e4umerischer Junge,", "tokens": ["Ich", "war", "ein", "tr\u00e4u\u00b7me\u00b7ri\u00b7scher", "Jun\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Las Cicero und Wilhelm Hauff", "tokens": ["Las", "Ci\u00b7ce\u00b7ro", "und", "Wil\u00b7helm", "Hauff"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "NE", "KON", "NE", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und trug das Herz auf meiner Zunge", "tokens": ["Und", "trug", "das", "Herz", "auf", "mei\u00b7ner", "Zun\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und spie\u00dfte Schmetterlinge auf.", "tokens": ["Und", "spie\u00df\u00b7te", "Schmet\u00b7ter\u00b7lin\u00b7ge", "auf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Auch lief ich, Katzengold zu suchen,", "tokens": ["Auch", "lief", "ich", ",", "Kat\u00b7zen\u00b7gold", "zu", "su\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "NE", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Oft Tage lang im Wald umher", "tokens": ["Oft", "Ta\u00b7ge", "lang", "im", "Wald", "um\u00b7her"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "NN", "ADJD", "APPRART", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und schw\u00e4rmte unter hohen Buchen", "tokens": ["Und", "schw\u00e4rm\u00b7te", "un\u00b7ter", "ho\u00b7hen", "Bu\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Von einstger Nimmerwiederkehr.", "tokens": ["Von", "einst\u00b7ger", "Nim\u00b7mer\u00b7wie\u00b7der\u00b7kehr", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Bet\u00e4ubend dufteten die Kressen,", "tokens": ["Be\u00b7t\u00e4u\u00b7bend", "duf\u00b7te\u00b7ten", "die", "Kres\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Gr\u00fcngolden flo\u00df das Licht herein;", "tokens": ["Gr\u00fcn\u00b7gol\u00b7den", "flo\u00df", "das", "Licht", "her\u00b7ein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Es war ein seliges Vergessen,", "tokens": ["Es", "war", "ein", "se\u00b7li\u00b7ges", "Ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Vergessen und Vergessensein!", "tokens": ["Ver\u00b7ges\u00b7sen", "und", "Ver\u00b7ges\u00b7sen\u00b7sein", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Der Lenzwind lie\u00df die Aeste knarren,", "tokens": ["Der", "Lenz\u00b7wind", "lie\u00df", "die", "A\u00b7es\u00b7te", "knar\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Vom Dorf her\u00fcber klang die Uhr,", "tokens": ["Vom", "Dorf", "her\u00b7\u00fc\u00b7ber", "klang", "die", "Uhr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich lag begraben unter Farren", "tokens": ["Ich", "lag", "be\u00b7gra\u00b7ben", "un\u00b7ter", "Far\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "VVPP", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und stammelte: \u201eNatur! Natur!", "tokens": ["Und", "stam\u00b7mel\u00b7te", ":", "\u201e", "Na\u00b7tur", "!", "Na\u00b7tur", "!"], "token_info": ["word", "word", "punct", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "$(", "NN", "$.", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "In alten B\u00fcchern steht geschrieben,", "tokens": ["In", "al\u00b7ten", "B\u00fc\u00b7chern", "steht", "ge\u00b7schrie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du bist ein Weib, ein sch\u00f6nes Weib;", "tokens": ["Du", "bist", "ein", "Weib", ",", "ein", "sch\u00f6\u00b7nes", "Weib", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich bin ein Mensch und mu\u00df dich lieben,", "tokens": ["Ich", "bin", "ein", "Mensch", "und", "mu\u00df", "dich", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "KON", "VMFIN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Denn diese Erde ist dein Leib!", "tokens": ["Denn", "die\u00b7se", "Er\u00b7de", "ist", "dein", "Leib", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Weh, jenem bleichen Nazarener!", "tokens": ["Weh", ",", "je\u00b7nem", "blei\u00b7chen", "Na\u00b7za\u00b7re\u00b7ner", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Er stie\u00df dich kalt von deinem Thron!", "tokens": ["Er", "stie\u00df", "dich", "kalt", "von", "dei\u00b7nem", "Thron", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich aber bin so gut wie jener", "tokens": ["Ich", "a\u00b7ber", "bin", "so", "gut", "wie", "je\u00b7ner"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VAFIN", "ADV", "ADJD", "KOKOM", "PDS"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Gottheit eingeborner Sohn!", "tokens": ["Der", "Got\u00b7theit", "ein\u00b7ge\u00b7bor\u00b7ner", "Sohn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Ich will nicht m\u00f6nchisch dich zergei\u00dfeln \u2014", "tokens": ["Ich", "will", "nicht", "m\u00f6n\u00b7chisch", "dich", "zer\u00b7gei\u00b7\u00dfeln"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "ADJD", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Her, deinen Freudenthr\u00e4nenwein!", "tokens": ["Her", ",", "dei\u00b7nen", "Freu\u00b7dent\u00b7hr\u00e4\u00b7nen\u00b7wein", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich will dein Bild in Feuer mei\u00dfeln", "tokens": ["Ich", "will", "dein", "Bild", "in", "Feu\u00b7er", "mei\u00b7\u00dfeln"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "APPR", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und Vollmensch wie ein Grieche sein!", "tokens": ["Und", "Voll\u00b7mensch", "wie", "ein", "Grie\u00b7che", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KOKOM", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Doch du, um die in ewgem Schwunge", "tokens": ["Doch", "du", ",", "um", "die", "in", "ew\u00b7gem", "Schwun\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "$,", "KOUI", "ART", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Welt sich dreht, o Poesie,", "tokens": ["Die", "Welt", "sich", "dreht", ",", "o", "Poe\u00b7sie", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "VVFIN", "$,", "FM", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "O, lege Gold auf meine Zunge", "tokens": ["O", ",", "le\u00b7ge", "Gold", "auf", "mei\u00b7ne", "Zun\u00b7ge"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "VVFIN", "NN", "APPR", "PPOSAT", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Und in mein Herz gie\u00df Melodie!", "tokens": ["Und", "in", "mein", "Herz", "gie\u00df", "Me\u00b7lo\u00b7die", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VVFIN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "In ewge Lieder la\u00df mich weben,", "tokens": ["In", "ew\u00b7ge", "Lie\u00b7der", "la\u00df", "mich", "we\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVIMP", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie du das Herz mir s\u00fc\u00df erhellt,", "tokens": ["Wie", "du", "das", "Herz", "mir", "s\u00fc\u00df", "er\u00b7hellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und wie so k\u00f6stlich doch das Leben", "tokens": ["Und", "wie", "so", "k\u00f6st\u00b7lich", "doch", "das", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ADV", "ADJD", "ADV", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und wie so wundersch\u00f6n die Welt!", "tokens": ["Und", "wie", "so", "wun\u00b7der\u00b7sch\u00f6n", "die", "Welt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ADV", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Noch g\u00e4hrt's von Blinden und von Tauben", "tokens": ["Noch", "g\u00e4hrt's", "von", "Blin\u00b7den", "und", "von", "Tau\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und mehr als ", "tokens": ["Und", "mehr", "als"], "token_info": ["word", "word", "word"], "pos": ["KON", "PIS", "KOKOM"], "meter": "+--", "measure": "dactylic.init"}, "line.3": {"text": "Ich aber lehre sie wieder glauben,", "tokens": ["Ich", "a\u00b7ber", "leh\u00b7re", "sie", "wie\u00b7der", "glau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ich will der neue Johannes sein!", "tokens": ["Ich", "will", "der", "neu\u00b7e", "Jo\u00b7han\u00b7nes", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NE", "VAINF", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.12": {"line.1": {"text": "In deine Wunder will ich wiegen", "tokens": ["In", "dei\u00b7ne", "Wun\u00b7der", "will", "ich", "wie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VMFIN", "PPER", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Sehnsucht ihres kranken Seins,", "tokens": ["Die", "Sehn\u00b7sucht", "ih\u00b7res", "kran\u00b7ken", "Seins", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In deine Arme will ich sie schmiegen,", "tokens": ["In", "dei\u00b7ne", "Ar\u00b7me", "will", "ich", "sie", "schmie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VMFIN", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Denn ich, du, sie ... o, wir alle sind Eins!\u201c", "tokens": ["Denn", "ich", ",", "du", ",", "sie", "...", "o", ",", "wir", "al\u00b7le", "sind", "Eins", "!", "\u201c"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPER", "$,", "PPER", "$,", "PPER", "$(", "FM", "$,", "PPER", "PIS", "VAFIN", "NN", "$.", "$("], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.13": {"line.1": {"text": "So lag ich tr\u00e4umend einst im Walde,", "tokens": ["So", "lag", "ich", "tr\u00e4u\u00b7mend", "einst", "im", "Wal\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn tiefblau rings der Himmel hing,", "tokens": ["Wenn", "tief\u00b7blau", "rings", "der", "Him\u00b7mel", "hing", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bis drau\u00dfen hinter gr\u00fcner Halde", "tokens": ["Bis", "drau\u00b7\u00dfen", "hin\u00b7ter", "gr\u00fc\u00b7ner", "Hal\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Sonne blutroth unterging.", "tokens": ["Die", "Son\u00b7ne", "blut\u00b7roth", "un\u00b7ter\u00b7ging", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "Dann schritt ich heimw\u00e4rts, und mit Singen", "tokens": ["Dann", "schritt", "ich", "heim\u00b7w\u00e4rts", ",", "und", "mit", "Sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "KON", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Begr\u00fc\u00dft ich meines Vaters Haus", "tokens": ["Be\u00b7gr\u00fc\u00dft", "ich", "mei\u00b7nes", "Va\u00b7ters", "Haus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und schaute, wenn die Sterne gingen,", "tokens": ["Und", "schau\u00b7te", ",", "wenn", "die", "Ster\u00b7ne", "gin\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "wor