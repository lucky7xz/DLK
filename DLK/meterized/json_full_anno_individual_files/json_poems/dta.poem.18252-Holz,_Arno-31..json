, "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Pre\u00dft er verzweifelt vors Gesicht", "tokens": ["Pre\u00dft", "er", "ver\u00b7zwei\u00b7felt", "vors", "Ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und rings die wei\u00dfget\u00fcnchten W\u00e4nde", "tokens": ["Und", "rings", "die", "wei\u00df\u00b7ge\u00b7t\u00fcnch\u00b7ten", "W\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Erzittern, wenn er schluchzend spricht:", "tokens": ["Er\u00b7zit\u00b7tern", ",", "wenn", "er", "schluch\u00b7zend", "spricht", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.23": {"line.1": {"text": "\u201eo Poesie, du Heiligsch\u00f6ne,", "tokens": ["\u201e", "o", "Poe\u00b7sie", ",", "du", "Hei\u00b7lig\u00b7sch\u00f6\u00b7ne", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "FM", "NN", "$,", "PPER", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Von Thr\u00e4nen ist mein Herz durchn\u00e4\u00dft,", "tokens": ["Von", "Thr\u00e4\u00b7nen", "ist", "mein", "Herz", "durc\u00b7hn\u00e4\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Weil du den treusten deiner S\u00f6hne", "tokens": ["Weil", "du", "den", "treus\u00b7ten", "dei\u00b7ner", "S\u00f6h\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "w