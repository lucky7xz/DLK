{"textgrid.poem.44100": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Hier hastu, sch\u00f6nes Kind, den herrlichsten Roman,", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hier hastu, sch\u00f6nes Kind, den herrlichsten Roman,", "tokens": ["Hier", "has\u00b7tu", ",", "sch\u00f6\u00b7nes", "Kind", ",", "den", "herr\u00b7lichs\u00b7ten", "Ro\u00b7man", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Aus dem die Keuschheit selbst viel Keuschheit lernen kan;", "tokens": ["Aus", "dem", "die", "Keuschheit", "selbst", "viel", "Keuschheit", "ler\u00b7nen", "kan", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "ADV", "PIAT", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Er ofenbahret dir die heilig-reinen Triebe,", "tokens": ["Er", "o\u00b7fen\u00b7bah\u00b7ret", "dir", "die", "hei\u00b7lig\u00b7rei\u00b7nen", "Trie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Zusamt der S\u00fc\u00dfigkeit der allerh\u00f6chsten Liebe.", "tokens": ["Zu\u00b7samt", "der", "S\u00fc\u00b7\u00dfig\u00b7keit", "der", "al\u00b7ler\u00b7h\u00f6chs\u00b7ten", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Verwirf das sch\u00f6ne Gift, so aus der Geilheit s\u00e4ugt", "tokens": ["Ver\u00b7wirf", "das", "sch\u00f6\u00b7ne", "Gift", ",", "so", "aus", "der", "Geil\u00b7heit", "s\u00e4ugt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN", "$,", "ADV", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und durch den stummen Brand sogar die Seelen t\u00f6dtet;", "tokens": ["Und", "durch", "den", "stum\u00b7men", "Brand", "so\u00b7gar", "die", "See\u00b7len", "t\u00f6d\u00b7tet", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Schau, wie die Sulamith vor Scham und Zorn err\u00f6thet,", "tokens": ["Schau", ",", "wie", "die", "Su\u00b7la\u00b7mith", "vor", "Scham", "und", "Zorn", "er\u00b7r\u00f6t\u00b7het", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ART", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.8": {"text": "Wenn Dinens Vorwiz sich den Landest\u00f6chtern zeigt.", "tokens": ["Wenn", "Di\u00b7nens", "Vor\u00b7wiz", "sich", "den", "Lan\u00b7de\u00b7st\u00f6ch\u00b7tern", "zeigt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "NN", "PRF", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Fleuch diese, folge der zum Myrrhenberge nach,", "tokens": ["Fleuch", "die\u00b7se", ",", "fol\u00b7ge", "der", "zum", "Myr\u00b7rhen\u00b7ber\u00b7ge", "nach", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PDAT", "$,", "VVFIN", "ART", "APPRART", "NN", "PTKVZ", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.10": {"text": "Wo Sarons Rose bl\u00fcht und wo der Lebensbach", "tokens": ["Wo", "Sa\u00b7rons", "Ro\u00b7se", "bl\u00fcht", "und", "wo", "der", "Le\u00b7bens\u00b7bach"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NE", "NE", "VVFIN", "KON", "PWAV", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und Str\u00f6me vor den Durst gequ\u00e4lter Sehnsucht flie\u00dfen.", "tokens": ["Und", "Str\u00f6\u00b7me", "vor", "den", "Durst", "ge\u00b7qu\u00e4l\u00b7ter", "Sehn\u00b7sucht", "flie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "ART", "NN", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Du bist ja auch wie sie des Himmels reine Braut,", "tokens": ["Du", "bist", "ja", "auch", "wie", "sie", "des", "Him\u00b7mels", "rei\u00b7ne", "Braut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "KOKOM", "PPER", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Mit der sich Gottes Kind von Ewigkeit vertraut;", "tokens": ["Mit", "der", "sich", "Got\u00b7tes", "Kind", "von", "E\u00b7wig\u00b7keit", "ver\u00b7traut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PRF", "NN", "NN", "APPR", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Sey wachsam und vergn\u00fcgt, den Br\u00e4utigam zu k\u00fc\u00dfen.", "tokens": ["Sey", "wach\u00b7sam",