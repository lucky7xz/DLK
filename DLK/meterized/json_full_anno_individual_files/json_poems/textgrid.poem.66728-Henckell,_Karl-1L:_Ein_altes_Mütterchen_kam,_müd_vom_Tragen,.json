{"textgrid.poem.66728": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein altes M\u00fctterchen kam, m\u00fcd vom Tragen,", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein altes M\u00fctterchen kam, m\u00fcd vom Tragen,", "tokens": ["Ein", "al\u00b7tes", "M\u00fct\u00b7ter\u00b7chen", "kam", ",", "m\u00fcd", "vom", "Tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "\u2013 Sie trug von Fallholz wohl die schwerste Last \u2013", "tokens": ["\u2013", "Sie", "trug", "von", "Fall\u00b7holz", "wohl", "die", "schwers\u00b7te", "Last", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "APPR", "NN", "ADV", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Den Weg daher, den j\u00fcngst wir eingeschlagen", "tokens": ["Den", "Weg", "da\u00b7her", ",", "den", "j\u00fcngst", "wir", "ein\u00b7ge\u00b7schla\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "PAV", "$,", "PRELS", "ADV", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zu Waldeswirtschaft und Erfrischungsrast.", "tokens": ["Zu", "Wal\u00b7des\u00b7wirt\u00b7schaft", "und", "Er\u00b7fri\u00b7schungs\u00b7rast", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Sie w\u00fcnschte freundlich, trotz der R\u00fcckenplagen,", "tokens": ["Sie", "w\u00fcnschte", "fre