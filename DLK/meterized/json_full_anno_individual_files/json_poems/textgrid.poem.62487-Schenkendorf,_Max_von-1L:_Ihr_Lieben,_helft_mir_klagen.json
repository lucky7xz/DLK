{"textgrid.poem.62487": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ihr Lieben, helft mir klagen", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr Lieben, helft mir klagen", "tokens": ["Ihr", "Lie\u00b7ben", ",", "helft", "mir", "kla\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "$,", "VVFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Um unsres Reiches Zier!", "tokens": ["Um", "uns\u00b7res", "Rei\u00b7ches", "Zier", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vom argen Tod erschlagen", "tokens": ["Vom", "ar\u00b7gen", "Tod", "er\u00b7schla\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Liegt unsre Herrin hier.", "tokens": ["Liegt", "uns\u00b7re", "Her\u00b7rin", "hier", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "O Mailand voll Cypressen,", "tokens": ["O", "Mai\u00b7land", "voll", "Cyp\u00b7res\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADJD", "NN", "$,"], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.6": {"text": "Du tr\u00fcbe Maienzeit,", "tokens": ["Du", "tr\u00fc\u00b7be", "Mai\u00b7en\u00b7zeit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Verona \u2013 nie vergessen", "tokens": ["Ve\u00b7ro\u00b7na", "\u2013", "nie", "ver\u00b7ges\u00b7sen"], "token_info": ["word", "punct", "word", "word"], "pos": ["NE", "$(", "ADV", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wird solches Weh und Leid.", "tokens": ["Wird", "sol\u00b7ches", "Weh", "und", "Leid", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ihr hattet sie gesendet,", "tokens": ["Ihr", "hat\u00b7tet", "sie", "ge\u00b7sen\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nun habt ihr sie geraubt,", "tokens": ["Nun", "habt", "ihr", "sie", "ge\u00b7raubt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das Kleinod uns entwendet,", "tokens": ["Das", "Klei\u00b7nod", "uns", "ent\u00b7wen\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den sch\u00f6nen Baum entlaubt.", "tokens": ["Den", "sch\u00f6\u00b7nen", "Baum", "ent\u00b7laubt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Du hoher Stamm von Este,", "tokens": ["Du", "ho\u00b7her", "Stamm", "von", "Es\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wie traurig stehst du nun,", "tokens": ["Wie", "trau\u00b7rig", "stehst", "du", "nun", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und senkest deine Aeste,", "tokens": ["Und", "sen\u00b7kest", "dei\u00b7ne", "A\u00b7es\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wie Thr\u00e4nenweiden thun.", "tokens": ["Wie", "Thr\u00e4\u00b7nen\u00b7wei\u00b7den", "thun", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Du schienest j\u00fcngst zu prangen", "tokens": ["Du", "schie\u00b7nest", "j\u00fcngst", "zu", "pran\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In deiner h\u00f6chsten Zier,", "tokens": ["In", "dei\u00b7ner", "h\u00f6chs\u00b7ten", "Zier", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es blickte voll Verlangen", "tokens": ["Es", "blick\u00b7te", "voll", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Kaiser Franz nach dir,", "tokens": ["Der", "Kai\u00b7ser", "Franz", "nach", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "APPR", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wie nach den Lorbeerzweigen", "tokens": ["Wie", "nach", "den", "Lor\u00b7beer\u00b7zwei\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Dein Tasso k\u00fchn geschaut:", "tokens": ["Dein", "Tas\u00b7so", "k\u00fchn", "ge\u00b7schaut", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wir sahen fromm sich neigen", "tokens": ["Wir", "sa\u00b7hen", "fromm", "sich", "nei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "PRF", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die kaiserliche Braut.", "tokens": ["Die", "kai\u00b7ser\u00b7li\u00b7che", "Braut", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Italia magst du meinen,", "tokens": ["I\u00b7ta\u00b7lia", "magst", "du", "mei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "O Land, so lusterf\u00fcllt,", "tokens": ["O", "Land", ",", "so", "lus\u00b7ter\u00b7f\u00fcllt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es k\u00e4m' aus ", "tokens": ["Es", "k\u00e4m'", "aus"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Das gnadenreiche Bild?", "tokens": ["Das", "gna\u00b7den\u00b7rei\u00b7che", "Bild", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wir haben sie erzogen", "tokens": ["Wir", "ha\u00b7ben", "sie", "er\u00b7zo\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "So fromm, so stark und weich,", "tokens": ["So", "fromm", ",", "so", "stark", "und", "weich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wir an der Donau Wogen,", "tokens": ["Wir", "an", "der", "Do\u00b7nau", "Wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NE", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wir in dem deutschen Reich.", "tokens": ["Wir", "in", "dem", "deut\u00b7schen", "Reich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Die deutschen Kl\u00e4nge drangen", "tokens": ["Die", "deut\u00b7schen", "Kl\u00e4n\u00b7ge", "dran\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Allm\u00e4chtig an ihr Herz,", "tokens": ["All\u00b7m\u00e4ch\u00b7tig", "an", "ihr", "Herz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die deutschen Lieder sangen", "tokens": ["Die", "deut\u00b7schen", "Lie\u00b7der", "san\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ihr eigen Lust und Schmerz.", "tokens": ["Ihr", "ei\u00b7gen", "Lust", "und", "Schmerz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da kam sie, zu verkl\u00e4ren", "tokens": ["Da", "kam", "sie", ",", "zu", "ver\u00b7kl\u00e4\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Das Marterthum der Zeit,", "tokens": ["Das", "Mar\u00b7ter\u00b7thum", "der", "Zeit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und ew'gen Kranz der Ehren", "tokens": ["Und", "ew'\u00b7gen", "Kranz", "der", "Eh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wand ihr das bittre Leid.", "tokens": ["Wand", "ihr", "das", "bitt\u00b7re", "Leid", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "O Lied, du sollst nicht melden", "tokens": ["O", "Lied", ",", "du", "sollst", "nicht", "mel\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NN", "$,", "PPER", "VMFIN", "PTKNEG", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Entschwundner Leiden Zahl,", "tokens": ["Ent\u00b7schwund\u00b7ner", "Lei\u00b7den", "Zahl", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Den ew'gen Schmerz der Helden,", "tokens": ["Den", "ew'\u00b7gen", "Schmerz", "der", "Hel\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Fraun und Jungfraun Qual.", "tokens": ["Der", "Fraun", "und", "Jung\u00b7fraun", "Qual", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ihr B\u00f6heims Wunderquellen,", "tokens": ["Ihr", "B\u00f6\u00b7heims", "Wun\u00b7der\u00b7quel\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Du gottgeweihte Flut,", "tokens": ["Du", "gott\u00b7ge\u00b7weih\u00b7te", "Flut", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Saht ihre Thr\u00e4nen schwellen,", "tokens": ["Saht", "ih\u00b7re", "Thr\u00e4\u00b7nen", "schwel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ihr saht auch ihren Muth.", "tokens": ["Ihr", "saht", "auch", "ih\u00b7ren", "Muth", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Ein leuchtend Himmelszeichen,", "tokens": ["Ein", "leuch\u00b7tend", "Him\u00b7mels\u00b7zei\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So schwebte sie uns vor,", "tokens": ["So", "schweb\u00b7te", "sie", "uns", "vor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Hob aus des Staubes Reichen", "tokens": ["Hob", "aus", "des", "Stau\u00b7bes", "Rei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "ART", "NN", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Ihr nach uns all' empor.", "tokens": ["Ihr", "nach", "uns", "all'", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPER", "PIS", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der S\u00fcnder floh verlegen", "tokens": ["Der", "S\u00fcn\u00b7der", "floh", "ver\u00b7le\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Vor ihrem reinen Blick,", "tokens": ["Vor", "ih\u00b7rem", "rei\u00b7nen", "Blick", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und wich von ihren Wegen", "tokens": ["Und", "wich", "von", "ih\u00b7ren", "We\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Mit Scham und Grimm zur\u00fcck.", "tokens": ["Mit", "Scham", "und", "Grimm", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NE", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Und sollen wir dich missen,", "tokens": ["Und", "sol\u00b7len", "wir", "dich", "mis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du gl\u00e4nzendes Panier?", "tokens": ["Du", "gl\u00e4n\u00b7zen\u00b7des", "Pa\u00b7nier", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bist ewig uns entrissen,", "tokens": ["Bist", "e\u00b7wig", "uns", "ent\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Frauen Stolz und Zier?", "tokens": ["Der", "Frau\u00b7en", "Stolz", "und", "Zier", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wer soll die K\u00e4mpfer leiten?", "tokens": ["Wer", "soll", "die", "K\u00e4mp\u00b7fer", "lei\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Sind Frauen doch ihr Stern!", "tokens": ["Sind", "Frau\u00b7en", "doch", "ihr", "Stern", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wer wandelt nun zur Seiten", "tokens": ["Wer", "wan\u00b7delt", "nun", "zur", "Sei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ADV", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Dem h\u00f6chsten deutschen Herrn?", "tokens": ["Dem", "h\u00f6chs\u00b7ten", "deut\u00b7schen", "Herrn", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Du wirst uns nicht vers\u00e4umen,", "tokens": ["Du", "wirst", "uns", "nicht", "ver\u00b7s\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "O treues Mutterherz!", "tokens": ["O", "treu\u00b7es", "Mut\u00b7ter\u00b7