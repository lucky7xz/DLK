, "Gr\u00fcn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Und des Laubes smaragdene Flammen", "tokens": ["Und", "des", "Lau\u00b7bes", "sma\u00b7rag\u00b7de\u00b7ne", "Flam\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Uns zur Seite, zu H\u00e4upten spr\u00fchn.", "tokens": ["Uns", "zur", "Sei\u00b7te", ",", "zu", "H\u00e4up\u00b7ten", "spr\u00fchn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "$,", "APPR", "NN", "VVINF", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.3": {"line.1": {"text": "Aber dichter und dichter", "tokens": ["A\u00b7ber", "dich\u00b7ter", "und", "dich\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJD", "KON", "ADJA"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Schlie\u00df um uns sich das Bl\u00e4ttergerank,", "tokens": ["Schlie\u00df", "um", "uns", "sich", "das", "Bl\u00e4t\u00b7ter\u00b7ge\u00b7rank", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "PRF", "ART", "NN", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Immer noch spielen zitternde Lichter", "tokens": ["Im\u00b7mer", "noch", "spie\u00b7len", "zit\u00b7tern\u00b7de", "Lich\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "ADJA", "NN"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Zu uns herab auf die Rasenbank.", "tokens": ["Zu", "uns", "her\u00b7ab", "auf", "die", "Ra\u00b7sen\u00b7bank", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["