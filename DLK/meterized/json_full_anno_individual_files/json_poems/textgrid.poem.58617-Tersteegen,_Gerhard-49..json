{"textgrid.poem.58617": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "49.", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Los ist mir, o Gnad', aufs lieblichste gefallen,", "tokens": ["Das", "Los", "ist", "mir", ",", "o", "Gnad'", ",", "aufs", "lieb\u00b7lichs\u00b7te", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "$,", "FM", "NN", "$,", "APPRART", "ADJA", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Marias besten Teil hab' ich mir auserw\u00e4hlt;", "tokens": ["Ma\u00b7rias", "bes\u00b7ten", "Teil", "hab'", "ich", "mir", "au\u00b7ser\u00b7w\u00e4hlt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "VAFIN", "PPER", "PPER", "VVPP", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Mein Sch\u00f6pfer ist mein Mann, mein Gut, mein Schatz vor allen,", "tokens": ["Mein", "Sch\u00f6p\u00b7fer", "ist", "mein", "Mann", ",", "mein", "Gut", ",", "mein", "Schatz", "vor", "al\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "APPR", "PIAT", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ja, ja, mein Herz sich ihm in keuscher Lieb' verm\u00e4hlt.", "tokens": ["Ja", ",", "ja", ",", "mein", "Herz", "sich", "ihm", "in", "keu\u00b7scher", "Lieb'", "ver\u00b7m\u00e4hlt", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PTKANT", "$,", "PPOSAT", "NN", "PRF", "PPER", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Weg alle Kreatur! du bist dem Geist zu wenig,", "tokens": ["Weg", "al\u00b7le", "Kre\u00b7a\u00b7tur", "!", "du", "bist", "dem", "Geist", "zu", "we\u00b7nig", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "$.", "PPER", "VAFIN", "ART", "NN", "APPR", "PIS", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Mein Held, auf den ich trau', der hei\u00dft Herr Zebaoth,", "tokens": ["Mein", "Held", ",", "auf", "den", "ich", "trau'", ",", "der", "hei\u00dft", "Herr", "Ze\u00b7bao\u00b7th", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "APPR", "PRELS", "PPER", "VVFIN", "$,", "PRELS", "VVFIN", "NN", "NE", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und mein Erl\u00f6ser ist der Erd- und Himmelsk\u00f6nig;", "tokens": ["Und", "mein", "Er\u00b7l\u00f6\u00b7ser", "ist", "der", "Erd", "und", "Him\u00b7mels\u00b7k\u00f6\u00b7nig", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ART", "TRUNC", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ich bin sein Eigentum, und er allein mein Gott.", "tokens": ["Ich", "bin", "sein", "Ei\u00b7gen\u00b7tum", ",", "und", "er", "al\u00b7lein", "mein", "Gott", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "$,", "KON", "PPER", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic