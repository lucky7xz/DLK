{"dta.poem.11637": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Gesang Weyla's.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.99"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "Das ferne leuchtet;", "tokens": ["Das", "fer\u00b7ne", "leuch\u00b7tet", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Vom Meere dampfet dein erw\u00e4rmter Strand", "tokens": ["Vom", "Mee\u00b7re", "damp\u00b7fet", "dein", "er\u00b7w\u00e4rm\u00b7ter", "Strand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Den Nebel, so der G\u00f6tter Wange feuchtet.", "tokens": ["Den", "Ne\u00b7bel", ",", "so", "der", "G\u00f6t\u00b7ter", "Wan\u00b7ge", "feuch\u00b7tet", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Uralte Wasser steigen", "tokens": ["Ur\u00b7al\u00b7te", "Was\u00b7ser", "stei\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Verj\u00fcngt um deine H\u00fcften, Kind!", "tokens": ["Ver\u00b7j\u00fcngt", "um", "dei\u00b7ne", "H\u00fcf\u00b7ten", ",", "Kind", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vor deiner Gottheit beugen", "tokens": ["Vor", "dei\u00b7ner", "Got\u00b7theit", "beu\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sich K\u00f6nige, die deine W\u00e4rter sind.", "tokens": ["Sich", "K\u00f6\u00b7ni\u00b7ge", ",", "die", "dei\u00b7ne", "W\u00e4r\u00b7ter", "sind", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PRF", "NN", "$,", "PRELS", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}