{"textgrid.poem.30558": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "72.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df die M\u00e4rter-Kron, die Haube, Charitea hat erworben,", "tokens": ["Da\u00df", "die", "M\u00e4r\u00b7ter\u00b7Kron", ",", "die", "Hau\u00b7be", ",", "Cha\u00b7ri\u00b7tea", "hat", "er\u00b7wor\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "ART", "NN", "$,", "NE", "VAFIN", "VVPP", "$,"], "meter": "--+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Da\u00df als eine keusche Jungfer sie zu einer Frau erstorben,", "tokens": ["Da\u00df", "als", "ei\u00b7ne", "keu\u00b7sche", "Jung\u00b7fer", "sie", "zu", "ei\u00b7ner", "Frau", "ers\u00b7tor\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "ART", "ADJA", "NN", "PPER", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "H\u00f6rte Chloris, seuffzte hertzlich: Wolt ich doch f\u00fcr meine S\u00fcnden", "tokens": ["H\u00f6r\u00b7te", "Chlo\u00b7ris", ",", "seuffz\u00b7te", "hertz\u00b7lich", ":", "Wolt", "ich", "doch", "f\u00fcr", "mei\u00b7ne", "S\u00fcn\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "NE", "$,", "VVFIN", "ADJD", "$.", "VMFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Auch wol mich, wann GOTT nur wolte, mit dem sterben abefinden!", "tokens": ["Auch", "wol", "mich", ",", "wann", "GoTT", "nur", "wol\u00b7te", ",", "mit", "dem", "ste