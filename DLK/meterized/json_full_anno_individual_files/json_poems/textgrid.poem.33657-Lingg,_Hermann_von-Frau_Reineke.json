{"textgrid.poem.33657": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "Frau Reineke", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Mann ist auf der Vogeljagd,", "tokens": ["Mein", "Mann", "ist", "auf", "der", "Vo\u00b7gel\u00b7jagd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er schie\u00dft H\u00fchner und H\u00e4hne;", "tokens": ["Er", "schie\u00dft", "H\u00fch\u00b7ner", "und", "H\u00e4h\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Er hat eine B\u00fcchs, die nie versagt,", "tokens": ["Er", "hat", "ei\u00b7ne", "B\u00fcchs", ",", "die", "nie", "ver\u00b7sagt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PRELS", "ADV", "VVPP", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Er hat auch lange, scharfe Z\u00e4hne.", "tokens": ["Er", "hat", "auch", "lan\u00b7ge", ",", "schar\u00b7fe", "Z\u00e4h\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wo ist er? Auf der Heide draus,", "tokens": ["Wo", "ist", "er", "?", "Auf", "der", "Hei\u00b7de", "draus", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "$.", "APPR", "ART", "NN", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wo die schwarzen S\u00e4ue pferchen.", "tokens": ["Wo", "die", "schwar\u00b7zen", "S\u00e4u\u00b7e", "pfer\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Enten bringt er gewi\u00df nach Haus,", "tokens": ["En\u00b7ten", "bringt", "er", "ge\u00b7wi\u00df", "nach", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "APPR", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Vielleicht auch honigs\u00fc\u00dfe Lerchen.", "tokens": ["Viel\u00b7leicht", "auch", "ho\u00b7nig\u00b7s\u00fc\u00b7\u00dfe", "Ler\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Mein Mann ist auf der Vogeljagd,", "tokens": ["Mein", "Mann", "ist", "auf", "der", "Vo\u00b7gel\u00b7jagd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er schie\u00dft H\u00fchner und H\u00e4hne;", "tokens": ["Er", "schie\u00dft", "H\u00fch\u00b7ner", "und", "H\u00e4h\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Er hat eine B\u00fcchs, die nie versagt,", "tokens": ["Er", "hat", "ei\u00b7ne", "B\u00fcchs", ",", "die", "nie", "ver\u00b7sagt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PRELS", "ADV", "VVPP", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.