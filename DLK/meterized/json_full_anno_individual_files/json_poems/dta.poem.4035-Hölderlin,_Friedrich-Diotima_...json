{"dta.poem.4035": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Diotima ..", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905197104", "language": ["de:0.99"], "booktitle": "H\u00f6lderlin, Friedrich: Gedichte. Stuttgart u. a., 1826."}, "poem": {"stanza.1": {"line.1": {"text": "Du schweigst und duldest, denn sie verstehen dich", "tokens": ["Du", "schweigst", "und", "dul\u00b7dest", ",", "denn", "sie", "ver\u00b7ste\u00b7hen", "dich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$,", "KON", "PPER", "VVFIN", "PPER"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "nicht.", "tokens": ["nicht", "."], "token_info": ["word", "punct"], "pos": ["PTKNEG", "$."], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Du edles Leben! siehest zur Erd' und schweigst", "tokens": ["Du", "ed\u00b7les", "Le\u00b7ben", "!", "sie\u00b7hest", "zur", "Erd'", "und", "schweigst"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADJA", "NN", "$.", "VVFIN", "APPRART", "NN", "KON", "VVFIN"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Am sch\u00f6nen Tag, denn ach! umsonst nur", "tokens": ["Am", "sch\u00f6\u00b7nen", "Tag", ",", "denn", "ach", "!", "um\u00b7sonst", "nur"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "$,", "KON", "XY", "$.", "ADV", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Suchst du die Deinen im Sonnenlichte,", "tokens": ["Suchst", "du", "die", "Dei\u00b7nen", "im", "Son\u00b7nen\u00b7lich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "PPOSAT", "APPRART", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Die K\u00f6niglichen, welche wie Br\u00fcder doch,", "tokens": ["Die", "K\u00f6\u00b7nig\u00b7li\u00b7chen", ",", "wel\u00b7che", "wie", "Br\u00fc\u00b7der", "doch", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "KOKOM", "NN", "ADV", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wie eines Hains gesellige Gipfel sonst", "tokens": ["Wie", "ei\u00b7nes", "Hains", "ge\u00b7sel\u00b7li\u00b7ge", "Gip\u00b7fel", "sonst"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ADJA", "NN", "ADV"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Der Lieb' und Heimath sich und ihres", "tokens": ["Der", "Lieb'", "und", "Hei\u00b7math", "sich", "und", "ih\u00b7res"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN", "PRF", "KON", "PPOSAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Immer umfangenden Himmels freuten.", "tokens": ["Im\u00b7mer", "um\u00b7fan\u00b7gen\u00b7den", "Him\u00b7mels", "freu\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Des Ursprungs noch in t\u00f6nender Brust gedenk;", "tokens": ["Des", "Ur\u00b7sprungs", "noch", "in", "t\u00f6\u00b7nen\u00b7der", "Brust", "ge\u00b7denk", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die Dankbarn, sie, sie mein' ich, die einzig treu", "tokens": ["Die", "Dank\u00b7barn", ",", "sie", ",", "sie", "mein'", "ich", ",", "die", "ein\u00b7zig", "treu"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PPER", "$,", "PPER", "VVFIN", "PPER", "$,", "PRELS", "ADJD", "ADJD"], "meter": "-+---+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Bis in den Tartarus die Freude", "tokens": ["Bis", "in", "den", "Tar\u00b7ta\u00b7rus", "die", "Freu\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Brachten, die Freien, die G\u00f6ttermenschen.", "tokens": ["Brach\u00b7ten", ",", "die", "Frei\u00b7en", ",", "die", "G\u00f6t\u00b7ter\u00b7men\u00b7schen", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "Die z\u00e4rtlich gro\u00dfen Seelen, die nimmer sind;", "tokens": ["Die", "z\u00e4rt\u00b7lich", "gro\u00b7\u00dfen", "See\u00b7len", ",", "die", "nim\u00b7mer", "sind", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$,", "PRELS", "ADV", "VAFIN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Denn sie beweint, so lange das Trauerjahr", "tokens": ["Denn", "sie", "be\u00b7weint", ",", "so", "lan\u00b7ge", "das", "Trau\u00b7er\u00b7jahr"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVPP", "$,", "ADV", "ADV", "ART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Schon dauert, von den vor'gen Sternen", "tokens": ["Schon", "dau\u00b7ert", ",", "von", "den", "vor'\u00b7gen", "Ster\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "T\u00e4glich gemahnet, das Herz noch immer.", "tokens": ["T\u00e4g\u00b7lich", "ge\u00b7mah\u00b7net", ",", "das", "Herz", "noch", "im\u00b7mer", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,", "ART", "NN", "ADV", "ADV", "$."], "meter": "+--+--+-++", "measure": "dactylic.di.plus"}}, "stanza.5": {"line.1": {"text": "Und diese Todtenklage, sie ruht nicht aus,", "tokens": ["Und", "die\u00b7se", "Tod\u00b7ten\u00b7kla\u00b7ge", ",", "sie", "ruht", "nicht", "aus", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "$,", "PPER", "VVFIN", "PTKNEG", "PTKVZ", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die Zeit doch heilt. Die Himmlischen sind jetzt stark,", "tokens": ["Die", "Zeit", "doch", "heilt", ".", "Die", "Himm\u00b7li\u00b7schen", "sind", "jetzt", "stark", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "$.", "ART", "NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Sind schnell. Nimmt denn nicht schon ihr altes", "tokens": ["Sind", "schnell", ".", "Nimmt", "denn", "nicht", "schon", "ihr", "al\u00b7tes"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADJD", "$.", "VVFIN", "ADV", "PTKNEG", "ADV", "PPOSAT", "ADJA"], "meter": "--+-++-+-", "measure": "anapaest.init"}, "line.4": {"text": "Freudiges Recht die Natur sich wieder?", "tokens": ["Freu\u00b7di\u00b7ges", "Recht", "die", "Na\u00b7tur", "sich", "wie\u00b7der", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "PRF", "ADV", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.6": {"line.1": {"text": "Sieh! eh noch unser H\u00fcgel, o Liebe, sinkt,", "tokens": ["Sieh", "!", "eh", "noch", "un\u00b7ser", "H\u00fc\u00b7gel", ",", "o", "Lie\u00b7be", ",", "sinkt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NE", "$.", "KOUS", "ADV", "PPOSAT", "NN", "$,", "FM", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Geschieht's und ja! noch siehet mein sterblich Lied", "tokens": ["Ge\u00b7schieht's", "und", "ja", "!", "noch", "sie\u00b7het", "mein", "sterb\u00b7lich", "Lied"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "KON", "ADV", "$.", "ADV", "VVFIN", "PPOSAT", "ADJD", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Den Tag, der, Diotima! n\u00e4chst den", "tokens": ["Den", "Tag", ",", "der", ",", "Dio\u00b7ti\u00b7ma", "!", "n\u00e4chst", "den"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "$,", "NE", "$.", "ADV", "ART"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "G\u00f6ttern mit Helden dich nennt, und dir gleicht.", "tokens": ["G\u00f6t\u00b7tern", "mit", "Hel\u00b7den", "dich", "nennt", ",", "und", "dir", "gleicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "PPER", "VVFIN", "$,", "KON", "PPER", "VVFIN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}}}}