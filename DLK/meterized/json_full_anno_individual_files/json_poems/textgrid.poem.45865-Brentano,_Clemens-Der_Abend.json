{"textgrid.poem.45865": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Der Abend", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nach seiner Heimat k\u00fchlen Lorbeerhainen", "tokens": ["Nach", "sei\u00b7ner", "Hei\u00b7mat", "k\u00fch\u00b7len", "Lor\u00b7beer\u00b7hai\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Schwebt auf der goldnen Schale", "tokens": ["Schwebt", "auf", "der", "gold\u00b7nen", "Scha\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Schon Helios, es gl\u00fchen rings die Wellen,", "tokens": ["Schon", "He\u00b7li\u00b7os", ",", "es", "gl\u00fc\u00b7hen", "rings", "die", "Wel\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "$,", "PPER", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Ozean erschwillt in frohen Scheinen,", "tokens": ["Der", "O\u00b7ze\u00b7an", "er\u00b7schwillt", "in", "fro\u00b7hen", "Schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Die wie mit Blitzesstrahle", "tokens": ["Die", "wie", "mit", "Blit\u00b7zes\u00b7strah\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "KOKOM", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die ernste Nacht der fernen Ufer hellen,", "tokens": ["Die", "erns\u00b7te", "Nacht", "der", "fer\u00b7nen", "U\u00b7fer", "hel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und \u00fcber alle Schwellen", "tokens": ["Und", "\u00fc\u00b7ber", "al\u00b7le", "Schwel\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ergie\u00dft der Gott die stillen Feuerwogen", "tokens": ["Er\u00b7gie\u00dft", "der", "Gott", "die", "stil\u00b7len", "Feu\u00b7er\u00b7wo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Zum ew'gen Himmelsbogen,", "tokens": ["Zum", "ew'\u00b7gen", "Him\u00b7mels\u00b7bo\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Da\u00df von den Bergen durch das dunkle Leben", "tokens": ["Da\u00df", "von", "den", "Ber\u00b7gen", "durch", "das", "dunk\u00b7le", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Des Tages Flammen widerhallend beben.", "tokens": ["Des", "Ta\u00b7ges", "Flam\u00b7men", "wi\u00b7der\u00b7hal\u00b7lend", "be\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVPP", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Hoch auf den Bergen wehen seine Flammen,", "tokens": ["Hoch", "auf", "den", "Ber\u00b7gen", "we\u00b7hen", "sei\u00b7ne", "Flam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den raschen Mann zu f\u00fchren,", "tokens": ["Den", "ra\u00b7schen", "Mann", "zu", "f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der seiner Reise Ziel noch nicht errungen,", "tokens": ["Der", "sei\u00b7ner", "Rei\u00b7se", "Ziel", "noch", "nicht", "er\u00b7run\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "NN", "ADV", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Er strahlet mit dem Glanze stets zusammen,", "tokens": ["Er", "strah\u00b7let", "mit", "dem", "Glan\u00b7ze", "stets", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wenn gleich die F\u00fc\u00dfe gleiten,", "tokens": ["Wenn", "gleich", "die", "F\u00fc\u00b7\u00dfe", "glei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Bleibt von dem Lichte doch sein Haupt umschlungen.", "tokens": ["Bleibt", "von", "dem", "Lich\u00b7te", "doch", "sein", "Haupt", "um\u00b7schlun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Nie von der Nacht bezwungen", "tokens": ["Nie", "von", "der", "Nacht", "be\u00b7zwun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Lenkt ruhig nach der Sterne heil'gem Feuer,", "tokens": ["Lenkt", "ru\u00b7hig", "nach", "der", "Ster\u00b7ne", "heil'\u00b7gem", "Feu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Das ernste Schiff den Steuer", "tokens": ["Das", "erns\u00b7te", "Schiff", "den", "Steu\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Und wandelt heimw\u00e4rts durch die dunkeln Fluten", "tokens": ["Und", "wan\u00b7delt", "heim\u00b7w\u00e4rts", "durch", "die", "dun\u00b7keln", "Flu\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Vertrauend auf des Leuchtturms hohe Gluten.", "tokens": ["Ver\u00b7trau\u00b7end", "auf", "des", "Leucht\u00b7turms", "ho\u00b7he", "Glu\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Vom k\u00fchnen Felsen rinnen Lichter nieder,", "tokens": ["Vom", "k\u00fch\u00b7nen", "Fel\u00b7sen", "rin\u00b7nen", "Lich\u00b7ter", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die T\u00e4ler zu ergr\u00fcnden,", "tokens": ["Die", "T\u00e4\u00b7ler", "zu", "er\u00b7gr\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und wo des Feuers milde Quelle ziehet,", "tokens": ["Und", "wo", "des", "Feu\u00b7ers", "mil\u00b7de", "Quel\u00b7le", "zie\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Verglimmen bald des Haines wilde Lieder,", "tokens": ["Ver\u00b7glim\u00b7men", "bald", "des", "Hai\u00b7nes", "wil\u00b7de", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Denn alle T\u00f6ne schwinden,", "tokens": ["Denn", "al\u00b7le", "T\u00f6\u00b7ne", "schwin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Bis sie des Abends Flammen rein gegl\u00fchet \u2013", "tokens": ["Bis", "sie", "des", "A\u00b7bends", "Flam\u00b7men", "rein", "ge\u00b7gl\u00fc\u00b7het", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und welch ein Lied erbl\u00fchet \u2013", "tokens": ["Und", "welch", "ein", "Lied", "er\u00b7bl\u00fc\u00b7het", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAT", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Es flicht die Nachtigall die goldnen Schlingen", "tokens": ["Es", "flicht", "die", "Nach\u00b7ti\u00b7gall", "die", "gold\u00b7nen", "Schlin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Und s\u00fc\u00df gefangen ringen", "tokens": ["Und", "s\u00fc\u00df", "ge\u00b7fan\u00b7gen", "rin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJD", "ADJD", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Im Liede Liebesschmerz und Schmerzesliebe,", "tokens": ["Im", "Lie\u00b7de", "Lie\u00b7bes\u00b7schmerz", "und", "Schmer\u00b7zes\u00b7lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Da\u00df Schmerz in Liebe, Lieb' in Schmerz sich \u00fcbe", "tokens": ["Da\u00df", "Schmerz", "in", "Lie\u00b7be", ",", "Lieb'", "in", "Schmerz", "sich", "\u00fc\u00b7be"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "APPR", "NN", "$,", "NN", "APPR", "NN", "PRF", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "So drang der T\u00f6ne Fr\u00fchling aus dem Schweigen,", "tokens": ["So", "drang", "der", "T\u00f6\u00b7ne", "Fr\u00fch\u00b7ling", "aus", "dem", "Schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So auch in reinen Seelen", "tokens": ["So", "auch", "in", "rei\u00b7nen", "See\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Des Tages wilde K\u00e4mpfe bald zerrinnen,", "tokens": ["Des", "Ta\u00b7ges", "wil\u00b7de", "K\u00e4mp\u00b7fe", "bald", "zer\u00b7rin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wenn Lieb' und Schmerz sich hold zusammen neigen,", "tokens": ["Wenn", "Lieb'", "und", "Schmerz", "sich", "hold", "zu\u00b7sam\u00b7men", "nei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "PRF", "ADJD", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Die Zwietracht zu verhehlen,", "tokens": ["Die", "Zwiet\u00b7racht", "zu", "ver\u00b7heh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und r\u00fchrend doch den ew'gen Streit beginnen.", "tokens": ["Und", "r\u00fch\u00b7rend", "doch", "den", "ew'\u00b7gen", "Streit", "be\u00b7gin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Ach keine mag gewinnen! \u2013", "tokens": ["Ach", "kei\u00b7ne", "mag", "ge\u00b7win\u00b7nen", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ITJ", "PIAT", "VMFIN", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ein Wundergift flie\u00dft beiden von den Pfeilen,", "tokens": ["Ein", "Wun\u00b7der\u00b7gift", "flie\u00dft", "bei\u00b7den", "von", "den", "Pfei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIAT", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Zu t\u00f6ten und zu heilen \u2013", "tokens": ["Zu", "t\u00f6\u00b7ten", "und", "zu", "hei\u00b7len", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Denn er mu\u00df stets an ihrem Pfeil gesunden,", "tokens": ["Denn", "er", "mu\u00df", "stets", "an", "ih\u00b7rem", "Pfeil", "ge\u00b7sun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "ADV", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Und sterbend lebt sie nur in seinen Wunden.", "tokens": ["Und", "ster\u00b7bend", "lebt", "sie", "nur", "in", "sei\u00b7nen", "Wun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Doch bald wird nun die Ruhe niederschweben,", "tokens": ["Doch", "bald", "wird", "nun", "die", "Ru\u00b7he", "nie\u00b7der\u00b7schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df alle Schmerzen fliehen,", "tokens": ["Da\u00df", "al\u00b7le", "Schmer\u00b7zen", "flie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Den hei\u00dfen Kampf die stillen Schatten k\u00fchlen,", "tokens": ["Den", "hei\u00b7\u00dfen", "Kampf", "die", "stil\u00b7len", "Schat\u00b7ten", "k\u00fch\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dann mag der Sehnsucht ungel\u00f6stes Leben", "tokens": ["Dann", "mag", "der", "Sehn\u00b7sucht", "un\u00b7ge\u00b7l\u00f6s\u00b7tes", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "In heil'gen Phantasien,", "tokens": ["In", "heil'\u00b7gen", "Phan\u00b7ta\u00b7si\u00b7en", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "In sch\u00f6nen Tr\u00e4umen dichtend sich erw\u00fchlen.", "tokens": ["In", "sch\u00f6\u00b7nen", "Tr\u00e4u\u00b7men", "dich\u00b7tend", "sich", "er\u00b7w\u00fch\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "K\u00f6nnt ihr solch Leben f\u00fchlen?", "tokens": ["K\u00f6nnt", "ihr", "solch", "Le\u00b7ben", "f\u00fch\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "So will, mit seinem Rausch euch zu erf\u00fcllen,", "tokens": ["So", "will", ",", "mit", "sei\u00b7nem", "Rausch", "euch", "zu", "er\u00b7f\u00fcl\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "$,", "APPR", "PPOSAT", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Mein Bild ich gern enth\u00fcllen,", "tokens": ["Mei