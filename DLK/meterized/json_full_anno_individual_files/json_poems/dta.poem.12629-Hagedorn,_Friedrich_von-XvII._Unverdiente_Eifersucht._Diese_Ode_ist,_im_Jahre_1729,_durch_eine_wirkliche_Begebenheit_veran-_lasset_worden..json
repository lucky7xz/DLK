{"dta.poem.12629": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "XvII.  \n  Unverdiente Eifersucht. Diese Ode ist, im Jahre 1729, durch eine wirkliche Begebenheit veran-  \n lasset worden.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1744", "urn": "urn:nbn:de:kobv:b4-200905199128", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Neulich sah man aus den Str\u00e4uchen", "tokens": ["Neu\u00b7lich", "sah", "man", "aus", "den", "Str\u00e4u\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Den verschwiegenen Elpin", "tokens": ["Den", "ver\u00b7schwie\u00b7ge\u00b7nen", "El\u00b7pin"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Heimlich von der Weide schleichen,", "tokens": ["Heim\u00b7lich", "von", "der", "Wei\u00b7de", "schlei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Heimlich in die Waldung fliehn.", "tokens": ["Heim\u00b7lich", "in", "die", "Wal\u00b7dung", "fliehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Die Begierde, dort zu sehn,", "tokens": ["Die", "Be\u00b7gier\u00b7de", ",", "dort", "zu", "sehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Warum dieser Gang geschehn,", "tokens": ["Wa\u00b7rum", "die\u00b7ser", "Gang", "ge\u00b7schehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Trieb Myrtillen nachzugehn.", "tokens": ["Trieb", "Myr\u00b7til\u00b7len", "nach\u00b7zu\u00b7gehn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "VVIZU", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ach! Elpin ist zu beneiden:", "tokens": ["Ach", "!", "El\u00b7pin", "ist", "zu", "be\u00b7nei\u00b7den", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "NE", "VAFIN", "PTKZU", "VVINF", "$."], "meter": "-+-++-+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Fiel dem schlauen Sch\u00e4fer ein;", "tokens": ["Fiel", "dem", "schlau\u00b7en", "Sch\u00e4\u00b7fer", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ja! ihr folgt ihm, s\u00fcsse Freuden!", "tokens": ["Ja", "!", "ihr", "folgt", "ihm", ",", "s\u00fcs\u00b7se", "Freu\u00b7den", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKANT", "$.", "PPER", "VVFIN", "PPER", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In den lustgewohnten Hayn,", "tokens": ["In", "den", "lust\u00b7ge\u00b7wohn\u00b7ten", "Hayn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Wo in jener Schatten Nacht", "tokens": ["Wo", "in", "je\u00b7ner", "Schat\u00b7ten", "Nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "PDAT", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Jhm vielleicht die Hirtinn lacht,", "tokens": ["Jhm", "viel\u00b7leicht", "die", "Hir\u00b7tinn", "lacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Die mein Herze sehnend macht.", "tokens": ["Die", "mein", "Her\u00b7ze", "seh\u00b7nend", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "VVFIN", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Mitten unter hohen Fichten", "tokens": ["Mit\u00b7ten", "un\u00b7ter", "ho\u00b7hen", "Fich\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Traf Myrtill den Fl\u00fcchtling an,", "tokens": ["Traf", "Myr\u00b7till", "den", "Fl\u00fccht\u00b7ling", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Der bereits in stillem Dichten", "tokens": ["Der", "be\u00b7reits", "in", "stil\u00b7lem", "Dich\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Voller Liebe sa\u00df und sann,", "tokens": ["Vol\u00b7ler", "Lie\u00b7be", "sa\u00df", "und", "sann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Bis sein fertiger Gesang", "tokens": ["Bis", "sein", "fer\u00b7ti\u00b7ger", "Ge\u00b7sang"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Muthig durch die L\u00fcfte drang", "tokens": ["Mut\u00b7hig", "durch", "die", "L\u00fcf\u00b7te", "drang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und den Hall zum Nachruf zwang.", "tokens": ["Und", "den", "Hall", "zum", "Nach\u00b7ruf", "zwang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Muster, sang er, wahrer G\u00fcte!", "tokens": ["Mus\u00b7ter", ",", "sang", "er", ",", "wah\u00b7rer", "G\u00fc\u00b7te", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Herz, das Treu und Huld belebt!", "tokens": ["Herz", ",", "das", "Treu", "und", "Huld", "be\u00b7lebt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "KON", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "G\u00f6nne mir, da\u00df mein Gem\u00fcthe", "tokens": ["G\u00f6n\u00b7ne", "mir", ",", "da\u00df", "mein", "Ge\u00b7m\u00fc\u00b7the"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PPER", "$,", "KOUS", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Einsam deinen Wehrt erhebt.", "tokens": ["Ein\u00b7sam", "dei\u00b7nen", "Wehrt", "er\u00b7hebt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sag ich Neidern und der Welt", "tokens": ["Sag", "ich", "Nei\u00b7dern", "und", "der", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "NN", "KON", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Minder als dein Lob erh\u00e4lt;", "tokens": ["Min\u00b7der", "als", "dein", "Lob", "er\u00b7h\u00e4lt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "So vernehm es Wald und Feld.", "tokens": ["So", "ver\u00b7nehm", "es", "Wald", "und", "Feld", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Mit wie z\u00e4rtlichem Umfangen", "tokens": ["Mit", "wie", "z\u00e4rt\u00b7li\u00b7chem", "Um\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "KOKOM", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hat dein Arm mich oft ergetzt!", "tokens": ["Hat", "dein", "Arm", "mich", "oft", "er\u00b7getzt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und wie oft hat deine Wangen", "tokens": ["Und", "wie", "oft", "hat", "dei\u00b7ne", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ADV", "VAFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mein vergn\u00fcgter Mund genetzt!", "tokens": ["Mein", "ver\u00b7gn\u00fcg\u00b7ter", "Mund", "ge\u00b7netzt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Selten hab ich was begehrt,", "tokens": ["Sel\u00b7ten", "hab", "ich", "was", "be\u00b7gehrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIS", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Das, so bald ich mich erkl\u00e4rt,", "tokens": ["Das", ",", "so", "bald", "ich", "mich", "er\u00b7kl\u00e4rt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "ADV", "ADV", "PPER", "PRF", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Du mir nicht mit Lust gew\u00e4hrt.", "tokens": ["Du", "mir", "nicht", "mit", "Lust", "ge\u00b7w\u00e4hrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPER", "PTKNEG", "APPR", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "O mit welchen treuen K\u00fcssen", "tokens": ["O", "mit", "wel\u00b7chen", "treu\u00b7en", "K\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "PWAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dr\u00fccktest du mich an dein Herz!", "tokens": ["Dr\u00fcck\u00b7test", "du", "mich", "an", "dein", "Herz", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Auch in eignen K\u00fcmmernissen", "tokens": ["Auch", "in", "eig\u00b7nen", "K\u00fcm\u00b7mer\u00b7nis\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Scherztest du bey meinem Scherz.", "tokens": ["Scherz\u00b7test", "du", "bey", "mei\u00b7nem", "Scherz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Nur dein L\u00e4cheln und dein Ku\u00df,", "tokens": ["Nur", "dein", "L\u00e4\u00b7cheln", "und", "dein", "Ku\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Die ich stets verehren mu\u00df,", "tokens": ["Die", "ich", "stets", "ver\u00b7eh\u00b7ren", "mu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVFIN", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Stillten allen Ueberdru\u00df.", "tokens": ["Still\u00b7ten", "al\u00b7len", "Ue\u00b7berd\u00b7ru\u00df", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Deine kluge Huld erblicken,", "tokens": ["Dei\u00b7ne", "klu\u00b7ge", "Huld", "er\u00b7bli\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Deiner Liebe Regung sehn,", "tokens": ["Dei\u00b7ner", "Lie\u00b7be", "Re\u00b7gung", "sehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Das allein darf mich entz\u00fccken,", "tokens": ["Das", "al\u00b7lein", "darf", "mich", "ent\u00b7z\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Das allein bleibt wundersch\u00f6n:", "tokens": ["Das", "al\u00b7lein", "bleibt", "wun\u00b7der\u00b7sch\u00f6n", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sch\u00f6n in deiner Seltenheit,", "tokens": ["Sch\u00f6n", "in", "dei\u00b7ner", "Sel\u00b7ten\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Sch\u00f6n in meiner Dankbarkeit,", "tokens": ["Sch\u00f6n", "in", "mei\u00b7ner", "Dank\u00b7bar\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Sch\u00f6n auf unsre Lebens-Zeit.", "tokens": ["Sch\u00f6n", "auf", "uns\u00b7re", "Le\u00b7bens\u00b7Zeit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Wahrheit, Zeuginn meiner Triebe!", "tokens": ["Wahr\u00b7heit", ",", "Zeu\u00b7ginn", "mei\u00b7ner", "Trie\u00b7be", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Leiste selber die Gew\u00e4hr.", "tokens": ["Leis\u00b7te", "sel\u00b7ber", "die", "Ge\u00b7w\u00e4hr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sage: F\u00fcr so grosse Liebe", "tokens": ["Sa\u00b7ge", ":", "F\u00fcr", "so", "gros\u00b7se", "Lie\u00b7be"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "APPR", "ADV", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "F\u00e4llt die Gegen-Pflicht nicht schwer.", "tokens": ["F\u00e4llt", "die", "Ge\u00b7gen\u00b7Pflicht", "nicht", "schwer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKNEG", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sag ihr st\u00fcndlich, da\u00df ihr Bild,", "tokens": ["Sag", "ihr", "st\u00fcnd\u00b7lich", ",", "da\u00df", "ihr", "Bild", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJD", "$,", "KOUS", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Das mein ganzes Herz erf\u00fcllt,", "tokens": ["Das", "mein", "gan\u00b7zes", "Herz", "er\u00b7f\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Mehr bey mir, als alles, gilt.", "tokens": ["Mehr", "bey", "mir", ",", "als", "al\u00b7les", ",", "gilt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PIS", "APPR", "PPER", "$,", "KOUS", "PIS", "$,", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Eil ich, wann es Tag will werden,", "tokens": ["Eil", "ich", ",", "wann", "es", "Tag", "will", "wer\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "PWAV", "PPER", "NN", "VMFIN", "VAINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In die herdenvolle Flur;", "tokens": ["In", "die", "her\u00b7den\u00b7vol\u00b7le", "Flur", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "O so zeigen mir die Herden", "tokens": ["O", "so", "zei\u00b7gen", "mir", "die", "Her\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gleiche Wirkung der Natur:", "tokens": ["Glei\u00b7che", "Wir\u00b7kung", "der", "Na\u00b7tur", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.5": {"text": "Was auch ich von ihr erhielt,", "tokens": ["Was", "auch", "ich", "von", "ihr", "er\u00b7hielt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PPER", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Was die Zucht der L\u00e4mmer f\u00fchlt,", "tokens": ["Was", "die", "Zucht", "der", "L\u00e4m\u00b7mer", "f\u00fchlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wann sie mit den Schafen spielt.", "tokens": ["Wann", "sie", "mit", "den", "Scha\u00b7fen", "spielt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Nein! ich will mich nicht entfernen,", "tokens": ["Nein", "!", "ich", "will", "mich", "nicht", "ent\u00b7fer\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Weil mein Abschied sie betr\u00fcbt;", "tokens": ["Weil", "mein", "Ab\u00b7schied", "sie", "be\u00b7tr\u00fcbt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nein! ich will von ihr erlernen,", "tokens": ["Nein", "!", "ich", "will", "von", "ihr", "er\u00b7ler\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "PPER", "VMFIN", "APPR", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie man unaussprechlich liebt.", "tokens": ["Wie", "man", "un\u00b7aus\u00b7sprech\u00b7lich", "liebt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ja ich will dir, k\u00fchler Hayn!", "tokens": ["Ja", "ich", "will", "dir", ",", "k\u00fch\u00b7ler", "Hayn", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKANT", "PPER", "VMFIN", "PPER", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Hiermit ihren Namen weihn,", "tokens": ["Hier\u00b7mit", "ih\u00b7ren", "Na\u00b7men", "weihn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Dieser Fichte Schmuck zu seyn.", "tokens": ["Die\u00b7ser", "Fich\u00b7te", "Schmuck", "zu", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "NN", "PTKZU", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Name! wachse mit den Rinden!", "tokens": ["Na\u00b7me", "!", "wach\u00b7se", "mit", "den", "Rin\u00b7den", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wachse, Denkmal meiner Hand!", "tokens": ["Wach\u00b7se", ",", "Denk\u0