 ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "Auf unsern Sternen schweben", "tokens": ["Auf", "un\u00b7sern", "Ster\u00b7nen", "schwe\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.16": {"text": "Uns\u00e4glich tiefe Leiden,", "tokens": ["Un\u00b7s\u00e4g\u00b7lich", "tie\u00b7fe", "Lei\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.17": {"text": "Doch sind wir still bescheiden,", "tokens": ["Doch", "sind", "wir", "still", "be\u00b7schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.18": {"text": "O la\u00df uns dir zur Seiten", "tokens": ["O", "la\u00df", "uns", "dir", "zur", "Sei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "PPER", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.19": {"text": "Ganz linde und gelassen", "tokens": ["Ganz", "lin\u00b7de", "und", "ge\u00b7las\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJA", "KON", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.20": {"text": "Verbl\u00fchen und verblassen,", "tokens": ["Ver\u00b7bl\u00fc\u00b7hen", "und", "ver\u00b7blas\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.21": {"text": "O Jesus! ohne Hassen!", "tokens": ["O", "Je\u00b7sus", "!", "oh\u00b7ne", "Has\u00b7sen", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$.", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.22": {"text": "O Jesus! ohne H\u00f6hnen!", "tokens": ["O", "Je\u00b7sus", "!", "oh\u00b7ne", "H\u00f6h\u00b7nen", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$.", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.23": {"text": "O Jesus! dich versch\u00f6nen,", "tokens": ["O", "Je\u00b7sus", "!", "dich", "ver\u00b7sch\u00f6\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$.", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.24": {"text": "In dem wir uns vers\u00f6hnen!", "tokens": ["In", "dem", "wir", "uns", "ver\u00b7s\u00f6h\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.25": {"text": "Der uns hat hergesendet,", "tokens": ["Der", "uns", "hat", "her\u00b7ge\u00b7sen\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.26": {"text": "Der hat ja bald vollendet,", "tokens": ["Der", "hat", "ja", "bald", "voll\u00b7en\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.27": {"text": "Doch wir sind nicht verschwendet,", "tokens": ["Doch", "wir", "sind", "nicht", "ver\u00b7schwen\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.28": {"text": "Wir stehen auf dem Grabe", "tokens": ["Wir", "ste\u00b7hen", "auf", "dem", "Gra\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.29": {"text": "Gleich einer Tr\u00e4nengabe,", "tokens": ["Gleich", "ei\u00b7ner", "Tr\u00e4\u00b7nen\u00b7ga\u00b7be", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.30": {"text": "Gleich einem schwachen Stabe", "tokens": ["Gleich", "ei\u00b7nem", "schwa\u00b7chen", "Sta\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.31": {"text": "Des armen Tr\u00e4nenblinden,", "tokens": ["Des", "ar\u00b7men", "Tr\u00e4\u00b7nen\u00b7blin\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.32": {"text": "Sein' Ruheort zu finden,", "tokens": ["Sein'", "Ru\u00b7he\u00b7ort", "zu", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.33": {"text": "Den letzten Kranz zu winden,", "tokens": ["Den", "letz\u00b7ten", "Kranz", "zu", "win\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.34": {"text": "Zu F\u00fc\u00dfen einer Linden", "tokens": ["Zu", "F\u00fc\u00b7\u00dfen", "ei\u00b7ner", "Lin\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "ART", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.35": {"text": "Dem, der bald \u00fcberwunden", "tokens": ["Dem", ",", "der", "bald", "\u00fc\u00b7berw\u00b7un\u00b7den"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PDS", "$,", "PRELS", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.36": {"text": "Verblutend unverbunden", "tokens": ["Ver\u00b7blu\u00b7tend", 