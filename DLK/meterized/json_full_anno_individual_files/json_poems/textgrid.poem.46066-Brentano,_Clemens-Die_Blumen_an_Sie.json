{"textgrid.poem.46066": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Die Blumen an Sie", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als Sonnenfeuer spr\u00fchte", "tokens": ["Als", "Son\u00b7nen\u00b7feu\u00b7er", "spr\u00fch\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und hei\u00df der Sommer gl\u00fchte", "tokens": ["Und", "hei\u00df", "der", "Som\u00b7mer", "gl\u00fch\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und s\u00fc\u00df die Linde bl\u00fchte", "tokens": ["Und", "s\u00fc\u00df", "die", "Lin\u00b7de", "bl\u00fch\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "ART", "NE", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und lieb die Turtel girrte,", "tokens": ["Und", "lieb", "die", "Tur\u00b7tel", "girr\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und licht der Gl\u00fchwurm schwirrte", "tokens": ["Und", "licht", "der", "Gl\u00fch\u00b7wurm", "schwirr\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Sprach sterbend zu der Mirte", "tokens": ["Sprach", "ster\u00b7bend", "zu", "der", "Mir\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Das letzte Licht der Lilie:", "tokens"