{"dta.poem.8249": {"metadata": {"author": {"name": "Storm, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "In hoc signo vinces.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1852", "urn": "urn:nbn:de:kobv:b4-200905196098", "language": ["de:0.99"], "booktitle": "Storm, Theodor: Gedichte. Kiel, 1852."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Morgen nur, ein Gestern gab es nicht;", "tokens": ["Ein", "Mor\u00b7gen", "nur", ",", "ein", "Ge\u00b7stern", "gab", "es", "nicht", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "ART", "NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da sah der Tod im hellsten Sonnenglanze,", "tokens": ["Da", "sah", "der", "Tod", "im", "hells\u00b7ten", "Son\u00b7nen\u00b7glan\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mein Haar ber\u00fchrend, mir in's Angesicht.", "tokens": ["Mein", "Haar", "be\u00b7r\u00fch\u00b7rend", ",", "mir", "in's", "An\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$,", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die Welt erlosch, der Himmel brannte tr\u00fcbe;", "tokens": ["Die", "Welt", "er\u00b7losch", ",", "der", "Him\u00b7mel", "brann\u00b7te", "tr\u00fc\u00b7be", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,", "ART", "NN", "VVFIN", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich sprang empor entsetzt und ungest\u00fcm.", "tokens": ["Ich", "sprang", "em\u00b7por", "ent\u00b7setzt", "und", "un\u00b7ge\u00b7st\u00fcm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "VVPP", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch er verschwand. Die Ewigkeit der Liebe", "tokens": ["Doch", "er", "ver\u00b7schwand", ".", "Die", "E\u00b7wig\u00b7keit", "der", "Lie\u00b7be"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "$.", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Lag vor mir noch, und trennte mich von ihm.", "tokens": ["Lag", "vor", "mir", "noch", ",", "und", "trenn\u00b7te", "mich", "von", "ihm", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "ADV", "$,", "KON", "VVFIN", "PRF", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und heute nun \u2014 im sonnigen Gemache", "tokens": ["Und", "heu\u00b7te", "nun", "im", "son\u00b7ni\u00b7gen", "Ge\u00b7ma\u00b7che"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "$(", "APPRART", "ADJA", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Zur Rechten und zur Linken schlief mein Kind;", "tokens": ["Zur", "Rech\u00b7ten", "und", "zur", "Lin\u00b7ken", "schlief", "mein", "Kind", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "APPRART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Des zarten Athems lauschend hielt ich Wache,", "tokens": ["Des", "zar\u00b7ten", "A\u00b7thems", "lau\u00b7schend", "hielt", "ich", "Wa\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und an den Fenstern ging der Sommerwind.", "tokens": ["Und", "an", "den", "Fens\u00b7tern", "ging", "der", "Som\u00b7mer\u00b7wind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Da sanken Nebelschleier dicht und dichter", "tokens": ["Da", "san\u00b7ken", "Ne\u00b7bel\u00b7schlei\u00b7er", "dicht", "und", "dich\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NN", "ADJD", "KON", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auf mich herab; kaum schienen noch hervor", "tokens": ["Auf", "mich", "her\u00b7ab", ";", "kaum", "schie\u00b7nen", "noch", "her\u00b7vor"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "ADV", "$.", "ADV", "VVFIN", "ADV", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Kinder schlummerselige Gesichter,", "tokens": ["Der", "Kin\u00b7der", "schlum\u00b7mer\u00b7se\u00b7li\u00b7ge", "Ge\u00b7sich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und nicht mehr drang ihr Athem an mein Ohr.", "tokens": ["Und", "nicht", "mehr", "drang", "ihr", "A\u00b7them", "an", "mein", "Ohr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ADV", "VVFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Ich wollte rufen; doch die Stimme keuchte,", "tokens": ["Ich", "woll\u00b7te", "ru\u00b7fen", ";", "doch", "die", "Stim\u00b7me", "keuch\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "$.", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bis hell die Angst aus meinem Herzen schrie.", "tokens": ["Bis", "hell", "die", "Angst", "aus", "mei\u00b7nem", "Her\u00b7zen", "schrie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Vergebens doch; kein Schrei der Angst erreichte,", "tokens": ["Ver\u00b7ge\u00b7bens", "doch", ";", "kein", "Schrei", "der", "Angst", "er\u00b7reich\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$.", "PIAT", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Kein Laut der Liebe mehr erreichte sie.", "tokens": ["Kein", "Laut", "der", "Lie\u00b7be", "mehr", "er\u00b7reich\u00b7te", "sie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "APPR", "ART", "NN", "ADV", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "In grauer Finsterni\u00df stand ich verlassen,", "tokens": ["In", "grau\u00b7er", "Fins\u00b7ter\u00b7ni\u00df", "stand", "ich", "ver\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bewegungslos und schauernden Gebeins;", "tokens": ["Be\u00b7we\u00b7gungs\u00b7los", "und", "schau\u00b7ern\u00b7den", "Ge\u00b7beins", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich f\u00fchlte kalt mein schlagend Herz erfassen,", "tokens": ["Ich", "f\u00fchl\u00b7te", "kalt", "mein", "schla\u00b7gend", "Herz", "er\u00b7fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PPOSAT", "ADJD", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und ein entsetzlich Auge sank in meins.", "tokens": ["Und", "ein", "ent\u00b7setz\u00b7lich", "Au\u00b7ge", "sank", "in", "meins", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJD", "NN", "VVFIN", "APPR", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Ich floh nicht mehr; ich fesselte das Grauen,", "tokens": ["Ich", "floh", "nicht", "mehr", ";", "ich", "fes\u00b7sel\u00b7te", "das", "Grau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "$.", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und fa\u00dfte m\u00fchsam meines Auges Kraft;", "tokens": ["Und", "fa\u00df\u00b7te", "m\u00fch\u00b7sam", "mei\u00b7nes", "Au\u00b7ges", "Kraft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dann \u00fcberkam vorahnend mich Vertrauen", "tokens": ["Dann", "\u00fc\u00b7ber\u00b7kam", "vor\u00b7ah\u00b7nend", "mich", "Ver\u00b7trau\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "VVPP", "PPER", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zu dem, der meine Sinne hielt in Haft.", "tokens": ["Zu", "dem", ",", "der", "mei\u00b7ne", "Sin\u00b7ne", "hielt", "in", "Haft", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Und als ich fest den Blick zur\u00fcckgegeben,", "tokens": ["Und", "als", "ich", "fest", "den", "Blick", "zu\u00b7r\u00fcck\u00b7ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Lag pl\u00f6tzlich tief zu F\u00fc\u00dfen mir die Welt;", "tokens": ["Lag", "pl\u00f6tz\u00b7lich", "tief", "zu", "F\u00fc\u00b7\u00dfen", "mir", "die", "Welt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ADJD", "APPR", "NN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich sah mich hoch und frei ob allem Leben", "tokens": ["Ich", "sah", "mich", "hoch", "und", "frei", "ob", "al\u00b7lem", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "KOUS", "PIS", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "An deiner Hand, furchtbarer F\u00fcrst, gestellt.", "tokens": ["An", "dei\u00b7ner", "Hand", ",", "furcht\u00b7ba\u00b7rer", "F\u00fcrst", ",", "ge\u00b7stellt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "ADJA", "NN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Den Dampf der Erde sah empor ich streben,", "tokens": ["Den", "Dampf", "der", "Er\u00b7de", "sah", "em\u00b7por", "ich", "stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "PTKVZ", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und ballen sich zu Mensch- und Thiergestalt;", "tokens": ["Und", "bal\u00b7len", "sich", "zu", "Men\u00b7sch", "und", "Thier\u00b7ge\u00b7stalt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "TRUNC", "KON", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Sah es sich sch\u00fctteln, tasten, sah es leben,", "tokens": ["Sah", "es", "sich", "sch\u00fct\u00b7teln", ",", "tas\u00b7ten", ",", "sah", "es", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "VVINF", "$,", "VVFIN", "$,", "VVFIN", "PPER", "VVINF", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Und taumeln dann, und schwinden alsobald.", "tokens": ["Und", "tau\u00b7meln", "dann", ",", "und", "schwin\u00b7den", "al\u00b7so\u00b7bald", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "$,", "KON", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Im fahlen Schein im Abgrund sah ich's liegen,", "tokens": ["Im", "fah\u00b7len", "Schein", "im", "Ab\u00b7grund", "sah", "ich's", "lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "APPRART", "NN", "VVFIN", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und sah sich's regen in der St\u00e4dte Rauch;", "tokens": ["Und", "sah", "sich's", "re\u00b7gen", "in", "der", "St\u00e4d\u00b7te", "Rauch", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "ADJA", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich sah es wimmeln, hasten, sich bekriegen,", "tokens": ["Ich", "sah", "es", "wim\u00b7meln", ",", "has\u00b7ten", ",", "sich", "be\u00b7krie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVINF", "$,", "VVFIN", "$,", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und sah mich selbst bei den Gestalten auch.", "tokens": ["Und", "sah", "mich", "selbst", "bei", "den", "Ge\u00b7stal\u00b7ten", "auch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "ADV", "$."], "meter": "-+-+---+-+", "measure": "zehnsilber"}}, "stanza.11": {"line.1": {"text": "Und niederschauend von des Todes Warte", "tokens": ["Und", "nie\u00b7der\u00b7schau\u00b7end", "von", "des", "To\u00b7des", "War\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVPP", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Kam mir der Drang, das Leben zu bestehn,", "tokens": ["Kam", "mir", "der", "Drang", ",", "das", "Le\u00b7ben", "zu", "be\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ART", "NN", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Lust, dem Feind, der unten meiner harrte,", "tokens": ["Die", "Lust", ",", "dem", "Feind", ",", "der", "un\u00b7ten", "mei\u00b7ner", "harr\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "PRELS", "ADV", "PPOSAT", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mit vollem Aug' in's Angesicht zu sehn.", "tokens": ["Mit", "vol\u00b7lem", "Aug'", "in's", "An\u00b7ge\u00b7sicht", "zu", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.12": {"line.1": {"text": "Und k\u00fchlen Hauches durch die Adern rinnen", "tokens": ["Und", "k\u00fch\u00b7len", "Hau\u00b7ches", "durch", "die", "A\u00b7dern", "rin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "F\u00fchlt' ich die Kraft, entgegen Lust und Schmerz", "tokens": ["F\u00fchlt'", "ich", "die", "Kraft", ",", "ent\u00b7ge\u00b7gen", "Lust", "und", "Schmerz"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Vom Leben fest mich selber zu gewinnen,", "tokens": ["Vom", "Le\u00b7ben", "fest", "mich", "sel\u00b7ber", "zu", "ge\u00b7win\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wenn Andres nicht, so doch ein ganzes Herz. \u2014", "tokens": ["Wenn", "And\u00b7res", "nicht", ",", "so", "doch", "ein", "gan\u00b7zes", "Herz", "."], "token_