{"dta.poem.17766": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "11  Der Seelige weise.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Wie Seelig ist der Mensch/ der alle seine zeit", "tokens": ["Wie", "See\u00b7lig", "ist", "der", "Mensch", "/", "der", "al\u00b7le", "sei\u00b7ne", "zeit"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VAFIN", "ART", "NN", "$(", "ART", "PIAT", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit anders nichts verbringt/ als mit der Ewigkeit!", "tokens": ["Mit", "an\u00b7ders", "nichts", "ver\u00b7bringt", "/", "als", "mit", "der", "E\u00b7wig\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "PIS", "VVFIN", "$(", "KOKOM", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der jung und alt allein betrachtet und beschaut", "tokens": ["Der", "jung", "und", "alt", "al\u00b7lein", "be\u00b7trach\u00b7tet", "und", "be\u00b7schaut"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "KON", "ADJD", "ADV", "VVPP", "KON", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Wei\u00dfheit Schlo\u00df/ das GOtt sein Vater hat gebaut.", "tokens": ["Der", "Wei\u00df\u00b7heit", "Schlo\u00df", "/", "das", "Gott", "sein", "Va\u00b7ter", "hat", "ge\u00b7baut", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$(", "ART", "NN", "PPOSAT", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der sich auf seinen Stab/ das ewge Wort/ aufst\u00fctzt/", "tokens": ["Der", "sich", "auf", "sei\u00b7nen", "Stab", "/", "das", "ew\u00b7ge", "Wort", "/", "auf\u00b7st\u00fctzt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PRF", "APPR", "PPOSAT", "NN", "$(", "ART", "ADJA", "NN", "$(", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und nicht/ wie mancher Thor/ im frembden sande sitzt.", "tokens": ["Und", "nicht", "/", "wie", "man\u00b7cher", "Thor", "/", "im", "fremb\u00b7den", "san\u00b7de", "sitzt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "$(", "KOKOM", "PIAT", "NN", "$(", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der nicht nach Hau\u00df und Hoff/ nach Gold und Silber", "tokens": ["Der", "nicht", "nach", "Hau\u00df", "und", "Hoff", "/", "nach", "Gold", "und", "Sil\u00b7ber"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "PTKNEG", "APPR", "NN", "KON", "VVFIN", "$(", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "sieht/", "tokens": ["sieht", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+", "measure": "single.up"}, "line.9": {"text": "Noch seines Lebens zeit zu zehlen sich bem\u00fcht.", "tokens": ["Noch", "sei\u00b7nes", "Le\u00b7bens", "zeit", "zu", "zeh\u00b7len", "sich", "be\u00b7m\u00fcht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "NN", "PTKZU", "VVFIN", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Jhn wird das blinde Gl\u00fck nicht hin und her vexirn/", "tokens": ["Jhn", "wird", "das", "blin\u00b7de", "Gl\u00fck", "nicht", "hin", "und", "her", "ve\u00b7xirn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "PTKNEG", "PTKVZ", "KON", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Noch etwann eitler Durst zu frembden Wassern f\u00fchrn.", "tokens": ["Noch", "et\u00b7wann", "eit\u00b7ler", "Durst", "zu", "fremb\u00b7den", "Was\u00b7sern", "f\u00fchrn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJA", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Er wei\u00df von keinem Zang/ er liebt nicht kr\u00e4merey/", "tokens": ["Er", "wei\u00df", "von", "kei\u00b7nem", "Zang", "/", "er", "liebt", "nicht", "kr\u00e4\u00b7me\u00b7rey", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN", "$(", "PPER", "VVFIN", "PTKNEG", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Er trachtet nicht darnach/ da\u00df er gesehen sey/", "tokens": ["Er", "trach\u00b7tet", "nicht", "dar\u00b7nach", "/", "da\u00df", "er", "ge\u00b7se\u00b7hen", "sey", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "PAV", "$(", "KOUS", "PPER", "VVPP", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Er ist der Welt ein kind/ die allern\u00e4chste stadt", "tokens": ["Er", "ist", "der", "Welt", "ein", "kind", "/", "die", "al\u00b7ler\u00b7n\u00e4chs\u00b7te", "stadt"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "NN", "$(", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Ist ihm so viel bekand/ als die der ", "tokens": ["Ist", "ihm", "so", "viel", "be\u00b7kand", "/", "als", "die", "der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "VVFIN", "$(", "KOUS", "ART", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Er schaut nur \u00fcber sich/ so frey er immer kan/", "tokens": ["Er", "schaut", "nur", "\u00fc\u00b7ber", "sich", "/", "so", "frey", "er", "im\u00b7mer", "kan", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PRF", "$(", "ADV", "ADJD", "PPER", "ADV", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Sein rechtes Vaterland/ den lieben Himmel an.", "tokens": ["Sein", "rech\u00b7tes", "Va\u00b7ter\u00b7land", "/", "den", "lie\u00b7ben", "Him\u00b7mel", "an", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$(", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Sein alter rechnet er nicht nach der Jahre zahl/", "tokens": ["Sein", "al\u00b7ter", "rech\u00b7net", "er", "nicht", "nach", "der", "Jah\u00b7re", "zahl", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "VVFIN", "PPER", "PTKNEG", "APPR", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}