{"dta.poem.396": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Er bittet um einen Ku\u00df.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Sch\u00f6ne Bella la\u00df den Mund deiner Br\u00fcste Perlen k\u00fcssen/", "tokens": ["Sch\u00f6\u00b7ne", "Be\u00b7lla", "la\u00df", "den", "Mund", "dei\u00b7ner", "Br\u00fcs\u00b7te", "Per\u00b7len", "k\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "FM", "FM", "ART", "NN", "PPOSAT", "ADJA", "NN", "VVINF", "$("], "meter": "+--+--+--+-+-+-", "measure": "elegiambus"}, "line.2": {"text": "Wo die s\u00fcsse Wollust schl\u00e4ffet/ und die reine Keuschheit", "tokens": ["Wo", "die", "s\u00fcs\u00b7se", "Wol\u00b7lust", "schl\u00e4f\u00b7fet", "/", "und", "die", "rei\u00b7ne", "Keuschheit"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVFIN", "$(", "KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.3": {"text": "wacht/", "tokens": ["wacht", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+", "measure": "single.up"}, "line.4": {"text": "La\u00df aus dieser Purpur-Frucht/ ", "tokens": ["La\u00df", "aus", "die\u00b7ser", "Pur\u00b7pur\u00b7Frucht", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "APPR", "PDAT", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Die Corallen und Rubinen in der Sch\u00f6nheit schaam-roth", "tokens": ["Die", "Co\u00b7ral\u00b7len", "und", "Ru\u00b7bi\u00b7nen", "in", "der", "Sch\u00f6n\u00b7heit", "schaam\u00b7roth"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN", "APPR", "ART", "NN", "ADJD"], "meter": "--+-+-+-+-+--+", "measure": "iambic.hexa.chol"}, "line.6": {"text": "macht/", "tokens": ["macht", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+", "measure": "single.up"}, "line.7": {"text": "Ein entflam\u0303ter Liebes-Trieb treibt mich zu den Lust-Narcissen.", "tokens": ["Ein", "ent\u00b7flam\u0303ter", "Lie\u00b7bes\u00b7Trieb", "treibt", "mich", "zu", "den", "Lust\u00b7\u00b7N\u00b7a\u00b7rcis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PRF", "APPR", "ART", "NN", "$."], "meter": "+--+-+-++-+--+-", "measure": "iambic.septa.invert"}, "line.8": {"text": "Zarte Rosen Knospen bl\u00fchen bey der Liljen Silber-Pracht/", "tokens": ["Zar\u00b7te", "Ro\u00b7sen", "Knos\u00b7pen", "bl\u00fc\u00b7hen", "bey", "der", "Lil\u00b7jen", "Sil\u00b7ber\u00b7Pracht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "VVFIN", "APPR", "ART", "NN", "NN", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.9": {"text": "Wollust-Aepffel kan man da in beliebter Lust geniessen", "tokens": ["Wol\u00b7lust\u00b7Ae\u00b7pf\u00b7fel", "kan", "man", "da", "in", "be\u00b7lieb\u00b7ter", "Lust", "ge\u00b7nies\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VMFIN", "PIS", "ADV", "APPR", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.10": {"text": "Und das angenehme Wesen mit vergn\u00fcgter Anmuth lacht.", "tokens": ["Und", "das", "an\u00b7ge\u00b7neh\u00b7me", "We\u00b7sen", "mit", "ver\u00b7gn\u00fcg\u00b7ter", "An\u00b7muth", "lacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}}}}