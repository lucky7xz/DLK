{"dta.poem.2655": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Trauer-Ode  \n Uber fr\u00fchzeitiges Verbl\u00fchn J. L. S. v. S.  \n den 27. Decembr. 1673.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Soll JEsus in dem Stalle liegen/", "tokens": ["Soll", "Je\u00b7sus", "in", "dem", "Stal\u00b7le", "lie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NE", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Und findt nicht in der Herberg Raum?", "tokens": ["Und", "findt", "nicht", "in", "der", "Her\u00b7berg", "Raum", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So ruffst du ihn in deine Wiegen", "tokens": ["So", "ruffst", "du", "ihn", "in", "dei\u00b7ne", "Wie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Louys' und k\u00fcst ihn in dem Traum?", "tokens": ["Louys'", "und", "k\u00fcst", "ihn", "in", "dem", "Traum", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Doch eylst du solchen Traum zu schliessen/", "tokens": ["Doch", "eylst", "du", "sol\u00b7chen", "Traum", "zu", "schlies\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PIAT", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Hier in der blassen Sterbligkeit/", "tokens": ["Hier", "in", "der", "blas\u00b7sen", "Ster\u00b7blig\u00b7keit", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Umb da\u00df du ihn dort m\u00f6gest k\u00fcssen/", "tokens": ["Umb", "da\u00df", "du", "ihn", "dort", "m\u00f6\u00b7gest", "k\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "KOUS", "PPER", "PPER", "ADV", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und seyn in deinem Schatz erfreut.", "tokens": ["Und", "seyn", "in", "dei\u00b7nem", "Schatz", "er\u00b7freut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Es ehre unsers Heylands Krippen", "tokens": ["Es", "eh\u00b7re", "un\u00b7sers", "Hey\u00b7lands", "Krip\u00b7pen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Entbrandter Hertzen Andachts-Schall/", "tokens": ["Ent\u00b7brand\u00b7ter", "Hert\u00b7zen", "An\u00b7dachts\u00b7Schall", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NE", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du lispelst noch mit zarten Lippen", "tokens": ["Du", "lis\u00b7pelst", "noch", "mit", "zar\u00b7ten", "Lip\u00b7pen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "(ach! eine s\u00fcsse Nachtigall.)", "tokens": ["(", "ach", "!", "ei\u00b7ne", "s\u00fcs\u00b7se", "Nach\u00b7ti\u00b7gall", ".", ")"], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ITJ", "$.", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Jetzt unter tausend Cherubinen;", "tokens": ["Jetzt", "un\u00b7ter", "tau\u00b7send", "Che\u00b7ru\u00b7bi\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "CARD", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ein Kind hat uns das Heyl gebracht:", "tokens": ["Ein", "Kind", "hat", "uns", "das", "Heyl", "ge\u00b7bracht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und stimmest bey den Seraphinen:", "tokens": ["Und", "stim\u00b7mest", "bey", "den", "Se\u00b7ra\u00b7phi\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Di\u00df Kind hat uns befreyt gemacht.", "tokens": ["Di\u00df", "Kind", "hat", "uns", "be\u00b7freyt", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "VAFIN", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Man la\u00df\u2019 aus der Cumaner Gr\u00fcfften/", "tokens": ["Man", "la\u00df'", "aus", "der", "Cu\u00b7ma\u00b7ner", "Gr\u00fcff\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sibyllen haben propheceyt/", "tokens": ["Si\u00b7byl\u00b7len", "ha\u00b7ben", "pro\u00b7phe\u00b7ceyt", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "NE", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie GOtt ein B\u00fcndn\u00fc\u00df w\u00fcrde stifften/", "tokens": ["Wie", "Gott", "ein", "B\u00fcnd\u00b7n\u00fc\u00df", "w\u00fcr\u00b7de", "stiff\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ART", "NN", "VAFIN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Verm\u00e4hlen Zeit und Ewigkeit.", "tokens": ["Ver\u00b7m\u00e4h\u00b7len", "Zeit", "und", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du keine Heydnische Sibylle/", "tokens": ["Du", "kei\u00b7ne", "Heyd\u00b7ni\u00b7sche", "Si\u00b7byl\u00b7le", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Hast JEsum dir zum Trost erkiest/", "tokens": ["Hast", "Je\u00b7sum", "dir", "zum", "Trost", "er\u00b7kiest", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "PPER", "APPRART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und wei\u00df da\u00df aller Wei\u00dfheit F\u00fclle/", "tokens": ["Und", "wei\u00df", "da\u00df", "al\u00b7ler", "Wei\u00df\u00b7heit", "F\u00fcl\u00b7le", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KOUS", "PIAT", "NN", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und aller G\u00fctter Schatz er ist.", "tokens": ["Und", "al\u00b7ler", "G\u00fct\u00b7ter", "Schatz", "er", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "NN", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Sein Arm der hat dich auch gef\u00fchret", "tokens": ["Sein", "Arm", "der", "hat", "dich", "auch", "ge\u00b7f\u00fch\u00b7ret"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ART", "VAFIN", "PPER", "ADV", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In so ein ruhig Schlaff-Gemach/", "tokens": ["In", "so", "ein", "ru\u00b7hig", "Schlaff\u00b7Ge\u00b7mach", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ART", "ADJD", "NN", "$("], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Da dich kein Schmertzen mehr ber\u00fchret/", "tokens": ["Da", "dich", "kein", "Schmert\u00b7zen", "mehr", "be\u00b7r\u00fch\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "NN", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da dir kein Elend folget nach.", "tokens": ["Da", "dir", "kein", "E\u00b7lend", "fol\u00b7get", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie friedsam lisst du deine Wiegen/", "tokens": ["Wie", "fried\u00b7sam", "lisst", "du", "dei\u00b7ne", "Wie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "PPER", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die Windeln/ als wie Fesseln/ stehn/", "tokens": ["Die", "Win\u00b7deln", "/", "als", "wie", "Fes\u00b7seln", "/", "stehn", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$(", "KOUS", "KOKOM", "NN", "$(", "VVINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Nun du an JEsus Brust solst liegen/", "tokens": ["Nun", "du", "an", "Je\u00b7sus", "Brust", "solst", "lie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "APPR", "NE", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und mit ihm ein in Himmel gehn.", "tokens": ["Und", "mit", "ihm", "ein", "in", "Him\u00b7mel", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "ART", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Dein\u2019 Augen werden nicht ersterben/", "tokens": ["Dein'", "Au\u00b7gen", "wer\u00b7den", "nicht", "ers\u00b7ter\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PTKNEG", "VVINF", "$("], "meter": "-+-+--+--", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Bricht sie schon hier des Todes Hand.", "tokens": ["Bricht", "sie", "schon", "hier", "des", "To\u00b7des", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er wil mit seinem Blute f\u00e4rben", "tokens": ["Er", "wil", "mit", "sei\u00b7nem", "Blu\u00b7te", "f\u00e4r\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Wangen jetzt verblasten Rand.", "tokens": ["Der", "Wan\u00b7gen", "jetzt", "ver\u00b7blas\u00b7ten", "Rand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Er wil den Todes-Schwei\u00df weg wischen/", "tokens": ["Er", "wil", "den", "To\u00b7des\u00b7Schwei\u00df", "weg", "wi\u00b7schen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Dich L\u00e4mchen nehmen in die Scho\u00df/", "tokens": ["Dich", "L\u00e4m\u00b7chen", "neh\u00b7men", "in", "die", "Scho\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "VVINF", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und dich mit jenem Kelch erfrischen/", "tokens": ["Und", "dich", "mit", "je\u00b7nem", "Kelch", "er\u00b7fri\u00b7schen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "PDAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Den er zu deinem Heyl vergo\u00df.", "tokens": ["Den", "er", "zu", "dei\u00b7nem", "Heyl", "ver\u00b7go\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Mu\u00df schon das Fleisch wie Heu verschwinden/", "tokens": ["Mu\u00df", "schon", "das", "Fleisch", "wie", "Heu", "ver\u00b7schwin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "KOKOM", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Schlieff doch dein Heiland auff dem Heu/", "tokens": ["Schlieff", "doch", "dein", "Hei\u00b7land", "auff", "dem", "Heu", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPOSAT", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als er sich erst bey uns lie\u00df finden;", "tokens": ["Als", "er", "sich", "erst", "bey", "uns", "lie\u00df", "fin\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "APPR", "PPER", "VVFIN", "VVINF", "$."], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und alles Fleisch ist Heu und Spreu.", "tokens": ["Und", "al\u00b7les", "Fleisch", "ist", "Heu", "und", "Spreu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie eine Blume auff der Wiese", "tokens": ["Wie", "ei\u00b7ne", "Blu\u00b7me", "auff", "der", "Wie\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Gar bald des M\u00e4ders Sen\u00df\u2019 abmeyht/", "tokens": ["Gar", "bald", "des", "M\u00e4\u00b7ders", "Sen\u00df'", "ab\u00b7meyht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "NE", "VVFIN", "$("], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.7": {"text": "So auch Holdseeligste Louyse", "tokens": ["So", "auch", "Hold\u00b7see\u00b7ligs\u00b7te", "Lou\u00b7yse"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADJA", "NN"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.8": {"text": "Verf\u00e4llst du in der ersten Zeit.", "tokens": ["Ver\u00b7f\u00e4llst", "du", "in", "der", "ers\u00b7ten", "Zeit", "."], "token_info": ["word", "word", "word", "wo