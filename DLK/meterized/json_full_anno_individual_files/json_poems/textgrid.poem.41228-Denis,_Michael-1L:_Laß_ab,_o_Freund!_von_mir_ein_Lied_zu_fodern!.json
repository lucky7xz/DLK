{"textgrid.poem.41228": {"metadata": {"author": {"name": "Denis, Michael", "birth": "N.A.", "death": "N.A."}, "title": "1L: La\u00df ab, o Freund! von mir ein Lied zu fodern!", "genre": "verse", "period": "N.A.", "pub_year": 1764, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "La\u00df ab, o Freund! von mir ein Lied zu fodern!", "tokens": ["La\u00df", "ab", ",", "o", "Freund", "!", "von", "mir", "ein", "Lied", "zu", "fo\u00b7dern", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$,", "FM", "NN", "$.", "APPR", "PPER", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So werden Lieder nicht.", "tokens": ["So", "wer\u00b7den", "Lie\u00b7der", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ha, gieb mir Regen, wenn des Himmels Antlitz", "tokens": ["Ha", ",", "gieb", "mir", "Re\u00b7gen", ",", "wenn", "des", "Him\u00b7mels", "Ant\u00b7litz"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "VVIMP", "PPER", "NN", "$,", "KOUS", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Kein einzig W\u00f6lkchen tr\u00fcbt!", "tokens": ["Kein", "ein\u00b7zig", "W\u00f6lk\u00b7chen", "tr\u00fcbt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Dort \u00fcberm Rheine nimmt ein fremder S\u00e4nger", "tokens": ["Dort", "\u00fc\u00b7berm", "Rhei\u00b7ne", "nimmt", "ein", "frem\u00b7der", "S\u00e4n\u00b7ger"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NE", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dir jeden Inhalt auf,", "tokens": ["Dir", "je\u00b7den", "In\u00b7halt", "auf", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und w\u00e4lzt ihn tagelang vor seinem Geiste,", "tokens": ["Und", "w\u00e4lzt", "ihn", "ta\u00b7ge\u00b7lang", "vor", "sei\u00b7nem", "Geis\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und flammt sich k\u00fcnstlich an;", "tokens": ["Und", "flammt", "sich", "k\u00fcnst\u00b7lich", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und wacht in N\u00e4chten, l\u00e4uft in Morgenstunden", "tokens": ["Und", "wacht", "in", "N\u00e4ch\u00b7ten", ",", "l\u00e4uft", "in", "Mor\u00b7gen\u00b7stun\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NN", "$,", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Fluren auf und ab;", "tokens": ["Die", "Flu\u00b7ren", "auf", "und", "ab", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Entwirft, ver\u00e4ndert, schaltet ein, und zeichnet,", "tokens": ["Ent\u00b7wirft", ",", "ver\u00b7\u00e4n\u00b7dert", ",", "schal\u00b7tet", "ein", ",", "und", "zeich\u00b7net", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVPP", "$,", "VVFIN", "PTKVZ", "$,", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und tilgt es wieder aus,", "tokens": ["Und", "tilgt", "es", "wie\u00b7der", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Bis endlich ihm nach manchen Tageschichten", "tokens": ["Bis", "end\u00b7lich", "ihm", "nach", "man\u00b7chen", "Ta\u00b7ge\u00b7schich\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "PPER", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sein Werk vollendet scheint.", "tokens": ["Sein", "Werk", "voll\u00b7en\u00b7det", "scheint", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Freund, nimm es hin! Was hat er dir gegeben?", "tokens": ["Freund", ",", "nimm", "es", "hin", "!", "Was", "hat", "er", "dir", "ge\u00b7ge\u00b7ben", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVIMP", "PPER", "PTKVZ", "$.", "PWS", "VAFIN", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein feines, kaltes Lied.", "tokens": ["Ein", "fei\u00b7nes", ",", "kal\u00b7tes", "Lied", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Nicht so der Barde! Niemal sucht er Lieder.", "tokens": ["Nicht", "so", "der", "Bar\u00b7de", "!", "Nie\u00b7mal", "sucht", "er", "Lie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ART", "NN", "$.", "ADV", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Lieder suchen ihn.", "tokens": ["Die", "Lie\u00b7der", "su\u00b7chen", "ihn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Schie\u00dft Feuerstralen ihm ein Stoff entgegen,", "tokens": ["Schie\u00dft", "Feu\u00b7er\u00b7stra\u00b7len", "ihm", "ein", "Stoff", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dann flammt das Herz ihm auf;", "tokens": ["Dann", "flammt", "das", "Herz", "ihm", "auf", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Dann pocht es ihm, und dr\u00e4nget allgewaltig,", "tokens": ["Dann", "pocht", "es", "ihm", ",", "und", "dr\u00e4n\u00b7get", "all\u00b7ge\u00b7wal\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "$,", "KON", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und l\u00e4\u00dft ihn nimmer ruhn.", "tokens": ["Und", "l\u00e4\u00dft", "ihn", "nim\u00b7mer", "ruhn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er gleichet einem \u00fcbereilten Hirschen,", "tokens": ["Er", "glei\u00b7chet", "ei\u00b7nem", "\u00fc\u00b7be\u00b7reil\u00b7ten", "Hir\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In dem der Jagdpfeil steckt.", "tokens": ["In", "dem", "der", "Jagd\u00b7pfeil", "steckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Nicht bis herab vom k\u00e4ltlichen Verstande", "tokens": ["Nicht", "bis", "her\u00b7ab", "vom", "k\u00e4lt\u00b7li\u00b7chen", "Ver\u00b7stan\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Schl\u00fcsse Kette schleppt,", "tokens": ["Der", "Schl\u00fcs\u00b7se", "Ket\u00b7te", "schleppt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nein! bis herauf vom Bardenliedersitze,", "tokens": ["Nein", "!", "bis", "her\u00b7auf", "vom", "Bar\u00b7den\u00b7lie\u00b7der\u00b7sit\u00b7ze", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "ADV", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Vom Herzen, Sang erbraust.", "tokens": ["Vom", "Her\u00b7zen", ",", "Sang", "er\u00b7braust", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Und wollten Lippen diesem Sange wehren,", "tokens": ["Und", "woll\u00b7ten", "Lip\u00b7pen", "die\u00b7sem", "San\u00b7ge", "weh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "NN", "PDAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "O Freund! sie k\u00f6nnten nicht.", "tokens": ["O", "Freund", "!", "sie", "k\u00f6nn\u00b7ten", "nicht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "PPER", "VMFIN", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Winde rissen ihn, vielleicht verst\u00fcmmelt,", "tokens": ["Die", "Win\u00b7de", "ris\u00b7sen", "ihn", ",", "viel\u00b7leicht", "ver\u00b7st\u00fcm\u00b7melt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Doch weg ihn rissen sie!", "tokens": ["Doch", "weg", "ihn", "ris\u00b7sen", "sie", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPER", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Trifft aber ihm von einem Gegenstande", "tokens": ["Trifft", "a\u00b7ber", "ihm", "von", "ei\u00b7nem", "Ge\u00b7gen\u00b7stan\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Kein Feuerstral das Herz,", "tokens": ["Kein", "Feu\u00b7er\u00b7stral", "das", "Herz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und schien' er andern Augen noch so wichtig,", "tokens": ["Und", "schien'", "er", "an\u00b7dern", "Au\u00b7gen", "noch", "so", "wich\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJA", "NN", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und noch so liederwerth;", "tokens": ["Und", "noch", "so", "lie\u00b7der\u00b7werth", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Vergib, o Freund! Es lodert nichts hierinnen,", "tokens": ["Ver\u00b7gib", ",", "o", "Freund", "!", "Es", "lo\u00b7dert", "nichts", "hie\u00b7rin\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "FM", "NN", "$.", "PPER", "VVFIN", "PIS", "PAV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es pocht und dr\u00e4nget nichts.", "tokens": ["Es", "pocht", "und", "dr\u00e4n\u00b7get", "nichts", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PIS", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie g\u00e4b' es Regen, wenn des Himmels Antlitz", "tokens": ["Wie", "g\u00e4b'", "es", "Re\u00b7gen", ",", "wenn", "des", "Him\u00b7mels", "Ant\u00b7litz"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "NN", "$,", "KOUS", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Kein einzig W\u00f6lkchen tr\u00fcbt?", "tokens": ["Kein", "ein\u00b7zig", "W\u00f6lk\u00b7chen", "tr\u00fcbt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Du sagest! Forsche, pr\u00fcfe, sinn', erfinde! \u2013", "tokens": ["Du", "sa\u00b7gest", "!", "For\u00b7sche", ",", "pr\u00fc\u00b7fe", ",", "sinn'", ",", "er\u00b7fin\u00b7de", "!", "\u2013"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "$.", "NN", "$,", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Freund, noch einmal! Vergib!", "tokens": ["Freund", ",", "noch", "ein\u00b7mal", "!", "Ver\u00b7gib", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "ADV", "ADV", "$.", "VVIMP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Kann das ein Barde? \u2013 Freundschaft ist mir theuer,", "tokens": ["Kann", "das", "ein", "Bar\u00b7de", "?", "\u2013", "Freund\u00b7schaft", "ist", "mir", "theu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PDS", "ART", "NN", "$.", "$(", "NN", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Doch theuer auch der Ruhm!", "tokens": ["Doch", "theu\u00b7er", "auch", "der", "Ruhm", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "La\u00df ab, o Freund! von mir ein Lied zu fodern!", "tokens": ["La\u00df", "ab", ",", "o", "Freund", "!", "von", "mir", "ein", "Lied", "zu", "fo\u00b7dern", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$,", "FM", "NN", "$.", "APPR", "PPER", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So werden Lieder nicht.", "tokens": ["So", "wer\u00b7den", "Lie\u00b7der", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ha, gieb mir Regen, wenn des Himmels Antlitz", "tokens": ["Ha", ",", "gieb", "mir", "Re\u00b7gen", ",", "wenn", "des", "Him\u00b7mels", "Ant\u00b7litz"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "VVIMP", "PPER", "NN", "$,", "KOUS", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Kein einzig W\u00f6lkchen tr\u00fcbt!", "tokens": ["Kein", "ein\u00b7zig", "W\u00f6lk\u00b7chen", "tr\u00fcbt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Dort \u00fcberm Rheine nimmt ein fremder S\u00e4nger", "tokens": ["Dort", "\u00fc\u00b7berm", "Rhei\u00b7ne", "nimmt", "ein", "frem\u00b7der", "S\u00e4n\u00b7ger"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NE", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dir jeden Inhalt auf,", "tokens": ["Dir", "je\u00b7den", "In\u00b7halt", "auf", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und w\u00e4lzt ihn tagelang vor seinem Geiste,", "tokens": ["Und", "w\u00e4lzt", "ihn", "ta\u00b7ge\u00b7lang", "vor", "sei\u00b7nem", "Geis\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und flammt sich k\u00fcnstlich an;", "tokens": ["Und", "flammt", "sich", "k\u00fcnst\u00b7lich", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": 