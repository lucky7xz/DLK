, "line.3": {"text": "Freund, nimm es hin! Was hat er dir gegeben?", "tokens": ["Freund", ",", "nimm", "es", "hin", "!", "Was", "hat", "er", "dir", "ge\u00b7ge\u00b7ben", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVIMP", "PPER", "PTKVZ", "$.", "PWS", "VAFIN", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein feines, kaltes Lied.", "tokens": ["Ein", "fei\u00b7nes", ",", "kal\u00b7tes", "Lied", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.16": {"line.1": {"text": "Nicht so der Barde! Niemal sucht er Lieder.", "tokens": ["Nicht", "so", "der", "Bar\u00b7de", "!", "Nie\u00b7mal", "sucht", "er", "Lie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ART", "NN", "$.", "ADV", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Lieder suchen ihn.", "tokens": ["Die", "Lie\u00b7der", "su\u00b7chen", "ihn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Schie\u00dft Feuerstralen ihm ein Stoff entgegen,", "tokens": ["Schie\u00dft", "Feu\u00b7er\u00b7stra\u00b7len", "ihm", "ein", "Stoff", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dann flammt das Herz ihm auf;", "tokens": ["Dann", "flammt", "das", "Herz", "ihm", "auf", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.17": {"line.1": {"text": "Dann pocht es ihm, und dr\u00e4nget allgewaltig,", "tokens": ["Dann", "pocht", "es", "ihm", ",", "und", "dr\u00e4n\u00b7get", "all\u00b7ge\u00b7wal\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "$,", "KON", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und l\u00e4\u00dft ihn nimmer ruhn.", "tokens": ["Und", "l\u00e4\u00dft", "ihn", "nim\u00b7mer", "ruhn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er gleichet einem \u00fcbereilten Hirschen,", "tokens": ["Er", "glei\u00b7chet", "ei\u00b7nem", "\u00fc\u00b7be\u00b7reil\u00b7ten", "Hir\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In dem der Jagdpfeil steckt.", "tokens": ["In", "dem", "der", "Jagd\u00b7pfeil", "steckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.18": {"line.1": {"text": "Nicht bis herab vom k\u00e4ltlichen Verstande", "tokens": ["Nicht", "bis", "her\u00b7ab", "vom", "k\u00e4lt\u00b7li\u00b7chen", "Ver\u00b7stan\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Schl\u00fcsse Kette schleppt,", "tokens": ["Der", "Schl\u00fcs\u00b7se", "Ket\u00b7te", "schleppt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$,"], "meter"