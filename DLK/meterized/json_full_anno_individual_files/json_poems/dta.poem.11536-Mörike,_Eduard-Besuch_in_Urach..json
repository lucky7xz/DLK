{"dta.poem.11536": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Besuch in Urach.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.99"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df ich in dies geliebte Thal verirrt;", "tokens": ["Da\u00df", "ich", "in", "dies", "ge\u00b7lieb\u00b7te", "Thal", "ver\u00b7irrt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PDS", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Kein Wunder ist, was meine Augen sehen,", "tokens": ["Kein", "Wun\u00b7der", "ist", ",", "was", "mei\u00b7ne", "Au\u00b7gen", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "$,", "PRELS", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch schwankt der Boden, Luft und Staude schwirrt,", "tokens": ["Doch", "schwankt", "der", "Bo\u00b7den", ",", "Luft", "und", "Stau\u00b7de", "schwirrt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Aus tausend gr\u00fcnen Spiegeln scheint zu gehen", "tokens": ["Aus", "tau\u00b7send", "gr\u00fc\u00b7nen", "Spie\u00b7geln", "scheint", "zu", "ge\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "CARD", "ADJA", "NN", "VVFIN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Vergang'ne Zeit, die l\u00e4chelnd mich verwirrt,", "tokens": ["Ver\u00b7gang'\u00b7ne", "Zeit", ",", "die", "l\u00e4\u00b7chelnd", "mich", "ver\u00b7wirrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PRELS", "ADJD", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Die Wahrheit selber wird hier zum Gedichte,", "tokens": ["Die", "Wahr\u00b7heit", "sel\u00b7ber", "wird", "hier", "zum", "Ge\u00b7dich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Mein eigen Bild ein fremd und hold Gesichte!", "tokens": ["Mein", "ei\u00b7gen", "Bild", "ein", "fremd", "und", "hold", "Ge\u00b7sich\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ART", "ADJD", "KON", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Da seyd ihr alle wieder aufgerichtet,", "tokens": ["Da", "seyd", "ihr", "al\u00b7le", "wie\u00b7der", "auf\u00b7ge\u00b7rich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIS", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Besonnte Felsen, alte Wolkenst\u00fchle!", "tokens": ["Be\u00b7sonn\u00b7te", "Fel\u00b7sen", ",", "al\u00b7te", "Wol\u00b7ken\u00b7st\u00fch\u00b7le", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Auf W\u00e4ldern schwer, wo kaum der Mittag lichtet", "tokens": ["Auf", "W\u00e4l\u00b7dern", "schwer", ",", "wo", "kaum", "der", "Mit\u00b7tag", "lich\u00b7tet"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "ADJD", "$,", "PWAV", "ADV", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und Schatten mischt mit balsamreicher Schw\u00fcle;", "tokens": ["Und", "Schat\u00b7ten", "mischt", "mit", "bal\u00b7sam\u00b7rei\u00b7cher", "Schw\u00fc\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Kennt ihr mich noch, der sonst hieher gefl\u00fcchtet,", "tokens": ["Kennt", "ihr", "mich", "noch", ",", "der", "sonst", "hie\u00b7her", "ge\u00b7fl\u00fcch\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "$,", "PRELS", "ADV", "PAV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Im Moose bei s\u00fc\u00df-schl\u00e4ferndem Gef\u00fchle,", "tokens": ["Im", "Moo\u00b7se", "bei", "s\u00fc\u00df\u00b7schl\u00e4\u00b7fern\u00b7dem", "Ge\u00b7f\u00fch\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Der M\u00fccke Sumsen hier ein Ohr geliehen,", "tokens": ["Der", "M\u00fc\u00b7cke", "Sum\u00b7sen", "hier", "ein", "Ohr", "ge\u00b7lie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Ach, kennt ihr mich, und wollt nicht vor mir fliehen?", "tokens": ["Ach", ",", "kennt", "ihr", "mich", ",", "und", "wollt", "nicht", "vor", "mir", "flie\u00b7hen", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVFIN", "PPER", "PRF", "$,", "KON", "VMFIN", "PTKNEG", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Hier wird ein Strauch, ein jeder Halm zur Schlinge,", "tokens": ["Hier", "wird", "ein", "Strauch", ",", "ein", "je\u00b7der", "Halm", "zur", "Schlin\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "$,", "ART", "PIAT", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die mich in liebliche Betrachtung f\u00e4ngt,", "tokens": ["Die", "mich", "in", "lieb\u00b7li\u00b7che", "Be\u00b7trach\u00b7tung", "f\u00e4ngt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Kein M\u00e4uerchen, kein Holz ist so geringe,", "tokens": ["Kein", "M\u00e4u\u00b7er\u00b7chen", ",", "kein", "Holz", "ist", "so", "ge\u00b7rin\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "VAFIN", "ADV", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df nicht mein Blick voll Wehmuth an ihm h\u00e4ngt;", "tokens": ["Da\u00df", "nicht", "mein", "Blick", "voll", "Weh\u00b7muth", "an", "ihm", "h\u00e4ngt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "PPOSAT", "NN", "ADJD", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ein jedes spricht mir halbverge\u00dfne Dinge,", "tokens": ["Ein", "je\u00b7des", "spricht", "mir", "halb\u00b7ver\u00b7ge\u00df\u00b7ne", "Din\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ich f\u00fchle, wie von Schmerz und Lust gedr\u00e4ngt", "tokens": ["Ich", "f\u00fch\u00b7le", ",", "wie", "von", "Schmerz", "und", "Lust", "ge\u00b7dr\u00e4ngt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "APPR", "NN", "KON", "NN", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Die Thr\u00e4ne stockt, inde\u00df ich ohne Weile,", "tokens": ["Die", "Thr\u00e4\u00b7ne", "stockt", ",", "in\u00b7de\u00df", "ich", "oh\u00b7ne", "Wei\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ADV", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Unschl\u00fcssig, satt und durstig, weiter eile.", "tokens": ["Un\u00b7schl\u00fcs\u00b7sig", ",", "satt", "und", "durs\u00b7tig", ",", "wei\u00b7ter", "ei\u00b7le", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "KON", "ADJD", "$,", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}