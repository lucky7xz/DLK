{"dta.poem.23369": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die sieben Sachen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie hei\u00dfen doch die sieben Sachen,", "tokens": ["Wie", "hei\u00b7\u00dfen", "doch", "die", "sie\u00b7ben", "Sa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ADV", "ART", "CARD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die einen Mann von Stande machen?", "tokens": ["Die", "ei\u00b7nen", "Mann", "von", "Stan\u00b7de", "ma\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Nichts lernen fr\u00fch von Kindesbeinen", "tokens": ["Nichts", "ler\u00b7nen", "fr\u00fch", "von", "Kin\u00b7des\u00b7bei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VVINF", "ADJD", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und Alles doch zu wissen meinen,", "tokens": ["Und", "Al\u00b7les", "doch", "zu", "wis\u00b7sen", "mei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "ADV", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die ganze Nacht beim Spiel durchwachen,", "tokens": ["Die", "gan\u00b7ze", "Nacht", "beim", "Spiel", "durch\u00b7wa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Den ganzen Tag brav Schulden machen,", "tokens": ["Den", "gan\u00b7zen", "Tag", "brav", "Schul\u00b7den", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Das Deutsch so schlecht als m\u00f6glich sprechen,", "tokens": ["Das", "Deutsch", "so", "schlecht", "als", "m\u00f6g\u00b7lich", "spre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "KOKOM", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Franz\u00f6sisch trefflich radebrechen,", "tokens": ["Fran\u00b7z\u00f6\u00b7sisch", "treff\u00b7lich", "ra\u00b7de\u00b7bre\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Champagner trinken obendrein", "tokens": ["Cham\u00b7pag\u00b7ner", "trin\u00b7ken", "o\u00b7ben\u00b7drein"], "token_info": ["word", "word", "word"], "pos": ["NE", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Und \u00fcberall hoff\u00e4hig sein.", "tokens": ["Und", "\u00fc\u00b7be\u00b7rall", "hof\u00b7f\u00e4\u00b7hig", "sein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Das sind, das sind die sieben Sachen,", "tokens": ["Das", "sind", ",", "das", "sind", "die", "sie\u00b7ben", "Sa\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "$,", "PDS", "VAFIN", "ART", "CARD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Die einen Mann von Stande machen.", "tokens": ["Die", "ei\u00b7nen", "Mann", "von", "Stan\u00b7de", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie hei\u00dfen doch die sieben Sachen,", "tokens": ["Wie", "hei\u00b7\u00dfen", "doch", "die", "sie\u00b7ben", "Sa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ADV", "ART", "CARD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die keinen Mann von Stande machen?", "tokens": ["Die", "kei\u00b7nen", "Mann", "von", "Stan\u00b7de", "ma\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Nicht sich allein auf Erden leben,", "tokens": ["Nicht", "sich", "al\u00b7lein", "auf", "Er\u00b7den", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PRF", "ADV", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fcr Andre still zu wirken streben,", "tokens": ["F\u00fcr", "And\u00b7re", "still", "zu", "wir\u00b7ken", "stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "ADJD", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Sich nie um Schulden mahnen lassen", "tokens": ["Sich", "nie", "um", "Schul\u00b7den", "mah\u00b7nen", "las\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PRF", "ADV", "APPR", "NN", "VVINF", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und nie auf Andrer Kosten prassen,", "tokens": ["Und", "nie", "auf", "A\u00b7ndrer", "Kos\u00b7ten", "pras\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Der Knechtschaft Sprache radebrechen,", "tokens": ["Der", "Knecht\u00b7schaft", "Spra\u00b7che", "ra\u00b7de\u00b7bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Gut Deutsch f\u00fcr Recht und Freiheit sprechen,", "tokens": ["Gut", "Deutsch", "f\u00fcr", "Recht", "und", "Frei\u00b7heit", "spre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "APPR", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Und lieber leiden Noth und Pein", "tokens": ["Und", "lie\u00b7ber", "lei\u00b7den", "Noth", "und", "Pein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Als irgendwo hoff\u00e4hig sein.", "tokens": ["Als", "ir\u00b7gend\u00b7wo", "hof\u00b7f\u00e4\u00b7hig", "sein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Das sind, das sind die sieben Sachen,", "tokens": ["Das", "sind", ",", "das", "sind", "die", "sie\u00b7ben", "Sa\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "$,", "PDS", "VAFIN", "ART", "CARD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Die keinen Mann von Stande machen.", "tokens": ["Die", "kei\u00b7nen", "Mann", "von", "Stan\u00b7de", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}