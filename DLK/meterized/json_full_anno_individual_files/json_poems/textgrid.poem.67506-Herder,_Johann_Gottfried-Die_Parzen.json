{"textgrid.poem.67506": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Die Parzen", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Furchtbar waren mir sonst die Schwestern des ehernen Schicksals,", "tokens": ["Furcht\u00b7bar", "wa\u00b7ren", "mir", "sonst", "die", "Schwes\u00b7tern", "des", "e\u00b7her\u00b7nen", "Schick\u00b7sals", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "ADV", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+-+--+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Graue T\u00f6chter der Nacht, fremde dem Menschengef\u00fchl;", "tokens": ["Grau\u00b7e", "T\u00f6ch\u00b7ter", "der", "Nacht", ",", "frem\u00b7de", "dem", "Men\u00b7schen\u00b7ge\u00b7f\u00fchl", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "$,", "ADJA", "ART", "NN", "$."], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Jetzt verehr' ich die Hohen, die Mildegesinneten. Klotho,", "tokens": ["Jetzt", "ver\u00b7ehr'", "ich", "die", "Ho\u00b7hen", ",", "die", "Mil\u00b7de\u00b7ge\u00b7sin\u00b7ne\u00b7ten", ".", "Klo\u00b7tho", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,", "ART", "NN", "$.", "NE", "$,"], "meter": "--+--+--+--+---+", "measure": "anapaest.tetra.plus"}, "line.4": {"text": "Jugendlich unbesorgt, munter und r\u00fcstig am Werk,", "tokens": ["Ju\u00b7gend\u00b7lich", "un\u00b7be\u00b7sorgt", ",", "mun\u00b7ter", "und", "r\u00fcs\u00b7tig", "am", "Werk", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADJD", "KON", "ADJD", "APPRART", "NN", "$,"], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.5": {"text": "Zieht vom vollesten Rocken den bunten Faden; es weitet", "tokens": ["Zieht", "vom", "vol\u00b7les\u00b7ten", "Ro\u00b7cken", "den", "bun\u00b7ten", "Fa\u00b7den", ";", "es", "wei\u00b7tet"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "APPRART", "ADJA", "NN", "ART", "ADJA", "NN", "$.", "PPER", "VVFIN"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.6": {"text": "Lachesis ihn; sie hebt schwebend und leicht ihn empor.", "tokens": ["La\u00b7che\u00b7sis", "ihn", ";", "sie", "hebt", "schwe\u00b7bend", "und", "leicht", "ihn", "em\u00b7por", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "$.", "PPER", "VVFIN", "ADJD", "KON", "ADJD", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.7": {"text": "Atropos schneidet \u2013 doch nein! mit weggewendetem Antlitz", "tokens": ["A\u00b7tro\u00b7pos", "schnei\u00b7det", "\u2013", "doch", "nein", "!", "mit", "weg\u00b7ge\u00b7wen\u00b7de\u00b7tem", "Ant\u00b7litz"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VVFIN", "$(", "ADV", "PTKANT", "$.", "APPR", "ADJA", "NN"], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.8": {"text": "S\u00e4umt sie, zu schneiden; die Hand f\u00fchlet den kommenden Schmerz.", "tokens": ["S\u00e4umt", "sie", ",", "zu", "schnei\u00b7den", ";", "die", "Hand", "f\u00fch\u00b7let", "den", "kom\u00b7men\u00b7den", "Schmerz", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PTKZU", "VVINF", "$.", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+---+--+", "