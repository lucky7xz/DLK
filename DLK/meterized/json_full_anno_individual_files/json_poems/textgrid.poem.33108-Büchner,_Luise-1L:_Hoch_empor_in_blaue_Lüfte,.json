: ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Die euch heut' die Fahne weih'n,", "tokens": ["Die", "euch", "heut'", "die", "Fah\u00b7ne", "weih'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Schw\u00f6ret Alle sie zu wahren", "tokens": ["Schw\u00f6\u00b7ret", "Al\u00b7le", "sie", "zu", "wah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "PPER", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Unverf\u00e4lschet, treu und rein!", "tokens": ["Un\u00b7ver\u00b7f\u00e4l\u00b7schet", ",", "treu", "und", "rein", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Sich ein k\u00fchler Quell ergie\u00dfet,", "tokens": ["Sich", "ein", "k\u00fch\u00b7ler", "Quell", "er\u00b7gie\u00b7\u00dfet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die Ermatteten zu tr\u00f6sten", "tokens": ["Die", "Er\u00b7mat\u00b7te\u00b7ten", "zu", "tr\u00f6s\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Durch versengte Th\u00e4ler flie\u00dfet,", "tokens": ["Durch", "ver\u00b7seng\u00b7te", "Th\u00e4\u00b7ler", "flie\u00b7\u00dfet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Also frisch zu jedem Werke,", "tokens": ["Al\u00b7so", "frisch", "zu", "je\u00b7dem", "Wer\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sei der Turner, doch vor allem,", "tokens": ["Sei", "der", "Tur\u00b7ner", ",", "doch", "vor", "al\u00b7lem", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$,", "ADV", "APPR", "PIS", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wo die Freiheit flehend naht.", "tokens": ["Wo", "die", "Frei\u00b7heit", "fle\u00b7hend", "naht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVPP", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "Aber ", "tokens": ["A\u00b7ber"], "token_info": ["word"], "pos": ["KON"], "meter": "+-", "measure": "trochaic.single"}, "line.2": {"text": "Nicht ein Beter nur und Heuchler", "tokens": ["Nicht", "ein", "Be\u00b7ter", "nur", "und", "Heuch\u00b7ler"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ART", "NN", "ADV", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "In der Pharis\u00e4er Kreise,", "tokens": ["In", "der", "Pha\u00b7ri\u00b7s\u00e4\u00b7er", "Krei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Das Gesetz der Bruderliebe", "tokens": ["Das", "Ge\u00b7setz", "der", "Bru\u00b7der\u00b7lie\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sei des Turners h\u00f6chste Lehre \u2013", "tokens": ["Sei", "des", "Tur\u00b7ners", "h\u00f6chs\u00b7te", "Leh\u00b7re", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Schaut euch um im deutschen Lande", "tokens": ["Schaut", "euch", "um", "im", "deut\u00b7schen", "Lan\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Von den Alpen bis zum Meere,", "tokens": ["Von", "den", "Al\u00b7pen", "bis", "zum", "Mee\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "APPRART", "NN", "$,"], "meter": "---+--+-", "measure": "iambic.di.relaxed"}}, "stanza.16": {"line.1": {"text": "Korn und Wein auf allen Fluren,", "tokens": ["Korn", "und", "Wein", "auf", "al\u00b7len", "Flu\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Eisen in der Berge Schachten,", "tokens": ["Ei\u00b7sen", "in", "der", "Ber\u00b7ge", "Schach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Stolze W\u00e4lder, reiche Wiesen \u2013", "tokens": ["Stol\u00b7ze", "W\u00e4l\u00b7der", ",", "rei\u00b7che", "Wie\u00b7sen", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und so viele doch, die schmachten!", "tokens": ["Und", "so", "vie\u00b7le", "doch", ",", "die", "schmach\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "PIS", "ADV", "$,", "PRELS", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Da\u00df hinfort die deutsche Erde", "tokens": ["Da\u00df", "hin\u00b7fort", "die", "deut\u00b7sche", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.6": {"text": "Darum fromm zu jedem Opfer,", "tokens": ["Da\u00b7rum", "fromm", "zu", "je\u00b7dem", "Op\u00b7fer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADJD", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Das der Menschheit frommen k\u00f6nnte!", "tokens": ["Das", "der", "Menschheit", "from\u00b7men", "k\u00f6nn\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.17": {"line.1": {"text": "Wo der volle Becher kreiset,", "tokens": ["Wo", "der", "vol\u00b7le", "Be\u00b7cher", "krei\u00b7set", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo sich muntre Paare schwingen", "tokens": ["Wo", "sich", "mun\u00b7tre", "Paa\u00b7re", "schwin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PRF", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Bei Gesang und heitern Scherzen.", "tokens": ["Bei", "Ge\u00b7sang", "und", "hei\u00b7tern", "Scher\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Doch das starke Turnerherz", "tokens": ["Doch", "das", "star\u00b7ke", "Tur\u00b7ner\u00b7herz"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Trage willig, frohen Muthes", "tokens": ["Tra\u00b7ge", "wil\u00b7lig", ",", "fro\u00b7hen", "Mu\u00b7thes"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "ADJD", "$,", "ADJA", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Auch des Lebens herbsten Schmerz.", "tokens": ["Auch", "des", "Le\u00b7bens", "herbs\u00b7ten", "Schmerz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.18": {"line.1": {"text": "Und die Herzen h\u00f6r' ich schlagen,", "tokens": ["Und", "die", "Her\u00b7zen", "h\u00f6r'", "ich", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und die Augen seh' ich flammend", "tokens": ["Und", "die", "Au\u00b7gen", "seh'", "ich", "flam\u00b7mend"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Zu dem Adler aufgeschlagen!", "tokens": ["Zu", "dem", "Ad\u00b7ler", "auf\u00b7ge\u00b7schla\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Jede deutsche Brust durchbebet,", "tokens": ["Je\u00b7de", "deut\u00b7sche", "Brust", "durch\u00b7be\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Aecht und unverf\u00e4lscht vor allem", "tokens": ["A\u00b7ech