{"textgrid.poem.33108": {"metadata": {"author": {"name": "B\u00fcchner, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Hoch empor in blaue L\u00fcfte,", "genre": "verse", "period": "N.A.", "pub_year": 1849, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hoch empor in blaue L\u00fcfte,", "tokens": ["Hoch", "em\u00b7por", "in", "blau\u00b7e", "L\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Turner, la\u00dft die Fahne wehen,", "tokens": ["Tur\u00b7ner", ",", "la\u00dft", "die", "Fah\u00b7ne", "we\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Hebt sie auf, da\u00df weithin Alle", "tokens": ["Hebt", "sie", "auf", ",", "da\u00df", "weit\u00b7hin", "Al\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKVZ", "$,", "KOUS", "ADV", "PIAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihre hohe Deutung sehen:", "tokens": ["Ih\u00b7re", "ho\u00b7he", "Deu\u00b7tung", "se\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie ein Adler drauf erhebet", "tokens": ["Wie", "ein", "Ad\u00b7ler", "drauf", "er\u00b7he\u00b7bet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "PAV", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sich zur Sonne stolz und gro\u00df,", "tokens": ["Sich", "zur", "Son\u00b7ne", "stolz", "und", "gro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPRART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Da\u00df: ", "tokens": ["Da\u00df", ":"], "token_info": ["word", "punct"], "pos": ["KOUS", "$."], "meter": "+", "measure": "single.up"}, "line.8": {"text": "Ist des Turners hohes Loos.", "tokens": ["Ist", "des", "Tur\u00b7ners", "ho\u00b7hes", "Loos", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Was ", "tokens": ["Was"], "token_info": ["word"], "pos": ["PWS"], "meter": "-", "measure": "single.down"}, "line.2": {"text": "Diesen stolzen Sinn zu wahren", "tokens": ["Die\u00b7sen", "stol\u00b7zen", "Sinn", "zu", "wah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDAT", "ADJA", "NN", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Woll't euch, Turner, treu verb\u00fcnden;", "tokens": ["Woll't", "euch", ",", "Tur\u00b7ner", ",", "treu", "ver\u00b7b\u00fcn\u00b7den", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "PPER", "$,", "NN", "$,", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Vor den Augen deutscher Frauen,", "tokens": ["Vor", "den", "Au\u00b7gen", "deut\u00b7scher", "Frau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Die euch heut' die Fahne weih'n,", "tokens": ["Die", "euch", "heut'", "die", "Fah\u00b7ne", "weih'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Schw\u00f6ret Alle sie zu wahren", "tokens": ["Schw\u00f6\u00b7ret", "Al\u00b7le", "sie", "zu", "wah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "PPER", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Unverf\u00e4lschet, treu und rein!", "tokens": ["Un\u00b7ver\u00b7f\u00e4l\u00b7schet", ",", "treu", "und", "rein", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Sich ein k\u00fchler Quell ergie\u00dfet,", "tokens": ["Sich", "ein", "k\u00fch\u00b7ler", "Quell", "er\u00b7gie\u00b7\u00dfet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die Ermatteten zu tr\u00f6sten", "tokens": ["Die", "Er\u00b7mat\u00b7te\u00b7ten", "zu", "tr\u00f6s\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Durch versengte Th\u00e4ler flie\u00dfet,", "tokens": ["Durch", "ver\u00b7seng\u00b7te", "Th\u00e4\u00b7ler", "flie\u00b7\u00dfet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Also frisch zu jedem Werke,", "tokens": ["Al\u00b7so", "frisch", "zu", "je\u00b7dem", "Wer\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sei der Turner, doch vor allem,", "tokens": ["Sei", "der", "Tur\u00b7ner", ",", "doch", "vor", "al\u00b7lem", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$,", "ADV", "APPR", "PIS", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wo die Freiheit flehend naht.", "tokens": ["Wo", "die", "Frei\u00b7heit", "fle\u00b7hend", "naht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVPP", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Aber ", "tokens": ["A\u00b7ber"], "token_info": ["word"], "pos": ["KON"], "meter": "+-", "measure": "trochaic.single"}, "line.2": {"text": "Nicht ein Beter nur und Heuchler", "tokens": ["Nicht", "ein", "Be\u00b7ter", "nur", "und", "Heuch\u00b7ler"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ART", "NN", "ADV", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "In der Pharis\u00e4er Kreise,", "tokens": ["In", "der", "Pha\u00b7ri\u00b7s\u00e4\u00b7er", "Krei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Das Gesetz der Bruderliebe", "tokens": ["Das", "Ge\u00b7setz", "der", "Bru\u00b7der\u00b7lie\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sei des Turners h\u00f6chste Lehre \u2013", "tokens": ["Sei", "des", "Tur\u00b7ners", "h\u00f6chs\u00b7te", "Leh\u00b7re", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Schaut euch um im deutschen Lande", "tokens": ["Schaut", "euch", "um", "im", "deut\u00b7schen", "Lan\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Von den Alpen bis zum Meere,", "tokens": ["Von", "den", "Al\u00b7pen", "bis", "zum", "Mee\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "APPRART", "NN", "$,"], "meter": "---+--+-", "measure": "iambic.di.relaxed"}}, "stanza.5": {"line.1": {"text": "Korn und Wein auf allen Fluren,", "tokens": ["Korn", "und", "Wein", "auf", "al\u00b7len", "Flu\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Eisen in der Berge Schachten,", "tokens": ["Ei\u00b7sen", "in", "der", "Ber\u00b7ge", "Schach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Stolze W\u00e4lder, reiche Wiesen \u2013", "tokens": ["Stol\u00b7ze", "W\u00e4l\u00b7der", ",", "rei\u00b7che", "Wie\u00b7sen", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und so viele doch, die schmachten!", "tokens": ["Und", "so", "vie\u00b7le", "doch", ",", "die", "schmach\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "PIS", "ADV", "$,", "PRELS", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Da\u00df hinfort die deutsche Erde", "tokens": ["Da\u00df", "hin\u00b7fort", "die", "deut\u00b7sche", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.6": {"text": "Darum fromm zu jedem Opfer,", "tokens": ["Da\u00b7rum", "fromm", "zu", "je\u00b7dem", "Op\u00b7fer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADJD", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Das der Menschheit frommen k\u00f6nnte!", "tokens": ["Das", "der", "Menschheit", "from\u00b7men", "k\u00f6nn\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Wo der volle Becher kreiset,", "tokens": ["Wo", "der", "vol\u00b7le", "Be\u00b7cher", "krei\u00b7set", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo sich muntre Paare schwingen", "tokens": ["Wo", "sich", "mun\u00b7tre", "Paa\u00b7re", "schwin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PRF", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Bei Gesang und heitern Scherzen.", "tokens": ["Bei", "Ge\u00b7sang", "und", "hei\u00b7tern", "Scher\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Doch das starke Turnerherz", "tokens": ["Doch", "das", "star\u00b7ke", "Tur\u00b7ner\u00b7herz"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Trage willig, frohen Muthes", "tokens": ["Tra\u00b7ge", "wil\u00b7lig", ",", "fro\u00b7hen", "Mu\u00b7thes"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "ADJD", "$,", "ADJA", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Auch des Lebens herbsten Schmerz.", "tokens": ["Auch", "des", "Le\u00b7bens", "herbs\u00b7ten", "Schmerz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Und die Herzen h\u00f6r' ich schlagen,", "tokens": ["Und", "die", "Her\u00b7zen", "h\u00f6r'", "ich", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und die Augen seh' ich flammend", "tokens": ["Und", "die", "Au\u00b7gen", "seh'", "ich", "flam\u00b7mend"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Zu dem Adler aufgeschlagen!", "tokens": ["Zu", "dem", "Ad\u00b7ler", "auf\u00b7ge\u00b7schla\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Jede deutsche Brust durchbebet,", "tokens": ["Je\u00b7de", "deut\u00b7sche", "Brust", "durch\u00b7be\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Aecht und unverf\u00e4lscht vor allem", "tokens": ["A\u00b7echt", "und", "un\u00b7ver\u00b7f\u00e4lscht", "vor", "al\u00b7lem"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KON", "ADJD", "APPR", "PIS"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "In des Turners Seele lebet:", "tokens": ["In", "des", "Tur\u00b7ners", "See\u00b7le", "le\u00b7bet", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Da\u00df hinfort aus deutscher Erde", "tokens": ["Da\u00df", "hin\u00b7fort", "aus", "deut\u00b7scher", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Eine stolze Eiche steige,", "tokens": ["Ei\u00b7ne", "stol\u00b7ze", "Ei\u00b7che", "stei\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Deren gr\u00fcne Bl\u00e4tterkrone", "tokens": ["De\u00b7ren", "gr\u00fc\u00b7ne", "Bl\u00e4t\u00b7ter\u00b7kro\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["PDS", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ueber alle Lande reiche,", "tokens": ["Ue\u00b7ber", "al\u00b7le", "Lan\u00b7de", "rei\u00b7che", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Deren Stamm sich ungespalten", "tokens": ["De\u00b7ren", "Stamm", "sich", "un\u00b7ge\u00b7spal\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PRF", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Heb' empor, ", "tokens": ["Heb'", "em\u00b7por", ","], "token_info": ["word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Weil die hohe, heil'ge Freiheit", "tokens": ["Weil", "die", "ho\u00b7he", ",", "heil'\u00b7ge", "Frei\u00b7heit"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Nur in Einheit wurzeln kann.", "tokens": ["Nur", "in", "Ein\u00b7heit", "wur\u00b7zeln", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Bis dies hohe Ziel errungen,", "tokens": ["Bis", "dies", "ho\u00b7he", "Ziel", "er\u00b7run\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDS", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sieh, durch dunkle Wetterwolken", "tokens": ["Sieh", ",", "durch", "dunk\u00b7le", "Wet\u00b7ter\u00b7wol\u00b7ken"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Leuchtet hell der Zukunft Schimmer,", "tokens": ["Leuch\u00b7tet", "hell", "der", "Zu\u00b7kunft", "Schim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bis die deutsche Eiche gr\u00fcnet,", "tokens": ["Bis", "die", "deut\u00b7sche", "Ei\u00b7che", "gr\u00fc\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Bis im Staub die Feinde liegen,", "tokens": ["Bis", "im", "Staub", "die", "Fein\u00b7de", "lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "La\u00dft zu jedem rechten Streite", "tokens": ["La\u00dft", "zu", "je\u00b7dem", "rech\u00b7ten", "Strei\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "APPR", "PIAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Euch voran die Fahne fliegen!", "tokens": ["Euch", "vo\u00b7ran", "die", "Fah\u00b7ne", "flie\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Folgt ihr, wo es gilt zu sch\u00fctzen", "tokens": ["Folgt", "ihr", ",", "wo", "es", "gilt", "zu", "sch\u00fct\u00b7zen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "PPER", "VVFIN", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hof und Herd mit frohem Muthe,", "tokens": ["Hof", "und", "Herd", "mit", "fro\u00b7hem", "Mu\u00b7the", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Folgt ihr, wenn die junge Freiheit", "tokens": ["Folgt", "ihr", ",", "wenn", "die", "jun\u00b7ge", "Frei\u00b7heit"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Heischt von eurem bestem Blute:", "tokens": ["Heischt", "von", "eu\u00b7rem", "bes\u00b7tem", "Blu\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Darum weiht euch diese Fahne", "tokens": ["Da\u00b7rum", "weiht", "euch", "die\u00b7se", "Fah\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"tex