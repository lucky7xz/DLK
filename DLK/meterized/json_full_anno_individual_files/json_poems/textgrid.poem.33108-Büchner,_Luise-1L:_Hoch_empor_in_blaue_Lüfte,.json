ct", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "$,", "KOUS", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Eures Spruchs erf\u00fcllet sei:", "tokens": ["Eu\u00b7res", "Spruchs", "er\u00b7f\u00fcl\u00b7let", "sei", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Da\u00df nun jede deutsche Seele", "tokens": ["Da\u00df", "nun", "je\u00b7de", "deut\u00b7sche", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "PIAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Hoch empor in blaue L\u00fcfte,", "tokens": ["Hoch", "em\u00b7por", "in", "blau\u00b7e", "L\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Turner, la\u00dft die Fahne wehen,", "tokens": ["Tur\u00b7ner", ",", "la\u00dft", "die", "Fah\u00b7ne", "we\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Hebt sie auf, da\u00df weithin Alle", "tokens": ["Hebt", "sie", "auf", ",", "da\u00df", "weit\u00b7hin", "Al\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKVZ", "$,", "KOUS", "ADV", "PIAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihre hohe Deutung sehen:", "tokens": ["Ih\u00b7re", "ho\u00b7he", "Deu\u00b7tung", "se\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie ein Ad