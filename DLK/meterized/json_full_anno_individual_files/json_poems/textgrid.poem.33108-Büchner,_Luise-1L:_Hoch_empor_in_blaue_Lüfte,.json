", "an", "ei\u00b7nem", "gr\u00fc\u00b7nen", "Zwei\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Als ein Zeichen, da\u00df die Deutung", "tokens": ["Als", "ein", "Zei\u00b7chen", ",", "da\u00df", "die", "Deu\u00b7tung"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "$,", "KOUS", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Eures Spruchs erf\u00fcllet sei:", "tokens": ["Eu\u00b7res", "Spruchs", "er\u00b7f\u00fcl\u00b7let", "sei", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Da\u00df nun jede deutsche Seele", "tokens": ["Da\u00df", "nun", "je\u00b7de", "deut\u00b7sche", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "PIAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}