{"textgrid.poem.65356": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Fr\u00fchlingseinzug", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Fenster auf, die Herzen auf!", "tokens": ["Die", "Fens\u00b7ter", "auf", ",", "die", "Her\u00b7zen", "auf", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Geschwinde! Geschwinde!", "tokens": ["Ge\u00b7schwin\u00b7de", "!", "Ge\u00b7schwin\u00b7de", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$.", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Der alte Winter will heraus,", "tokens": ["Der", "al\u00b7te", "Win\u00b7ter", "will", "he\u00b7raus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er trippelt \u00e4ngstlich durch das Haus,", "tokens": ["Er", "trip\u00b7pelt", "\u00e4ngst\u00b7lich", "durch", "das", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Er windet bang' sich in der Brust,", "tokens": ["Er", "win\u00b7det", "bang'", "sich", "in", "der", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und kramt zusammen seinen Wust", "tokens": ["Und", "kramt", "zu\u00b7sam\u00b7men", "sei\u00b7nen", "Wust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Geschwinde, geschwinde.", "tokens": ["Ge\u00b7schwin\u00b7de", ",", "ge\u00b7schwin\u00b7de", "."], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "ADJA", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Die Fenster auf, die Herzen auf!", "tokens": ["Die", "Fens\u00b7ter", "auf", ",", "die", "Her\u00b7zen", "auf", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Geschwinde! Geschwinde!", "tokens": ["Ge\u00b7schwin\u00b7de", "!", "Ge\u00b7schwin\u00b7de", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$.", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Er sp\u00fcrt den Fr\u00fchling vor dem Thor,", "tokens": ["Er", "sp\u00fcrt", "den", "Fr\u00fch\u00b7ling", "vor", "dem", "Thor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der will ihn zupfen bei dem Ohr,", "tokens": ["Der", "will", "ihn", "zup\u00b7fen", "bei", "dem", "Ohr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ihn zausen an dem wei\u00dfen Bart", "tokens": ["Ihn", "zau\u00b7sen", "an", "dem", "wei\u00b7\u00dfen", "Bart"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nach solcher wilden Buben Art,", "tokens": ["Nach", "sol\u00b7cher", "wil\u00b7den", "Bu\u00b7ben", "Art", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Geschwinde, geschwinde.", "tokens": ["Ge\u00b7schwin\u00b7de", ",", "ge\u00b7schwin\u00b7de", "."], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "ADJA", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Die Fenster auf, die Herzen auf!", "tokens": ["Die", "Fens\u00b7ter", "auf", ",", "die", "Her\u00b7zen", "auf", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Geschwinde! Geschwinde!", "tokens": ["Ge\u00b7schwin\u00b7de", "!", "Ge\u00b7schwin\u00b7de", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$.", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Der Fr\u00fchling pocht und klopft ja schon \u2013", "tokens": ["Der", "Fr\u00fch\u00b7ling", "pocht", "und", "klopft", "ja", "schon", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "ADV", "ADV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Horcht, horcht, es ist sein lieber Ton!", "tokens": ["Horcht", ",", "horcht", ",", "es", "ist", "sein", "lie\u00b7ber", "Ton", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$,", "PPER", "VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Er pocht und klopfet, was er kann,", "tokens": ["Er", "pocht", "und", "klop\u00b7fet", ",", "was", "er", "kann", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$,", "PWS", "PPER", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit kleinen Blumenknospen an,", "tokens": ["Mit", "klei\u00b7nen", "Blu\u00b7men\u00b7knos\u00b7pen", "an", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Geschwinde, geschwinde.", "tokens": ["Ge\u00b7schwin\u00b7de", ",", "ge\u00b7schwin\u00b7de", "."], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "ADJA", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.4": {"line.1": {"text": "Die Fenster auf, die Herzen auf!", "tokens": ["Die", "Fens\u00b7ter", "auf", ",", "die", "Her\u00b7zen", "auf", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Geschwinde! Geschwinde!", "tokens": ["Ge\u00b7schwin\u00b7de", "!", "Ge\u00b7schwin\u00b7de", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$.", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Und wenn ihr noch nicht \u00f6ffnen wollt,", "tokens": ["Und", "wenn", "ihr", "noch", "nicht", "\u00f6ff\u00b7nen", "wollt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "PTKNEG", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er hat viel Dienerschaft im Sold,", "tokens": ["Er", "hat", "viel", "Die\u00b7ner\u00b7schaft", "im", "Sold", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die ruft er sich zur H\u00fclfe her,", "tokens": ["Die", "ruft", "er", "sich", "zur", "H\u00fcl\u00b7fe", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PRF", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und pocht und klopfet immer mehr,", "tokens": ["Und", "pocht", "und", "klop\u00b7fet", "im\u00b7mer", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Geschwinde, geschwinde.", "tokens": ["Ge\u00b7schwin\u00b7de", ",", "ge\u00b7schwin\u00b7de", "."], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "ADJA", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.5": {"line.1": {"text": "Die Fenster auf, die Herzen auf!", "tokens": ["Die", "Fens\u00b7ter", "auf", ",", "die", "Her\u00b7zen", "auf", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Geschwinde! Geschwinde!", "tokens": ["Ge\u00b7schwin\u00b7de", "!", "Ge\u00b7schwin\u00b7de", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$.", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Es k\u00f6mmt der Junker Morgenwind,", "tokens": ["Es", "k\u00f6mmt", "der", "Jun\u00b7ker", "Mor\u00b7gen\u00b7wind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein bausebackig rotes Kind,", "tokens": ["Ein", "bau\u00b7se\u00b7ba\u00b7ckig", "ro\u00b7tes", "Kind", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und bl\u00e4st, das Alles klingt und klirrt,", "tokens": ["Und", "bl\u00e4st", ",", "das", "Al\u00b7les", "klingt", "und", "klirrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PRELS", "PIS", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Bis seinem Herrn ge\u00f6ffnet wird,", "tokens": ["Bis", "sei\u00b7nem", "Herrn", "ge\u00b7\u00f6ff\u00b7net", "wird", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Geschwinde, geschwinde.", "tokens": ["Ge\u00b7schwin\u00b7de", ",", "ge\u00b7schwin\u00b7de", "."], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "ADJA", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.6": {"line.1": {"text": "Die Fenster auf, die Herzen auf!", "tokens": ["Die", "Fens\u00b7ter", "auf", ",", "die", "Her\u00b7zen", "auf", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Geschwinde! Geschwinde!", "tokens": ["Ge\u00b7schwin\u00b7de", "!", "Ge\u00b7schwin\u00b7de", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$.", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Es k\u00f6mmt der Ritter Sonnenschein,", "tokens": ["Es", "k\u00f6mmt", "der", "Rit\u00b7ter", "Son\u00b7nen\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der bricht mit goldnen Lanzen ein,", "tokens": ["Der", "bricht", "mit", "gold\u00b7nen", "Lan\u00b7zen", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der sanfte Schmeichler Bl\u00fcthenhauch", "tokens": ["Der", "sanf\u00b7te", "Schmeich\u00b7ler", "Bl\u00fc\u00b7then\u00b7hauch"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Schleicht durch die engsten Ritzen auch,", "tokens": ["Schleicht", "durch", "die", "engs\u00b7ten", "Rit\u00b7zen", "auch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Geschwinde, geschwinde.", "tokens": ["Ge\u00b7schwin\u00b7de", ",", "ge\u00b7schwin\u00b7de", "."], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "ADJA", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.7": {"line.1": {"text": "Die Fenster auf, die Herzen auf!", "tokens": ["Die", "Fens\u00b7ter", "auf", ",", "die", "Her\u00b7zen", "auf", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Geschwinde! Geschwinde!", "tokens": ["Ge\u00b7schwin\u00b7de", "!", "Ge\u00b7schwin\u00b7de", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$.", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Zum Angriff schl\u00e4gt die Nachtigall,", "tokens": ["Zum", "An\u00b7griff", "schl\u00