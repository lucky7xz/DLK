{"dta.poem.21643": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Resignation.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "M\u00f6gen Andre ihrer Seele Bl\u00fcthen", "tokens": ["M\u00f6\u00b7gen", "And\u00b7re", "ih\u00b7rer", "See\u00b7le", "Bl\u00fc\u00b7then"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PIS", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Und des Lebens Ruhegl\u00fcck,", "tokens": ["Und", "des", "Le\u00b7bens", "Ru\u00b7he\u00b7gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Deren Herzen nach dem Lorbeer gl\u00fchten,", "tokens": ["De\u00b7ren", "Her\u00b7zen", "nach", "dem", "Lor\u00b7beer", "gl\u00fch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Weihen dem Geschick.", "tokens": ["Wei\u00b7hen", "dem", "Ge\u00b7schick", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Haschen in des Lebens reichen Tagen,", "tokens": ["Ha\u00b7schen", "in", "des", "Le\u00b7bens", "rei\u00b7chen", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Was der Gott als K\u00f6der streut,", "tokens": ["Was", "der", "Gott", "als", "K\u00f6\u00b7der", "streut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "KOUS", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "M\u00f6gen Andre nach den Kr\u00e4nzen jagen,", "tokens": ["M\u00f6\u00b7gen", "And\u00b7re", "nach", "den", "Kr\u00e4n\u00b7zen", "ja\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Die die Mitwelt beut!", "tokens": ["Die", "die", "Mit\u00b7welt", "beut", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Schaal und nichtig ist wie nichts die Ehre,", "tokens": ["Schaal", "und", "nich\u00b7tig", "ist", "wie", "nichts", "die", "Eh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "VAFIN", "KOKOM", "PIS", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Die die Namen heut umschallt:", "tokens": ["Die", "die", "Na\u00b7men", "heut", "um\u00b7schallt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und gef\u00e4lschter Lorbeer dr\u00fcckt, der leere,", "tokens": ["Und", "ge\u00b7f\u00e4lschter", "Lor\u00b7beer", "dr\u00fcckt", ",", "der", "lee\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der die Stirn umwallt.", "tokens": ["Der", "die", "Stirn", "um\u00b7wallt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Ewig zeuget aus der Erden Scho\u00dfe", "tokens": ["E\u00b7wig", "zeu\u00b7get", "aus", "der", "Er\u00b7den", "Scho\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Neue Wesen die Natur,", "tokens": ["Neu\u00b7e", "We\u00b7sen", "die", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und verweht mit ihrem Todeslose", "tokens": ["Und", "ver\u00b7weht", "mit", "ih\u00b7rem", "To\u00b7des\u00b7lo\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Schaust du kaum die Spur.", "tokens": ["Schaust", "du", "kaum", "die", "Spur", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Selig preis\u2019 ich, wer im Leben schaffen,", "tokens": ["Se\u00b7lig", "preis'", "ich", ",", "wer", "im", "Le\u00b7ben", "schaf\u00b7fen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "$,", "PWS", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wirken kann, ein treuer Hirt;", "tokens": ["Wir\u00b7ken", "kann", ",", "ein", "treu\u00b7er", "Hirt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "$,", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Den des Todes Schauer sanft entraffen,", "tokens": ["Den", "des", "To\u00b7des", "Schau\u00b7er", "sanft", "en\u00b7traf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "NE", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Der vergessen wird. \u2014", "tokens": ["Der", "ver\u00b7ges\u00b7sen", "wird", "."], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ART", "VVPP", "VAFIN", "$.", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}