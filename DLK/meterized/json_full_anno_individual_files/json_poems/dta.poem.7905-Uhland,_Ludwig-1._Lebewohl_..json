{"dta.poem.7905": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1.  Lebewohl .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Lebe wohl, lebe wohl, mein Lieb!", "tokens": ["Le\u00b7be", "wohl", ",", "le\u00b7be", "wohl", ",", "mein", "Lieb", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$,", "VVFIN", "ADV", "$,", "PPOSAT", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Mu\u00df noch heute scheiden.", "tokens": ["Mu\u00df", "noch", "heu\u00b7te", "schei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ADV", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Einen Ku\u00df, einen Ku\u00df mir gib!", "tokens": ["Ei\u00b7nen", "Ku\u00df", ",", "ei\u00b7nen", "Ku\u00df", "mir", "gib", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "PPER", "VVIMP", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Mu\u00df dich ewig meiden.", "tokens": ["Mu\u00df", "dich", "e\u00b7wig", "mei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "ADJD", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Eine Bl\u00fcth\u2019, eine Bl\u00fcth\u2019 mir brich,", "tokens": ["Ei\u00b7ne", "Bl\u00fcth'", ",", "ei\u00b7ne", "Bl\u00fcth'", "mir", "brich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "PPER", "ADJD", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Von dem Baum im Garten!", "tokens": ["Von", "dem", "Baum", "im", "Gar\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "NN", "$."], "meter": "--+-+-", "measure": "anapaest.init"}, "line.3": {"text": "Keine Frucht, keine Frucht f\u00fcr mich!", "tokens": ["Kei\u00b7ne", "Frucht", ",", "kei\u00b7ne", "Frucht", "f\u00fcr", "mich", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "APPR", "PPER", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Darf sie nicht erwarten.", "tokens": ["Darf", "sie", "nicht", "er\u00b7war\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}