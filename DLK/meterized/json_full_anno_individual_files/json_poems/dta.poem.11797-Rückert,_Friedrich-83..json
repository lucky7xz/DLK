{"dta.poem.11797": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "83.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Liebe Blick ist gut, b\u00f6s ist der Blick des Neides,", "tokens": ["Der", "Lie\u00b7be", "Blick", "ist", "gut", ",", "b\u00f6s", "ist", "der", "Blick", "des", "Nei\u00b7des", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADJD", "$,", "ADJD", "VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Liebe Blick thut wohl, der Blick des Neids thut Leides.", "tokens": ["Der", "Lie\u00b7be", "Blick", "thut", "wohl", ",", "der", "Blick", "des", "Neids", "thut", "Lei\u00b7des", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "$,", "ART", "NN", "ART", "NN", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Der Blick des Neides rei\u00dft das Haus des Nachbarn ein,", "tokens": ["Der", "Blick", "des", "Nei\u00b7des", "rei\u00dft", "das", "Haus", "des", "Nach\u00b7barn", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "ART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Blick der Liebe f\u00e4llt hinein wie Sonnenschein.", "tokens": ["Der", "Blick", "der", "Lie\u00b7be", "f\u00e4llt", "hin\u00b7ein", "wie", "Son\u00b7nen\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "PTKVZ", "KOKOM", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Der Blick des Neides zehrt wie Sommerglut die Bronnen,", "tokens": ["Der", "Blick", "des", "Nei\u00b7des", "zehrt", "wie", "Som\u00b7mer\u00b7glut", "die", "Bron\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "KOKOM", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Blick der Liebe schwellt das Herz wie Fr\u00fchlingswonnen.", "tokens": ["Der", "Blick", "der", "Lie\u00b7be", "schwellt", "das", "Herz", "wie", "Fr\u00fch\u00b7lings\u00b7won\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "ART", "NN", "KOKOM", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Dem Blick der Liebe blickt entgegen Lieb' aus allen,", "tokens": ["Dem", "Blick", "der", "Lie\u00b7be", "blickt", "ent\u00b7ge\u00b7gen", "Lieb'", "aus", "al\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "APPR", "NN", "APPR", "PIAT", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des Neides Wohlthun ist aufs eigne Herz gefallen.", "tokens": ["Des", "Nei\u00b7des", "Wohl\u00b7thun", "ist", "aufs", "eig\u00b7ne", "Herz", "ge\u00b7fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Der Blick des Neides sieht zu seiner eignen Pein", "tokens": ["Der", "Blick", "des", "Nei\u00b7des", "sieht", "zu", "sei\u00b7ner", "eig\u00b7nen", "Pein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nur alles fremde gro\u00df und alles eigne klein.", "tokens": ["Nur", "al\u00b7les", "frem\u00b7de", "gro\u00df", "und", "al\u00b7les", "eig\u00b7ne", "klein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "ADJA", "ADJD", "KON", "PIAT", "ADJA", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Der Blick der Liebe sieht gern alle gut und reich;", "tokens": ["Der", "Blick", "der", "Lie\u00b7be", "sieht", "gern", "al\u00b7le", "gut", "und", "reich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "ADV", "PIS", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Denn nur die Liebe macht dem Eignen Fremdes gleich.", "tokens": ["Denn", "nur", "die", "Lie\u00b7be", "macht", "dem", "Eig\u00b7nen", "Frem\u00b7des", "gleich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}