{"dta.poem.5227": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "Der weinende Amor,  \n bey Betrachtung einer Bilds\u00e4ule zu Char-  \n lottenburg im Garten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-200905197778", "language": ["de:0.85", "sv:0.14"], "booktitle": "Karsch, Anna Luise: Auserlesene Gedichte. Berlin, 1764."}, "poem": {"stanza.1": {"line.1": {"text": "Was fehlt doch dem allm\u00e4chtgen G\u00f6tterkinde,", "tokens": ["Was", "fehlt", "doch", "dem", "all\u00b7m\u00e4cht\u00b7gen", "G\u00f6t\u00b7ter\u00b7kin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das alle Welt zu drohen scheint,", "tokens": ["Das", "al\u00b7le", "Welt", "zu", "dro\u00b7hen", "scheint", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Cytherens Sohn? Was that er denn f\u00fcr S\u00fcnde?", "tokens": ["Cy\u00b7the\u00b7rens", "Sohn", "?", "Was", "that", "er", "denn", "f\u00fcr", "S\u00fcn\u00b7de", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "PWS", "VVFIN", "PPER", "ADV", "APPR", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Er ward gestraft, und weint?", "tokens": ["Er", "ward", "ge\u00b7straft", ",", "und", "weint", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}