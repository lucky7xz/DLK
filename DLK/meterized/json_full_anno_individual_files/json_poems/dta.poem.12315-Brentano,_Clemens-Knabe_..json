{"dta.poem.12315": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Knabe .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1806", "urn": "urn:nbn:de:kobv:b4-20090519157", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Zum Sterben bin ich,               ", "tokens": ["Zum", "Ster\u00b7ben", "bin", "ich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Verliebet in dich,", "tokens": ["Ver\u00b7lie\u00b7bet", "in", "dich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Deine schwarzbraune Aeugelein,", "tokens": ["Dei\u00b7ne", "schwarz\u00b7brau\u00b7ne", "A\u00b7e\u00b7u\u00b7ge\u00b7lein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Verf\u00fchren ja mich::", "tokens": ["Ver\u00b7f\u00fch\u00b7ren", "ja", "mich", ":", ":"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["NN", "ADV", "PPER", "$.", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Bist hier oder bist dort,", "tokens": ["Bist", "hier", "o\u00b7der", "bist", "dort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "KON", "VAFIN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Oder sonst an eim Ort,", "tokens": ["O\u00b7der", "sonst", "an", "eim", "Ort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wollt' w\u00fcnsche, konnt rede,", "tokens": ["Wollt'", "w\u00fcn\u00b7sche", ",", "konnt", "re\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "VVFIN", "$,", "VMFIN", "VVFIN", "$,"], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.4": {"text": "Mit dir ein Paar Wort. ::", "tokens": ["Mit", "dir", "ein", "Paar", "Wort", ".", ":", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "NN", "$.", "$.", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Wollt' w\u00fcnsche es w\u00e4r Nacht,", "tokens": ["Wollt'", "w\u00fcn\u00b7sche", "es", "w\u00e4r", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVFIN", "PPER", "VAFIN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Mein Bettlein w\u00e4r gemacht,", "tokens": ["Mein", "Bet\u00b7tlein", "w\u00e4r", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich wollt mich drein legen,", "tokens": ["Ich", "wollt", "mich", "drein", "le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Feins Liebchen darneben,", "tokens": ["Feins", "Lieb\u00b7chen", "dar\u00b7ne\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "PAV", "$,"], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.5": {"text": "Wollt s' herzen da\u00df s' lacht.", "tokens": ["Wollt", "s'", "her\u00b7zen", "da\u00df", "s'", "lacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NE", "VVFIN", "KOUS", "NE", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Mein Herz ist verwund't,", "tokens": ["Mein", "Herz", "ist", "ver\u00b7wund't", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Komme Sch\u00e4tzl' machs gesund,", "tokens": ["Kom\u00b7me", "Sch\u00e4tzl'", "machs", "ge\u00b7sund", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "VVFIN", "ADJD", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Erlaub mir zu k\u00fcssen", "tokens": ["Er\u00b7laub", "mir", "zu", "k\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "PTKZU", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Dein'n purpurrothen Mund. ::", "tokens": ["Dein'n", "pur\u00b7pur\u00b7ro\u00b7then", "Mund", ".", ":", ":"], "token_info": ["word", "word", "word", "punct", "punct", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$.", "$.", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Dein purpurrother Mund,", "tokens": ["Dein", "pur\u00b7pur\u00b7ro\u00b7ther", "Mund", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Macht Herzen gesund,", "tokens": ["Macht", "Her\u00b7zen", "ge\u00b7sund", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "VVPP", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Macht Jugend verst\u00e4ndig,", "tokens": ["Macht", "Ju\u00b7gend", "ver\u00b7st\u00e4n\u00b7dig", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "ADJD", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Macht Todte lebendig,", "tokens": ["Macht", "Tod\u00b7te", "le\u00b7ben\u00b7dig", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "ADJD", "$,"], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.5": {"text": "Macht Kranke gesund.", "tokens": ["Macht", "Kran\u00b7ke", "ge\u00b7sund", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "VVPP", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.6": {"line.1": {"text": "Meine Mutter hat nur", "tokens": ["Mei\u00b7ne", "Mut\u00b7ter", "hat", "nur"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Ein schwarzbraune Kuh,", "tokens": ["Ein", "schwarz\u00b7brau\u00b7ne", "Kuh", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Wer wird sie denn melken,", "tokens": ["Wer", "wird", "sie", "denn", "mel\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Wenn ich heurathen thu. ::", "tokens": ["Wenn", "ich", "heu\u00b7ra\u00b7then", "thu", ".", ":", ":"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["KOUS", "PPER", "VVINF", "VVFIN", "$.", "$.", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Der dies Liedchen gemacht,", "tokens": ["Der", "dies", "Lied\u00b7chen", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PDS", "NN", "VVPP", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Hat's Lieben erdacht,", "tokens": ["Hat's", "Lie\u00b7ben", "er\u00b7dacht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Drum w\u00fcnsch ich mein feins Liebchen,", "tokens": ["Drum", "w\u00fcnsch", "ich", "mein", "feins", "Lieb\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Viel tausend gute Nacht. ::", "tokens": ["Viel", "tau\u00b7send", "gu\u00b7te", "Nacht", ".", ":", ":"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ADV", "ADJD", "ADJA", "NN", "$.", "$.", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}