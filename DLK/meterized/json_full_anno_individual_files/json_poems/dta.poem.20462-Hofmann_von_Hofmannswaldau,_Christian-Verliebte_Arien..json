{"dta.poem.20462": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Verliebte Arien.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was wiltu Doris machen/", "tokens": ["Was", "wil\u00b7tu", "Do\u00b7ris", "ma\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "NE", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Brich deinen stoltzen geist;", "tokens": ["Brich", "dei\u00b7nen", "stolt\u00b7zen", "geist", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Di\u00df was du sch\u00f6nheit heist/", "tokens": ["Di\u00df", "was", "du", "sch\u00f6n\u00b7heit", "heist", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRELS", "PPER", "ADJD", "VAFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Sind blumen gleiche sachen/", "tokens": ["Sind", "blu\u00b7men", "glei\u00b7che", "sa\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ADJA", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die unbest\u00e4ndig sind/", "tokens": ["Die", "un\u00b7be\u00b7st\u00e4n\u00b7dig", "sind", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VAFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Und fliehen wie der wind.", "tokens": ["Und", "flie\u00b7hen", "wie", "der", "wind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Es wird auff deinen wangen", "tokens": ["Es", "wird", "auff", "dei\u00b7nen", "wan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nicht steter fr\u00fchling seyn.", "tokens": ["Nicht", "ste\u00b7ter", "fr\u00fch\u00b7ling", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "ADJD", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es weicht der sternen schein/", "tokens": ["Es", "weicht", "der", "ster\u00b7nen", "schein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Als wie der blumen prangen.", "tokens": ["Als", "wie", "der", "blu\u00b7men", "pran\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die zeit so alles bricht/", "tokens": ["Die", "zeit", "so", "al\u00b7les", "bricht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PIS", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Schont auch des leibes nicht.", "tokens": ["Schont", "auch", "des", "lei\u00b7bes", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "PTKNEG", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.3": {"line.1": {"text": "Was ist der sch\u00f6nheit gl\u00e4ntzen/", "tokens": ["Was", "ist", "der", "sch\u00f6n\u00b7heit", "gl\u00e4nt\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Als ein geschwinder plitz?", "tokens": ["Als", "ein", "ge\u00b7schwin\u00b7der", "plitz", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sein zubereiter sitz", "tokens": ["Sein", "zu\u00b7be\u00b7rei\u00b7ter", "sitz"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Besteht in engen gr\u00e4ntzen.", "tokens": ["Be\u00b7steht", "in", "en\u00b7gen", "gr\u00e4nt\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Kein flu\u00df verrauscht so bald/", "tokens": ["Kein", "flu\u00df", "ver\u00b7rauscht", "so", "bald", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ADV", "ADV", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Als sch\u00f6nheit und gestalt.", "tokens": ["Als", "sch\u00f6n\u00b7heit", "und", "ge\u00b7stalt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Was heute purpur tr\u00e4get/", "tokens": ["Was", "heu\u00b7te", "pur\u00b7pur", "tr\u00e4\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und alabaster f\u00fchrt:", "tokens": ["Und", "a\u00b7la\u00b7bas\u00b7ter", "f\u00fchrt", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Was sich mit rosen ziert/", "tokens": ["Was", "sich", "mit", "ro\u00b7sen", "ziert", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "APPR", "NE", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wird morgen hingeleget/", "tokens": ["Wird", "mor\u00b7gen", "hin\u00b7ge\u00b7le\u00b7get", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und ruhet ungeacht", "tokens": ["Und", "ru\u00b7het", "un\u00b7ge\u00b7acht"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "In seiner todes nacht.", "tokens": ["In", "sei\u00b7ner", "to\u00b7des", "nacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Nun Doris lerne kennen/", "tokens": ["Nun", "Do\u00b7ris", "ler\u00b7ne", "ken\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "VVFIN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was falscher hochmuth sey:", "tokens": ["Was", "fal\u00b7scher", "hoch\u00b7muth", "sey", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bleib nicht alleine frey/", "tokens": ["Bleib", "nicht", "al\u00b7lei\u00b7ne", "frey", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ADV", "ADJD", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "La\u00df deine jugend brennen/", "tokens": ["La\u00df", "dei\u00b7ne", "ju\u00b7gend", "bren\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und la\u00df der liebe glut", "tokens": ["Und", "la\u00df", "der", "lie\u00b7be", "glut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Durchwandern hertz und blut.", "tokens": ["Durch\u00b7wan\u00b7dern", "hertz", "und", "blut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Gebrauche deine sch\u00e4tze/", "tokens": ["Ge\u00b7brau\u00b7che", "dei\u00b7ne", "sch\u00e4t\u00b7ze", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Weil blut und bl\u00fcte siegt.", "tokens": ["Weil", "blut", "und", "bl\u00fc\u00b7te", "siegt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wann dich die zeit betriegt/", "tokens": ["Wann", "dich", "die", "zeit", "be\u00b7triegt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "So trennet auch das netze/", "tokens": ["So", "tren\u00b7net", "auch", "das", "net\u00b7ze", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "ADJA", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "So vormahls um dich hieng/", "tokens": ["So", "vor\u00b7mahls", "um", "dich", "hieng", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PPER", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Und manche seele fieng.", "tokens": ["Und", "man\u00b7che", "see\u00b7le", "fi\u00b7eng", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.7": {"line.1": {"text": "So du dich selbst kanst lieben/", "tokens": ["So", "du", "dich", "selbst", "kanst", "lie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "PRF", "ADV", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So nimm die warnung an/", "tokens": ["So", "nimm", "die", "war\u00b7nung", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "ART", "NN", "PTKVZ", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die ich dir itzt gethan.", "tokens": ["Die", "ich", "dir", "itzt", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ich werde mich betr\u00fcben/", "tokens": ["Ich", "wer\u00b7de", "mich", "be\u00b7tr\u00fc\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "So diese rose stirbt/", "tokens": ["So", "die\u00b7se", "ro\u00b7se", "stirbt", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PDAT", "ADJA", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Und ohne lust verdirbt.", "tokens": ["Und", "oh\u00b7ne", "lust", "ver\u00b7dirbt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}