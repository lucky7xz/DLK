{"textgrid.poem.38505": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Tr\u00f6stung", "genre": "verse", "period": "N.A.", "pub_year": 1796, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "S\u00e4uselten gleich nicht immer Fr\u00fchlingsl\u00fcfte", "tokens": ["S\u00e4u\u00b7sel\u00b7ten", "gleich", "nicht", "im\u00b7mer", "Fr\u00fch\u00b7lings\u00b7l\u00fcf\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "PTKNEG", "ADV", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Um den rosigen Lenzbaum meiner Jugend,", "tokens": ["Um", "den", "ro\u00b7si\u00b7gen", "Lenz\u00b7baum", "mei\u00b7ner", "Ju\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "PPOSAT", "NN", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Beugte gleich verheerender Nachtsturm oft sein", "tokens": ["Beug\u00b7te", "gleich", "ver\u00b7hee\u00b7ren\u00b7der", "Nachts\u00b7turm", "oft", "sein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ADJA", "NN", "ADV", "PPOSAT"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Bl\u00fctenhaupt nieder!", "tokens": ["Bl\u00fc\u00b7ten\u00b7haupt", "nie\u00b7der", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKVZ", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "O! so enth\u00fcllte sch\u00f6ner, nach dem Wetter,", "tokens": ["O", "!", "so", "ent\u00b7h\u00fcll\u00b7te", "sch\u00f6\u00b7ner", ",", "nach", "dem", "Wet\u00b7ter", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ADV", "VVFIN", "ADJD", "$,", "APPR", "ART", "NN", "$,"], "me