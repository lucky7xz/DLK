{"dta.poem.13126": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Achtes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.85", "pl:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201ejhr seht wie Riesen-starck sie jene Last ertr\u00e4gt,", "tokens": ["\u201e", "jhr", "seht", "wie", "Rie\u00b7sen\u00b7sta\u00b7rck", "sie", "je\u00b7ne", "Last", "er\u00b7tr\u00e4gt", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "KOKOM", "NN", "PPER", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+----+-+-+", "measure": "unknown.measure.penta"}, "line.2": {"text": "\u201edie manches st\u00e4hlern Herz in tiefe Schwermuth schl\u00e4gt.", "tokens": ["\u201e", "die", "man\u00b7ches", "st\u00e4h\u00b7lern", "Herz", "in", "tie\u00b7fe", "Schwer\u00b7muth", "schl\u00e4gt", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "PIAT", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "625\u201dJhr wi\u00dft, da\u00df Kronen-Schmuck und K\u00f6nigliche W\u00fcrden", "tokens": ["\"", "Ihr", "wi\u00dft", ",", "da\u00df", "Kro\u00b7nen\u00b7Schmuck", "und", "K\u00f6\u00b7nig\u00b7li\u00b7che", "W\u00fcr\u00b7den"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VVFIN", "$,", "KOUS", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u201emehr Last als Herrlichkeit auf solche Stirnen b\u00fcrden:", "tokens": ["\u201e", "mehr", "Last", "als", "Herr\u00b7lich\u00b7keit", "auf", "sol\u00b7che", "Stir\u00b7nen", "b\u00fcr\u00b7den", ":"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PIAT", "NN", "KOUS", "NN", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "\u201ewie, wer den Zepter f\u00fchrt, nicht sich, nur andern lebt,", "tokens": ["\u201e", "wie", ",", "wer", "den", "Zep\u00b7ter", "f\u00fchrt", ",", "nicht", "sich", ",", "nur", "an\u00b7dern", "lebt", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "$,", "PWS", "ART", "NN", "VVFIN", "$,", "PTKNEG", "PRF", "$,", "ADV", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "\u201eund mehr nach fremdem Heil als nach dem eignen strebt.", "tokens": ["\u201e", "und", "mehr", "nach", "frem\u00b7dem", "Heil", "als", "nach", "dem", "eig\u00b7nen", "strebt", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "ADV", "APPR", "ADJA", "NN", "KOKOM", "APPR", "ART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}