{"dta.poem.23475": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Trostlied.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Bedr\u00e4ngtes Deutschland, sch\u00f6pfe Muth!", "tokens": ["Be\u00b7dr\u00e4ng\u00b7tes", "Deutschland", ",", "sch\u00f6p\u00b7fe", "Muth", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Der Himmel wird nicht immer wittern", "tokens": ["Der", "Him\u00b7mel", "wird", "nicht", "im\u00b7mer", "wit\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PTKNEG", "ADV", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und dieser L\u00e4nder Grund ersch\u00fcttern;", "tokens": ["Und", "die\u00b7ser", "L\u00e4n\u00b7der", "Grund", "er\u00b7sch\u00fct\u00b7tern", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Er schl\u00e4gt dich jetzt zwar bis aufs Blut,", "tokens": ["Er", "schl\u00e4gt", "dich", "jetzt", "zwar", "bis", "aufs", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "APPR", "APPRART", "NN", "$,"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.5": {"text": "Doch sch\u00f6pfe du nur wieder Muth.", "tokens": ["Doch", "sch\u00f6p\u00b7fe", "du", "nur", "wie\u00b7der", "Muth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADV", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Es wird nicht immer dunkel sein,", "tokens": ["Es", "wird", "nicht", "im\u00b7mer", "dun\u00b7kel", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADV", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Wolken werden bald verschwinden,", "tokens": ["Die", "Wol\u00b7ken", "wer\u00b7den", "bald", "ver\u00b7schwin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die Sonne wird sich wiederfinden", "tokens": ["Die", "Son\u00b7ne", "wird", "sich", "wie\u00b7der\u00b7fin\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PRF", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und Finsterni\u00df und Nacht zerstreu'n;", "tokens": ["Und", "Fins\u00b7ter\u00b7ni\u00df", "und", "Nacht", "zer\u00b7streu'n", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es wird nicht immer dunkel sein.", "tokens": ["Es", "wird", "nicht", "im\u00b7mer", "dun\u00b7kel", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Bedr\u00e4ngtes Deutschland, nur Geduld!", "tokens": ["Be\u00b7dr\u00e4ng\u00b7tes", "Deutschland", ",", "nur", "Ge\u00b7duld", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADV", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Wirf, wenn es sonst an Tr\u00f6stern fehlet,", "tokens": ["Wirf", ",", "wenn", "es", "sonst", "an", "Tr\u00f6s\u00b7tern", "feh\u00b7let", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "PPER", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die Sorge, die dich kr\u00e4nkt und qu\u00e4let,", "tokens": ["Die", "Sor\u00b7ge", ",", "die", "dich", "kr\u00e4nkt", "und", "qu\u00e4\u00b7let", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Auf Gottes Lieb' und Vaterhuld!", "tokens": ["Auf", "Got\u00b7tes", "Lieb'", "und", "Va\u00b7ter\u00b7huld", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Betr\u00fcbtes Deutschland, nur Geduld!", "tokens": ["Be\u00b7tr\u00fcb\u00b7tes", "Deutschland", ",", "nur", "Ge\u00b7duld", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "ADV", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}