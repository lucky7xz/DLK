{"textgrid.poem.38542": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Sehnsucht", "genre": "verse", "period": "N.A.", "pub_year": 1778, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gottes D\u00e4mrung ist sch\u00f6n! Wonne der Himlischen", "tokens": ["Got\u00b7tes", "D\u00e4m\u00b7rung", "ist", "sch\u00f6n", "!", "Won\u00b7ne", "der", "Him\u00b7li\u00b7schen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "NN", "VAFIN", "ADJD", "$.", "NN", "ART", "NN"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "T\u00f6nt dein Abendgesang, fl\u00f6tende Nachtigall!", "tokens": ["T\u00f6nt", "dein", "A\u00b7bend\u00b7ge\u00b7sang", ",", "fl\u00f6\u00b7ten\u00b7de", "Nach\u00b7ti\u00b7gall", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "ADJA", "NN", "$."], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.3": {"text": "Und es h\u00fcllet mein Auge", "tokens": ["Und", "es", "h\u00fcl\u00b7let", "mein", "Au\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "In den Schleier der Wehmuth sich?", "tokens": ["In", "den", "Schlei\u00b7er", "der", "Weh\u00b7muth", "sich", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "PRF", "$."], "meter": "--+--++-", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Die du liebest ist fern! fl\u00fcstert mein Genius,", "tokens": ["Die", "du", "lie\u00b7best", "ist", "fern", "!", "fl\u00fcs\u00b7tert", "mein", "Ge\u00b7nius", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["A