{"dta.poem.2932": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "131.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.85", "sv:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was man nicht \u00e4ndern kann, soll man nicht \u00e4ndern wollen;", "tokens": ["Was", "man", "nicht", "\u00e4n\u00b7dern", "kann", ",", "soll", "man", "nicht", "\u00e4n\u00b7dern", "wol\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "PTKNEG", "VVINF", "VMFIN", "$,", "VMFIN", "PIS", "PTKNEG", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gott hat es so gef\u00fcgt, wie wirs ertragen sollen.", "tokens": ["Gott", "hat", "es", "so", "ge\u00b7f\u00fcgt", ",", "wie", "wirs", "er\u00b7tra\u00b7gen", "sol\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADV", "VVPP", "$,", "PWAV", "PIS", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Den starren Dingen nicht allein bequeme dich,", "tokens": ["Den", "star\u00b7ren", "Din\u00b7gen", "nicht", "al\u00b7lein", "be\u00b7que\u00b7me", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKNEG", "ADV", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den Menschen auch, wenn sie sind unverbesserlich.", "tokens": ["Den", "Men\u00b7schen", "auch", ",", "wenn", "sie", "sind", "un\u00b7ver\u00b7bes\u00b7ser\u00b7lich", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV