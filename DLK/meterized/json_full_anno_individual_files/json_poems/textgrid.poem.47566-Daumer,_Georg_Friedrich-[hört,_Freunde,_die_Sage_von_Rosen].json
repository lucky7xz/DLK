{"textgrid.poem.47566": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[h\u00f6rt, Freunde, die Sage von Rosen]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "H\u00f6rt, Freunde, die Sage von Rosen,", "tokens": ["H\u00f6rt", ",", "Freun\u00b7de", ",", "die", "Sa\u00b7ge", "von", "Ro\u00b7sen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "ART", "NN", "APPR", "NE", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Narcissen, Lilien, Tulipanen geh'n!", "tokens": ["Nar\u00b7cis\u00b7sen", ",", "Li\u00b7li\u00b7en", ",", "Tu\u00b7li\u00b7pa\u00b7nen", "geh'n", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NE", "$,", "NN", "VVPP", "$."], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Nicht m\u00f6ge sie ohne die fromme", "tokens": ["Nicht", "m\u00f6\u00b7ge", "sie", "oh\u00b7ne", "die", "from\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "VMFIN", "PPER", "APPR", "ART", "ADJA"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Sprenge des Weines ihre Bahnen geh'n! \u2013", "tokens": ["Spren\u00b7ge", "des", "Wei\u00b7nes", "ih\u00b7re", "Bah\u00b7nen", "geh'n", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "ART", "NN", "PPOSAT", "NN", "VVPP", "$.", "$("], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.2": {"line.1": {"text": "O seht dies reizende Schelmchen,", "tokens": ["O", "seht", "dies", "rei\u00b7zen\u00b7de", "Schelm\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PDS", "ADJA"