{"textgrid.poem.64765": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "12. Der Liebhaber ohne Eifersucht", "genre": "verse", "period": "N.A.", "pub_year": 1744, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ja, neiderf\u00fcllter Buhler Seelen", "tokens": ["Ja", ",", "nei\u00b7der\u00b7f\u00fcll\u00b7ter", "Buh\u00b7ler", "See\u00b7len"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$,", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mag ihrer Sch\u00f6nen Auge qu\u00e4len,", "tokens": ["Mag", "ih\u00b7rer", "Sch\u00f6\u00b7nen", "Au\u00b7ge", "qu\u00e4\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn es nach Andern seitw\u00e4rts zielt;", "tokens": ["Wenn", "es", "nach", "An\u00b7dern", "seit\u00b7w\u00e4rts", "zielt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch, da\u00df ich ", "tokens": ["Doch", ",", "da\u00df", "ich"], "token_info": ["word", "punct", "word", "word"], "pos": ["KON", "$,", "KOUS", "PPER"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Macht, weil sie z\u00e4rtlich hei\u00dfe Triebe", "tokens": ["Macht", ",", "weil", "sie", "z\u00e4rt\u00b7lich", "hei\u00b7\u00dfe", "Trie\u00b7be"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "PPER", "ADJD", "ADJA", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.6": {"text": "In Jedem wirkt, f\u00fcr Jeden f\u00fchlt.", "tokens": ["In", "Je\u00b7dem", "wirkt", ",", "f\u00fcr", "Je\u00b7den", "f\u00fchlt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "VVFIN", "$,", "APPR", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ein einzig Herze zu besiegen,", "tokens": ["Ein", "ein\u00b7zig", "Her\u00b7ze", "zu", "be\u00b7sie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Kann schlechter Sch\u00f6nen Geist vergn\u00fcgen;", "tokens": ["Kann", "schlech\u00b7ter", "Sch\u00f6\u00b7nen", "Geist", "ver\u00b7gn\u00fc\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJA", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Kein Wunsch ist, der sich weiter regt,", "tokens": ["Kein", "Wunsch", "ist", ",", "der", "sich", "wei\u00b7ter", "regt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "$,", "PRELS", "PRF", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn einer nur im Sklavenstande", "tokens": ["Wenn", "ei\u00b7ner", "nur", "im", "Skla\u00b7ven\u00b7stan\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der matten Reizung schwache Bande", "tokens": ["Der", "mat\u00b7ten", "Rei\u00b7zung", "schwa\u00b7che", "Ban\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mit Treue voller Einfalt tr\u00e4gt.", "tokens": ["Mit", "Treu\u00b7e", "vol\u00b7ler", "Ein\u00b7falt", "tr\u00e4gt", "."], "token_info"