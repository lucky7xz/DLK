{"dta.poem.19276": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "9.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die M\u00fccke, wenn sie d\u00e4cht' und spr\u00e4ch', o Mensch, wie du;", "tokens": ["Die", "M\u00fc\u00b7cke", ",", "wenn", "sie", "d\u00e4cht'", "und", "spr\u00e4ch'", ",", "o", "Mensch", ",", "wie", "du", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "VVFIN", "KON", "VVFIN", "$,", "FM", "NN", "$,", "PWAV", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dem H\u00f6chsten legte sie wol ihre Fl\u00fcgel zu:", "tokens": ["Dem", "H\u00f6chs\u00b7ten", "leg\u00b7te", "sie", "wol", "ih\u00b7re", "Fl\u00fc\u00b7gel", "zu", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "\u201ewie sollte seinem Bild mein Sch\u00f6pfer mir nicht gleichen?", "tokens": ["\u201e", "wie", "soll\u00b7te", "sei\u00b7nem", "Bild", "mein", "Sch\u00f6p\u00b7fer", "mir", "nicht", "glei\u00b7chen", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "VMFIN", "PPOSAT", "NN", "PPOSAT", "NN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dem, was er schuf, wird er nicht an Vollendung weichen.", "tokens": ["Dem", ",", "was", "er", "schuf", ",", "wird", "er", "nicht", "an", "Vol\u00b7len\u00b7dung", "wei\u00b7chen", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PWS", "PPER", "VVFIN", "$,", "VAFIN", "PPER", "PTKNEG", "APPR", "NN", "VVINF", "$."], "meter": "+--+--+-+--+-", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Drum m\u00fccken\u00e4hnlich, nur vollkommner wird er seyn;", "tokens": ["Drum", "m\u00fc\u00b7cke\u00b7n\u00e4hn\u00b7lich", ",", "nur", "voll\u00b7komm\u00b7ner", "wird", "er", "seyn", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADJD", "$,", "ADV", "ADJD", "VAFIN", "PPER", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie w\u00e4r' er Gott, wenn er nicht h\u00e4tte Fl\u00fcgelein?\u201c", "tokens": ["Wie", "w\u00e4r'", "er", "Gott", ",", "wenn", "er", "nicht", "h\u00e4t\u00b7te", "Fl\u00fc\u00b7ge\u00b7lein", "?", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "NN", "$,", "KOUS", "PPER", "PTKNEG", "VAFIN", "NN", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}