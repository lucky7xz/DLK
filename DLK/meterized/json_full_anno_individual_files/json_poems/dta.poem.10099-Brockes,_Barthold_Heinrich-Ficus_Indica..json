{"dta.poem.10099": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Ficus Indica.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20086-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie? wachsen Rosen sonder Stengel denn gar aus ei-\nnem Blat hervor?", "tokens": ["Wie", "?", "wach\u00b7sen", "Ro\u00b7sen", "son\u00b7der", "Sten\u00b7gel", "denn", "gar", "aus", "ei", "nem", "Blat", "her\u00b7vor", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "ADJA", "NN", "KON", "NN", "KON", "ADV", "APPR", "TRUNC", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "So rieff ich mit erstaunter Seel\u2019, als ich an Indianschen", "tokens": ["So", "rieff", "ich", "mit", "er\u00b7staun\u00b7ter", "Seel'", ",", "als", "ich", "an", "In\u00b7di\u00b7an\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,", "KOUS", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.3": {"text": "So mir ein wircklich Wunder schien, in sch\u00f6nster Bildung", "tokens": ["So", "mir", "ein", "wir\u00b7ck\u00b7lich", "Wun\u00b7der", "schien", ",", "in", "sch\u00f6ns\u00b7ter", "Bil\u00b7dung"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "PPER", "ART", "ADJD", "NN", "VVFIN", "$,", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}}, "stanza.2": {"line.1": {"text": "Ach ja! was j\u00fcngst ", "tokens": ["Ach", "ja", "!", "was", "j\u00fcngst"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ITJ", "ITJ", "$.", "PWS", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Und trifft es immer mehr und mehr, wenn man es untersu-", "tokens": ["Und", "trifft", "es", "im\u00b7mer", "mehr", "und", "mehr", ",", "wenn", "man", "es", "un\u00b7ter\u00b7su"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADV", "KON", "ADV", "$,", "KOUS", "PIS", "PPER", "TRUNC"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "Ach la\u00df mich denn ie mehr und mehr, aus Deinen Wunder-", "tokens": ["Ach", "la\u00df", "mich", "denn", "ie", "mehr", "und", "mehr", ",", "aus", "Dei\u00b7nen", "Wun\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "VVIMP", "PPER", "ADV", "ADV", "ADV", "KON", "ADV", "$,", "APPR", "PPOSAT", "TRUNC"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "O grosser Sch\u00f6pfer aller Dinge, Dein\u2019 Allmacht, Lieb\u2019 und", "tokens": ["O", "gros\u00b7ser", "Sch\u00f6p\u00b7fer", "al\u00b7ler", "Din\u00b7ge", ",", "Dein'", "All\u00b7macht", ",", "Lieb'", "und"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NE", "ADJA", "NN", "PIAT", "NN", "$,", "PPOSAT", "NN", "$,", "NN", "KON"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}}}}}