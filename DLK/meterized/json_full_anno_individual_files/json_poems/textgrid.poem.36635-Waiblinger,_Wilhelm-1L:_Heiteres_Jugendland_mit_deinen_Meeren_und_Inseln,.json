{"textgrid.poem.36635": {"metadata": {"author": {"name": "Waiblinger, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Heiteres Jugendland mit deinen Meeren und Inseln,", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Heiteres Jugendland mit deinen Meeren und Inseln,", "tokens": ["Hei\u00b7te\u00b7res", "Ju\u00b7gend\u00b7land", "mit", "dei\u00b7nen", "Mee\u00b7ren", "und", "In\u00b7seln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "+--+-+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Sey mir gegr\u00fc\u00dft! Nach dir sehnt sich das weinende Herz!", "tokens": ["Sey", "mir", "ge\u00b7gr\u00fc\u00dft", "!", "Nach", "dir", "sehnt", "sich", "das", "wei\u00b7nen\u00b7de", "Herz", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "$.", "APPR", "PPER", "VVFIN", "PRF", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.3": {"text": "Sch\u00f6nes hab' ich getr\u00e4umt, und mit liebendem Sinne gebildet,", "tokens": ["Sch\u00f6\u00b7nes", "hab'", "ich", "ge\u00b7tr\u00e4umt", ",", "und", "mit", "lie\u00b7ben\u00b7dem", "Sin\u00b7ne", "ge\u00b7bil\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "VVPP", "$,", "KON", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Aber die Tr\u00e4ume floh'n, ach! und ich ward nicht gestillt:", "tokens": ["A\u00b7ber", "die", "Tr\u00e4u\u00b7me", "floh'n", ",", "ach", "!", "und", "ich", "ward", "nicht", "ge\u00b7stillt", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$,", "ITJ", "$.", "KON", "PPER", "VAFIN", "PTKNEG", "VVPP", "$."], "meter": "+--+--+-+-+-+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Ewig strebet das Herz und hei\u00df aus den zw\u00e4ngenden Schranken,", "tokens": ["E\u00b7wig", "stre\u00b7bet", "das", "Herz", "und", "hei\u00df", "aus", "den", "zw\u00e4n\u00b7gen\u00b7den", "Schran\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "KON", "ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.6": {"text": "Weil es da leider nicht fand, was es so gl\u00fchend gesucht.", "tokens": ["Weil", "es", "da", "lei\u00b7der", "nicht", "fand", ",", "was", "es", "so", "gl\u00fc\u00b7hend", "ge\u00b7sucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "PTKNEG", "VVFIN", "$,", "PWS", "PPER", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Weiter und weiter strebt's und dr\u00e4ngt' es im schwellenden Busen;", "tokens": ["Wei\u00b7ter", "und", "wei\u00b7ter", "strebt's", "und", "dr\u00e4ngt'", "es", "im", "schwel\u00b7len\u00b7den", "Bu\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADV", "VVFIN", "KON", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "+--+-+-+--+--+-", "measure": "hexameter"}, "line.8": {"text": "Ach! wie so innig und warm sehnst du verlangend dich fort!", "tokens": ["Ach", "!", "wie", "so", "in\u00b7nig", "und", "warm", "sehnst", "du", "ver\u00b7lan\u00b7gend", "dich", "fort", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PWAV", "ADV", "ADJD", "KON", "ADJD", "VVFIN", "PPER", "VVPP", "PPER", "PTKVZ", "$."], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Dr\u00fcben da hebt sich ein heiteres Land aus wallendem Meere,", "tokens": ["Dr\u00fc\u00b7ben", "da", "hebt", "sich", "ein", "hei\u00b7te\u00b7res", "Land", "aus", "wal\u00b7len\u00b7dem", "Mee\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PRF", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.10": {"text": "Wie aus des Himmels Blau jugendlich Morgengew\u00f6lk.", "tokens": ["Wie", "aus", "des", "Him\u00b7mels", "Blau", "ju\u00b7gend\u00b7lich", "Mor\u00b7gen\u00b7ge\u00b7w\u00f6lk", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "NN", "ADJD", "NN", "$."], "meter": "-+-+-+---+--+", "measure": "iambic.penta.chol"}, "line.11": {"text": "L\u00e4chelnd und gr\u00fcnend ruht es im Ku\u00df der milderen Sonne;", "tokens": ["L\u00e4\u00b7chelnd", "und", "gr\u00fc\u00b7nend", "ruht", "es", "im", "Ku\u00df", "der", "mil\u00b7de\u00b7ren", "Son\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "VVFIN", "PPER", "APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.12": {"text": "Weicher entquillt der Natur dr\u00fcben der mildere Geist.", "tokens": ["Wei\u00b7cher", "ent\u00b7quillt", "der", "Na\u00b7tur", "dr\u00fc\u00b7ben", "der", "mil\u00b7de\u00b7re", "Geist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.13": {"text": "Dunkel auf blumiger H\u00f6b' erhebet die Krone der Lorbeer,", "tokens": ["Dun\u00b7kel", "auf", "blu\u00b7mi\u00b7ger", "H\u00f6b'", "er\u00b7he\u00b7bet", "die", "Kro\u00b7ne", "der", "Lor\u00b7beer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.14": {"text": "Und die L\u00fcfte wie lau, und das Gestade wie gr\u00fcn!", "tokens": ["Und", "die", "L\u00fcf\u00b7te", "wie", "lau", ",", "und", "das", "Ge\u00b7sta\u00b7de", "wie", "gr\u00fcn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "KOKOM", "ADJD", "$,", "KON", "ART", "NN", "KOKOM", "ADJD", "$."], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.15": {"text": "D\u00e4mmernd verm\u00e4hlen der Luft sich der blauenden Berge Gestalten;", "tokens": ["D\u00e4m\u00b7mernd", "ver\u00b7m\u00e4h\u00b7len", "der", "Luft", "sich", "der", "blau\u00b7en\u00b7den", "Ber\u00b7ge", "Ge\u00b7stal\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "PRF", "ART", "ADJA", "NN", "NN", "$."], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.16": {"text": "Wie die Fernen im Hauch linderen Duftes ergl\u00fchn!", "tokens": ["Wie", "die", "Fer\u00b7nen", "im", "Hauch", "lin\u00b7de\u00b7ren", "Duf\u00b7tes", "er\u00b7gl\u00fchn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "APPRART", "NN", "ADJA", "NN", "VVINF", "$."], "meter": "--+--++--+--+", "measure": "anapaest.di.plus"}, "line.17": {"text": "Alles so reg' und alles so zart, in tieferer F\u00fclle!", "tokens": ["Al\u00b7les", "so", "reg'", "und", "al\u00b7les", "so", "zart", ",", "in", "tie\u00b7fe\u00b7rer", "F\u00fcl\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "VVFIN", "KON", "PIS", "ADV", "ADJD", "$,", "APPR", "ADJA", "NN", "$."], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.18": {"text": "Hat er, der ewige Geist, liebender dr\u00fcben verweilt?", "tokens": ["Hat", "er", ",", "der", "e\u00b7wi\u00b7ge", "Geist", ",", "lie\u00b7ben\u00b7der", "dr\u00fc\u00b7ben", "ver\u00b7weilt", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "ART", "ADJA", "NN", "$,", "ADJD", "ADV", "VVPP", "$."], "meter": "-+-+--++--+--+", "measure": "iambic.hexa.relaxed"}, "line.19": {"text": "Wei\u00df, wie die Sonne, bekr\u00e4nzt sich am Ufer das lockige M\u00e4dchen,", "tokens": ["Wei\u00df", ",", "wie", "die", "Son\u00b7ne", ",", "be\u00b7kr\u00e4nzt", "sich", "am", "U\u00b7fer", "das", "lo\u00b7cki\u00b7ge", "M\u00e4d\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "ART", "NN", "$,", "VVFIN", "PRF", "APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.20": {"text": "Wo um das Myrrtengestr\u00e4uch spielet der wogende Schwan.", "tokens": ["Wo", "um", "das", "Myrr\u00b7ten\u00b7ge\u00b7str\u00e4uch", "spie\u00b7let", "der", "wo\u00b7gen\u00b7de", "Schwan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--++--+--+", "measure": "iambic.hexa.relaxed"}, "line.21": {"text": "Stolz, wie die S\u00e4ule sich th\u00fcrmt, das gewaltige Denkmal der Vorzeit,", "tokens": ["Stolz", ",", "wie", "die", "S\u00e4u\u00b7le", "sich", "th\u00fcrmt", ",", "das", "ge\u00b7wal\u00b7ti\u00b7ge", "Denk\u00b7mal", "der", "Vor\u00b7zeit", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ART", "NN", "PRF", "VVFIN", "$,", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.22": {"text": "Stehet der J\u00fcngling, von Kraft feurig den Busen geschwellt.", "tokens": ["Ste\u00b7het", "der", "J\u00fcng\u00b7ling", ",", "von", "Kraft", "feu\u00b7rig", "den", "Bu\u00b7sen", "ge\u00b7schwellt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "APPR", "NN", "ADJD", "ART", "NN", "VVPP", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.23": {"text": "Ach! wie so sinnig das Bild, das entfaltete! Wird es noch werden?", "tokens": ["Ach", "!", "wie", "so", "sin\u00b7nig", "das", "Bild", ",", "das", "ent\u00b7fal\u00b7te\u00b7te", "!", "Wird", "es", "noch", "wer\u00b7den", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PWAV", "ADV", "ADJD", "ART", "NN", "$,", "PDS", "VVFIN", "$.", "VAFIN", "PPER", "ADV", "VAINF", "$."], "meter": "-+-+--+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.24": {"text": "Wird sich der Blume Pracht dr\u00e4ngen aus h\u00fcllendem Keim?", "tokens": ["Wird", "sich", "der", "Blu\u00b7me", "Pracht", "dr\u00e4n\u00b7gen", "aus", "h\u00fcl\u00b7len\u00b7dem", "Keim", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PRF", "ART", "NN", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "---+-++--+--+", "measure": "iambic.penta.relaxed"}, "line.25": {"text": "Wiederkehrt es! Du ahnst es so tief! du verlangst es so ahnend!", "tokens": ["Wie\u00b7der\u00b7kehrt", "es", "!", "Du", "ahnst", "es", "so", "tief", "!", "du", "ver\u00b7langst", "es", "so", "ah\u00b7nend", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$.", "PPER", "VVFIN", "PPER", "ADV", "ADJD", "$.", "PPER", "VVFIN", "PPER", "ADV", "ADJD", "$."], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.26": {"text": "Liebender Busen! es kehrt wieder die gl\u00fcckliche Zeit.", "tokens": ["Lie\u00b7ben\u00b7der", "Bu\u00b7sen", "!", "es", "kehrt", "wie\u00b7der", "die", "gl\u00fcck\u00b7li\u00b7che", "Zeit", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}}, "stanza.2": {"line.1": {"text": "Heiteres Jugendland mit deinen Meeren und Inseln,", "tokens": ["Hei\u00b7te\u00b7res", "Ju\u00b7gend\u00b7land", "mit", "dei\u00b7nen", "Mee\u00b7ren", "und", "In\u00b7seln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "+--+-+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Sey mir gegr\u00fc\u00dft! Nach dir sehnt sich das weinende Herz!", "tokens": ["Sey", "mir", "ge\u00b7gr\u00fc\u00dft", "!", "Nach", "dir", "sehnt", "sich", "das", "wei\u00b7nen\u00b7de", "Herz", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "$.", "APPR", "PPER", "VVFIN", "PRF", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.3": {"text": "Sch\u00f6nes hab' ich getr\u00e4umt, und mit liebendem Sinne gebildet,", "tokens": ["Sch\u00f6\u00b7n