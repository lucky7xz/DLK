{"textgrid.poem.67852": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "24. Liedchen der Sehnsucht", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der s\u00fcsse Schlaf, der sonst stillt alles wohl,", "tokens": ["Der", "s\u00fcs\u00b7se", "Schlaf", ",", "der", "sonst", "stillt", "al\u00b7les", "wohl", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ADV", "VVFIN", "PIS", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Kann stillen nicht mein Herz mit Trauren voll;", "tokens": ["Kann", "stil\u00b7len", "nicht", "mein", "Herz", "mit", "Trau\u00b7ren", "voll", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PTKNEG", "PPOSAT", "NN", "APPR", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das schafft allein, die mich erfreuen soll!", "tokens": ["Das", "schafft", "al\u00b7lein", ",", "die", "mich", "er\u00b7freu\u00b7en", "soll", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "$,", "PRELS", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Kein' Speis' und Trank mir Lust noch Nahrung geit,", "tokens": ["Kein'", "Speis'", "und", "Trank", "mir", "Lust", "noch", "Nah\u00b7rung", "geit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "NN", "PPER", "NN", "ADV", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Kein Kurzweil ist die mir mein Herz erfreut;", "tokens": ["Kein", "Kurz