{"dta.poem.20414": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Uber die erlangte Chur-w\u00fcrde seiner  \n Churf\u00fcrstl. Durchl. von Braunschweig  \n und L\u00fcneburg.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ohimmel! fieng unl\u00e4ngst das m\u00fcde Deuschland an/", "tokens": ["O\u00b7him\u00b7mel", "!", "fi\u00b7eng", "un\u00b7l\u00e4ngst", "das", "m\u00fc\u00b7de", "Deu\u00b7schland", "an", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "ADV", "ART", "ADJA", "NN", "PTKVZ", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Wie geht es doch nur zu? da\u00df meiner kinder degen/", "tokens": ["Wie", "geht", "es", "doch", "nur", "zu", "?", "da\u00df", "mei\u00b7ner", "kin\u00b7der", "de\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "ADV", "PTKVZ", "$.", "KOUS", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dem vor das stoltze Rom die pforten auffgethan/", "tokens": ["Dem", "vor", "das", "stolt\u00b7ze", "Rom", "die", "pfor\u00b7ten", "auff\u00b7ge\u00b7than", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "ADJA", "NE", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Itzt seine spitze mu\u00df mit schaden niederlegen?", "tokens": ["Itzt", "sei\u00b7ne", "spit\u00b7ze", "mu\u00df", "mit", "scha\u00b7den", "nie\u00b7der\u00b7le\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "VVFIN", "VMFIN", "APPR", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Da\u00df/ weil der k\u00e4yser nur von sieben ward erw\u00e4hlt/", "tokens": ["Da\u00df", "/", "weil", "der", "k\u00e4y\u00b7ser", "nur", "von", "sie\u00b7ben", "ward", "er\u00b7w\u00e4hlt", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$(", "KOUS", "ART", "ADJA", "ADV", "APPR", "CARD", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die st\u00e4tte sich gemehrt/ die grentzen zugenommen?", "tokens": ["Die", "st\u00e4t\u00b7te", "sich", "ge\u00b7mehrt", "/", "die", "grent\u00b7zen", "zu\u00b7ge\u00b7nom\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "VVPP", "$(", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Nachdem man aber auch den achten Churf\u00fcrst zehlt/", "tokens": ["Nach\u00b7dem", "man", "a\u00b7ber", "auch", "den", "ach\u00b7ten", "Chur\u00b7f\u00fcrst", "zehlt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "ADV", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "An Schweden Pommern ist/ an Franckreich Elsas kommen?", "tokens": ["An", "Schwe\u00b7den", "Pom\u00b7mern", "ist", "/", "an", "Fran\u00b7ck\u00b7reich", "El\u00b7sas", "kom\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NE", "VAFIN", "$(", "APPR", "NE", "NE", "VVINF", "$."], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Nimmt dann die tapfferkeit in meinen helden ab?", "tokens": ["Nimmt", "dann", "die", "tapf\u00b7fer\u00b7keit", "in", "mei\u00b7nen", "hel\u00b7den", "ab", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "War Friedrich Wilhelm nicht mehr/ weder ihrer sieben?", "tokens": ["War", "Fried\u00b7rich", "Wil\u00b7helm", "nicht", "mehr", "/", "we\u00b7der", "ih\u00b7rer", "sie\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "NE", "PTKNEG", "ADV", "$(", "KON", "PPOSAT", "CARD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und ist der grosse geist/ den ich dem vater gab/", "tokens": ["Und", "ist", "der", "gros\u00b7se", "geist", "/", "den", "ich", "dem", "va\u00b7ter", "gab", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "ADJA", "NN", "$(", "PRELS", "PPER", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Nicht in dem sohne noch der welt zur\u00fccke blieben?", "tokens": ["Nicht", "in", "dem", "soh\u00b7ne", "noch", "der", "welt", "zu\u00b7r\u00fc\u00b7cke", "blie\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ART", "NN", "ADV", "ART", "NN", "PTKVZ", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ach ja! di\u00df alles ist/ wie man gew\u00fcnscht/ geschehn:", "tokens": ["Ach", "ja", "!", "di\u00df", "al\u00b7les", "ist", "/", "wie", "man", "ge\u00b7w\u00fcnscht", "/", "ge\u00b7schehn", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ITJ", "ITJ", "$.", "PDS", "PIS", "VAFIN", "$(", "PWAV", "PIS", "VVPP", "$(", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Allein der himmel spielt offt hinter larv\u2019 und decke.", "tokens": ["Al\u00b7lein", "der", "him\u00b7mel", "spielt", "offt", "hin\u00b7ter", "la\u00b7rv'", "und", "de\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "ADV", "APPR", "NE", "KON", "VVFIN", "$."], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.15": {"text": "Wir haben dazumahl vor schmertzen nicht gesehn/", "tokens": ["Wir", "ha\u00b7ben", "da\u00b7zu\u00b7mahl", "vor", "schmert\u00b7zen", "nicht", "ge\u00b7sehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "VVFIN", "PTKNEG", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Das in der achten zahl nicht/ was in sieben/ stecke.", "tokens": ["Das", "in", "der", "ach\u00b7ten", "zahl", "nicht", "/", "was", "in", "sie\u00b7ben", "/", "ste\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "APPR", "ART", "ADJA", "NN", "PTKNEG", "$(", "PWS", "APPR", "CARD", "$(", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Doch was beklag ich mich? In Leopoldens macht", "tokens": ["Doch", "was", "be\u00b7klag", "ich", "mich", "?", "In", "Leo\u00b7pol\u00b7dens", "macht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PWS", "VVFIN", "PPER", "PRF", "$.", "APPR", "NE", "VVFIN"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Baut ihm Apollo selbst ein k\u00e4yserthum auff erden.", "tokens": ["Baut", "ihm", "A\u00b7pol\u00b7lo", "selbst", "ein", "k\u00e4y\u00b7ser\u00b7thum", "auff", "er\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NE", "ADV", "ART", "NN", "APPR", "NN", "$."], "meter": "+-++-+-+-+-+-", "measure": "unknown.measure.septa"}, "line.3": {"text": "Acht Musen hat er schon als F\u00fcrsten angebracht/", "tokens": ["Acht", "Mu\u00b7sen", "hat", "er", "schon", "als", "F\u00fcrs\u00b7ten", "an\u00b7ge\u00b7bracht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VAFIN", "PPER", "ADV", "KOUS", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Warum soll k\u00fcnfftig nun nicht auch der neundte werden?", "tokens": ["Wa\u00b7rum", "soll", "k\u00fcnff\u00b7tig", "nun", "nicht", "auch", "der", "neund\u00b7te", "wer\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "ADJD", "ADV", "PTKNEG", "ADV", "ART", "ADJA", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Auff Gwelfischer August! Ich wende mich zu dir.", "tokens": ["Auff", "Gwel\u00b7fi\u00b7scher", "Au\u00b7gust", "!", "Ich", "wen\u00b7de", "mich", "zu", "dir", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$.", "PPER", "VVFIN", "PRF", "APPR", "PPER", "$."], "meter": "-+-+++-+-+-+", "measure": "unknown.measure.septa"}, "line.6": {"text": "Du solst die letzte noch zu dieser w\u00fcrde schwingen.", "tokens": ["Du", "solst", "die", "letz\u00b7te", "noch", "zu", "die\u00b7ser", "w\u00fcr\u00b7de", "schwin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "ADV", "APPR", "PDS", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Denn glaube/ lieber sohn/ dein nahme sagt es mir/", "tokens": ["Denn", "glau\u00b7be", "/", "lie\u00b7ber", "sohn", "/", "dein", "nah\u00b7me", "sagt", "es", "mir", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$(", "ADV", "ADV", "$(", "PPOSAT", "NN", "VVFIN", "PPER", "PPER", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Uns Deutschen wirstu ruh/ aus Ungarn friede bringen.", "tokens": ["Uns", "Deut\u00b7schen", "wirs\u00b7tu", "ruh", "/", "aus", "Un\u00b7garn", "frie\u00b7de", "brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "VAFIN", "NN", "$(", "APPR", "NE", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wohl uns! erklang hierauff die Mosel und der Rhein/", "tokens": ["Wohl", "uns", "!", "er\u00b7klang", "hier\u00b7auff", "die", "Mo\u00b7sel", "und", "der", "Rhein", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "$.", "VVFIN", "PAV", "ART", "NN", "KON", "ART", "NE", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Nun wei\u00dfheit k\u00e4yser ist und Musen f\u00fcrsten seyn.", "tokens": ["Nun", "wei\u00df\u00b7heit", "k\u00e4y\u00b7ser", "ist", "und", "Mu\u00b7sen", "f\u00fcrs\u00b7ten", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "VAFIN", "KON", "NN", "VVFIN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}