{"dta.poem.3198": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "49.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Finke, der am Weg ein trocknes K\u00f6rnlein hascht,", "tokens": ["Der", "Fin\u00b7ke", ",", "der", "am", "Weg", "ein", "trock\u00b7nes", "K\u00f6rn\u00b7lein", "hascht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPRART", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hat Kirschen wohl im Lenz, Trauben im Herbst genascht.", "tokens": ["Hat", "Kir\u00b7schen", "wohl", "im", "Lenz", ",", "Trau\u00b7ben", "im", "Herbst", "ge\u00b7nascht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ADV", "APPRART", "NN", "$,", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-++--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Er nimmt es wie es kommt, bleibt frisch an Leib und Seele,", "tokens": ["Er", "nimmt", "es", "wie", "es", "kommt", ",", "bleibt", "frisch", "an", "Leib", "und", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PWAV", "PPER", "VVFIN", "$,", "VVFIN", "ADJD", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nur singt er nicht, und bla\u00df ward ihm das Roth der Kehle.", "tokens": ["Nur", "singt", "er", "nicht", ",", "und", "bla\u00df", "ward", "ihm", "das", "Roth", "der", "Keh\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "$,", "KON", "ADJD", "VAFIN", "PPER", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Einst singt er wieder, und sein blasser Hals wird roth,", "tokens": ["Einst", "singt", "er", "wie\u00b7der", ",", "und", "sein", "blas\u00b7ser", "Hals", "wird", "roth", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "KON", "PPOSAT", "ADJA", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wann wieder Kirsch' und Traub' ihm gibt sein t\u00e4glich Brod.", "tokens": ["Wann", "wie\u00b7der", "Kir\u00b7sch'", "und", "Traub'", "ihm", "gibt", "sein", "t\u00e4g\u00b7lich", "Brod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "NN", "KON", "NN", "PPER", "VVFIN", "PPOSAT", "ADJD", "NN", "$."], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "Auf, schwinge dich, mein Geist, aus diesen Kummerschranken,", "tokens": ["Auf", ",", "schwin\u00b7ge", "dich", ",", "mein", "Geist", ",", "aus", "die\u00b7sen", "Kum\u00b7mer\u00b7schran\u00b7ken", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "$,", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie mit den Fl\u00fcgeln er, mit muthigen Gedanken.", "tokens": ["Wie", "mit", "den", "Fl\u00fc\u00b7geln", "er", ",", "mit", "mut\u00b7hi\u00b7gen", "Ge\u00b7dan\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "PPER", "$,", "APPR", "ADJA", "NN", "$."], "meter": "+--+--+-+--+-", "measure": "dactylic.di.plus"}}}}}