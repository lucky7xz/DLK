{"textgrid.poem.59555": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "Aufruf an die Deutschen bei Schills Tode", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Deutsche, nicht mehr Deutsche,", "tokens": ["O", "Deut\u00b7sche", ",", "nicht", "mehr", "Deut\u00b7sche", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PTKNEG", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nicht M\u00e4nner, eitel Weiber!", "tokens": ["Nicht", "M\u00e4n\u00b7ner", ",", "ei\u00b7tel", "Wei\u00b7ber", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "$,", "ADJD", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Was kr\u00fcmmt ihr tief die Leiber", "tokens": ["Was", "kr\u00fcmmt", "ihr", "tief", "die", "Lei\u00b7ber"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "ADJD", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dem Schlag der Sklavenpeitsche?", "tokens": ["Dem", "Schlag", "der", "Skla\u00b7ven\u00b7peit\u00b7sche", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Was kriecht ihr wie die Hunde", "tokens": ["Was", "kriecht", "ihr", "wie", "die", "Hun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "KOKOM", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Vor Henkern und Banditen", "tokens": ["Vor", "Hen\u00b7kern", "und", "Ban\u00b7di\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und lernt die Worte h\u00fcten", "tokens": ["Und", "lernt", "die", "Wor\u00b7te", "h\u00fc\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Des Zorns vom freien Munde?", "tokens": ["Des", "Zorns", "vom", "frei\u00b7en", "Mun\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "O eure tapfern V\u00e4ter!", "tokens": ["O", "eu\u00b7re", "tap\u00b7fern", "V\u00e4\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "O eure gro\u00dfen Ahnen!", "tokens": ["O", "eu\u00b7re", "gro\u00b7\u00dfen", "Ah\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Helden, die Germanen!", "tokens": ["Die", "Hel\u00b7den", ",", "die", "Ger\u00b7ma\u00b7nen", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das waren k\u00fchne T\u00e4ter,", "tokens": ["Das", "wa\u00b7ren", "k\u00fch\u00b7ne", "T\u00e4\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Nicht sch\u00f6ner Worte Sprecher,", "tokens": ["Nicht", "sch\u00f6\u00b7ner", "Wor\u00b7te", "Spre\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Nein, stolzer Freiheit Kinder,", "tokens": ["Nein", ",", "stol\u00b7zer", "Frei\u00b7heit", "Kin\u00b7der", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Tyrannen\u00fcberwinder,", "tokens": ["Ty\u00b7ran\u00b7nen\u00b7\u00fc\u00b7berw\u00b7in\u00b7der", ","], "token_info": ["word", "punct"], "pos": ["NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Entnervter Tugend R\u00e4cher.", "tokens": ["Ent\u00b7nerv\u00b7ter", "Tu\u00b7gend", "R\u00e4\u00b7cher", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ihr aber, Sklavenhorden,", "tokens": ["Ihr", "a\u00b7ber", ",", "Skla\u00b7ven\u00b7hor\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PPER", "ADV", "$,", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie macht ihr Eisen blutig?", "tokens": ["Wie", "macht", "ihr", "Ei\u00b7sen", "blu\u00b7tig", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wie seid ihr k\u00fchn und mutig?", "tokens": ["Wie", "seid", "ihr", "k\u00fchn", "und", "mu\u00b7tig", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ach! nur euch selbst zu morden.", "tokens": ["Ach", "!", "nur", "euch", "selbst", "zu", "mor\u00b7den", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ADV", "PPER", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Feil steht ihr dem Tyrannen", "tokens": ["Feil", "steht", "ihr", "dem", "Ty\u00b7ran\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Zum Brudermorde fertig,", "tokens": ["Zum", "Bru\u00b7der\u00b7mor\u00b7de", "fer\u00b7tig", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Steht seines Winks gew\u00e4rtig,", "tokens": ["Steht", "sei\u00b7nes", "Winks", "ge\u00b7w\u00e4r\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Euch selber zu entmannen.", "tokens": ["Euch", "sel\u00b7ber", "zu", "ent\u00b7man\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "O Sonne, die noch scheinet!", "tokens": ["O", "Son\u00b7ne", ",", "die", "noch", "schei\u00b7net", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PRELS", "ADV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "O Himmel, der noch rollet!", "tokens": ["O", "Him\u00b7mel", ",", "der", "noch", "rol\u00b7let", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PRELS", "ADV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Versteht ihr, was ihr wollet?", "tokens": ["Ver\u00b7steht", "ihr", ",", "was", "ihr", "wol\u00b7let", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWS", "PPER", "VMFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Weint eurem Wahnsinn! Weinet!", "tokens": ["Weint", "eu\u00b7rem", "Wahn\u00b7sinn", "!", "Wei\u00b7net", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$.", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Verflucht den Trug, die Schande,", "tokens": ["Ver\u00b7flucht", "den", "Trug", ",", "die", "Schan\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Womit ihr euch zerrei\u00dfet,", "tokens": ["Wo\u00b7mit", "ihr", "euch", "zer\u00b7rei\u00b7\u00dfet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Womit ihr Knechte hei\u00dfet", "tokens": ["Wo\u00b7mit", "ihr", "Knech\u00b7te", "hei\u00b7\u00dfet"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "In freier V\u00e4ter Lande.", "tokens": ["In", "frei\u00b7er", "V\u00e4\u00b7ter", "Lan\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Zusammen! Risch zusammen!", "tokens": ["Zu\u00b7sam\u00b7men", "!", "Risch", "zu\u00b7sam\u00b7men", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$.", "NN", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Es will die Welt vergehen,", "tokens": ["Es", "will", "die", "Welt", "ver\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ihr seht sie schon verwehen", "tokens": ["Ihr", "seht", "sie", "schon", "ver\u00b7we\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In hellen, lichten Flammen.", "tokens": ["In", "hel\u00b7len", ",", "lich\u00b7ten", "Flam\u00b7men", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ihr habt den Brand gez\u00fcndet,", "tokens": ["Ihr", "habt", "den", "Brand", "ge\u00b7z\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ihr m\u00fc\u00dft mit Blut ihn d\u00e4mpfen", "tokens": ["Ihr", "m\u00fc\u00dft", "mit", "Blut", "ihn", "d\u00e4mp\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "NN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und mit den R\u00e4ubern k\u00e4mpfen,", "tokens": ["Und", "mit", "den", "R\u00e4u\u00b7bern", "k\u00e4mp\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Bis ihr die Freiheit findet.", "tokens": ["Bis", "ihr", "die", "Frei\u00b7heit", "fin\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Dann, auf get\u00fcrmten Leichen", "tokens": ["Dann", ",", "auf", "ge\u00b7t\u00fcrm\u00b7ten", "Lei\u00b7chen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Sch\u00e4nder schreitend, pfl\u00fccket", "tokens": ["Der", "Sch\u00e4n\u00b7der", "schrei\u00b7tend", ",", "pfl\u00fc\u00b7cket"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "ADJD", "$,", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Den Schmuck, der Freie schm\u00fccket,", "tokens": ["Den", "Schmuck", ",", "der", "Frei\u00b7e", "schm\u00fc\u00b7cket", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das Laub der deutschen Eichen;", "tokens": ["Das", "Laub", "der", "deut\u00b7schen", "Ei\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Dann schw\u00f6rt den Schwur der Treue", "tokens": ["Dann", "schw\u00f6rt", "den", "Schwur", "der", "Treu\u00b7e"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Dem lieben Vaterlande,", "tokens": ["Dem", "lie\u00b7ben", "Va\u00b7ter\u00b7lan\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Da\u00df nie Despotenschande", "tokens": ["Da\u00df", "nie", "Des\u00b7po\u00b7ten\u00b7schan\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "ADV", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die heilige Erd' entweihe.", "tokens": ["Die", "hei\u00b7li\u00b7ge", "Erd'", "ent\u00b7wei\u00b7he", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+--+-+-", "mea