{"dta.poem.13041": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Siebendes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201esagt! klunge nicht sehr oft die Zeitung in den Ohren:", "tokens": ["\u201e", "sagt", "!", "klun\u00b7ge", "nicht", "sehr", "oft", "die", "Zei\u00b7tung", "in", "den", "Oh\u00b7ren", ":"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$.", "VVFIN", "PTKNEG", "ADV", "ADV", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "\u201eder Feind hat wiederum, wer wei\u00df es, was, verlohren?", "tokens": ["\u201e", "der", "Feind", "hat", "wie\u00b7de\u00b7rum", ",", "wer", "wei\u00df", "es", ",", "was", ",", "ver\u00b7loh\u00b7ren", "?"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["$(", "ART", "NN", "VAFIN", "ADV", "$,", "PWS", "VVFIN", "PPER", "$,", "PRELS", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "285\u201dWer hat in solchem Fall ihn dann davon gejagt?", "tokens": ["\"", "Wer", "hat", "in", "sol\u00b7chem", "Fall", "ihn", "dann", "da\u00b7von", "ge\u00b7jagt", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VAFIN", "APPR", "PIAT", "NN", "PPER", "ADV", "PAV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u201evon wem war er besiegt? wer hat den Kampf gewagt?", "tokens": ["\u201e", "von", "wem", "war", "er", "be\u00b7siegt", "?", "wer", "hat", "den", "Kampf", "ge\u00b7wagt", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PWS", "VAFIN", "PPER", "VVPP", "$.", "PWS", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "\u201eich habe manches Mahl das Heer in Lorber stehen,", "tokens": ["\u201e", "ich", "ha\u00b7be", "man\u00b7ches", "Mahl", "das", "Heer", "in", "Lor\u00b7ber", "ste\u00b7hen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "PIAT", "NN", "ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "\u201ehingegen unsern Feind zerstreut entfliehn gesehen.", "tokens": ["\u201e", "hin\u00b7ge\u00b7gen", "un\u00b7sern", "Feind", "zer\u00b7streut", "ent\u00b7fliehn", "ge\u00b7se\u00b7hen", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "PPOSAT", "NN", "VVPP", "VVINF", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "\u201eh\u00e4tt ich nur Lust und Zeit und mehr Ged\u00e4chtni\u00df-Kraft,", "tokens": ["\u201e", "h\u00e4tt", "ich", "nur", "Lust", "und", "Zeit", "und", "mehr", "Ge\u00b7d\u00e4cht\u00b7ni\u00df\u00b7Kraft", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "PPER", "ADV", "NN", "KON", "NN", "KON", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "290\u201dSo g\u00e4b' ich euch davon genaue Rechenschaft.", "tokens": ["\"", "So", "g\u00e4b'", "ich", "euch", "da\u00b7von", "ge\u00b7nau\u00b7e", "Re\u00b7chen\u00b7schaft", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "PRF", "PAV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "\u201eich wollte Mann vor Mann von hundert tausend Helden,", "tokens": ["\u201e", "ich", "woll\u00b7te", "Mann", "vor", "Mann", "von", "hun\u00b7dert", "tau\u00b7send", "Hel\u00b7den", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VMFIN", "NN", "APPR", "NN", "APPR", "CARD", "CARD", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "\u201eund eines jeden Pflicht, Amt, Thun und Lassen melden;", "tokens": ["\u201e", "und", "ei\u00b7nes", "je\u00b7den", "Pflicht", ",", "Amt", ",", "Thun", "und", "Las\u00b7sen", "mel\u00b7den", ";"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "ART", "PIAT", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "\u201ehieraus erkenntet ihr, wer unsre Feinde schlug,", "tokens": ["\u201e", "hier\u00b7aus", "er\u00b7kenn\u00b7tet", "ihr", ",", "wer", "uns\u00b7re", "Fein\u00b7de", "schlug", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PAV", "VVFIN", "PPER", "$,", "PWS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "\u201ewer bey dem Haupt-Triumpf die Sieges-Fahnen trug.", "tokens": ["\u201e", "wer", "bey", "dem", "Haup\u00b7tTri\u00b7umpf", "die", "Sie\u00b7ges\u00b7Fah\u00b7nen", "trug", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "295\u201dBetrachtet jenes Volck, so wir Soldaten nennen;", "tokens": ["\"", "Be\u00b7trach\u00b7tet", "je\u00b7nes", "Volck", ",", "so", "wir", "Sol\u00b7da\u00b7ten", "nen\u00b7nen", ";"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PDAT", "NN", "$,", "ADV", "PPER", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "\u201edem wir des Vaterlands Besch\u00fczung zuerkennen!", "tokens": ["\u201e", "dem", "wir", "des", "Va\u00b7ter\u00b7lands", "Be\u00b7sch\u00fc\u00b7zung", "zu\u00b7er\u00b7ken\u00b7nen", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PRELS", "PPER", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "\u201eden Schwarm, der aus dem Stahl der schwersten Helmen blizt,", "tokens": ["\u201e", "den", "Schwarm", ",", "der", "aus", "dem", "Stahl", "der", "schwers\u00b7ten", "Hel\u00b7men", "blizt", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "\u201eund Mann und Pferd und Feld troz einer Mauer sch\u00fczt;", "tokens": ["\u201e", "und", "Mann", "und", "Pferd", "und", "Feld", "troz", "ei\u00b7ner", "Mau\u00b7er", "sch\u00fczt", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "NN", "KON", "NN", "KON", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "\u201edas Ansehn und das Herz der tapfersten Schwadronen;", "tokens": ["\u201e", "das", "An\u00b7sehn", "und", "das", "Herz", "der", "tap\u00b7fers\u00b7ten", "Schwad\u00b7ro\u00b7nen", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "KON", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+---+-++--+-", "measure": "iambic.penta.relaxed"}, "line.18": {"text": "300\u201dDen k\u00fchnen Helden-Blick der edelsten Personen;", "tokens": ["\"", "Den", "k\u00fch\u00b7nen", "Hel\u00b7den\u00b7Blick", "der", "e\u00b7dels\u00b7ten", "Per\u00b7so\u00b7nen", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+--", "measure": "iambic.penta.relaxed"}, "line.19": {"text": "\u201edas schimmernde Gewehr, so stets zum K\u00e4mpfen gl\u00fcht;", "tokens": ["\u201e", "das", "schim\u00b7mern\u00b7de", "Ge\u00b7wehr", ",", "so", "stets", "zum", "K\u00e4mp\u00b7fen", "gl\u00fcht", ";"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADJA", "NN", "$,", "ADV", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "\u201eden Haufen, welcher nicht als vor sich selber flieht;", "tokens": ["\u201e", "den", "Hau\u00b7fen", ",", "wel\u00b7cher", "nicht", "als", "vor", "sich", "sel\u00b7ber", "flieht", ";"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "$,", "PRELS", "PTKNEG", "KOKOM", "APPR", "PRF", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "\u201edie Schwerter; das Gesch\u00fcz; die scharff-gekr\u00fcm\u0303ten Klingen;", "tokens": ["\u201e", "die", "Schwer\u00b7ter", ";", "das", "Ge\u00b7sch\u00fcz", ";", "die", "scha\u00b7rff\u00b7ge\u00b7kr\u00fcm\u0303ten", "Klin\u00b7gen", ";"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "$.", "ART", "NN", "$.", "ART", "ADJA", "NN", "$."], "meter": "-+---+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.22": {"text": "\u201edie Taschen, welche Bliz und Feur und Donner bringen.", "tokens": ["\u201e", "die", "Ta\u00b7schen", ",", "wel\u00b7che", "Bliz", "und", "Feur", "und", "Don\u00b7ner", "brin\u00b7gen", "."], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "$,", "PWAT", "NN", "KON", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "305\u201dSeht jenen, dessen Brust dem Feind entgegen r\u00fcckt,", "tokens": ["\"", "Seht", "je\u00b7nen", ",", "des\u00b7sen", "Brust", "dem", "Feind", "ent\u00b7ge\u00b7gen", "r\u00fcckt", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PDS", "$,", "PRELAT", "NN", "ART", "NN", "PTKVZ", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "\u201ezu Pferd, auch oft zu Fu\u00df den frechen S\u00e4bel z\u00fcckt;", "tokens": ["\u201e", "zu", "Pferd", ",", "auch", "oft", "zu", "Fu\u00df", "den", "fre\u00b7chen", "S\u00e4\u00b7bel", "z\u00fcckt", ";"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "NN", "$,", "ADV", "ADV", "APPR", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "\u201eschaut jene, welche nur im Feur den Eifer k\u00fchlen,", "tokens": ["\u201e", "schaut", "je\u00b7ne", ",", "wel\u00b7che", "nur", "im", "Feur", "den", "Ei\u00b7fer", "k\u00fch\u00b7len", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PDS", "$,", "PRELS", "ADV", "APPRART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "\u201evon welchem sie das Herz zum Kampf ermuntert f\u00fchlen.", "tokens": ["\u201e", "von", "wel\u00b7chem", "sie", "das", "Herz", "zum", "Kampf", "er\u00b7mun\u00b7tert", "f\u00fch\u00b7len", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PRELS", "PPER", "ART", "NN", "APPRART", "NN", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "\u201ewas ist die Tapferkeit, was ist der Angriff werth,", "tokens": ["\u201e", "was", "ist", "die", "Tap\u00b7fer\u00b7keit", ",", "was", "ist", "der", "An\u00b7griff", "werth", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VAFIN", "ART", "NN", "$,", "PWS", "VAFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "310\u201dDen oft der Feinde Stolz von jenem Volck erf\u00e4hrt,", "tokens": ["\"", "Den", "oft", "der", "Fein\u00b7de", "Stolz", "von", "je\u00b7nem", "Volck", "er\u00b7f\u00e4hrt", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADV", "ART", "NN", "NN", "APPR", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "\u201edas, Wetter-Strahlen gleich, sich und den S\u00e4bel wendet,", "tokens": ["\u201e", "das", ",", "Wet\u00b7ter\u00b7Strah\u00b7len", "gleich", ",", "sich", "und", "den", "S\u00e4\u00b7bel", "wen\u00b7det", ","], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PDS", "$,", "NN", "ADV", "$,", "PRF", "KON", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "\u201eder K\u00f6niginn zum Schuz sein Gut und Bluth verschwendet?", "tokens": ["\u201e", "der", "K\u00f6\u00b7ni\u00b7ginn", "zum", "Schuz", "sein", "Gut", "und", "Bluth", "ver\u00b7schwen\u00b7det", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "APPRART", "NN", "PPOSAT", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "\u201eerweget! ist ein Volck so feurig und so starck,", "tokens": ["\u201e", "er\u00b7we\u00b7get", "!", "ist", "ein", "Volck", "so", "feu\u00b7rig", "und", "so", "starck", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$.", "VAFIN", "ART", "NN", "ADV", "ADJD", "KON", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "\u201eals jenes, dem die Treu und Liebe nichts als Marck", "tokens": ["\u201e", "als", "je\u00b7nes", ",", "dem", "die", "Treu", "und", "Lie\u00b7be", "nichts", "als", "Marck"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "KOUS", "PDS", "$,", "PRELS", "ART", "NN", "KON", "NN", "PIS", "KOKOM", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "315\u201dDer Unerschrockenheit in Herz und Adern fl\u00f6sset,", "tokens": ["\"", "Der", "Un\u00b7er\u00b7schro\u00b7cken\u00b7heit", "in", "Herz", "und", "A\u00b7dern", "fl\u00f6s\u00b7set", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "\u201edas nur in Mord und Brand die Helden-Brust entbl\u00f6sset?", "tokens": ["\u201e", "das", "nur", "in", "Mord", "und", "Brand", "die", "Hel\u00b7den\u00b7Brust", "ent\u00b7bl\u00f6s\u00b7set", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PDS", "ADV", "APPR", "NN", "KON", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "\u201edie Schaar, die wie ein Pfeil auf schnellen Pferden jagt,", "tokens": ["\u201e", "die", "Schaar", ",", "die", "wie", "ein", "Pfeil", "auf", "schnel\u00b7len", "Pfer\u00b7den", "jagt", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "$,", "PRELS", "KOKOM", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "\u201enach keinem Mord-Metall, nicht nach Gefahren fragt;", "tokens": ["\u201e", "nach", "kei\u00b7nem", "Mord\u00b7Me\u00b7tall", ",", "nicht", "nach", "Ge\u00b7fah\u00b7ren", "fragt", ";"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PIAT", "NN", "$,", "PTKNEG", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "\u201ezum Anfall pl\u00f6zlich ist, auch wie der Bliz entfliehet,", "tokens": ["\u201e", "zum", "An\u00b7fall", "pl\u00f6z\u00b7lich", "ist", ",", "auch", "wie", "der", "Bliz", "ent\u00b7flie\u00b7het", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPRART", "NN", "ADJD", "VAFIN", "$,", "ADV", "KOKOM", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "320\u201dUnd dannoch in der Flucht den Sieg dem Feind entziehet.", "tokens": ["\"", "Und", "dan\u00b7noch", "in", "der", "Flucht", "den", "Sieg", "dem", "Feind", "ent\u00b7zie\u00b7het", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "ADV", "APPR", "ART", "NN", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}