{"textgrid.poem.44010": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Welt, was hab ich noch mit dir", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Welt, was hab ich noch mit dir", "tokens": ["Welt", ",", "was", "hab", "ich", "noch", "mit", "dir"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PWS", "VAFIN", "PPER", "ADV", "APPR", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und mit deiner Gunst zu schafen?", "tokens": ["Und", "mit", "dei\u00b7ner", "Gunst", "zu", "scha\u00b7fen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Adams s\u00fcndliche Begier", "tokens": ["A\u00b7dams", "s\u00fcnd\u00b7li\u00b7che", "Be\u00b7gier"], "token_info": ["word", "word", "word"], "pos": ["NE", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mag sich an der Lust vergafen,", "tokens": ["Mag", "sich", "an", "der", "Lust", "ver\u00b7ga\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Die in Sodoms H\u00e4usern spielt", "tokens": ["Die", "in", "So\u00b7doms", "H\u00e4u\u00b7sern", "spielt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "NE", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Und auf Tod und Schande zielt.", "tokens": ["Und", "auf", "Tod", "und", "Schan\u00b7de", "zielt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.2": {"line.1": {"text": "Deines Hochmuths Brandaltar", "tokens": ["Dei\u00b7nes", "Hoch\u00b7muths", "Bran\u00b7dal\u00b7tar"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Gl\u00e4nzt von lauter Zauberkerzen,", "tokens": ["Gl\u00e4nzt", "von", "lau\u00b7ter", "Zau\u00b7ber\u00b7ker\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo die Thoren mit Gefahr", "tokens": ["Wo", "die", "Tho\u00b7ren", "mit", "Ge\u00b7fahr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "In dem Ehrsuchtstempel scherzen,", "tokens": ["In", "dem", "Ehr\u00b7suchts\u00b7tem\u00b7pel", "scher\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.5": {"text": "Bis der Fall den Schein entdeckt", "tokens": ["Bis", "der", "Fall", "den", "Schein", "ent\u00b7deckt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Und sie in den Abgrund steckt.", "tokens": ["Und", "sie", "in", "den", "Ab\u00b7grund", "steckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.3": {"line.1": {"text": "Mammons g\u00fcldner \u00dcberflu\u00df", "tokens": ["Mam\u00b7mons", "g\u00fcld\u00b7ner", "\u00dc\u00b7berf\u00b7lu\u00df"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "\u00dcbert\u00fcncht die schwersten Sorgen;", "tokens": ["\u00dc\u00b7ber\u00b7t\u00fcncht", "die", "schwers\u00b7ten", "Sor\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wer in Marmor wohnen mu\u00df,", "tokens": ["Wer", "in", "Mar\u00b7mor", "woh\u00b7nen", "mu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "NN", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Der hat keinen guten Morgen.", "tokens": ["Der", "hat", "kei\u00b7nen", "gu\u00b7ten", "Mor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PIAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "In Pall\u00e4sten reicher Zeit", "tokens": ["In", "Pal\u00b7l\u00e4s\u00b7ten", "rei\u00b7cher", "Zeit"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "ADJD", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Herrschen Furcht und Gram und Neid.", "tokens": ["Herr\u00b7schen", "Furcht", "und", "Gram", "und", "Neid", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Herr der Warheit, auf dein Wort", "tokens": ["Herr", "der", "War\u00b7heit", ",", "auf", "dein", "Wort"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ART", "NN", "$,", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Gr\u00fcndet sich mein froh Gewi\u00dfen.", "tokens": ["Gr\u00fcn\u00b7det", "sich", "mein", "froh", "Ge\u00b7wi\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PPOSAT", "ADJA", "NN", "$."], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.3": {"text": "O wie seelig wohnt man dort,", "tokens": ["O", "wie", "see\u00b7lig", "wohnt", "man", "dort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "ADJD", "VVFIN", "PIS", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo wir dein Ged\u00e4chtn\u00fc\u00df k\u00fc\u00dfen,", "tokens": ["Wo", "wir", "dein", "Ge\u00b7d\u00e4cht\u00b7n\u00fc\u00df", "k\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wo dein Nahme, Recht und Licht", "tokens": ["Wo", "dein", "Nah\u00b7me", ",", "Recht", "und", "Licht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "PPOSAT", "NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Allzeit von Erl\u00f6sung spricht.", "tokens": ["All\u00b7zeit", "von", "Er\u00b7l\u00f6\u00b7sung", "spricht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Ach, was giebt mir Zions H\u00f6h", "tokens": ["Ach", ",", "was", "giebt", "mir", "Zi\u00b7ons", "H\u00f6h"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PWS", "VVFIN", "PPER", "NE", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Vor ein sehnsuchtsvoll Erg\u00f6zen,", "tokens": ["Vor", "ein", "sehn\u00b7suchts\u00b7voll", "Er\u00b7g\u00f6\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn ich in den Vorhof geh,", "tokens": ["Wenn", "ich", "in", "den", "Vor\u00b7hof", "geh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Meinen Glauben fest zu sezen,", "tokens": ["Mei\u00b7nen", "Glau\u00b7ben", "fest", "zu", "se\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Der die Hofnung dorthin f\u00fchrt,", "tokens": ["Der", "die", "Hof\u00b7nung", "dor\u00b7thin", "f\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wo die Kirche triumphiert.", "tokens": ["Wo", "die", "Kir\u00b7che", "tri\u00b7um\u00b7phiert", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Auf, mein Geist, und schau empor,", "tokens": ["Auf", ",", "mein", "Geist", ",", "und", "schau", "em\u00b7por", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "PPOSAT", "NN", "$,", "KON", "ADJD", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Was sich dort vor Wollust findet,", "tokens": ["Was", "sich", "dort", "vor", "Wol\u00b7lust", "fin\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Welche hier kein Aug und Ohr", "tokens": ["Wel\u00b7che", "hier", "kein", "Aug", "und", "Ohr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "PIAT", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Noch kein menschlich Herz ergr\u00fcndet:", "tokens": ["Noch", "kein", "menschlich", "Herz", "er\u00b7gr\u00fcn\u00b7det", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "ADJD", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Dies ist Salems Friedensstadt,", "tokens": ["Dies", "ist", "Sa\u00b7lems", "Frie\u00b7dens\u00b7stadt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "NE", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Die den Quell des Lebens hat.", "tokens": ["Die", "den", "Quell", "des", "Le\u00b7bens", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ART", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Diese St\u00e4tte suchen wir,", "tokens": ["Die\u00b7se", "St\u00e4t\u00b7te", "su\u00b7chen", "wir", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wir als Pilger auf der Erden.", "tokens": ["Wir", "als", "Pil\u00b7ger", "auf", "der", "Er\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KOUS", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sollte nun der Creuzweg dir", "tokens": ["Soll\u00b7te", "nun", "der", "Creuz\u00b7weg", "dir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "ART", "NN", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Etwas rauh und sauer werden,", "tokens": ["Et\u00b7was", "rauh", "und", "sau\u00b7er", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "VAINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "O so st\u00e4rcke Fu\u00df und Muth", "tokens": ["O", "so", "st\u00e4r\u00b7cke", "Fu\u00df", "und", "Muth"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "VVFIN", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Durch den Blick auf jenes Gut!", "tokens": ["Durch", "den", "Blick", "auf", "je\u00b7nes", "Gut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "PDAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Aller Kummer leget sich,", "tokens": ["Al\u00b7ler", "Kum\u00b7mer", "le\u00b7get", "sich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PRF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn sich nur dein Herz besinnet,", "tokens": ["Wenn", "sich", "nur", "dein", "Herz", "be\u00b7sin\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df der Heiland auch vor dich", "tokens": ["Da\u00df", "der", "Hei\u00b7land", "auch", "vor", "dich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADV", "APPR", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Dort das B\u00fcrgerrecht gewinnet.", "tokens": ["Dort", "das", "B\u00fcr\u00b7ger\u00b7recht", "ge\u00b7win\u00b7net", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Eile nun durch W\u00fcst und Sand", "tokens": ["Ei\u00b7le", "nun", "durch", "W\u00fcst", "und", "Sand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Freudig in dein Vaterland!", "tokens": ["Freu\u00b7dig", "in", "dein", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Welt, was hab ich noch mit dir", "tokens": ["Welt", ",", "was", "hab", "ich", "noch", "mit", "dir"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PWS", "VAFIN", "PPER", "ADV", "APPR", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und mit deiner Gunst zu schafen?", "tokens": ["Und", "mit", "dei\u00b7ner", "Gunst", "zu", "scha\u00b7fen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Adams s\u00fcndliche Begier", "tokens": ["A\u00b7dams", "s\u00fcnd\u00b7li\u00b7che", "Be\u00b7gier"], "token_info": ["word", "word", "word"], "pos": ["NE", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mag sich an der Lust vergafen,", "tokens": ["Mag", "sich", "an", "der", "Lust", "ver\u00b7ga\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Die in Sodoms H\u00e4usern spielt", "tokens": ["Die", "in", "So\u00b7doms", "H\u00e4u\