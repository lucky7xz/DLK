land", "auch", "vor", "dich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADV", "APPR", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Dort das B\u00fcrgerrecht gewinnet.", "tokens": ["Dort", "das", "B\u00fcr\u00b7ger\u00b7recht", "ge\u00b7win\u00b7net", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Eile nun durch W\u00fcst und Sand", "tokens": ["Ei\u00b7le", "nun", "durch", "W\u00fcst", "und", "Sand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Freudig in dein Vaterland!", "tokens": ["Freu\u00b7dig", "in", "dein", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}