{"dta.poem.1257": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Monde/ du F\u00fcrste der blinckenden Sternen/", "tokens": ["Mon\u00b7de", "/", "du", "F\u00fcrs\u00b7te", "der", "blin\u00b7cken\u00b7den", "Ster\u00b7nen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "PPER", "NN", "ART", "ADJA", "NN", "$("], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Welcher mein Sehnen und Thr\u00e4nen beschaut/", "tokens": ["Wel\u00b7cher", "mein", "Seh\u00b7nen", "und", "Thr\u00e4\u00b7nen", "be\u00b7schaut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "PPOSAT", "NN", "KON", "NN", "VVPP", "$("], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Gl\u00e4ntzende Paphie/ der ich von fernen", "tokens": ["Gl\u00e4nt\u00b7zen\u00b7de", "Pa\u00b7phie", "/", "der", "ich", "von", "fer\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$(", "PRELS", "PPER", "APPR", "ADJA"], "meter": "+---+-+-+-", "measure": "dactylic.init"}, "line.4": {"text": "Meine betr\u00fcbte Gedancken vertraut/", "tokens": ["Mei\u00b7ne", "be\u00b7tr\u00fcb\u00b7te", "Ge\u00b7dan\u00b7cken", "ver\u00b7traut", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADJD", "$("], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.5": {"text": "Ziehe dein strahlendes Silber nur ein/", "tokens": ["Zie\u00b7he", "dein", "strah\u00b7len\u00b7des", "Sil\u00b7ber", "nur", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "ADV", "ART", "$("], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.6": {"text": "Schw\u00e4rtze mit Wolcken den spielenden Schein.", "tokens": ["Schw\u00e4rt\u00b7ze", "mit", "Wol\u00b7cken", "den", "spie\u00b7len\u00b7den", "Schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.2": {"line.1": {"text": "Himmel/ f\u00fcr dem ich mein Leiden nicht h\u00e4le/", "tokens": ["Him\u00b7mel", "/", "f\u00fcr", "dem", "ich", "mein", "Lei\u00b7den", "nicht", "h\u00e4\u00b7le", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "APPR", "PRELS", "PPER", "PPOSAT", "NN", "PTKNEG", "VVFIN", "$("], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "L\u00fcffte/ mit Seuffzen und Klagen erf\u00fcllt/", "tokens": ["L\u00fcff\u00b7te", "/", "mit", "Seuff\u00b7zen", "und", "Kla\u00b7gen", "er\u00b7f\u00fcllt", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "APPR", "NN", "KON", "NN", "VVPP", "$("], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Erde/ bey der ich mit Weinen erz\u00e4hle/", "tokens": ["Er\u00b7de", "/", "bey", "der", "ich", "mit", "Wei\u00b7nen", "er\u00b7z\u00e4h\u00b7le", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "APPR", "PRELS", "PPER", "APPR", "NN", "VVFIN", "$("], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Wie mir in St\u00fccke mein Hertze zuspillt/", "tokens": ["Wie", "mir", "in", "St\u00fc\u00b7cke", "mein", "Hert\u00b7ze", "zus\u00b7pillt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "NN", "PPOSAT", "VVFIN", "VVFIN", "$("], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "F\u00fchret mein Aechzen in einsame Klufft/", "tokens": ["F\u00fch\u00b7ret", "mein", "A\u00b7ech\u00b7zen", "in", "ein\u00b7sa\u00b7me", "Klufft", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$("], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.6": {"text": "Berget mein Lechzen in finsterer Grufft.", "tokens": ["Ber\u00b7get", "mein", "Lech\u00b7zen", "in", "fins\u00b7te\u00b7rer", "Grufft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.3": {"line.1": {"text": "Zeugin der st\u00fcndlich empfindenden Schmertzen/", "tokens": ["Zeu\u00b7gin", "der", "st\u00fcnd\u00b7lich", "emp\u00b7fin\u00b7den\u00b7den", "Schmert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJD", "ADJA", "NN", "$("], "meter": "---+--+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Tunckele Finsternis/ traurige Nacht/", "tokens": ["Tun\u00b7cke\u00b7le", "Fins\u00b7ter\u00b7nis", "/", "trau\u00b7ri\u00b7ge", "Nacht", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$(", "ADJA", "NN", "$("], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Welche mein thr\u00e4nendes Auge den Kertzen", "tokens": ["Wel\u00b7che", "mein", "thr\u00e4\u00b7nen\u00b7des", "Au\u00b7ge", "den", "Kert\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAT", "PPOSAT", "ADJA", "NN", "ART", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "Himmlischer Lichter zur Wette durchwacht/", "tokens": ["Himm\u00b7li\u00b7scher", "Lich\u00b7ter", "zur", "Wet