{"dta.poem.3575": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Bei der Schusterrechnung zu singen .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519172", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sechsmal hab ich sie angetroffen,               ", "tokens": ["Sechs\u00b7mal", "hab", "ich", "sie", "an\u00b7ge\u00b7trof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "VVPP", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Siebenmal bin ich fehl geloffen,", "tokens": ["Sie\u00b7ben\u00b7mal", "bin", "ich", "fehl", "ge\u00b7lof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Auf der Haide hin und her!", "tokens": ["Auf", "der", "Hai\u00b7de", "hin", "und", "her", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NE", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "\u201enein mein Bue, es geschieht nicht mehr.\u201c", "tokens": ["\u201e", "nein", "mein", "Bue", ",", "es", "ge\u00b7schieht", "nicht", "mehr", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PTKANT", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "PTKNEG", "ADV", "$.", "$("], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.5": {"text": "Sechs paar Schuh und sieben paar Sohlen", "tokens": ["Sechs", "paar", "Schuh", "und", "sie\u00b7ben", "paar", "Soh\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["CARD", "PIAT", "NN", "KON", "CARD", "PIAT", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Hab' ich von wegen meiner Sennerin verloffen,", "tokens": ["Hab'", "ich", "von", "we\u00b7gen", "mei\u00b7ner", "Sen\u00b7ne\u00b7rin", "ver\u00b7lof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Auf der Haide hin und her!", "tokens": ["Auf", "der", "Hai\u00b7de", "hin", "und", "her", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NE", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "\u201enein mein Bue, es geschieht nicht mehr!\u201c", "tokens": ["\u201e", "nein", "mein", "Bue", ",", "es", "ge\u00b7schieht", "nicht", "mehr", "!", "\u201c"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PTKANT", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "PTKNEG", "ADV", "$.", "$("], "meter": "+-+--+-+", "measure": "glykoneus"}}}}}