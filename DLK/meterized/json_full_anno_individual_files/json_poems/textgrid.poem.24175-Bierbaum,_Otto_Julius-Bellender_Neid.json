{"textgrid.poem.24175": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Bellender Neid", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie es dir schlecht ging, lie\u00dfen sie dich gelten,", "tokens": ["Wie", "es", "dir", "schlecht", "ging", ",", "lie\u00b7\u00dfen", "sie", "dich", "gel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "ADJD", "VVFIN", "$,", "VVFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du warst talentvoll und ein Biedermann;", "tokens": ["Du", "warst", "ta\u00b7lent\u00b7voll", "und", "ein", "Bie\u00b7der\u00b7mann", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "KON", "ART", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Da f\u00fchrte dich das Gl\u00fcck nur einen Schritt voran,", "tokens": ["Da", "f\u00fchr\u00b7te", "dich", "das", "Gl\u00fcck", "nur", "ei\u00b7nen", "Schritt", "vo\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und schon hubs an, das dumpf verhaltne Schelten.", "tokens": ["Und", "schon", "hubs", "an", ",", "das", "dumpf", "ver\u00b7halt\u00b7ne", "Schel\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "NE", "PTKVZ", "$,", "PRELS", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Nun aber, da sich ganz die Himmel dir erhellten", "tokens": ["Nun", "a\u00b7ber", ",", "da", "sich", "ganz", "die", "Him\u00b7mel", "dir", "er\u00b7hell\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "$,", "KOUS", "PRF", "ADV", "ART", "NN", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und dir das Gl\u00fcck ", "tokens": ["Und", "dir", "das", "Gl\u00fcck"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPER", "ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Jetzt fangen heulend sie zu bellen an,", "tokens": ["Jetzt", "fan\u00b7gen", "heu\u00b7lend", "sie", "zu", "bel\u00b7len", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "VVPP", "PPER", "PTKZU", "VVINF", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wie hungerstoll nur je im Walde W\u00f6lfe bellten:", "tokens": ["Wie", "hun\u00b7ger\u00b7stoll", "nur", "je", "im", "Wal\u00b7de", "W\u00f6l\u00b7fe", "bell\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ADV", "ADV", "APPRART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Du bist ein St\u00fcmper und ein Schuft, pack ein!", "tokens": ["Du", "bist", "ein", "St\u00fcm\u00b7per", "und", "ein", "Schuft", ",", "pack", "ein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "KON", "ART", "NN", "$,", "NE", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Talentlos, ehrlos, schamlos ohnegleichen;", "tokens": ["Ta\u00b7lent\u00b7los", ",", "ehr\u00b7los", ",", "scham\u00b7los", "oh\u00b7ne\u00b7glei\u