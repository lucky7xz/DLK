{"textgrid.poem.59412": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Nachtgebet", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es rauschte leise in den B\u00e4umen,", "tokens": ["Es", "rauschte", "lei\u00b7se", "in", "den", "B\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ich h\u00f6rte nur der Str\u00f6me Lauf,", "tokens": ["Ich", "h\u00f6r\u00b7te", "nur", "der", "Str\u00f6\u00b7me", "Lauf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und Berg und Gr\u00fcnde, wie aus Tr\u00e4umen,", "tokens": ["Und", "Berg", "und", "Gr\u00fcn\u00b7de", ",", "wie", "aus", "Tr\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "$,", "PWAV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sie sahn so fremd zu mir herauf.", "tokens": ["Sie", "sahn", "so", "fremd", "zu", "mir", "her\u00b7auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Drin aber in der stillen Halle", "tokens": ["Drin", "a\u00b7ber", "in", "der", "stil\u00b7len", "Hal\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ruht' Sang und Plaudern m\u00fcde aus,", "tokens": ["Ruht'", "Sang", "und", "Plau\u00b7dern", "m\u00fc\u00b7de", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es schliefen meine Lieben alle,", "tokens": ["Es", "schlie\u00b7fen", "mei\u00b7ne", "Lie\u00b7ben", "al\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "PIS", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Kaum wieder kannt ich nun mein Haus.", "tokens": ["Kaum", "wie\u00b7der", "kannt", "ich", "nun", "mein", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Mir war's, als l\u00e4gen sie zur Stunde", "tokens": ["Mir", "wa\u00b7r's", ",", "als", "l\u00e4\u00b7gen", "sie", "zur", "Stun\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "KOUS", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Gestorben, bleich im Mondenschein,", "tokens": ["Ge\u00b7stor\u00b7ben", ",", "bleich", "im", "Mon\u00b7den\u00b7schein", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und schauernd in der weiten Runde", "tokens": ["Und", "schau\u00b7ernd", "in", "der", "wei\u00b7ten", "Run\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fchlt ich auf einmal mich allein.", "tokens": ["F\u00fchlt", "ich", "auf", "ein\u00b7mal", "mich", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADV", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "So blickt in Meeres \u00f6den Reichen", "tokens": ["So", "blickt", "in", "Mee\u00b7res", "\u00f6\u00b7den", "Rei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Schiffer einsam himmelan \u2013", "tokens": ["Ein", "Schif\u00b7fer", "ein\u00b7sam", "him\u00b7me\u00b7lan", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ADV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "O Herr, wenn einst die Ufer weichen,", "tokens": ["O", "Herr", ",", "wenn", "einst", "die", "U\u00b7fer", "wei\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "KOUS", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sei gn\u00e4dig du dem Steuermann!", "tokens": ["Sei", "gn\u00e4\u00b7dig", "du", "dem", "Steu\u00b7er\u00b7mann", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Es rauschte leise in den B\u00e4umen,", "tokens": ["Es", "rauschte", "lei\u00b7se", "in", "den", "B\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ich h\u00f6rte nur der Str\u00f6me Lauf,", "tokens": ["