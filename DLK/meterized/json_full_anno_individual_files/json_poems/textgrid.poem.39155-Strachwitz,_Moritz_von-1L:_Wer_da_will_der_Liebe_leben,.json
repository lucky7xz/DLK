{"textgrid.poem.39155": {"metadata": {"author": {"name": "Strachwitz, Moritz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wer da will der Liebe leben,", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer da will der Liebe leben,", "tokens": ["Wer", "da", "will", "der", "Lie\u00b7be", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mu\u00df sich ganz der Liebe geben,", "tokens": ["Mu\u00df", "sich", "ganz", "der", "Lie\u00b7be", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sich nicht teilen, nicht zersplittern,", "tokens": ["Sich", "nicht", "tei\u00b7len", ",", "nicht", "zer\u00b7split\u00b7tern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PRF", "PTKNEG", "VVINF", "$,", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ganz im Ku\u00df hin\u00fcberzittern;", "tokens": ["Ganz", "im", "Ku\u00df", "hin\u00b7\u00fc\u00b7ber\u00b7zit\u00b7tern", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Mu\u00df des Herzens ganzes Dr\u00e4ngen", "tokens": ["Mu\u00df", "des", "Her\u00b7zens", "gan\u00b7zes", "Dr\u00e4n\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Auf des Mundes Spitze zw\u00e4ngen;", "tokens": ["Auf", "des", "Mun\u00b7des", "