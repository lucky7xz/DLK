{"dta.poem.7004": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Vereinigung und dem Unter-  \n scheid der Seelen und des C\u00f6rpers.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie k\u00f6nnen sie den Leib in einen gleichen Wehrt", "tokens": ["Wie", "k\u00f6n\u00b7nen", "sie", "den", "Leib", "in", "ei\u00b7nen", "glei\u00b7chen", "Wehrt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "PPER", "ART", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit jenen grossen Seelen setzen,", "tokens": ["Mit", "je\u00b7nen", "gros\u00b7sen", "See\u00b7len", "set\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die, blo\u00df da\u00df ihren Ruhm die Nachwelt m\u00f6chte sch\u00e4tzen,", "tokens": ["Die", ",", "blo\u00df", "da\u00df", "ih\u00b7ren", "Ruhm", "die", "Nach\u00b7welt", "m\u00f6ch\u00b7te", "sch\u00e4t\u00b7zen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "ADV", "KOUS", "PPOSAT", "NN", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und da\u00df ihr grosser Nahm\u2019 erkannt w\u00fcrd\u2019, und verehrt;", "tokens": ["Und", "da\u00df", "ihr", "gros\u00b7ser", "Nahm'", "er\u00b7kannt", "w\u00fcrd'", ",", "und", "ver\u00b7ehrt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPOSAT", "ADJA", "NN", "VVPP", "VAFIN", "$,", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Als ein verdienter Lohn, der Tugenden geh\u00f6rt,", "tokens": ["Als", "ein", "ver\u00b7dien\u00b7ter", "Lohn", ",", "der", "Tu\u00b7gen\u00b7den", "ge\u00b7h\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Nicht Arbeit, Stahl noch Gluht gescheuet?", "tokens": ["Nicht", "Ar\u00b7beit", ",", "Stahl", "noch", "Gluht", "ge\u00b7scheu\u00b7et", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "$,", "NN", "ADV", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Die Seelen, die man sah\u2019 sich von den L\u00fcsten trennen,", "tokens": ["Die", "See\u00b7len", ",", "die", "man", "sah'", "sich", "von", "den", "L\u00fcs\u00b7ten", "tren\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PIS", "VVFIN", "PRF", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und die ein endlich Gl\u00fcck nicht mehr bewegen k\u00f6nnen,", "tokens": ["Und", "die", "ein", "end\u00b7lich", "Gl\u00fcck", "nicht", "mehr", "be\u00b7we\u00b7gen", "k\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ART", "ADV", "NN", "PTKNEG", "ADV", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Sind sie durch Hoffnungs-Dunst, der sie betrogen,", "tokens": ["Sind", "sie", "durch", "Hoff\u00b7nungs\u00b7Dunst", ",", "der", "sie", "be\u00b7tro\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Getrieben worden und gezogen?", "tokens": ["Ge\u00b7trie\u00b7ben", "wor\u00b7den", "und", "ge\u00b7zo\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAPP", "KON", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Ist dies ein Gegenwurff des C\u00f6rpers, der zu sterben", "tokens": ["Ist", "dies", "ein", "Ge\u00b7gen\u00b7wurff", "des", "C\u00f6r\u00b7pers", ",", "der", "zu", "ster\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PDS", "ART", "NN", "ART", "NN", "$,", "PRELS", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Gef\u00fcget und gemacht, und nicht vielmehr", "tokens": ["Ge\u00b7f\u00fc\u00b7get", "und", "ge\u00b7macht", ",", "und", "nicht", "viel\u00b7mehr"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVPP", "KON", "VVPP", "$,", "KON", "PTKNEG", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Von einem ew\u2019gen Geist, der sich nach nichts so sehr,", "tokens": ["Von", "ei\u00b7nem", "ew'\u00b7gen", "Geist", ",", "der", "sich", "nach", "nichts", "so", "sehr", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "PRELS", "PRF", "APPR", "PIS", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Als blo\u00df nach G\u00fctern sehnt, die nimmermehr verderben?", "tokens": ["Als", "blo\u00df", "nach", "G\u00fc\u00b7tern", "sehnt", ",", "die", "nim\u00b7mer\u00b7mehr", "ver\u00b7der\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPR", "NN", "VVFIN", "$,", "PRELS", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}