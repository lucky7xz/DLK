{"textgrid.poem.35001": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Traum und Leben", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es gl\u00fchte der Tag, es gl\u00fchte mein Herz,", "tokens": ["Es", "gl\u00fch\u00b7te", "der", "Tag", ",", "es", "gl\u00fch\u00b7te", "mein", "Herz", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Still trug ich mit mir herum den Schmerz.", "tokens": ["Still", "trug", "ich", "mit", "mir", "he\u00b7rum", "den", "Schmerz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "PPER", "APZR", "ART", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und als die Nacht kam, schlich ich fort", "tokens": ["Und", "als", "die", "Nacht", "kam", ",", "schlich", "ich", "fort"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "VVFIN", "$,", "VVFIN", "PPER", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zur bl\u00fchenden Rose am stillen Ort.", "tokens": ["Zur", "bl\u00fc\u00b7hen\u00b7den", "Ro\u00b7se", "am", "stil\u00b7len", "Ort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.2": {"line.1": {"text": "Ich nahte mich leise und stumm wie das Grab;", "tokens": ["Ich", "nah\u00b7te", "mich", "lei\u00b7se", "und", "stumm", "wie", "das", "Grab", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "KOKOM", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Nur Tr\u00e4nen rollten die Wangen hinab;", "tokens": ["Nur", "Tr\u00e4\u00b7nen", "roll\u00b7ten", "die", "Wan\u00b7gen", "hin\u00b7ab", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ich schaut in den Kelch der Rose hinein \u2013", "tokens": ["Ich", "schaut", "in", "den", "Kelch", "der", "Ro\u00b7se", "hin\u00b7ein", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "PTKVZ", "$("], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Da glomm's hervor, wie ein gl\u00fchender Schein. \u2013", "tokens": ["Da", "glom\u00b7m's", "her\u00b7vor", ",", "wie", "ein", "gl\u00fc\u00b7hen\u00b7der", "Schein", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "NE", "PTKVZ", "$,", "PWAV", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.3": {"line.1": {"text": "Und freudig entschlief ich beim Rosenbaum;", "tokens": ["Und", "freu\u00b7dig", "ent\u00b7schlief", "ich", "beim", "Ro\u00b7sen\u00b7baum", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Da trieb sein Spiel ein neckender Traum:", "tokens": ["Da", "trieb", "sein", "Spiel", "ein", "ne\u00b7cken\u00b7der", "Traum", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Ich sah ein rosiges M\u00e4dchenbild,", "tokens": ["Ich", "sah", "ein", "ro\u00b7si\u00b7ges", "M\u00e4d\u00b7chen\u00b7bild", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Den Busen ein rosiges Mieder umh\u00fcllt.", "tokens": ["Den", "Bu\u00b7sen", "ein", "ro\u00b7si\u00b7ges", "Mie\u00b7der", "um\u00b7h\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.4": {"line.1": {"text": "Sie gab mir was H\u00fcbsches, recht goldig und weich;", "tokens": ["Sie", "gab", "mir", "was", "H\u00fcb\u00b7sches", ",", "recht", "gol\u00b7dig", "und", "weich", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIS", "ADJA", "$,", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Ich trug's in ein goldenes H\u00e4uschen sogleich.", "tokens": ["Ich", "trug's", "in", "ein", "gol\u00b7de\u00b7nes", "H\u00e4usc\u00b7hen", "sog\u00b7leich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "ADV", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Im H\u00e4uschen, da geht es gar wunderlich bunt,", "tokens": ["Im", "H\u00e4usc\u00b7hen", ",", "da", "geht", "es", "gar", "wun\u00b7der\u00b7lich", "bunt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "ADV", "VVFIN", "PPER", "ADV", "ADJD", "ADJD", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Da dreht sich ein V\u00f6lkchen in zierlicher Rund'.", "tokens": ["Da", "dreht", "sich", "ein", "V\u00f6lk\u00b7chen", "in", "zier\u00b7li\u00b7cher", "Rund'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.5": {"line.1": {"text": "Da tanzen zw\u00f6lf T\u00e4nzer, ohn' Ruh' und Rast,", "tokens": ["Da", "tan\u00b7zen", "zw\u00f6lf", "T\u00e4n\u00b7zer", ",", "ohn'", "Ruh'", "und", "Rast", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "CARD", "NN", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Sie haben sich fest bei den H\u00e4nden gefa\u00dft;", "tokens": ["Sie", "ha\u00b7ben", "sich", "fest", "bei", "den", "H\u00e4n\u00b7den", "ge\u00b7fa\u00dft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PRF", "ADJD", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Und wenn ein Tanz zu enden begann,", "tokens": ["Und", "wenn", "ein", "Tanz", "zu", "en\u00b7den", "be\u00b7gann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "So f\u00e4ngt ein andrer von vorne an.", "tokens": ["So", "f\u00e4ngt", "ein", "an\u00b7drer", "von", "vor\u00b7ne", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "APPR", "ADV", "PTKVZ", "$."], "meter": "-+-+-