{"textgrid.poem.46386": {"metadata": {"author": {"name": "Ewald, Johann Joachim", "birth": "N.A.", "death": "N.A."}, "title": "Der Sturm", "genre": "verse", "period": "N.A.", "pub_year": 1744, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es wird auf einmahl Nacht, die Winde heulen laut,", "tokens": ["Es", "wird", "auf", "ein\u00b7mahl", "Nacht", ",", "die", "Win\u00b7de", "heu\u00b7len", "laut", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ADV", "NN", "$,", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und Himmel, Meer und Grund wird wie vermengt geschaut.", "tokens": ["Und", "Him\u00b7mel", ",", "Meer", "und", "Grund", "wird", "wie", "ver\u00b7mengt", "ge\u00b7schaut", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "NN", "KON", "NN", "VAFIN", "KOKOM", "VVPP", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das Schiff fliegt Sternen zu, st\u00fcrzt wieder tief herab,", "tokens": ["Das", "Schiff", "fliegt", "Ster\u00b7nen", "zu", ",", "st\u00fcrzt", "wie\u00b7der", "tief", "her\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "PTKVZ", "$,", "VVFIN", "ADV", "ADJD", "ADV", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "L\u00e4ufft unter Wellen fort, sieht um sich nichts als Grab,", "tokens": ["L\u00e4ufft", "un\u00b7ter", "Wel\u00b7len", "fort", ",", "sieht", "um", "sich", "nichts", "als", "Grab", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "PTKVZ", "$,", "VVFIN", "APPR", "PRF", "PIS", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Hier blitzt, dort donnert es, der ganze Aether st\u00fcrmt,", "tokens": ["Hier", "blitzt", ",", "dort", "don\u00b7nert", "es", ",", "der", "gan\u00b7ze", "A\u00b7e\u00b7ther", "st\u00fcrmt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "$,", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+----+", "measure": "unknown.measure.penta"}, "line.6": {"text": "Die Fluten sind auf Flut, und Wolk auf Wolk geth\u00fcrmt,", "tokens": ["Die", "Flu\u00b7ten", "sind", "auf", "Flut", ",", "und", "Wolk", "auf", "Wolk", "ge\u00b7th\u00fcrmt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "NN", "$,", "KON", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Das Schiff zerscheitert itzt, und mir ... ist nichts geschehn,", "tokens": ["Das", "Schiff", "zer\u00b7schei\u00b7tert", "itzt", ",", "und", "mir", "...", "ist", "nichts", "ge\u00b7schehn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$,", "KON", "PPER", "$(", "VAFIN", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Weil ich dem Sturme nur vom Ufer zugesehn.", "tokens": ["Weil", "ich", "dem", "Stur\u00b7me", "nur", "vom", "U\u00b7fer", "zu\u00b7ge\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza