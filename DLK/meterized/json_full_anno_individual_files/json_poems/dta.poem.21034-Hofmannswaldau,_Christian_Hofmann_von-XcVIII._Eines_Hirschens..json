{"dta.poem.21034": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "XcVIII.  \n Eines Hirschens.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Ich hab in einer Jagt mich in ein Thal gest\u00fcrtzet/", "tokens": ["Ich", "hab", "in", "ei\u00b7ner", "Jagt", "mich", "in", "ein", "Thal", "ge\u00b7st\u00fcrt\u00b7zet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PIS", "VVFIN", "PRF", "APPR", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und mir durch k\u00fchnen Sprung das Leben selbst", "tokens": ["Und", "mir", "durch", "k\u00fch\u00b7nen", "Sprung", "das", "Le\u00b7ben", "selbst"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "APPR", "ADJA", "NN", "ART", "NN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "verk\u00fcrtzet/", "tokens": ["ver\u00b7k\u00fcrt\u00b7zet", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Ich wil/ wilstu mein Aa\u00df auff deiner Gr\u00e4ntze leiden/", "tokens": ["Ich", "wil", "/", "wils\u00b7tu", "mein", "Aa\u00df", "auff", "dei\u00b7ner", "Gr\u00e4nt\u00b7ze", "lei\u00b7den", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$(", "VMFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Zwar deiner Frau die Brunst/ doch dir das Horn", "tokens": ["Zwar", "dei\u00b7ner", "Frau", "die", "Brunst", "/", "doch", "dir", "das", "Horn"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "ART", "NN", "$(", "ADV", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "bescheiden.", "tokens": ["be\u00b7schei\u00b7den", "."], "token_info": ["word", "punct"], "pos": ["ADJD", "$."], "meter": "-+-", "measure": "amphibrach.single"}}}}}