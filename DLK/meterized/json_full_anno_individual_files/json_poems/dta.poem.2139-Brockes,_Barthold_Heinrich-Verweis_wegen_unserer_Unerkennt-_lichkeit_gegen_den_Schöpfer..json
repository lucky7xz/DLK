{"dta.poem.2139": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Verweis wegen unserer Unerkennt-  \n lichkeit gegen den Sch\u00f6pfer.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So bald wir ein Gem\u00e4hld', das sch\u00f6n,", "tokens": ["So", "bald", "wir", "ein", "Ge\u00b7m\u00e4hld'", ",", "das", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "ART", "NN", "$,", "PRELS", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Erblicken, wird man es besehn;", "tokens": ["Er\u00b7bli\u00b7cken", ",", "wird", "man", "es", "be\u00b7sehn", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VAFIN", "PIS", "PPER", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Man pflegt sich nah hinbey zu f\u00fcgen,", "tokens": ["Man", "pflegt", "sich", "nah", "hin\u00b7bey", "zu", "f\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PRF", "ADJD", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Man wird sich sehr daran vergn\u00fcgen,", "tokens": ["Man", "wird", "sich", "sehr", "da\u00b7ran", "ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PRF", "ADV", "PAV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Gleich nach des Meisters Namen fragen,", "tokens": ["Gleich", "nach", "des", "Meis\u00b7ters", "Na\u00b7men", "fra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und viel zu seinem Ruhme sagen.", "tokens": ["Und", "viel", "zu", "sei\u00b7nem", "Ruh\u00b7me", "sa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Wenn aber wir von Berg und Thal,", "tokens": ["Wenn", "a\u00b7ber", "wir", "von", "Berg", "und", "Thal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPER", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Bestralt vom wahren Sonnenstral,", "tokens": ["Be\u00b7stralt", "vom", "wah\u00b7ren", "Son\u00b7nen\u00b7stral", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Das herrlichste Original,", "tokens": ["Das", "herr\u00b7lichs\u00b7te", "O\u00b7rig\u00b7i\u00b7nal", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.10": {"text": "Von Gottes Finger selbst gezieret,", "tokens": ["Von", "Got\u00b7tes", "Fin\u00b7ger", "selbst", "ge\u00b7zie\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "So wird man kaum dadurch ger\u00fchret,", "tokens": ["So", "wird", "man", "kaum", "da\u00b7durch", "ge\u00b7r\u00fch\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "ADV", "PAV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Wobey die trefflichste Copey", "tokens": ["Wo\u00b7bey", "die", "treff\u00b7lichs\u00b7te", "Co\u00b7pey"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.13": {"text": "Nur eine schlechte Schmiererey,", "tokens": ["Nur", "ei\u00b7ne", "schlech\u00b7te", "Schmie\u00b7re\u00b7rey", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Viel minder, da\u00df man ein Behagen", "tokens": ["Viel", "min\u00b7der", ",", "da\u00df", "man", "ein", "Be\u00b7ha\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "$,", "KOUS", "PIS", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "An solchen Wundern sollte tragen.", "tokens": ["An", "sol\u00b7chen", "Wun\u00b7dern", "soll\u00b7te", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Wer wird nach dem, der diese Pracht", "tokens": ["Wer", "wird", "nach", "dem", ",", "der", "die\u00b7se", "Pracht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "VAFIN", "APPR", "ART", "$,", "PRELS", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "So wunderbar hervorgebracht,", "tokens": ["So", "wun\u00b7der\u00b7bar", "her\u00b7vor\u00b7ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Wer nach des Meisters Namen fragen?", "tokens": ["Wer", "nach", "des", "Meis\u00b7ters", "Na\u00b7men", "fra\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}