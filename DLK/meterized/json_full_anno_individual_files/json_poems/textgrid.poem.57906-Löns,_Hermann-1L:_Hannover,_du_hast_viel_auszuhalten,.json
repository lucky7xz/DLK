{"textgrid.poem.57906": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "1L: Hannover, du hast viel auszuhalten,", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hannover, du hast viel auszuhalten,", "tokens": ["Han\u00b7no\u00b7ver", ",", "du", "hast", "viel", "aus\u00b7zu\u00b7hal\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VAFIN", "ADV", "VVIZU", "$,"], "meter": "+--+-++-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Du lebst in einer b\u00f6sen Zeit,", "tokens": ["Du", "lebst", "in", "ei\u00b7ner", "b\u00f6\u00b7sen", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn ich an die letzte Woche denke,", "tokens": ["Wenn", "ich", "an", "die", "letz\u00b7te", "Wo\u00b7che", "den\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Dann tust du mir faktisch von Herzen leid.", "tokens": ["Dann", "tust", "du", "mir", "fak\u00b7tisch", "von", "Her\u00b7zen", "leid", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADJD", "APPR", "NN", "ADJD", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.2": {"line.1": {"text": "Zuerst der Ausstand der Stra\u00dfenbahner,", "tokens": ["Zu\u00b7erst", "der", "Aus\u00b7stand", "der", "Stra\u00b7\u00dfen\u00b7bah\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ART", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Das ist f\u00fcrwahr kein Pfingstfestspa\u00df,", "tokens": ["Das", "ist", "f\u00fcr\u00b7wahr", "kein", "Pfingst\u00b7fest\u00b7spa\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dann die Krawalle, die Schutzmannss\u00e4bel,", "tokens": ["Dann", "die", "Kra\u00b7wal\u00b7le", ",", "die", "Schutz\u00b7manns\u00b7s\u00e4\u00b7bel", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "+-++--+-+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Die B\u00fcrgersteige, von Menschenblut na\u00df.", "tokens": ["Die", "B\u00fcr\u00b7ger\u00b7stei\u00b7ge", ",", "von", "Men\u00b7schen\u00b7blut", "na\u00df", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "NN", "ADJD", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Zu all dem Elend kommt noch der Sousa,", "tokens": ["Zu", "all", "dem", "E\u00b7lend", "kommt", "noch", "der", "Sou\u00b7sa", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ART", "NN", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Halb Schlangenmensch, halb Musikus,", "tokens": ["Halb", "Schlan\u00b7gen\u00b7mensch", ",", "halb", "Mu\u00b7si\u00b7kus", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "NN", "$,", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und gibt hier mehrere Konzerte,", "tokens": ["Und", "gibt", "hier", "meh\u00b7re\u00b7re", "Kon\u00b7zer\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Da\u00df man das auch noch ausstehen mu\u00df!", "tokens": ["Da\u00df", "man", "das", "auch", "noch", "aus\u00b7ste\u00b7hen", "mu\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PDS", "ADV", "ADV", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Und schlie\u00dflich schickt nach dem armen Hannover", "tokens": ["Und", "schlie\u00df\u00b7lich", "schickt", "nach", "dem", "ar\u00b7men", "Han\u00b7no\u00b7ver"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Man tausend Posaunenbl\u00e4ser her,", "tokens": ["Man", "tau\u00b7send", "Po\u00b7sau\u00b7nen\u00b7bl\u00e4\u00b7ser", "her", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "CARD", "NN", "PTKVZ", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Die sollen Sonntags auf einmal tuten;", "tokens": ["Die", "sol\u00b7len", "Sonn\u00b7tags", "auf", "ein\u00b7mal", "tu\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "APPR", "ADV", "VVFIN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "O Herr, du strafst dein Volk zu schwer!", "tokens": ["O", "Herr", ",", "du", "strafst", "dein", "Volk", "zu", "schwer", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"