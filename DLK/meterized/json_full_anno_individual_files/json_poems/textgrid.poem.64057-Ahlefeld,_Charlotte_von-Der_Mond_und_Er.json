{"textgrid.poem.64057": {"metadata": {"author": {"name": "Ahlefeld, Charlotte von", "birth": "N.A.", "death": "N.A."}, "title": "Der Mond und Er", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "L\u00e4chelndes sch\u00f6nes Gestirn, zu Deiner unendlichen H\u00f6he", "tokens": ["L\u00e4\u00b7cheln\u00b7des", "sch\u00f6\u00b7nes", "Ge\u00b7stirn", ",", "zu", "Dei\u00b7ner", "un\u00b7end\u00b7li\u00b7chen", "H\u00f6\u00b7he"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "ADJA", "NN", "$,", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Wend' ich den traurigen Blick, und er erheitert sich oft.", "tokens": ["Wend'", "ich", "den", "trau\u00b7ri\u00b7gen", "Blick", ",", "und", "er", "er\u00b7hei\u00b7tert", "sich", "oft", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "$,", "KON", "PPER", "VVFIN", "PRF", "ADV", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.3": {"text": "So auch erheb' ich ", "tokens": ["So", "auch", "er\u00b7heb'", "ich"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PPER"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Und dann scheint mir die Welt nicht mehr ein Kerker zu seyn.", "tokens": ["Und", "dann", "scheint", "mir", "die", "Welt", "nicht", "mehr", "ein", "Ker\u00b7ker", "zu", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ART", "NN", "PTKNEG", "ADV", "ART", "NN", "PTKZU", "VAINF", "$."], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Freundlich winkt mir ", "tokens": ["Freund\u00b7lich", "winkt", "mir"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Still und schweigend wie Du, wandelt Er ferne von mir.", "tokens": ["Still", "und", "schwei\u00b7gend", "wie", "Du", ",", "wan\u00b7delt", "Er", "fer\u00b7ne", "von", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "KOKOM", "PPER", "$,", "VVFIN", "PPER", "ADV", "APPR", "PPER", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Aber es nahet mir hold auf muthlos umd\u00e4mmerten Bahnen,", "tokens": ["A\u00b7ber", "es", "na\u00b7het", "mir", "hold", "auf", "muth\u00b7los", "um\u00b7d\u00e4m\u00b7mer\u00b7ten", "Bah\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADJD", "APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Sanft wie Dein leuchtender Schein, seiner Erinnerung Gruss.", "tokens": ["Sanft", "wie", "Dein", "leuch\u00b7ten\u00b7der", "Schein", ",", "sei\u00b7ner", "E\u00b7rin\u00b7ne\u00b7rung", "Gruss", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "PPOSAT", "ADJA", "NN", "$,", "PPOSAT", "NN", "NN", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}}, "stanza.3": {"line.1": {"text": "Unerreichbar bist Du, o Mond, in der Ferne des Himmels,", "tokens": ["Un\u00b7er\u00b7reich\u00b7bar", "bist", "Du", ",", "o", "Mond", ",", "in", "der", "Fer\u00b7ne", "des", "Him\u00b7mels", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "$,", "FM", "NN", "$,", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+--+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Dennoch verkl\u00e4rst Du die Nacht still mit erquickendem Glanz;", "tokens": ["Den\u00b7noch", "ver\u00b7kl\u00e4rst", "Du", "die", "Nacht", "still", "mit", "er\u00b7qui\u00b7cken\u00b7dem", "Glanz", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "So erf\u00fcllet auch Er mit Licht und Kraft mir den Busen,", "tokens": ["So", "er\u00b7f\u00fcl\u00b7let", "auch", "Er", "mit", "Licht", "und", "Kraft", "mir", "den", "Bu\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPER", "APPR", "NN", "KON", "NN", "PPER", "ART", "NN", "$,"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Ewig mir ferne wie Du, ist er dem Geiste doch nah.", "tokens": ["E\u00b7wig", "mir", "fer\u00b7ne", "wie", "Du", ",", "ist", "er", "dem", "Geis\u00b7te", "doch", "nah", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "ADV", "KOKOM", "PPER", "$,", "VAFIN", "PPER", "ART", "NN", "ADV", "ADJD", "$."], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}}, "stanza.4": {"line.1": {"text": "L\u00e4chelndes sch\u00f6nes Gestirn, zu Deiner unendlichen H\u00f6he", "tokens": ["L\u00e4\u00b7cheln\u00b7des", "sch\u00f6\u00b7nes", "Ge\u00b7stirn", ",", "zu", "Dei\u00b7ner", "un\u00b7end\u00b7li\u00b7chen", "H\u00f6\u00b7he"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "ADJA", "NN", "$,", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Wend'