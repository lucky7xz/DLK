{"dta.poem.12802": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Als  \n Herr  D.  Joh. Franciscus Buddeus  \n das Academische Regiment wieder  \n niederlegte.  \n Jm namen Herr  M.  Uhles.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das macht, da\u00df ihn die weisheit auferzogen,", "tokens": ["Das", "macht", ",", "da\u00df", "ihn", "die", "weis\u00b7heit", "auf\u00b7er\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "KOUS", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das macht, da\u00df er die klugheit eingesogen,", "tokens": ["Das", "macht", ",", "da\u00df", "er", "die", "klug\u00b7heit", "ein\u00b7ge\u00b7so\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "KOUS", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die itzt aus seinem mund und seiner feder qvillt.", "tokens": ["Die", "itzt", "aus", "sei\u00b7nem", "mund", "und", "sei\u00b7ner", "fe\u00b7der", "qvillt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}