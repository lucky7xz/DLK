{"dta.poem.6864": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Sitz der Sinnlichkeiten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Hat man der Tr\u00e4ume dann vergessen,", "tokens": ["Hat", "man", "der", "Tr\u00e4u\u00b7me", "dann", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Erinnert man sich nicht der holden L\u00fcgen,", "tokens": ["E\u00b7rin\u00b7nert", "man", "sich", "nicht", "der", "hol\u00b7den", "L\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PRF", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die, sammt der weiten Lufft, so nicht zu messen,", "tokens": ["Die", ",", "sammt", "der", "wei\u00b7ten", "Lufft", ",", "so", "nicht", "zu", "mes\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "VVFIN", "ART", "ADJA", "NN", "$,", "ADV", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Auch Berge, nebst des Meeres Wellen,", "tokens": ["Auch", "Ber\u00b7ge", ",", "nebst", "des", "Mee\u00b7res", "Wel\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Auch Ufer, die bebl\u00fchmt, von hellen Fl\u00fcssen,", "tokens": ["Auch", "U\u00b7fer", ",", "die", "be\u00b7bl\u00fchmt", ",", "von", "hel\u00b7len", "Fl\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "PRELS", "VVFIN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "In solcher Deutlichkeit uns vorzustellen,", "tokens": ["In", "sol\u00b7cher", "Deut\u00b7lich\u00b7keit", "uns", "vor\u00b7zu\u00b7stel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PPER", "VVIZU", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Bey gantz geschlo\u00dfnen Augen, wissen.", "tokens": ["Bey", "gantz", "ge\u00b7schlo\u00df\u00b7nen", "Au\u00b7gen", ",", "wis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Des tr\u00fcgerischen Traums so liebe Zauberey", "tokens": ["Des", "tr\u00fc\u00b7ge\u00b7ri\u00b7schen", "Traums", "so", "lie\u00b7be", "Zau\u00b7be\u00b7rey"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Zeigt viele Gegenw\u00fcrff mit solchen starcken Z\u00fcgen,", "tokens": ["Zeigt", "vie\u00b7le", "Ge\u00b7gen\u00b7w\u00fcrff", "mit", "sol\u00b7chen", "star\u00b7cken", "Z\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "APPR", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Bald nah\u2019 und bald entfernt mit Farben, mancherley,", "tokens": ["Bald", "nah'", "und", "bald", "ent\u00b7fernt", "mit", "Far\u00b7ben", ",", "man\u00b7cher\u00b7ley", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADV", "ADJD", "APPR", "NN", "$,", "PIS", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ohn da\u00df von allen diesen Dingen,", "tokens": ["Ohn", "da\u00df", "von", "al\u00b7len", "die\u00b7sen", "Din\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "APPR", "PIAT", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Um selbige hervor zu bringen,", "tokens": ["Um", "sel\u00b7bi\u00b7ge", "her\u00b7vor", "zu", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADJA", "PTKVZ", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Von aussen etwas w\u00fcrcklich sey.", "tokens": ["Von", "aus\u00b7sen", "et\u00b7was", "w\u00fcrck\u00b7lich", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}