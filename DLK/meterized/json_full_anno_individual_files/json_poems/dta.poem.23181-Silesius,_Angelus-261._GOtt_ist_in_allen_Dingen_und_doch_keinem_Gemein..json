{"dta.poem.23181": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "261. GOtt ist in allen Dingen/ und doch  \n keinem Gemein.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1657", "urn": "urn:nbn:de:kobv:b4-200905198292", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Geistreiche Sinn- vnd Schlussrime. Wien, 1657."}, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df wesen GOttes macht sich keinem Ding gemein:", "tokens": ["Da\u00df", "we\u00b7sen", "Got\u00b7tes", "macht", "sich", "kei\u00b7nem", "Ding", "ge\u00b7mein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "VVFIN", "PRF", "PIAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und mu\u00df nothwendig doch auch in den Teufeln sein.", "tokens": ["Und", "mu\u00df", "noth\u00b7wen\u00b7dig", "doch", "auch", "in", "den", "Teu\u00b7feln", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADJD", "ADV", "ADV", "APPR", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}