{"textgrid.poem.60977": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "Europ\u00e4ischer Tierkreis", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Eine ", "tokens": ["Ei\u00b7ne"], "token_info": ["word"], "pos": ["ART"], "meter": "+-", "measure": "trochaic.single"}, "line.2": {"text": "Heutzutage sieht sie freilich einer alten Jungfer Gleich.", "tokens": ["Heut\u00b7zu\u00b7ta\u00b7ge", "sieht", "sie", "frei\u00b7lich", "ei\u00b7ner", "al\u00b7ten", "Jung\u00b7fer", "Gleich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "ADV", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.2": {"line.1": {"text": "Halt o deutsches Volk die ", "tokens": ["Halt", "o", "deut\u00b7sches", "Volk", "die"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "FM", "ADJA", "NN", "ART"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Zwischen wandelbarn Franzosen und der schn\u00f6den Mongolei!", "tokens": ["Zwi\u00b7schen", "wan\u00b7del\u00b7barn", "Fran\u00b7zo\u00b7sen", "und", "der", "schn\u00f6\u00b7den", "Mon\u00b7go\u00b7lei", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.3": {"line.1": {"text": "Wenn Monarchen nicht die L\u00fccke f\u00fcllen zwischen Volk und Thron,", "tokens": ["Wenn", "Mon\u00b7ar\u00b7chen", "nicht", "die", "L\u00fc\u00b7cke", "f\u00fcl\u00b7len", "zwi\u00b7schen", "Volk", "und", "Thron", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PTKNEG", "ART", "NN", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Wird hinein sich Mi\u00dfverst\u00e4ndnis schleichen, wie ein", "tokens": ["Wird", "hin\u00b7ein", "sich", "Mi\u00df\u00b7ver\u00b7st\u00e4nd\u00b7nis", "schlei\u00b7chen", ",", "wie", "ein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "ADV", "PRF", "NN", "VVINF", "$,", "PWAV", "ART"], "meter": "+-+-+-+-+--+", "measure": "iambic.hexa.chol"}}, "stanza.4": {"line.1": {"text": "Eure Lober, Weltgebieter, sind f\u00fcrwahr gar wenig n\u00fctz,", "tokens": ["Eu\u00b7re", "Lo\u00b7ber", ",", "Welt\u00b7ge\u00b7bie\u00b7ter", ",", "sind", "f\u00fcr\u00b7wahr", "gar", "we\u00b7nig", "n\u00fctz", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "NN", "$,", "VAFIN", "ADV", "ADV", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Besser, als der Hunde Wedeln, meint es mit dem Pfeil der ", "tokens": ["Bes\u00b7ser", ",", "als", "der", "Hun\u00b7de", "We\u00b7deln", ",", "meint", "es", "mit", "dem", "Pfeil", "der"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "$,", "KOUS", "ART", "NN", "NN", "$,", "VVFIN", "PPER", "APPR", "ART", "NN", "ART"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}}, "stanza.5": {"line.1": {"text": "Auf Gebirgen schweift die Freiheit, wie ein", "tokens": ["Auf", "Ge\u00b7bir\u00b7gen", "schweift", "die", "Frei\u00b7heit", ",", "wie", "ein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "$,", "PWAV", "ART"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Wird sie einst im See der T\u00e4ler spiegeln ihr geliebtes Bild?", "tokens": ["Wird", "sie", "einst", "im", "See", "der", "T\u00e4\u00b7ler", "spie\u00b7geln", "ihr", "ge\u00b7lieb\u00b7tes", "Bild", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "APPRART", "NN", "ART", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.6": {"line.1": {"text": "In die Totenauen, welche jenes Volk zum Grab gewann,", "tokens": ["In", "die", "To\u00b7te\u00b7nau\u00b7en", ",", "wel\u00b7che", "je\u00b7nes", "Volk", "zum", "Grab", "ge\u00b7wann", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PDAT", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Sammelt nun den Rest der Z\u00e4hren jener gro\u00dfe", "tokens": ["Sam\u00b7melt", "nun", "den", "Rest", "der", "Z\u00e4h\u00b7ren", "je\u00b7ner", "gro\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "NN", "ART", "NN", "PDAT", "ADJA"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}, "stanza.7": {"line.1": {"text": "Wird Herakles seine S\u00e4ulen reinigen nie von dieser Brut?", "tokens": ["Wird", "Her\u00b7ak\u00b7les", "sei\u00b7ne", "S\u00e4u\u00b7len", "rei\u00b7ni\u00b7gen", "nie", "von", "die\u00b7ser", "Brut", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "PPOSAT", "NN", "VVFIN", "ADV", "APPR", "PDAT", "NN", "$."], "meter": "+-+-+-+-+--+-+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.2": {"text": "Diese beiden ", "tokens": ["Die\u00b7se", "bei\u00b7den"], "token_info": ["word", "word"], "pos": ["PDAT", "PIAT"], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.8": {"line.1": {"text": "Wenn das Tor ihr festlich \u00f6ffnet, ziehn wir festlich auch hinein,", "tokens": ["Wenn", "das", "Tor", "ihr", "fest\u00b7lich", "\u00f6ff\u00b7net", ",", "ziehn", "wir", "fest\u00b7lich", "auch", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "ADJD", "VVFIN", "$,", "VVFIN", "PPER", "ADJD", "ADV", "PTKVZ", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "\u00d6ffnet ihr es nicht, so st\u00f6\u00dft es jener b\u00f6se ", "tokens": ["\u00d6ff\u00b7net", "ihr", "es", "nicht", ",", "so", "st\u00f6\u00dft", "es", "je\u00b7ner", "b\u00f6\u00b7se"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPER", "PTKNEG", "$,", "ADV", "VVFIN", "PPER", "PDAT", "ADJA"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}, "stanza.9": {"line.1": {"text": "Welch ein Held, o ", "tokens": ["Welch", "ein", "Held", ",", "o"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["PIAT", "ART", "NN", "$,", "FM"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Um's zu f\u00fcllen wie ein F\u00fcllhorn an des Friedens Silberborn?", "tokens": ["Um's", "zu", "f\u00fcl\u00b7len", "wie", "ein", "F\u00fcll\u00b7horn", "an", "des", "Frie\u00b7dens", "Sil\u00b7ber\u00b7born", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKZU", "VVINF", "KOKOM", "ART", "NN", "APPR", "ART", "NN", "NE", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.10": {"line.1": {"text": "Nimm, o ", "tokens": ["Nimm", ",", "o"], "token_info": ["word", "punct", "word"], "pos": ["NE", "$,", "FM"], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "In des Vaterm\u00f6rders Stapfen tritt des V\u00f6lkerm\u00f6rders Fu\u00df!", "tokens