{"textgrid.poem.39384": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Zweite Weise", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "La\u00df dich mit gelinden Schl\u00e4gen", "tokens": ["La\u00df", "dich", "mit", "ge\u00b7lin\u00b7den", "Schl\u00e4\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "R\u00fchren, meine zarte Laute!", "tokens": ["R\u00fch\u00b7ren", ",", "mei\u00b7ne", "zar\u00b7te", "Lau\u00b7te", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da die Nacht hernieder thaute,", "tokens": ["Da", "die", "Nacht", "her\u00b7nie\u00b7der", "thau\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PTKVZ", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "M\u00fc\u00dfen wir Gelispel pflegen.", "tokens": ["M\u00fc\u00b7\u00dfen", "wir", "Ge\u00b7lis\u00b7pel", "pfle\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie sich deine T\u00f6ne regen,", "tokens": ["Wie", "sich", "dei\u00b7ne", "T\u00f6\u00b7ne", "re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "PPOSAT", "NN", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wie sie athmen, klagen, st\u00f6hnen,", "tokens": ["Wie", "sie", "ath\u00b7men", ",", "kla\u00b7gen", ",", "st\u00f6h\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PWAV", "PPER", "VVINF", "$,", "VVFIN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wallt das Herz zu meiner Sch\u00f6nen,", "tokens": ["Wallt", "das", "Herz", "zu", "mei\u00b7ner", "Sch\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Bringt ihr aus der Seele Tiefen", "tokens": ["Bringt", "ihr", "aus", "der", "See\u00b7le", "Tie\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Alle Schmerzen, welche schliefen;", "tokens": ["Al\u00b7le", "Schmer\u00b7zen", ",", "wel\u00b7che", "schlie\u00b7fen", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PRELS", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Zu dem friedlichen Gemach", "tokens": ["Zu", "dem", "fried\u00b7li\u00b7chen", "Ge\u00b7mach"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo sie ruht in Blumend\u00fcften,", "tokens": ["Wo", "sie", "ruht", "in", "Blu\u00b7men\u00b7d\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "La\u00df noch in den k\u00fchlen L\u00fcften", "tokens": ["La\u00df", "noch", "in", "den", "k\u00fch\u00b7len", "L\u00fcf\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "T\u00f6nen unser schmelzend Ach.", "tokens": ["T\u00f6\u00b7nen", "un\u00b7ser", "schmel\u00b7zend", "Ach", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJD", "ITJ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Halb entschlummert, halb noch wach,", "tokens": ["Halb", "ent\u00b7schlum\u00b7mert", ",", "halb", "noch", "wach", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,", "ADJD", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Angeblickt vom Abendstern", "tokens": ["An\u00b7ge\u00b7blickt", "vom", "A\u00b7bends\u00b7tern"], "token_info": ["word", "word", "word"], "pos": ["VVPP", "APPRART", "NN"], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.7": {"text": "Liegt sie, und vernimmt wohl gern", "tokens": ["Liegt", "sie", ",", "und", "ver\u00b7nimmt", "wohl", "gern"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "KON", "VVFIN", "ADV", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "In den leisen Harmonieen", "tokens": ["In", "den", "lei\u00b7sen", "Har\u00b7mo\u00b7ni\u00b7e\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.9": {"text": "Tr\u00e4ume, Bilder, Fantasieen,", "tokens": ["Tr\u00e4u\u00b7me", ",", "Bil\u00b7der", ",", "Fan\u00b7ta\u00b7si\u00b7e\u00b7en", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Inn'ger, liebe Saiten, bebet!", "tokens": ["Inn'\u00b7ger", ",", "lie\u00b7be", "Sai\u00b7ten", ",", "be\u00b7bet", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lockt hervor den Wiederhall!", "tokens": ["Lockt", "her\u00b7vor", "den", "Wie\u00b7der\u00b7hall", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Weckt das Lied der Nachtigall,", "tokens": ["Weckt", "das", "Lied", "der", "Nach\u00b7ti\u00b7gall", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und wetteifernd mit ihr strebet!", "tokens": ["Und", "wett\u00b7ei\u00b7fernd", "mit", "ihr", "stre\u00b7bet", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Doch wenn Sie die Stimm' erhebet,", "tokens": ["Doch", "wenn", "Sie", "die", "Stimm'", "er\u00b7he\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Dann erkennet euren Herrn,", "tokens": ["Dann", "er\u00b7ken\u00b7net", "eu\u00b7ren", "Herrn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Lauscht dem\u00fcthig und von fern.", "tokens": ["Lauscht", "de\u00b7m\u00fct\u00b7hig", "und", "von", "fern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "KON", "APPR", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Horch! schon singt der holde Mund,", "tokens": ["Horch", "!", "schon", "singt", "der", "hol\u00b7de", "Mund", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Denn verrathen unsern Bund", "tokens": ["Denn", "ver\u00b7ra\u00b7then", "un\u00b7sern", "Bund"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Nun noch einmal, gute Nacht!", "tokens": ["Nun", "noch", "ein\u00b7mal", ",", "gu\u00b7te", "Nacht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "$,", "ADJA", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Und an deinem Lager s\u00e4ume", "tokens": ["Und", "an", "dei\u00b7nem", "La\u00b7ger", "s\u00e4u\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nur der z\u00e4rtlichste der Tr\u00e4ume,", "tokens": ["Nur", "der", "z\u00e4rt\u00b7lichs\u00b7te", "der", "Tr\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bis der Morgen wieder lacht.", "tokens": ["Bis", "der", "Mor\u00b7gen", "wie\u00b7der", "lacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Dann geh' auf in stiller Pracht,", "tokens": ["Dann", "geh'", "auf", "in", "stil\u00b7ler", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "APPR", "ADJA", "NN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.6": {"text": "Wie der Tag den Erdens\u00f6hnen,", "tokens": ["Wie", "der", "Tag", "den", "Er\u00b7den\u00b7s\u00f6h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Meine Hoffnungen zu kr\u00f6nen.", "tokens": ["Mei\u00b7ne", "Hoff\u00b7nun\u00b7gen", "zu", "kr\u00f6\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Kann doch deine Bl\u00fcthenjugend,", "tokens": ["Kann", "doch", "dei\u00b7ne", "Bl\u00fc\u00b7then\u00b7ju\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Unschuld, Anmuth, reine Tugend,", "tokens": ["Un\u00b7schuld", ",", "An\u00b7muth", ",", "rei\u00b7ne", "Tu\u00b7gend", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$,", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "La\u00df dich mit gelinden Schl\u00e4gen", "tokens": ["La\u00df", "dich", "mit", "ge\u00b7lin\u00b7den", "Schl\u00e4\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "R\u00fchren, meine zarte Laute!", "tokens": ["R\u00fch\u00b7ren", ",", "mei\u00b7ne", "zar\u00b7te", "Lau\u00b7te", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da die Nacht hernieder thaute,", "tokens": ["Da", "die", "Nacht", "her\u00b7nie\u00b7d