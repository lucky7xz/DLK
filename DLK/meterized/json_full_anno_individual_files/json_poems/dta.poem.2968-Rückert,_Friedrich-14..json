{"dta.poem.2968": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "14.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein edler K\u00f6nig sprach: des F\u00fcrsten Sch\u00e4tze ruhen", "tokens": ["Ein", "ed\u00b7ler", "K\u00f6\u00b7nig", "sprach", ":", "des", "F\u00fcrs\u00b7ten", "Sch\u00e4t\u00b7ze", "ru\u00b7hen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$.", "ART", "NN", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In seiner B\u00fcrger, nicht in seinen eignen Truhen.", "tokens": ["In", "sei\u00b7ner", "B\u00fcr\u00b7ger", ",", "nicht", "in", "sei\u00b7nen", "eig\u00b7nen", "Tru\u00b7hen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PTKNEG", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Er hat es so gemeint, der gr\u00f6ste Reichthum sei", "tokens": ["Er", "hat", "es", "so", "ge\u00b7meint", ",", "der", "gr\u00f6s\u00b7te", "Reicht\u00b7hum", "sei"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVPP", "$,", "ART", "ADJA", "NN", "VAFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des F\u00fcrsten, dessen Volk ist reich und sorgenfrei.", "tokens": ["Des", "F\u00fcrs\u00b7ten", ",", "des\u00b7sen", "Volk", "ist", "reich", "und", "sor\u00b7gen\u00b7frei", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELAT", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Allein der F\u00fcrstensohn hat so es ausgelegt:", "tokens": ["Al\u00b7lein", "der", "F\u00fcrs\u00b7ten\u00b7sohn", "hat", "so", "es", "aus\u00b7ge\u00b7legt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "ADV", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mein von Rechtswegen ist, was jeder Kasten hegt.", "tokens": ["Mein", "von", "Rechts\u00b7we\u00b7gen", "ist", ",", "was", "je\u00b7der", "Kas\u00b7ten", "hegt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "APPR", "NN", "VAFIN", "$,", "PRELS", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}