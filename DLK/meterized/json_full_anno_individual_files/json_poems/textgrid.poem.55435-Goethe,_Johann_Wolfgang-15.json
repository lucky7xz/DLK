{"textgrid.poem.55435": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "15", "genre": "verse", "period": "N.A.", "pub_year": 1816, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von Berges Luft, dem \u00c4ther gleich zu achten,", "tokens": ["Von", "Ber\u00b7ges", "Luft", ",", "dem", "\u00c4\u00b7ther", "gleich", "zu", "ach\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ART", "NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Umweht, auf Gipfelfels hochwaldiger Schl\u00fcnde,", "tokens": ["Um\u00b7weht", ",", "auf", "Gip\u00b7fel\u00b7fels", "hoch\u00b7wal\u00b7di\u00b7ger", "Schl\u00fcn\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "APPR", "NE", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Im engsten Stollen wie in tiefsten Schachten", "tokens": ["Im", "engs\u00b7ten", "Stol\u00b7len", "wie", "in", "tiefs\u00b7ten", "Schach\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "KOKOM", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein Licht zu suchen, das den Geist entz\u00fcnde,", "tokens": ["Ein", "Licht", "zu", "su\u00b7chen", ",", "das", "den", "Geist", "ent\u00b7z\u00fcn\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "War ein gemeinsam k\u00f6stliches Betrachten,", "tokens": ["War", "ein", "ge\u00b7mein\u00b7sam", "k\u00f6st\u00b7li\u00b7ches", "Be\u00b7trach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ob nicht Natur zuletzt sich doch ergr\u00fcnde.", "tokens": ["Ob", "nicht", "Na\u00b7tur", "zu\u00b7letzt", "sich", "doch", "er\u00b7gr\u00fcn\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "NN", "ADV", "PRF", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und manches Jahr des stillsten Erdelebens", "tokens": ["Und", "man\u00b7ches", "Jahr", "des", "stills\u00b7ten", "Er\u00b7de\u00b7le\u00b7bens"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Ward so zum Zeugen edelsten Bestrebens.", "tokens": ["Ward", "so", "zum", "Zeu\u00b7gen", "e\u00b7dels\u00b7ten", "Be\u00b7stre\u00b7bens", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPRART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+---+-", "measure": "unknown.measure.tetra"}}, "stanza.2": {"line.1": {"text": "Im Garten auch, wo Dichterblumen sprossen,", "tokens": ["Im", "Gar\u00b7ten", "auch", ",", "wo", "Dich\u00b7ter\u00b7blu\u00b7men", "spros\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "$,", "PWAV", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den \u00e4u\u00dfern Sinn, den innern Sinn erquicken,", "tokens": ["Den", "\u00e4u\u00b7\u00dfern", "Sinn", ",", "den", "in\u00b7nern", "Sinn", "er\u00b7qui\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Gefahrlos nicht vor luftigen Geschossen,", "tokens": ["Ge\u00b7fahr\u00b7los", "nicht", "vor", "luf\u00b7ti\u00b7gen", "Ge\u00b7schos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie sie Eroten hin und wider schicken,", "tokens": ["Wie", "sie", "E\u00b7ro\u00b7ten", "hin", "und", "wi\u00b7der", "schi\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "PTKVZ", "KON", "APPR", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Da haben wir der Stunden viel genossen", "tokens": ["Da", "ha\u00b7ben", "wir", "der", "Stun\u00b7den", "viel", "ge\u00b7nos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "ADV", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "An frisch belebter Vorwelt heitern Blicken,", "tokens": ["An", "frisch", "be\u00b7leb\u00b7ter", "Vor\u00b7welt", "hei\u00b7tern", "Bli\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Gesellend uns den ewig teuren Geistern,", "tokens": ["Ge\u00b7sel\u00b7lend", "uns", "den", "e\u00b7wig", "teu\u00b7ren", "Geis\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "PPER", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Den stets beredten, unerreichten Meistern.", "tokens": ["Den", "stets", "be\u00b7red\u00b7ten", ",", "un\u00b7er\u00b7reich\u00b7ten", "Meis\u00b7tern", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Dahin bewegten wir von dornigen Pfaden", "tokens": ["Da\u00b7hin", "be\u00b7weg\u00b7ten", "wir", "von", "dor\u00b7ni\u00b7gen", "Pfa\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Verworrnen Lebens gern die m\u00fcden Schritte,", "tokens": ["Ver\u00b7worr\u00b7nen", "Le\u00b7bens", "gern", "die", "m\u00fc\u00b7den", "Schrit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dort fanden sich, zu gleicher Lust geladen,", "tokens": ["Dort", "fan\u00b7den", "sich", ",", "zu", "glei\u00b7cher", "Lust", "ge\u00b7la\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "$,", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der M\u00e4nner Tiefsinn, Frauengeist und \u2013 sitte", "tokens": ["Der", "M\u00e4n\u00b7ner", "Tief\u00b7sinn", ",", "Frau\u00b7en\u00b7geist", "und", "\u2013", "sit\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ART", "NN", "NN", "$,", "NN", "KON", "$(", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und Wissenschaft und Kunst und alle Gnaden", "tokens": ["Und", "Wis\u00b7sen\u00b7schaft", "und", "Kunst", "und", "al\u00b7le", "Gna\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN", "KON", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Des Musengottes reich in unsrer Mitte,", "tokens": ["Des", "Mu\u00b7sen\u00b7got\u00b7tes", "reich", "in", "uns\u00b7rer", "Mit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Bis endlich, l\u00e4ngst umw\u00f6lkt, der Himmel wettert,", "tokens": ["Bis", "end\u00b7lich", ",", "l\u00e4ngst", "um\u00b7w\u00f6lkt", ",", "der", "Him\u00b7mel", "wet\u00b7tert", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "$,", "ADV", "VVPP", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Das Paradies und seinen Hain zerschmettert.", "tokens": ["Das", "Pa\u00b7ra\u00b7dies", "und", "sei\u00b7nen", "Hain", "zer\u00b7schmet\u00b7tert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Nun aber Friede tr\u00f6stend wiederkehret,", "tokens": ["Nun", "a\u00b7ber", "Frie\u00b7de", "tr\u00f6s\u00b7tend", "wie\u00b7der\u00b7keh\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Kehrt unser Sinn sich treulich nach dem Alten,", "tokens": ["Kehrt", "un\u00b7ser", "Sinn", "sich", "treu\u00b7lich", "nach", "dem", "Al\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "PRF", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zu bauen auf, was Kampf und Zug zerst\u00f6ret,", "tokens": ["Zu", "bau\u00b7en", "auf", ",", "was", "Kampf", "und", "Zug", "zer\u00b7st\u00f6\u00b7ret", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PTKVZ", "$,", "PWS", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zu sichern, wie's ein guter Geist erhalten. \u2013", "tokens": ["Zu", "si\u00b7chern", ",", "wie's", "ein", "gu\u00b7ter", "Geist", "er\u00b7hal\u00b7ten", ".", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PTKZU", "VVINF", "$,", "KOUS", "ART", "ADJA", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Verwirrend ist's, wenn man die Menge h\u00f6ret;", "tokens": ["Ver\u00b7wir\u00b7rend", "ist's", ",", "wenn", "man", "die", "Men\u00b7ge", "h\u00f6\u00b7ret", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "$,", "KOUS", "PIS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Denn jeder will nach eignem Willen schalten;", "tokens": ["Denn", "je\u00b7der", "will", "nach", "eig\u00b7nem", "Wil\u00b7len", "schal\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VMFIN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Beharren wir zusamt in gleichem Sinne,", "tokens": ["Be\u00b7har\u00b7ren", "wir", "zu\u00b7samt", "in", "glei\u00b7chem", "Sin\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das rechn' ich uns zum k\u00f6stlichsten Gewinne.", "tokens": ["Das", "rechn'", "ich", "uns", "zum", "k\u00f6st\u00b7lichs\u00b7ten", "Ge\u00b7win\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PRF", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+---+-", "measure": "unknown.measure.tetra"}}, "stanza.6": {"line.1": {"text": "Von Berges Luft, dem \u00c4ther gleich zu achten,", "tokens": ["Von", "Ber\u00b7ges", "Luft", ",", "dem", "\u00c4\u00b7ther", "gleich", "zu", "ach\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ART", "NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Umweht, auf Gipfelfels hochwaldiger Schl\u00fcnde,", "tokens": ["Um\u00b7weht", ",", "auf", "Gip\u00b7fel\u00b7fels", "hoch\u00b7wal\u00b7di\u00b7ger", "Schl\u00fcn\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "APPR", "NE", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Im engsten Stollen wie in tiefsten Schachten", "tokens": ["Im", "engs\u00b7ten", "Stol\u00b7len", "wie", "in", "tiefs\u00b7ten", "Schach\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "KOKOM", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein Licht zu suchen, das den Geist entz\u00fcnde,", "tokens": ["Ein", "Licht", "zu", "su\u00b7chen", ",", "das", "den", "Geist", "ent\u00b7z\u00fcn\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "War ein gemeinsam k\u00f6stliches B