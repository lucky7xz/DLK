{"dta.poem.10333": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "V.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Sch\u00f6ne Wiege meiner Leiden,", "tokens": ["Sch\u00f6\u00b7ne", "Wie\u00b7ge", "mei\u00b7ner", "Lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sch\u00f6nes Grabmal meiner Ruh,", "tokens": ["Sch\u00f6\u00b7nes", "Grab\u00b7mal", "mei\u00b7ner", "Ruh", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sch\u00f6ne Stadt, wir m\u00fcssen scheiden, \u2014", "tokens": ["Sch\u00f6\u00b7ne", "Stadt", ",", "wir", "m\u00fcs\u00b7sen", "schei\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ADJA", "NN", "$,", "PPER", "VMFIN", "VVINF", "$,", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lebe wohl! ruf' ich dir zu.", "tokens": ["Le\u00b7be", "wohl", "!", "ruf'", "ich", "dir", "zu", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$.", "VVFIN", "PPER", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Lebe wohl, du heilge Schwelle,", "tokens": ["Le\u00b7be", "wohl", ",", "du", "heil\u00b7ge", "Schwel\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo da wandelt Liebchen traut:", "tokens": ["Wo", "da", "wan\u00b7delt", "Lieb\u00b7chen", "traut", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Lebe wohl! du heilge Stelle,", "tokens": ["Le\u00b7be", "wohl", "!", "du", "heil\u00b7ge", "Stel\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$.", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo ich sie zuerst geschaut.", "tokens": ["Wo", "ich", "sie", "zu\u00b7erst", "ge\u00b7schaut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "H\u00e4tt' ich dich doch nie gesehen,", "tokens": ["H\u00e4tt'", "ich", "dich", "doch", "nie", "ge\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sch\u00f6ne Herzensk\u00f6nigin!", "tokens": ["Sch\u00f6\u00b7ne", "Her\u00b7zens\u00b7k\u00f6\u00b7ni\u00b7gin", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nimmer w\u00e4r es dann geschehen,", "tokens": ["Nim\u00b7mer", "w\u00e4r", "es", "dann", "ge\u00b7sche\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df ich jetzt so elend bin.", "tokens": ["Da\u00df", "ich", "jetzt", "so", "e\u00b7lend", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Nie wollt' ich dein Herze r\u00fchren,", "tokens": ["Nie", "wollt'", "ich", "dein", "Her\u00b7ze", "r\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Liebe hab' ich nie erfleht;", "tokens": ["Lie\u00b7be", "hab'", "ich", "nie", "er\u00b7fleht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nur ein stilles Leben f\u00fchren", "tokens": ["Nur", "ein", "stil\u00b7les", "Le\u00b7ben", "f\u00fch\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wollt' ich, wo dein Odem weht.", "tokens": ["Wollt'", "ich", ",", "wo", "dein", "O\u00b7dem", "weht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "PWAV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Doch du dr\u00e4ngst mich selbst von hinnen,", "tokens": ["Doch", "du", "dr\u00e4ngst", "mich", "selbst", "von", "hin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADV", "APPR", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bittre Worte spricht dein Mund;", "tokens": ["Bitt\u00b7re", "Wor\u00b7te", "spricht", "dein", "Mund", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wahnsinn w\u00fchlt in meinen Sinnen,", "tokens": ["Wahn\u00b7sinn", "w\u00fchlt", "in", "mei\u00b7nen", "Sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und mein Herz ist krank und wund.", "tokens": ["Und", "mein", "Herz", "ist", "krank", "und", "wund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Und die Glieder matt und tr\u00e4ge", "tokens": ["Und", "die", "Glie\u00b7der", "matt", "und", "tr\u00e4\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJD", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schlepp' ich fort am Wanderstab,", "tokens": ["Schlepp'", "ich", "fort", "am", "Wan\u00b7der\u00b7stab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKVZ", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bis mein m\u00fcdes Haupt ich lege", "tokens": ["Bis", "mein", "m\u00fc\u00b7des", "Haupt", "ich", "le\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "PPER", "VVFIN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Ferne in ein k\u00fchles Grab.", "tokens": ["Fer\u00b7ne", "in", "ein", "k\u00fch\u00b7les", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}