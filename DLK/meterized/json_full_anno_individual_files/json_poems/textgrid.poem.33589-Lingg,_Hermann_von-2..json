{"textgrid.poem.33589": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Grab Virgil's schm\u00fcckt ewig ein Bl\u00fctenkranz", "tokens": ["Das", "Grab", "Vir\u00b7gil's", "schm\u00fcckt", "e\u00b7wig", "ein", "Bl\u00fc\u00b7ten\u00b7kranz"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NE", "VVFIN", "ADJD", "ART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Im dunklen Reblaub \u00fcber dem Felsenpfad,", "tokens": ["Im", "dunk\u00b7len", "Re\u00b7blaub", "\u00fc\u00b7ber", "dem", "Fel\u00b7sen\u00b7pfad", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und dr\u00fcben in Sorrento fl\u00fcstern", "tokens": ["Und", "dr\u00fc\u00b7ben", "in", "Sor\u00b7ren\u00b7to", "fl\u00fcs\u00b7tern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "NE", "VVINF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Schwellende Wogen Ges\u00e4nge Tasso's.", "tokens": ["Schwel\u00b7len\u00b7de", "Wo\u00b7gen", "Ge\u00b7s\u00e4n\u00b7ge", "Tas\u00b7so'", "s."], "token_info": ["word", "word", "word", "word", "abbreviation"], "pos": ["ADJA", "NN", "NN", "NE", "NE"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Auch dort, wo Typhon's Zorn in den Kratern rollt,", "tokens": ["Auch", "dort", ",", "wo", "Ty\u00b7phon's", "Zorn", "in", "den", "Kra\u00b7tern", "rollt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "PWAV", "NE", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wo donnernd ausstr\u00f6mt Rauch und unendlich Gl\u00fchn,", "tokens": ["Wo", "don\u00b7nernd", "aus\u00b7str\u00f6mt", "Rauch", "und", "un\u00b7end\u00b7lich", "Gl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVPP", "NN", "KON", "ADJD", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Aus Asche bl\u00fcht auch dort der Weinstock \u2013", "tokens": ["Aus", "A\u00b7sche", "bl\u00fcht", "auch", "dort", "der", "Wein\u00b7stock", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ADV", "ADV", "ART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Siege, ja siege Ly\u00e4us, Sieger!", "tokens": ["Sie\u00b7ge", ",", "ja", "sie\u00b7ge", "Ly\u00b7\u00e4us", ",", "Sie\u00b7ger", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "ADV", "VVFIN", "NE", "$,", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Du k\u00fchn mit Panthern scherzender Genius,", "tokens": ["Du", "k\u00fchn", "mit", "Pan\u00b7thern", "scher\u00b7zen\u00b7der", "Ge\u00b7nius", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPR", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "O schreit' hervor aus deinem Gebirg, wo sp\u00e4t", "tokens": ["O", "schreit'", "her\u00b7vor", "aus", "dei\u00b7nem", "Ge\u00b7birg", ",", "wo", "sp\u00e4t"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "VVFIN", "PTKVZ", "APPR", "PPOSAT", "NN", "$,", "PWAV", "ADJD"], "meter": "-+-+-+--+-+", "measure": "ia