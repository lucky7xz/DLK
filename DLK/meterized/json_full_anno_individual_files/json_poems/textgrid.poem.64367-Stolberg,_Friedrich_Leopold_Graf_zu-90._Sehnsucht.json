{"textgrid.poem.64367": {"metadata": {"author": {"name": "Stolberg, Friedrich Leopold Graf zu", "birth": "N.A.", "death": "N.A."}, "title": "90. Sehnsucht", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fl\u00fcgel! wer giebt mir Fl\u00fcgel! da\u00df ich Ruhe", "tokens": ["Fl\u00fc\u00b7gel", "!", "wer", "giebt", "mir", "Fl\u00fc\u00b7gel", "!", "da\u00df", "ich", "Ru\u00b7he"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$.", "PWS", "VVFIN", "PPER", "NN", "$.", "KOUS", "PPER", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Finde? Fl\u00fcgel mir her! Des Liedes Fittich", "tokens": ["Fin\u00b7de", "?", "Fl\u00fc\u00b7gel", "mir", "her", "!", "Des", "Lie\u00b7des", "Fit\u00b7tich"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$.", "NN", "PPER", "PTKVZ", "$.", "ART", "NN", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Schweift in Banden, trunken, nicht frei, vergi\u00dft der", "tokens": ["Schweift", "in", "Ban\u00b7den", ",", "trun\u00b7ken", ",", "nicht", "frei", ",", "ver\u00b7gi\u00dft", "der"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "APPR", "NN", "$,", "ADJD", "$,", "PTKNEG", "ADJD", "$,", "VVFIN", "ART"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Dichter die Kette,", "tokens": ["Dich\u00b7ter", "die", "Ket\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Aber sie klirret selbst im Schwung geteilter", "tokens": ["A\u00b7ber", "sie", "klir\u00b7ret", "selbst", "im", "Schwung", "ge\u00