{"textgrid.poem.39679": {"metadata": {"author": {"name": "G\u00fcnderode, Karoline von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Dunkel ruhet auf den Wassern,", "genre": "verse", "period": "N.A.", "pub_year": 1793, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dunkel ruhet auf den Wassern,", "tokens": ["Dun\u00b7kel", "ru\u00b7het", "auf", "den", "Was\u00b7sern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tiefe Stille weit umher,", "tokens": ["Tie\u00b7fe", "Stil\u00b7le", "weit", "um\u00b7her", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Piedro's Schiff nur theilt die Wellen,", "tokens": ["Piedro's", "Schiff", "nur", "theilt", "die", "Wel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Seine Ruder schl\u00e4gt das Meer.", "tokens": ["Sei\u00b7ne", "Ru\u00b7der", "schl\u00e4gt", "das", "Meer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Aber Piedro steht am Maste", "tokens": ["A\u00b7ber", "Pie\u00b7dro", "steht", "am", "Mas\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NE", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und sein Aug' in tr\u00fcber Glut,", "tokens": ["Und", "sein", "Aug'", "in", "tr\u00fc\u00b7ber", "Glut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sucht den R\u00e4uber der Geliebten,", "tokens": ["Sucht", "den", "R\u00e4u\u00b7ber", "der", "Ge\u00b7lieb\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sucht sie durch des Meeres Fluth.", "tokens": ["Sucht", "sie", "durch", "des", "Mee\u00b7res", "Fluth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Endlich naht er ihrem Segel,", "tokens": ["End\u00b7lich", "naht", "er", "ih\u00b7rem", "Se\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Endlich geht die lange Nacht,", "tokens": ["End\u00b7lich", "geht", "die", "lan\u00b7ge", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und mit ungedult'ger Eile", "tokens": ["Und", "mit", "un\u00b7ge\u00b7dult'\u00b7ger", "Ei\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ordnet er der Schiffe Schlacht.", "tokens": ["Ord\u00b7net", "er", "der", "Schif\u00b7fe", "Schlacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Viele fallen, Viele siegen,", "tokens": ["Vie\u00b7le", "fal\u00b7len", ",", "Vie\u00b7le", "sie\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "VVINF", "$,", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Einer k\u00e4mpft mit L\u00f6wenmuth,", "tokens": ["Ei\u00b7ner", "k\u00e4mpft", "mit", "L\u00f6\u00b7wen\u00b7muth", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Naht sich Piedron durch die Menge", "tokens": ["Naht", "sich", "Pie\u00b7dron", "durch", "die", "Men\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PRF", "NE", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "K\u00fchnlich mit bescheidnem Muth.", "tokens": ["K\u00fchn\u00b7lich", "mit", "be\u00b7scheid\u00b7nem", "Muth", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Und sie k\u00e4mpfen, keiner weichet,", "tokens": ["Und", "sie", "k\u00e4mp\u00b7fen", ",", "kei\u00b7ner", "wei\u00b7chet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "VVINF", "$,", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tapferkeit wird wilde Wuth;", "tokens": ["Tap\u00b7fer\u00b7keit", "wird", "wil\u00b7de", "Wuth", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und in zornigen Str\u00f6men mischet", "tokens": ["Und", "in", "zor\u00b7ni\u00b7gen", "Str\u00f6\u00b7men", "mi\u00b7schet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN", "VVFIN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Sich der K\u00e4mpfer hei\u00dfes Blut.", "tokens": ["Sich", "der", "K\u00e4mp\u00b7fer", "hei\u00b7\u00dfes", "Blut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Endlich in des J\u00fcnglings Busen", "tokens": ["End\u00b7lich", "in", "des", "J\u00fcng\u00b7lings", "Bu\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Senket Piedro seinen Stahl,", "tokens": ["Sen\u00b7ket", "Pie\u00b7dro", "sei\u00b7nen", "Stahl", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Vor dem unwillkommenen Gaste", "tokens": ["Vor", "dem", "un\u00b7will\u00b7kom\u00b7me\u00b7nen", "Gas\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Flieht sein s\u00fc\u00dfes \u2013 Leben all.", "tokens": ["Flieht", "sein", "s\u00fc\u00b7\u00dfes", "\u2013", "Le\u00b7ben", "all", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "$(", "NN", "PIAT", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Und er stirbt so hold im Tode,", "tokens": ["Und", "er", "stirbt", "so", "hold", "im", "To\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "ADJD", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df Piedro niedersinkt,", "tokens": ["Da\u00df", "Pie\u00b7dro", "nie\u00b7der\u00b7sinkt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "NE", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und von seinen blassen Lippen", "tokens": ["Und", "von", "sei\u00b7nen", "blas\u00b7sen", "Lip\u00b7pen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Reuig hei\u00dfe K\u00fcsse trinkt.", "tokens": ["Reu\u00b7ig", "hei\u00b7\u00dfe", "K\u00fcs\u00b7se", "trinkt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Nacht will endlich niedersinken,", "tokens": ["Nacht", "will", "end\u00b7lich", "nie\u00b7der\u00b7sin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tiefe Stille weit umher;", "tokens": ["Tie\u00b7fe", "Stil\u00b7le", "weit", "um\u00b7her", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Piedro's Schiff nur theilt die Wellen,", "tokens": ["Piedro's", "Schiff", "nur", "theilt", "die", "Wel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Seine Ruder schl\u00e4gt das Meer.", "tokens": ["Sei\u00b7ne", "Ru\u00b7der", "schl\u00e4gt", "das", "Meer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Piedro aber liegt verwundet", "tokens": ["Pie\u00b7dro", "a\u00b7ber", "liegt", "ver\u00b7wun\u00b7det"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "ADV", "VVFIN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Einsam in des Schiffes Raum;", "tokens": ["Ein\u00b7sam", "in", "des", "Schif\u00b7fes", "Raum", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Seine Seele ist gefangen,", "tokens": ["Sei\u00b7ne", "See\u00b7le", "ist", "ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ganz und gar in einem Traum.", "tokens": ["Ganz", "und", "gar", "in", "ei\u00b7nem", "Traum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KON", "ADV", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Denn ihm d\u00e4ucht er sey umschlungen", "tokens": ["Denn", "ihm", "d\u00e4ucht", "er", "sey", "um\u00b7schlun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPER", "VAFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Von des todten J\u00fcnglings Arm,", "tokens": ["Von", "des", "tod\u00b7ten", "J\u00fcng\u00b7lings", "Arm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Freundlich will sein Auge brechen,", "tokens": ["Freund\u00b7lich", "will", "sein", "Au\u00b7ge", "bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Doch es schl\u00e4gt sein Herz noch warm.", "tokens": ["Doch", "es", "schl\u00e4gt", "sein", "Herz", "noch", "warm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "NN", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Piedro will sich von ihm rei\u00dfen,", "tokens": ["Pie\u00b7dro", "will", "sich", "von", "ihm", "rei\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PRF", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Doch mit sehnsuchtsvollem Blick", "tokens": ["Doch", "mit", "sehn\u00b7suchts\u00b7vol\u00b7lem", "Blick"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und mit hei\u00dfen Liebesk\u00fcssen", "tokens": ["Und", "mit", "hei\u00b7\u00dfen", "Lie\u00b7bes\u00b7k\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "H\u00e4lt der Knabe ihn zur\u00fcck.", "tokens": ["H\u00e4lt", "der", "Kna\u00b7be", "ihn", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Freudig, da\u00df er sie befreiet,", "tokens": ["Freu\u00b7dig", ",", "da\u00df", "er", "sie", "be\u00b7frei\u00b7et", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tritt die Braut zu Piedro hin,", "tokens": ["Tritt", "die", "Braut", "zu", "Pie\u00b7dro", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "NE", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Will ihn tr\u00f6sten, will versuchen,", "tokens": ["Will", "ihn", "tr\u00f6s\u00b7ten", ",", "will", "ver\u00b7su\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "$,", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ob die b\u00f6sen Tr\u00e4ume fliehn.", "tokens": ["Ob", "die", "b\u00f6\u00b7sen", "Tr\u00e4u\u00b7me", "fliehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Und sie neigt sich zu ihm nieder,", "tokens": ["Und", "sie", "neigt", "sich", "zu", "ihm", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PRF", "APPR", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ruft des Theuern Namen laut.", "tokens": ["Ruft", "des", "Theu\u00b7ern", "Na\u00b7men", "laut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Er erwacht und mit Entsetzen", "tokens": ["Er", "er\u00b7wacht", "und", "mit", "Ent\u00b7set\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wendet er sich von der Braut.", "tokens": ["Wen\u00b7det", "er", "sich", "von", "der", "Braut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "ART", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.14": {"line.1": {"text": "Und er mag sie nicht mehr schauen,", "tokens": ["Und", "er", "mag", "sie", "nicht", "mehr", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PPER", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ihre Liebe ist ihm Pein.", "tokens": ["Ih\u00b7re", "Lie\u00b7be", "ist", "ihm", "Pein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Tief versenkt nur im Betrachten", "tokens": ["Tief", "ver\u00b7senkt", "nur", "im", "Be\u00b7trach\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ADV", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Des Gestorbenen mag er seyn.", "tokens": ["Des", "Ge\u00b7stor\u00b7be\u00b7nen", "mag", "er", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VAINF", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.15": {"line.1": {"text": "Und das s\u00fc\u00dfe M\u00e4dchen weinet,", "tokens": ["Und", "das", "s\u00fc\u00b7\u00dfe", "M\u00e4d\u00b7chen", "wei\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sie verh\u00fcllt ihr Angesicht,", "tokens": ["Sie", "ver\u00b7h\u00fcllt", "ihr", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "M\u00f6chte gern vor Schmerzen ste