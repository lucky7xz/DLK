"}}, "stanza.32": {"line.1": {"text": "Und das s\u00fc\u00dfe M\u00e4dchen weinet,", "tokens": ["Und", "das", "s\u00fc\u00b7\u00dfe", "M\u00e4d\u00b7chen", "wei\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sie verh\u00fcllt ihr Angesicht,", "tokens": ["Sie", "ver\u00b7h\u00fcllt", "ihr", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "M\u00f6chte gern vor Schmerzen sterben,", "tokens": ["M\u00f6ch\u00b7te", "gern", "vor", "Schmer\u00b7zen", "ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nur den Theuern lassen nicht.", "tokens": ["Nur", "den", "Theu\u00b7ern", "las\u00b7sen", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.33": {"line.1": {"text": "Piedro siehts, ein tiefes Sehnen", "tokens": ["Pie\u00b7dro", "siehts", ",", "ein", "tie\u00b7fes", "Seh\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VVFIN", "$,", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zieht ihn nach des Grabes Ruh,", "tokens": ["Zieht", "ihn", "nach", "des", "Gra\u00b7bes", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Er zerrei\u00dft der Wunde Banden", "tokens": ["Er", "zer\u00b7rei\u00dft", "der", "Wun\u00b7de", "Ban\u0