{"dta.poem.21815": {"metadata": {"author": {"name": "Stieler, Kaspar von", "birth": "N.A.", "death": "N.A."}, "title": "ViI.  \n  Treugeliebt/ unbetr\u00fcbt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1660", "urn": "urn:nbn:de:kobv:b4-20586-2", "language": ["de:0.99"], "booktitle": "Filidor der Dorfferer [i. e. Stieler, Kaspar von]: Die Geharnschte Venus. Hamburg, 1660."}, "poem": {"stanza.1": {"line.1": {"text": "Es ist ein Ort in d\u00fcstrer Nacht/", "tokens": ["Es", "ist", "ein", "Ort", "in", "d\u00fcst\u00b7rer", "Nacht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "wo Pech und blauer Schwefel bren\u0303et/", "tokens": ["wo", "Pech", "und", "blau\u00b7er", "Schwe\u00b7fel", "bre\u00f1et", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "KON", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "de\u00df holer Schlund nie wird erkennet/", "tokens": ["de\u00df", "ho\u00b7ler", "Schlund", "nie", "wird", "er\u00b7ken\u00b7net", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VAFIN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "als wenn ein Blizz ihn hettermacht/", "tokens": ["als", "wenn", "ein", "Blizz", "ihn", "het\u00b7ter\u00b7macht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOKOM", "KOUS", "ART", "NN", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "mit Schlam\u0303 und schwarzen Wasserwogen", "tokens": ["mit", "Schlam\u0303", "und", "schwar\u00b7zen", "Was\u00b7ser\u00b7wo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "ist sein verfluchter Sizz umzogen.", "tokens": ["ist", "sein", "ver\u00b7fluch\u00b7ter", "Sizz", "um\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Megera denkt dar Martern au\u00df", "tokens": ["Me\u00b7ge\u00b7ra", "denkt", "dar", "Mar\u00b7tern", "au\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PTKVZ", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "mit ihren Schwestern/ denen Schlangen", "tokens": ["mit", "ih\u00b7ren", "Schwes\u00b7tern", "/", "de\u00b7nen", "Schlan\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$(", "PDS", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "um die vergifften Schl\u00e4ffe hangen.", "tokens": ["um", "die", "ver\u00b7giff\u00b7ten", "Schl\u00e4f\u00b7fe", "han\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dar ist die Grausamkeit zu Hau\u00df/", "tokens": ["Dar", "ist", "die", "Grau\u00b7sam\u00b7keit", "zu", "Hau\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "ART", "NN", "APPR", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "da wohnet Neid und Wiederwillen/", "tokens": ["da", "woh\u00b7net", "Neid", "und", "Wie\u00b7der\u00b7wil\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "man h\u00f6ret dar des Zerbers