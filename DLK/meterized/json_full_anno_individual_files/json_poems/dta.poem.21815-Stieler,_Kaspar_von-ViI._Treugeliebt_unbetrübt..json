oken_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Zu dieser H\u00f6len ist bestimmt/", "tokens": ["Zu", "die\u00b7ser", "H\u00f6\u00b7len", "ist", "be\u00b7stimmt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "wer mit der zarten Liebe spottet/", "tokens": ["wer", "mit", "der", "zar\u00b7ten", "Lie\u00b7be", "spot\u00b7tet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "wer gegen Amorn auff-sich-rottet/", "tokens": ["wer", "ge\u00b7gen", "A\u00b7morn", "auf\u00b7fsich\u00b7rot\u00b7tet", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "NE", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "und wieder Venus/ Waffen nimt/", "tokens": ["und", "wie\u00b7der", "Ve\u00b7nus", "/", "Waf\u00b7fen", "nimt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "NN", "$(", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "treibt mit Verliebten/ Scherz und Possen:", "tokens": ["treibt", "mit", "Ver\u00b7lieb\u00b7ten", "/", "Scherz", "und", "Pos\u00b7sen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$(", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "wird hier in Ketten eingeschlossen.", "tokens": ["wird", "hier", "in", "Ket\u00b7ten", "ein\u00b7ge\u00b7schlos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Hergegen ist ein gr\u00fcnes Tahl", "tokens": ["Her\u00b7ge\u00b7gen", "ist", "ein", "gr\u00fc\u00b7nes", "Tahl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "wo die bebl\u00fchmten Weste k\u00fchlen.", "tokens": ["wo", "die", "be\u00b7bl\u00fchm\u00b7ten", "Wes\u00b7te", "k\u00fch\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Hier h\u00f6ret man von Seiten-spielen", "tokens": ["Hier", "h\u00f6\u00b7ret", "man", "von", "Sei\u00b7ten\u00b7spie\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "von Lust und Freuden ohne Zahl.", "tokens": ["von", "Lust", "und", "Freu\u00b7den", "oh\u00b7ne", "Zahl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "die Felder bl\u00fchn in bunten Nelken", "tokens": ["die", "Fel\u00b7der", "bl\u00fchn", "in", "bun\u00b7ten", "Nel\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "und Rosen/ welche nie verwelken.", "tokens": ["und", "Ro\u00b7sen", "/", "wel\u00b7che", "nie", "ver\u00b7wel\u00b7ken", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$(", "PRELS", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Hier wehet eine Zimmet-Lufft/", "tokens": ["Hier", "we\u00b7het", "ei\u00b7ne", "Zim\u00b7met\u00b7Lufft", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "man h\u00f6ret dar ohn Ende schallen", "tokens": ["man", "h\u00f6\u00b7ret", "dar", "ohn", "En\u00b7de", "schal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PTKVZ", "APPR", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "den Schlag der muntern Nachtigallen/", "tokens": ["den", "Schlag", "der", "mun\u00b7tern", "Nach\u00b7ti\u00b7gal\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "hier ist kein Frost/ kein Nebel-dufft/", "tokens": ["hier", "ist", "kein", "Frost", "/", "kein", "Ne\u00b7bel\u00b7dufft", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIAT", "NN", "$(", "PIAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "kein Blizz/ kein Donnerschlag noch Re-", "tokens": ["kein", "Blizz", "/", "kein", "Don\u00b7ner\u00b7schlag", "noch", "Re"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "$(", "PIAT", "NN", "ADV", "TRUNC"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "zieht schwarzen Wolken hier entgegen.", "tokens": ["zieht", "schwar\u00b7zen", "Wol\u00b7ken", "hier", "ent\u00b7ge\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Hier ist ein milder Liebes-streit/", "tokens": ["Hier", "ist", "ein", "mil\u00b7der", "Lie\u00b7bes\u00b7streit", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "das junge Volk spielt mit Jungfrauen", "tokens": ["das", "jun\u00b7ge", "Volk", "spielt", "mit", "Jung\u00b7frau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "auff Elis bunten silber-auen.", "tokens": ["auff", "E\u00b7lis", "bun\u00b7ten", "sil\u00b7ber\u00b7au\u00b7en", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Scherz/ Liebe/ Lust und Fr\u00f6ligkeit", "tokens": ["Scherz", "/", "Lie\u00b7be", "/", "Lust", "und", "Fr\u00f6\u00b7lig\u00b7keit"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$(", "NN", "$(", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Vergn\u00fcgung/ Ruh und s\u00fcsses Lachen", "tokens": ["Ver\u00b7gn\u00fc\u00b7gung", "/", "Ruh", "und", "s\u00fcs\u00b7ses", "La\u00b7chen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$(", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "verk\u00fcrzt ihr unauffh\u00f6rlichs Wachen.", "tokens": ["ver\u00b7k\u00fcrzt", "ihr", "un\u00b7auff\u00b7h\u00f6r\u00b7lichs", "Wa\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Wol dehm/ der sich der Lieb\u2019 ergiebt!", "tokens": ["Wol", "dehm", "/", "der", "sich", "der", "Lieb'", "er\u00b7giebt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$(", "PRELS", "PRF", "ART", "NN", "VVFIN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "der wird bekr\u00f6nt mit Myrten-kr\u00e4nzen", "tokens": ["der", "wird", "be\u00b7kr\u00f6nt", "mit", "Myr\u00b7ten\u00b7kr\u00e4n\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "VAFIN", "ADJD", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "geniessen dieses steten Lenzen.", "tokens": ["ge\u00b7nies\u00b7sen", "die\u00b7ses", "ste\u00b7ten", "Len\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wol dehm/ der keusch und treulich liebt!", "tokens": ["Wol", "dehm", "/", "der", "keusch", "und", "treu\u00b7lich", "liebt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$(", "ART", "ADJD", "KON", "ADJD", "VVFIN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Jhn wird mit Sieg/ Triumff und singen", "tokens": ["Jhn", "wird", "mit", "Sieg", "/", "Tri\u00b7umff", "und", "sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "NN", "$(", "NN", "KON", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "der bleiche Charon \u00fcberbringen.", "tokens": ["der", "blei\u00b7che", "Cha\u00b7ron", "\u00fc\u00b7berb\u00b7rin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}