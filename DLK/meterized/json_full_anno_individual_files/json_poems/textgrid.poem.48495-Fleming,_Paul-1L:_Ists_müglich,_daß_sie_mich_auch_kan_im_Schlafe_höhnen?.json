ht gestehen.", "tokens": ["Sag'", "ichs", "ihr", "o\u00b7der", "nicht", "?", "Sie", "wirds", "doch", "nicht", "ge\u00b7ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIS", "PPOSAT", "KON", "PTKNEG", "$.", "PPER", "VAFIN", "ADV", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Wer, o wer wird mich denn entnehmen dieser Last?", "tokens": ["Wer", ",", "o", "wer", "wird", "mich", "denn", "ent\u00b7neh\u00b7men", "die\u00b7ser", "Last", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "$,", "FM", "PWS", "VAFIN", "PPER", "ADV", "VVFIN", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Ach, Schwester, f\u00fchlst du nicht, da\u00df du zwo Seelen hast?", "tokens": ["Ach", ",", "Schwes\u00b7ter", ",", "f\u00fchlst", "du", "nicht", ",", "da\u00df", "du", "zwo", "See\u00b7len", "hast", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "NN", "$,", "VVFIN", "PPER", "PTKNEG", "$,", "KOUS", "PPER", "CARD", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}