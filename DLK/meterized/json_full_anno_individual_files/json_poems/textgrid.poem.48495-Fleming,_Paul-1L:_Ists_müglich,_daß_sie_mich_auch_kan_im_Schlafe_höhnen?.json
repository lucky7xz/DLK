{"textgrid.poem.48495": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ists m\u00fcglich, da\u00df sie mich auch kan im Schlafe h\u00f6hnen?", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ists m\u00fcglich, da\u00df sie mich auch kan im Schlafe h\u00f6hnen?", "tokens": ["Ists", "m\u00fcg\u00b7lich", ",", "da\u00df", "sie", "mich", "auch", "kan", "im", "Schla\u00b7fe", "h\u00f6h\u00b7nen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "$,", "KOUS", "PPER", "PRF", "ADV", "VMFIN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wars noch nicht gnung, da\u00df ich mich wachend nach ihr sehnen", "tokens": ["Wars", "noch", "nicht", "gnung", ",", "da\u00df", "ich", "mich", "wa\u00b7chend", "nach", "ihr", "seh\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "PTKNEG", "ADV", "$,", "KOUS", "PPER", "PRF", "ADJD", "APPR", "PPER", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "und so bek\u00fcmmern mu\u00df, im Fall' sie nicht ist hier?", "tokens": ["und", "so", "be\u00b7k\u00fcm\u00b7mern", "mu\u00df", ",", "im", "Fall'", "sie", "nicht", "ist", "hier", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVINF", "VMFIN", "$,", "APPRART", "NN", "PPER", "PTKNEG", "VAFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Doch sie ist au\u00dfer Schuld. Du, Morpheu, machtest dir", "tokens": ["Doch", "sie", "ist", "au\u00b7\u00dfer", "Schuld", ".", "Du", ",", "Mor\u00b7pheu", ",", "mach\u00b7test", "dir"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "APPR", "NN", "$.", "PPER", "$,", "NN", "$,", "VVFIN", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "aus mir ein leichtes Spiel! Der alte Schalk, der liefe,", "tokens": ["aus", "mir", "ein", "leich\u00b7tes", "Spiel", "!", "Der", "al\u00b7te", "Schalk", ",", "der", "lie\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "ADJA", "NN", "$.", "ART", "ADJA", "NN", "$,", "PRELS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "indem ich, gleich wie sie, frei aller Sorgen schliefe.", "tokens": ["in\u00b7dem", "ich", ",", "gleich", "wie", "sie", ",", "frei", "al\u00b7ler", "Sor\u00b7gen", "schlie\u00b7fe", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "ADV", "KOKOM", "PPER", "$,", "ADJD", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Er dr\u00fcckt' ihr sch\u00f6nes Bild in einen Schatten ab", "tokens": ["Er", "dr\u00fcckt'", "ihr", "sch\u00f6\u00b7nes", "Bild", "in", "ei\u00b7nen", "Schat\u00b7ten", "ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "APPR", "ART", "NN", "PTKVZ"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "und bracht' es mir so vor. Die liebe Sch\u00f6nheit gab", "tokens": ["und", "bracht'", "es", "mir", "so", "vor", ".", "Die", "lie\u00b7be", "Sch\u00f6n\u00b7heit", "gab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PPER", "ADV", "PTKVZ", "$.", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "der Seelen ihren Geist. Sie fingen sich zu lieben,", "tokens": ["der", "See\u00b7len", "ih\u00b7ren", "Geist", ".", "Sie", "fin\u00b7gen", "sich", "zu", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$.", "PPER", "VVFIN", "PRF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "zu sehn, zu k\u00fcssen an. Die s\u00fc\u00dfen Freunde trieben", "tokens": ["zu", "sehn", ",", "zu", "k\u00fcs\u00b7sen", "an", ".", "Die", "s\u00fc\u00b7\u00dfen", "Freun\u00b7de", "trie\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "$,", "PTKZU", "VVINF", "PTKVZ", "$.", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "ihr sch\u00f6nes Tun mit sich so herzlich und so viel,", "tokens": ["ihr", "sch\u00f6\u00b7nes", "Tun", "mit", "sich", "so", "herz\u00b7lich", "und", "so", "viel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "PRF", "ADV", "ADJD", "KON", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "bis da\u00df, indem der Geist noch hat sein Liebesspiel", "tokens": ["bis", "da\u00df", ",", "in\u00b7dem", "der", "Geist", "noch", "hat", "sein", "Lie\u00b7bes\u00b7spiel"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "KOUS", "$,", "KOUS", "ART", "NN", "ADV", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "und in dem Schatten scherzt, mein matter Leib erwachet.", "tokens": ["und", "in", "dem", "Schat\u00b7ten", "scherzt", ",", "mein", "mat\u00b7ter", "Leib", "er\u00b7wa\u00b7chet", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "$,", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Das Bild, in dem er sich noch so ergetzlich machet,", "tokens": ["Das", "Bild", ",", "in", "dem", "er", "sich", "noch", "so", "er\u00b7getz\u00b7lich", "ma\u00b7chet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "PRELS", "PPER", "PRF", "ADV", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "fleugt ganz mit ihm darvon und kehrt an seinen Ort.", "tokens": ["fleugt", "ganz", "mit", "ihm", "dar\u00b7von", "und", "kehrt", "an", "sei\u00b7nen", "Ort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PPER", "PAV", "KON", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Was tu' ich Armer nun? Die Seele, die ist fort,", "tokens": ["Was", "tu'", "ich", "Ar\u00b7mer", "nun", "?", "Die", "See\u00b7le", ",", "die", "ist", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "NN", "ADV", "$.", "ART", "NN", "$,", "PRELS", "VAFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "mein Leib lebt auf den Schein. Wie wird mirs doch noch gehen?", "tokens": ["mein", "Leib", "lebt", "auf", "den", "Schein", ".", "Wie", "wird", "mirs", "doch", "noch", "ge\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ART", "NN", "$.", "PWAV", "VAFIN", "NE", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Sag' ichs ihr oder nicht? Sie wirds doch nicht gestehen.", "tokens": ["Sag'", "ichs", "ihr", "o\u00b7der", "nicht", "?", "Sie", "wirds", "doch", "nicht", "ge\u00b7ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIS", "PPOSAT", "KON", "PTKNEG", "$.", "PPER", "VAFIN", "ADV", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Wer, o wer wird mich denn entnehmen dieser Last?", "tokens": ["Wer", ",", "o", "wer", "wird", "mich", "denn", "ent\u00b7neh\u00b7men", "die\u00b7ser", "Last", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "$,", "FM", "PWS", "VAFIN", "PPER", "ADV", "VVFIN", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Ach, Schwester, f\u00fchlst du nicht, da\u00df du zwo Seelen hast?", "tokens": ["Ach", ",", "Schwes\u00b7ter", ",", "f\u00fchlst", "du", "nicht", ",", "da\u00df", "du", "zwo", "See\u00b7len", "hast", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "NN", "$,", "VVFIN", "PPER", "PTKNEG", "$,", "KOUS", "PPER", "CARD", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ists m\u00fcglich, da\u00df sie mich auch kan im Schlafe h\u00f6hnen?", "tokens": ["Ists", "m\u00fc