{"textgrid.poem.27822": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "37.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich w\u00e4re gerne reich; denn da\u00df ich reich nicht bin,", "tokens": ["Ich", "w\u00e4\u00b7re", "ger\u00b7ne", "reich", ";", "denn", "da\u00df", "ich", "reich", "nicht", "bin", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$.", "KON", "KOUS", "PPER", "ADJD", "PTKNEG", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Drum wil man mich dazu noch zu der Strafe ziehn.", "tokens": ["Drum", "wil", "man", "mich", "da\u00b7zu", "noch", "zu", "der", "Stra\u00b7fe", "ziehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PIS", "PRF", "PAV", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich w\u00e4re gerne reich; wer arm mich nicht kan leiden,", "tokens": ["Ich", "w\u00e4\u00b7re", "ger\u00b7ne", "reich", ";", "wer", "arm", "mich", "nicht", "kan", "lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$.", "PWS", "ADJD", "PPER", "PTKNEG", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der mag mir tausend Pfund und noch so viel bescheiden.", "tokens": ["Der", "mag", "mir", "tau\u00b7send", "Pfund", "und", "noch", "so", "viel", "be\u00b7schei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "CARD", "NN", "KON", "ADV", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich hab ein Ungrisch Gold nicht Ungern im Beschlu\u00df;", "tokens": ["Ich", "hab", "ein", "Un\u00b7grisch", "Gold", "nicht", "Un\u00b7gern", "im", "Be\u00b7schlu\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "NN", "PTKNEG", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Nicht haben, haben nicht, das bringet mir Verdru\u00df.", "tokens": ["Nicht", "ha\u00b7ben", ",", "ha\u00b7ben", "nicht", ",", "das", "brin\u00b7get", "mir", "Ver\u00b7dru\u00df", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VAINF", "$,", "VAFIN", "PTKNEG", "$,", "PDS", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wer Gold nicht geben wil, der mag mir Silber geben;", "tokens": ["Wer", "Gold", "nicht", "ge\u00b7ben", "wil", ",", "der", "mag", "mir", "Sil\u00b7ber", "ge\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "PTKNEG", "VVINF", "VMFIN", "$,", "PRELS", "VMFIN", "PPER", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Das Silber nehm ich auch. Ich wil gar friedlich leben", "tokens": ["Das", "Sil\u00b7ber", "nehm", "ich", "auch", ".", "Ich", "wil", "gar", "fried\u00b7lich", "le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "$.", "PPER", "VMFIN", "ADV", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, 