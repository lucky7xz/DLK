, "liegt", "mein", "In\u00b7ne\u00b7res", "ver\u00b7schmacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Indem das Aeussere vermodert.", "tokens": ["In\u00b7dem", "das", "A\u00b7e\u00b7us\u00b7se\u00b7re", "ver\u00b7mo\u00b7dert", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Ich z\u00f6ge gerne noch", "tokens": ["Ich", "z\u00f6\u00b7ge", "ger\u00b7ne", "noch"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Ein Jahr an Christi Joch,", "tokens": ["Ein", "Jahr", "an", "Chris\u00b7ti", "Joch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Ich komme langsam; Mag ich kommen?", "tokens": ["Ich", "kom\u00b7me", "lang\u00b7sam", ";", "Mag", "ich", "kom\u00b7men", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$.", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Der Eingang zeiget sich,", "tokens": ["Der", "Ein\u00b7gang", "zei\u00b7get", "sich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Ein Blick versichert mich,", "tokens": ["Ein", "Blick", "ver\u00b7si\u00b7chert", "mich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.14": {"text": "Komm. ", "tokens": ["Kom\u00b7m."], "token_info":