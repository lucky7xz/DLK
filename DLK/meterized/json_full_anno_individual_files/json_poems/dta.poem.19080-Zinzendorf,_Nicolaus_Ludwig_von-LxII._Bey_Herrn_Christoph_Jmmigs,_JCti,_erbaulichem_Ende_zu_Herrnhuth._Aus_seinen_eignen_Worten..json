{"dta.poem.19080": {"metadata": {"author": {"name": "Zinzendorf, Nicolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "LxII.  Bey Herrn Christoph Jmmigs,  JCti,  \n erbaulichem Ende zu Herrnhuth.   Aus seinen eignen Worten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20688-6", "language": ["de:0.99"], "booktitle": "Zinzendorf, Nicolaus Ludwig von: Teutscher Gedichte Erster Theil. Herrnhuth, 1735."}, "poem": {"stanza.1": {"line.1": {"text": "Du heiliger und reiner Geist,", "tokens": ["Du", "hei\u00b7li\u00b7ger", "und", "rei\u00b7ner", "Geist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Geist, darnach nicht Noth zu fragen,", "tokens": ["Ein", "Geist", ",", "dar\u00b7nach", "nicht", "Noth", "zu", "fra\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PAV", "PTKNEG", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Indem er sich genug beweist,", "tokens": ["In\u00b7dem", "er", "sich", "ge\u00b7nug", "be\u00b7weist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du Alter ausser allen Tagen!", "tokens": ["Du", "Al\u00b7ter", "aus\u00b7ser", "al\u00b7len", "Ta\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Allgegenwart, Allwissenheit,", "tokens": ["All\u00b7ge\u00b7gen\u00b7wart", ",", "All\u00b7wis\u00b7sen\u00b7heit", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$,", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Sind deiner Gottheit Eigenschaften,", "tokens": ["Sind", "dei\u00b7ner", "Got\u00b7theit", "Ei\u00b7gen\u00b7schaf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und Zeugen deiner Ewigkeit.", "tokens": ["Und", "Zeu\u00b7gen", "dei\u00b7ner", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die unzertrennlich an dir haften.", "tokens": ["Die", "un\u00b7zer\u00b7trenn\u00b7lich", "an", "dir", "haf\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Du sitzest in der Ruh,", "tokens": ["Du", "sit\u00b7zest", "in", "der", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Und h\u00f6rst den Bl\u00f6den zu,", "tokens": ["Und", "h\u00f6rst", "den", "Bl\u00f6\u00b7den", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Die vor dem Thron der Gnade wimmern:", "tokens": ["Die", "vor", "dem", "Thron", "der", "Gna\u00b7de", "wim\u00b7mern", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Hier liegt ein altes Kind,", "tokens": ["Hier", "liegt", "ein", "al\u00b7tes", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Das erst sein Hertze findt,", "tokens": ["Das", "erst", "sein", "Hert\u00b7ze", "findt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "VAINF", "VVFIN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.14": {"text": "Und will sich um sein Heyl bek\u00fcmmern.", "tokens": ["Und", "will", "sich", "um", "sein", "Heyl", "be\u00b7k\u00fcm\u00b7mern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PRF", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich zehle eilfmal sieben Jahr", "tokens": ["Ich", "zeh\u00b7le", "eilf\u00b7mal", "sie\u00b7ben", "Jahr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "CARD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In dieser unbest\u00e4ndgen H\u00fctte.", "tokens": ["In", "die\u00b7ser", "un\u00b7be\u00b7st\u00e4nd\u00b7gen", "H\u00fct\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Was meine gr\u00f6ste Sorge war,", "tokens": ["Was", "mei\u00b7ne", "gr\u00f6s\u00b7te", "Sor\u00b7ge", "war", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Zweck, wornach ich hellig schritte,", "tokens": ["Der", "Zweck", ",", "wor\u00b7nach", "ich", "hel\u00b7lig", "schrit\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Den heisset man ", "tokens": ["Den", "heis\u00b7set", "man"], "token_info": ["word", "word", "word"], "pos": ["NE", "VVFIN", "PIS"], "meter": "-+--", "measure": "dactylic.init"}, "line.6": {"text": "Das nennt man ein solides Wissen:", "tokens": ["Das", "nennt", "man", "ein", "so\u00b7li\u00b7des", "Wis\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PIS", "ART", "PIAT", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.7": {"text": "Ich sorgete vor Spei\u00df und Kleid,", "tokens": ["Ich", "sor\u00b7ge\u00b7te", "vor", "Spei\u00df", "und", "Kleid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "F\u00fcr mich, und die ich n\u00e4hren m\u00fcssen.", "tokens": ["F\u00fcr", "mich", ",", "und", "die", "ich", "n\u00e4h\u00b7ren", "m\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "KON", "PRELS", "PPER", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Jtzt bin ich Gnaden-lo\u00df,", "tokens": ["Jtzt", "bin", "ich", "Gna\u00b7den\u00b7lo\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Am Geiste blind und blo\u00df:", "tokens": ["Am", "Geis\u00b7te", "blind", "und", "blo\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "KON", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Mein Dienst wars opus operatum.", "tokens": ["Mein", "Dienst", "wars", "o\u00b7pus", "o\u00b7pe\u00b7ra\u00b7tum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "FM", "FM", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Die Tauf ist l\u00e4ngst vorbey,", "tokens": ["Die", "Tauf", "ist", "l\u00e4ngst", "vor\u00b7bey", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Der Gnaden-Bund entzwey;", "tokens": ["Der", "Gna\u00b7den\u00b7Bund", "ent\u00b7zwey", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.14": {"text": "Mit Schrecken wart ich auf mein Fatum.", "tokens": ["Mit", "Schre\u00b7cken", "wart", "ich", "auf", "mein", "Fa\u00b7tum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Der gute Saame liegt erstickt,", "tokens": ["Der", "gu\u00b7te", "Saa\u00b7me", "liegt", "er\u00b7stickt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Weil ihn die D\u00f6rner \u00fcberwachsen;", "tokens": ["Weil", "ihn", "die", "D\u00f6r\u00b7ner", "\u00fc\u00b7ber\u00b7wach\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und eh ich weiter fortger\u00fcckt,", "tokens": ["Und", "eh", "ich", "wei\u00b7ter", "fort\u00b7ge\u00b7r\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zerbrechen meines Leibes Achsen.", "tokens": ["Zer\u00b7bre\u00b7chen", "mei\u00b7nes", "Lei\u00b7bes", "Ach\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ich sehe mich in meinem Blut,", "tokens": ["Ich", "se\u00b7he", "mich", "in", "mei\u00b7nem", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ich wei\u00df mich selber nicht zu waschen:", "tokens": ["Ich", "wei\u00df", "mich", "sel\u00b7ber", "nicht", "zu", "wa\u00b7schen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Dar\u00fcber f\u00e4llt mir Hertz und Muth.", "tokens": ["Da\u00b7r\u00fc\u00b7ber", "f\u00e4llt", "mir", "Hertz", "und", "Muth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der letzte Feind wird mich erhaschen!", "tokens": ["Der", "letz\u00b7te", "Feind", "wird", "mich", "er\u00b7ha\u00b7schen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Du aber, dem der Tod", "tokens": ["Du", "a\u00b7ber", ",", "dem", "der", "Tod"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ADV", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Des S\u00fcnders eine Noth,", "tokens": ["Des", "S\u00fcn\u00b7ders", "ei\u00b7ne", "Noth", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Und seine Rettung eine Freude,", "tokens": ["Und", "sei\u00b7ne", "Ret\u00b7tung", "ei\u00b7ne", "Freu\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Ach! schau mich armen Mann", "tokens": ["Ach", "!", "schau", "mich", "ar\u00b7men", "Mann"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "VVFIN", "PPER", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Mit Gnaden-Augen an,", "tokens": ["Mit", "Gna\u00b7den\u00b7Au\u00b7gen", "an", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.14": {"text": "Und st\u00fctze mein zerlechtzt Geb\u00e4ude!", "tokens": ["Und", "st\u00fct\u00b7ze", "mein", "zer\u00b7lechtzt", "Ge\u00b7b\u00e4u\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "VVPP", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ach HErr! du Majest\u00e4tischer,", "tokens": ["Ach", "Herr", "!", "du", "Ma\u00b7jes\u00b7t\u00e4\u00b7ti\u00b7scher", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "NN", "$.", "PPER", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du schrecklicher und grosser K\u00f6nig;", "tokens": ["Du", "schreck\u00b7li\u00b7cher", "und", "gros\u00b7ser", "K\u00f6\u00b7nig", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Du aber auch so freundlicher,", "tokens": ["Du", "a\u00b7ber", "auch", "so", "freund\u00b7li\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.4": {"text": "Dem eine Seele nicht zu wenig;", "tokens": ["Dem", "ei\u00b7ne", "See\u00b7le", "nicht", "zu", "we\u00b7nig", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "PTKNEG", "PTKA", "PIS", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "La\u00df mich durch deinen lieben Sohn", "tokens": ["La\u00df", "mich", "durch", "dei\u00b7nen", "lie\u00b7ben", "Sohn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die ewige Erl\u00f6sung finden:", "tokens": ["Die", "e\u00b7wi\u00b7ge", "Er\u00b7l\u00f6\u00b7sung", "fin\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "In ihm, dem wahren Gnaden-Thron,", "tokens": ["In", "ihm", ",", "dem", "wah\u00b7ren", "Gna\u00b7den\u00b7Thron", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "La\u00df mich den Hofnungs-Ancker gr\u00fcnden.", "tokens": ["La\u00df", "mich", "den", "Hof\u00b7nungs\u00b7An\u00b7cker", "gr\u00fcn\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Weil aber JEsu Blut", "tokens": ["Weil", "a\u00b7ber", "Je\u00b7su", "Blut"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ADV", "NE", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Nur denen H\u00fclffe thut,", "tokens": ["Nur", "de\u00b7nen", "H\u00fclf\u00b7fe", "thut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PDS", "NN", "VVFIN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.11": {"text": "Die in dem Licht, wie er ist, wandeln:", "tokens": ["Die", "in", "dem", "Licht", ",", "wie", "er", "ist", ",", "wan\u00b7deln", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "$,", "PWAV", "PPER", "VAFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "So schencke mir doch nur", "tokens": ["So", "schen\u00b7cke", "mir", "doch", "nur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Die neue Creatur,", "tokens": ["Die", "neu\u00b7e", "Crea\u00b7tur", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.14": {"text": "Denn, womit wolt ich sie erhandeln?", "tokens": ["Denn", ",", "wo\u00b7mit", "wolt", "ich", "sie", "er\u00b7han\u00b7deln", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWAV", "VMFIN", "PPER", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Mein JEsu! wer zum Vater will,", "tokens": ["Mein", "Je\u00b7su", "!", "wer", "zum", "Va\u00b7ter", "will", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NE", "$.", "PWS", "APPRART", "NN", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der mu\u00df durch dich den Eingang finden:", "tokens": ["Der", "mu\u00df", "durch", "dich", "den", "Ein\u00b7gang", "fin\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "APPR", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "In dir ist alle GOttes-F\u00fcll;", "tokens": ["In", "dir", "ist", "al\u00b7le", "Got\u00b7tes\u00b7F\u00fcll", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du machest selig von den S\u00fcnden.", "tokens": ["Du", "ma\u00b7chest", "se\u00b7lig", "von", "den", "S\u00fcn\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Hier lieg ich armer matter Wurm,", "tokens": ["Hier", "lieg", "ich", "ar\u00b7mer", "mat\u00b7ter", "Wurm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und winde mich um deine Wiege:", "tokens": ["Und", "win\u00b7de", "mich", "um", "dei\u00b7ne", "Wie\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Ich f\u00fchle Seelen-Noth und Sturm,", "tokens": ["Ich", "f\u00fch\u00b7le", "See\u00b7len\u00b7Noth", "und", "Sturm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Doch merck ich auch noch Liebes-Z\u00fcge", "tokens": ["Doch", "merck", "ich", "auch", "noch", "L