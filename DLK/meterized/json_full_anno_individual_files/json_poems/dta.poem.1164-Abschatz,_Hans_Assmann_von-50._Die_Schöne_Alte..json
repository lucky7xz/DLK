{"dta.poem.1164": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "50. Die Sch\u00f6ne Alte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Was deiner Jahre Lentz nicht hat vermocht zu fangen/ ", "tokens": ["Was", "dei\u00b7ner", "Jah\u00b7re", "Lentz", "nicht", "hat", "ver\u00b7mocht", "zu", "fan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "NE", "PTKNEG", "VAFIN", "VVPP", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Steckt deines Herbstes Blitz mit hei\u00dfen Flammen an.", "tokens": ["Steckt", "dei\u00b7nes", "Herbs\u00b7tes", "Blitz", "mit", "hei\u00b7\u00dfen", "Flam\u00b7men", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Alten Sch\u00f6nheit Glantz/ die nicht ersterben kan/", "tokens": ["Der", "Al\u00b7ten", "Sch\u00f6n\u00b7heit", "Glantz", "/", "die", "nicht", "ers\u00b7ter\u00b7ben", "kan", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$(", "PRELS", "PTKNEG", "VVINF", "VMFIN", "$("], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Hat durch die Zeit mehr Macht und neuen Trieb empfangen.", "tokens": ["Hat", "durch", "die", "Zeit", "mehr", "Macht", "und", "neu\u00b7en", "Trieb", "emp\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "PIAT", "NN", "KON", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Man erndtet reiffe Treu\u2019 in Furchen deiner Wangen/", "tokens": ["Man", "ernd\u00b7tet", "reif\u00b7fe", "Treu'", "in", "Fur\u00b7chen", "dei\u00b7ner", "Wan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADJA", "NN", "APPR", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der Jugend Wanckelmutt steht hier auff fester Bahn:", "tokens": ["Der", "Ju\u00b7gend", "Wan\u00b7ckel\u00b7mutt", "steht", "hier", "auff", "fes\u00b7ter", "Bahn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ob Brust/ ob Haare wei\u00df/ beruht in blossem Wahn.", "tokens": ["Ob", "Brust", "/", "ob", "Haa\u00b7re", "wei\u00df", "/", "be\u00b7ruht", "in", "blos\u00b7sem", "Wahn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$(", "KOUS", "NN", "VVFIN", "$(", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die Liljen sieht man auch mit wei\u00dfer Krone prangen.", "tokens": ["Die", "Lil\u00b7jen", "sieht", "man", "auch", "mit", "wei\u00b7\u00dfer", "Kro\u00b7ne", "pran\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIS", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Brennt wohl ein gr\u00fcnes Holtz? die Eiche/ wenn sie alt/", "tokens": ["Brennt", "wohl", "ein", "gr\u00fc\u00b7nes", "Holtz", "?", "die", "Ei\u00b7che", "/", "wenn", "sie", "alt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "$.", "ART", "NN", "$(", "KOUS", "PPER", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ist f\u00fcr die Sonn ein Schild/ ein Schirm f\u00fcr rauhe Winde.", "tokens": ["Ist", "f\u00fcr", "die", "Sonn", "ein", "Schild", "/", "ein", "Schirm", "f\u00fcr", "rau\u00b7he", "Win\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "ART", "NN", "$(", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und ihre Zweige seyn der V\u00f6gel Auffenthalt.", "tokens": ["Und", "ih\u00b7re", "Zwei\u00b7ge", "seyn", "der", "V\u00f6\u00b7gel", "Auf\u00b7fent\u00b7halt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PPOSAT", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Du/ die ich gantz hierinn Cybelen \u00e4hnlich finde/", "tokens": ["Du", "/", "die", "ich", "gantz", "hie\u00b7rinn", "Cy\u00b7be\u00b7len", "\u00e4hn\u00b7lich", "fin\u00b7de", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$(", "PRELS", "PPER", "ADV", "ADV", "VVFIN", "ADJD", "VVFIN", "$("], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "H\u00e4ltst in verborgner Flamm ein ewig Feur bereit:", "tokens": ["H\u00e4ltst", "in", "ver\u00b7borg\u00b7ner", "Flamm", "ein", "e\u00b7wig", "Feur", "be\u00b7reit", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "ART", "ADJD", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Cupid\u2019/ ein Kind zu erst/ ist Riese mit der Zeit.", "tokens": ["Cu\u00b7pid'", "/", "ein", "Kind", "zu", "erst", "/", "ist", "Rie\u00b7se", "mit", "der", "Zeit", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "ART", "NN", "APPR", "ADV", "$(", "VAFIN", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}