{"dta.poem.15926": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "131.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie ist die Autorschaft ein dorniger Beruf", "tokens": ["Wie", "ist", "die", "Au\u00b7tor\u00b7schaft", "ein", "dor\u00b7ni\u00b7ger", "Be\u00b7ruf"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "F\u00fcr einen, dessen Herz wie meines Gott erschuf!", "tokens": ["F\u00fcr", "ei\u00b7nen", ",", "des\u00b7sen", "Herz", "wie", "mei\u00b7nes", "Gott", "er\u00b7schuf", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "PRELAT", "NN", "KOKOM", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Mag doch ein anderer f\u00fcr Andre Rosen brechen,", "tokens": ["Mag", "doch", "ein", "an\u00b7de\u00b7rer", "f\u00fcr", "And\u00b7re", "Ro\u00b7sen", "bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "ADJA", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dem auch die Rosen bl\u00fchn, nicht blos die Dorne stechen!", "tokens": ["Dem", "auch", "die", "Ro\u00b7sen", "bl\u00fchn", ",", "nicht", "blos", "die", "Dor\u00b7ne", "ste\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ART", "NN", "VVINF", "$,", "PTKNEG", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Kaum freut mich, was dabei erfreulichs unterl\u00e4uft,", "tokens": ["Kaum", "freut", "mich", ",", "was", "da\u00b7bei", "er\u00b7freu\u00b7lichs", "un\u00b7ter\u00b7l\u00e4uft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PRELS", "PAV", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und alle Kr\u00e4nkung ist aufs kranke Herz geh\u00e4uft.", "tokens": ["Und", "al\u00b7le", "Kr\u00e4n\u00b7kung", "ist", "aufs", "kran\u00b7ke", "Herz", "ge\u00b7h\u00e4uft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}