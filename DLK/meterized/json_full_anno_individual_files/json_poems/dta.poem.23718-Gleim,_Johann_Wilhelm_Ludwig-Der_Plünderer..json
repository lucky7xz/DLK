{"dta.poem.23718": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Der Pl\u00fcnderer.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-200905197682", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich sahe mit Erbarmen,", "tokens": ["Ich", "sa\u00b7he", "mit", "Er\u00b7bar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das Rauben und das Pl\u00fcndern", "tokens": ["Das", "Rau\u00b7ben", "und", "das", "Pl\u00fcn\u00b7dern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der wilden Kriegesleute.", "tokens": ["Der", "wil\u00b7den", "Krie\u00b7ge\u00b7sleu\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sie nahmen Gold und Silber,", "tokens": ["Sie", "nah\u00b7men", "Gold", "und", "Sil\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sie nahmen Wein und Weitzen,", "tokens": ["Sie", "nah\u00b7men", "Wein", "und", "Weit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und s\u00e4umten nicht im Nehmen,", "tokens": ["Und", "s\u00e4um\u00b7ten", "nicht", "im", "Neh\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Obgleich ein freundlich M\u00e4dchen,", "tokens": ["Ob\u00b7gleich", "ein", "freund\u00b7lich", "M\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ein liebes schwarzes M\u00e4dchen,", "tokens": ["Ein", "lie\u00b7bes", "schwar\u00b7zes", "M\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Um Gnade bat, und weinte.", "tokens": ["Um", "Gna\u00b7de", "bat", ",", "und", "wein\u00b7te", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUI", "NN", "VVFIN", "$,", "KON", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Wie gern h\u00e4tt ich, ich Bl\u00f6der,", "tokens": ["Wie", "gern", "h\u00e4tt", "ich", ",", "ich", "Bl\u00f6\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VAFIN", "PPER", "$,", "PPER", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Die Pl\u00fcnderer get\u00f6dtet!", "tokens": ["Die", "Pl\u00fcn\u00b7de\u00b7rer", "ge\u00b7t\u00f6d\u00b7tet", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Allein, sie waren tapfer.", "tokens": ["Al\u00b7lein", ",", "sie", "wa\u00b7ren", "tap\u00b7fer", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPER", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Sie w\u00fcteten mit Augen,", "tokens": ["Sie", "w\u00fc\u00b7te\u00b7ten", "mit", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Sie w\u00fcteten mit Schwerdtern,", "tokens": ["Sie", "w\u00fc\u00b7te\u00b7ten", "mit", "Schwerd\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "Sie trotzten mit den Panzern.", "tokens": ["Sie", "trotz\u00b7ten", "mit", "den", "Pan\u00b7zern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.16": {"text": "Ich konte nicht besch\u00fctzen,", "tokens": ["Ich", "kon\u00b7te", "nicht", "be\u00b7sch\u00fct\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.17": {"text": "Ich konte nur das M\u00e4dchen", "tokens": ["Ich", "kon\u00b7te", "nur", "das", "M\u00e4d\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.18": {"text": "Und seine Mutter tr\u00f6sten.", "tokens": ["Und", "sei\u00b7ne", "Mut\u00b7ter", "tr\u00f6s\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.19": {"text": "Ich tr\u00f6stete die Mutter,", "tokens": ["Ich", "tr\u00f6s\u00b7te\u00b7te", "die", "Mut\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.20": {"text": "Ich tr\u00f6stete die Tochter,", "tokens": ["Ich", "tr\u00f6s\u00b7te\u00b7te", "die", "Toch\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.21": {"text": "Und als die arme Mutter", "tokens": ["Und", "als", "die", "ar\u00b7me", "Mut\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.22": {"text": "Sich f\u00fcr den Trost bedankte,", "tokens": ["Sich", "f\u00fcr", "den", "Trost", "be\u00b7dank\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}