{"dta.poem.591": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Auf die spr\u00f6den M\u00e4dgen zu  J  * *", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Auch bisweilen streuben sich", "tokens": ["Auch", "bis\u00b7wei\u00b7len", "streu\u00b7ben", "sich"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PRF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Da\u00df sie lauffen vor den Stich", "tokens": ["Da\u00df", "sie", "lauf\u00b7fen", "vor", "den", "Stich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Gar aus unsern St\u00e4dgen.", "tokens": ["Gar", "aus", "un\u00b7sern", "St\u00e4d\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Doch wenns ihnen kommt im Sinn/", "tokens": ["Doch", "wenns", "ih\u00b7nen", "kommt", "im", "Sinn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "APPRART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Halten sie/ sie jeden hin/", "tokens": ["Hal\u00b7ten", "sie", "/", "sie", "je\u00b7den", "hin", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$(", "PPER", "PIAT", "PTKVZ", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Lassen sich den auch recht derbe stossen.", "tokens": ["Las\u00b7sen", "sich", "den", "auch", "recht", "der\u00b7be", "stos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "ADV", "ADJD", "ADJA", "VVINF", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Ja sie wollen sch\u00f6ner seyn/", "tokens": ["Ja", "sie", "wol\u00b7len", "sch\u00f6\u00b7ner", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PPER", "VMFIN", "ADJD", "VAINF", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Als die besten ", "tokens": ["Als", "die", "bes\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "ART", "ADJA"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Und der abgeschmackte Schein", "tokens": ["Und", "der", "ab\u00b7ge\u00b7schmack\u00b7te", "Schein"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Tr\u00e4gt ", "tokens": ["Tr\u00e4gt"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Da sie doch mit ihrer Pracht", "tokens": ["Da", "sie", "doch", "mit", "ih\u00b7rer", "Pracht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Leuchten wie ein Rotz bey Nacht/", "tokens": ["Leuch\u00b7ten", "wie", "ein", "Rotz", "bey", "Nacht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "NN", "APPR", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Oder wie ein schwartzer Schornstein-Fegen.", "tokens": ["O\u00b7der", "wie", "ein", "schwart\u00b7zer", "Schorn\u00b7stein\u00b7Fe\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Doch sie sind sehr rahr und theur", "tokens": ["Doch", "sie", "sind", "sehr", "rahr", "und", "theur"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "ADV", "ADJD", "KON", "NN"], "meter": "----+-+", "measure": "unknown.measure.di"}, "line.2": {"text": "Mit den gelben Dingern/", "tokens": ["Mit", "den", "gel\u00b7ben", "Din\u00b7gern", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Und gedeyn zum Ungeheur", "tokens": ["Und", "ge\u00b7deyn", "zum", "Un\u00b7ge\u00b7heur"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn man denckt zu fingern/", "tokens": ["Wenn", "man", "denckt", "zu", "fin\u00b7gern", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "PTKZU", "VVINF", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Decken gleich die Lappen zu/", "tokens": ["De\u00b7cken", "gleich", "die", "Lap\u00b7pen", "zu", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "PTKZU", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Da\u00df man nichts ", "tokens": ["Da\u00df", "man", "nichts"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PIS", "PIS"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Noch derselben Gr\u00f6sse was vermehre.", "tokens": ["Noch", "der\u00b7sel\u00b7ben", "Gr\u00f6s\u00b7se", "was", "ver\u00b7meh\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDAT", "NN", "PWS", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Gehet man denn auch zu weit/", "tokens": ["Ge\u00b7het", "man", "denn", "auch", "zu", "weit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "ADV", "PTKA", "ADJD", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Tastet nach den Beinen/", "tokens": ["Tas\u00b7tet", "nach", "den", "Bei\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Sehn sie als die theure Zeit/", "tokens": ["Sehn", "sie", "als", "die", "theu\u00b7re", "Zeit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KOUS", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Fangen an zu weinen/", "tokens": ["Fan\u00b7gen", "an", "zu", "wei\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "PTKZU", "VVINF", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Denn sie gleichen da dem Ahl/", "tokens": ["Denn", "sie", "glei\u00b7chen", "da", "dem", "Ahl", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Sind um Schoo\u00df und M\u00e4ulgen kahl/", "tokens": ["Sind", "um", "Schoo\u00df", "und", "M\u00e4ul\u00b7gen", "kahl", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NN", "KON", "NN", "ADJD", "$("], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.7": {"text": "Und verg\u00f6nnen keinen Griff in Ehren.", "tokens": ["Und", "ver\u00b7g\u00f6n\u00b7nen", "kei\u00b7nen", "Griff", "in", "Eh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Drum so zieht die Pfeiffen ein/", "tokens": ["Drum", "so", "zieht", "die", "Pfeif\u00b7fen", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "VVFIN", "ART", "NN", "ART", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Haltet k\u00fcnfftig stille/", "tokens": ["Hal\u00b7tet", "k\u00fcnff\u00b7tig", "stil\u00b7le", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "VVFIN", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "H\u00f6ret bald auf spr\u00f6d zu seyn/", "tokens": ["H\u00f6\u00b7ret", "bald", "auf", "spr\u00f6d", "zu", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ADJD", "PTKZU", "VAINF", "$("], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.4": {"text": "Thut was unser Wille/", "tokens": ["Thut", "was", "un\u00b7ser", "Wil\u00b7le", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PWS", "PPOSAT", "NN", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "La\u00dft euch k\u00fcssen/ und dabey", "tokens": ["La\u00dft", "euch", "k\u00fcs\u00b7sen", "/", "und", "da\u00b7bey"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVIMP", "PPER", "VVINF", "$(", "KON", "PAV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Gebet Griff und St\u00f6sse frey/", "tokens": ["Ge\u00b7bet", "Griff", "und", "St\u00f6s\u00b7se", "frey", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "NN", "ADJD", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "So seyd ihr die allerbesten M\u00e4dgen.", "tokens": ["So", "seyd", "ihr", "die", "al\u00b7ler\u00b7bes\u00b7ten", "M\u00e4d\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}}}}