{"dta.poem.7076": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Entwurff einiger Betrachtungen,  \n \u00fcber den  \n  Zustand der Seeligen.  \n Beym Anfang des 1727sten Jahres.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn in Besitz der ew\u2019gen Seeligkeit", "tokens": ["Wenn", "in", "Be\u00b7sitz", "der", "ew'\u00b7gen", "See\u00b7lig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Seele nun sich wird begabet finden", "tokens": ["Die", "See\u00b7le", "nun", "sich", "wird", "be\u00b7ga\u00b7bet", "fin\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "PRF", "VAFIN", "VVFIN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mit solcher dauernden Beschaffenheit,", "tokens": ["Mit", "sol\u00b7cher", "dau\u00b7ern\u00b7den", "Be\u00b7schaf\u00b7fen\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mit so gesch\u00e4ff tigen und reinen Lebens-Geistern;", "tokens": ["Mit", "so", "ge\u00b7sch\u00e4ff", "ti\u00b7gen", "und", "rei\u00b7nen", "Le\u00b7bens\u00b7Geis\u00b7tern", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJD", "VVINF", "KON", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.5": {"text": "Wie aufgekl\u00e4rt, wie lebhafft werden nicht", "tokens": ["Wie", "auf\u00b7ge\u00b7kl\u00e4rt", ",", "wie", "leb\u00b7hafft", "wer\u00b7den", "nicht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "VVPP", "$,", "PWAV", "ADJD", "VAFIN", "PTKNEG"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Derselben Kr\u00e4ffte seyn? Als wie ein schnelles Licht,", "tokens": ["Der\u00b7sel\u00b7ben", "Kr\u00e4ff\u00b7te", "seyn", "?", "Als", "wie", "ein", "schnel\u00b7les", "Licht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VAINF", "$.", "KOUS", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wird sie die dickste Dunckelheit", "tokens": ["Wird", "sie", "die", "dicks\u00b7te", "Dun\u00b7ckel\u00b7heit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Des Zweifels schnell durchdringen und bemeistern.", "tokens": ["Des", "Zwei\u00b7fels", "schnell", "durch\u00b7drin\u00b7gen", "und", "be\u00b7meis\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Es wird des Geistes F\u00e4higkeit", "tokens": ["Es", "wird", "des", "Geis\u00b7tes", "F\u00e4\u00b7hig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Um desto mehr erh\u00f6ht, gest\u00e4rcket und erweitert,", "tokens": ["Um", "des\u00b7to", "mehr", "er\u00b7h\u00f6ht", ",", "ge\u00b7st\u00e4r\u00b7cket", "und", "er\u00b7wei\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUI", "ADV", "ADV", "VVPP", "$,", "VVPP", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Vermehrt und ausgeheitert,", "tokens": ["Ver\u00b7mehrt", "und", "aus\u00b7ge\u00b7hei\u00b7tert", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "KON", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Je hurtiger, subtiler und geschwinder,", "tokens": ["Je", "hur\u00b7ti\u00b7ger", ",", "sub\u00b7ti\u00b7ler", "und", "ge\u00b7schwin\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADJA", "KON", "ADJD", "$,"], "meter": "-+--++-+-+-", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "Die Werckzeug\u2019 ihrer Handlung seyn.", "tokens": ["Die", "Wer\u00b7ck\u00b7zeug'", "ih\u00b7rer", "Hand\u00b7lung", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VAINF", "$."], "meter": "----+-+-+", "measure": "unknown.measure.tri"}, "line.14": {"text": "Es dringt der Geist in alles schnell hinein,", "tokens": ["Es", "dringt", "der", "Geist", "in", "al\u00b7les", "schnell", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "PIS", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Es m\u00f6gen sich die Schw\u00fcrigkeiten h\u00e4uffen;", "tokens": ["Es", "m\u00f6\u00b7gen", "sich", "die", "Schw\u00fc\u00b7rig\u00b7kei\u00b7ten", "h\u00e4uf\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Er ist geschickt sie alle zu begreiffen.", "tokens": ["Er", "ist", "ge\u00b7schickt", "sie", "al\u00b7le", "zu", "be\u00b7greif\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "PPER", "PIS", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Er kan geschwind und richtig schliessen,", "tokens": ["Er", "kan", "ge\u00b7schwind", "und", "rich\u00b7tig", "schlies\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADJD", "KON", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Wie Sachen aus einander fliessen.", "tokens": ["Wie", "Sa\u00b7chen", "aus", "ein\u00b7an\u00b7der", "flies\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Es \u00f6ffnet sich vor ihm die Wahrheit.", "tokens": ["Es", "\u00f6ff\u00b7net", "sich", "vor", "ihm", "die", "Wahr\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PPER", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.20": {"text": "Ged\u00e4chtni\u00df nebst der Phantasey,", "tokens": ["Ge\u00b7d\u00e4cht\u00b7ni\u00df", "nebst", "der", "Phan\u00b7ta\u00b7sey", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Sind in viel gr\u00f6ssrer St\u00e4rck und Klarheit,", "tokens": ["Sind", "in", "viel", "gr\u00f6ss\u00b7rer", "St\u00e4rck", "und", "Klar\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PIAT", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Als sie auf Erden", "tokens": ["Als", "sie", "auf", "Er\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.23": {"text": "Bey Sterblichen gefunden werden.", "tokens": ["Bey", "Sterb\u00b7li\u00b7chen", "ge\u00b7fun\u00b7den", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}