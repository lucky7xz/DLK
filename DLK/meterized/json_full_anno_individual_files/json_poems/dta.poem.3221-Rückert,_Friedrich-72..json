{"dta.poem.3221": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "72.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sprich es nicht aus, noch mit Gedanken denk' es aus,", "tokens": ["Sprich", "es", "nicht", "aus", ",", "noch", "mit", "Ge\u00b7dan\u00b7ken", "denk'", "es", "aus", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKNEG", "PTKVZ", "$,", "ADV", "APPR", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Was dir die Seele f\u00fcllt mit dunkler Ahnung Graus.", "tokens": ["Was", "dir", "die", "See\u00b7le", "f\u00fcllt", "mit", "dunk\u00b7ler", "Ah\u00b7nung", "Graus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Genug, da\u00df Todesschreck dem Sinn entgegentritt,", "tokens": ["Ge\u00b7nug", ",", "da\u00df", "To\u00b7des\u00b7schreck", "dem", "Sinn", "ent\u00b7ge\u00b7gen\u00b7tritt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn auch die Phantasie ihn nicht zum Voraus litt.", "tokens": ["Wenn", "auch", "die", "Phan\u00b7ta\u00b7sie", "ihn", "nicht", "zum", "Vo\u00b7raus", "litt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "PPER", "PTKNEG", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Den furchtbar'n Augenblick ertrag, und sei nicht schwach;", "tokens": ["Den", "furcht\u00b7ba\u00b7r'n", "Au\u00b7gen\u00b7blick", "er\u00b7trag", ",", "und", "sei", "nicht", "schwach", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "KON", "VAFIN", "PTKNEG", "ADJD", "$."], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Nicht bilde dir ihn vor, noch bilde dir ihn nach.", "tokens": ["Nicht", "bil\u00b7de", "dir", "ihn", "vor", ",", "noch", "bil\u00b7de", "dir", "ihn", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "PPER", "PPER", "PTKVZ", "$,", "ADV", "VVFIN", "PPER", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Der Wirbel fa\u00dft das Schiff, es geht villeicht in Scheiter,", "tokens": ["Der", "Wir\u00b7bel", "fa\u00dft", "das", "Schiff", ",", "es", "geht", "vil\u00b7leicht", "in", "Schei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch, kommt es gl\u00fccklich durch, so schwimmt es ruhig weiter.", "tokens": ["Doch", ",", "kommt", "es", "gl\u00fcck\u00b7lich", "durch", ",", "so", "schwimmt", "es", "ru\u00b7hig", "wei\u00b7ter", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,", "ADV", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}