{"textgrid.poem.44308": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mit einer tiefgelehrten Miene,", "genre": "verse", "period": "N.A.", "pub_year": 1807, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit einer tiefgelehrten Miene,", "tokens": ["Mit", "ei\u00b7ner", "tief\u00b7ge\u00b7lehr\u00b7ten", "Mie\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von einem dichten Bauernkreis umringt,", "tokens": ["Von", "ei\u00b7nem", "dich\u00b7ten", "Bau\u00b7ern\u00b7kreis", "um\u00b7ringt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der jedes seiner Worte froh verschlingt,", "tokens": ["Der", "je\u00b7des", "sei\u00b7ner", "Wor\u00b7te", "froh", "ver\u00b7schlingt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "PPOSAT", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sa\u00df Meister Klaus, der K\u00fcster, am Kamine", "tokens": ["Sa\u00df", "Meis\u00b7ter", "Klaus", ",", "der", "K\u00fcs\u00b7ter", ",", "am", "Ka\u00b7mi\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "NE", "NE", "$,", "ART", "NN", "$,", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und sprach mit Donnern der Beredsamkeit", "tokens": ["Und", "sprach", "mit", "Don\u00b7nern", "der", "Be\u00b7red\u00b7sam\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Von Danieln, wie die Susanna er befreit,", "tokens": ["Von", "Da\u00b7ni\u00b7eln", ",", "wie", "die", "Su\u00b7san\u00b7na", "er", "be\u00b7freit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PWAV", "ART", "NE", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Von seiner Freunde hei\u00dfer Stube", "tokens": ["Von", "sei\u00b7ner", "Freun\u00b7de", "hei\u00b7\u00dfer", "Stu\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und kurz von allem, was der Gottesmann", "tokens": ["Und", "kurz", "von", "al\u00b7lem", ",", "was", "der", "Got\u00b7tes\u00b7mann"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "PIS", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "In seinem Leben je gedacht, gesagt, getan", "tokens": ["In", "sei\u00b7nem", "Le\u00b7ben", "je", "ge\u00b7dacht", ",", "ge\u00b7sagt", ",", "ge\u00b7tan"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "VVPP", "$,", "VVPP", "$,", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und kam auch endlich auf die L\u00f6wengrube.", "tokens": ["Und", "kam", "auch", "end\u00b7lich", "auf", "die", "L\u00f6\u00b7wen\u00b7gru\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "\u00bbin diese\u00ab, sprach er, \u00bbwurde der Prophet gesperrt,", "tokens": ["\u00bb", "in", "die\u00b7se", "\u00ab", ",", "sprach", "er", ",", "\u00bb", "wur\u00b7de", "der", "Pro\u00b7phet", "ge\u00b7sperrt", ","], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PDAT", "$(", "$,", "VVFIN", "PPER", "$,", "$(", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Doch, wie die heilge Bibel lehrt,", "tokens": ["Doch", ",", "wie", "die", "heil\u00b7ge", "Bi\u00b7bel", "lehrt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWAV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Ward ihm darin auch nicht ein Haar versehrt.", "tokens": ["Ward", "ihm", "da\u00b7rin", "auch", "nicht", "ein", "Haar", "ver\u00b7sehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PAV", "ADV", "PTKNEG", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Und mit den L\u00f6wen war mein Seele nicht zu spassen,", "tokens": ["Und", "mit", "den", "L\u00f6\u00b7wen", "war", "mein", "See\u00b7le", "nicht", "zu", "spas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VAFIN", "PPOSAT", "NN", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Denn denkt! Man hatte sie in Hoffnung auf den Fang", "tokens": ["Denn", "denkt", "!", "Man", "hat\u00b7te", "sie", "in", "Hoff\u00b7nung", "auf", "den", "Fang"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$.", "PIS", "VAFIN", "PPER", "APPR", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Schon achtundvierzig Tage lang", "tokens": ["Schon", "ach\u00b7tund\u00b7vier\u00b7zig", "Ta\u00b7ge", "lang"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "CARD", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "In ihrer H\u00f6hle hungern lassen.\u00ab", "tokens": ["In", "ih\u00b7rer", "H\u00f6h\u00b7le", "hun\u00b7gern", "las\u00b7sen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "VVINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Mit einer tiefgelehrten Miene,", "tokens": ["Mit", "ei\u00b7ner", "tief\u00b7ge\u00b7lehr\u00b7ten", "Mie\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von einem dichten Bauernkreis umringt,", "tokens": ["Von", "ei\u00b7nem", "dich\u00b7ten", "Bau\u00b7ern\u00b7kreis", "um\u00b7ringt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der jedes seiner Worte froh verschlingt,", "tokens": ["Der", "je\u00b7des", "sei\u00b7ner", "Wor\u00b7te", "froh", "ver\u00b7schlingt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "PPOSAT", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sa\u00df Meister Klaus, der K\u00fcster, am Kamine", "tokens": ["Sa\u00df", "Meis\u00b7ter", "Klaus", ",", "der", "K\u00fcs\u00b7ter", ",", "am", "Ka\u00b7mi\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "NE", "NE", "$,", "ART", "NN", "$,", "APPRART", "NN"], "meter": "-+-+-+-+-+-", 