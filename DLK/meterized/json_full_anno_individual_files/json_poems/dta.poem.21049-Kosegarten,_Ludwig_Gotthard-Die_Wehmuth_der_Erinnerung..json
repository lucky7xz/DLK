{"dta.poem.21049": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Die  \n Wehmuth der Erinnerung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1798", "urn": "urn:nbn:de:kobv:b4-200905193300", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sch\u00f6ner herbstlicher Tag, dunkel und sch\u00f6n zu-               ", "tokens": ["Sch\u00f6\u00b7ner", "herbst\u00b7li\u00b7cher", "Tag", ",", "dun\u00b7kel", "und", "sch\u00f6n", "zu"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "ADJA", "NN", "$,", "ADJD", "KON", "ADJD", "TRUNC"], "meter": "+-+---+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "gleich,", "tokens": ["gleich", ","], "token_info": ["word", "punct"], "pos": ["ADV", "$,"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Welches ahnende Weh, welche Melancholie", "tokens": ["Wel\u00b7ches", "ah\u00b7nen\u00b7de", "Weh", ",", "wel\u00b7che", "Me\u00b7lan\u00b7cho\u00b7lie"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PWAT", "ADJA", "NN", "$,", "PWAT", "NN"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.4": {"text": "Weckt dein traurendes L\u00e4cheln", "tokens": ["Weckt", "dein", "trau\u00b7ren\u00b7des", "L\u00e4\u00b7cheln"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "In des Wandrers emp\u00f6rter Brust!", "tokens": ["In", "des", "Wand\u00b7rers", "em\u00b7p\u00f6r\u00b7ter", "Brust", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Welk ist jegliches Gr\u00fcn. Jeder Gesang ist stumm.", "tokens": ["Welk", "ist", "jeg\u00b7li\u00b7ches", "Gr\u00fcn", ".", "Je\u00b7der", "Ge\u00b7sang", "ist", "stumm", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PIAT", "NN", "$.", "PIAT", "NN", "VAFIN", "ADJD", "$."], "meter": "+-+--+---+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Jeder Schimmer erblasst, jeglicher Saft versiegt.", "tokens": ["Je\u00b7der", "Schim\u00b7mer", "er\u00b7blasst", ",", "jeg\u00b7li\u00b7cher", "Saft", "ver\u00b7siegt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "$,", "PIAT", "NN", "VVPP", "$."], "meter": "+-+---+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Laublos trauren die B\u00e4ume,", "tokens": ["Laub\u00b7los", "trau\u00b7ren", "die", "B\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Schmucklos jammert die Blumenflur.", "tokens": ["Schmuck\u00b7los", "jam\u00b7mert", "die", "Blu\u00b7men\u00b7flur", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.3": {"line.1": {"text": "Wenig schwirrendes Laub, golden und bunt schattirt,", "tokens": ["We\u00b7nig", "schwir\u00b7ren\u00b7des", "Laub", ",", "gol\u00b7den", "und", "bunt", "schat\u00b7tirt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,", "ADJA", "KON", "ADJD", "VVPP", "$,"], "meter": "+-+--+---+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Wenig welkendes Laub schm\u00fccket die Laube noch,", "tokens": ["We\u00b7nig", "wel\u00b7ken\u00b7des", "Laub", "schm\u00fc\u00b7cket", "die", "Lau\u00b7be", "noch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.3": {"text": "Wo ich tr\u00e4umend und w\u00e4hnend", "tokens": ["Wo", "ich", "tr\u00e4u\u00b7mend", "und", "w\u00e4h\u00b7nend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "VVPP", "KON", "ADJD"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "An Odaliens Busen lag.", "tokens": ["An", "O\u00b7da\u00b7li\u00b7ens", "Bu\u00b7sen", "lag", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NE", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.4": {"line.1": {"text": "Abend war es. Der Mond flimmerte durch das", "tokens": ["A\u00b7bend", "war", "es", ".", "Der", "Mond", "flim\u00b7mer\u00b7te", "durch", "das"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "$.", "ART", "NN", "VVFIN", "APPR", "ART"], "meter": "+-++-++-+-+", "measure": "unknown.measure.septa"}, "line.2": {"text": "Laub.", "tokens": ["Laub", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Bl\u00fcthen bebten im Thau. D\u00fcfte umwallten uns.", "tokens": ["Bl\u00fc\u00b7then", "beb\u00b7ten", "im", "Thau", ".", "D\u00fcf\u00b7te", "um\u00b7wall\u00b7ten", "uns", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NN", "$.", "NN", "VVFIN", "PPER", "$."], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.4": {"text": "Bl\u00fchender, duftender, sch\u00f6ner", "tokens": ["Bl\u00fc\u00b7hen\u00b7der", ",", "duf\u00b7ten\u00b7der", ",", "sch\u00f6\u00b7ner"], "token_info": ["word", "punct", "word", "punct", "word"], "pos": ["NN", "$,", "ADJA", "$,", "ADJA"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.5": {"text": "Sass Odalia neben mir.", "tokens": ["Sass", "O\u00b7da\u00b7lia", "ne\u00b7ben", "mir", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "APPR", "PPER", "$."], "meter": "-+-+---", "measure": "unknown.measure.di"}}, "stanza.5": {"line.1": {"text": "Hingesunken an sie, innigst geschmiegt an sie", "tokens": ["Hin\u00b7ge\u00b7sun\u00b7ken", "an", "sie", ",", "in\u00b7nigst", "ge\u00b7schmiegt", "an", "sie"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PPER", "$,", "ADJD", "VVPP", "APPR", "PPER"], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Tr\u00e4umt' ich seligen Traum, schmolz in Vergessen-", "tokens": ["Tr\u00e4umt'", "ich", "se\u00b7li\u00b7gen", "Traum", ",", "schmolz", "in", "Ver\u00b7ges\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJA", "NN", "$,", "ADJD", "APPR", "TRUNC"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "heit,", "tokens": ["heit", ","], "token_info": ["word", "punct"], "pos": ["ADJD", "$,"], "meter": "-", "measure": "single.down"}, "line.4": {"text": "Bis das Flistern der Bl\u00e4tter", "tokens": ["Bis", "das", "Flis\u00b7tern", "der", "Bl\u00e4t\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "Mich dem seidenen Traum entriss.", "tokens": ["Mich", "dem", "sei\u00b7de\u00b7nen", "Traum", "ent\u00b7riss", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.6": {"line.1": {"text": "Itzund flistern sie nicht. Jedes Gelispel schweigt;", "tokens": ["It\u00b7zund", "flis\u00b7tern", "sie", "nicht", ".", "Je\u00b7des", "Ge\u00b7lis\u00b7pel", "schweigt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PTKNEG", "$.", "PIAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Jeder Jubel verhallt; jedes Ged\u00fcft verweht.", "tokens": ["Je\u00b7der", "Ju\u00b7bel", "ver\u00b7hallt", ";", "je\u00b7des", "Ge\u00b7d\u00fcft", "ver\u00b7weht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVPP", "$.", "PIAT", "NN", "VVFIN", "$."], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.3": {"text": "Schlaff ist jegliche Sehne,", "tokens": ["Schlaff", "ist", "jeg\u00b7li\u00b7che", "Seh\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PIAT", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Leer der K\u00f6cher der Sch\u00f6pferin.", "tokens": ["Leer", "der", "K\u00f6\u00b7cher", "der", "Sch\u00f6p\u00b7fe\u00b7rin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "ART", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.7": {"line.1": {"text": "Jede Bl\u00fcthe verbl\u00fcht, welche der Lenz gebar,", "tokens": ["Je\u00b7de", "Bl\u00fc\u00b7the", "ver\u00b7bl\u00fcht", ",", "wel\u00b7che", "der", "Lenz", "ge\u00b7bar", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVPP", "$,", "PRELS", "ART", "NN", "ADJD", "$,"], "meter": "+-+--+---+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Jede Sch\u00f6ne verwelkt, welche dem Staub' ent-", "tokens": ["Je\u00b7de", "Sch\u00f6\u00b7ne", "ver\u00b7welkt", ",", "wel\u00b7che", "dem", "Staub'", "ent"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVPP", "$,", "PRELS", "ART", "NN", "TRUNC"], "meter": "+-+---+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "spross.", "tokens": ["spross", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-", "measure": "single.down"}}, "stanza.8": {"line.1": {"text": "Doch die sch\u00f6nere Seele", "tokens": ["Doch", "die", "sch\u00f6\u00b7ne\u00b7re", "See\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Bl\u00fcht unsterblichen sch\u00f6nen Lenz.", "tokens": ["Bl\u00fcht", "uns\u00b7terb\u00b7li\u00b7chen", "sch\u00f6\u00b7nen", "Lenz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.9": {"line.1": {"text": "Ewig jugendlich bl\u00fcht meine Odalia,", "tokens": ["E\u00b7wig", "ju\u00b7gend\u00b7lich", "bl\u00fcht", "mei\u00b7ne", "O\u00b7da\u00b7lia", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PPOSAT", "NE", "$,"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Jeden kehrenden Lenz sch\u00f6ner und bl\u00fchender,", "tokens": ["Je\u00b7den", "keh\u00b7ren\u00b7den", "Lenz", "sch\u00f6\u00b7ner", "und", "bl\u00fc\u00b7hen\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "ADJA", "KON", "ADJA", "$,"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.3": {"text": "Jeden rollenden \u00c4on", "tokens": ["Je\u00b7den", "rol\u00b7len\u00b7den", "\u00c4\u00b7on"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Reiner, edler, vollkommener.", "tokens": ["Rei\u00b7ner", ",", "ed\u00b7ler", ",", "voll\u00b7kom\u00b7me\u00b7ner", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "ADJA", "$,", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Ewig flammet die Gluth heiliger Sympathie.", "tokens": ["E\u00b7wig", "flam\u00b7met", "die", "Gluth", "hei\u00b7li\u00b7ger", "Sym\u00b7pa\u00b7thie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.2": {"text": "Nie ermattet der Zug, welcher mich zu dir zog,", "tokens": ["Nie", "er\u00b7mat\u00b7tet", "der", "Zug", ",", "wel\u00b7cher", "mich", "zu", "dir", "zog", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "PRELS", "PPER", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+--++-+--+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Meine Reine, als Ahnung", "tokens": ["Mei\u00b7ne", "Rei\u00b7ne", ",", "als", "Ah\u00b7nung"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "KOUS", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Deines Werthes mein Herz ergriff.", "tokens": ["Dei\u00b7nes", "Wert\u00b7hes", "mein", "Herz", "er\u00b7griff", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}}}}