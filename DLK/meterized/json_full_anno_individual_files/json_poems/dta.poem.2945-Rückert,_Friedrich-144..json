{"dta.poem.2945": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "144.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ihr geht, und glaubet euch vollkommen Herr im Haus,", "tokens": ["Ihr", "geht", ",", "und", "glau\u00b7bet", "euch", "voll\u00b7kom\u00b7men", "Herr", "im", "Haus", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KON", "VVFIN", "PPER", "ADJD", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von eures christlichen Bewu\u00dftseyns Thatsach' aus.", "tokens": ["Von", "eu\u00b7res", "christ\u00b7li\u00b7chen", "Be\u00b7wu\u00df\u00b7tseyns", "That\u00b7sach'", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Urspr\u00fcnglich glaubet ihr von Gott und von Natur", "tokens": ["Ur\u00b7spr\u00fcng\u00b7lich", "glau\u00b7bet", "ihr", "von", "Gott", "und", "von", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Euch eingepflanzt, was ihr habt von der Mutter nur.", "tokens": ["Euch", "ein\u00b7ge\u00b7pflanzt", ",", "was", "ihr", "habt", "von", "der", "Mut\u00b7ter", "nur", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "$,", "PWS", "PPER", "VAFIN", "APPR", "ART", "NN", "ADV", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Mit eurer Amme Milch habt ihr es eingesogen,", "tokens": ["Mit", "eu\u00b7rer", "Am\u00b7me", "Milch", "habt", "ihr", "es", "ein\u00b7ge\u00b7so\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "VAFIN", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit ihrem Wiegensang ist es euch angeflogen,", "tokens": ["Mit", "ih\u00b7rem", "Wie\u00b7gen\u00b7sang", "ist", "es", "euch", "an\u00b7ge\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Und mit dem G\u00e4ngelband ward es euch angezogen.", "tokens": ["Und", "mit", "dem", "G\u00e4n\u00b7gel\u00b7band", "ward", "es", "euch", "an\u00b7ge\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VAFIN", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Nicht sag' ich, da\u00df ihr dem euch sollt und k\u00f6nnt entziehn;", "tokens": ["Nicht", "sag'", "ich", ",", "da\u00df", "ihr", "dem", "euch", "sollt", "und", "k\u00f6nnt", "ent\u00b7ziehn", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "PPER", "$,", "KOUS", "PPER", "ART", "PPER", "VMFIN", "KON", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Ma\u00dfstab sei es euch, gebraucht mit Ma\u00dfen ihn!", "tokens": ["Ein", "Ma\u00df\u00b7stab", "sei", "es", "euch", ",", "ge\u00b7braucht", "mit", "Ma\u00b7\u00dfen", "ihn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "PPER", "$,", "VVPP", "APPR", "NN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Und legt ihn nicht an dem, dem andrer ist verliehn.", "tokens": ["Und", "legt", "ihn", "nicht", "an", "dem", ",", "dem", "an\u00b7drer", "ist", "ver\u00b7liehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "APPR", "ART", "$,", "ART", "ADJA", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Gott, der in H\u00e4nden h\u00e4lt das Richtma\u00df f\u00fcr die Welt,", "tokens": ["Gott", ",", "der", "in", "H\u00e4n\u00b7den", "h\u00e4lt", "das", "Richt\u00b7ma\u00df", "f\u00fcr", "die", "Welt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "APPR", "NN", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Hat jedem das ihm Angeme\u00dfne zugestellt.", "tokens": ["Hat", "je\u00b7dem", "das", "ihm", "An\u00b7ge\u00b7me\u00df\u00b7ne", "zu\u00b7ge\u00b7stellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ART", "PPER", "NE", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}