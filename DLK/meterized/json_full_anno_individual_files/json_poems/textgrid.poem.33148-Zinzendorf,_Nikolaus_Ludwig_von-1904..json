{"textgrid.poem.33148": {"metadata": {"author": {"name": "Zinzendorf, Nikolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "1904.", "genre": "verse", "period": "N.A.", "pub_year": 1730, "urn": "N.A.", "language": ["de:0.71", "nl:0.28"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das w\u00e4re mein plan, ", "tokens": ["Das", "w\u00e4\u00b7re", "mein", "plan", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Ger\u00fchret vom strahl der blutigen maal, und niemals gewandt von deiner mit n\u00e4geln durchbohreten hand:", "tokens": ["Ge\u00b7r\u00fch\u00b7ret", "vom", "strahl", "der", "blu\u00b7ti\u00b7gen", "maal", ",", "und", "nie\u00b7mals", "ge\u00b7wandt", "von", "dei\u00b7ner", "mit", "n\u00e4\u00b7geln", "durch\u00b7boh\u00b7re\u00b7ten", "hand", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "ART", "ADJA", "NN", "$,", "KON", "ADV", "VVPP", "APPR", "PPOSAT", "APPR", "VVINF", "VMFIN", "NN", "$."], "meter": "-+--+-+--+-+--+-+--+--+--+", "measure": "iambic.octa.plus.relaxed"}}, "stanza.3": {"line.1": {"text": "Im innersten grund vom blutigen bund gef\u00fchlig und warm, und doch niemals anders als elend und arm:", "tokens": ["Im", "in\u00b7ners\u00b7ten", "grund", "vom", "blu\u00b7ti\u00b7gen", "bund", "ge\u00b7f\u00fch\u00b7lig", "und", "warm", ",", "und", "doch", "nie\u00b7mals", "an\u00b7ders", "als", "e\u00b7lend", "und", "arm", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "APPRART", "ADJA", "NN", "ADJD", "KON", "ADJD", "$,", "KON", "ADV", "ADV", "ADV", "KOKOM", "ADJD", "KON", "ADJD", "$."], "meter": "--+-+-+--+-+--+--+-+--+--+", "measure": "iambic.octa.plus.relaxed"}}, "stanza.4": {"line.1": {"text": "Mit herzen und hand auf ewig verbannt, verw\u00fcnschet ins Lamm, und in den mit blute geweyheten stamm:", "tokens": ["Mit", "her\u00b7zen", "und", "hand", "auf", "e\u00b7wig", "ver\u00b7bannt", ",", "ver\u00b7w\u00fcn\u00b7schet", "ins", "Lamm", ",", "und", "in", "den", "mit", "blu\u00b7te", "ge\u00b7wey\u00b7he\u00b7ten", "stamm", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVINF", "KON", "NN", "APPR", "ADJD", "VVPP", "$,", "VVFIN", "APPRART", "NN", "$,", "KON", "APPR", "ART", "APPR", "ADJA", "ADJA", "NN", "$."], "meter": "-+--+-+--+-+--+-+--+--+--+", "measure": "iambic.octa.plus.relaxed"}}, "stanza.5": {"line.1": {"text": "Weich ieder gedank, der etwa noch hang zum alten erh\u00e4lt! vor dir bin ich ewiglich sicher gestellt:", "tokens": ["Weich", "ie\u00b7der", "ge\u00b7dank", ",", "der", "et\u00b7wa", "noch", "hang", "zum", "al\u00b7ten", "er\u00b7h\u00e4lt", "!", "vor", "dir", "bin", "ich", "e\u00b7wig\u00b7lich", "si\u00b7cher", "ge\u00b7stellt", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "$,", "PRELS", "ADV", "ADV", "VVFIN", "APPRART", "ADJA", "VVFIN", "$.", "APPR", "PPER", "VAFIN", "PPER", "ADV", "ADJD", "VVPP", "$."], "meter": "+---+-+--+-+--++-+-+-+-+-+", "measure": "trochaic.octa.plus.relaxed"}}, "stanza.6": {"line.1": {"text": "Zum tieffsten respect des salb-\u00f6ls erwekt, das du mir vertraut, da du mich zu deinem geh\u00e4use erbaut:", "tokens": ["Zum", "tieffs\u00b7ten", "res\u00b7pect", "des", "salb\u00b7\u00f6ls", "er\u00b7wekt", ",", "das", "du", "mir", "ver\u00b7traut", ",", "da", "du", "mich", "zu", "dei\u00b7nem", "ge\u00b7h\u00e4u\u00b7se", "er\u00b7baut", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "VVPP", "$,", "PRELS", "PPER", "PPER", "ADJD", "$,", "KOUS", "PPER", "PRF", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+--+-+--+-+--+--+-+--+--+", "measure": "iambic.octa.plus.relaxed"}}, "stanza.7": {"line.1": {"text": "In deiner Gemein so fertig und fein, zu land oder see, als ein candidate der ewigen eh:", "tokens": ["In", "dei\u00b7ner", "Ge\u00b7mein", "so", "fer\u00b7tig", "und", "fein", ",", "zu", "land", "o\u00b7der", "see", ",", "als", "ein", "can\u00b7di\u00b7da\u00b7te", "der", "e\u00b7wi\u00b7gen", "eh", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "ADJD", "KON", "ADJD", "$,", "APPR", "NN", "KON", "NE", "$,", "KOUS", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+--+-+--+--+-+--+--+", "measure": "iambic.octa.plus.relaxed"}}, "stanza.8": {"line.1": {"text": "In ieder minut ins heilige blut der wunden getaucht, und von deinem ewigen Geiste durchhaucht:", "tokens": ["In", "ie\u00b7der", "mi\u00b7nut", "ins", "hei\u00b7li\u00b7ge", "blut", "der", "wun\u00b7den", "ge\u00b7taucht", ",", "und", "von", "dei\u00b7nem", "e\u00b7wi\u00b7gen", "Geis\u00b7te", "durch\u00b7haucht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "APPRART", "ADJA", "NN", "ART", "ADJA", "VVPP", "$,", "KON", "APPR", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+--+-+--+-+--+--+-+--+--+", "measure": "iambic.octa.plus.relaxed"}}, "stanza.9": {"line.1": {"text": "Du hast mich einmal aus ewiger wahl zum volke gebracht, das du dir zum lustspiel auf erden gemacht:", "tokens": ["Du", "hast", "mich", "ein\u00b7mal", "aus", "e\u00b7wi\u00b7ger", "wahl", "zum", "vol\u00b7ke", "ge\u00b7bracht", ",", "das", "du", "dir", "zum", "lust\u00b7spiel", "auf", "er\u00b7den", "ge\u00b7macht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "APPRART", "NN", "VVPP", "$,", "PRELS", "PPER", "PPER", "APPRART", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+--+----+--+--+-+-+-+-+", "measure": "iambic.octa.plus.relaxed"}}, "stanza.10": {"line.1": {"text": "Wie Petrus geliebt, da er dich betr\u00fcbt, und thr\u00e4nen gezollt, die \u00fcber die wangen so sanfte gerollt:", "tokens": ["Wie", "Pet\u00b7rus", "ge\u00b7liebt", ",", "da", "er", "dich", "be\u00b7tr\u00fcbt", ",", "und", "thr\u00e4\u00b7nen", "ge\u00b7zollt", ",", "die", "\u00fc\u00b7ber", "die", "wan\u00b7gen", "so", "sanf\u00b7te", "ge\u00b7rollt", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "FM", "VVPP", "$,", "KOUS", "PPER", "PRF", "VVPP", "$,", "KON", "NN", "VVPP", "$,", "PRELS", "APPR", "ART", "NN", "ADV", "VVFIN", "VVPP", "$."], "meter": "-+--+--+-+-+--+-+--+--+--+", "measure": "amphibrach.tri.plus"}}, "stanza.11": {"line.1": {"text": "Wie Thomas gethr\u00e4nt, da er sich gesehnt, die wunden zu sehn, und in die ge\u00f6ffnete seite zu gehn:", "tokens": ["Wie", "Tho\u00b7mas", "ge\u00b7thr\u00e4nt", ",", "da", "er", "sich", "ge\u00b7sehnt", ",", "die", "wun\u00b7den", "zu", "sehn", ",", "und", "in", "die", "ge\u00b7\u00f6ff\u00b7ne\u00b7te", "sei\u00b7te", "zu", "gehn", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "VVPP", "$,", "KOUS", "PPER", "PRF", "VVPP", "$,", "ART", "ADJA", "PTKZU", "VVINF", "$,", "KON", "APPR", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+--+-+-+--+-+--+--+--+", "measure": "amphibrach.tri.plus"}}, "stanza.12": {"line.1": {"text": "Und was f\u00fcr ein gl\u00fck Maria beym blik des Lammes geno\u00df, da sie die durchgrabene f\u00fcsse umschlo\u00df:", "tokens": ["Und", "was", "f\u00fcr", "ein", "gl\u00fck", "Ma\u00b7ria", "beym", "blik", "des", "Lam\u00b7mes", "ge\u00b7no\u00df", ",", "da", "sie", "die", "durch\u00b7gra\u00b7be\u00b7ne", "f\u00fcs\u00b7se", "um\u00b7schlo\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "APPR", "ART", "NE", "NE", "APPRART", "NN", "ART", "NN", "VVFIN", "$,", "KOUS", "PPER", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "--------+-+--+--+-+--+--+", "measure": "iambic.septa.relaxed"}}, "stanza.13": {"line.1": {"text": "So w\u00fcnschte ich mir best\u00e4ndig von dir den innigen ku\u00df und nie unterbrochenen wunden-genu\u00df.", "tokens": ["So", "w\u00fcnschte", "ich", "mir", "be\u00b7st\u00e4n\u00b7dig", "von", "dir", "den", "in\u00b7ni\u00b7gen", "ku\u00df", "und", "nie", "un\u00b7ter\u00b7bro\u00b7che\u00b7nen", "wun\u00b7den\u00b7ge\u00b7nu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADJD", "APPR", "PPER", "ART", "ADJA", "NN", "KON", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+--+----+-+--+--+--+", "measure": "iambic.octa.plus.relaxed"}}, "stanza.14": {"line.1": {"text": "Johannes im schoos, das w\u00e4re mein loos, ans liegen im schrein