{"dta.poem.12881": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Schertz-gedichte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Als ich und Chloris n\u00e4chst im frischen gr\u00fcnen spielten,", "tokens": ["Als", "ich", "und", "Chlo\u00b7ris", "n\u00e4chst", "im", "fri\u00b7schen", "gr\u00fc\u00b7nen", "spiel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "KON", "NE", "ADV", "APPRART", "ADJA", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und bey beqvemer lufft die m\u00fcden glieder k\u00fchlten,", "tokens": ["Und", "bey", "be\u00b7qve\u00b7mer", "lufft", "die", "m\u00fc\u00b7den", "glie\u00b7der", "k\u00fchl\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So traff sich ohngefehr, da\u00df eine biene sa\u00df,", "tokens": ["So", "traff", "sich", "ohn\u00b7ge\u00b7fehr", ",", "da\u00df", "ei\u00b7ne", "bie\u00b7ne", "sa\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ADJD", "$,", "KOUS", "ART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und den beperlten thau von frischen rosen la\u00df.", "tokens": ["Und", "den", "be\u00b7perl\u00b7ten", "thau", "von", "fri\u00b7schen", "ro\u00b7sen", "la\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ach! rieff ich: Wilst du mir nicht gleiche lust verg\u00f6nnen?", "tokens": ["Ach", "!", "rieff", "ich", ":", "Wilst", "du", "mir", "nicht", "glei\u00b7che", "lust", "ver\u00b7g\u00f6n\u00b7nen", "?"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVFIN", "PPER", "$.", "VMFIN", "PPER", "PPER", "PTKNEG", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Das wird, versetzte sie: wol nicht geschehen k\u00f6nnen.", "tokens": ["Das", "wird", ",", "ver\u00b7setz\u00b7te", "sie", ":", "wol", "nicht", "ge\u00b7sche\u00b7hen", "k\u00f6n\u00b7nen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "$,", "VVFIN", "PPER", "$.", "ADV", "PTKNEG", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Sehr wohl, Beliebtes Kind! fiel ich hier wieder ein:", "tokens": ["Sehr", "wohl", ",", "Be\u00b7lieb\u00b7tes", "Kind", "!", "fiel", "ich", "hier", "wie\u00b7der", "ein", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "ADJA", "NN", "$.", "VVFIN", "PPER", "ADV", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Du kanst der rosen-knopff, ich will die biene seyn.", "tokens": ["Du", "kanst", "der", "ro\u00b7sen\u00b7knopff", ",", "ich", "will", "die", "bie\u00b7ne", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "$,", "PPER", "VMFIN", "ART", "ADJA", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}