{"textgrid.poem.61073": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Schlaflosigkeit", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn ich in den Knabenjahren", "tokens": ["Wenn", "ich", "in", "den", "Kna\u00b7ben\u00b7jah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Abends hinsank auf mein Bette,", "tokens": ["A\u00b7bends", "hin\u00b7sank", "auf", "mein", "Bet\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "O wie war die Rast mir lieblich!", "tokens": ["O", "wie", "war", "die", "Rast", "mir", "lieb\u00b7lich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "VAFIN", "ART", "NN", "PPER", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schon nach wenig Atemz\u00fcgen", "tokens": ["Schon", "nach", "we\u00b7nig", "A\u00b7tem\u00b7z\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "L\u00f6sten sich von selbst die Wimpern,", "tokens": ["L\u00f6s\u00b7ten", "sich", "von", "selbst", "die", "Wim\u00b7pern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und des Schlafes Wellen sp\u00fclten", "tokens": ["Und", "des", "Schla\u00b7fes", "Wel\u00b7len", "sp\u00fcl\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Um die Brust mir leicht und linde,", "tokens": ["Um", "die", "Brust", "mir", "leicht", "und", "lin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "PPER", "ADJD", "KON", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und der Traum mit Elfenh\u00e4nden", "tokens": ["Und", "der", "Traum", "mit", "El\u00b7fen\u00b7h\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Nahm mir von der jungen Seele", "tokens": ["Nahm", "mir", "von", "der", "jun\u00b7gen", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Allen kleinen Harm des Tages.", "tokens": ["Al\u00b7len", "klei\u00b7nen", "Harm", "des", "Ta\u00b7ges", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Aber jetzt wie ward es anders!", "tokens": ["A\u00b7ber", "jetzt", "wie", "ward", "es", "an\u00b7ders", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "KOKOM", "VAFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Such' ich mitternachts mein Lager", "tokens": ["Such'", "ich", "mit\u00b7ter\u00b7nachts", "mein", "La\u00b7ger"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Mit herabgebrannter Kerze,", "tokens": ["Mit", "her\u00b7ab\u00b7ge\u00b7brann\u00b7ter", "Ker\u00b7ze", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bleibt der s\u00fc\u00dfe Schlaf mir ferne;", "tokens": ["Bleibt", "der", "s\u00fc\u00b7\u00dfe", "Schlaf", "mir", "fer\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PPER", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Denn die Sehnsucht ruckt am Kissen,", "tokens": ["Denn", "die", "Sehn\u00b7sucht", "ruckt", "am", "Kis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und es lasten die Gedanken", "tokens": ["Und", "es", "las\u00b7ten", "die", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Auf mir wie ein b\u00f6ser Alpdruck,", "tokens": ["Auf", "mir", "wie", "ein", "b\u00f6\u00b7ser", "A\u00b7lpdruck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und mit Rabenfl\u00fcgeln schwirren", "tokens": ["Und", "mit", "Ra\u00b7ben\u00b7fl\u00fc\u00b7geln", "schwir\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Um mein Haupt die schlimmen Sorgen.", "tokens": ["Um", "mein", "Haupt", "die", "schlim\u00b7men", "Sor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Stundenlang mit hei\u00dfem Auge", "tokens": ["Stun\u00b7den\u00b7lang", "mit", "hei\u00b7\u00dfem", "Au\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Starr' ich dann hinaus ins Dunkel,", "tokens": ["Starr'", "ich", "dann", "hin\u00b7aus", "ins", "Dun\u00b7kel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APZR", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Bis zuletzt die matte Seele", "tokens": ["Bis", "zu\u00b7letzt", "die", "mat\u00b7te", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sich verliert in dumpfen Tr\u00e4umen.", "tokens": ["Sich", "ver\u00b7liert", "in", "dum\u00b7pfen", "Tr\u00e4u\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Ach, was g\u00e4b' ich drum, ihr Freunde,", "tokens": ["Ach", ",", "was", "g\u00e4b'", "ich", "drum", ",", "ihr", "Freun\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWS", "VVFIN", "PPER", "PAV", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "K\u00f6nnt' ich nur noch einmal wieder,", "tokens": ["K\u00f6nnt'", "ich", "nur", "noch", "ein\u00b7mal", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "ADV", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Einmal wie ein J\u00fcngling weinen,", "tokens": ["Ein\u00b7mal", "wie", "ein", "J\u00fcng\u00b7ling", "wei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Einmal schlafen wie ein Knabe!", "tokens": ["Ein\u00b7mal", "schla\u00b7fen", "wie", "ein", "Kna\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "KOKOM", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Wenn ich in den Knabenjahren", "tokens": ["Wenn", "ich", "in", "den", "Kna\u00b7ben\u00b7jah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Abends hinsank auf mein Bette,", "tokens": ["A\u00b7bends", "hin\u00b7sank", "auf", "mein", "Bet\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "O wie war die Rast mir lieblich!", "tokens": ["O", "wie", "war", "die", "Rast", "mir", "lieb\u00b7lich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "VAFIN", "ART", "NN", "PPER", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schon nach wenig Atemz\u00fcgen", "tokens": ["Schon", "nach", "we\u00b7nig", "A\u00b7tem\u00b7z\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "L\u00f6sten sich von selbst die Wimpern,", "tokens": ["L\u00f6s\u00b7ten", "sich", "von", "selbst", "die", "Wim\u00b7pern", ","], "token_info": ["word", "