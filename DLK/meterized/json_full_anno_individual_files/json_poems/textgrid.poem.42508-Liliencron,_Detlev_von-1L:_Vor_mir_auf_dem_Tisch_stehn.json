": ["Sie", "beugt", "die", "Stirn", "tief", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Und schlie\u00dft die Augen,", "tokens": ["Und", "schlie\u00dft", "die", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Und trinkt den Duft, wie aus einem Giftbecher,", "tokens": ["Und", "trinkt", "den", "Duft", ",", "wie", "aus", "ei\u00b7nem", "Gift\u00b7be\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "PWAV", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-+--", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Als ob sie den Tod ersehne.", "tokens": ["Als", "ob", "sie", "den", "Tod", "er\u00b7seh\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Und sie \u00f6ffnet die Lider", "tokens": ["Und", "sie", "\u00f6ff\u00b7net", "die", "Li\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.10": {"text": "Und sieht vision\u00e4r nach oben.", "tokens": ["Und", "sieht", "vi\u00b7si\u00b7o\u00b7n\u00e4r", "nach", "o\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Dann schlie\u00dfen sich wieder die Lider.", "tokens": ["Dann", "schlie\u00b7\u00dfen", "sich", "wie\u00b7der", "die", "Li\u00b7der", "."], "token_inf