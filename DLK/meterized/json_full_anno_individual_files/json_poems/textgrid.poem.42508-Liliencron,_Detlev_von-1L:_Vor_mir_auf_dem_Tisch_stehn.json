{"textgrid.poem.42508": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Vor mir auf dem Tisch stehn", "genre": "verse", "period": "N.A.", "pub_year": 1876, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vor mir auf dem Tisch stehn", "tokens": ["Vor", "mir", "auf", "dem", "Tisch", "stehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "APPR", "ART", "NN", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Bl\u00e4ulichrote Hyazinthen.", "tokens": ["Bl\u00e4u\u00b7lich\u00b7ro\u00b7te", "Hy\u00b7a\u00b7zin\u00b7then", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die krausen Sechsbl\u00e4ttchen sind zur\u00fcckgebogen.", "tokens": ["Die", "krau\u00b7sen", "Sechs\u00b7bl\u00e4tt\u00b7chen", "sind", "zu\u00b7r\u00fcck\u00b7ge\u00b7bo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "VVPP", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Eine Geruchwelle wie von Leichen nach einer Schlacht,", "tokens": ["Ei\u00b7ne", "Ge\u00b7ruch\u00b7wel\u00b7le", "wie", "von", "Lei\u00b7chen", "nach", "ei\u00b7ner", "Schlacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KOKOM", "APPR", "NN", "APPR", "ART", "NN", "$,"], "meter": "+--+--+-+--+-+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Wie von Pestfeldern,", "tokens": ["Wie", "von", "Pest\u00b7fel\u00b7dern", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "$,"], "meter": "+-+--", "measure": "unknown.measure.di"}, "line.6": {"text": "Kommt zu mir von den Blumen hergezogen.", "tokens": ["Kommt", "zu", "mir", "von", "den", "Blu\u00b7men", "her\u00b7ge\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Wie von dumpfen, tr\u00fcben Trieben.", "tokens": ["Wie", "von", "dum\u00b7pfen", ",", "tr\u00fc\u00b7ben", "Trie\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Gr\u00e4\u00dflich.", "tokens": ["Gr\u00e4\u00df\u00b7lich", "."], "token_info": ["word", "punct"], "pos": ["ADJD", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.2": {"line.1": {"text": "Da seh ich ein unendlich r\u00fchrendes Bild:", "tokens": ["Da", "seh", "ich", "ein", "un\u00b7end\u00b7lich", "r\u00fch\u00b7ren\u00b7des", "Bild", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Eine sch\u00f6ne, blasse, ernste junge Frau", "tokens": ["Ei\u00b7ne", "sch\u00f6\u00b7ne", ",", "blas\u00b7se", ",", "erns\u00b7te", "jun\u00b7ge", "Frau"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "$,", "VVFIN", "$,", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Hat die Hyazinthen", "tokens": ["Hat", "die", "Hy\u00b7a\u00b7zin\u00b7then"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Hart an ihre Brust gerissen.", "tokens": ["Hart", "an", "ih\u00b7re", "Brust", "ge\u00b7ris\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sie beugt die Stirn tief hinein,", "tokens": ["Sie", "beugt", "die", "Stirn", "tief", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Und schlie\u00dft die Augen,", "tokens": ["Und", "schlie\u00dft", "die", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Und trinkt den Duft, wie aus einem Giftbecher,", "tokens": ["Und", "trinkt", "den", "Duft", ",", "wie", "aus", "ei\u00b7nem", "Gift\u00b7be\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "PWAV", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-+--", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Als ob sie den Tod ersehne.", "tokens": ["Als", "ob", "sie", "den", "Tod", "er\u00b7seh\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Und sie \u00f6ffnet die Lider", "tokens": ["Und", "sie", "\u00f6ff\u00b7net", "die", "Li\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.10": {"text": "Und sieht vision\u00e4r nach oben.", "tokens": ["Und", "sieht", "vi\u00b7si\u00b7o\u00b7n\u00e4r", "nach", "o\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Dann schlie\u00dfen sich wieder die Lider.", "tokens": ["Dann", "schlie\u00b7\u00dfen", "sich", "wie\u00b7der", "die", "Li\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ADV", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.12": {"text": "Und auf ihnen gewahr ich", "tokens": ["Und", "auf", "ih\u00b7nen", "ge\u00b7wahr", "ich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "ADJD", "PPER"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.13": {"text": "Feine, m\u00fcde \u00c4derchen...", "tokens": ["Fei\u00b7ne", ",", "m\u00fc\u00b7de", "\u00c4\u00b7der\u00b7chen", "..."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$("], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.14": {"text": "Und noch einmal sah ich", "tokens": ["Und", "noch", "ein\u00b7mal", "sah", "ich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "VVFIN", "PPER"], "meter": "--+-+-", "measure": "anapaest.init"}, "line.15": {"text": "Die bl\u00e4ulichroten Hyazinthen:", "tokens": ["Die", "bl\u00e4u\u00b7lich\u00b7ro\u00b7ten", "Hy\u00b7a\u00b7zin\u00b7then", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Ein hei\u00dfer Julitag:", "tokens": ["Ein", "hei\u00b7\u00dfer", "Ju\u00b7li\u00b7tag", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.17": {"text": "Ich gehe im Schatten eines Waldrands", "tokens": ["Ich", "ge\u00b7he", "im", "Schat\u00b7ten", "ei\u00b7nes", "Wald\u00b7rands"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ART", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.18": {"text": "In einem dicken Sandweg.", "tokens": ["In", "ei\u00b7nem", "di\u00b7cken", "Sand\u00b7weg", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.19": {"text": "Die Aussicht nach der andern Seite", "tokens": ["Die", "Aus\u00b7sicht", "nach", "der", "an\u00b7dern", "Sei\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Ist versperrt durch ein Knick.", "tokens": ["Ist", "ver\u00b7sperrt", "durch", "ein", "Knick", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "APPR", "ART", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.21": {"text": "Eine Dame, ohne jede Begleitung,", "tokens": ["Ei\u00b7ne", "Da\u00b7me", ",", "oh\u00b7ne", "je\u00b7de", "Be\u00b7glei\u00b7tung", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUI", "PIAT", "NN", "$,"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.22": {"text": "Kommt mir im Paradegalopp entgegengeritten", "tokens": ["Kommt", "mir", "im", "Pa\u00b7ra\u00b7de\u00b7ga\u00b7lopp", "ent\u00b7ge\u00b7gen\u00b7ge\u00b7rit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "APPRART", "NN", "VVFIN"], "meter": "-+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.23": {"text": "Auf ihrem Hunter.", "tokens": ["Auf", "ih\u00b7rem", "Hun\u00b7ter", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.24": {"text": "Als wir uns begegnen, bleib ich stehn", "tokens": ["Als", "wir", "uns", "be\u00b7geg\u00b7nen", ",", "bleib", "ich", "stehn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "VVINF", "$,", "VVFIN", "PPER", "VVINF"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.25": {"text": "Und ziehe den Hut.", "tokens": ["Und", "zie\u00b7he", "den", "Hut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.26": {"text": "Und sie gr\u00fc\u00dft mich mit der Gerte,", "tokens": ["Und", "sie", "gr\u00fc\u00dft", "mich", "mit", "der", "Ger\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.27": {"text": "Die sie senkrecht bis an die rechte Schl\u00e4fe hebt,", "tokens": ["Die", "sie", "sen\u00b7krecht", "bis", "an", "die", "rech\u00b7te", "Schl\u00e4\u00b7fe", "hebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "ADV", "APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Ihr Haupt zu mir neigend.", "tokens": ["Ihr", "Haupt", "zu", "mir", "nei\u00b7gend", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPER", "ADJD", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.29": {"text": "Ein B\u00fcndelchen Hyazinthen", "tokens": ["Ein", "B\u00fcn\u00b7del\u00b7chen", "Hy\u00b7a\u00b7zin\u00b7then"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.30": {"text": "Ist am Kopf der Gerte mit einem Bastb\u00e4ndchen", "tokens": ["Ist", "am", "Kopf", "der", "Ger\u00b7te", "mit", "ei\u00b7nem", "Bast\u00b7b\u00e4nd\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPRART", "NN", "ART", "NN", "APPR", "ART", "NN"], "meter": "--+-+--+-++-", "measure": "iambic.penta.relaxed"}, "line.31": {"text": "Festgenestelt.", "tokens": ["Fest\u00b7ge\u00b7nes\u00b7telt", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.32": {"text": "Es ist dieselbe sch\u00f6ne, blasse, ernste junge Frau.", "tokens": ["Es", "ist", "die\u00b7sel\u00b7be", "sch\u00f6\u00b7ne", ",", "blas\u00b7se", ",", "erns\u00b7te", "jun\u00b7ge", "Frau", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PDAT", "ADJA", "$,", "VVFIN", "$,", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.33": {"text": "Und \u00fcber alle die kleinen unschuldigen", "tokens": ["Und", "\u00fc\u00b7ber", "al\u00b7le", "die", "klei\u00b7nen", "un\u00b7schul\u00b7di\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PIS", "ART", "ADJA", "NN"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.34": {"text": "Knick- und Waldrandbl\u00fcmchen weht,", "tokens": ["Knick", "und", "Wald\u00b7rand\u00b7bl\u00fcm\u00b7chen", "weht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["TRUNC", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.35": {"text": "Es ist nur wie die letzte Spur eines Hauchs,", "tokens": ["Es", "ist", "nur", "wie", "die", "letz\u00b7te", "Spur", "ei\u00b7nes", "Hauchs", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "KOKOM", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.36": {"text": "Der f\u00fcrchterliche Hyazinthen-Atem.", "tokens": ["Der", "f\u00fcrch\u00b7ter\u00b7li\u00b7che", "Hya\u00b7zin\u00b7then\u0