{"textgrid.poem.45914": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "[s\u00fc\u00dfer Maie Bl\u00fctenjunge]", "genre": "verse", "period": "N.A.", "pub_year": 1804, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "S\u00fc\u00dfer Maie Bl\u00fctenjunge", "tokens": ["S\u00fc\u00b7\u00dfer", "Mai\u00b7e", "Bl\u00fc\u00b7ten\u00b7jun\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bring' ihr bl\u00fchnde Friedenszweige,", "tokens": ["Bring'", "ihr", "bl\u00fchn\u00b7de", "Frie\u00b7dens\u00b7zwei\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Bitte sie mit s\u00fc\u00dfer Zunge,", "tokens": ["Bit\u00b7te", "sie", "mit", "s\u00fc\u00b7\u00dfer", "Zun\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df sie dir die Blume zeige", "tokens": ["Da\u00df", "sie", "dir", "die", "Blu\u00b7me", "zei\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Der sie gerne mag vertrauen", "tokens": ["Der", "sie", "ger\u00b7ne", "mag", "ver\u00b7trau\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADV", "VMFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "In den Busen ihr zu blicken.", "tokens": ["In", "den", "Bu\u00b7sen", "ihr", "zu", "bli\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR