{"dta.poem.8543": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Unaussprechlich .", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1860", "urn": "urn:nbn:de:kobv:b4-200905191007", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Nachtigall in den Kampf sich gab", "tokens": ["Die", "Nach\u00b7ti\u00b7gall", "in", "den", "Kampf", "sich", "gab"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN", "PRF", "VVFIN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mit der Lerche, der schwebenden Stimme,", "tokens": ["Mit", "der", "Ler\u00b7che", ",", "der", "schwe\u00b7ben\u00b7den", "Stim\u00b7me", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Da\u00df ihre Reize bes\u00e4ngen sie", "tokens": ["Da\u00df", "ih\u00b7re", "Rei\u00b7ze", "be\u00b7s\u00e4n\u00b7gen", "sie"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "VVFIN", "PPER"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und all ihre s\u00fc\u00dfe Geberde;", "tokens": ["Und", "all", "ih\u00b7re", "s\u00fc\u00b7\u00dfe", "Ge\u00b7ber\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+----", "measure": "iambic.di.relaxed"}, "line.5": {"text": "Doch die Nachtigallen reihten sich", "tokens": ["Doch", "die", "Nach\u00b7ti\u00b7gal\u00b7len", "reih\u00b7ten", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "PRF"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Und die Lerchen, wie Perlenschn\u00fcre,", "tokens": ["Und", "die", "Ler\u00b7chen", ",", "wie", "Per\u00b7len\u00b7schn\u00fc\u00b7re", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PWAV", "NN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.7": {"text": "All\u2019 lagen sie todt in Gras und Strauch,", "tokens": ["All'", "la\u00b7gen", "sie", "todt", "in", "Gras", "und", "Strauch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADJD", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Verhaucht im s\u00fc\u00dfen Gesange.", "tokens": ["Ver\u00b7haucht", "im", "s\u00fc\u00b7\u00dfen", "Ge\u00b7san\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}}}}