isch Kind.", "tokens": ["Gleich", "dir", "ein", "himm\u00b7lisch", "Kind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "O bleibet, s\u00fc\u00dfe beide,", "tokens": ["O", "blei\u00b7bet", ",", "s\u00fc\u00b7\u00dfe", "bei\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "VVFIN", "PIS", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "O bleibt mir ewig treu!", "tokens": ["O", "bleibt", "mir", "e\u00b7wig", "treu", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADJD", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df fr\u00f6hlich ich im Leide,", "tokens": ["Da\u00df", "fr\u00f6h\u00b7lich", "ich", "im", "Lei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In Freuden traurig sei.", "tokens": ["In", "Freu\u00b7den", "trau\u00b7rig", "sei", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Was flache Toren preisen,", "tokens": ["Was", "fla\u00b7che", "To\u00b7ren", "prei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Das mag mein Gl\u00fcck nicht sein,", "tokens": ["Das", "mag", "mein", "Gl\u00fcck", "nicht", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPOSAT", "NN", "PTKNEG", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wo eur