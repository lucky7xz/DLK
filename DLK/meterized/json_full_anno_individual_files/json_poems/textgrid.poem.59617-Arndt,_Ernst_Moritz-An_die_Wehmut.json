{"textgrid.poem.59617": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "An die Wehmut", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du, die im Sternenschleier", "tokens": ["Du", ",", "die", "im", "Ster\u00b7nen\u00b7schlei\u00b7er"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PPER", "$,", "PRELS", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der N\u00e4chte wandeln geht,", "tokens": ["Der", "N\u00e4ch\u00b7te", "wan\u00b7deln", "geht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wo Traum und Ahnung freier", "tokens": ["Wo", "Traum", "und", "Ah\u00b7nung", "frei\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "KON", "NN", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Um fromme Seelen weht,", "tokens": ["Um", "from\u00b7me", "See\u00b7len", "weht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wo sich von gr\u00fcnen Gr\u00fcften", "tokens": ["Wo", "sich", "von", "gr\u00fc\u00b7nen", "Gr\u00fcf\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PRF", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die gr\u00fcne Hoffnung hebt", "tokens": ["Die", "gr\u00fc\u00b7ne", "Hoff\u00b7nung", "hebt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und in den Himmelsl\u00fcften", "tokens": ["Und", "in", "den", "Him\u00b7mel\u00b7sl\u00fcf\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Mit Engeln selig schwebt,", "tokens": ["Mit", "En\u00b7geln", "se\u00b7lig", "schwebt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die dann um stille Seelen", "tokens": ["Die", "dann", "um", "stil\u00b7le", "See\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ihr zartes Dunkel spinnt,", "tokens": ["Ihr", "zar\u00b7tes", "Dun\u00b7kel", "spinnt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Wunder zu erz\u00e4hlen,", "tokens": ["Die", "Wun\u00b7der", "zu", "er\u00b7z\u00e4h\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die hoch im Himmel sind,", "tokens": ["Die", "hoch", "im", "Him\u00b7mel", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPRART", "NN", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die dann die hellen Saiten", "tokens": ["Die", "dann", "die", "hel\u00b7len", "Sai\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Des tiefsten Herzens r\u00fchrt", "tokens": ["Des", "tiefs\u00b7ten", "Her\u00b7zens", "r\u00fchrt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und durch die langen Zeiten", "tokens": ["Und", "durch", "die", "lan\u00b7gen", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die Geister wandeln f\u00fchrt \u2013", "tokens": ["Die", "Geis\u00b7ter", "wan\u00b7deln", "f\u00fchrt", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Sei s\u00fc\u00df mir, o Huldinne,", "tokens": ["Sei", "s\u00fc\u00df", "mir", ",", "o", "Hul\u00b7din\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "PPER", "$,", "FM", "FM", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sei, Wehmut, mir gegr\u00fc\u00dft!", "tokens": ["Sei", ",", "Weh\u00b7mut", ",", "mir", "ge\u00b7gr\u00fc\u00dft", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "$,", "NN", "$,", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die mild durch alle Sinne", "tokens": ["Die", "mild", "durch", "al\u00b7le", "Sin\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Gleich Himmelsquellen flie\u00dft,", "tokens": ["Gleich", "Him\u00b7mels\u00b7quel\u00b7len", "flie\u00dft", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die Gram und hei\u00dfes Sehnen", "tokens": ["Die", "Gram", "und", "hei\u00b7\u00dfes", "Seh\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "In sanften Schlummer lullt", "tokens": ["In", "sanf\u00b7ten", "Schlum\u00b7mer", "lullt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und in der Flut der Tr\u00e4nen", "tokens": ["Und", "in", "der", "Flut", "der", "Tr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ertr\u00e4nkt die bittre Schuld.", "tokens": ["Er\u00b7tr\u00e4nkt", "die", "bitt\u00b7re", "Schuld", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Dir will ich ewig danken,", "tokens": ["Dir", "will", "ich", "e\u00b7wig", "dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dir, meiner N\u00e4chte Lust,", "tokens": ["Dir", ",", "mei\u00b7ner", "N\u00e4ch\u00b7te", "Lust", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die weich mit Himmelsranken", "tokens": ["Die", "weich", "mit", "Him\u00b7mels\u00b7ran\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJD", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Umflicht die wunde Brust,", "tokens": ["Um\u00b7flicht", "die", "wun\u00b7de", "Brust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die s\u00fc\u00dfe Liebesworte", "tokens": ["Die", "s\u00fc\u00b7\u00dfe", "Lie\u00b7bes\u00b7wor\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mit Engelt\u00f6nen singt", "tokens": ["Mit", "En\u00b7gel\u00b7t\u00f6\u00b7nen", "singt"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und an der Himmelspforte", "tokens": ["Und", "an", "der", "Him\u00b7mel\u00b7spfor\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der Sehnsucht Glocken ringt.", "tokens": ["Der", "Sehn\u00b7sucht", "Glo\u00b7cken", "ringt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Dich will ich ewig loben,", "tokens": ["Dich", "will", "ich", "e\u00b7wig", "lo\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dich und die Schwester dein,", "tokens": ["Dich", "und", "die", "Schwes\u00b7ter", "dein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KON", "ART", "NN", "PPOSAT", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Liebe, die nach oben", "tokens": ["Die", "Lie\u00b7be", ",", "die", "nach", "o\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Auch lockt der lichte Schein,", "tokens": ["Auch", "lockt", "der", "lich\u00b7te", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die Liebe, die auf Erden", "tokens": ["Die", "Lie\u00b7be", ",", "die", "auf", "Er\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wohl nie Gen\u00fcge find't,", "tokens": ["Wohl", "nie", "Ge\u00b7n\u00fc\u00b7ge", "find't", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Oft traurig an Geb\u00e4rden,", "tokens": ["Oft", "trau\u00b7rig", "an", "Ge\u00b7b\u00e4r\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Gleich dir ein himmlisch Kind.", "tokens": ["Gleich", "dir", "ein", "himm\u00b7lisch", "Kind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "O bleibet, s\u00fc\u00dfe beide,", "tokens": ["O", "blei\u00b7bet", ",", "s\u00fc\u00b7\u00dfe", "bei\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "VVFIN", "PIS", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "O bleibt mir ewig treu!", "tokens": ["O", "bleibt", "mir", "e\u00b7wig", "treu", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADJD", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df fr\u00f6hlich ich im Leide,", "tokens": ["Da\u00df", "fr\u00f6h\u00b7lich", "ich", "im", "Lei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In Freuden traurig sei.", "tokens": ["In", "Freu\u00b7den", "trau\u00b7rig", "sei", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Was flache Toren preisen,", "tokens": ["Was", "fla\u00b7che", "To\u00b7ren", "prei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Das mag mein Gl\u00fcck nicht sein,", "tokens": ["Das", "mag", "mein", "Gl\u00fcck", "nicht", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPOSAT", "NN", "PTKNEG", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wo eure Sterne kreisen,", "tokens": ["Wo", "eu\u00b7re", "Ster\u00b7ne", "krei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Da kann ich selig sein.", "tokens": ["Da", "kann", "ich", "se\u00b7lig", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Du, die im Sternenschleier", "tokens": ["Du", ",", "die", "im", "Ster\u00b7nen\u00b7schlei\u00b7er"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PPER", "$,", "PRELS", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der N\u00e4chte wandeln geht,", "tokens": ["Der", "N\u00e4ch\u00b7te", "wan\u00b7deln", "geht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wo Traum und Ahnung freier"