{"textgrid.poem.63026": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Adler hielt auf der bereiften Spitze", "genre": "verse", "period": "N.A.", "pub_year": 1785, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Adler hielt auf der bereiften Spitze", "tokens": ["Der", "Ad\u00b7ler", "hielt", "auf", "der", "be\u00b7reif\u00b7ten", "Spit\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des himmelhohen Kaukasus", "tokens": ["Des", "him\u00b7mel\u00b7ho\u00b7hen", "Kau\u00b7ka\u00b7sus"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sein Parlament. Er legte seine Blitze", "tokens": ["Sein", "Par\u00b7la\u00b7ment", ".", "Er", "leg\u00b7te", "sei\u00b7ne", "Blit\u00b7ze"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$.", "PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Voll Huld zu seines Thrones Fu\u00df,", "tokens": ["Voll", "Huld", "zu", "sei\u00b7nes", "Thro\u00b7nes", "Fu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und wog den Gro\u00dfen und dem Volke", "tokens": ["Und", "wog", "den", "Gro\u00b7\u00dfen", "und", "dem", "Vol\u00b7ke"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Das Recht in ebnen Schalen aus.", "tokens": ["Das", "Recht", "in", "eb\u00b7nen", "Scha\u00b7len", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da fuhr, gleich einem Strahl aus einer Donnerwolke,", "tokens": ["Da", "fuhr", ",", "gleich", "ei\u00b7nem", "Strahl", "aus", "ei\u00b7ner", "Don\u00b7ner\u00b7wol\u00b7ke", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "ADV", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ein Habicht in das Oberhaus.", "tokens": ["Ein", "Ha\u00b7bicht", "in", "das", "O\u00b7ber\u00b7haus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Er hielt ein fremdes Thier in seinen Krallen;", "tokens": ["Er", "hielt", "ein", "frem\u00b7des", "Thier", "in", "sei\u00b7nen", "Kral\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Es war ein alter Kakadu,", "tokens": ["Es", "war", "ein", "al\u00b7ter", "Ka\u00b7ka\u00b7du", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Der Indostan verlie\u00df, um durch die Welt zu wallen.", "tokens": ["Der", "In\u00b7dos\u00b7tan", "ver\u00b7lie\u00df", ",", "um", "durch", "die", "Welt", "zu", "wal\u00b7len", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "KOUI", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Sir! rief dem Schach der Schnapphahn zu:", "tokens": ["Sir", "!", "rief", "dem", "Schach", "der", "Schnap\u00b7phahn", "zu", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Hier ist ein arger Wicht, der dir dein Erzamt raubet,", "tokens": ["Hier", "ist", "ein", "ar\u00b7ger", "Wicht", ",", "der", "dir", "dein", "Er\u00b7zamt", "rau\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "$,", "PRELS", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Ein Philosoph, der den Olymp zerst\u00f6rt,", "tokens": ["Ein", "Phi\u00b7lo\u00b7soph", ",", "der", "den", "O\u00b7lymp", "zer\u00b7st\u00f6rt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.15": {"text": "Der keinen Zevs und keinen Pluto glaubet,", "tokens": ["Der", "kei\u00b7nen", "Zevs", "und", "kei\u00b7nen", "Plu\u00b7to", "glau\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NE", "KON", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Und nur bey seinem Brama schw\u00f6rt:", "tokens": ["Und", "nur", "bey", "sei\u00b7nem", "Bra\u00b7ma", "schw\u00f6rt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Ja, was noch \u00e4rger ist, er macht sich ein Gewissen,", "tokens": ["Ja", ",", "was", "noch", "\u00e4r\u00b7ger", "ist", ",", "er", "macht", "sich", "ein", "Ge\u00b7wis\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PRELS", "ADV", "ADJD", "VAFIN", "$,", "PPER", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Die Kost, die meinen K\u00f6nig n\u00e4hrt,", "tokens": ["Die", "Kost", ",", "die", "mei\u00b7nen", "K\u00f6\u00b7nig", "n\u00e4hrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Das Fleisch der Thiere zu genie\u00dfen,", "tokens": ["Das", "Fleisch", "der", "Thie\u00b7re", "zu", "ge\u00b7nie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Drum halt ich ihn des Todes werth.", "tokens": ["Drum", "halt", "ich", "ihn", "des", "To\u00b7des", "werth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PPER", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "\u00bbda Zevs ihn leben l\u00e4\u00dft, so la\u00df auch ich ihn leben.\u00ab", "tokens": ["\u00bb", "da", "Zevs", "ihn", "le\u00b7ben", "l\u00e4\u00dft", ",", "so", "la\u00df", "auch", "ich", "ihn", "le\u00b7ben", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "KOUS", "NE", "PPER", "VVINF", "VVFIN", "$,", "ADV", "VVIMP", "ADV", "PPER", "PPER", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Versetzt der gute Schach, und winkt ihn loszugeben.", "tokens": ["Ver\u00b7setzt", "der", "gu\u00b7te", "Schach", ",", "und", "winkt", "ihn", "los\u00b7zu\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,", "KON", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Der Inquisitor barst vor Wuth;", "tokens": ["Der", "In\u00b7qui\u00b7si\u00b7tor", "barst", "vor", "Wuth", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "NN", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.24": {"text": "Allein das Hofgesind, zumal die Papageyen,", "tokens": ["Al\u00b7lein", "das", "Hof\u00b7ge\u00b7sind", ",", "zu\u00b7mal", "die", "Pa\u00b7pa\u00b7ge\u00b7yen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "KOUS", "ART", "NN", "$,"], "meter": "---+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.25": {"text": "Der Virtuos aus Calekut,", "tokens": ["Der", "Vir\u00b7tu\u00b7os", "aus", "Ca\u00b7le\u00b7kut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Und die beredte G\u00e4nsebrut", "tokens": ["Und", "die", "be\u00b7red\u00b7te", "G\u00e4n\u00b7seb\u00b7rut"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Verg\u00f6tterten in wilden Melodeyen", "tokens": ["Ver\u00b7g\u00f6t\u00b7ter\u00b7ten", "in", "wil\u00b7den", "Me\u00b7lo\u00b7de\u00b7yen"], "token_info"