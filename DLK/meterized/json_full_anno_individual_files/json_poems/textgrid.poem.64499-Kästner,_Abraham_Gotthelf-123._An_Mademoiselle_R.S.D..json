{"textgrid.poem.64499": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "123. An Mademoiselle R.S.D.", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So soll ich dich, entz\u00fcckend ", "tokens": ["So", "soll", "ich", "dich", ",", "ent\u00b7z\u00fc\u00b7ckend"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "$,", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das Unschuld nur, sonst nichts, zum Kinde macht!", "tokens": ["Das", "Un\u00b7schuld", "nur", ",", "sonst", "nichts", ",", "zum", "Kin\u00b7de", "macht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "ADV", "PIS", "$,", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und soll nicht mehr die zarten Lippen k\u00fcssen,", "tokens": ["Und", "soll", "nicht", "mehr", "die", "zar\u00b7ten", "Lip\u00b7pen", "k\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PTKNEG", "ADV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Aus denen Witz von achtzehn Jahren lacht.", "tokens": ["Aus", "de\u00b7nen", "Witz", "von", "acht\u00b7zehn", "Jah\u00b7ren", "lacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "NN", "APPR", "CARD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Dein sanfter Ernst, dein sittsam muntres Wesen,", "tokens": ["Dein", "sanf\u00b7ter", "Ernst", ",", "dein", "sitt\u00b7sam", "mun\u00b7tres", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PPOSAT", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Das allzu oft erwachsnen Sch\u00f6nen fehlt,", "tokens": ["Das", "all\u00b7zu", "oft", "er\u00b7wachs\u00b7nen", "Sch\u00f6\u00b7nen", "fehlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PTKA", "ADV", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Der edle Trieb, mit so viel Lust zu lesen,", "tokens": ["Der", "ed\u00b7le", "Trieb", ",", "mit", "so", "viel", "Lust", "zu", "le\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "ADV", "PIAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Als jener Flei\u00df groteske Tarocs z\u00e4hlt;", "tokens": ["Als", "je\u00b7ner", "Flei\u00df", "gro\u00b7tes\u00b7ke", "Ta\u00b7rocs", "z\u00e4hlt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-++--+-+", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Dein g\u00fctig Herz r\u00e4umt, was es selbst begehret,", "tokens": ["Dein", "g\u00fc\u00b7tig", "Herz", "r\u00e4umt", ",", "was", "es", "selbst", "be\u00b7ge\u00b7hret", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "VVFIN", "$,", "PWS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.10": {"text": "Gro\u00dfm\u00fcthig Kind! des Bruders W\u00fcnschen ein,", "tokens": ["Gro\u00df\u00b7m\u00fct\u00b7hig", "Kind", "!", "des", "Bru\u00b7ders", "W\u00fcn\u00b7schen", "ein", ","], "token_info": ["