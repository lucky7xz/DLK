{"textgrid.poem.26121": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Die kleinen schwachblauen Vergi\u00dfmeinnicht", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die kleinen schwachblauen Vergi\u00dfmeinnicht", "tokens": ["Die", "klei\u00b7nen", "schwach\u00b7blau\u00b7en", "Ver\u00b7gi\u00df\u00b7mein\u00b7nicht"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Sind die Blumen vom wachsenden Vertrauen.", "tokens": ["Sind", "die", "Blu\u00b7men", "vom", "wach\u00b7sen\u00b7den", "Ver\u00b7trau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Sie sehen dir offenherzig ins Angesicht", "tokens": ["Sie", "se\u00b7hen", "dir", "of\u00b7fen\u00b7her\u00b7zig", "ins", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "APPRART", "NN"], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Wie Gedanken, die im Denken aufschauen;", "tokens": ["Wie", "Ge\u00b7dan\u00b7ken", ",", "die", "im", "Den\u00b7ken", "auf\u00b7schau\u00b7en", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "$,", "PRELS", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Gedanken, die Pl\u00e4ne ins Gr\u00fcne bauen,", "tokens": ["Ge\u00b7dan\u00b7ken", ",", "die", "Pl\u00e4\u00b7ne", "ins", "Gr\u00fc\u00b7ne", "bau\u00b7en", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punc