{"textgrid.poem.64435": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "56. Eines Sachsen Wunsch auf Karl den XII.", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Held, der uns so gepre\u00dft, dein eifriges Bestreben", "tokens": ["Held", ",", "der", "uns", "so", "ge\u00b7pre\u00dft", ",", "dein", "eif\u00b7ri\u00b7ges", "Be\u00b7stre\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "PPER", "ADV", "VVPP", "$,", "PPOSAT", "ADJA", "NN"], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "War, sp\u00e4t im eitlen Hauch der letzten Welt zu leben;", "tokens": ["War", ",", "sp\u00e4t", "im", "eit\u00b7len", "Hauch", "der", "letz\u00b7ten", "Welt", "zu", "le\u00b7ben", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "ADJD", "APPRART", "ADJA", "NN", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch, wird mein Wunsch erf\u00fcllt, die Rache giebt ihn ein,", "tokens": ["Doch", ",", "wird", "mein", "Wunsch", "er\u00b7f\u00fcllt", ",", "die", "Ra\u00b7che", "giebt", "ihn", "ein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VAFIN", "PPOSAT", "NN", "VVPP", "$,", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So soll einst dein Homer ein zweyter", "tokens": ["So", "soll", "einst", "dein", "Ho\u00b7mer", "ein", "zwey\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV