{"textgrid.poem.42966": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "An meine Herberge in Stuttgart", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr habt mich reich und leise", "tokens": ["Ihr", "habt", "mich", "reich", "und", "lei\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Verw\u00f6hnt. Das mir geschenkte Gl\u00fcck \u2013", "tokens": ["Ver\u00b7w\u00f6hnt", ".", "Das", "mir", "ge\u00b7schenk\u00b7te", "Gl\u00fcck", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$.", "ART", "PPER", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In irgendwelcher Weise", "tokens": ["In", "ir\u00b7gend\u00b7wel\u00b7cher", "Wei\u00b7se"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Kehrt es gewi\u00df zu euch zur\u00fcck.", "tokens": ["Kehrt", "es", "ge\u00b7wi\u00df", "zu", "euch", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie ich Meinzeit durchhetze,", "tokens": ["Wie", "ich", "Mein\u00b7zeit", "durch\u00b7het\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Geb ich euch keine Dankbarkeit.", "tokens": ["Geb", "ich", "euch", "kei\u00b7ne", "Dank\u00b7bar\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "