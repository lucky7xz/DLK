{"textgrid.poem.48111": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "1L: Durch Alltagsl\u00e4rm und Fabrikenstaub", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Durch Alltagsl\u00e4rm und Fabrikenstaub", "tokens": ["Durch", "All\u00b7tags\u00b7l\u00e4rm", "und", "Fab\u00b7ri\u00b7ken\u00b7staub"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "leuchtet ein Tag voll Licht und Laub.", "tokens": ["leuch\u00b7tet", "ein", "Tag", "voll", "Licht", "und", "Laub", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "NN", "KON", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.2": {"line.1": {"text": "Leuchtet ein einziger Tag im Jahr:", "tokens": ["Leuch\u00b7tet", "ein", "ein\u00b7zi\u00b7ger", "Tag", "im", "Jahr", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "rote Rosen im blonden Haar.", "tokens": ["ro\u00b7te", "Ro\u00b7sen", "im", "blon\u00b7den", "Haar", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.3": {"line.1": {"text": "In Wanderschuhen, im Festgewand", "tokens": ["In", "Wan\u00b7der\u00b7schu\u00b7hen", ",", "im", "Fest\u00b7ge\u00b7wand"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "$,", "APPRART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "schreitet der leuchtende Tag durchs Land.", "tokens": ["schrei\u00b7tet", "der", "leuch\u00b7ten\u00b7de", "Tag", "durchs", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "Und wo er wandert durch Feld und Dorn,", "tokens": ["Und", "wo", "er", "wan\u00b7dert", "durch", "Feld", "und", "Dorn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "da bl\u00fchen die Schlehen und treibt das Korn.", "tokens": ["da", "bl\u00fc\u00b7hen", "die", "Schle\u00b7hen", "und", "treibt", "das", "Korn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.5": {"line.1": {"text": "Und wo er r\u00fchrt an des Armen Haus,", "tokens": ["Und", "wo", "er", "r\u00fchrt", "an", "des", "Ar\u00b7men", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "lockt er lachende Menschen hinaus.", "tokens": ["lockt", "er", "la\u00b7chen\u00b7de", "Men\u00b7schen", "hin\u00b7aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Viel jubelnde Kindlein laufen mit \u2013", "tokens": ["Viel", "ju\u00b7beln\u00b7de", "Kin\u00b7dlein", "lau\u00b7fen", "mit", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "APPR", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "hart auf der Erde dr\u00f6hnt sein Schritt:", "tokens": ["hart", "auf", "der", "Er\u00b7de", "dr\u00f6hnt", "sein", "Schritt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Bis in die Tiefen der ewigen Nacht,", "tokens": ["Bis", "in", "die", "Tie\u00b7fen", "der", "e\u00b7wi\u00b7gen", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "zu den schwarzen Gesellen im Kohlenschacht.", "tokens": ["zu", "den", "schwar\u00b7zen", "Ge\u00b7sel\u00b7len", "im", "Koh\u00b7len\u00b7schacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}}, "stanza.8": {"line.1": {"text": "Da sinkt der Hammer zum letzten Schlag \u2013", "tokens": ["Da", "sinkt", "der", "Ham\u00b7mer", "zum", "letz\u00b7ten", "Schlag", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "ADJA", "NN", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "und sie steigen empor und gr\u00fc\u00dfen den Tag.", "tokens": ["und", "sie", "stei\u00b7gen", "em\u00b7por", "und", "gr\u00fc\u00b7\u00dfen", "den", "Tag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PTKVZ", "KON", "VVFIN", "ART", "NN", "$."], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}}, "stanza.9": {"line.1": {"text": "Und gr\u00fc\u00dfen das Licht und das blauende Meer . . .", "tokens": ["Und", "gr\u00fc\u00b7\u00dfen", "das", "Licht", "und", "das", "blau\u00b7en\u00b7de", "Meer", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "KON", "ART", "ADJA", "NN", "$.", "$.", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Ueber die Wogen rollt es daher.", "tokens": ["Ue\u00b7ber", "die", "Wo\u00b7gen", "rollt", "es", "da\u00b7her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "PAV", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}}, "stanza.10": {"line.1": {"text": "Heim ziehen die Schiffe in freudiger Hast \u2013", "tokens": ["Heim", "zie\u00b7hen", "die", "Schif\u00b7fe", "in", "freu\u00b7di\u00b7ger", "Hast", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$("], "meter": "++--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "der rote Wimpel fliegt am Mast.", "tokens": ["der", "ro\u00b7te", "Wim\u00b7pel", "fliegt", "am", "Mast", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Und \u00fcber der Menschheit, erl\u00f6st und frei,", "tokens": ["Und", "\u00fc\u00b7ber", "der", "Menschheit", ",", "er\u00b7l\u00f6st", "und", "frei", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "VVFIN", "KON", "ADJD", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "leuchtet der gro\u00dfe, der heilige Mai!", "tokens": ["leuch\u00b7tet", "der", "gro\u00b7\u00dfe", ",", "der", "hei\u00b7li\u00b7ge", "Mai", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "$,", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.12": {"line.1": {"text": "O Mai der Menschheit, du Traum voll Gl\u00fcck!", "tokens": ["O", "Mai", "der", "Menschheit", ",", "du", "Traum", "voll", "Gl\u00fcck", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ART", "NN", "$,", "PPER", "NN", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u2013 Eine Tr\u00e4ne feuchtet den Mannesblick . . .", "tokens": ["\u2013", "Ei\u00b7ne", "Tr\u00e4\u00b7ne", "feuch\u00b7tet", "den", "Man\u00b7nes\u00b7blick", ".", ".", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "ART", "NN", "$.", "$.", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.13": {"line.1": {"text": "O Tag der Zukunft, voll Glanz und Gr\u00fcn,", "tokens": ["O", "Tag", "der", "Zu\u00b7kunft", ",", "voll", "Glanz", "und", "Gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ART", "NN", "$,", "ADJD", "NN", "KON", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "wann wird deine strahlende Sonne gl\u00fchn?", "tokens": ["wann", "wird", "dei\u00b7ne", "strah\u00b7len\u00b7de", "Son\u00b7ne", "gl\u00fchn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "----+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.14": {"line.1": {"text": "O Fest der Freiheit, du bl\u00fchender Mai,", "tokens": ["O", "Fest", "der", "Frei\u00b7heit", ",", "du", "bl\u00fc\u00b7hen\u00b7der", "Mai", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ART", "NN", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "kein Sinnen und Tr\u00e4umen sehnt dich herbei.", "tokens": ["kein", "Sin\u00b7nen", "und", "Tr\u00e4u\u00b7men", "sehnt", "dich", "her\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.15": {"line.1": {"text": "Dich hegt das Dunkel, dich schirmt die Macht \u2013", "tokens": ["Dich", "hegt", "das", "Dun\u00b7kel", ",", "dich", "schirmt", "die", "Macht", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "und wir m\u00fcssen hindurch durch den Kampf und die Nacht!", "tokens": ["und", "wir", "m\u00fcs\u00b7sen", "hin\u00b7durch", "durch", "den", "Kampf", "und", "die", "Nacht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PAV", "APPR", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "--+---+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.16": {"line.1": {"text": "Die weichliche Tr\u00e4ne versiegt und erstarrt,", "tokens": ["Die", "weich\u00b7li\u00b7che", "Tr\u00e4\u00b7ne", "ver\u00b7siegt", "und", "er\u00b7starrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "KON", "VVFIN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "und es ruft deine Zukunft: \u00bbMein Volk, werde hart!", "tokens": ["und", "es", "ruft", "dei\u00b7ne", "Zu\u00b7kunft", ":", "\u00bb", "Mein", "Volk", ",", "wer\u00b7de", "hart", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "NN", "$.", "$(", "PPOSAT", "NN", "$,", "VAFIN", "ADJD", "$."], "meter": "--+--+-++--+", "measure": "anapaest.di.plus"}}, "stanza.17": {"line.1": {"text": "Werde hart und sei wach, du, und schlage den Schlag,", "tokens": ["Wer\u00b7de", "hart", "und", "sei", "wach", ",", "du", ",", "und", "schla\u00b7ge", "den", "Schlag", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "KON", "VAFIN", "ADJD", "$,", "PPER", "$,", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "--+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "dein harrt ein fruchtschwangerer Sommertag!", "tokens": ["dein", "harrt", "ein", "frucht\u00b7schwan\u00b7ge\u00b7rer", "Som\u00b7mer\u00b7tag", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.18": {"line.1": {"text": "Und w\u00e4hlst du gut, wie das Los dir f\u00e4llt,", "tokens": ["Und", "w\u00e4hlst", "du", "gut", ",", "wie", "das", "Los", "dir", "f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "$,", "PWAV", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "so ist dein die Macht und die bl\u00fchende Welt \u2013", "tokens": ["so", "ist", "dein", "die", "Macht", "und", "die", "bl\u00fc\u00b7hen\u00b7de", "Welt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "