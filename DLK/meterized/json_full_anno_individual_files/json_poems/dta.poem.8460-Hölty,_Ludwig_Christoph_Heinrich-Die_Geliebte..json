{"dta.poem.8460": {"metadata": {"author": {"name": "H\u00f6lty, Ludwig Christoph Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Geliebte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1783", "urn": "urn:nbn:de:kobv:b4-200905192568", "language": ["de:0.99"], "booktitle": "H\u00f6lty, Ludwig Christoph Heinrich: Gedichte. Hamburg, 1783."}, "poem": {"stanza.1": {"line.1": {"text": "Br\u00e4cht' ein g\u00fctig Geschick mich ihr entgegen,", "tokens": ["Br\u00e4cht'", "ein", "g\u00fc\u00b7tig", "Ge\u00b7schick", "mich", "ihr", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJD", "NN", "PPER", "PPER", "PTKVZ", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Eine fl\u00fcgelschnelle Minut' in ihrem", "tokens": ["Ei\u00b7ne", "fl\u00fc\u00b7gel\u00b7schnel\u00b7le", "Mi\u00b7nut'", "in", "ih\u00b7rem"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "Himmel zu athmen;", "tokens": ["Him\u00b7mel", "zu", "ath\u00b7men", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Seliger w\u00e4r' ich dann, als Staubbewohner!", "tokens": ["Se\u00b7li\u00b7ger", "w\u00e4r'", "ich", "dann", ",", "als", "Staub\u00b7be\u00b7woh\u00b7ner", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "ADV", "$,", "KOUS", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "O dann w\u00fcrd' ich den Fr\u00fchling besser f\u00fchlen,", "tokens": ["O", "dann", "w\u00fcrd'", "ich", "den", "Fr\u00fch\u00b7ling", "bes\u00b7ser", "f\u00fch\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "VAFIN", "PPER", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Besser meinen Sch\u00f6pfer in jeder Blume", "tokens": ["Bes\u00b7ser", "mei\u00b7nen", "Sch\u00f6p\u00b7fer", "in", "je\u00b7der", "Blu\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "NN", "APPR", "PIAT", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Schauen und lieben!", "tokens": ["Schau\u00b7en", "und", "lie\u00b7ben", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}}}}