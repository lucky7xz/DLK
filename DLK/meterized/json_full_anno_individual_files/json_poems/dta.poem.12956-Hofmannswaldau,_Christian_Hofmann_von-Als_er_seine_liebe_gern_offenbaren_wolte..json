{"dta.poem.12956": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Als er seine liebe gern offenbaren  \n wolte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Schweig ich, so nimmt mein schmertz von tag zu tage zu.", "tokens": ["Schweig", "ich", ",", "so", "nimmt", "mein", "schmertz", "von", "tag", "zu", "ta\u00b7ge", "zu", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADV", "VVFIN", "PPOSAT", "NN", "APPR", "NN", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Red ich, so w\u00e4chst ihr zorn zum nachtheil meiner ruh.", "tokens": ["Red", "ich", ",", "so", "w\u00e4chst", "ihr", "zorn", "zum", "nacht\u00b7heil", "mei\u00b7ner", "ruh", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "ADV", "VVFIN", "PPOSAT", "NN", "APPRART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Hier \u00f6ffnet Seylla sich, dort steht Charybdis offen;", "tokens": ["Hier", "\u00f6ff\u00b7net", "Seyl\u00b7la", "sich", ",", "dort", "steht", "Cha\u00b7ryb\u00b7dis", "of\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "PRF", "$,", "ADV", "VVFIN", "NE", "PTKVZ", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Was ist nun hier vor mich zu hoffen?", "tokens": ["Was", "ist", "nun", "hier", "vor", "mich", "zu", "hof\u00b7fen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "ADV", "APPR", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Drum, Liebe! nimm dich meiner an,", "tokens": ["Drum", ",", "Lie\u00b7be", "!", "nimm", "dich", "mei\u00b7ner", "an", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "NN", "$.", "VVIMP", "PPER", "PPOSAT", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und f\u00fchre Sylvien die tief-verborgnen schmertzen", "tokens": ["Und", "f\u00fch\u00b7re", "Syl\u00b7vi\u00b7en", "die", "tie\u00b7fver\u00b7bor\u00b7gnen", "schmert\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "NE", "ART", "ADJA", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Durch meiner augen sprach\u2019 und nachdruck so zu hertzen,", "tokens": ["Durch", "mei\u00b7ner", "au\u00b7gen", "sprach'", "und", "nach\u00b7druck", "so", "zu", "hert\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "KON", "NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Da\u00df sie nicht anders thun, als sich erbarmen kan.", "tokens": ["Da\u00df", "sie", "nicht", "an\u00b7ders", "thun", ",", "als", "sich", "er\u00b7bar\u00b7men", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADV", "VVINF", "$,", "KOUS", "PRF", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}