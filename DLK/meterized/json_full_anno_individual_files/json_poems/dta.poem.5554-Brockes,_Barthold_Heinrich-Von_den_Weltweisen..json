{"dta.poem.5554": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Weltweisen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sie konnten von der Welt-Weisheit", "tokens": ["Sie", "konn\u00b7ten", "von", "der", "Welt\u00b7Weis\u00b7heit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Als Ober-Richter Urtheil f\u00e4llen.", "tokens": ["Als", "O\u00b7ber\u00b7Rich\u00b7ter", "Ur\u00b7theil", "f\u00e4l\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sie unterrichteten die Menschen; sie entdeckten,", "tokens": ["Sie", "un\u00b7ter\u00b7rich\u00b7te\u00b7ten", "die", "Men\u00b7schen", ";", "sie", "ent\u00b7deck\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$.", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wie weit die Kr\u00e4ffte sich von der Vernunfft erstreckten.", "tokens": ["Wie", "weit", "die", "Kr\u00e4ff\u00b7te", "sich", "von", "der", "Ver\u00b7nunfft", "er\u00b7streck\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "PRF", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Die seltensten Gem\u00fchtes-Gaben,", "tokens": ["Die", "sel\u00b7ten\u00b7sten", "Ge\u00b7m\u00fch\u00b7tes\u00b7Ga\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die sie, durch feur\u2019gen Trieb entglommen,", "tokens": ["Die", "sie", ",", "durch", "feur'\u00b7gen", "Trieb", "ent\u00b7glom\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "$,", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Aufs fleissigste gepfleget haben,", "tokens": ["Aufs", "fleis\u00b7sigs\u00b7te", "ge\u00b7pfle\u00b7get", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "VVPP", "VAINF", "$,"], "meter": "-++--+-+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Die sind in ihrer Zunfft zum h\u00f6chsten Gipffel kommen.", "tokens": ["Die", "sind", "in", "ih\u00b7rer", "Zunfft", "zum", "h\u00f6chs\u00b7ten", "Gipf\u00b7fel", "kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "PPOSAT", "NN", "APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Nichts gutes findet man als da, wo sie gewandelt.", "tokens": ["Nichts", "gu\u00b7tes", "fin\u00b7det", "man", "als", "da", ",", "wo", "sie", "ge\u00b7wan\u00b7delt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "ADJA", "VVFIN", "PIS", "KOKOM", "ADV", "$,", "PWAV", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Zum Muster dient ein Ding, wenn sie es abgehandelt.", "tokens": ["Zum", "Mus\u00b7ter", "dient", "ein", "Ding", ",", "wenn", "sie", "es", "ab\u00b7ge\u00b7han\u00b7delt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$,", "KOUS", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Sie haben es dahin gebracht,", "tokens": ["Sie", "ha\u00b7ben", "es", "da\u00b7hin", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PAV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Da\u00df wir selbst ihre Fehler ehren.", "tokens": ["Da\u00df", "wir", "selbst", "ih\u00b7re", "Feh\u00b7ler", "eh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Doch worauf soll man sich verlassen? da die Lehren,", "tokens": ["Doch", "wo\u00b7rauf", "soll", "man", "sich", "ver\u00b7las\u00b7sen", "?", "da", "die", "Leh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "VMFIN", "PIS", "PRF", "VVINF", "$.", "KOUS", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Die einen sichern Weg zur Wahrheit sollten zeigen,", "tokens": ["Die", "ei\u00b7nen", "si\u00b7chern", "Weg", "zur", "Wahr\u00b7heit", "soll\u00b7ten", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "APPRART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Die einzig ist, wozu wir alles Sinnen neigen,", "tokens": ["Die", "ein\u00b7zig", "ist", ",", "wo\u00b7zu", "wir", "al\u00b7les", "Sin\u00b7nen", "nei\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VAFIN", "$,", "PWAV", "PPER", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "In Secten sich zertheilen, sich zertrennen?", "tokens": ["In", "Sec\u00b7ten", "sich", "zer\u00b7thei\u00b7len", ",", "sich", "zer\u00b7tren\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "PRF", "VVINF", "$,", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Und was wird man mit Recht erwehlen k\u00f6nnen", "tokens": ["Und", "was", "wird", "man", "mit", "Recht", "er\u00b7weh\u00b7len", "k\u00f6n\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "VAFIN", "PIS", "APPR", "NN", "VVINF", "VMINF"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.18": {"text": "Aus den unz\u00e4hligen Gedancken,", "tokens": ["Aus", "den", "un\u00b7z\u00e4h\u00b7li\u00b7gen", "Ge\u00b7dan\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.19": {"text": "Die nichts sind, als nur Streit und Zancken?", "tokens": ["Die", "nichts", "sind", ",", "als", "nur", "Streit", "und", "Zan\u00b7cken", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VAFIN", "$,", "KOUS", "ADV", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}