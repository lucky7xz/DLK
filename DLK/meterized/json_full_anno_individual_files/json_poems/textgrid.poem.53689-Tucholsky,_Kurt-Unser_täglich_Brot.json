{"textgrid.poem.53689": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Unser t\u00e4glich Brot", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "An deinem Brot f\u00fcr f\u00fcnfzehn Mark und achtzig", "tokens": ["An", "dei\u00b7nem", "Brot", "f\u00fcr", "f\u00fcnf\u00b7zehn", "Mark", "und", "acht\u00b7zig"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "CARD", "NN", "KON", "CARD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "h\u00e4ngt, wenn du hinsiehst, allerlei \u2013:", "tokens": ["h\u00e4ngt", ",", "wenn", "du", "hin\u00b7siehst", ",", "al\u00b7ler\u00b7lei", "\u2013", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PPER", "VVFIN", "$,", "PIAT", "$(", "$."], "meter": "---+-+-+", "measure": "unknown.measure.tri"}}, "stanza.2": {"line.1": {"text": "Der Landmann l\u00e4\u00dft sich neue St\u00e4lle bauen,", "tokens": ["Der", "Land\u00b7mann", "l\u00e4\u00dft", "sich", "neu\u00b7e", "St\u00e4l\u00b7le", "bau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "beh\u00e4ngt mit Pelz und Perlen seine Frauen;", "tokens": ["be\u00b7h\u00e4ngt", "mit", "Pelz", "und", "Per\u00b7len", "sei\u00b7ne", "Frau\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "KON", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "er z\u00e4hlt das Geld nicht mehr \u2013 er mu\u00df es wiegen \u2013", "tokens": ["er", "z\u00e4hlt", "das", "Geld", "nicht", "mehr", "\u2013", "er", "mu\u00df", "es", "wie\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKNEG", "ADV", "$(", "PPER", "VMFIN", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "wo soll er nur den Krempel unterkriegen?", "tokens": ["wo", "soll", "er", "nur", "den", "Krem\u00b7pel", "un\u00b7ter\u00b7krie\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Im Flusse treibt ein neues Segelboot \u2013", "tokens": ["Im", "Flus\u00b7se", "treibt", "ein", "neu\u00b7es", "Se\u00b7gel\u00b7boot", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "von deinem Brot.", "tokens": ["von", "dei\u00b7nem", "Brot", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Die M\u00fchlen mahlen. Unternehmer grinsen.", "tokens": ["Die", "M\u00fch\u00b7len", "mah\u00b7len", ".", "Un\u00b7ter\u00b7neh\u00b7mer", "grin\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$.", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Werke tragen unerh\u00f6rte Zinsen.", "tokens": ["Die", "Wer\u00b7ke", "tra\u00b7gen", "un\u00b7er\u00b7h\u00f6r\u00b7te", "Zin\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Kein K\u00e4ufer streikt. Er kann und mu\u00df es tragen.", "tokens": ["Kein", "K\u00e4u\u00b7fer", "streikt", ".", "Er", "kann", "und", "mu\u00df", "es", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "$.", "PPER", "VMFIN", "KON", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In den Garagen summen neue Wagen,", "tokens": ["In", "den", "Ga\u00b7ra\u00b7gen", "sum\u00b7men", "neu\u00b7e", "Wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "weil man die besten Dividenden bot", "tokens": ["weil", "man", "die", "bes\u00b7ten", "Di\u00b7vi\u00b7den\u00b7den", "bot"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "ART", "ADJA", "NN", "VVFIN"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.6": {"text": "von deinem Brot.", "tokens": ["von", "dei\u00b7nem", "Brot", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Der B\u00e4cker backt. Die L\u00f6hne steigen munter,", "tokens": ["Der", "B\u00e4\u00b7cker", "backt", ".", "Die", "L\u00f6h\u00b7ne", "stei\u00b7gen", "mun\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "ART", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "doch vom Gewinne geht kein Pfennig runter.", "tokens": ["doch", "vom", "Ge\u00b7win\u00b7ne", "geht", "kein", "Pfen\u00b7nig", "run\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVFIN", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Menschen leben vom Gehalte in den Mund.", "tokens": ["Die", "Men\u00b7schen", "le\u00b7ben", "vom", "Ge\u00b7hal\u00b7te", "in", "den", "Mund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der B\u00e4cker backt. Und macht sich sehr gesund.", "tokens": ["Der", "B\u00e4\u00b7cker", "backt", ".", "Und", "macht", "sich", "sehr", "ge\u00b7sund", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "KON", "VVFIN", "PRF", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Er ist der Preisek\u00f6nig, der Despot \u2013", "tokens": ["Er", "ist", "der", "Prei\u00b7se\u00b7k\u00f6\u00b7nig", ",", "der", "Des\u00b7pot", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "ART", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "von deinem Brot.", "tokens": ["von", "dei\u00b7nem", "Brot", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "So geht der Kreis: kein Landbetrieb geniert sich.", "tokens": ["So", "geht", "der", "Kreis", ":", "kein", "Land\u00b7be\u00b7trieb", "ge\u00b7niert", "sich", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "PIAT", "NN", "VVFIN", "PRF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Industrie hingegen revanchiert sich.", "tokens": ["Die", "In\u00b7dust\u00b7rie", "hin\u00b7ge\u00b7gen", "re\u00b7van\u00b7chiert", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "PRF", "$."], "meter": "--+--+--+--", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Wer hat, der hat. Nun seht ihr andern zu.", "tokens": ["Wer", "hat", ",", "der", "hat", ".", "Nun", "seht", "ihr", "an\u00b7dern", "zu", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "$,", "PRELS", "VAFIN", "$.", "ADV", "VVFIN", "PPER", "PIS", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sie teilen sichs. Wer unten liegt, bist du.", "tokens": ["Sie", "tei\u00b7len", "sichs", ".", "Wer", "un\u00b7ten", "liegt", ",", "bist", "du", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "$.", "PWS", "ADV", "VVFIN", "$,", "VAFIN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Sie klopfen auf die Waren ihres Baus.", "tokens": ["Sie", "klop\u00b7fen", "auf", "die", "Wa\u00b7ren", "ih\u00b7res", "Baus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Das ist noch drin. Und das mu\u00df noch heraus!", "tokens": ["Das", "ist", "noch", "drin", ".", "Und", "das", "mu\u00df", "noch", "he\u00b7raus", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADV", "$.", "KON", "PDS", "VMFIN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Sie wollen alle leben, fett und reich:", "tokens": ["Sie", "wol\u00b7len", "al\u00b7le", "le\u00b7ben", ",", "fett", "und", "reich", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIS", "VVINF", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "in Villen, Autos, teppichwarm und weich . . .", "tokens": ["in", "Vil\u00b7len", ",", "Au\u00b7tos", ",", "tep\u00b7pich\u00b7warm", "und", "weich", ".", ".", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "punct", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,", "ADJD", "KON", "ADJD", "$.", "$.", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Goldtaschen, Zobel und der Frauen Lippenrot \u2013:", "tokens": ["Gold\u00b7ta\u00b7schen", ",", "Zo\u00b7bel", "und", "der", "Frau\u00b7en", "Lip\u00b7pen\u00b7rot", "\u2013", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "$,", "NN", "KON", "ART", "NN", "NE", "$(", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.10": {"text": "Das, Deutscher, ist dein Brot.", "tokens": ["Das", ",", "Deut\u00b7scher", ",", "ist", "dein", "Brot", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "NN", "$,", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "An deinem Brot f\u00fcr f\u00fcnfzehn Mark und achtzig", "tokens": ["An", "dei\u00b7nem", "Brot", "f\u00fcr", "f\u00fcnf\u00b7zehn", "Mark", "und", "acht\u00b7zig"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "CARD", "NN", "KON", "CARD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "h\u00e4ngt, wenn du hinsiehst, allerlei \u2013:", "tokens": ["h\u00e4ngt", ",", "wenn", "du", "hin