{"dta.poem.10353": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "XiV.  \n  Wasserfahrt .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Ich stand gelehnet an den Mast,", "tokens": ["Ich", "stand", "ge\u00b7leh\u00b7net", "an", "den", "Mast", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und z\u00e4hlte jede Welle.", "tokens": ["Und", "z\u00e4hl\u00b7te", "je\u00b7de", "Wel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ade! mein sch\u00f6nes Vaterland", "tokens": ["A\u00b7de", "!", "mein", "sch\u00f6\u00b7nes", "Va\u00b7ter\u00b7land"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$.", "PPOSAT", "ADJA", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Mein Schiff, das segelt schnelle!", "tokens": ["Mein", "Schiff", ",", "das", "se\u00b7gelt", "schnel\u00b7le", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich kam sch\u00f6n Liebchens Haus vorbei,", "tokens": ["Ich", "kam", "sch\u00f6n", "Lieb\u00b7chens", "Haus", "vor\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Fensterscheiben blinken;", "tokens": ["Die", "Fens\u00b7ter\u00b7schei\u00b7ben", "blin\u00b7ken", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich guck' mir fast die Augen aus,", "tokens": ["Ich", "guck'", "mir", "fast", "die", "Au\u00b7gen", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch will mir niemand winken.", "tokens": ["Doch", "will", "mir", "nie\u00b7mand", "win\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PIS", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ihr Thr\u00e4nen, bleibt mir aus dem Aug',", "tokens": ["Ihr", "Thr\u00e4\u00b7nen", ",", "bleibt", "mir", "aus", "dem", "Aug'", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df ich nicht dunkel sehe.", "tokens": ["Da\u00df", "ich", "nicht", "dun\u00b7kel", "se\u00b7he", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mein krankes Herze, brich mir nicht", "tokens": ["Mein", "kran\u00b7kes", "Her\u00b7ze", ",", "brich", "mir", "nicht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "VVFIN", "$,", "VVIMP", "PPER", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vor allzugro\u00dfem Wehe.", "tokens": ["Vor", "all\u00b7zu\u00b7gro\u00b7\u00dfem", "We\u00b7he", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}