{"dta.poem.393": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "An die geschminckte  Bellise.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Bellise schone doch der Unschuld deiner Wangen", "tokens": ["Bel\u00b7li\u00b7se", "scho\u00b7ne", "doch", "der", "Un\u00b7schuld", "dei\u00b7ner", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ADV", "ART", "NN", "PPOSAT", "NN"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Du qu\u00e4hlst und folterst sie gewi\u00dflich ohne Noht;", "tokens": ["Du", "qu\u00e4hlst", "und", "fol\u00b7terst", "sie", "ge\u00b7wi\u00df\u00b7lich", "oh\u00b7ne", "Noht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PPER", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wo blasse Liljen stehn/ da ist kein Rosen-Prangen", "tokens": ["Wo", "blas\u00b7se", "Lil\u00b7jen", "stehn", "/", "da", "ist", "kein", "Ro\u00b7sen\u00b7Pran\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "ADJA", "NN", "VVINF", "$(", "ADV", "VAFIN", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Drum halt bey Zeiten ein/ du wirst sonst allzu roht.", "tokens": ["Drum", "halt", "bey", "Zei\u00b7ten", "ein", "/", "du", "wirst", "sonst", "all\u00b7zu", "roht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "APPR", "NN", "ART", "$(", "PPER", "VAFIN", "ADV", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}