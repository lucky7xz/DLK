{"dta.poem.16266": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn einen Henkel zum Anfassen hat der Krug,", "tokens": ["Wenn", "ei\u00b7nen", "Hen\u00b7kel", "zum", "An\u00b7fas\u00b7sen", "hat", "der", "Krug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPRART", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mag bei dem Henkel ihn anfassen, wer ist klug.", "tokens": ["Mag", "bei", "dem", "Hen\u00b7kel", "ihn", "an\u00b7fas\u00b7sen", ",", "wer", "ist", "klug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "ART", "NN", "PPER", "VVINF", "$,", "PWS", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Doch wenn der Henkel fehlt, so greift, wer es versteht,", "tokens": ["Doch", "wenn", "der", "Hen\u00b7kel", "fehlt", ",", "so", "greift", ",", "wer", "es", "ver\u00b7steht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVFIN", "$,", "ADV", "VVFIN", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Auch ohne Henkel an, und trinkt sogut es geht.", "tokens": ["Auch", "oh\u00b7ne", "Hen\u00b7kel", "an", ",", "und", "trinkt", "so\u00b7gut", "es", "geht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "PTKVZ", "$,", "KON", "VVFIN", "ADJD", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Man mu\u00df Gelegenheit, wo sie sich zeigt, benutzen,", "tokens": ["Man", "mu\u00df", "Ge\u00b7le\u00b7gen\u00b7heit", ",", "wo", "sie", "sich", "zeigt", ",", "be\u00b7nut\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PIS", "VMFIN", "NN", "$,", "PWAV", "PPER", "PRF", "VVFIN", "$,", "VVINF", "$,"], "meter": "++-+-+-+-+-+-", "measure": "unknown.measure.septa"}, "line.2": {"text": "Und vor Verlegenheit, wo sie erscheint, nicht stutzen.", "tokens": ["Und", "vor", "Ver\u00b7le\u00b7gen\u00b7heit", ",", "wo", "sie", "er\u00b7scheint", ",", "nicht", "stut\u00b7zen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "$,", "PWAV", "PPER", "VVFIN", "$,", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}