{"dta.poem.13939": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der neue Rattenf\u00e4nger.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Juchhei\u00dfa! und ich f\u00fchr' den Zug", "tokens": ["Juch\u00b7hei\u00b7\u00dfa", "!", "und", "ich", "f\u00fchr'", "den", "Zug"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$.", "KON", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hopp \u00fcber Feld und Graben.", "tokens": ["Hopp", "\u00fc\u00b7ber", "Feld", "und", "Gra\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Des alten Plunders ist genug,", "tokens": ["Des", "al\u00b7ten", "Plun\u00b7ders", "ist", "ge\u00b7nug", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wir wollen neuen haben.", "tokens": ["Wir", "wol\u00b7len", "neu\u00b7en", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}