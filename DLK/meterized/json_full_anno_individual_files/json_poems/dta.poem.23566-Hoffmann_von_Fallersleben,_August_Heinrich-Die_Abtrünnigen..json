{"dta.poem.23566": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Abtr\u00fcnnigen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1840", "urn": "urn:nbn:de:kobv:b4-200905192626", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das Wasser sprach zum Eise: \u201eKind,", "tokens": ["Das", "Was\u00b7ser", "sprach", "zum", "Ei\u00b7se", ":", "\u201e", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$.", "$(", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So bleib doch nicht so stehn!", "tokens": ["So", "bleib", "doch", "nicht", "so", "stehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PTKNEG", "ADV", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Weg ist weit, die Zeit verrinnt,", "tokens": ["Der", "Weg", "ist", "weit", ",", "die", "Zeit", "ver\u00b7rinnt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wir m\u00fcssen weiter gehn.\u201c", "tokens": ["Wir", "m\u00fcs\u00b7sen", "wei\u00b7ter", "gehn", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VMFIN", "ADV", "VVINF", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "\u201e\u201eLeb wohl! ich kehre nicht zur\u00fcck,", "tokens": ["\u201e", "\u201e", "Leb", "wohl", "!", "ich", "keh\u00b7re", "nicht", "zu\u00b7r\u00fcck", ","], "token_info": ["punct", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "$(", "VVIMP", "ADV", "$.", "PPER", "VVFIN", "PTKNEG", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Leb wohl! ich bleibe hier:", "tokens": ["Leb", "wohl", "!", "ich", "blei\u00b7be", "hier", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$.", "PPER", "VVFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Beschert ward mir ein h\u00f6hres Gl\u00fcck,", "tokens": ["Be\u00b7schert", "ward", "mir", "ein", "h\u00f6h\u00b7res", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Jetzt bin ich mehr als ihr.\u201c\u201c", "tokens": ["Jetzt", "bin", "ich", "mehr", "als", "ihr", ".", "\u201c", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "KOUS", "PPER", "$.", "$(", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "\u201ekomm aus dem Himmel doch geschwind!\u201c", "tokens": ["\u201e", "komm", "aus", "dem", "Him\u00b7mel", "doch", "ge\u00b7schwind", "!", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "APPR", "ART", "NN", "ADV", "ADJD", "$.", "$("], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Sprach's Wasser zu dem Schnee,", "tokens": ["Sprach's", "Was\u00b7ser", "zu", "dem", "Schnee", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u201eder Weg ist weit, die Zeit verrinnt,", "tokens": ["\u201e", "der", "Weg", "ist", "weit", ",", "die", "Zeit", "ver\u00b7rinnt", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VAFIN", "ADJD", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wir m\u00fcssen in die See.\u201c", "tokens": ["Wir", "m\u00fcs\u00b7sen", "in", "die", "See", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VMFIN", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "\u201e\u201eLeb wohl und gr\u00fc\u00df das Vaterland!", "tokens": ["\u201e", "\u201e", "Leb", "wohl", "und", "gr\u00fc\u00df", "das", "Va\u00b7ter\u00b7land", "!"], "token_info": ["punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "$(", "VVIMP", "ADV", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich gehe nicht mit dir:", "tokens": ["Ich", "ge\u00b7he", "nicht", "mit", "dir", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Jetzt hab' ich einen h\u00f6hern Stand,", "tokens": ["Jetzt", "hab'", "ich", "ei\u00b7nen", "h\u00f6\u00b7hern", "Stand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Jetzt bin ich mehr als ihr.\u201c\u201c \u2014", "tokens": ["Jetzt", "bin", "ich", "mehr", "als", "ihr", ".", "\u201c", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "KOUS", "PPER", "$.", "$(", "$(", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "So bliebt ihr Freund' uns auch zur\u00fcck,", "tokens": ["So", "bliebt", "ihr", "Freund'", "uns", "auch", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Weil Stillstand euch gefiel;", "tokens": ["Weil", "Still\u00b7stand", "euch", "ge\u00b7fiel", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihr suchtet nur ein andres Gl\u00fcck,", "tokens": ["Ihr", "such\u00b7tet", "nur", "ein", "and\u00b7res", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein andres Lebensziel.", "tokens": ["Ein", "and\u00b7res", "Le\u00b7bens\u00b7ziel", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Einst gingen wir wohl Hand in Hand,", "tokens": ["Einst", "gin\u00b7gen", "wir", "wohl", "Hand", "in", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Mutter rief so laut \u2014", "tokens": ["Die", "Mut\u00b7ter", "rief", "so", "laut"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADJD", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Mutter war das Vaterland,", "tokens": ["Die", "Mut\u00b7ter", "war", "das", "Va\u00b7ter\u00b7land", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Freiheit unsre Braut.", "tokens": ["Die", "Frei\u00b7heit", "uns\u00b7re", "Braut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Ihr die ihr Eis und Schnee jetzt seid", "tokens": ["Ihr", "die", "ihr", "Eis", "und", "Schnee", "jetzt", "seid"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ART", "PPOSAT", "NN", "KON", "NN", "ADV", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und d\u00fcnkt euch mehr als wir,", "tokens": ["Und", "d\u00fcnkt", "euch", "mehr", "als", "wir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "KOUS", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "O wartet nur, es kommt die Zeit", "tokens": ["O", "war\u00b7tet", "nur", ",", "es", "kommt", "die", "Zeit"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ADV", "$,", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und \u2014 Wasser seid auch ihr.", "tokens": ["Und", "Was\u00b7ser", "seid", "auch", "ihr", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$(", "PWS", "VAFIN", "ADV", "PPER", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}