{"dta.poem.2848": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "49.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Den Kr\u00fcppel schilt man nicht, da\u00df er nicht wandeln kann;", "tokens": ["Den", "Kr\u00fcp\u00b7pel", "schilt", "man", "nicht", ",", "da\u00df", "er", "nicht", "wan\u00b7deln", "kann", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIS", "PTKNEG", "$,", "KOUS", "PPER", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und auch ein Kr\u00fcppel ist der haltungslose Mann.", "tokens": ["Und", "auch", "ein", "Kr\u00fcp\u00b7pel", "ist", "der", "hal\u00b7tungs\u00b7lo\u00b7se", "Mann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wer nun kann hei\u00dfen gehn den Kr\u00fcppel und den Lahmen,", "tokens": ["Wer", "nun", "kann", "hei\u00b7\u00dfen", "gehn", "den", "Kr\u00fcp\u00b7pel", "und", "den", "Lah\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VMFIN", "VVINF", "VVFIN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der fordre Haltung auch von dem in Gottes Namen.", "tokens": ["Der", "ford\u00b7re", "Hal\u00b7tung", "auch", "von", "dem", "in", "Got\u00b7tes", "Na\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "APPR", "ART", "APPR", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wer aber das nicht kann, der m\u00f6ge sich bedenken,", "tokens": ["Wer", "a\u00b7ber", "das", "nicht", "kann", ",", "der", "m\u00f6\u00b7ge", "sich", "be\u00b7den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PDS", "PTKNEG", "VMFIN", "$,", "PRELS", "VMFIN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ob er dem armen Mann nicht mu\u00dfe Nachsicht schenken.", "tokens": ["Ob", "er", "dem", "ar\u00b7men", "Mann", "nicht", "mu\u00b7\u00dfe", "Nach\u00b7sicht", "schen\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "PTKNEG", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}