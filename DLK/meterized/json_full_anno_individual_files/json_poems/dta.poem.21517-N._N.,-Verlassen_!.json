{"dta.poem.21517": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Verlassen !", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.85", "no:0.14"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Im Morgengrauen ging ich fort \u2014", "tokens": ["Im", "Mor\u00b7gen\u00b7grau\u00b7en", "ging", "ich", "fort"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nebel lag in den Gassen \u2014", "tokens": ["Ne\u00b7bel", "lag", "in", "den", "Gas\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "In Qualen war mir das Herz verdorrt,", "tokens": ["In", "Qua\u00b7len", "war", "mir", "das", "Herz", "ver\u00b7dorrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die Lippe sprach kein Abschiedswort \u2014", "tokens": ["Die", "Lip\u00b7pe", "sprach", "kein", "Ab\u00b7schieds\u00b7wort"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sie st\u00f6hnte nur leise: Verlassen!", "tokens": ["Sie", "st\u00f6hn\u00b7te", "nur", "lei\u00b7se", ":", "Ver\u00b7las\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "$.", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "Verlassen! Kennt ihr das Marterwort? \u2014", "tokens": ["Ver\u00b7las\u00b7sen", "!", "Kennt", "ihr", "das", "Mar\u00b7ter\u00b7wort", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVPP", "$.", "VVFIN", "PPER", "ART", "NN", "$.", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Das fri\u00dft wie verruchte Schande! . .", "tokens": ["Das", "fri\u00dft", "wie", "ver\u00b7ruch\u00b7te", "Schan\u00b7de", "!", ".", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PDS", "VVFIN", "KOKOM", "ADJA", "NN", "$.", "$.", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "In Qualen war mir das Herz verdorrt \u2014", "tokens": ["In", "Qua\u00b7len", "war", "mir", "das", "Herz", "ver\u00b7dorrt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "ART", "NN", "VVPP", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Im Morgengrauen ging ich fort,", "tokens": ["Im", "Mor\u00b7gen\u00b7grau\u00b7en", "ging", "ich", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Hinaus in die d\u00e4mmernden Lande \u2026", "tokens": ["Hin\u00b7aus", "in", "die", "d\u00e4m\u00b7mern\u00b7den", "Lan\u00b7de", "\u2026"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.3": {"line.1": {"text": "Entgegen dem jungen Maientag \u2014", "tokens": ["Ent\u00b7ge\u00b7gen", "dem", "jun\u00b7gen", "Mai\u00b7en\u00b7tag"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Das war ein seltsam Passen!", "tokens": ["Das", "war", "ein", "selt\u00b7sam", "Pas\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "M\u00e4hlich wurde die Welt nun wach \u2014 \u2014", "tokens": ["M\u00e4h\u00b7lich", "wur\u00b7de", "die", "Welt", "nun", "wach"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "ADV", "ADJD", "$(", "$("], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Was sollt\u2019 mir der junge Fr\u00fchlingstag? \u2014", "tokens": ["Was", "sollt'", "mir", "der", "jun\u00b7ge", "Fr\u00fch\u00b7lings\u00b7tag", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Ich st\u00f6hnte nur leise: Verlassen!", "tokens": ["Ich", "st\u00f6hn\u00b7te", "nur", "lei\u00b7se", ":", "Ver\u00b7las\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "$.", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}}}}