{"dta.poem.19979": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Ueber das in Metall gepr\u00e4gte Bild  \n Katharinas .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905193111", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "H\u00e4ngt als s\u00fc\u00dfes Angebinde,", "tokens": ["H\u00e4ngt", "als", "s\u00fc\u00b7\u00dfes", "An\u00b7ge\u00b7bin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "H\u00e4nget als der Tugend Schild,", "tokens": ["H\u00e4n\u00b7get", "als", "der", "Tu\u00b7gend", "Schild", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schwabens Frauen! eurem Kinde", "tokens": ["Schwa\u00b7bens", "Frau\u00b7en", "!", "eu\u00b7rem", "Kin\u00b7de"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "NN", "$.", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "An das Herz die\u00df edle Bild.", "tokens": ["An", "das", "Herz", "die\u00df", "ed\u00b7le", "Bild", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PDS", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Sagt ihm, wer Sie ist gewesen,", "tokens": ["Sagt", "ihm", ",", "wer", "Sie", "ist", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWS", "PPER", "VAFIN", "VAPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie gesegnet Sie das Land,", "tokens": ["Wie", "ge\u00b7seg\u00b7net", "Sie", "das", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bis Sie schnell von Gott erlesen,", "tokens": ["Bis", "Sie", "schnell", "von", "Gott", "er\u00b7le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Eine Heil'ge uns verschwand.", "tokens": ["Ei\u00b7ne", "Heil'\u00b7ge", "uns", "ver\u00b7schwand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Baut ihr f\u00fcr die Armuth milde,", "tokens": ["Baut", "ihr", "f\u00fcr", "die", "Ar\u00b7muth", "mil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo ein Haus, wird es gedeih'n,", "tokens": ["Wo", "ein", "Haus", ",", "wird", "es", "ge\u00b7deih'n", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Legt das Erz mit Ihrem Bilde", "tokens": ["Legt", "das", "Erz", "mit", "Ih\u00b7rem", "Bil\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihr in seines Grundes Stein.", "tokens": ["Ihr", "in", "sei\u00b7nes", "Grun\u00b7des", "Stein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Wird, wo f\u00fcr des Feldes Fr\u00fcchte", "tokens": ["Wird", ",", "wo", "f\u00fcr", "des", "Fel\u00b7des", "Fr\u00fcch\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "$,", "PWAV", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Eine Scheuer neu erbaut,", "tokens": ["Ei\u00b7ne", "Scheu\u00b7er", "neu", "er\u00b7baut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df kein Donner sie zernichte,", "tokens": ["Da\u00df", "kein", "Don\u00b7ner", "sie", "zer\u00b7nich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Werd' Ihr Bild dem Grund vertraut.", "tokens": ["Werd'", "Ihr", "Bild", "dem", "Grund", "ver\u00b7traut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "R\u00f6m'scher Herrscherinnen Bilder,", "tokens": ["R\u00f6m'\u00b7scher", "Herr\u00b7sche\u00b7rin\u00b7nen", "Bil\u00b7der", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wahrt die schw\u00e4b'sche Erde noch,", "tokens": ["Wahrt", "die", "schw\u00e4b'\u00b7sche", "Er\u00b7de", "noch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wahret r\u00f6m'sche Schwerter, Schilder,", "tokens": ["Wah\u00b7ret", "r\u00f6m'\u00b7sche", "Schwer\u00b7ter", ",", "Schil\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mahnend nur an's r\u00f6m'sche Joch.", "tokens": ["Mah\u00b7nend", "nur", "an's", "r\u00f6m'\u00b7sche", "Joch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "O wie treu wird sie bewahren,", "tokens": ["O", "wie", "treu", "wird", "sie", "be\u00b7wah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "ADJD", "VAFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Heilige! Dein Bild im Schoo\u00df!", "tokens": ["Hei\u00b7li\u00b7ge", "!", "Dein", "Bild", "im", "Schoo\u00df", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "$.", "PPOSAT", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dich, die einst in Hungerjahren", "tokens": ["Dich", ",", "die", "einst", "in", "Hun\u00b7ger\u00b7jah\u00b7ren"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PRELS", "ADV", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ueber sie Ihr F\u00fcllhorn go\u00df!", "tokens": ["Ue\u00b7ber", "sie", "Ihr", "F\u00fcll\u00b7horn", "go\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Nach Jahrhunderten noch pfl\u00fcget", "tokens": ["Nach", "Jahr\u00b7hun\u00b7der\u00b7ten", "noch", "pfl\u00fc\u00b7get"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "ADV", "VVFIN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Es der Landmann aus dem Grund,", "tokens": ["Es", "der", "Land\u00b7mann", "aus", "dem", "Grund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Rufet Weib und Kind vergn\u00fcget,", "tokens": ["Ru\u00b7fet", "Weib", "und", "Kind", "ver\u00b7gn\u00fc\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Anzuschau'n den theuren Fund.", "tokens": ["An\u00b7zu\u00b7schau'n", "den", "theu\u00b7ren", "Fund", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Spricht: o! la\u00dft uns treu bewahren", "tokens": ["Spricht", ":", "o", "!", "la\u00dft", "uns", "treu", "be\u00b7wah\u00b7ren"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$.", "FM", "$.", "VVIMP", "PPER", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sie, von der die Sage geht:", "tokens": ["Sie", ",", "von", "der", "die", "Sa\u00b7ge", "geht", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "APPR", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Da\u00df Sie hab' in Hungerjahren", "tokens": ["Da\u00df", "Sie", "hab'", "in", "Hun\u00b7ger\u00b7jah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VAFIN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Unsrem Ahn das Feld bes\u00e4t.", "tokens": ["Uns\u00b7rem", "Ahn", "das", "Feld", "be\u00b7s\u00e4t", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}