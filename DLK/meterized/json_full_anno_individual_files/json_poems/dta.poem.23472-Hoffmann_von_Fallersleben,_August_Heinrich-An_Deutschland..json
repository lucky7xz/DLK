{"dta.poem.23472": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "An Deutschland.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Auf, auf, wer deutsche Freiheit liebet,", "tokens": ["Auf", ",", "auf", ",", "wer", "deut\u00b7sche", "Frei\u00b7heit", "lie\u00b7bet", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "PTKVZ", "$,", "PWS", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wer Lust f\u00fcr Gott zu fechten hat!", "tokens": ["Wer", "Lust", "f\u00fcr", "Gott", "zu", "fech\u00b7ten", "hat", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "APPR", "NN", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Schein, den Mancher von sich giebet,", "tokens": ["Der", "Schein", ",", "den", "Man\u00b7cher", "von", "sich", "gie\u00b7bet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "APPR", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Verbringet keine Ritterthat.", "tokens": ["Ver\u00b7brin\u00b7get", "kei\u00b7ne", "Rit\u00b7ter\u00b7that", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wann Fug und Ursach ist, zu brechen,", "tokens": ["Wann", "Fug", "und", "Ur\u00b7sach", "ist", ",", "zu", "bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "VAFIN", "$,", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wann Feind nicht Freund mehr bleiben kann,", "tokens": ["Wann", "Feind", "nicht", "Freund", "mehr", "blei\u00b7ben", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "PTKNEG", "NN", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da mu\u00df man nur vom Sehen sprechen,", "tokens": ["Da", "mu\u00df", "man", "nur", "vom", "Se\u00b7hen", "spre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "ADV", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Da zeigt das Herze seinen Mann.", "tokens": ["Da", "zeigt", "das", "Her\u00b7ze", "sei\u00b7nen", "Mann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PDS", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "La\u00df die von ihren Kr\u00e4ften sagen,", "tokens": ["La\u00df", "die", "von", "ih\u00b7ren", "Kr\u00e4f\u00b7ten", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die schwach und blo\u00df von Tugend sind:", "tokens": ["Die", "schwach", "und", "blo\u00df", "von", "Tu\u00b7gend", "sind", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "KON", "ADV", "APPR", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit Trotzen wird man Bienen jagen,", "tokens": ["Mit", "Trot\u00b7zen", "wird", "man", "Bie\u00b7nen", "ja\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PIS", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Sinn von Ehren der gewinnt.", "tokens": ["Ein", "Sinn", "von", "Eh\u00b7ren", "der", "ge\u00b7winnt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "ART", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie gro\u00df und stark der Feind sich mache,", "tokens": ["Wie", "gro\u00df", "und", "stark", "der", "Feind", "sich", "ma\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "KON", "ADJD", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wie hoch er schwinge Muth und Schwert,", "tokens": ["Wie", "hoch", "er", "schwin\u00b7ge", "Muth", "und", "Schwert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So glaube doch, die gute Sache", "tokens": ["So", "glau\u00b7be", "doch", ",", "die", "gu\u00b7te", "Sa\u00b7che"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ist hundert tausend K\u00f6pfe werth.", "tokens": ["Ist", "hun\u00b7dert", "tau\u00b7send", "K\u00f6p\u00b7fe", "werth", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "CARD", "CARD", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Der mu\u00df nicht eben allzeit siegen,", "tokens": ["Der", "mu\u00df", "nicht", "e\u00b7ben", "all\u00b7zeit", "sie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PTKNEG", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Bei dem der K\u00f6pfe Menge steht;", "tokens": ["Bei", "dem", "der", "K\u00f6p\u00b7fe", "Men\u00b7ge", "steht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der pfleget mehr den Preis zu kriegen,", "tokens": ["Der", "pfle\u00b7get", "mehr", "den", "Preis", "zu", "krie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dem Billigkeit zu Herzen geht,", "tokens": ["Dem", "Bil\u00b7lig\u00b7keit", "zu", "Her\u00b7zen", "geht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und der mit redlichem Gewissen", "tokens": ["Und", "der", "mit", "red\u00b7li\u00b7chem", "Ge\u00b7wis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "F\u00fcr Gott und f\u00fcr das Vaterland,", "tokens": ["F\u00fcr", "Gott", "und", "f\u00fcr", "das", "Va\u00b7ter\u00b7land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "F\u00fcr Gott, der ihn es l\u00e4\u00dft genie\u00dfen,", "tokens": ["F\u00fcr", "Gott", ",", "der", "ihn", "es", "l\u00e4\u00dft", "ge\u00b7nie\u00b7\u00dfen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "PPER", "PPER", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Zu fechten geht mit strenger Hand.", "tokens": ["Zu", "fech\u00b7ten", "geht", "mit", "stren\u00b7ger", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "So vieler St\u00e4dte schwache Sinnen,", "tokens": ["So", "vie\u00b7ler", "St\u00e4d\u00b7te", "schwa\u00b7che", "Sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So vieler Herzen Wankelmuth", "tokens": ["So", "vie\u00b7ler", "Her\u00b7zen", "Wan\u00b7kel\u00b7muth"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "PIAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die List, der Abfall, das Beginnen", "tokens": ["Die", "List", ",", "der", "Ab\u00b7fall", ",", "das", "Be\u00b7gin\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sind freilich wohl nicht allzugut.", "tokens": ["Sind", "frei\u00b7lich", "wohl", "nicht", "all\u00b7zu\u00b7gut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch Obst, so bald von B\u00e4umen gehet,", "tokens": ["Doch", "Obst", ",", "so", "bald", "von", "B\u00e4u\u00b7men", "ge\u00b7het", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "ADV", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Das taug gemeiniglich nicht viel;", "tokens": ["Das", "taug", "ge\u00b7mei\u00b7nig\u00b7lich", "nicht", "viel", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ich denke was im Liede stehet,", "tokens": ["Ich", "den\u00b7ke", "was", "im", "Lie\u00b7de", "ste\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "La\u00df fahren was nicht bleiben will!", "tokens": ["La\u00df", "fah\u00b7ren", "was", "nicht", "blei\u00b7ben", "will", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "VVINF", "PWS", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Was kann der stolze Feind dir rauben?", "tokens": ["Was", "kann", "der", "stol\u00b7ze", "Feind", "dir", "rau\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "ADJA", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dein Hab' und Gut bleibt doch allhier;", "tokens": ["Dein", "Hab'", "und", "Gut", "bleibt", "doch", "all\u00b7hier", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "ADJD", "VVFIN", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Geh aber du ihm auf die Hauben", "tokens": ["Geh", "a\u00b7ber", "du", "ihm", "auf", "die", "Hau\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "KON", "PPER", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und brich ihm seinen Hals darf\u00fcr!", "tokens": ["Und", "brich", "ihm", "sei\u00b7nen", "Hals", "dar\u00b7f\u00fcr", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "PPOSAT", "NN", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Auf, auf, ihr Br\u00fcder! in Quartieren", "tokens": ["Auf", ",", "auf", ",", "ihr", "Br\u00fc\u00b7der", "!", "in", "Quar\u00b7tie\u00b7ren"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["APPR", "$,", "PTKVZ", "$,", "PPOSAT", "NN", "$.", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Bekriegt man mehrmals nur den Wein:", "tokens": ["Be\u00b7kriegt", "man", "mehr\u00b7mals", "nur", "den", "Wein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Des Feindes Blut im Siege f\u00fchren,", "tokens": ["Des", "Fein\u00b7des", "Blut", "im", "Sie\u00b7ge", "f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Dies wird die beste Beute sein.", "tokens": ["Dies", "wird", "die", "bes\u00b7te", "Beu\u00b7te", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}