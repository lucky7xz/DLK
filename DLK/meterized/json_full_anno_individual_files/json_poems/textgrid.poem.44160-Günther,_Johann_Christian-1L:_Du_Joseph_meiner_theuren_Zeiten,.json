{"textgrid.poem.44160": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du Joseph meiner theuren Zeiten,", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du Joseph meiner theuren Zeiten,", "tokens": ["Du", "Jo\u00b7se\u00b7ph", "mei\u00b7ner", "theu\u00b7ren", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NE", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Du Vater meiner Poesie,", "tokens": ["Du", "Va\u00b7ter", "mei\u00b7ner", "Poe\u00b7sie", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Freund, de\u00dfen Ruhm ich mit den Saythen", "tokens": ["Freund", ",", "de\u00b7\u00dfen", "Ruhm", "ich", "mit", "den", "Say\u00b7then"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PRELAT", "NN", "PPER", "APPR", "ART", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "In Abgrund sp\u00e4ter Jahre zieh,", "tokens": ["In", "Ab\u00b7grund", "sp\u00e4\u00b7ter", "Jah\u00b7re", "zieh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Bisher kont ich noch nicht erme\u00dfen,", "tokens": ["Bis\u00b7her", "kont", "ich", "noch", "nicht", "er\u00b7me\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Was ich vor ein Gesch\u00f6pfe sey,", "tokens": ["Was", "ich", "vor", "ein", "Ge\u00b7sch\u00f6p\u00b7fe", "sey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Jezt, da ich so dein Herz bese\u00dfen,", "tokens": ["Jezt", ",", "da", "ich", "so", "dein", "Herz", "be\u00b7se\u00b7\u00dfen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "ADV", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "So zehl ich mich den Geistern bey.", "tokens": ["So", "zehl", "ich", "mich", "den", "Geis\u00b7tern", "bey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Auch niemand wird hier anders dencken,", "tokens": ["Auch", "nie\u00b7mand", "wird", "hier", "an\u00b7ders", "den\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VAFIN", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wer nur ein wenig Achtung giebt,", "tokens": ["Wer", "nur", "ein", "we\u00b7nig", "Ach\u00b7tung", "giebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie dein so reich- als \u00f6fters Schencken", "tokens": ["Wie", "dein", "so", "reich", "als", "\u00f6f\u00b7ters", "Schen\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPOSAT", "ADV", "TRUNC", "KOKOM", "ADV", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Danckbarkeit der Musen \u00fcbt.", "tokens": ["Die", "Dan\u00b7ck\u00b7bar\u00b7keit", "der", "Mu\u00b7sen", "\u00fcbt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Die Gro\u00dfmuth giebt sich niemahls m\u00fcde", "tokens": ["Die", "Gro\u00df\u00b7muth", "giebt", "sich", "nie\u00b7mahls", "m\u00fc\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PRF", "ADV", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und kommt der Bitte selbst zuvor,", "tokens": ["Und", "kommt", "der", "Bit\u00b7te", "selbst", "zu\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wenn ich zwey lahme Verse schmiede,", "tokens": ["Wenn", "ich", "zwey", "lah\u00b7me", "Ver\u00b7se", "schmie\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "CARD", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Dergleichen Bavius verlor.", "tokens": ["Derg\u00b7lei\u00b7chen", "Ba\u00b7vius", "ver\u00b7lor", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "NE", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Du must von k\u00f6niglichem Blute", "tokens": ["Du", "must", "von", "k\u00f6\u00b7nig\u00b7li\u00b7chem", "Blu\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und edler als M\u00e4cenas seyn.", "tokens": ["Und", "ed\u00b7ler", "als", "M\u00e4\u00b7ce\u00b7nas", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "KOUS", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er that dem Flaccus viel zu Gute,", "tokens": ["Er", "that", "dem", "Flac\u00b7cus", "viel", "zu", "Gu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Du nimmst mich mit der Wohlthat ein.", "tokens": ["Du", "nimmst", "mich", "mit", "der", "Wohlt\u00b7hat", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Horaz verdiente noch die Gnade,", "tokens": ["Ho\u00b7raz", "ver\u00b7dien\u00b7te", "noch", "die", "Gna\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Weil sie des Dichtens Preis erh\u00e4lt,", "tokens": ["Weil", "sie", "des", "Dich\u00b7tens", "Preis", "er\u00b7h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Allein um deine Huld ist's schade,", "tokens": ["Al\u00b7lein", "um", "dei\u00b7ne", "Huld", "ist's", "scha\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Weil sie auf keinen W\u00fcrdgern f\u00e4llt.", "tokens": ["Weil", "sie", "auf", "kei\u00b7nen", "W\u00fcrd\u00b7gern", "f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Was ich nicht bin, das will ich werden,", "tokens": ["Was", "ich", "nicht", "bin", ",", "das", "will", "ich", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PTKNEG", "VAFIN", "$,", "PDS", "VMFIN", "PPER", "VAINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die F\u00e4higkeit nehm ich von dir.", "tokens": ["Die", "F\u00e4\u00b7hig\u00b7keit", "nehm", "ich", "von", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dein Mund soll mir ein Phoebus werden,", "tokens": ["Dein", "Mund", "soll", "mir", "ein", "Phoe\u00b7bus", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "ART", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sein Lob erhizt die Ruhmbegier.", "tokens": ["Sein", "Lob", "er\u00b7hizt", "die", "Ruhm\u00b7be\u00b7gier", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dein Umgang fl\u00fcgelt Flei\u00df und Sinnen", "tokens": ["Dein", "Um\u00b7gang", "fl\u00fc\u00b7gelt", "Flei\u00df", "und", "Sin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und greift mich mit Versuchung an,", "tokens": ["Und", "greift", "mich", "mit", "Ver\u00b7su\u00b7chung", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Bis ich dein Bildn\u00fc\u00df auf die Zinnen", "tokens": ["Bis", "ich", "dein", "Bild\u00b7n\u00fc\u00df", "auf", "die", "Zin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Des Ehrentempels sezen kan.", "tokens": ["Des", "Eh\u00b7ren\u00b7tem\u00b7pels", "se\u00b7zen", "kan", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.9": {"text": "Hab ich nicht f\u00fcrstliche Patronen,", "tokens": ["Hab", "ich", "nicht", "f\u00fcrst\u00b7li\u00b7che", "Pat\u00b7ro\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PTKNEG", "ADJA", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.10": {"text": "So ehr ich dich als meinen F\u00fcrst.", "tokens": ["So", "ehr", "ich", "dich", "als", "mei\u00b7nen", "F\u00fcrst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "PPER", "PRF", "KOUS", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Darf ich in keinem Louvre wohnen,", "tokens": ["Darf", "ich", "in", "kei\u00b7nem", "Louv\u00b7re", "woh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Gnung, da\u00df du mich beschatten wirst.", "tokens": ["Gnung", ",", "da\u00df", "du", "mich", "be\u00b7schat\u00b7ten", "wirst", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "PRF", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Die Hoheit liegt nicht an dem Stande,", "tokens": ["Die", "Ho\u00b7heit", "liegt", "nicht", "an", "dem", "Stan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Dein gro\u00df- und heldenm\u00fcthger Geist", "tokens": ["Dein", "gro\u00df", "und", "hel\u00b7den\u00b7m\u00fcth\u00b7ger", "Geist"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "TRUNC", "KON", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Zeigt manchen Seelen ihre Schande,", "tokens": ["Zeigt", "man\u00b7chen", "See\u00b7len", "ih\u00b7re", "Schan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Die unsre Welt durchlauchtig heist.", "tokens": ["Die", "uns\u00b7re", "Welt", "durch\u00b7lauch\u00b7tig", "heist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Auf, Fama, schm\u00fccke deinen Wagen,", "tokens": ["Auf", ",", "Fa\u00b7ma", ",", "schm\u00fc\u00b7cke", "dei\u00b7nen", "Wa\u00b7gen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "NN", "$,", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Spann ihn an meinen Pegasus,", "tokens": ["Spann", "ihn", "an", "mei\u00b7nen", "Pe\u00b7ga\u00b7sus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den edlen Freund dahin zu tragen,", "tokens": ["Den", "ed\u00b7len", "Freund", "da\u00b7hin", "zu", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PAV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wo ihn die Sonne neiden mu\u00df!", "tokens": ["Wo", "ihn", "die", "Son\u00b7ne", "nei\u00b7den", "mu\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Solang ein Blat von meinen B\u00fcchern", "tokens": ["So\u00b7lang", "ein", "Blat", "von", "mei\u00b7nen", "B\u00fc\u00b7chern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Das Lob der k\u00fcnftgen Zeit erlebt,", "tokens": ["Das", "Lob", "der", "k\u00fcnft\u00b7gen", "Zeit", "er\u00b7lebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So [lange] will ich ihn versichern,", "tokens": ["So", "lan\u00b7ge", "will", "ich", "ihn", "ver\u00b7si\u00b7chern", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "ADV", "$(", "VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Da\u00df er im achten Krei\u00dfe schwebt.", "tokens": ["Da\u00df", "er", "im", "ach\u00b7ten", "Krei\u00b7\u00dfe", "schwebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Du Joseph meiner theuren Zeiten,", "tokens": ["Du", "Jo\u00b7se\u00b7ph", "mei\u00b7ner", "theu\u00b7ren", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NE", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Du Vater meiner Poesie,", "tokens": ["Du", "Va\u00b7ter", "mei\u00b7ner", "Poe\u00b7sie", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Freund, de\u00dfen Ruhm ich mit den Saythen", "tokens": ["Freund", ",", "de\u00b7\u00dfen", "Ruhm", "ich", "mit", "den", "Say\u00b7then"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PRELAT", "NN", "PPER", "APPR", "ART", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "In Abgrund sp\u00e4ter Jahre zieh,", "tokens": ["In", "Ab\u00b7grund", "sp\u00e4\u00b7ter", "Jah\u00b7re", "zieh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Bisher kont ich noch nicht erme\u00dfen,", "tokens": ["Bis\u00b7her", "kont", "ich", "noch", "nicht", "er\u00b7me\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Was ich vor ein Gesch\u00f6pfe sey,", "tokens": ["Was", "ich", "vor", "ein", "Ge\u00b7sch\u00f6p\u00b7fe", "sey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Jezt, da ich so dein Herz bese\u00dfen,", "tokens": ["Jezt", ",", "da", "ich", "so", "dein", "Herz", "be\u00b7se\u00b7\u00dfen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "ADV", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "So zehl ich mich den Geistern bey.", "tokens": ["So", "zehl", "ich", "mich", "den", "Geis\u00b7tern", "bey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Auch niemand wird hier anders dencken,", "tokens": ["Auch", "nie\u00b7mand", "wird", "hier", "an\u00b7ders", "den\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VAFIN", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wer nur ein wenig Achtung giebt,", "tokens": ["Wer", "nur", "ein", "we\u00b7nig", "Ach\u00b7tung", "giebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie dein so reich- als \u00f6fters Schencken", "tokens": ["Wie", "dein", "so", "reich", "als", "\u00f6f\u00b7ters", "Schen\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPOSAT", "ADV", "TRUNC", "KOKOM", "ADV", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Danckbarkeit der Musen \u00fcbt.", "tokens": ["Die", "Dan\u00b7ck\u00b7bar\u00b7keit", "der", "Mu\u00b7sen", "\u00fcbt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART