{"dta.poem.3457": {"metadata": {"author": {"name": "N.A.", "birth": "N.A.", "death": "N.A."}, "title": "Das .xxviij.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1497", "urn": "urn:nbn:de:kobv:b4-30121-8", "language": ["de:0.99"], "booktitle": "Des Edlen Ritter Morgeners Walfart. Erfurt, 1497."}, "poem": {"stanza.1": {"line.1": {"text": "Da es gieng zu der abent stund", "tokens": ["Da", "es", "gieng", "zu", "der", "a\u00b7bent", "stund"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "ART", "NN", "VVFIN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "das die praut solt zu petthe gan", "tokens": ["das", "die", "praut", "solt", "zu", "pet\u00b7the", "gan"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "sy waren all fr\u00f6lich gesund", "tokens": ["sy", "wa\u00b7ren", "all", "fr\u00f6\u00b7lich", "ge\u00b7sund"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "PIAT", "ADJD", "VVPP"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "da sprach der peste dienst man", "tokens": ["da", "sprach", "der", "pes\u00b7te", "dienst", "man"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "PIS"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "herr morgner het einen syt", "tokens": ["herr", "morg\u00b7ner", "het", "ei\u00b7nen", "syt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VAFIN", "ART", "NE"], "meter": "+---+-+", "measure": "dactylic.init"}, "line.6": {"text": "das kein gast in der burg nicht bleib", "tokens": ["das", "kein", "gast", "in", "der", "burg", "nicht", "bleib"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PIAT", "VVFIN", "APPR", "ART", "NN", "PTKNEG", "VVFIN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.7": {"text": "er s\u00fcng im dan ein hoffe lyt", "tokens": ["er", "s\u00fcng", "im", "dan", "ein", "hof\u00b7fe", "lyt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "ADV", "ART", "VVFIN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}