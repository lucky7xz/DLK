{"dta.poem.5637": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Vom Geist und C\u00f6rper", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Allein,", "tokens": ["Al\u00b7lein", ","], "token_info": ["word", "punct"], "pos": ["ADV", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "Man halte sich nicht auf nur bey dem blossen ", "tokens": ["Man", "hal\u00b7te", "sich", "nicht", "auf", "nur", "bey", "dem", "blos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PRF", "PTKNEG", "APPR", "ADV", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Weil unsre Seele wir- wir die seyn, so gedencken;", "tokens": ["Weil", "uns\u00b7re", "See\u00b7le", "wir", "wir", "die", "seyn", ",", "so", "ge\u00b7den\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "TRUNC", "PPER", "ART", "VAINF", "$,", "ADV", "NN", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "So l\u00e4sst uns dieses ja ganz \u00fcberzeuglich lesen:", "tokens": ["So", "l\u00e4sst", "uns", "die\u00b7ses", "ja", "ganz", "\u00fc\u00b7berz\u00b7eug\u00b7lich", "le\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PDS", "ADV", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So Seel\u2019 als Dencken sey auch unser wahres Wesen.", "tokens": ["So", "Seel'", "als", "Den\u00b7cken", "sey", "auch", "un\u00b7ser", "wah\u00b7res", "We\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KOKOM", "PDS", "VAFIN", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}