{"textgrid.poem.44398": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[kaiphas, Kaiphas, Sanchedrin!]", "genre": "verse", "period": "N.A.", "pub_year": 1823, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kaiphas, Kaiphas, Sanchedrin!", "tokens": ["Kai\u00b7phas", ",", "Kai\u00b7phas", ",", "San\u00b7che\u00b7drin", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "NE", "$,", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Habt ihn vor Gericht gezogen,", "tokens": ["Habt", "ihn", "vor", "Ge\u00b7richt", "ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Weil, statt in die Synagogen,", "tokens": ["Weil", ",", "statt", "in", "die", "Syn\u00b7a\u00b7go\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "KOUI", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Er zum Weltgeist strebte hin?", "tokens": ["Er", "zum", "Welt\u00b7geist", "streb\u00b7te", "hin", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "M\u00f6chtet gar ans Kreuz ihn schlagen,", "tokens": ["M\u00f6ch\u00b7tet", "gar", "ans", "Kreuz", "ihn", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "APPRART", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Das er zehen Jahr getragen?", "tokens": ["Das", "er", "ze\u00b7hen", "Jahr", "ge\u00b7tra\u00b7