{"textgrid.poem.36900": {"metadata": {"author": {"name": "Gellert, Christian F\u00fcrchtegott", "birth": "N.A.", "death": "N.A."}, "title": "Wider den Geiz", "genre": "verse", "period": "N.A.", "pub_year": 1742, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wohl dem, der be\u00dfre Sch\u00e4tze liebt,", "tokens": ["Wohl", "dem", ",", "der", "be\u00df\u00b7re", "Sch\u00e4t\u00b7ze", "liebt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "$,", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als Sch\u00e4tze dieser Erden!", "tokens": ["Als", "Sch\u00e4t\u00b7ze", "die\u00b7ser", "Er\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PDAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wohl dem, der sich mit Eifer \u00fcbt,", "tokens": ["Wohl", "dem", ",", "der", "sich", "mit", "Ei\u00b7fer", "\u00fcbt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "$,", "PRELS", "PRF", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "An Tugend reich zu werden;", "tokens": ["An", "Tu\u00b7gend", "reich", "zu", "wer\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und in dem Glauben, des er lebt,", "tokens": ["Und", "in", "dem", "Glau\u00b7ben", ",", "des", "er", "lebt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sich \u00fcber diese Welt erhebt!", "tokens": ["Sich", "\u00fc\u00b7ber", "die\u00b7se", "Welt", "er\u00b7hebt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wahr ist es, Gott verwehrt uns nicht,", "tokens": ["Wahr", "ist", "es", ",", "Gott", "ver\u00b7wehrt", "uns", "nicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "$,", "NN", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hier G\u00fcter zu besitzen.", "tokens": ["Hier", "G\u00fc\u00b7ter", "zu", "be\u00b7sit\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er gab sie uns, und auch die Pflicht,", "tokens": ["Er", "gab", "sie", "uns", ",", "und", "auch", "die", "Pflicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPER", "$,", "KON", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Weisheit sie zu n\u00fctzen.", "tokens": ["Mit", "Weis\u00b7heit", "sie", "zu", "n\u00fct\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sie d\u00fcrfen unser Herz erfreun,", "tokens": ["Sie", "d\u00fcr\u00b7fen", "un\u00b7ser", "Herz", "er\u00b7freun", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und unsers Flei\u00dfes Antrieb sein.", "tokens": ["Und", "un\u00b7sers", "Flei\u00b7\u00dfes", "An\u00b7trieb", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Doch nach den G\u00fctern dieser Zeit", "tokens": ["Doch", "nach", "den", "G\u00fc\u00b7tern", "die\u00b7ser", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit ganzer Seele schmachten,", "tokens": ["Mit", "gan\u00b7zer", "See\u00b7le", "schmach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Nicht erst nach der Gerechtigkeit", "tokens": ["Nicht", "erst", "nach", "der", "Ge\u00b7rech\u00b7tig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "APPR", "ART", "NN"], "meter": "+----+-+", "measure": "dactylic.init"}, "line.4": {"text": "Und Gottes Reiche trachten;", "tokens": ["Und", "Got\u00b7tes", "Rei\u00b7che", "trach\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "NE", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ist dieses eines Menschen Ruf,", "tokens": ["Ist", "die\u00b7ses", "ei\u00b7nes", "Men\u00b7schen", "Ruf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDAT", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den Gott zur Ewigkeit erschuf?", "tokens": ["Den", "Gott", "zur", "E\u00b7wig\u00b7keit", "er\u00b7schuf", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Der Geiz erniedrigt unser Herz,", "tokens": ["Der", "Geiz", "er\u00b7nied\u00b7rigt", "un\u00b7ser", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Erstickt die edlern Triebe.", "tokens": ["Er\u00b7stickt", "die", "ed\u00b7lern", "Trie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Liebe f\u00fcr ein schimmernd Erz", "tokens": ["Die", "Lie\u00b7be", "f\u00fcr", "ein", "schim\u00b7mernd", "Erz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Verdr\u00e4ngt der Tugend Liebe,", "tokens": ["Ver\u00b7dr\u00e4ngt", "der", "Tu\u00b7gend", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und machet, der Vernunft zum Spott,", "tokens": ["Und", "ma\u00b7chet", ",", "der", "Ver\u00b7nunft", "zum", "Spott", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein elend Gold zu deinem Gott.", "tokens": ["Ein", "e\u00b7lend", "Gold", "zu", "dei\u00b7nem", "Gott", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Der Geiz, so viel er an sich rei\u00dft,", "tokens": ["Der", "Geiz", ",", "so", "viel", "er", "an", "sich", "rei\u00dft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "ADV", "PPER", "APPR", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "L\u00e4\u00dft dich kein Gut genie\u00dfen;", "tokens": ["L\u00e4\u00dft", "dich", "kein", "Gut", "ge\u00b7nie\u00b7\u00dfen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er qu\u00e4lt durch Habsucht deinen Geist,", "tokens": ["Er", "qu\u00e4lt", "durch", "Hab\u00b7sucht", "dei\u00b7nen", "Geist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und t\u00f6tet dein Gewissen,", "tokens": ["Und", "t\u00f6\u00b7tet", "dein", "Ge\u00b7wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und rei\u00dft durch schmeichelnden Gewinn", "tokens": ["Und", "rei\u00dft", "durch", "schmei\u00b7cheln\u00b7den", "Ge\u00b7winn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dich blind zu jedem Frevel hin.", "tokens": ["Dich", "blind", "zu", "je\u00b7dem", "Fre\u00b7vel", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPR", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Um wenig Vorteil wird er schon", "tokens": ["Um", "we\u00b7nig", "Vor\u00b7teil", "wird", "er", "schon"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "PIAT", "NN", "VAFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Aus dir mit Meineid sprechen;", "tokens": ["Aus", "dir", "mit", "Mei\u00b7neid", "spre\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dich zwingen, der Arbeiter Lohn", "tokens": ["Dich", "zwin\u00b7gen", ",", "der", "Ar\u00b7bei\u00b7ter", "Lohn"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVINF", "$,", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Unmenschlich abzubrechen;", "tokens": ["Un\u00b7menschlich", "ab\u00b7zu\u00b7bre\u00b7chen", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVIZU", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Er wird in dir der Witwen Flehn,", "tokens": ["Er", "wird", "in", "dir", "der", "Wit\u00b7wen", "Flehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Waisen Tr\u00e4nen widerstehn.", "tokens": ["Der", "Wai\u00b7sen", "Tr\u00e4\u00b7nen", "wi\u00b7der\u00b7stehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Wie k\u00f6nnt ein Herz, vom Geize hart,", "tokens": ["Wie", "k\u00f6nnt", "ein", "Herz", ",", "vom", "Gei\u00b7ze", "hart", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "$,", "APPRART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Wohltat Freuden schmecken,", "tokens": ["Der", "Wohl\u00b7tat", "Freu\u00b7den", "schme\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und in des Ungl\u00fccks Gegenwart", "tokens": ["Und", "in", "des", "Un\u00b7gl\u00fccks", "Ge\u00b7gen\u00b7wart"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Ruf zur H\u00fclf entdecken?", "tokens": ["Den", "Ruf", "zur", "H\u00fclf", "ent\u00b7de\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und wo ist eines Standes Pflicht,", "tokens": ["Und", "wo", "ist", "ei\u00b7nes", "Stan\u00b7des", "Pflicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die nicht der Geiz entehrt und bricht?", "tokens": ["Die", "nicht", "der", "Geiz", "ent\u00b7ehrt", "und", "bricht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "ART", "NN", "VVPP", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Du bist ein Vater; und aus Geiz", "tokens": ["Du", "bist", "ein", "Va\u00b7ter", ";", "und", "aus", "Geiz"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "$.", "KON", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Entziehst du dich den Kindern,", "tokens": ["Ent\u00b7ziehst", "du", "dich", "den", "Kin\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und l\u00e4ssest dich des Goldes Reiz,", "tokens": ["Und", "l\u00e4s\u00b7sest", "dich", "des", "Gol\u00b7des", "Reiz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr Herz zu bilden, hindern;", "tokens": ["Ihr", "Herz", "zu", "bil\u00b7den", ",", "hin\u00b7dern", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "$,", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und glaubst, du habst sie wohl bedacht,", "tokens": ["Und", "glaubst", ",", "du", "habst", "sie", "wohl", "be\u00b7dacht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PPER", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wenn du sie reich, wie dich, gemacht.", "tokens": ["Wenn", "du", "sie", "reich", ",", "wie", "dich", ",", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADJD", "$,", "PWAV", "PPER", "$,", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Du hast ein richterliches Amt;", "tokens": ["Du", "hast", "ein", "rich\u00b7ter\u00b7li\u00b7ches", "Amt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und du wirst dich erfrechen,", "tokens": ["Und", "du", "wirst", "dich", "er\u00b7fre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Sache, die das Recht verdammt,", "tokens": ["Die", "Sa\u00b7che", ",", "die", "das", "Recht", "ver\u00b7dammt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Aus Habsucht recht zu sprechen;", "tokens": ["Aus", "Hab\u00b7sucht", "recht", "zu", "spre\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und selbst der Tugend gr\u00f6\u00dfter Feind", "tokens": ["Und", "selbst", "der", "Tu\u00b7gend", "gr\u00f6\u00df\u00b7ter", "Feind"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Erkauft an dir sich einen Freund.", "tokens": ["Er\u00b7kauft", "an", "dir", "sich", "ei\u00b7nen", "Freund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "PRF", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Gewinnsucht raubt dir Mut und Geist,", "tokens": ["Ge\u00b7winn\u00b7sucht", "raubt", "dir", "Mut", "und", "Geist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Wahrheit frei zu lehren;", "tokens": ["Die", "Wahr\u00b7heit", "frei", "zu", "leh\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Du schweigst, wenn sie dich reden hei\u00dft,", "tokens": ["Du", "schweigst", ",", "wenn", "sie", "dich", "