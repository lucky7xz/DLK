{"dta.poem.14566": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der S\u00e4nger.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Leis w\u00e4chst durch's dunkle Schweigen", "tokens": ["Leis", "w\u00e4chst", "durch's", "dunk\u00b7le", "Schwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein Fl\u00fcstern rings und Neigen", "tokens": ["Ein", "Fl\u00fcs\u00b7tern", "rings", "und", "Nei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wie ein geheimes Singen,", "tokens": ["Wie", "ein", "ge\u00b7hei\u00b7mes", "Sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In immer weitern Ringen", "tokens": ["In", "im\u00b7mer", "wei\u00b7tern", "Rin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADV", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Zieht's alle die da lauschen", "tokens": ["Zieht's", "al\u00b7le", "die", "da", "lau\u00b7schen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "PIS", "ART", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "In seine duft'ge Rund',", "tokens": ["In", "sei\u00b7ne", "duft'\u00b7ge", "Rund'", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wo k\u00fchl im stillen Grund", "tokens": ["Wo", "k\u00fchl", "im", "stil\u00b7len", "Grund"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "APPRART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Die Wasserk\u00fcnste rauschen.", "tokens": ["Die", "Was\u00b7ser\u00b7k\u00fcns\u00b7te", "rau\u00b7schen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}