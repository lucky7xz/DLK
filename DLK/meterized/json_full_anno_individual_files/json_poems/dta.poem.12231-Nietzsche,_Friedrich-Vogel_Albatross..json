{"dta.poem.12231": {"metadata": {"author": {"name": "Nietzsche, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Vogel Albatross.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-30172-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "             O Wunder! Fliegt er noch?", "tokens": ["O", "Wun\u00b7der", "!", "Fliegt", "er", "noch", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "             Er steigt empor und seine Fl\u00fcgel ruhn!", "tokens": ["Er", "steigt", "em\u00b7por", "und", "sei\u00b7ne", "Fl\u00fc\u00b7gel", "ruhn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "KON", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "             Was hebt und tr\u00e4gt ihn doch?", "tokens": ["Was", "hebt", "und", "tr\u00e4gt", "ihn", "doch", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "KON", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "             Was ist ihm Ziel und Zug und Z\u00fcgel nun?", "tokens": ["Was", "ist", "ihm", "Ziel", "und", "Zug", "und", "Z\u00fc\u00b7gel", "nun", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "NN", "KON", "NN", "KON", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "         Er flog zu h\u00f6chst \u2014 nun hebt", "tokens": ["Er", "flog", "zu", "h\u00f6chst", "nun", "hebt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADV", "$(", "ADV", "VVFIN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "         Der Himmel selbst den siegreich Fliegenden:", "tokens": ["Der", "Him\u00b7mel", "selbst", "den", "sieg\u00b7reich", "Flie\u00b7gen\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.3": {"text": "             Nun ruht er still und schwebt,", "tokens": ["Nun", "ruht", "er", "still", "und", "schwebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "             Den Sieg vergessend und den Siegenden.", "tokens": ["Den", "Sieg", "ver\u00b7ges\u00b7send", "und", "den", "Sie\u00b7gen\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}}, "stanza.3": {"line.1": {"text": "         Gleich Stern und Ewigkeit", "tokens": ["Gleich", "Stern", "und", "E\u00b7wig\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "         Lebt er in H\u00f6hn jetzt, die das Leben flieht,", "tokens": ["Lebt", "er", "in", "H\u00f6hn", "jetzt", ",", "die", "das", "Le\u00b7ben", "flieht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "ADV", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "         Mitleidig selbst dem Neid \u2014:", "tokens": ["Mit\u00b7lei\u00b7dig", "selbst", "dem", "Neid", ":"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "ADV", "ART", "NN", "$(", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "             Und hoch flog, wer ihn auch nur schweben sieht!", "tokens": ["Und", "hoch", "flog", ",", "wer", "ihn", "auch", "nur", "schwe\u00b7ben", "sieht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "$,", "PWS", "PPER", "ADV", "ADV", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "             O Vogel Albatross!", "tokens": ["O", "Vo\u00b7gel", "Al\u00b7bat\u00b7ross", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "             Zur H\u00f6he treibt\u2019s mit ew\u2019gem Triebe mich!", "tokens": ["Zur", "H\u00f6\u00b7he", "treibt's", "mit", "ew'\u00b7gem", "Trie\u00b7be", "mich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "APPR", "ADJA", "NN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "         Ich dachte dein: da floss", "tokens": ["Ich", "dach\u00b7te", "dein", ":", "da", "floss"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "$.", "ADV", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "         Mir Thr\u00e4n\u2019 um Thr\u00e4ne \u2014 ja, ich liebe dich!", "tokens": ["Mir", "Thr\u00e4n'", "um", "Thr\u00e4\u00b7ne", "ja", ",", "ich", "lie\u00b7be", "dich", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPR", "NN", "$(", "PTKANT", "$,", "PPER", "VVFIN", "PPER", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}}}}