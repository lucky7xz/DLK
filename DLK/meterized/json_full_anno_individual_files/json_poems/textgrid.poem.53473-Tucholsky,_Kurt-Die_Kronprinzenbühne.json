{"textgrid.poem.53473": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Die Kronprinzenb\u00fchne", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sieh da, sieh da: am preu\u00df'schen Hof", "tokens": ["Sieh", "da", ",", "sieh", "da", ":", "am", "preu\u00df'\u00b7schen", "Hof"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "PTKVZ", "$,", "VVIMP", "ADV", "$.", "APPRART", "ADJA", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "erblickt man einen Musenschwof.", "tokens": ["er\u00b7blickt", "man", "ei\u00b7nen", "Mu\u00b7sen\u00b7schwof", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Man spielt beim Sohn vom Vater", "tokens": ["Man", "spielt", "beim", "Sohn", "vom", "Va\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "APPRART", "NN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Theater.", "tokens": ["The\u00b7a\u00b7ter", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.2": {"line.1": {"text": "Die kleine Zote, lieb und nett,", "tokens": ["Die", "klei\u00b7ne", "Zo\u00b7te", ",", "lieb", "und", "nett", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "wird blank poliert f\u00fcr das Parkett \u2013", "tokens": ["wird", "blank", "po\u00b7liert", "f\u00fcr", "das", "Par\u00b7kett", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punc