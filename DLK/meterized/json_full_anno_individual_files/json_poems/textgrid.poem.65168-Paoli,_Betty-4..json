{"textgrid.poem.65168": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbseht ihr, wenn wir im Kampf", "tokens": ["\u00bb", "seht", "ihr", ",", "wenn", "wir", "im", "Kampf"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "VVFIN", "PPER", "$,", "KOUS", "PPER", "APPRART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Dem Feind gen\u00fcber stehen,", "tokens": ["Dem", "Feind", "ge\u00b7n\u00fc\u00b7ber", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Durch Staub und Pulverdampf", "tokens": ["Durch", "Staub", "und", "Pul\u00b7ver\u00b7dampf"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Mein wei\u00dfes Banner wehen,", "tokens": ["Mein", "wei\u00b7\u00dfes", "Ban\u00b7ner", "we\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Dann folget mir!", "tokens": ["Dann", "fol\u00b7get", "mir", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Seht ihr mich in dem Streit,", "tokens": ["Seht", "ihr", "mich", "in", "dem", "Streit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "ART", "NN", "$,"], "meter": "---+-+", "measure": "unknown.measure.di"}, "line.2": {"text": "Zu dem mit Gnadenzeichen", "tokens": ["Zu", "de