{"dta.poem.792": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "XxVIII.  \n Dominus de me cogitat.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Jn meiner Ersten bl\u00fctt. Jm fr\u00fcling zarter tage ", "tokens": ["In", "mei\u00b7ner", "Ers\u00b7ten", "bl\u00fctt", ".", "Jm", "fr\u00fc\u00b7ling", "zar\u00b7ter", "ta\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$.", "APPRART", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hat mich der grimme Todt verwaiset/ vnd die Nacht", "tokens": ["Hat", "mich", "der", "grim\u00b7me", "Todt", "ver\u00b7wai\u00b7set", "/", "vnd", "die", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "VVFIN", "$(", "KON", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Trawrigkeit vmbh\u00fcllt/ mich hat die herbe macht", "tokens": ["Der", "Traw\u00b7rig\u00b7keit", "vmb\u00b7h\u00fcllt", "/", "mich", "hat", "die", "her\u00b7be", "macht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVPP", "$(", "PPER", "VAFIN", "ART", "ADJA", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Seuchen au\u00dfgezehrt. Ich schmacht in st\u00e4tter plage.", "tokens": ["Der", "Seu\u00b7chen", "au\u00df\u00b7ge\u00b7zehrt", ".", "Ich", "schmacht", "in", "st\u00e4t\u00b7ter", "pla\u00b7ge", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$.", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ich theilte meine zeit/ in Seuffzer/ Noth vnd klage/", "tokens": ["Ich", "theil\u00b7te", "mei\u00b7ne", "zeit", "/", "in", "Seuff\u00b7zer", "/", "Noth", "vnd", "kla\u00b7ge", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$(", "APPR", "NN", "$(", "NN", "KON", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die mittel/ die ich offt f\u00fcr feste Pfeiler acht/", "tokens": ["Die", "mit\u00b7tel", "/", "die", "ich", "offt", "f\u00fcr", "fes\u00b7te", "Pfei\u00b7ler", "acht", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PRELS", "PPER", "ADV", "APPR", "ADJA", "NN", "CARD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die haben (leider!) all erzittert vnd gekracht", "tokens": ["Die", "ha\u00b7ben", "(", "lei\u00b7der", "!", ")", "all", "er\u00b7zit\u00b7tert", "vnd", "ge\u00b7kracht"], "token_info": ["word", "word", "punct", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "$(", "ADV", "$.", "$(", "PIAT", "VVFIN", "KON", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ich trage nur allein den jammer den ich trage.", "tokens": ["Ich", "tra\u00b7ge", "nur", "al\u00b7lein", "den", "jam\u00b7mer", "den", "ich", "tra\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ART", "NN", "ART", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Doch nein! der trewe GOTT. beut mir noch aug v\u00f1 hand", "tokens": ["Doch", "nein", "!", "der", "tre\u00b7we", "GoTT", ".", "beut", "mir", "noch", "aug", "v\u00f1", "hand"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PTKANT", "$.", "ART", "ADJA", "NN", "$.", "VVFIN", "PPER", "ADV", "FM", "FM", "NN"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Sein Hertz ist gegen mir mit Vatertrew\u2019 entbrand/", "tokens": ["Sein", "Hertz", "ist", "ge\u00b7gen", "mir", "mit", "Va\u00b7ter\u00b7tre\u00b7w'", "ent\u00b7brand", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "APPR", "PPER", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+---+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Er ists/ der jeder zeit vor mich/ sein Kind mu\u00df sorgen.", "tokens": ["Er", "ists", "/", "der", "je\u00b7der", "zeit", "vor", "mich", "/", "sein", "Kind", "mu\u00df", "sor\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$(", "ART", "PIAT", "NN", "APPR", "PPER", "$(", "PPOSAT", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Wenn man kein Mittel find/ siht man sein wunderwerck", "tokens": ["Wenn", "man", "kein", "Mit\u00b7tel", "find", "/", "siht", "man", "sein", "wun\u00b7der\u00b7werck"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "PIAT", "NN", "VVFIN", "$(", "VVFIN", "PIS", "PPOSAT", "NN"], "meter": "-+-+-+---+-+", "measure": "unknown.measure.penta"}, "line.2": {"text": "Wenn vnsre krafft vergeht/ bewei\u00dft er seine st\u00e4rck", "tokens": ["Wenn", "vns\u00b7re", "krafft", "ver\u00b7geht", "/", "be\u00b7wei\u00dft", "er", "sei\u00b7ne", "st\u00e4rck"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "VVFIN", "$(", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Man schaw\u2019t jhn/ wenn man meynt/ Er habe sich verborgen.", "tokens": ["Man", "scha\u00b7w't", "jhn", "/", "wenn", "man", "meynt", "/", "Er", "ha\u00b7be", "sich", "ver\u00b7bor\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "$(", "KOUS", "PIS", "VVFIN", "$(", "PPER", "VAFIN", "PRF", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}}}}}