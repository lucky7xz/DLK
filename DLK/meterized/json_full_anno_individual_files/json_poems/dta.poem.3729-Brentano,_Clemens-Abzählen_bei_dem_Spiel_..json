{"dta.poem.3729": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Abz\u00e4hlen bei dem Spiel .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519172", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Eins, zwei, drei,               ", "tokens": ["Eins", ",", "zwei", ",", "drei", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "CARD", "$,", "CARD", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Hicke, hacke, Heu,", "tokens": ["Hi\u00b7cke", ",", "ha\u00b7cke", ",", "Heu", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Hicke, hacke Haberstroh,", "tokens": ["Hi\u00b7cke", ",", "ha\u00b7cke", "Ha\u00b7bers\u00b7troh", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Vater ist ein Schnitzler worden,", "tokens": ["Va\u00b7ter", "ist", "ein", "Schnitz\u00b7ler", "wor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "VAPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Schnitzelt mir ein Bolz,", "tokens": ["Schnit\u00b7zelt", "mir", "ein", "Bolz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Zieh ich mit ins Holz,", "tokens": ["Zieh", "ich", "mit", "ins", "Holz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "APPRART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Zieh ich mit ins gr\u00fcne Gras,", "tokens": ["Zieh", "ich", "mit", "ins", "gr\u00fc\u00b7ne", "Gras", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Altvater, was ist das?", "tokens": ["Alt\u00b7va\u00b7ter", ",", "was", "ist", "das", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWS", "VAFIN", "PDS", "$."], "meter": "-+-++-", "measure": "unknown.measure.tri"}, "line.9": {"text": "Kind, es ist ein wei\u00dfer Haas!", "tokens": ["Kind", ",", "es", "ist", "ein", "wei\u00b7\u00dfer", "Haas", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Puh, den schie\u00df ich auf die Nas.", "tokens": ["Puh", ",", "den", "schie\u00df", "ich", "auf", "die", "Nas", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}