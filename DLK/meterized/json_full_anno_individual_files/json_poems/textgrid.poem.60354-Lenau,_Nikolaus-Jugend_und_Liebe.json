{"textgrid.poem.60354": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Jugend und Liebe", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Jugend folgt, ein Rosenblatt, den Winden;", "tokens": ["Die", "Ju\u00b7gend", "folgt", ",", "ein", "Ro\u00b7sen\u00b7blatt", ",", "den", "Win\u00b7den", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn, jung getrennt, sich wiedersehn die Alten,", "tokens": ["Wenn", ",", "jung", "ge\u00b7trennt", ",", "sich", "wie\u00b7der\u00b7sehn", "die", "Al\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "ADJD", "VVPP", "$,", "PRF", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sie meinen doch, in ihren ernsten Falten", "tokens": ["Sie", "mei\u00b7nen", "doch", ",", "in", "ih\u00b7ren", "erns\u00b7ten", "Fal\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "$,", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den Strahl der s\u00fc\u00dfen Jugend noch zu finden.", "tokens": ["Den", "Strahl", "der", "s\u00fc\u00b7\u00dfen", "Ju\u00b7gend", "noch", "zu", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Des Dauerns Wahn, wer l\u00e4\u00dft ihn gerne schwinden?", "tokens": ["Des", "Dau\u00b7erns", "Wahn", ",", "wer", "l\u00e4\u00dft", "ihn", "ger\u00b7ne", "schwin\u00b7den", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "PWS", "VVFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mag auch ein Herz, das uns geliebt, erkalten,", "tokens": ["Mag", "auch", "ein", "Herz", ",", "das", "uns", "ge\u00b7liebt", ",", "er\u00b7kal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wir suchen immer noch den Traum zu halten,", "tokens": ["Wir", "su\u00b7chen", "im\u00b7mer", "noch", "den", "Traum", "zu", "hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nur stiller sei geworden sein Empfinden.", "tokens": ["Nur", "stil\u00b7ler", "sei", "ge\u00b7wor\u00b7den", "sein", "Emp\u00b7fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "VAPP", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Die Jugend folgt, ein Rosenblatt, den L\u00fcften;", "tokens": ["Die", "Ju\u00b7gend", "folgt", ",", "ein", "Ro\u00b7sen\u00b7blatt", ",", "den", "L\u00fcf\u00b7ten", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Noch leichter als die Jugend flieht die Liebe,", "tokens": ["Noch", "leich\u00b7ter", "als", "die", "Ju\u00b7gend", "flieht", "die", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die nur des Blattes wonnereiches D\u00fcften.", "tokens": ["Die", "nur", "des", "Blat\u00b7tes", "won\u00b7ne\u00b7rei\u00b7ches", "D\u00fcf\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und dennoch an den herben Tod des Sch\u00f6nen,", "tokens": ["Und", "den\u00b7noch", "an", "den", "her\u00b7ben", "Tod", "des", "Sch\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im treuen Wahn, als ob es ihm noch bliebe,", "tokens": ["Im", "treu\u00b7en", "Wahn", ",", "als", "ob", "es", "ihm", "noch", "blie\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "KOKOM", "KOUS", "PPER", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Kann sich das Herz auch sterbend nicht gew\u00f6hnen.", "tokens": ["Kann", "sich", "das", "Herz", "auch", "ster\u00b7bend", "nicht", "ge\u00b7w\u00f6h\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "ART", "NN", "ADV", "ADJD", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Die Jugend folgt, ein Rosenblatt, den Winden;", "t