iegt,", "tokens": ["Die", "Ku\u00b7gel", "pfeift", ",", "der", "Kie\u00b7sel", "fliegt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In L\u00fcften wallt die Fahne!", "tokens": ["In", "L\u00fcf\u00b7ten", "wallt", "die", "Fah\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein General am Boden liegt \u2013", "tokens": ["Ein", "Ge\u00b7ne\u00b7ral", "am", "Bo\u00b7den", "liegt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "O Vorstadt St. Antoine!", "tokens": ["O", "Vor\u00b7stadt", "St.", "An\u00b7toi\u00b7ne", "!"], "token_info": ["word", "word", "abbreviation", "word", "punct"], "pos": ["NE", "NN", "NE", "NE", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Massen auf Massen! Keiner wankt \u2013", "tokens": ["Mas\u00b7sen", "auf", "Mas\u00b7sen", "!", "Kei\u00b7ner", "wankt", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$.", "PIS", "VVFIN", "$("], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Schon hat der Guizot abgedankt,", "tokens": ["Schon", "hat", "der", "Gui\u00b7zot", "ab\u00b7ge\u00b7dankt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Bleich, zitternd mit den Lippen.", "tokens": ["Bleich", ",", "zit\u00b7ternd", "mit", "den", "Lip\u00b7pen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "VVPP", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "\u00bb", "tokens": ["\u00bb"], "token_info": ["punct"], "pos": ["$("]}, "line.2": {"text": "O treffliche Gesellen!", "tokens": ["O", "treff\u00b7li\u00b7che", "Ge\u00b7sel\u00b7len", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Birne Sch\u00fctteltag ist da!", "tokens": ["Der", "Bir\u00b7ne", "Sch\u00fct\u00b7tel\u00b7tag", "ist", "da", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die halbe Linie, ", "tokens": ["Die", "hal\u00b7be", "Li\u00b7nie", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Und Amiens sind Rebellen!", "tokens": ["Und", "A\u00b7miens", "sind", "Re\u00b7bel\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VAFIN", "NN", "$."], "meter": "-+---+-", "measure": "dactylic.init"}, "line.6": {"text": "Keine neue Kriegsmacht naht:", "tokens": ["Kei\u00b7ne", "neu\u00b7e", "Kriegs\u00b7macht", "naht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Das Volk zerst\u00f6rte Schien' und Draht \u2013", "tokens": ["Das", "Volk", "zer\u00b7st\u00f6r\u00b7te", "Schien'", "und", "Draht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Bahnzug und Telegraphen!", "tokens": ["Bahn\u00b7zug", "und", "Te\u00b7le\u00b7gra\u00b