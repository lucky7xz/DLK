e mir,", "tokens": ["Die", "Tr\u00e4\u00b7ne", "springt", "ins", "Au\u00b7ge", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In meinem Herzen singt's: \u00bb", "tokens": ["In", "mei\u00b7nem", "Her\u00b7zen", "singt's", ":", "\u00bb"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Gl\u00fcck auf das ist ein glorreich Jahr,", "tokens": ["Gl\u00fcck", "auf", "das", "ist", "ein", "glor\u00b7reich", "Jahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PDS", "VAFIN", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das ist ein stolzer Februar \u2013", "tokens": ["Das", "ist", "ein", "stol\u00b7zer", "Feb\u00b7ru\u00b7ar", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "\u00bb", "tokens": ["\u00bb"], "token_info": ["punct"], "pos": ["$("]}}}}}