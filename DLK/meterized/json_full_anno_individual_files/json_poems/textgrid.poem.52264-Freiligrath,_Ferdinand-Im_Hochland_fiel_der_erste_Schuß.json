{"textgrid.poem.52264": {"metadata": {"author": {"name": "Freiligrath, Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Im Hochland fiel der erste Schu\u00df", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Hochland fiel der erste Schu\u00df \u2013", "tokens": ["Im", "Hoch\u00b7land", "fiel", "der", "ers\u00b7te", "Schu\u00df", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im Hochland wider die Pfaffen!", "tokens": ["Im", "Hoch\u00b7land", "wi\u00b7der", "die", "Pfaf\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Da kam, die fallen wird und mu\u00df,", "tokens": ["Da", "kam", ",", "die", "fal\u00b7len", "wird", "und", "mu\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PRELS", "VVINF", "VAFIN", "KON", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ja, die Lawine kam in Schu\u00df \u2013", "tokens": ["Ja", ",", "die", "La\u00b7wi\u00b7ne", "kam", "in", "Schu\u00df", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ART", "NN", "VVFIN", "APPR", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Drei L\u00e4nder in den Waffen!", "tokens": ["Drei", "L\u00e4n\u00b7der", "in", "den", "Waf\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Schon kann die Schweiz vom Siegen ruhn:", "tokens": ["Schon", "kann", "die", "Schweiz", "vom", "Sie\u00b7gen", "ruhn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NE", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Das Urgebirg' und die Nagelfluhn", "tokens": ["Das", "Ur\u00b7ge\u00b7bir\u00b7g'", "und", "die", "Na\u00b7gel\u00b7fluhn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Zittern vor Lust bis zum Kerne!", "tokens": ["Zit\u00b7tern", "vor", "Lust", "bis", "zum", "Ker\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "APPR", "APPRART", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.2": {"line.1": {"text": "Drauf ging der Tanz in Welschland los \u2013", "tokens": ["Drauf", "ging", "der", "Tanz", "in", "Wel\u00b7schland", "los", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "NN", "APPR", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Szyllen und Charybden,", "tokens": ["Die", "Szyl\u00b7len", "und", "Cha\u00b7ryb\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Vesuv und \u00c4tna brachen los:", "tokens": ["Ve\u00b7suv", "und", "\u00c4t\u00b7na", "bra\u00b7chen", "los", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NE", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ausbruch auf Ausbruch, Sto\u00df auf Sto\u00df!", "tokens": ["Aus\u00b7bruch", "auf", "Aus\u00b7bruch", ",", "Sto\u00df", "auf", "Sto\u00df", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,", "NN", "APPR", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "\u2013 \u00bbSehr bedenklich, Euer Liebden!\u00ab", "tokens": ["\u2013", "\u00bb", "Sehr", "be\u00b7denk\u00b7lich", ",", "Eu\u00b7er", "Lieb\u00b7den", "!", "\u00ab"], "token_info": ["punct", "punct", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "$(", "ADV", "ADJD", "$,", "PPOSAT", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Also schallt's von Berlin nach Wien,", "tokens": ["Al\u00b7so", "schallt's", "von", "Ber\u00b7lin", "nach", "Wi\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NE", "APPR", "NE", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "Und von Wien zur\u00fcck wieder nach Berlin \u2013", "tokens": ["Und", "von", "Wi\u00b7en", "zu\u00b7r\u00fcck", "wie\u00b7der", "nach", "Ber\u00b7lin", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NE", "ADV", "ADV", "APPR", "NE", "$("], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.8": {"text": "Sogar den Nickel graut es!", "tokens": ["So\u00b7gar", "den", "Ni\u00b7ckel", "graut", "es", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und nun ist denn auch abermals", "tokens": ["Und", "nun", "ist", "denn", "auch", "a\u00b7ber\u00b7mals"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "ADV", "ADV", "ADV"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.2": {"text": "Das Pflaster aufgerissen,", "tokens": ["Das", "Pflas\u00b7ter", "auf\u00b7ge\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Auf dem die Freiheit, nackten Stahls,", "tokens": ["Auf", "dem", "die", "Frei\u00b7heit", ",", "nack\u00b7ten", "Stahls", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Aus der lumpigen Pracht des K\u00f6nigsaals", "tokens": ["Aus", "der", "lum\u00b7pi\u00b7gen", "Pracht", "des", "K\u00f6\u00b7nig\u00b7saals"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Zwei K\u00f6nige schon geschmissen;", "tokens": ["Zwei", "K\u00f6\u00b7ni\u00b7ge", "schon", "ge\u00b7schmis\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "ADV", "VVPP", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Einen von ihnen gar gek\u00f6pft \u2013", "tokens": ["Ei\u00b7nen", "von", "ih\u00b7nen", "gar", "ge\u00b7k\u00f6pft", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "ADV", "VVPP", "$("], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Und drauf du lang genug geschr\u00f6pft", "tokens": ["Und", "drauf", "du", "lang", "ge\u00b7nug", "ge\u00b7schr\u00f6pft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PAV", "PPER", "ADJD", "ADV", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Dein Volk, o Julik\u00f6nig!", "tokens": ["Dein", "Volk", ",", "o", "Ju\u00b7li\u00b7k\u00f6\u00b7nig", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "FM", "NE", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Anr\u00fcckt die Linie: Schu\u00df auf Schu\u00df!", "tokens": ["An\u00b7r\u00fcckt", "die", "Li\u00b7nie", ":", "Schu\u00df", "auf", "Schu\u00df", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$.", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und immer frisch geladen!", "tokens": ["Und", "im\u00b7mer", "frisch", "ge\u00b7la\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Doch dies ist ein Volk wie aus Eisengu\u00df,", "tokens": ["Doch", "dies", "ist", "ein", "Volk", "wie", "aus", "Ei\u00b7sen\u00b7gu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "ART", "NN", "KOKOM", "APPR", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "St\u00fclpen Karren um und Omnibus \u2013", "tokens": ["St\u00fcl\u00b7pen", "Kar\u00b7ren", "um", "und", "Om\u00b7ni\u00b7bus", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "PTKVZ", "KON", "NN", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Das sind Barrikaden!", "tokens": ["Das", "sind", "Bar\u00b7ri\u00b7ka\u00b7den", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Stolze, opferfrohe Reihn,", "tokens": ["Stol\u00b7ze", ",", "op\u00b7fer\u00b7fro\u00b7he", "Reihn", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADJA", "$,", "VVFIN", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Singen sie, in der Hand den Stein:", "tokens": ["Sin\u00b7gen", "sie", ",", "in", "der", "Hand", "den", "Stein", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.8": {"text": "\u00bb", "tokens": ["\u00bb"], "token_info": ["punct"], "pos": ["$("]}}, "stanza.5": {"line.1": {"text": "Die Kugel pfeift, der Kiesel fliegt,", "tokens": ["Die", "Ku\u00b7gel", "pfeift", ",", "der", "Kie\u00b7sel", "fliegt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In L\u00fcften wallt die Fahne!", "tokens": ["In", "L\u00fcf\u00b7ten", "wallt", "die", "Fah\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein General am Boden liegt \u2013", "tokens": ["Ein", "Ge\u00b7ne\u00b7ral", "am", "Bo\u00b7den", "liegt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "O Vorstadt St. Antoine!", "tokens": ["O", "Vor\u00b7stadt", "St.", "An\u00b7toi\u00b7ne", "!"], "token_info": ["word", "word", "abbreviation", "word", "punct"], "pos": ["NE", "NN", "NE", "NE", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Massen auf Massen! Keiner wankt \u2013", "tokens": ["Mas\u00b7sen", "auf", "Mas\u00b7sen", "!", "Kei\u00b7ner", "wankt", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$.", "PIS", "VVFIN", "$("], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Schon hat der Guizot abgedankt,", "tokens": ["Schon", "hat", "der", "Gui\u00b7zot", "ab\u00b7ge\u00b7dankt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Bleich, zitternd mit den Lippen.", "tokens": ["Bleich", ",", "zit\u00b7ternd", "mit", "den", "Lip\u00b7pen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "VVPP", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "\u00bb", "tokens": ["\u00bb"], "token_info": ["punct"], "pos": ["$("]}, "line.2": {"text": "O treffliche Gesellen!", "tokens": ["O", "treff\u00b7li\u00b7che", "Ge\u00b7sel\u00b7len", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Birne Sch\u00fctteltag ist da!", "tokens": ["Der", "Bir\u00b7ne", "Sch\u00fct\u00b7tel\u00b7tag", "ist", "da", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die halbe Linie, ", "tokens": ["Die", "hal\u00b7be", "Li\u00b7nie", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Und Amiens sind Rebellen!", "tokens": ["Und", "A\u00b7miens", "sind", "Re\u00b7bel\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VAFIN", "NN", "$."], "meter": "-+---+-", "measure": "dactylic.init"}, "line.6": {"text": "Keine neue Kriegsmacht naht:", "tokens": ["Kei\u00b7ne", "neu\u00b7e", "Kriegs\u00b7macht", "naht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Das Volk zerst\u00f6rte Schien' und Draht \u2013", "tokens": ["Das", "Volk", "zer\u00b7st\u00f6r\u00b7te", "Schien'", "und", "Draht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Bahnzug und Telegraphen!", "tokens": ["Bahn\u00b7zug", "und", "Te\u00b7le\u00b7gra\u00b7phen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.7": {"line.1": {"text": "Was weiter wird: \u2013 noch harren wir!", "tokens": ["Was", "wei\u00b7ter", "wird", ":", "\u2013", "noch", "har\u00b7ren", "wir", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VAFIN", "$.", "$(", "ADV", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Doch wird's die Freiheit werden!", "tokens": ["Doch", "wird's", "die", "Frei\u00b7heit", "wer\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Freiheit dort, die Freiheit hier,", "tokens": ["Die", "Frei\u00b7heit", "dort", ",", "die", "Frei\u00b7heit", "hier", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Freiheit jetzt und f\u00fcr und f\u00fcr,", "tokens": ["Die", "Frei\u00b7heit", "jetzt", "und", "f\u00fcr", "und", "f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "KON", "APPR", "KON", "APPR", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Freiheit rings auf Erden!", "tokens": ["Die", "Frei\u00b7heit", "rings", "auf", "Er\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Im Hochland fiel der erste Schu\u00df,", "tokens": ["Im", "Hoch\u00b7land", "fiel", "der", "ers\u00b7te", "Schu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und die da niederdonnern mu\u00df,", "tokens": ["Und", "die", "da", "nie\u00b7der\u00b7don\u00b7nern", "mu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die Lawine kam ins Rollen!", "tokens": ["Die", "La\u00b7wi\u00b7ne", "kam", "ins", "Rol\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.8": {"line.1": {"text": "Sie rollt \u2013 sie springt \u2013 o Lombardei,", "tokens": ["Sie", "rollt", "\u2013", "sie", "springt", "\u2013", "o", "Lom\u00b7bar\u00b7dei", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "PPER", "VVFIN", "$(", "FM", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bald f\u00fchlst auch du ihr W\u00e4lzen!", "tokens": ["Bald", "f\u00fchlst", "auch", "du", "ihr", "W\u00e4l\u00b7zen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ungarn und Polen