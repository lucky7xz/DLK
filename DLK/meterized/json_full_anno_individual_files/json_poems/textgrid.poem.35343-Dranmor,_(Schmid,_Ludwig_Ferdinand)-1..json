{"textgrid.poem.35343": {"metadata": {"author": {"name": "Dranmor, (Schmid, Ludwig Ferdinand)", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Herz bet\u00e4ubt und das Gehirn gespalten,", "tokens": ["Das", "Herz", "be\u00b7t\u00e4ubt", "und", "das", "Ge\u00b7hirn", "ge\u00b7spal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "KON", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bin ich gewohnt, mich willig zu bescheiden,", "tokens": ["Bin", "ich", "ge\u00b7wohnt", ",", "mich", "wil\u00b7lig", "zu", "be\u00b7schei\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "$,", "PRF", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Weil mich der Trost erf\u00fcllt, da\u00df allen Leiden", "tokens": ["Weil", "mich", "der", "Trost", "er\u00b7f\u00fcllt", ",", "da\u00df", "al\u00b7len", "Lei\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "VVPP", "$,", "KOUS", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein letztes, sichres Ende vorbehalten.", "tokens": ["Ein", "letz\u00b7tes", ",", "sich\u00b7res", "En\u00b7de", "vor\u00b7be\u00b7hal\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Gesegnet sei des Todes stilles Walten!", "tokens": ["Ge\u00b7seg\u00b7net", "sei", "des", "To\u00b7des", "stil\u00b7les", "Wal\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Geisterbanner kann ich nicht beneiden,", "tokens": ["Die", "Geis\u00b7ter\u00b7ban\u00b7ner", "kann", "ich", "nicht", "be\u00b7nei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die seiner hehren Gr\u00f6\u00dfe ihn entkleiden", "tokens": ["Die", "sei\u00b7ner", "heh\u00b7ren", "Gr\u00f6\u00b7\u00dfe", "ihn", "ent\u00b7klei\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mit keckem Griff in seiner Toga Falten.", "tokens": ["Mit", "ke\u00b7ckem", "Griff", "in", "sei\u00b7ner", "To\u00b7ga", "Fal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Die Leichen liegen starr auf ihren Betten,", "tokens": ["Die", "Lei\u00b7chen", "lie\u00b7gen", "starr", "auf", "ih\u00b7ren", "Bet\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn ihre Asche nicht zerstob im Winde,", "tokens": ["Wenn", "ih\u00b7re", "A\u00b7sche", "nicht", "zer\u00b7stob", "im", "Win\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PTKNEG", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und ruchlos ist das Spielen mit Skeletten.", "tokens": ["Und", "ruch\u00b7los", "ist", "das", "Spie\u00b7len", "mit", "Ske\u00b7let\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Das sei verk\u00fcndet jedem Menschenkinde,", "tokens": ["Das", "sei", "ver\u00b7k\u00fcn\u00b7det", "je\u00b7dem", "Men\u00b7schen\u00b7kin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und vor Nirwanas Heiligkeit verschwinde", "tokens": ["Und", "vor", "Nir\u00b7wa\u00b7nas", "Hei\u00b7lig\u00b7keit", "ver\u00b7schwin\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NE", "NN", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das Reich der Gaukler und der Marionetten.", "tokens": ["Das", "Reich", "der", "Gauk\u00b7ler", "und", "der", "Ma\u00b7ri\u00b7o\u00b7net\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.5": {"line.1": {"text": "Das Herz bet\u00e4ubt und das Gehirn gespalten,", "tokens": ["Das", "Herz", "be\u00b7t\u00e4ubt", "und", "das", "Ge\u00b7hirn", "ge\u00b7spal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "KON", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bin ich gewohnt, mich willig zu bescheiden,", "tokens": ["Bin", "ich", "ge\u00b7wohnt", ",", "mich", "wil\u00b7lig", "zu", "be\u00b7schei\