{"textgrid.poem.44112": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich, Bla\u00df, Charisius und der verbuhlte Bock,", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich, Bla\u00df, Charisius und der verbuhlte Bock,", "tokens": ["Ich", ",", "Bla\u00df", ",", "Cha\u00b7ri\u00b7si\u00b7us", "und", "der", "ver\u00b7buhl\u00b7te", "Bock", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "$,", "NE", "KON", "ART", "ADJA", "NE", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der, weil der Hundsstern brennt, nechst einen Haubenstock", "tokens": ["Der", ",", "weil", "der", "Hunds\u00b7stern", "brennt", ",", "nechst", "ei\u00b7nen", "Hau\u00b7ben\u00b7stock"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "$,", "KOUS", "ART", "NN", "VVFIN", "$,", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Vor seine Liebste gr\u00fc\u00dft, sind jezo gleich beysammen,", "tokens": ["Vor", "sei\u00b7ne", "Liebs\u00b7te", "gr\u00fc\u00dft", ",", "sind", "je\u00b7zo", "gleich", "bey\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,", "VAFIN", "ADV", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Grillen durch ein Thee bey Knaster zu verdammen,", "tokens": ["Die", "Gril\u00b7len", "durch", "ein", "Thee", "bey", "Knas\u00b7ter", "zu", "ver\u00b7dam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "APPR", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Da unterdes Herr Scharff noch auf der Canzel schwizt", "tokens": ["Da", "un\u00b7ter\u00b7des", "Herr", "Scharff", "noch", "auf", "der", "Can\u00b7zel", "schwizt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "NE", "ADV", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und sich um halber zehn durch Lehren starck erhizt.", "tokens": ["Und", "sich", "um", "hal\u00b7ber", "zehn", "durch", "Leh\u00b7ren", "starck", "er\u00b7hizt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "APPR", "APPO", "CARD", "APPR", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wir alle gr\u00fc\u00dfen dich und Wincklern Lobesan,", "tokens": ["Wir", "al\u00b7le", "gr\u00fc\u00b7\u00dfen", "dich", "und", "Win\u00b7ck\u00b7lern", "Lo\u00b7be\u00b7san", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "VVFIN", "PPER", "KON", "NN", "NE", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Und wo der lezte nur vor dem Studiren kan,", "tokens": ["Und", "wo", "der", "lez\u00b7te", "nur", "vor", "dem", "Stu\u00b7di\u00b7ren", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "ADJA", "ADV", "APPR", "ART", "NN", "VMFIN", "$,"], "meter": "-+-+-+--++-+", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "So soll er auch einmahl bey einem kalten Schincken,", "tokens": ["So", "soll", "er", "auch", "ein\u00b7mahl", "bey", "ei\u00b7nem", "kal\u00b7ten", "Schin\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Kommt's ihn gleich sauer an, auf unser Wohlseyn trincken.", "tokens": ["Kommt's", "ihn", "gleich", "sau\u00b7er", "an", ",", "auf", "un\u00b7ser", "Wohl\u00b7seyn", "trin\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJD", "PTKVZ", "$,", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ich, Bla\u00df, Charisius und der verbuhlte Bock,", "tokens": ["Ich", ",", "Bla\u00df", ",", "Cha\u00b7ri\u00b7si\u00b7us", "und", "der", "ver\u00b7buhl\u00b7te", "Bock", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "$,", "NE", "KON", "ART", "ADJA", "NE", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der, weil der Hundsstern brennt, nechst einen Haubenstock", "tokens": ["Der", ",", "weil", "der", "Hunds\u00b7stern", "brennt", ",", "nechst", "ei\u00b7nen", "Hau\u00b7ben\u00b7stock"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "$,", "KOUS", "ART", "NN", "VVFIN", "$,", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Vor seine Liebste gr\u00fc\u00dft, sind jezo gleich beysammen,", "tokens": ["Vor", "sei\u00b7ne", "Liebs\u00b7te", "gr\u00fc\u00dft", ",", "sind", "je\u00b7zo", "gleich", "bey\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,", "VAFIN", "ADV", "ADV", "PTKVZ"