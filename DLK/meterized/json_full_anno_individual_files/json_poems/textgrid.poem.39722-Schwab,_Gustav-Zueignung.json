{"textgrid.poem.39722": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Zueignung", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In das gelobte Land der Liebe", "tokens": ["In", "das", "ge\u00b7lob\u00b7te", "Land", "der", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Hab' ich nur einen Blick gethan:", "tokens": ["Hab'", "ich", "nur", "ei\u00b7nen", "Blick", "ge\u00b7than", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Drum, ob ich tausend Lieder schriebe,", "tokens": ["Drum", ",", "ob", "ich", "tau\u00b7send", "Lie\u00b7der", "schrie\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "KOUS", "PPER", "CARD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sind sie nur alle Traum und Wahn.", "tokens": ["Sind", "sie", "nur", "al\u00b7le", "Traum", "und", "Wahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "PIAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich selbst wei\u00df nicht, was ich gesungen", "tokens": ["Ich", "selbst", "wei\u00df", "nicht", ",", "was", "ich", "ge\u00b7sun\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "PTKNEG", "$,", "PWS", "PPER", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von Liebeslust und Liebeslicht;", "tokens": ["Von", "Lie\u00b7bes\u00b7lust", "und", "Lie\u00b7bes\u00b7licht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es floh mir stammelnd von der Zungen,", "tokens": ["Es", "floh", "mir", "stam\u00b7melnd", "von", "der", "Zun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Was ich gepriesen, ward mir nicht.", "tokens": ["Was", "ich", "ge\u00b7prie\u00b7sen", ",", "ward", "mir", "nicht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "$,", "VAFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Doch du betratst die sel'gen Gr\u00e4nzen,", "tokens": ["Doch", "du", "be\u00b7tratst", "die", "sel'\u00b7gen", "Gr\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nimm! \u2013 Lieb' um Liebe wurde dir;", "tokens": ["Nimm", "!", "\u2013", "Lieb'", "um", "Lie\u00b7be", "wur\u00b7de", "dir", ";"], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "$(", "NN", "APPR", "NN", "VAFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du kannst entziffern und erg\u00e4nzen:", "tokens": ["Du", "kannst", "ent\u00b7zif\u00b7fern", "und", "er\u00b7g\u00e4n\u00b7zen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "En