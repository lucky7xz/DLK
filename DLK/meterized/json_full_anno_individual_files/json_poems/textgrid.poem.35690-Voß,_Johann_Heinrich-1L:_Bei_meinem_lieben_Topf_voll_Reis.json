{"textgrid.poem.35690": {"metadata": {"author": {"name": "Vo\u00df, Johann Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Bei meinem lieben Topf voll Reis", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bei meinem lieben Topf voll Reis", "tokens": ["Bei", "mei\u00b7nem", "lie\u00b7ben", "Topf", "voll", "Reis"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Verschmaus' ich, Sklav des gro\u00dfen Deys,", "tokens": ["Ver\u00b7schmaus'", "ich", ",", "Sklav", "des", "gro\u00b7\u00dfen", "Deys", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "NN", "ART", "ADJA", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Freiheit Last und Kummer.", "tokens": ["Der", "Frei\u00b7heit", "Last", "und", "Kum\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Von Ketten lieblich eingeklirrt,", "tokens": ["Von", "Ket\u00b7ten", "lieb\u00b7lich", "ein\u00b7ge\u00b7klirrt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Schlaf' ich, bis fr\u00fch die Peitsche schwirrt,", "tokens": ["Schlaf'", "ich", ",", "bis", "fr\u00fch", "die", "Peit\u00b7sche", "schwirrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "KOUS", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Arbeit s\u00fc\u00dfen Schlummer.", "tok