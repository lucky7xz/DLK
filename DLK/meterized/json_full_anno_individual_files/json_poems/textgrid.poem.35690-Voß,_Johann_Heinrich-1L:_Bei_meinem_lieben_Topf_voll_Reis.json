iambic.tri"}}, "stanza.4": {"line.1": {"text": "Quecksilber hat der Narr im Kopf,", "tokens": ["Queck\u00b7sil\u00b7ber", "hat", "der", "Narr", "im", "Kopf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der nicht mit Lust bei deinem Topf,", "tokens": ["Der", "nicht", "mit", "Lust", "bei", "dei\u00b7nem", "Topf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "APPR", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Korsarenvater, bleibet!", "tokens": ["Kor\u00b7sa\u00b7ren\u00b7va\u00b7ter", ",", "blei\u00b7bet", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Du bist ja Herr, und wir sind Knecht!", "tokens": ["Du", "bist", "ja", "Herr", ",", "und", "wir", "sind", "Knecht", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "NN", "$,", "KON", "PPER", "VAFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das wollte Gott und V\u00f6lkerrecht!", "tokens": ["Das", "woll\u00b7te", "Gott", "und", "V\u00f6l\u00b7ker\u00b7recht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein Meuter, wer sich str\u00e4ubet!", "tokens": ["Ein", "Meu\u00b7ter", ",", "wer", "sich", "str\u00e4u\u00b7bet", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWS", "PRF", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Das Vaterland? Was