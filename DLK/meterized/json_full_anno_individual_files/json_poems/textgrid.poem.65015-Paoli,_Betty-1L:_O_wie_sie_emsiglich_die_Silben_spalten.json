{"textgrid.poem.65015": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: O wie sie emsiglich die Silben spalten", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O wie sie emsiglich die Silben spalten", "tokens": ["O", "wie", "sie", "em\u00b7sig\u00b7lich", "die", "Sil\u00b7ben", "spal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "PWAV", "PPER", "ADJD", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und m\u00fchevoll des Denkens Grabscheit schwingen", "tokens": ["Und", "m\u00fc\u00b7he\u00b7voll", "des", "Den\u00b7kens", "Grab\u00b7scheit", "schwin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "ART", "NN", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Gottheit tiefstes Wunder zu durchdringen,", "tokens": ["Der", "Got\u00b7theit", "tiefs\u00b7tes", "Wun\u00b7der", "zu", "durch\u00b7drin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Des Genius geheimni\u00dfvolles Walten!", "tokens": ["Des", "Ge\u00b7nius", "ge\u00b7heim\u00b7ni\u00df\u00b7vol\u00b7les", "Wal\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Ist ihm doch selbst die Kunde vorenthalten", "tokens": ["Ist", "ihm", "doch", "selbst", "die", "Kun\u00b7de", "vor\u00b7ent\u00b7hal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Kr\u00e4fte, die ihn r\u00e4thselhaft bedingen!", "tokens": ["Der", "Kr\u00e4f\u00b7te", ",", "die", "ihn", "r\u00e4th\u00b7sel\u00b7haft", "be\u00b7din\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wenn in dem Fr\u00fchlingshauch die Knospen springen,", "tokens": ["Wenn", "in", "dem", "Fr\u00fch\u00b7lings\u00b7hauch", "die", "Knos\u00b7pen", "sprin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sie wissen nicht, wodurch sie sich entfalten.", "tokens": ["Sie", "wis\u00b7sen", "nicht", ",", "wo\u00b7durch", "sie", "sich", "ent\u00b7fal\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWAV", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Der Diamant, gereift in dunkelm Schacht,", "tokens": ["Der", "Di\u00b7a\u00b7mant", ",", "ge\u00b7reift", "in", "dun\u00b7kelm", "Schacht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Perlen, die sich heiter schimmernd r\u00fcnden", "tokens": ["Die", "Per\u00b7len", ",", "die", "sich", "hei\u00b7ter", "schim\u00b7mernd", "r\u00fcn\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PRF", "ADJD", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In unerforschlich tiefer Meeresnacht \u2013", "tokens": ["In", "un\u00b7er\u00b7for\u00b7schlich", "tie\u00b7fer", "Mee\u00b7res\u00b7nacht", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Sie wissen nicht, woran sie sich entz\u00fcnden!", "tokens": ["Sie", "wis\u00b7sen", "nicht", ",", "wo\u00b7ran", "sie", "sich", "ent\u00b7z\u00fcn\u00b7den", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWAV", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ihr aber, freut euch ihres Daseins Pracht", "tokens": ["Ihr", "a\u00b7ber", ",", "freut", "euch", "ih\u00b7res", "Da\u00b7seins", "Pracht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "$,", "VVFIN", "PPER", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und gebt es auf, ihr Werden zu ergr\u00fcnden", "tokens": ["Und", "gebt", "es", "auf", ",", "ihr", "Wer\u00b7den", "zu", "er\u00b7gr\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "$,", "PPOSAT", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "O wie sie emsiglich die Silben spalten", "tokens": ["O", "wie", "sie", "em\u00b7sig\u00b7lich", "die", "Sil\u00b7ben", "spal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "PWAV", "PPER", "ADJD", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und m\u00fchevoll des Denkens Grabscheit schwingen", "tokens": ["Und", "m\u00fc\u00b7he\u00b7voll", "des", "Den\u