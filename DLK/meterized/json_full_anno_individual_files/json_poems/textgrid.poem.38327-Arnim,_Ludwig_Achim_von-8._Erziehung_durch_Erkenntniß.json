{"textgrid.poem.38327": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "8. Erziehung durch Erkenntni\u00df", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O finstre Nacht, wann wirst du doch vergehen,", "tokens": ["O", "finst\u00b7re", "Nacht", ",", "wann", "wirst", "du", "doch", "ver\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "NN", "$,", "PWAV", "VAFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wann bricht mein Lebenslicht herf\u00fcr;", "tokens": ["Wann", "bricht", "mein", "Le\u00b7bens\u00b7licht", "her\u00b7f\u00fcr", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wann werd ich doch von S\u00fcnden auferstehen,", "tokens": ["Wann", "werd", "ich", "doch", "von", "S\u00fcn\u00b7den", "auf\u00b7er\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und leben nur allein in dir.", "tokens": ["Und", "le\u00b7ben", "nur", "al\u00b7lein", "in", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wann werd ich in Gerechtigkeit", "tokens": ["Wann", "werd", "ich", "in", "Ge\u00b7rech\u00b7tig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-+", "m