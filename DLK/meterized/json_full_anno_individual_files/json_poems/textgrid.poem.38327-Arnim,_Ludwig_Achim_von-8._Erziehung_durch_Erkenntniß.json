rd", "punct", "word", "word", "word"], "pos": ["VVIMP", "$,", "PRELS", "ADV", "VAFIN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Steht noch an ihrem Orte dort.", "tokens": ["Steht", "noch", "an", "ih\u00b7rem", "Or\u00b7te", "dort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Streck dich darnach, eil nach ihr zu,", "tokens": ["Streck", "dich", "dar\u00b7nach", ",", "eil", "nach", "ihr", "zu", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PRF", "PAV", "$,", "KOUS", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Du findest sonsten doch nicht Ruh;", "tokens": ["Du", "fin\u00b7dest", "sons\u00b7ten", "doch", "nicht", "Ruh", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "PTKNEG", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Bis du hast diese Kron erstritten,", "tokens": ["Bis", "du", "hast", "die\u00b7se", "Kron", "er\u00b7strit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VAFIN", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und mit dem Herrn den Tod erlitten.", "tokens": ["Und", "mit", "dem", "Herrn", "den", "Tod", "er\u00b7lit\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "O goldnes Meer, durchbrich doch deine D\u00e4mme,", "tokens": ["O", "gold\u00b7nes", "Meer", ",", "durch\u00b7brich", "doch", "dei\u00b7n