{"dta.poem.14464": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Kirchenlied .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Aus des ew'gen Bundes Bogen,", "tokens": ["Aus", "des", "ew'\u00b7gen", "Bun\u00b7des", "Bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Ernst von Glorien umbl\u00fcht,", "tokens": ["Ernst", "von", "Glo\u00b7ri\u00b7en", "um\u00b7bl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NE", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Stehst Du \u00fcber Land und Wogen;", "tokens": ["Stehst", "Du", "\u00fc\u00b7ber", "Land", "und", "Wo\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und ein himmlisch Sehnen zieht", "tokens": ["Und", "ein", "himm\u00b7lisch", "Seh\u00b7nen", "zieht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJD", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Alles Leben himmelw\u00e4rts", "tokens": ["Al\u00b7les", "Le\u00b7ben", "him\u00b7mel\u00b7w\u00e4rts"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "NN", "ADV"], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.6": {"text": "An das gro\u00dfe Mutterherz.", "tokens": ["An", "das", "gro\u00b7\u00dfe", "Mut\u00b7ter\u00b7herz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}