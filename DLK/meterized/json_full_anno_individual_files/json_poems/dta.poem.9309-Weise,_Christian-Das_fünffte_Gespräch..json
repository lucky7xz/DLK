{"dta.poem.9309": {"metadata": {"author": {"name": "Weise, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Das f\u00fcnffte Gespr\u00e4ch.", "genre": "Lyrik; Drama; Prosa", "period": "N.A.", "pub_year": "1701", "urn": "urn:nbn:de:kobv:b4-25043-0", "language": ["de:0.99"], "booktitle": "Weise, Christian: \u00dcberfl\u00fc\u00dfige Gedancken Der gr\u00fcnenden jugend. Leipzig, 1701."}, "poem": {"stanza.1": {"line.1": {"text": "3. Das ander sieht was l\u00e4nger aus", "tokens": ["Das", "an\u00b7der", "sieht", "was", "l\u00e4n\u00b7ger", "aus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "ADJD", "VVFIN", "PWS", "ADJD", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als wie ein schw\u00e4ntzgen an der maus.", "tokens": ["Als", "wie", "ein", "schw\u00e4ntz\u00b7gen", "an", "der", "maus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "ART", "ADJA", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ergert euch nicht an dem gleichn\u00fcsse/ es geschach f\u00fcr-", "tokens": ["Er\u00b7gert", "euch", "nicht", "an", "dem", "gleich\u00b7n\u00fcs\u00b7se", "/", "es", "ge\u00b7schach", "f\u00fcr"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKNEG", "APPR", "ART", "ADJA", "$(", "PPER", "ADJD", "TRUNC"], "meter": "-+--+--+--+-+", "measure": "amphibrach.tetra.plus"}, "line.4": {"text": "wahr des reims wegen.", "tokens": ["wahr", "des", "reims", "we\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "APPR", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Das sol den junggesellen.", "tokens": ["Das", "sol", "den", "jung\u00b7ge\u00b7sel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ART", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Denn bier und wein das f\u00fcllt ihr maul/", "tokens": ["Denn", "bier", "und", "wein", "das", "f\u00fcllt", "ihr", "maul", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "KON", "NN", "PDS", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Zum essen pflegen sie sich faul", "tokens": ["Zum", "es\u00b7sen", "pfle\u00b7gen", "sie", "sich", "faul"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "VVINF", "VVFIN", "PPER", "PRF", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und langsam anzustellen.", "tokens": ["Und", "lang\u00b7sam", "an\u00b7zu\u00b7stel\u00b7len", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVIZU", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}