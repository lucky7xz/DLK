{"textgrid.poem.51287": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "19.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer Rosen brechen wil, verachtet Ri\u00df und Dorn:", "tokens": ["Wer", "Ro\u00b7sen", "bre\u00b7chen", "wil", ",", "ver\u00b7ach\u00b7tet", "Ri\u00df", "und", "Dorn", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VVINF", "VMFIN", "$,", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wer Bier und Brod begehrt: Der drischet Gerst und Korn.", "tokens": ["Wer", "Bier", "und", "Brod", "be\u00b7gehrt", ":", "Der", "dri\u00b7schet", "Gerst", "und", "Korn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "KON", "NN", "VVPP", "$.", "ART", "VVFIN", "ADV", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Kern mu\u00df vor entzwey, dann schmecken Mandeln wol.", "tokens": ["Der", "Kern", "mu\u00df", "vor", "ent\u00b7zwey", ",", "dann", "schme\u00b7cken", "Man\u00b7deln", "wol", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "APPR", "PTKVZ", "$,", "ADV", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Wein mu\u00df seyn gepre\u00dft, dann f\u00fcllt man F\u00e4\u00dfer voll.", "tokens": ["Der", "Wein", "mu\u00df", "seyn", "ge\u00b7pre\u00dft", ",", "dann", "f\u00fcllt", "man", "F\u00e4\u00b7\u00dfer", "voll", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPOSAT", "VVPP", "$,", "ADV", "VVFIN", "PIS", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wer Geld und Gold probirt, scheut nicht die scharffe Glut,", "tokens": ["Wer", "Geld", "und", "Gold", "pro\u00b7birt", ",", "scheut", "nicht", "die", "scharf\u00b7fe", "Glut", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "KON", "NN", "VVPP", "$,", "VVFIN", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wer auff die Erndte hofft, pfl\u00fcgt und bes\u00e4t sein Gut.", "tokens": ["Wer", "auff", "die", "Ernd\u00b7te", "hofft", ",", "pfl\u00fcgt", "und", "be\u00b7s\u00e4t", "sein", "Gut", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "VVFIN", "$,", "VVFIN", "KON", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Der findet wol gewi\u00df, der was recht vorgenommen,", "tokens": ["Der", "fin\u00b7det", "wol", "ge\u00b7wi\u00df", ",", "der", "was", "recht", "vor\u00b7ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ADV", "$,", "PRELS", "PWS", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So schwer das Suchen ist, so lieb ist das Bekommen.", "tokens": ["So", "schwer", "das", "Su\u00b7chen", "ist", ",", "so", "lieb", "ist", "das", "Be\u00b7kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "VAFIN", "$,", "ADV", "ADJD", "VAFIN", "ART", "NN", "$."], "meter": "-