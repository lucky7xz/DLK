{"dta.poem.20597": {"metadata": {"author": {"name": "Hebel, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "Sonntagsfr\u00fche .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1803", "urn": "urn:nbn:de:kobv:b4-200905192133", "language": ["de:0.85", "nl:0.14"], "booktitle": "[Hebel, Johann Peter]: Allemannische Gedichte. Karlsruhe, 1803."}, "poem": {"stanza.1": {"line.1": {"text": "Der Samstig het zum Sunntig gseit;               ", "tokens": ["Der", "Sams\u00b7tig", "het", "zum", "Sunn\u00b7tig", "gseit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VAFIN", "APPRART", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u201ejez hani alli schlofe gleit;", "tokens": ["\u201e", "jez", "ha\u00b7ni", "al\u00b7li", "schlo\u00b7fe", "gleit", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u201esie sin vom Schaffe her und hi", "tokens": ["\u201e", "sie", "sin", "vom", "Schaf\u00b7fe", "her", "und", "hi"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VAFIN", "APPRART", "NN", "PTKVZ", "KON", "XY"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u201egar s\u00f6lli m\u00fced und schl\u00f6srig gsi,", "tokens": ["\u201e", "gar", "s\u00f6l\u00b7li", "m\u00fced", "und", "schl\u00f6s\u00b7rig", "gsi", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "FM", "FM", "KON", "ADJD", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "\u201eund \u2019s gohtmer schier gar selber so,", "tokens": ["\u201e", "und", "'s", "goht\u00b7mer", "schier", "gar", "sel\u00b7ber", "so", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "PPER", "ADJA", "NN", "ADV", "ADV", "ADV", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "\u201ei cha fast uf ke Bei me stoh.\u201c", "tokens": ["\u201e", "i", "cha", "fast", "uf", "ke", "Bei", "me", "stoh", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "FM", "FM", "FM", "FM", "FM", "FM", "FM", "FM", "$.", "$("], "meter": "---+-+-+", "measure": "unknown.measure.tri"}}, "stanza.2": {"line.1": {"text": "So seit er, und wo\u2019s Zw\u00f6lfi schlacht,", "tokens": ["So", "seit", "er", ",", "und", "wo's", "Zw\u00f6l\u00b7fi", "schlacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "$,", "KON", "NE", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "se sinkt er aben in d\u2019 Mitternacht.", "tokens": ["se", "sinkt", "er", "a\u00b7ben", "in", "d'", "Mit\u00b7ter\u00b7nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "NE", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Sunntig seit: \u201eJez ischs an mir!\u201c", "tokens": ["Der", "Sunn\u00b7tig", "seit", ":", "\u201e", "Jez", "ischs", "an", "mir", "!", "\u201c"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "APPR", "$.", "$(", "ADV", "PIS", "APPR", "PPER", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gar still und heimli bschlie\u00dft er d\u2019 Th\u00fcr;", "tokens": ["Gar", "still", "und", "heim\u00b7li", "bschlie\u00dft", "er", "d'", "Th\u00fcr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "NE", "VVFIN", "PPER", "NE", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "er d\u00fcselet hinter de Sterne no,", "tokens": ["er", "d\u00fc\u00b7se\u00b7let", "hin\u00b7ter", "de", "Ster\u00b7ne", "no", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NE", "NN", "NE", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "und cha schier gar nit obsi cho.", "tokens": ["und", "cha", "schier", "gar", "nit", "ob\u00b7si", "cho", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "ADV", "ADV", "PTKNEG", "NE", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Doch endli ribt er d\u2019 Augen us,", "tokens": ["Doch", "end\u00b7li", "ribt", "er", "d'", "Au\u00b7gen", "us", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "NE", "NN", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "er chunnt der Sunn an Th\u00fcr und Hus;", "tokens": ["er", "chunnt", "der", "Sunn", "an", "Th\u00fcr", "und", "Hus", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NN", "KON", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "sie schloft im stille Ch\u00e4mmerli;", "tokens": ["sie", "schloft", "im", "stil\u00b7le", "Ch\u00e4m\u00b7mer\u00b7li", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "er p\u00f6pperlet am L\u00e4demli;", "tokens": ["er", "p\u00f6p\u00b7per\u00b7let", "am", "L\u00e4\u00b7dem\u00b7li", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "er r\u00fceft der Sunne: \u201ed\u2019 Zit isch do!\u201c", "tokens": ["er", "r\u00fceft", "der", "Sun\u00b7ne", ":", "\u201e", "d'", "Zit", "isch", "do", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$.", "$(", "FM.la", "FM.la", "FM.la", "FM.la", "$.", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Sie seit: \u201eJ chumm enanderno!\u201c \u2014", "tokens": ["Sie", "seit", ":", "\u201e", "J", "chumm", "en\u00b7an\u00b7der\u00b7no", "!", "\u201c"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PPER", "APPR", "$.", "$(", "NE", "NE", "NE", "$.", "$(", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und lisli uf de Zeche goht,", "tokens": ["Und", "lis\u00b7li", "uf", "de", "Ze\u00b7che", "goht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "APPR", "NE", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "und fr\u00fcndli uf de Berge stoht", "tokens": ["und", "fr\u00fcnd\u00b7li", "uf", "de", "Ber\u00b7ge", "stoht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "NE", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "der Sunntig, und \u2019s schloft alles no;", "tokens": ["der", "Sunn\u00b7tig", ",", "und", "'s", "schloft", "al\u00b7les", "no", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "$,", "KON", "PPER", "VVFIN", "PIS", "NE", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "es sieht und h\u00f6rt en niemes goh;", "tokens": ["es", "sieht", "und", "h\u00f6rt", "en", "nie\u00b7mes", "goh", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "FM", "FM", "FM", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "er chunnt ins Dorf mit stillem Tritt,", "tokens": ["er", "chunnt", "ins", "Dorf", "mit", "stil\u00b7lem", "Tritt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPRART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "und winkt im Guhl: \u201eVerroth mi nit!\u201c", "tokens": ["und", "winkt", "im", "Guhl", ":", "\u201e", "Ver\u00b7roth", "mi", "nit", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "$.", "$(", "NN", "NE", "PTKNEG", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Und wemmen endli au verwacht,", "tokens": ["Und", "wem\u00b7men", "end\u00b7li", "au", "ver\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NE", "NE", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "und gschlofe het die ganzi Nacht,", "tokens": ["und", "gschlo\u00b7fe", "het", "die", "gan\u00b7zi", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "se stoht er do im Sunne-Schi\u2019,", "tokens": ["se", "stoht", "er", "do", "im", "Sun\u00b7ne\u00b7Schi'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "und luegt eim zu de Fenstern i", "tokens": ["und", "luegt", "eim", "zu", "de", "Fens\u00b7tern", "i"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "APPR", "NE", "NE", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "mit sinen Auge mild und gut,", "tokens": ["mit", "si\u00b7nen", "Au\u00b7ge", "mild", "und", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "und mittem Meyen uffem Hut.", "tokens": ["und", "mit\u00b7tem", "Me\u00b7yen", "uf\u00b7fem", "Hut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Drum meint ers treu, und was i sag,", "tokens": ["Drum", "meint", "ers", "treu", ",", "und", "was", "i", "sag", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PIS", "ADJD", "$,", "KON", "PWS", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "es freut en wemme schlofe mag,", "tokens": ["es", "freut", "en", "wem\u00b7me", "schlo\u00b7fe", "mag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "FM", "FM", "FM", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "und meint es seig no dunkel Nacht,", "tokens": ["und", "meint", "es", "seig", "no", "dun\u00b7kel", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "VVFIN", "NE", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "wenn d\u2019 Sunn am heitere Himmel lacht;", "tokens": ["wenn", "d'", "Sunn", "am", "hei\u00b7te\u00b7re", "Him\u00b7mel", "lacht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "NE", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "drum isch er au so lisli cho,", "tokens": ["drum", "isch", "er", "au", "so", "lis\u00b7li", "cho", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADJD", "PPER", "NE", "ADV", "NE", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "drum stoht er au so liebli do.", "tokens": ["drum", "stoht", "er", "au", "so", "lieb\u00b7li", "do."], "token_info": ["word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["PAV", "VVFIN", "PPER", "NE", "ADV", "VVFIN", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Wie glitzeret uf Gras und Laub", "tokens": ["Wie", "glit\u00b7ze\u00b7ret", "uf", "Gras", "und", "Laub"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "vom Morgethau der Silberstaub!", "tokens": ["vom", "Mor\u00b7ge\u00b7thau", "der", "Sil\u00b7ber\u00b7staub", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie weiht e frische Mayeluft,", "tokens": ["Wie", "weiht", "e", "fri\u00b7sche", "Ma\u00b7ye\u00b7luft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "NE", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "voll Chriesi-Blust und Schleche-Duft!", "tokens": ["voll", "Chrie\u00b7si\u00b7Blust", "und", "Schle\u00b7che\u00b7Duft", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und d\u2019 Immli sammle flink und frisch,", "tokens": ["Und", "d'", "I\u00b7mmli", "samm\u00b7le", "flink", "und", "frisch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "NE", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.6": {"text": "sie w\u00fcsse nit, a\u00df \u2019s Sunntig isch.", "tokens": ["sie", "w\u00fcs\u00b7se", "nit", ",", "a\u00df", "'s", "Sunn\u00b7tig", "isch", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "VVFIN", "PPER", "ADJD", "ADJD", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.8": {"line.1": {"text": "Wie pranget nit im Garte-Land", "tokens": ["Wie", "pran\u00b7get", "nit", "im", "Gar\u00b7te\u00b7Land"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PTKNEG", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "der Chriesi-Baum im Maye-Gwand,", "tokens": ["der", "Chrie\u00b7si\u00b7Baum", "im", "Maye\u00b7\u00b7G\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gel Veieli und Tulipa,", "tokens": ["Gel", "Vei\u00b7e\u00b7li", "und", "Tu\u00b7li\u00b7pa", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "KON", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "und Sterneblume nebe dra,", "tokens": ["und", "Ster\u00b7ne\u00b7blu\u00b7me", "ne\u00b7be", "dra", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "NE", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "und gf\u00fcllti Z