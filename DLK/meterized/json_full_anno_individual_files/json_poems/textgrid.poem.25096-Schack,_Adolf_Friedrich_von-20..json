{"textgrid.poem.25096": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "20.", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn fl\u00fcchtig wir einander nahten,", "tokens": ["Wenn", "fl\u00fcch\u00b7tig", "wir", "ein\u00b7an\u00b7der", "nah\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "War deine Rede scheu und karg;", "tokens": ["War", "dei\u00b7ne", "Re\u00b7de", "scheu", "und", "karg", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Durch nichts ward mir der Schatz verraten,", "tokens": ["Durch", "nichts", "ward", "mir", "der", "Schatz", "ver\u00b7ra\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VAFIN", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den deine Seele still verbarg.", "tokens": ["Den", "dei\u00b7ne", "See\u00b7le", "still", "ver\u00b7barg", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Erst kurz, eh unter schwarzer H\u00fclle", "tokens": ["Erst", "kurz", ",", "eh", "un\u00b7ter", "schwar\u00b7zer", "H\u00fcl\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "$,", "KOUS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sie dich im Tempel aufgebahrt,", "t