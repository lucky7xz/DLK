{"textgrid.poem.54114": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Ru\u00dfland", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es brodelt, es brabbelt, es raunt in der Welt:", "tokens": ["Es", "bro\u00b7delt", ",", "es", "brab\u00b7belt", ",", "es", "raunt", "in", "der", "Welt", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Ru\u00dfland! Ru\u00dfland!", "tokens": ["Ru\u00df\u00b7land", "!", "Ru\u00df\u00b7land", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$.", "NE", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Sie morden! Sie pl\u00fcndern! sie rauben das Geld!", "tokens": ["Sie", "mor\u00b7den", "!", "Sie", "pl\u00fcn\u00b7dern", "!", "sie", "rau\u00b7ben", "das", "Geld", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "$.", "PPER", "VVINF", "$.", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Ru\u00dfland! Ru\u00dfland!", "tokens": ["Ru\u00df\u00b7land", "!", "Ru\u00df\u00b7land", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$.", "NE", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Wie sie die F\u00fcrsten durch Gossen schleifen \u2013", "tokens": ["Wie", "sie", "die", "F\u00fcrs\u00b7ten", "durch", "Gos\u00b7sen", "schlei\u00b7fen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "APPR", "NN", "VVINF", "$("], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "das wird auf den Nachbarn \u00fcbergreifen!", "tokens": ["das", "wird", "auf", "den", "Nach\u00b7barn", "\u00fc\u00b7berg\u00b7rei\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "++--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.7": {"text": "Sie arbeiten nicht! Alles bleibt stehn!", "tokens": ["Sie", "ar\u00b7bei\u00b7ten", "nicht", "!", "Al\u00b7les", "bleibt", "stehn", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$.", "PIS", "VVFIN", "VVINF", "$."], "meter": "--+-+---+", "measure": "anapaest.init"}, "line.8": {"text": "Das Chaos! So kann das nicht weitergehn . . . !", "tokens": ["Das", "Chaos", "!", "So", "kann", "das", "nicht", "wei\u00b7ter\u00b7gehn", ".", ".", ".", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct", "punct", "punct"], "pos": ["ART", "NN", "$.", "ADV", "VMFIN", "PDS", "PTKNEG", "VVINF", "$.", "$.", "$.", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Sperrt die Grenzen ab! Der Prolet wird begehrlich!", "tokens": ["Sperrt", "die", "Gren\u00b7zen", "ab", "!", "Der", "Pro\u00b7let", "wird", "be\u00b7gehr\u00b7lich", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKVZ", "$.", "ART", "NN", "VAFIN", "ADJD", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.10": {"text": "Ru\u00dfland \u2013", "tokens": ["Ru\u00df\u00b7land", "\u2013"], "token_info": ["word", "punct"], "pos": ["NE", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.11": {"text": "Ru\u00dfland ist gef\u00e4hrlich.", "tokens": ["Ru\u00df\u00b7land", "ist", "ge\u00b7f\u00e4hr\u00b7lich", "."], "token_info": ["