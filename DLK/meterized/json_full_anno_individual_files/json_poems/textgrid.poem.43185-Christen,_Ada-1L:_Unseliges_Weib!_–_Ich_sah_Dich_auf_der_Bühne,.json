": ["Du", "f\u00fchlst", "sein", "Aug'", "an", "Dei\u00b7nen", "Lip\u00b7pen", "han\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Voll hei\u00dfem, jugendfrohem Liebverlangen \u2013", "tokens": ["Voll", "hei\u00b7\u00dfem", ",", "ju\u00b7gend\u00b7fro\u00b7hem", "Lieb\u00b7ver\u00b7lan\u00b7gen", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "$,", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Da zuckt Dein Mund von unterdr\u00fccktem Weinen...", "tokens": ["Da", "zuckt", "Dein", "Mund", "von", "un\u00b7ter\u00b7dr\u00fcck\u00b7tem", "Wei\u00b7nen", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Wohl bist Du sch\u00f6n, die k\u00f6niglichen Glieder", "tokens": ["Wohl", "bist", "Du", "sch\u00f6n", ",", "die", "k\u00f6\u00b7nig\u00b7li\u00b7chen", "Glie\u00b7der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Sie leuchten durch die schimmernd-weiche H\u00fclle,", "tokens": ["Sie", "leuch\u00b7ten", "durch", "die", "schim\u00b7mern\u00b7dwei\u00b7che", "H\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Der gold'nen Locken \u00fcppig-duft'ge F\u00fclle", "tokens": ["Der", "gold'\u00b7nen", "Lo\u00b7cken", "\u00fcp\u00b7pig\u00b7duft'\u00b7ge", "F\