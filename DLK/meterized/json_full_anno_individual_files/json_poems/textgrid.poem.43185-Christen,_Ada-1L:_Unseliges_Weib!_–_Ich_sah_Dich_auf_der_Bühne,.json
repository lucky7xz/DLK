{"textgrid.poem.43185": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "1L: Unseliges Weib! \u2013 Ich sah Dich auf der B\u00fchne,", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Unseliges Weib! \u2013 Ich sah Dich auf der B\u00fchne,", "tokens": ["Un\u00b7se\u00b7li\u00b7ges", "Weib", "!", "\u2013", "Ich", "sah", "Dich", "auf", "der", "B\u00fch\u00b7ne", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "$(", "PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Ich h\u00f6rte Dein berauschend-s\u00fc\u00dfes Singen,", "tokens": ["Ich", "h\u00f6r\u00b7te", "Dein", "be\u00b7rau\u00b7schen\u00b7ds\u00fc\u00b7\u00dfes", "Sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich sah Dich lachen und den Becher schwingen,", "tokens": ["Ich", "sah", "Dich", "la\u00b7chen", "und", "den", "Be\u00b7cher", "schwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVINF", "KON", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sah Deinen Blick \u2013 und f\u00fchlte Deine S\u00fchne...", "tokens": ["Sah", "Dei\u00b7nen", "Blick", "\u2013", "und", "f\u00fchl\u00b7te", "Dei\u00b7ne", "S\u00fch\u00b7ne", "..."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$(", "KON", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Denn