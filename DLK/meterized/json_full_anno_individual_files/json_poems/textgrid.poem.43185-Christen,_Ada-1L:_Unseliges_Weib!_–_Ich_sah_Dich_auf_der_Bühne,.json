": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "ADV", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Du f\u00fchlst schon heute Deiner Jugend Sterben,", "tokens": ["Du", "f\u00fchlst", "schon", "heu\u00b7te", "Dei\u00b7ner", "Ju\u00b7gend", "Ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Die Todesangst sie klingt selbst durch Dein Lachen,", "tokens": ["Die", "To\u00b7de\u00b7sangst", "sie", "klingt", "selbst", "durch", "Dein", "La\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Du wei\u00dft es: der Geliebte wird erwachen \u2013", "tokens": ["Du", "wei\u00dft", "es", ":", "der", "Ge\u00b7lieb\u00b7te", "wird", "er\u00b7wa\u00b7chen", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$.", "ART", "NN", "VAFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Und sein Erwachen, Weib, ist Dein Verderben...", "tokens": ["Und", "sein", "Er\u00b7wa\u00b7chen", ",", "Weib", ",", "ist", "Dein", "Ver\u00b7der\u00b7ben", "..."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "NN", "$,", "VAFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "Umh\u00fcll' Dein Haupt alsdann mit schwarzen Schleiern", "tokens": ["Um\u00b7h\u00fcll'", "Dein", "Haupt", "als\u00b7dann", "mit", "schwar\u00b7zen", "Schlei\u0