{"dta.poem.10984": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Schuldigkeit  \n der Menschen Gott zu dienen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es ist mir oftermal der Zweifel aufgestiegen,", "tokens": ["Es", "ist", "mir", "of\u00b7ter\u00b7mal", "der", "Zwei\u00b7fel", "auf\u00b7ge\u00b7stie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ob auch die Menschheit wohl, der Gottheit zum Vergn\u00fcgen,", "tokens": ["Ob", "auch", "die", "Menschheit", "wohl", ",", "der", "Got\u00b7theit", "zum", "Ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "ADV", "$,", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+--+++-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Jhm Lob und Ruhm und Preis und Ehre", "tokens": ["Jhm", "Lob", "und", "Ruhm", "und", "Preis", "und", "Eh\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "NN", "KON", "NN", "KON", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zu geben, f\u00e4hig w\u00e4re,", "tokens": ["Zu", "ge\u00b7ben", ",", "f\u00e4\u00b7hig", "w\u00e4\u00b7re", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Da wir so niedrig und so klein,", "tokens": ["Da", "wir", "so", "nied\u00b7rig", "und", "so", "klein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "KON", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Jm Gegensatz von seiner Gr\u00f6\u00dfe, seyn?", "tokens": ["Jm", "Ge\u00b7gen\u00b7satz", "von", "sei\u00b7ner", "Gr\u00f6\u00b7\u00dfe", ",", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "PPOSAT", "NN", "$,", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "K\u00f6nnt eine M\u00fccke wohl, gedacht ich, durch ihr Singen,", "tokens": ["K\u00f6nnt", "ei\u00b7ne", "M\u00fc\u00b7cke", "wohl", ",", "ge\u00b7dacht", "ich", ",", "durch", "ihr", "Sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "ADV", "$,", "VVPP", "PPER", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Uns Ruhm und Preis und Ehre bringen?", "tokens": ["Uns", "Ruhm", "und", "Preis", "und", "Eh\u00b7re", "brin\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "KON", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Noch minder wird, durch uns, der Gottheit Ruhm erh\u00f6ht.", "tokens": ["Noch", "min\u00b7der", "wird", ",", "durch", "uns", ",", "der", "Got\u00b7theit", "Ruhm", "er\u00b7h\u00f6ht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "$,", "APPR", "PPER", "$,", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Zumal, da unser Dienst im Bethen meist besteht,", "tokens": ["Zu\u00b7mal", ",", "da", "un\u00b7ser", "Dienst", "im", "Be\u00b7then", "meist", "be\u00b7steht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPOSAT", "NN", "APPRART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Jm Bethen, worin wir, wenn wirs recht \u00fcberlegen,", "tokens": ["Jm", "Be\u00b7then", ",", "wo\u00b7rin", "wir", ",", "wenn", "wirs", "recht", "\u00fc\u00b7berl\u00b7e\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PWAV", "PPER", "$,", "KOUS", "PIS", "ADJD", "VVINF", "$,"], "meter": "--+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Weit mehr, als unserm Gott, uns selbst zu dienen pflegen.", "tokens": ["Weit", "mehr", ",", "als", "un\u00b7serm", "Gott", ",", "uns", "selbst", "zu", "die\u00b7nen", "pfle\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "$,", "KOUS", "PPOSAT", "NN", "$,", "PPER", "ADV", "PTKZU", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Allein es fiel mir auch hingegen ein,", "tokens": ["Al\u00b7lein", "es", "fiel", "mir", "auch", "hin\u00b7ge\u00b7gen", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "PPER", "ADV", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Was wir, durch den Verstand, f\u00fcr Gaben,", "tokens": ["Was", "wir", ",", "durch", "den", "Ver\u00b7stand", ",", "f\u00fcr", "Ga\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "PPER", "$,", "APPR", "ART", "NN", "$,", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "F\u00fcr andere Gesch\u00f6pf, empfangen haben,", "tokens": ["F\u00fcr", "an\u00b7de\u00b7re", "Ge\u00b7sch\u00f6pf", ",", "emp\u00b7fan\u00b7gen", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Ja da\u00df, selbst durchs Gebeth, ob es gleich nicht so scheint,", "tokens": ["Ja", "da\u00df", ",", "selbst", "durchs", "Ge\u00b7beth", ",", "ob", "es", "gleich", "nicht", "so", "scheint", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "KOUS", "$,", "ADV", "APPRART", "NN", "$,", "KOUS", "PPER", "ADV", "PTKNEG", "ADV", "VVFIN", "$,"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.17": {"text": "Man Gott mehr dienet, als man meynt.", "tokens": ["Man", "Gott", "mehr", "die\u00b7net", ",", "als", "man", "meynt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "NN", "ADV", "VVFIN", "$,", "KOUS", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Ich mu\u00df es, dir und mir zur Lehr und Trost, erz\u00e4hlen:", "tokens": ["Ich", "mu\u00df", "es", ",", "dir", "und", "mir", "zur", "Lehr", "und", "Trost", ",", "er\u00b7z\u00e4h\u00b7len", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "$,", "PPER", "KON", "PPER", "APPRART", "NN", "KON", "NN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Wir opfern im Gebeth, nicht nur von unsern Seelen,", "tokens": ["Wir", "op\u00b7fern", "im", "Ge\u00b7beth", ",", "nicht", "nur", "von", "un\u00b7sern", "See\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,", "PTKNEG", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Der Gottheit einen Theil; wir zeigen noch dabey,", "tokens": ["Der", "Got\u00b7theit", "ei\u00b7nen", "Theil", ";", "wir", "zei\u00b7gen", "noch", "da\u00b7bey", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$.", "PPER", "VVFIN", "ADV", "PAV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Da\u00df wir begreif- und \u00fcberzeuglich wissen,", "tokens": ["Da\u00df", "wir", "be\u00b7greif", "und", "\u00fc\u00b7berz\u00b7eug\u00b7lich", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "TRUNC", "KON", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Wie Gott so gro\u00df, und uns zu helfen m\u00e4chtig sey,", "tokens": ["Wie", "Gott", "so", "gro\u00df", ",", "und", "uns", "zu", "hel\u00b7fen", "m\u00e4ch\u00b7tig", "sey", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADV", "ADJD", "$,", "KON", "PPER", "PTKZU", "VVINF", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Da\u00df wir, allein durch ihn, geholfen werden m\u00fcssen.", "tokens": ["Da\u00df", "wir", ",", "al\u00b7lein", "durch", "ihn", ",", "ge\u00b7hol\u00b7fen", "wer\u00b7den", "m\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "ADV", "APPR", "PPER", "$,", "VVPP", "VAINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Glaub, Hoffnung, Demuth, Lieb und eine Zuversicht,", "tokens": ["Glaub", ",", "Hoff\u00b7nung", ",", "De\u00b7muth", ",", "Lieb", "und", "ei\u00b7ne", "Zu\u00b7ver\u00b7sicht", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Er werde, wie er kann, auch uns zum Besten, wollen,", "tokens": ["Er", "wer\u00b7de", ",", "wie", "er", "kann", ",", "auch", "uns", "zum", "Bes\u00b7ten", ",", "wol\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "PWAV", "PPER", "VMFIN", "$,", "ADV", "PPER", "APPRART", "NN", "$,", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Steckt alles im Gebeth. So la\u00dft nach unsrer Pflicht,", "tokens": ["Steckt", "al\u00b7les", "im", "Ge\u00b7beth", ".", "So", "la\u00dft", "nach", "uns\u00b7rer", "Pflicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPRART", "NN", "$.", "ADV", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Uns Gott, was ihm geb\u00fchrt, im br\u00fcnstgem Bethen, zollen.", "tokens": ["Uns", "Gott", ",", "was", "ihm", "ge\u00b7b\u00fchrt", ",", "im", "br\u00fcnst\u00b7gem", "Be\u00b7then", ",", "zol\u00b7len", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "NN", "$,", "PWS", "PPER", "VVPP", "$,", "APPRART", "ADJA", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Ja! la\u00dft uns weiter gehn,", "tokens": ["Ja", "!", "la\u00dft", "uns", "wei\u00b7ter", "gehn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "VVIMP", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.29": {"text": "Und wie wir unserm Gott, im Danken, zu erh\u00f6hn,", "tokens": ["Und", "wie", "wir", "un\u00b7serm", "Gott", ",", "im", "Dan\u00b7ken", ",", "zu", "er\u00b7h\u00f6hn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "PPOSAT", "NN", "$,", "APPRART", "NN", "$,", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Wie niedrig wir auch sind, mit frohem Ernst, besehn!", "tokens": ["Wie", "nied\u00b7rig", "wir", "auch", "sind", ",", "mit", "fro\u00b7hem", "Ernst", ",", "be\u00b7sehn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "ADV", "VAFIN", "$,", "APPR", "ADJA", "NN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Jm Dank ist alles das, was im Gebeth, nicht nur;", "tokens": ["Jm", "Dank", "ist", "al\u00b7les", "das", ",", "was", "im", "Ge\u00b7beth", ",", "nicht", "nur", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PIS", "PDS", "$,", "PRELS", "APPRART", "NN", "$,", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Es schlie\u00dft, zu Gottes Ehr,", "tokens": ["Es", "schlie\u00dft", ",", "zu", "Got\u00b7tes", "Ehr", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.33": {"text": "Das Danken noch viel mehr,", "tokens": ["Das", "Dan\u00b7ken", "noch", "viel", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.34": {"text": "Als wie das Bethen, ein. Wir finden eine Spur", "tokens": ["Als", "wie", "das", "Be\u00b7then", ",", "ein", ".", "Wir", "fin\u00b7den", "ei\u00b7ne", "Spur"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "KOKOM", "ART", "NN", "$,", "PTKVZ", "$.", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Von Inbrunst, Freundlichkeit, Erk\u00e4nntlichkeit und Liebe,", "tokens": ["Von", "In\u00b7brunst", ",", "Freund\u00b7lich\u00b7keit", ",", "Er\u00b7k\u00e4nnt\u00b7lich\u00b7keit", "und", "Lie\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Vergn\u00fcgen, Andacht, Lust und Flammen-reiche Triebe,", "tokens": ["Ver\u00b7gn\u00fc\u00b7gen", ",", "An\u00b7dacht", ",", "Lust", "und", "Flam\u00b7men\u00b7rei\u00b7che", "Trie\u00b7be", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Des gro\u00dfen Sch\u00f6pfers heilgen Willen,", "tokens": ["Des", "gro\u00b7\u00dfen", "Sch\u00f6p\u00b7fers", "heil\u00b7gen", "Wil\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.38": {"text": "Verbothnen Lastern feind, gehorsam zu erf\u00fcllen,", "tokens": ["Ver\u00b7both\u00b7nen", "Las\u00b7tern", "feind", ",", "ge\u00b7hor\u00b7sam", "zu", "er\u00b7f\u00fcl\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$,", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Bewunderung, Begier, in seinen Werken,", "tokens": ["Be\u00b7wun\u00b7de\u00b7rung", ",", "Be\u00b7gier", ",", "in", "sei\u00b7nen", "Wer\u00b7ken", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.40": {"text": "Des Sch\u00f6pfers Weisheit, Macht und G\u00fcte zu bemerken,", "tokens": ["Des", "Sch\u00f6p\u00b7fers", "Weis\u00b7heit", ",", "Macht", "und", "G\u00fc\u00b7te", "zu", "be\u00b7mer\u00b7ken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "NN", "KON", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Den Abgrund seiner Macht, erstaunt, zu \u00fcberdenken,", "tokens": ["Den", "Ab\u00b7grund", "sei\u00b7ner", "Macht", ",", "er\u00b7staunt", ",", "zu", "\u00fc\u00b7ber\u00b7den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,", "VVPP", "$,", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "In seine Lieb allein uns br\u00fcnstig zu versenken,", "tokens": ["In", "sei\u00b7ne", "Lieb", "al\u00b7lein", "uns", "br\u00fcns\u00b7tig", "zu", "ver\u00b7sen\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", 