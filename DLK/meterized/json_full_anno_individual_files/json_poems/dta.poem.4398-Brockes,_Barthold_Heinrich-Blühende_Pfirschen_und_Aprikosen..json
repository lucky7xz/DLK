{"dta.poem.4398": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Bl\u00fchende Pfirschen und Aprikosen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1727", "urn": "urn:nbn:de:kobv:b4-200905198599", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn den Mund die Pfirsich f\u00fcllet,", "tokens": ["Wenn", "den", "Mund", "die", "Pfir\u00b7sich", "f\u00fcl\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und den Durst mit Anmut stillet,", "tokens": ["Und", "den", "Durst", "mit", "An\u00b7mut", "stil\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df die Zung\u2019 in Honig schwimm\u2019t;", "tokens": ["Da\u00df", "die", "Zung'", "in", "Ho\u00b7nig", "schwim\u00b7m't", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Ach! so sch\u00e4tz\u2019t es nicht geringe!", "tokens": ["Ach", "!", "so", "sch\u00e4tz't", "es", "nicht", "ge\u00b7rin\u00b7ge", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ADV", "VVFIN", "PPER", "PTKNEG", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Dankt dem Sch\u00f6pfer aller Dinge,", "tokens": ["Dankt", "dem", "Sch\u00f6p\u00b7fer", "al\u00b7ler", "Din\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Der euch so viel Gut\u2019s bestimm\u2019t!", "tokens": ["Der", "euch", "so", "viel", "Gut's", "be\u00b7stim\u00b7m't", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "PIAT", "NN", "VVFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}