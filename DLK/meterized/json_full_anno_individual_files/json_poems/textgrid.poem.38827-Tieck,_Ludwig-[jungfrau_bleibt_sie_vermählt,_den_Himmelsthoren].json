{"textgrid.poem.38827": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "[jungfrau bleibt sie verm\u00e4hlt, den Himmelsthoren]", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Jungfrau bleibt sie verm\u00e4hlt, den Himmelsthoren", "tokens": ["Jung\u00b7frau", "bleibt", "sie", "ver\u00b7m\u00e4hlt", ",", "den", "Him\u00b7mel\u00b7stho\u00b7ren"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "VVPP", "$,", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Entsteigt ein lichter Engel, ihrem Flehen", "tokens": ["Ent\u00b7steigt", "ein", "lich\u00b7ter", "En\u00b7gel", ",", "ih\u00b7rem", "Fle\u00b7hen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Rauscht lieblich t\u00f6nend seiner Fl\u00fcgel Wehen,", "tokens": ["Rauscht", "lieb\u00b7lich", "t\u00f6\u00b7nend", "sei\u00b7ner", "Fl\u00fc\u00b7gel", "We\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ADJD", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Er singt: der Herr hat dich als sein erkohren.", "tokens": ["Er", "singt", ":", "der", "Herr", "hat", "dich", "als", "sein", "er\u00b7koh\u00b7ren", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "ART", "NN", "VAFIN", "PPER", "KOUS", "PPOSAT", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Da weint sie, da\u00df der Br\u00e4utigam verloren,", "tokens": ["Da", "weint", "sie", ",", "da\u00df", "der", "Br\u00e4u\u00b7ti\u00b7gam", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "ART", "NE", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df er den Bronn des Lebens will verschm\u00e4hen;", "tokens": ["Da\u00df", "er", "den", "Bronn", "des", "Le\u00b7bens", "will", "ver\u00b7schm\u00e4\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Kann dieser Blick, spricht er, den Engel sehen,", "tokens": ["Kann", "die\u00b7ser", "Blick", ",", "spricht", "er", ",", "den", "En\u00b7gel", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PDAT", "NN", "$,", "VVFIN", "PPER", "$,", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So sei alsbald der G\u00f6tzendienst verschworen.", "tokens": ["So", "sei", "als\u00b7bald", "der", "G\u00f6t\u00b7zen\u00b7dienst", "ver\u00b7schwo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Sie wirft sich betend nieder: la\u00df nicht rauben", "tokens": ["Sie", "wirft", "sich", "be\u00b7tend", "nie\u00b7der", ":", "la\u00df", "nicht", "rau\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "ADJD", "PTKVZ", "$.", "VVIMP", "PTKNEG", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dies edle Herz, im Zweifel nicht erblinden!", "tokens": ["Dies", "ed\u00b7le", "Herz", ",", "im", "Zwei\u00b7fel", "nicht", "er\u00b7blin\u00b7den", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJA", "NN", "$,", "APPRART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Er sieht den Seraph, glaubt, vom Licht getroffen.", "tokens": ["Er", "sieht", "den", "Se\u00b7raph", ",", "glaubt", ",", "vom", "Licht", "ge\u00b7trof\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "VVFIN", "$,", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Doch fester steht des frommen Christen Hoffen,", "tokens": ["Doch", "fes\u00b7ter", "steht", "des", "from\u00b7men", "Chris\u00b7ten", "Hof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Er h\u00f6rt wie alle Orgelt\u00f6ne k\u00fcnden:", "tokens": ["Er", "h\u00f6rt", "wie", "al\u00b7le", "Or\u00b7gel\u00b7t\u00f6\u00b7ne", "k\u00fcn\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ja, seelig sind, die nicht sehn und doch glauben.", "tokens": ["Ja", ",", "see\u00b7lig", "sind", ",", "die", "nicht", "sehn", "und", "doch", "glau\u00b7ben", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADJD", "VAFIN", "$,", "PRELS", "PTKNEG", "VVINF", "KON", "ADV", "VVINF", "$."], "meter": "-+-++-+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.5": {"