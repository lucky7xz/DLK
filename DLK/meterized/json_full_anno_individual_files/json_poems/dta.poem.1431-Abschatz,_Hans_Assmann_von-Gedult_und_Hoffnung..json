{"dta.poem.1431": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Gedult und Hoffnung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Mund ist zugethan/", "tokens": ["Mein", "Mund", "ist", "zu\u00b7ge\u00b7than", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Mein Hertz in GOtt zu GOtt sich neiget in der Stille/", "tokens": ["Mein", "Hertz", "in", "Gott", "zu", "Gott", "sich", "nei\u00b7get", "in", "der", "Stil\u00b7le", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "APPR", "NN", "PRF", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Was ich nicht \u00e4ndern kan/", "tokens": ["Was", "ich", "nicht", "\u00e4n\u00b7dern", "kan", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PTKNEG", "VVINF", "VMFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Steht ja in GOttes Hand. Es l\u00e4st sein weiser Wille", "tokens": ["Steht", "ja", "in", "Got\u00b7tes", "Hand", ".", "Es", "l\u00e4st", "sein", "wei\u00b7ser", "Wil\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "NN", "NN", "$.", "PPER", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "In seinem Rath kein Widersprechen ein:", "tokens": ["In", "sei\u00b7nem", "Rath", "kein", "Wi\u00b7der\u00b7spre\u00b7chen", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Drum soll mein Auffenthalt ", "tokens": ["Drum", "soll", "mein", "Auf\u00b7fent\u00b7halt"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "VMFIN", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wer hemmt Noth und Gefahr/", "tokens": ["Wer", "hemmt", "Noth", "und", "Ge\u00b7fahr", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "NN", "KON", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Wer kan mit Macht entgehn dem grossen Allmachts-Grim-", "tokens": ["Wer", "kan", "mit", "Macht", "ent\u00b7gehn", "dem", "gros\u00b7sen", "All\u00b7machts\u00b7Grim"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "APPR", "NN", "VVFIN", "ART", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das erste Menschen Paar/", "tokens": ["Das", "ers\u00b7te", "Men\u00b7schen", "Paar", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Da nach dem ersten Fall des H\u00f6chsten Eyfer-Stimme", "tokens": ["Da", "nach", "dem", "ers\u00b7ten", "Fall", "des", "H\u00f6chs\u00b7ten", "Ey\u00b7fer\u00b7Stim\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Den Fluch zwar sprach/ doch Segen sezte drauff/", "tokens": ["Den", "Fluch", "zwar", "sprach", "/", "doch", "Se\u00b7gen", "sez\u00b7te", "drauff", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "$(", "ADV", "NN", "VVFIN", "PAV", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Nahm jenes mit ", "tokens": ["Nahm", "je\u00b7nes", "mit"], "token_info": ["word", "word", "word"], "pos": ["NN", "PDAT", "APPR"], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "So sollen alle wir", "tokens": ["So", "sol\u00b7len", "al\u00b7le", "wir"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PIS", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Uns/ wie es immer laufft/ in das Verh\u00e4ngnis schicken/", "tokens": ["Uns", "/", "wie", "es", "im\u00b7mer", "laufft", "/", "in", "das", "Ver\u00b7h\u00e4ng\u00b7nis", "schi\u00b7cken", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$(", "PWAV", "PPER", "ADV", "VVFIN", "$(", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und GOtt nicht schreiben f\u00fcr/", "tokens": ["Und", "Gott", "nicht", "schrei\u00b7ben", "f\u00fcr", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PTKNEG", "VVINF", "APPR", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Er kennet unsre Krafft/ und pr\u00fcfet unsern R\u00fccken.", "tokens": ["Er", "ken\u00b7net", "uns\u00b7re", "Krafft", "/", "und", "pr\u00fc\u00b7fet", "un\u00b7sern", "R\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$(", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Schw\u00e4cht Unfall gleich Hertz/ Sinnen/ Bein und Marck/", "tokens": ["Schw\u00e4cht", "Un\u00b7fall", "gleich", "Hertz", "/", "Sin\u00b7nen", "/", "Bein", "und", "Marck", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "ADV", "NN", "$(", "NN", "$(", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Macht uns doch Stille-seyn/ ", "tokens": ["Macht", "uns", "doch", "Stil\u00b7le\u00b7seyn", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "NN", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.4": {"line.1": {"text": "Mag Ungedult und Sturm", "tokens": ["Mag", "Un\u00b7ge\u00b7dult", "und", "Sturm"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Auch einen blossen Winck des H\u00f6chsten widerfechten?", "tokens": ["Auch", "ei\u00b7nen", "blos\u00b7sen", "Win\u00b7ck", "des", "H\u00f6chs\u00b7ten", "wi\u00b7der\u00b7fech\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Ach Mensch! du S\u00fcnden-Wurm/", "tokens": ["Ach", "Mensch", "!", "du", "S\u00fcn\u00b7den\u00b7Wurm", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "NN", "$.", "PPER", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wilstu dich unterstehn mit deinem GOtt zu rechten?", "tokens": ["Wils\u00b7tu", "dich", "un\u00b7ter\u00b7stehn", "mit", "dei\u00b7nem", "Gott", "zu", "rech\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wirff Frevel weg/ erkenne deine Schuld/", "tokens": ["Wirff", "Fre\u00b7vel", "weg", "/", "er\u00b7ken\u00b7ne", "dei\u00b7ne", "Schuld", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "PTKVZ", "$(", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und schm\u00fccke deinen Lauff durch ", "tokens": ["Und", "schm\u00fc\u00b7cke", "dei\u00b7nen", "Lauff", "durch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "APPR"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Dr\u00fcckt GOtt/ so b\u00fccke dich/", "tokens": ["Dr\u00fcckt", "Gott", "/", "so", "b\u00fc\u00b7cke", "dich", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$(", "ADV", "VVFIN", "PPER", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Tritt er/ so strecke dich zu seinen Gnaden-F\u00fcssen/", "tokens": ["Tritt", "er", "/", "so", "stre\u00b7cke", "dich", "zu", "sei\u00b7nen", "Gna\u00b7den\u00b7F\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$(", "ADV", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Such ihn/ verbirgt er sich/", "tokens": ["Such", "ihn", "/", "ver\u00b7birgt", "er", "sich", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "$(", "VVFIN", "PPER", "PRF", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Schl\u00e4gt er/ la\u00df nur nicht ab die Gnaden-Hand zu k\u00fcssen.", "tokens": ["Schl\u00e4gt", "er", "/", "la\u00df", "nur", "nicht", "ab", "die", "Gna\u00b7den\u00b7Hand", "zu", "k\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$(", "VVIMP", "ADV", "PTKNEG", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "Nimmt er/ giebs hin/ und wenn er dir nichts l\u00e4st/", "tokens": ["Nimmt", "er", "/", "giebs", "hin", "/", "und", "wenn", "er", "dir", "nichts", "l\u00e4st", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$(", "VVFIN", "PTKVZ", "$(", "KON", "KOUS", "PPER", "PPER", "PIS", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "So halt den theuren Schatz ", "tokens": ["So", "halt", "den", "theu\u00b7ren", "Schatz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Lauff aus dem Bade nicht/", "tokens": ["Lauff", "aus", "dem", "Ba\u00b7de", "nicht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PTKNEG", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Will schon das Wasser schier bi\u00df an die Seele dringen/", "tokens": ["Will", "schon", "das", "Was\u00b7ser", "schier", "bi\u00df", "an", "die", "See\u00b7le", "drin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "ADJD", "ADV", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Thr\u00e4nt gleich dein Angesicht/", "tokens": ["Thr\u00e4nt", "gleich", "dein", "An\u00b7ge\u00b7sicht", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PPOSAT", "NN", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Du must ja deinem GOtt was abzuwischen bringen:", "tokens": ["Du", "must", "ja", "dei\u00b7nem", "Gott", "was", "ab\u00b7zu\u00b7wi\u00b7schen", "brin\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PPOSAT", "NN", "PWS", "VVIZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Di\u00df freuet ihn/ er ist recht treu geneigt", "tokens": ["Di\u00df", "freu\u00b7et", "ihn", "/", "er", "ist", "recht", "treu", "ge\u00b7neigt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "$(", "PPER", "VAFIN", "ADJD", "ADJD", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "F\u00fcr die/ bey welchen sich ", "tokens": ["F\u00fcr", "die", "/", "bey", "wel\u00b7chen", "sich"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "$(", "APPR", "PWAT", "PRF"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Wie wohl ist es gemeynt/", "tokens": ["Wie", "wohl", "ist", "es", "ge\u00b7meynt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VAFIN", "PPER", "VVPP", "$("], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Wenn er durch Ungemach uns beuget das Gem\u00fctte!", "tokens": ["Wenn", "er", "durch", "Un\u00b7ge\u00b7mach", "uns", "beu\u00b7get", "das", "Ge\u00b7m\u00fct\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ob es zwar bitter scheint/", "tokens": ["Ob", "es", "zwar", "bit\u00b7ter", "scheint", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "So schmeckt man doch darbey die holde JEsus-G\u00fctte.", "tokens": ["So", "schmeckt", "man", "doch", "dar\u00b7bey", "die", "hol\u00b7de", "Je\u00b7sus\u00b7G\u00fct\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "PAV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Sein Joch ist s\u00fc\u00df/ und seine Last ist leicht/", "tokens": ["Sein", "Joch", "ist", "s\u00fc\u00df", "/", "und", "sei\u00b7ne", "Last", "ist", "leicht", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$(", "KON", "PPOSAT", "NN", "VAFIN", "ADJD", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Wo nicht das edle Paar/ ", "tokens": ["Wo", "nicht", "das", "ed\u00b7le", "Paar", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PTKNEG", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Bleibt meine Schwestern/ bleibt/", "tokens": ["Bleibt", "mei\u00b7ne", "Schwes\u00b7tern", "/", "bleibt", "/"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$(", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Ihr Glaubens-T\u00f6chter/ dort im Paradie\u00df gebohren/", "tokens": ["Ihr", "Glau\u00b7bens\u00b7T\u00f6ch\u00b7ter", "/", "dort", "im", "Pa\u00b7ra\u00b7die\u00df", "ge\u00b7boh\u00b7ren", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "ADV", "APPRART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nichts/ nichts von euch mich treibt/", "tokens": ["Nichts", "/", "nichts", "von", "euch", "mich", "treibt", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$(", "PIS", "APPR", "PPER", "PPER", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ich hab auff euren Bund mit reiner Treu geschworen.", "tokens": ["Ich", "hab", "auff", "eu\u00b7ren", "Bund", "mit", "rei\u00b7ner", "Treu", "ge\u00b7schwo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ein andrer seh/ wornach er eyfrig strebt/", "tokens": ["Ein", "an\u00b7drer", "seh", "/", "wor\u00b7nach", "er", "ey\u00b7frig", "strebt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "$(", "PWAV", "PPER", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Genung/ wenn nur in mir ", "tokens": ["Ge\u00b7nung", "/", "wenn", "nur", "in", "mir"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$(", "KOUS", "ADV", "APPR", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "So lebt denn allezeit/", "tokens": ["So", "lebt", "denn", "al\u00b7le\u00b7zeit", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Es soll/ wolt ihr mich stets bi\u00df an mein Ende f\u00fchren/", "tokens": ["Es", "soll", "/", "wolt", "ihr", "mich", "stets", "bi\u00df", "an", "mein", "En\u00b7de", "f\u00fch\u00b7ren", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "wor