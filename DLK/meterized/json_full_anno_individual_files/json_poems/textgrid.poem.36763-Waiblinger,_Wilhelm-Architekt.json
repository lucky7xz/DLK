{"textgrid.poem.36763": {"metadata": {"author": {"name": "Waiblinger, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Architekt", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das ist schlimm, wie die Architektur so schrecklich gefallen,", "tokens": ["Das", "ist", "schlimm", ",", "wie", "die", "Ar\u00b7chi\u00b7tek\u00b7tur", "so", "schreck\u00b7lich", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADJD", "$,", "PWAV", "ART", "NN", "ADV", "ADJD", "VVPP", "$,"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Darum bin ich nach Rom, bess'res zu lernen, gereist.", "tokens": ["Da\u00b7rum", "bin", "ich", "nach", "Rom", ",", "bess'\u00b7res", "zu", "ler\u00b7nen", ",", "ge\u00b7reist", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "APPR", "NE", "$,", "ADJA", "PTKZU", "VVINF", "$,", "VVPP", "$."], "meter": "--+---+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Jahrlang hab' ich daselbst das Pantheon und den Farnese,", "tokens": ["Jahr\u00b7lang", "hab'", "ich", "da\u00b7selbst", "das", "Pan\u00b7the\u00b7on", "und", "den", "Far\u00b7ne\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "PAV", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+--+-+", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "Tempel, Basiliken und alle Pal\u00e4ste studirt.", "tokens": ["Tem\u00b7pel", ",", "Ba\u00b7si\u00b7li\u00b7ken", "und", "al\u00b7le", "Pa\u00b7l\u00e4s\u00b7te", "stu\u00b7dirt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos"