{"dta.poem.7242": {"metadata": {"author": {"name": "Zesen, Philipp von", "birth": "N.A.", "death": "N.A."}, "title": "Die f\u00fcnffte Abtheilung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1640", "urn": "urn:nbn:de:kobv:b4-20685-2", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u2014 \u2014 \u2014 \u2014 \u2014 \u2014 \u2014 \u2014 \u2014 1. Dieselbe war ein bild/", "tokens": ["1.", "Die\u00b7sel\u00b7be", "war", "ein", "bild", "/"], "token_info": ["punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "ordinal", "word", "word", "word", "word", "punct"], "pos": ["$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "ADJA", "NN", "VAFIN", "ART", "NN", "$("]}, "line.2": {"text": "da Tugend einlosiert vnd sch\u00f6nheit f\u00fchrt das schild/", "tokens": ["da", "Tu\u00b7gend", "ein\u00b7lo\u00b7siert", "vnd", "sch\u00f6n\u00b7heit", "f\u00fchrt", "das", "schild", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVPP", "KON", "NN", "VVFIN", "ART", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "der Mund war rother samt: 2. die Lippen ausge\u00e4tzet", "tokens": ["der", "Mund", "war", "ro\u00b7ther", "samt", ":", "2.", "die", "Lip\u00b7pen", "aus\u00b7ge\u00e4t\u00b7zet"], "token_info": ["word", "word", "word", "word", "word", "punct", "ordinal", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADJD", "VVFIN", "$.", "ADV", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "mit R\u00f6slein vnd Rubin; mit liljen vntersetzet/", "tokens": ["mit", "R\u00f6s\u00b7lein", "vnd", "Ru\u00b7bin", ";", "mit", "lil\u00b7jen", "vn\u00b7ter\u00b7set\u00b7zet", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NE", "$.", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Narcissen wei\u00df der hals: 3. die finger waren schnee/", "tokens": ["Nar\u00b7cis\u00b7sen", "wei\u00df", "der", "hals", ":", "3.", "die", "fin\u00b7ger", "wa\u00b7ren", "schnee", "/"], "token_info": ["word", "word", "word", "word", "punct", "ordinal", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$.", "ADV", "ART", "ADJA", "VAFIN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "die N\u00e4gel Perlen-gleich/ 4. das haar wie gold v. klee", "tokens": ["die", "N\u00e4\u00b7gel", "Per\u00b7len\u00b7gleich", "/", "4.", "das", "haar", "wie", "gold", "v.", "klee"], "token_info": ["word", "word", "word", "punct", "ordinal", "word", "word", "word", "word", "abbreviation", "word"], "pos": ["ART", "NN", "NE", "$(", "ADV", "ART", "NN", "KOKOM", "NN", "APPRART", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.7": {"text": "wenn er zum gelbsten ist: 5. sie zog mit lust zur\u00fccke", "tokens": ["wenn", "er", "zum", "gelbs\u00b7ten", "ist", ":", "5.", "sie", "zog", "mit", "lust", "zu\u00b7r\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "punct", "ordinal", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "ADJA", "VAFIN", "$.", "ADJA", "PPER", "VVFIN", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "den \u00fcberbliebnen sinn durch jhrer augen blicke/", "tokens": ["den", "\u00fc\u00b7berb\u00b7lieb\u00b7nen", "sinn", "durch", "jhrer", "au\u00b7gen", "bli\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "wie der Magnet das stahl: \u2014 \u2014 \u2014 \u2014 \u2014 \u2014 \u2014 \u2014", "tokens": ["wie", "der", "Mag\u00b7net", "das", "stahl", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "$.", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$("], "meter": "+-+--+", "measure": "iambic.tri.chol"}}}}}