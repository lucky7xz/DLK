{"dta.poem.3845": {"metadata": {"author": {"name": "Spindler, Christian Gotthold", "birth": "N.A.", "death": "N.A."}, "title": "Auf einen J\u00fcngling.  \n  J. G. Heckerling.  1738.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-20581-9", "language": ["de:0.99"], "booktitle": "Spindler, Christian Gotthold: Unschuldige Jugend-Fr\u00fcchte. Leipzig, 1745."}, "poem": {"stanza.1": {"line.1": {"text": "Hier liegt ein Bildni\u00df zarter Jugend,", "tokens": ["Hier", "liegt", "ein", "Bild\u00b7ni\u00df", "zar\u00b7ter", "Ju\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Hier liegt ein Muster frommer Tugend,", "tokens": ["Hier", "liegt", "ein", "Mus\u00b7ter", "from\u00b7mer", "Tu\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "An Jahren jung, alt an Verstand.", "tokens": ["An", "Jah\u00b7ren", "jung", ",", "alt", "an", "Ver\u00b7stand", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "$,", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er sucht das rechte Vaterland,", "tokens": ["Er", "sucht", "das", "rech\u00b7te", "Va\u00b7ter\u00b7land", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So ewiges Vergn\u00fcgen bringet/", "tokens": ["So", "e\u00b7wi\u00b7ges", "Ver\u00b7gn\u00fc\u00b7gen", "brin\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Allwo er Halleluja singet.", "tokens": ["All\u00b7wo", "er", "Hal\u00b7le\u00b7lu\u00b7ja", "sin\u00b7get", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und da ihm nun so wohl geschicht,", "tokens": ["Und", "da", "ihm", "nun", "so", "wohl", "ge\u00b7schicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Betr\u00fcbte Eltern, weinet nicht.", "tokens": ["Be\u00b7tr\u00fcb\u00b7te", "El\u00b7tern", ",", "wei\u00b7net", "nicht", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}