{"textgrid.poem.65235": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Meine Muse", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Meine Mus' ist gegangen", "tokens": ["Mei\u00b7ne", "Mus'", "ist", "ge\u00b7gan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "In des Schenken sein Haus,", "tokens": ["In", "des", "Schen\u00b7ken", "sein", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Hat die Sch\u00fcrz' umgebunden", "tokens": ["Hat", "die", "Sch\u00fcrz'", "um\u00b7ge\u00b7bun\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "VVPP"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Und will nicht heraus;", "tokens": ["Und", "will", "nicht", "he\u00b7raus", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PTKNEG", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Will Kellnerin werden,", "tokens": ["Will", "Kell\u00b7ne\u00b7rin", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "VAINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Will schenken den Wein:", "tokens": ["Will", "schen\u00b7ken", "den", "Wein", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Da steht sie am Thore,", "tokens": ["Da", "steht", "sie", "am", "Tho\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Und winkt mir herein.", "tokens": ["Und", "winkt", "mir", "her\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Und \u00fcber ihrem Haupte", "tokens": ["Und", "\u00fc\u00b7ber", "ih\u00b7rem", "Haup\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da spielet die Luft", "tokens": ["Da", "spie\u00b7let", "die", "Luft"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Mit gr\u00fcnenden Zweigen", "tokens": ["Mit", "gr\u00fc\u00b7nen\u00b7den", "Zwei\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Und w\u00fcrzigem Duft.", "tokens": ["Und", "w\u00fcr\u00b7zi\u00b7gem", "Duft", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Seht, wie sie sich drehet", "tokens": ["Seht", ",", "wie", "sie", "sich", "dre\u00b7het"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "PWAV", "PPER", "PRF", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "So flink, so gewandt,", "tokens": ["So", "flink", ",", "so", "ge\u00b7wandt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$,", "ADV", "VVPP", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Die Kann' unter'm Arme,", "tokens": ["Die", "Kann'", "un\u00b7ter'm", "Ar\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Das Glas in der Hand!", "tokens": ["Das", "Glas", "in", "der", "Hand", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "\u00bbherein, lieber Zecher!", "tokens": ["\u00bb", "her\u00b7ein", ",", "lie\u00b7ber", "Ze\u00b7cher", "!"], "token_info": ["punct", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PTKVZ", "$,", "ADV", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Ich schenke dir Wein,", "tokens": ["Ich", "schen\u00b7ke", "dir", "Wein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Ich schenke dir Lieder", "tokens": ["Ich", "schen\u00b7ke", "dir", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Noch obendarein.", "tokens": ["Noch", "o\u00b7ben\u00b7da\u00b7rein", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADV", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Nur mu\u00dft du h\u00fcbsch bleiben", "tokens": ["Nur", "mu\u00dft", "du", "h\u00fcbsch", "blei\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Im Wirthshaus bei mir:", "tokens": ["Im", "Wirths\u00b7haus", "bei", "mir", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "PPER", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Ich gebe freie Zeche", "tokens": ["Ich", "ge\u00b7be", "frei\u00b7e", "Ze\u00b7che"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und freies Quartier.", "tokens": ["Und", "frei\u00b7es", "Quar\u00b7tier", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Drum locke mich nimmer", "tokens": ["Drum", "lo\u00b7cke", "mich", "nim\u00b7mer"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Hinaus in den Hain", "tokens": ["Hin\u00b7aus", "in", "den", "Hain"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN"], "meter": "--+-+", "measure": "anapaest.init"}, "line.3": {"text": "Zu einsamen Klagen", "tokens": ["Zu", "ein\u00b7sa\u00b7men", "Kla\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Ob sehnlicher Pein.", "tokens": ["Ob", "sehn\u00b7li\u00b7cher", "Pein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Hier unter den Zweigen", "tokens": ["Hier", "un\u00b7ter", "den", "Zwei\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Vor unserem Haus", "tokens": ["Vor", "un\u00b7se\u00b7rem", "Haus"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Da schlafen die Leiden", "tokens": ["Da", "schla\u00b7fen", "die", "Lei\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Gar lustig sich aus.", "tokens": ["Gar", "lus\u00b7tig", "sich", "aus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PRF", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.5": {"line.1": {"text": "Auch la\u00df uns nicht schweifen", "tokens": ["Auch", "la\u00df", "uns", "nicht", "schwei\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVIMP", "PPER", "PTKNEG", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Umher in der Welt,", "tokens": ["Um\u00b7her", "in", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Einen Helden zu suchen,", "tokens": ["Ei\u00b7nen", "Hel\u00b7den", "zu", "su\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Der Allen gef\u00e4llt.", "tokens": ["Der", "Al\u00b7len", "ge\u00b7f\u00e4llt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Gar lang sind die Wege,", "tokens": ["Gar", "lang", "sind", "die", "We\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Gar kurz ist die Zeit,", "tokens": ["Gar", "kurz", "ist", "die", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Und auf den Karpathen", "tokens": ["Und", "auf", "den", "Kar\u00b7pa\u00b7then"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN"], "meter": "-+----", "measure": "dactylic.init"}, "line.8": {"text": "Sind die Stra\u00dfen verschneit.\u00ab", "tokens": ["Sind", "die", "Stra\u00b7\u00dfen", "ver\u00b7schneit", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "ART", "NN", "VVPP", "$.", "$("], "meter": "--+--+", "measure": "anapaest.di.plus"}}, "stanza.6": {"line.1": {"text": "So lie\u00df sie sich h\u00f6ren \u2013", "tokens": ["So", "lie\u00df", "sie", "sich", "h\u00f6\u00b7ren", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "VVINF", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Wer hielte das aus?", "tokens": ["Wer", "hiel\u00b7te", "das", "aus", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PDS", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Flugs bin ich gesprungen", "tokens": ["Flugs", "bin", "ich", "ge\u00b7sprun\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "VVPP"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Ihr nach in das Haus.", "tokens": ["Ihr", "nach", "in", "das", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "APPR", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Nun schenke mir Lieder", "tokens": ["Nun", "schen\u00b7ke", "mir", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Und schenke mir Wein,", "tokens": ["Und", "schen\u00b7ke", "mir", "Wein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON",