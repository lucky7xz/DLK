{"dta.poem.13193": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Z ehndes  B uch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201ewar deiner V\u00f6lcker Heer zum Fechten angeleitet;", "tokens": ["\u201e", "war", "dei\u00b7ner", "V\u00f6l\u00b7cker", "Heer", "zum", "Fech\u00b7ten", "an\u00b7ge\u00b7lei\u00b7tet", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "PPOSAT", "NN", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "\u201eso glaubtest du, da\u00df GOtt den Weeg zum Sieg bereitet.", "tokens": ["\u201e", "so", "glaub\u00b7test", "du", ",", "da\u00df", "Gott", "den", "Weeg", "zum", "Sieg", "be\u00b7rei\u00b7tet", "."], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "$,", "KOUS", "NN", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "125\u201dLag es in Festungen; so ward von dir betracht:", "tokens": ["\"", "Lag", "es", "in", "Fes\u00b7tun\u00b7gen", ";", "so", "ward", "von", "dir", "be\u00b7tracht", ":"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "PPER", "APPR", "NN", "$.", "ADV", "VAFIN", "APPR", "PPER", "VVPP", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.4": {"text": "\u201eda\u00df Wall und Schlo\u00df umsonst, wo GOtt nicht selber wacht.", "tokens": ["\u201e", "da\u00df", "Wall", "und", "Schlo\u00df", "um\u00b7sonst", ",", "wo", "Gott", "nicht", "sel\u00b7ber", "wacht", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "NE", "KON", "NN", "ADV", "$,", "PWAV", "NN", "PTKNEG", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "\u201estund es im freyen Feld, zu schwach den Kampf zu wagen;", "tokens": ["\u201e", "stund", "es", "im", "frey\u00b7en", "Feld", ",", "zu", "schwach", "den", "Kampf", "zu", "wa\u00b7gen", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "PPER", "APPRART", "ADJA", "NN", "$,", "PTKA", "ADJD", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "---+-+-+-+-+-", "measure": "unknown.measure.penta"}, "line.6": {"text": "\u201eso sagtest du, da\u00df GOtt ihm werde Schanzen schlagen.", "tokens": ["\u201e", "so", "sag\u00b7test", "du", ",", "da\u00df", "Gott", "ihm", "wer\u00b7de", "Schan\u00b7zen", "schla\u00b7gen", "."], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "$,", "KOUS", "NN", "PPER", "VAFIN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "\u201ebefande sich dein Heer im blutigsten Gefecht;", "tokens": ["\u201e", "be\u00b7fan\u00b7de", "sich", "dein", "Heer", "im", "blu\u00b7tigs\u00b7ten", "Ge\u00b7fecht", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PRF", "PPOSAT", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "130\u201dSo wu\u00dftest du, da\u00df GOtt die Krieger st\u00e4rckt und schw\u00e4cht.", "tokens": ["\"", "So", "wu\u00df\u00b7test", "du", ",", "da\u00df", "Gott", "die", "Krie\u00b7ger", "st\u00e4rckt", "und", "schw\u00e4cht", "."], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "$,", "KOUS", "NN", "ART", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "\u201ewar deiner Feinde Macht gezwungen durchzugehen;", "tokens": ["\u201e", "war", "dei\u00b7ner", "Fein\u00b7de", "Macht", "ge\u00b7zwun\u00b7gen", "durch\u00b7zu\u00b7ge\u00b7hen", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "PPOSAT", "NN", "NN", "VVPP", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "\u201eso glaubtest du den Sieg von GOtt gekr\u00f6nt zu sehen.", "tokens": ["\u201e", "so", "glaub\u00b7test", "du", "den", "Sieg", "von", "Gott", "ge\u00b7kr\u00f6nt", "zu", "se\u00b7hen", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "ART", "NN", "APPR", "NN", "VVPP", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "\u201egelassen in dem Sieg und herzhaft in Gefahr,", "tokens": ["\u201e", "ge\u00b7las\u00b7sen", "in", "dem", "Sieg", "und", "herz\u00b7haft", "in", "Ge\u00b7fahr", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVPP", "APPR", "ART", "NN", "KON", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "\u201ebescheiden in dem Gl\u00fcck, ist was dir eigen war.", "tokens": ["\u201e", "be\u00b7schei\u00b7den", "in", "dem", "Gl\u00fcck", ",", "ist", "was", "dir", "ei\u00b7gen", "war", "."], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "APPR", "ART", "NN", "$,", "VAFIN", "PWS", "PPER", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "135\u201dKlug, standhaft, unerschreckt, wo man von Ungl\u00fcck h\u00f6rte;", "tokens": ["\"", "Klug", ",", "stand\u00b7haft", ",", "un\u00b7er\u00b7schreckt", ",", "wo", "man", "von", "Un\u00b7gl\u00fcck", "h\u00f6r\u00b7te", ";"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "NE", "$,", "ADJD", "$,", "ADJD", "$,", "PWAV", "PIS", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "\u201egleichm\u00fcthig und beherzt, was immer sich emp\u00f6rte.", "tokens": ["\u201e", "gleich\u00b7m\u00fct\u00b7hig", "und", "be\u00b7herzt", ",", "was", "im\u00b7mer", "sich", "em\u00b7p\u00f6r\u00b7te", "."], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "KON", "ADJD", "$,", "PRELS", "ADV", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "\u201eein jeder Umstand wies, da\u00df du auf GOtt gebaut,", "tokens": ["\u201e", "ein", "je\u00b7der", "Um\u00b7stand", "wies", ",", "da\u00df", "du", "auf", "Gott", "ge\u00b7baut", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "PIAT", "NN", "VVFIN", "$,", "KOUS", "PPER", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "\u201eje mehr der Feinde Muth der eignen Macht vertraut.", "tokens": ["\u201e", "je", "mehr", "der", "Fein\u00b7de", "Muth", "der", "eig\u00b7nen", "Macht", "ver\u00b7traut", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADV", "ART", "NN", "NN", "ART", "ADJA", "NN", "ADJD", "$."], "meter": "---+-+-+-+-+", "measure": "unknown.measure.penta"}, "line.17": {"text": "\u201eso vieler Tugenden Werth und Vortrefflichkeiten", "tokens": ["\u201e", "so", "vie\u00b7ler", "Tu\u00b7gen\u00b7den", "Werth", "und", "Vor\u00b7treff\u00b7lich\u00b7kei\u00b7ten"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "ADV", "PIAT", "NN", "NN", "KON", "NN"], "meter": "-+-+--+-++-+-", "measure": "iambic.hexa.relaxed"}, "line.18": {"text": "140\u201dSeynd Ursach, da\u00df sie nun um Ehr und Vorrang streiten.", "tokens": ["\"", "Seynd", "Ur\u00b7sach", ",", "da\u00df", "sie", "nun", "um", "Ehr", "und", "Vor\u00b7rang", "strei\u00b7ten", "."], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "NE", "NE", "$,", "KOUS", "PPER", "ADV", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}