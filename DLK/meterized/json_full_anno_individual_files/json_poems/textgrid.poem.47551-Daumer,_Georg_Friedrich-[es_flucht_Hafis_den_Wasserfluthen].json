{"textgrid.poem.47551": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[es flucht Hafis den Wasserfluthen]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es flucht Hafis den Wasserfluthen;", "tokens": ["Es", "flucht", "Ha\u00b7fis", "den", "Was\u00b7ser\u00b7flut\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Er trieft von Lippenhonigseim;", "tokens": ["Er", "trieft", "von", "Lip\u00b7pen\u00b7ho\u00b7ni\u00b7gseim", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er flammt in eitel Minnegluthen,", "tokens": ["Er", "flammt", "in", "ei\u00b7tel", "Min\u00b7ne\u00b7glu\u00b7then", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und wenn er spricht, so ist's ein Reim;", "tokens": ["Und", "wenn", "er", "spricht", ",", "so", "ist's", "ein", "Reim", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "$,", "ADV", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Er lacht der Zucht und ihrer Ruthen;", "tokens": ["Er", "lacht", "der", "Zucht", "und", "ih\u00b7rer", "Ru\u00b7then", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Er ist so tr\u00e4g zu allem Guten;", "tokens": ["Er", "ist", "so", "tr\u00e4g", "zu", "al\u00b7lem", "Gu\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "APPR", "PIS", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Erstickt ist aller edle Keim \u2013", "tokens": ["Er\u00b7stickt", "ist", "al\u00b7ler", "ed\u00b7le", "Keim", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PIAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "So kommt man aus der Schenke heim.", "tokens": ["So", "kommt", "man", "aus", "der", "Schen\u00b7ke", "heim", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Es flucht Hafis den Wasserfluthen;", "tokens": ["Es", "flucht", "Ha\u00b7fis", "den", "Was\u00b7ser\u00b7flut\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Er trieft von Lippenhonigseim;", "tokens": ["Er", "trieft", "von", "Lip\u00b7pen\u00b7ho\u00b7ni\u00b7gseim", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er flammt in eitel Minnegluthen,", "tokens": ["Er", "flammt", "in", "ei\u00b7tel", "Min\u00b7ne\u00b7glu\u00b7then", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und wenn er spricht, so ist's ein Reim;", "tokens": ["Und", "wenn", "er", "spricht", ",", "so", "ist's", "ein", "Reim