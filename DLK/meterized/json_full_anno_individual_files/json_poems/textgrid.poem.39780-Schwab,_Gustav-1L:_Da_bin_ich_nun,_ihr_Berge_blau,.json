{"textgrid.poem.39780": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "1L: Da bin ich nun, ihr Berge blau,", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.85", "cy:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da bin ich nun, ihr Berge blau,", "tokens": ["Da", "bin", "ich", "nun", ",", "ihr", "Ber\u00b7ge", "blau", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "$,", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du fremdes Dorf, du neues Gau!", "tokens": ["Du", "frem\u00b7des", "Dorf", ",", "du", "neu\u00b7es", "Gau", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich hab' das Alte gar vergessen,", "tokens": ["Ich", "hab'", "das", "Al\u00b7te", "gar", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mir ist, als h\u00e4tt' ich's nie besessen.", "tokens": ["Mir", "ist", ",", "als", "h\u00e4tt'", "ich's", "nie", "be\u00b7ses\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "KOKOM", "VAFIN", "PIS", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Empfangt mich freundlich, Wald und Feld,", "tokens": ["Emp\u00b7fangt", "mich", "freund\u00b7lich", ",", "Wald", "und", "Feld", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Als k\u00e4m' ich eben erst zur Welt.", "tokens": ["Als", "k\u00e4m'", "ich", "e\u00b7ben", "erst", "zur", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "ADV", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Tief hinter mir in Duft und Schaum", "tokens": ["Tief", "hin\u00b7ter", "mir", "in", "Duft", "und", "Schaum"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "PPER", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Liegt die vergangne Zeit als Traum;", "tokens": ["Liegt", "die", "ver\u00b7gang\u00b7ne", "Zeit", "als", "Traum", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "KOUS", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Der Sommer f\u00e4llt mir kaum noch ein", "tokens": ["Der", "Som\u00b7mer", "f\u00e4llt", "mir", "kaum", "noch", "ein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "ADV", "ART"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Mit seinem lieben Sonnenschein:", "tokens": ["Mit", "sei\u00b7nem", "lie\u00b7ben", "Son\u00b7nen\u00b7schein", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Damit ich ihn nicht sollt' vermissen,", "tokens": ["Da\u00b7mit", "ich", "ihn", "nicht", "sollt'", "ver\u00b7mis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PTKNEG", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Verschied er unter Regeng\u00fcssen;", "tokens": ["Ver\u00b7schied", "er", "un\u00b7ter", "Re\u00b7gen\u00b7g\u00fcs\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Der Herbstwind nahte mit Gebraus,", "tokens": ["Der", "Herbst\u00b7wind", "nah\u00b7te", "mit", "Ge\u00b7braus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Und l\u00f6schte rings die Blumen aus;", "tokens": ["Und", "l\u00f6schte", "rings", "die", "Blu\u00b7men", "aus", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.15": {"text": "Auf weht' er selber mir die Th\u00fcr,", "tokens": ["Auf", "weht'", "er", "sel\u00b7ber", "mir", "die", "Th\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "PPER", "ADV", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Und ging die staub'ge Bahn mir f\u00fcr.", "tokens": ["Und", "ging", "die", "staub'\u00b7ge", "Bahn", "mir", "f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "PPER", "APPR", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Ich lie\u00df die H\u00fctte ruhig stehen,", "tokens": ["Ich", "lie\u00df", "die", "H\u00fct\u00b7te", "ru\u00b7hig", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Ich habe mich nicht umgesehen;", "tokens": ["Ich", "ha\u00b7be", "mich", "nicht", "um\u00b7ge\u00b7se\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Und da\u00df es mich nicht sollte reu'n,", "tokens": ["Und", "da\u00df", "es", "mich", "nicht", "soll\u00b7te", "reu'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PRF", "PTKNEG", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Schlug hinter mir der Blitz darein.", "tokens": ["Schlug", "hin\u00b7ter", "mir", "der", "Blitz", "da\u00b7rein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "ART", "NN", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Mein Liebchen auch mit Vorbedacht", "tokens": ["Mein", "Lieb\u00b7chen", "auch", "mit", "Vor\u00b7be\u00b7dacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADV", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Hat mir das Scheiden leicht gemacht;", "tokens": ["Hat", "mir", "das", "Schei\u00b7den", "leicht", "ge\u00b7macht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Um allen Gram mir zu ersparen,", "tokens": ["Um", "al\u00b7len", "Gram", "mir", "zu", "er\u00b7spa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PIAT", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Weil ich nun sollte von ihr fahren,", "tokens": ["Weil", "ich", "nun", "soll\u00b7te", "von", "ihr", "fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VMFIN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Da\u00df ich mich k\u00f6nnt' so leichter fassen,", "tokens": ["Da\u00df", "ich", "mich", "k\u00f6nnt'", "so", "leich\u00b7ter", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VMFIN", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Hat sie mich selbst zuvor verlassen.", "tokens": ["Hat", "sie", "mich", "selbst", "zu\u00b7vor", "ver\u00b7las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PRF", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "So sei es! hier im frischen Schnee", "tokens": ["So", "sei", "es", "!", "hier", "im", "fri\u00b7schen", "Schnee"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "$.", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vergrab' ich all mein altes Weh,", "tokens": ["Ver\u00b7grab'", "ich", "all", "mein", "al\u00b7tes", "Weh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und will es wieder Fr\u00fchling werden,", "tokens": ["Und", "will", "es", "wie\u00b7der", "Fr\u00fch\u00b7ling", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So keimt auch mir Lust aus der Erden;", "tokens": ["So", "keimt", "auch", "mir", "Lust", "aus", "der", "Er\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPER", "NN", "APPR", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Ein neues H\u00fcttlein l\u00e4\u00dft sich baun,", "tokens": ["Ein", "neu\u00b7es", "H\u00fctt\u00b7lein", "l\u00e4\u00dft", "sich", "baun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein neuer Sommer l\u00e4\u00dft sich schaun,", "tokens": ["Ein", "neu\u00b7er", "Som\u00b7mer", "l\u00e4\u00dft", "sich", "schaun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und unter euch, ihr fremden Kinder,", "tokens": ["Und", "un\u00b7ter", "euch", ",", "ihr", "frem\u00b7den", "Kin\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ein neues Sch\u00e4tzlein auch nicht minder!", "tokens": ["Ein", "neu\u00b7es", "Sch\u00e4tz\u00b7lein", "auch", "nicht", "min\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Da bin ich nun, ihr Berge blau,", "tokens": ["Da", "bin", "ich", "nun", ",", "ihr", "Ber\u00b7ge", "blau", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "$,", "PPOSAT", "NN", "ADJD",