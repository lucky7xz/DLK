c.tetra"}, "line.20": {"text": "Schlug hinter mir der Blitz darein.", "tokens": ["Schlug", "hin\u00b7ter", "mir", "der", "Blitz", "da\u00b7rein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "ART", "NN", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Mein Liebchen auch mit Vorbedacht", "tokens": ["Mein", "Lieb\u00b7chen", "auch", "mit", "Vor\u00b7be\u00b7dacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADV", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Hat mir das Scheiden leicht gemacht;", "tokens": ["Hat", "mir", "das", "Schei\u00b7den", "leicht", "ge\u00b7macht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Um allen Gram mir zu ersparen,", "tokens": ["Um", "al\u00b7len", "Gram", "mir", "zu", "er\u00b7spa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PIAT", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Weil ich nun sollte von ihr fahren,", "tokens": ["Weil", "ich", "nun", "soll\u00b7te", "von", "ihr", "fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VMFIN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Da\u00df ich mich k\u00f6nnt' so leichter fassen,", "tokens": ["Da\u00df", "ich", "mich", "k\u00f6nnt'", "so", "leich\u00b7ter", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VMFIN", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Hat sie mich selbst zuvor verlassen.", "tokens": ["Hat", "sie", "mich", "selbst", "zu\u00b7vor", "ver\u00b7las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PRF", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "So sei es! hier im frischen Schnee", "tokens": ["So", "sei", "es", "!", "hier", "im", "fri\u00b7schen", "Schnee"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "$.", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vergrab' ich all mein altes Weh,", "tokens": ["Ver\u00b7grab'", "ich", "all", "mein", "al\u00b7tes", "Weh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und will es wieder Fr\u00fchling werden,", "tokens": ["Und", "will", "es", "wie\u00b7der", "Fr\u00fch\u00b7ling", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So keimt auch mir Lust aus der Erden;", "tokens": ["So", "keimt", "auch", "mir", "Lust", "aus", "der", "Er\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPER", "NN", "APPR", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Ein neues H\u00fcttlein l\u00e4\u00dft sich baun,", "tokens": ["Ein", "neu\u00b7es", "H\u00fctt\u00b7lein", "l\u00e4\u00dft", "sich", "baun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein neuer Sommer l\u00e4\u00dft sich schaun,", "tokens": ["Ein", "neu\u00b7er", "Som\u00b7mer", "l\u00e4\u00dft", "sich", "schaun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und unter euch, ihr fremden Kinder,", "tokens": ["Und", "un\u00b7ter", "euch", ",", "ihr", "frem\u00b7den", "Kin\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ein neues Sch\u00e4tzlein auch nicht minder!", "tokens": ["Ein", "neu\u00b7es", "Sch\u00e4tz\u00b7lein", "auch", "nicht", "min\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}