{"dta.poem.15809": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "14.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df, der im Weg uns stand, zur Seite sei geschoben", "tokens": ["Da\u00df", ",", "der", "im", "Weg", "uns", "stand", ",", "zur", "Sei\u00b7te", "sei", "ge\u00b7scho\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "$,", "PRELS", "APPRART", "NN", "PPER", "VVFIN", "$,", "APPRART", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit Anstand, sei er dort hoch aufs Gestell erhoben.", "tokens": ["Mit", "An\u00b7stand", ",", "sei", "er", "dort", "hoch", "aufs", "Ge\u00b7stell", "er\u00b7ho\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "VAFIN", "PPER", "ADV", "ADJD", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Ihn nieder in den Koth zu werfen, ist nicht noth;", "tokens": ["Ihn", "nie\u00b7der", "in", "den", "Koth", "zu", "wer\u00b7fen", ",", "ist", "nicht", "noth", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "APPR", "ART", "NN", "PTKZU", "VVINF", "$,", "VAFIN", "PTKNEG", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er sei geehrt, und wir nichtmehr von Zwang bedroht.", "tokens": ["Er", "sei", "ge\u00b7ehrt", ",", "und", "wir", "nicht\u00b7mehr", "von", "Zwang", "be\u00b7droht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "w