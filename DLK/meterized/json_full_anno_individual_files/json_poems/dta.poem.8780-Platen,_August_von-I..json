{"dta.poem.8780": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "I.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Dein ew'ger Wunderspiegel sey die Welt;", "tokens": ["Dein", "ew'\u00b7ger", "Wun\u00b7der\u00b7spie\u00b7gel", "sey", "die", "Welt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Es schaut nach dir, wiewohl dich Keiner schaut,", "tokens": ["Es", "schaut", "nach", "dir", ",", "wie\u00b7wohl", "dich", "Kei\u00b7ner", "schaut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "$,", "KOUS", "PPER", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "In liebevoller Schw\u00e4rmerey die Welt;", "tokens": ["In", "lie\u00b7be\u00b7vol\u00b7ler", "Schw\u00e4r\u00b7me\u00b7rey", "die", "Welt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Du athmest Leben und du athmest aus", "tokens": ["Du", "ath\u00b7mest", "Le\u00b7ben", "und", "du", "ath\u00b7mest", "aus"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "KON", "PPER", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Mit jedem Athemzuge frey die Welt;", "tokens": ["Mit", "je\u00b7dem", "A\u00b7them\u00b7zu\u00b7ge", "frey", "die", "Welt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Du siehst dich selbst, und dir am Auge geht", "tokens": ["Du", "siehst", "dich", "selbst", ",", "und", "dir", "am", "Au\u00b7ge", "geht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "KON", "PPER", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "In jedem Augenblick vorbey die Welt;", "tokens": ["In", "je\u00b7dem", "Au\u00b7gen\u00b7blick", "vor\u00b7bey", "die", "Welt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Der einzig Eine bist du, doch du lenkst", "tokens": ["Der", "ein\u00b7zig", "Ei\u00b7ne", "bist", "du", ",", "doch", "du", "lenkst"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJD", "ART", "VAFIN", "PPER", "$,", "KON", "PPER", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Als eine mystischgro\u00dfe Drey die Welt.", "tokens": ["Als", "ei\u00b7ne", "mys\u00b7tischgro\u00b7\u00dfe", "Drey", "die", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}}}}