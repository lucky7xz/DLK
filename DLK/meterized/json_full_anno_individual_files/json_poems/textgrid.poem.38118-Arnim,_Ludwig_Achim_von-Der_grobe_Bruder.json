: ["\u00bb", "sieh", "an", "das", "hast", "du", "dei\u00b7ner", "Schwes\u00b7ter", "ge\u00b7than", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVIMP", "APPR", "PDS", "VAFIN", "PPER", "PPOSAT", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.26": {"line.1": {"text": "Er nahm sein Kind froh in den Arm:", "tokens": ["Er", "nahm", "sein", "Kind", "froh", "in", "den", "Arm", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbjezt hast keine Mutter mehr, da\u00df Gott erbarm!\u00ab", "tokens": ["\u00bb", "jezt", "hast", "kei\u00b7ne", "Mut\u00b7ter", "mehr", ",", "da\u00df", "Gott", "er\u00b7barm", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "VAFIN", "PIAT", "NN", "ADV", "$,", "KOUS", "NN", "ADJD", "$.", "$("], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.27": {"line.1": {"text": "Kuchlebu, Schifflebu fahren wohl \u00fcber den Rhein,", "tokens": ["Kuch\u00b7le\u00b7bu", ",", "Schif\u00b7fle\u00b7bu", "fah\u00b7ren", "wohl", "\u00fc\u00b7ber", "den", "Rhein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "VVFIN", "ADV", "APPR", "ART", "NE", "$,"], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.2": {"text": "Bey einem Markgrafen, da kehren sie ein.", "tokens": ["Bey", "ei\u00b7nem", "Mark\u00b7gra\u00b7fen", ",", "da", "keh\u00b7ren", "sie", "ein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ADV", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+---+--+", "measure": "iambic.tetra.chol"}}, "stanza.28": {"line.1": {"text": "\u00bbguten Morgen, junger Markgraf, guten Morgen,", "tokens": ["\u00bb", "gu\u00b7ten", "Mor\u00b7gen", ",", "jun\u00b7ger", "Mark\u00b7graf", ",", "gu\u00b7ten", "Mor\u00b7gen", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "ADJA", "NN", "$,", "ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Wo hast du dein adelich Schwesterlein verborgen?\u00ab", "tokens": ["Wo", "hast", "du", "dein", "a\u00b7de\u00b7lich", "Schwes\u00b7ter\u00b7lein", "ver\u00b7bor\u00b7gen", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "PPOSAT", "ADJD", "NN", "VVPP", "$.", "$("], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.29": {"line.1": {"text": "Was fragst du nach meinem adelichen Schwesterlein klein,", "tokens": ["Was", "fragst", "du", "nach", "mei\u00b7nem", "a\u00b7de\u00b7li\u00b7chen", "Schwes\u00b7ter\u00b7lein", "klein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "PPOSAT", "ADJA", "NN", "ADJD", "$,"], "meter": "-+--+-+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Es m\u00f6chte dir viel zu h\u00fcbsch und zu adelich seyn.", "tokens": ["Es", "m\u00f6ch\u00b7te", "dir", "viel", "zu", "h\u00fcbsch", "und", "zu", "a\u00b7de\u00b7lich", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "PTKA", "ADJD", "KON", "PTKA", "ADJD", "VAINF", "$."], "meter": "-+--+-+--+--+", "measure": "iambic.penta.relaxed"}}, "stanza.30": {"line.1": {"text": "\u00bbwarum m\u00f6cht es mir viel zu h\u00fcbsch und zu adelich seyn,", "tokens": ["\u00bb", "wa\u00b7rum", "m\u00f6cht", "es", "mir", "viel", "zu", "h\u00fcbsch", "und", "zu", "a\u00b7de\u00b7lich", "seyn", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "VMFIN", "PPER", "PPER", "ADV", "PTKA", "ADJD", "KON", "PTKA", "ADJD", "VAINF", "$,"], "meter": "--+--+-+--+--+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Es geht mit einem Kindelein klein.\u00ab", "tokens": ["Es", "geht", "mit", "ei\u00b7nem", "Kin\u00b7de\u00b7lein", "klein", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ADJD", "$.", "$("], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.31": {"line.1": {"text": "Geht es mit einem Kindelein klein,", "tokens": ["Geht", "es", "mit", "ei\u00b7nem", "Kin\u00b7de\u00b7lein", "klein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "So soll es auch nicht mehr mein Schwesterlein seyn.", "tokens": ["So", "soll", "es", "auch", "nicht", "mehr", "mein", "Schwes\u00b7ter\u00b7lein", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "PTKNEG", "ADV", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.32": {"line.1": {"text": "Er schickte sogleich Ro\u00df und Wagen,", "tokens": ["Er", "schick\u00b7te", "sog\u00b7leich", "Ro\u00df", "und", "Wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und lie\u00df sein adelichs Schwesterlein hertragen.", "tokens": ["Und", "lie\u00df", "sein", "a\u0