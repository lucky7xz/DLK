{"textgrid.poem.38118": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Der grobe Bruder", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kuchlebu, Schifflebu fahren wohl \u00fcber den Rhein,", "tokens": ["Kuch\u00b7le\u00b7bu", ",", "Schif\u00b7fle\u00b7bu", "fah\u00b7ren", "wohl", "\u00fc\u00b7ber", "den", "Rhein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "VVFIN", "ADV", "APPR", "ART", "NE", "$,"], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.2": {"text": "Bey einem Markgrafen, da kehren sie ein.", "tokens": ["Bey", "ei\u00b7nem", "Mark\u00b7gra\u00b7fen", ",", "da", "keh\u00b7ren", "sie", "ein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ADV", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+---+--+", "measure": "iambic.tetra.chol"}}, "stanza.2": {"line.1": {"text": "\u00bbguten Morgen, junger Markgraf, guten Morgen,", "tokens": ["\u00bb", "gu\u00b7ten", "Mor\u00b7gen", ",", "jun\u00b7ger", "Mark\u00b7graf", ",", "gu\u00b7ten", "Mor\u00b7gen", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "ADJA", "NN", "$,", "ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Wo hast du dein adelich Schwesterlein verborgen?\u00ab", "tokens": ["Wo", "hast", "du", "dein", "a\u00b7de\u00b7lich", "Schwes\u00b7ter\u00b7lein", "ver\u00b7bor\u00b7gen", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "PPOSAT", "ADJD", "NN", "VVPP", "$.", "$("], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.3": {"line.1": {"text": "Was fragst du nach meinem adelichen Schwesterlein klein,", "tokens": ["Was", "fragst", "du", "nach", "mei\u00b7nem", "a\u00b7de\u00b7li\u00b7chen", "Schwes\u00b7ter\u00b7lein", "klein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "PPOSAT", "ADJA", "NN", "ADJD", "$,"], "meter": "-+--+-+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Es m\u00f6chte dir viel zu h\u00fcbsch und zu adelich seyn.", "tokens": ["Es", "m\u00f6ch\u00b7te", "dir", "viel", "zu", "h\u00fcbsch", "und", "zu", "a\u00b7de\u00b7lich", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "PTKA", "ADJD", "KON", "PTKA", "ADJD", "VAINF", "$."], "meter": "-+--+-+--+--+", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "\u00bbwarum m\u00f6cht es mir viel zu h\u00fcbsch und zu adelich seyn,", "tokens": ["\u00bb", "wa\u00b7rum", "m\u00f6cht", "es", "mir", "viel", "zu", "h\u00fcbsch", "und", "zu", "a\u00b7de\u00b7lich", "seyn", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "VMFIN", "PPER", "PPER", "ADV", "PTKA", "ADJD", "KON", "PTKA", "ADJD", "VAINF", "$,"], "meter": "--+--+-+--+--+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Es geht mit einem Kindelein klein.\u00ab", "tokens": ["Es", "geht", "mit", "ei\u00b7nem", "Kin\u00b7de\u00b7lein", "klein", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ADJD", "$.", "$("], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.5": {"line.1": {"text": "Geht es mit einem Kindelein klein,", "tokens": ["Geht", "es", "mit", "ei\u00b7nem", "Kin\u00b7de\u00b7lein", "klein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "So soll es auch nicht mehr mein Schwesterlein seyn.", "tokens": ["So", "soll", "es", "auch", "nicht", "mehr", "mein", "Schwes\u00b7ter\u00b7lein", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "PTKNEG", "ADV", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.6": {"line.1": {"text": "Er schickte sogleich Ro\u00df und Wagen,", "tokens": ["Er", "schick\u00b7te", "sog\u00b7leich", "Ro\u00df", "und", "Wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und lie\u00df sein adelichs Schwesterlein hertragen.", "tokens": ["Und", "lie\u00df", "sein", "a\u00b7de\u00b7lichs", "Schwes\u00b7ter\u00b7lein", "her\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.7": {"line.1": {"text": "Sie versprach der Kindsmagd ein Paar neue Schuh,", "tokens": ["Sie", "ver\u00b7sprach", "der", "Kinds\u00b7magd", "ein", "Paar", "neu\u00b7e", "Schuh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Soll ihrem Kindlein die Sach recht thun.", "tokens": ["Soll", "ih\u00b7rem", "Kin\u00b7dlein", "die", "Sach", "recht", "thun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.8": {"line.1": {"text": "Versprach dem Kutscher ein Paar silberne Sporen,", "tokens": ["Ver\u00b7sprach", "dem", "Kut\u00b7scher", "ein", "Paar", "sil\u00b7ber\u00b7ne", "Spo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+--++--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Er soll auch tapfer in Hof nein fahren.", "tokens": ["Er", "soll", "auch", "tap\u00b7fer", "in", "Hof", "nein", "fah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADJD", "APPR", "NN", "PTKANT", "VVINF", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.9": {"line.1": {"text": "Und da sie in den Hof nein kamen,", "tokens": ["Und", "da", "sie", "in", "den", "Hof", "nein", "ka\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "ART", "NN", "PTKANT", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da sagt der Bruder ihr gleich willkommen:", "tokens": ["Da", "sagt", "der", "Bru\u00b7der", "ihr", "gleich", "will\u00b7kom\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.10": {"line.1": {"text": "\u00bbliebes adeliches Schwesterlein mein,", "tokens": ["\u00bb", "lie\u00b7bes", "a\u00b7de\u00b7li\u00b7ches", "Schwes\u00b7ter\u00b7lein", "mein", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJA", "ADJA", "NN", "PPOSAT", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Wo hast du dein Kindelein klein?\u00ab", "tokens": ["Wo", "hast", "du", "dein", "Kin\u00b7de\u00b7lein", "klein", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "PPOSAT", "NN", "ADJD", "$.", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.11": {"line.1": {"text": "Ich hab f\u00fcrwahr kein Kindelein klein,", "tokens": ["Ich", "hab", "f\u00fcr\u00b7wahr", "kein", "Kin\u00b7de\u00b7lein", "klein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PIAT", "NN", "ADJD", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Die Leute gehn mit L\u00fcgen auf mich ein.", "tokens": ["Die", "Leu\u00b7te", "gehn", "mit", "L\u00fc\u00b7gen", "auf", "mich", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.12": {"line.1": {"text": "Er nahm sie bey ihrer schneeweisesten Hand,", "tokens": ["Er", "nahm", "sie", "bey", "ih\u00b7rer", "schnee\u00b7wei\u00b7ses\u00b7ten", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+----+-+-+", "measure": "dactylic.init"}, "line.2": {"text": "Und f\u00fchrt sie auf Ulm zu dem Tanz.", "tokens": ["Und", "f\u00fchrt", "sie", "auf", "Ulm", "zu", "dem", "Tanz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "NE", "APPR", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.13": {"line.1": {"text": "\u00bbihr Musikanten macht mir auf einen langen Tanz,", "tokens": ["\u00bb", "ihr", "Mu\u00b7si\u00b7kan\u00b7ten", "macht", "mir", "auf", "ei\u00b7nen", "lan\u00b7gen", "Tanz", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Mein Schwester ist hier im N\u00e4gelkranz.\u00ab", "tokens": ["Mein", "Schwes\u00b7ter", "ist", "hier", "im", "N\u00e4\u00b7gel\u00b7kranz", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "APPRART", "NN", "$.", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.14": {"line.1": {"text": "Der Tanz der w\u00e4hrte dritthalbe Stund,", "tokens": ["Der", "Tanz", "der", "w\u00e4hr\u00b7te", "dritt\u00b7hal\u00b7be", "Stund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Bis ihr die Milch aus den Br\u00fcsten raussprung.", "tokens": ["Bis", "ihr", "die", "Milch", "aus", "den", "Br\u00fcs\u00b7ten", "raus\u00b7sprung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.15": {"line.1": {"text": "Der Bruder nahm sie bey der schneeweisesten Hand", "tokens": ["Der", "Bru\u00b7der", "nahm", "sie", "bey", "der", "schnee\u00b7wei\u00b7ses\u00b7ten", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und f\u00fchrt sie in sein Schlafzimmer alsbald.", "tokens": ["Und", "f\u00fchrt", "sie", "in", "sein", "Schlaf\u00b7zim\u00b7mer", "als\u00b7bald", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-++--+", "measure": "iambic.penta.chol"}}, "stanza.16": {"line.1": {"text": "Und sprang mit Stiefel und Sporen auf sie,", "tokens": ["Und", "sprang", "mit", "Stie\u00b7fel", "und", "Spo\u00b7ren", "auf", "sie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "KON", "NN", "APPR", "PPER", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da\u00df sie vor grossem Schmerze laut schrie.", "tokens": ["Da\u00df", "sie", "vor", "gros\u00b7sem", "Schmer\u00b7ze", "laut", "schrie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.17": {"line.1": {"text": "H\u00f6r auf, h\u00f6r auf, grober Bruder mein,", "tokens": ["H\u00f6r", "auf", ",", "h\u00f6r", "auf", ",", "gro\u00b7ber", "Bru\u00b7der", "mein", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "VVFIN", "PTKVZ", "$,", "ADJA", "NN", "PPOSAT", "$,"], "meter": "----+-+-+", "measure": "unknown.measure.tri"}, "line.2": {"text": "Es ist ja genug, das Kind ist nicht dein.", "tokens": ["Es", "ist", "ja", "ge\u00b7nug", ",", "das", "Kind", "ist", "nicht", "dein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "$,", "ART", "NN", "VAFIN", "PTKNEG", "PPOSAT", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.18": {"line.1": {"text": "Es geh\u00f6rt ja dem K\u00f6nig in Engeland zu!", "tokens": ["Es", "ge\u00b7h\u00f6rt", "ja", "dem", "K\u00f6\u00b7nig", "in", "En\u00b7ge\u00b7land", "zu", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "APPR", "NE", "PTKVZ", "$."], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.2": {"text": "\u00bbach h\u00e4ttst du es b\u00e4lder gesaget nur!", "tokens": ["\u00bb", "ach", "h\u00e4ttst", "du", "es", "b\u00e4l\u00b7der", "ge\u00b7sa\u00b7get", "nur", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "XY", "VAFIN", "PPER", "PPER", "ADV", "VVFIN", "ADV", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.19": {"line.1": {"text": "H\u00e4tt ich f\u00fcrwahr einen Schwager gehabt,", "tokens": ["H\u00e4tt", "ich", "f\u00fcr\u00b7wahr", "ei\u00b7nen", "Schwa\u00b7ger", "ge\u00b7habt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "VAPP", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Ist dir noch zu helfen, mein Schwesterlein sags?\u00ab", "tokens": ["Ist", "dir", "noch", "zu", "hel\u00b7fen", ",", "mein", "Schwes\u00b7ter\u00b7lein", "sags", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "PPER", "ADV", "PTKZU", "VVINF", "$,", "PPOSAT", "NN", "ADV", "$.", "$("], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}}, "stanza.20": {"line.1": {"text": "Warum wird es mir zu helfen seyn,", "tokens": ["Wa\u00b7rum", "wird", "es", "mir", "zu", "hel\u00b7fen", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "PPER", "PTKZU", "VVINF", "VAINF", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Man sieht auf Lung und Leber hinein!", "tokens": ["Man", "sieht", "auf", "Lung", "und", "Le\u00b7ber", "hin\u00b7ein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.21": {"line.1": {"text": "Es stand nicht l\u00e4nger an als dritthalbe Tag,", "tokens": ["Es", "stand", "nicht", "l\u00e4n\u00b7ger", "an", "als", "dritt\u00b7hal\u00b7be", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADJD", "PTKVZ", "KOKOM", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Da war der K\u00f6nig von England selber da.", "tokens": ["Da", "war", "der", "K\u00f6\u00b7nig", "von", "En\u00b7gland", "sel\u00b7ber", "da", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "APPR", "NE", "ADV", "ADV", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.22": {"line.1": {"text": "\u00bbwillkommen, willkommen junger Markgraf mein,", "tokens": ["\u00bb", "will\u00b7kom\u00b7men", ",", "will\u00b7kom\u00b7men", "jun\u00b7ger", "Mark\u00b7graf", "mein", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "$,", "ADJD", "ADJA", "NN", "PPOSAT", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Wo hast du dein adelich Schwesterlein klein?\u00ab", "tokens": ["Wo", "hast", "du", "dein", "a\u00b7de\u00b7lich", "Schwes\u00b7ter\u00b7lein", "klein", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "PPOSAT", "ADJD", "NN", "ADJD", "$.", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.23": {"line.1": {"text": "Es liegt im k\u00fchlen Grab und da liegts,", "tokens": ["Es", "liegt", "im", "k\u0