text": "Bis ihr die Milch aus den Br\u00fcsten raussprung.", "tokens": ["Bis", "ihr", "die", "Milch", "aus", "den", "Br\u00fcs\u00b7ten", "raus\u00b7sprung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.41": {"line.1": {"text": "Der Bruder nahm sie bey der schneeweisesten Hand", "tokens": ["Der", "Bru\u00b7der", "nahm", "sie", "bey", "der", "schnee\u00b7wei\u00b7ses\u00b7ten", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und f\u00fchrt sie in sein Schlafzimmer alsbald.", "tokens": ["Und", "f\u00fchrt", "sie", "in", "sein", "Schlaf\u00b7zim\u00b7mer", "als\u00b7bald", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-++--+", "measure": "iambic.penta.chol"}}, "stanza.42": {"line.1": {"text": "Und sprang mit Stiefel und Sporen auf sie,", "tokens": ["Und", "sprang", "mit", "Stie\u00b7fel", "und", "Spo\u00b7ren", "auf", "sie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "KON", "NN", "APPR", "PPER", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da\u00df sie vor grossem Schmerze laut schrie.", "tokens": ["Da\u00df", "sie", "vor", "gros\u00b7sem", "Schmer\u00b7ze", "laut", "schrie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.43": {"line.1": {"text": "H\u00f6r auf, h\u00f6r 