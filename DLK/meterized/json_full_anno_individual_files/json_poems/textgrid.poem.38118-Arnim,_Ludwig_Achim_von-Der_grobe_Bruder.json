"}, "line.2": {"text": "Ist dir noch zu helfen, mein Schwesterlein sags?\u00ab", "tokens": ["Ist", "dir", "noch", "zu", "hel\u00b7fen", ",", "mein", "Schwes\u00b7ter\u00b7lein", "sags", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "PPER", "ADV", "PTKZU", "VVINF", "$,", "PPOSAT", "NN", "ADV", "$.", "$("], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}}, "stanza.46": {"line.1": {"text": "Warum wird es mir zu helfen seyn,", "tokens": ["Wa\u00b7rum", "wird", "es", "mir", "zu", "hel\u00b7fen", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "PPER", "PTKZU", "VVINF", "VAINF", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Man sieht auf Lung und Leber hinein!", "tokens": ["Man", "sieht", "auf", "Lung", "und", "Le\u00b7ber", "hin\u00b7ein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.47": {"line.1": {"text": "Es stand nicht l\u00e4nger an als dritthalbe Tag,", "tokens": ["Es", "stand", "nicht", "l\u00e4n\u00b7ger", "an", "als", "dritt\u00b7hal\u00b7be", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADJD", "PTKVZ", "KOKOM", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Da war der K\u00f6nig von England selber da.", "tokens": ["Da", "war", "der", "K\u00f6\u00b7nig", "von", "En\u00b7gland", "sel\u00b7ber", "da", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "APPR", "NE", "ADV", "ADV", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.48": {"line.1": {"text": "\u00bbwillkommen, willkommen junger Markgraf mein,", "tokens": ["\u00bb", "will\u00b7kom\u00b7men", ",", "will\u00b7kom\u00b7men", "jun\u00b7ger", "Mark\u00b7graf", "mein", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "$,", "ADJD", "ADJA", "NN", "PPOSAT", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Wo hast du dein adelich Schwesterlein klein?\u00ab", "tokens": ["Wo", "hast", "du", "dein", "a\u00b7de\u00b7lich", "Schwes\u00b7ter\u00b7lein", "klein", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "PPOSAT", "ADJD", "NN", "ADJD", "$.", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.49": {"line.1": {"text": "Es liegt im k\u00fchlen Grab und da liegts,", "tokens": ["Es", "liegt", "im", "k\u00fch\u00b7len", "Grab", "und", "da", "liegts", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN", "KON", "ADV", "VVFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Da\u00df du es nimmermehr hier wiedersiehst.", "tokens": ["Da\u00df", "du", "es", "nim\u00b7mer\u00b7mehr", "hier", "wie\u00b7der\u00b7siehst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.50": {"line.1": {"text": "Was zog der K\u00f6nig? Sein glitzeriges Schwerdt,", "tokens": ["Was", "zog", "der", "K\u00f6\u00b7nig", "?", "Sein", "glit\u00b7ze\u00b7ri\u00b7ges", "Schwerdt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "$.", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "lin