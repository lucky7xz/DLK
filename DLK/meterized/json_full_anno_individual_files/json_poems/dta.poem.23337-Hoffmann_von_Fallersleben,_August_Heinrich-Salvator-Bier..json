{"dta.poem.23337": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Salvator-Bier.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was k\u00fcmmert uns die zweite Kammer?", "tokens": ["Was", "k\u00fcm\u00b7mert", "uns", "die", "zwei\u00b7te", "Kam\u00b7mer", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und was die Constitution?", "tokens": ["Und", "was", "die", "Con\u00b7sti\u00b7tu\u00b7ti\u00b7on", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nie lindert sich der Steuerjammer,", "tokens": ["Nie", "lin\u00b7dert", "sich", "der", "Steu\u00b7er\u00b7jam\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nie steigert sich der Arbeitslohn.", "tokens": ["Nie", "stei\u00b7gert", "sich", "der", "Ar\u00b7beits\u00b7lohn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wir kommen niemals doch in Flor,", "tokens": ["Wir", "kom\u00b7men", "nie\u00b7mals", "doch", "in", "Flor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und m\u00fcssen zahlen nach wie vor.", "tokens": ["Und", "m\u00fcs\u00b7sen", "zah\u00b7len", "nach", "wie", "vor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "VVINF", "APPR", "KOKOM", "APPR", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Was k\u00fcmmern uns die Dankadressen?", "tokens": ["Was", "k\u00fcm\u00b7mern", "uns", "die", "Dan\u00b7kad\u00b7res\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wer schaut in unsern Topf hinein?", "tokens": ["Wer", "schaut", "in", "un\u00b7sern", "Topf", "hin\u00b7ein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wer fragt, ob wir uns satt noch essen?", "tokens": ["Wer", "fragt", ",", "ob", "wir", "uns", "satt", "noch", "es\u00b7sen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "KOUS", "PPER", "PRF", "ADJD", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie wir uns plagen und kastei'n?", "tokens": ["Wie", "wir", "uns", "pla\u00b7gen", "und", "kas\u00b7tei'n", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "VVFIN", "KON", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sie sind f\u00fcr unsre Leiden stumm", "tokens": ["Sie", "sind", "f\u00fcr", "uns\u00b7re", "Lei\u00b7den", "stumm"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und scheren sich den Teufel drum.", "tokens": ["Und", "sche\u00b7ren", "sich", "den", "Teu\u00b7fel", "drum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ART", "NN", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}