{"dta.poem.13776": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "An die Meisten.  \n 1810.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Wohlfeil Ruhm sich zu erringen,", "tokens": ["Wohl\u00b7feil", "Ruhm", "sich", "zu", "er\u00b7rin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PRF", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jeder \u00e4ngstlich schreibt und treibet;", "tokens": ["Je\u00b7der", "\u00e4ngst\u00b7lich", "schreibt", "und", "trei\u00b7bet", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADJD", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Keinem m\u00f6cht' das Herz zerspringen,", "tokens": ["Kei\u00b7nem", "m\u00f6cht'", "das", "Herz", "zer\u00b7sprin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Glaubt sich selbst nicht, was er schreibet.", "tokens": ["Glaubt", "sich", "selbst", "nicht", ",", "was", "er", "schrei\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "PTKNEG", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}