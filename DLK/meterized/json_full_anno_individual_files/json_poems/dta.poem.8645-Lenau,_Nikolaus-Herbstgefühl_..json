{"dta.poem.8645": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Herbstgef\u00fchl .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1832", "urn": "urn:nbn:de:kobv:b4-200905193572", "language": ["de:0.99"], "booktitle": "Lenau, Nikolaus: Gedichte. Stuttgart, 1832."}, "poem": {"stanza.1": {"line.1": {"text": "M\u00fcrrisch braust der Eichenwald,", "tokens": ["M\u00fcr\u00b7risch", "braust", "der", "Ei\u00b7chen\u00b7wald", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Aller Himmel ist umzogen,", "tokens": ["Al\u00b7ler", "Him\u00b7mel", "ist", "um\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "VVIZU", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und dem Wandrer, rauh und kalt,", "tokens": ["Und", "dem", "Wand\u00b7rer", ",", "rauh", "und", "kalt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Kommt der Herbstwind nachgeflogen.", "tokens": ["Kommt", "der", "Herbst\u00b7wind", "nach\u00b7ge\u00b7flo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "VVIZU", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie der Wind in Herbstes Zeit", "tokens": ["Wie", "der", "Wind", "in", "Herbs\u00b7tes", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Mordend hinsaust in den W\u00e4ldern,", "tokens": ["Mor\u00b7dend", "hin\u00b7saust", "in", "den", "W\u00e4l\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+--++-+-", "measure": "dactylic.init"}, "line.3": {"text": "Weht mir die Vergangenheit", "tokens": ["Weht", "mir", "die", "Ver\u00b7gan\u00b7gen\u00b7heit"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Von des Gl\u00fcckes Stoppelfeldern.", "tokens": ["Von", "des", "Gl\u00fc\u00b7ckes", "Stop\u00b7pel\u00b7fel\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "An den B\u00e4umen, welk und matt,", "tokens": ["An", "den", "B\u00e4u\u00b7men", ",", "welk", "und", "matt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Schwebt des Laubes lezte Neige,", "tokens": ["Schwebt", "des", "Lau\u00b7bes", "lez\u00b7te", "Nei\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Niedertaumelt Blatt f\u00fcr Blatt,", "tokens": ["Nie\u00b7der\u00b7tau\u00b7melt", "Blatt", "f\u00fcr", "Blatt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und verh\u00fcllt die Waldessteige;", "tokens": ["Und", "ver\u00b7h\u00fcllt", "die", "Wal\u00b7des\u00b7stei\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Immer dichter f\u00e4llt es, will", "tokens": ["Im\u00b7mer", "dich\u00b7ter", "f\u00e4llt", "es", ",", "will"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "ADJD", "VVFIN", "PPER", "$,", "VMFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Mir den Reisepfad verderben,", "tokens": ["Mir", "den", "Rei\u00b7se\u00b7pfad", "ver\u00b7der\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df ich lieber halte still,", "tokens": ["Da\u00df", "ich", "lie\u00b7ber", "hal\u00b7te", "still", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Gleich am Orte hier zu sterben.", "tokens": ["Gleich", "am", "Or\u00b7te", "hier", "zu", "ster\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "ADV", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}