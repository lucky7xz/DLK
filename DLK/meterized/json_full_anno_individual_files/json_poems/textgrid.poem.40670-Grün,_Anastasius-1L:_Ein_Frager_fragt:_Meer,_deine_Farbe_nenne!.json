{"textgrid.poem.40670": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein Frager fragt: Meer, deine Farbe nenne!", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Frager fragt: Meer, deine Farbe nenne!", "tokens": ["Ein", "Fra\u00b7ger", "fragt", ":", "Meer", ",", "dei\u00b7ne", "Far\u00b7be", "nen\u00b7ne", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "NN", "$,", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Bald bist du gr\u00fcn, als ob die Lenze sprossen,", "tokens": ["Bald", "bist", "du", "gr\u00fcn", ",", "als", "ob", "die", "Len\u00b7ze", "spros\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "$,", "KOKOM", "KOUS", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Bald blau, als ob dich nichts vom Himmel trenne,", "tokens": ["Bald", "blau", ",", "als", "ob", "dich", "nichts", "vom", "Him\u00b7mel", "tren\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "KOKOM", "KOUS", "PPER", "PIS", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Bald roth, wie blutend von Apolls Geschossen;", "tokens": ["Bald", "roth", ",", "wie", "blu\u00b7tend", "von", "A\u00b7polls", "Ge\u00b7schos\u00b7sen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "PWAV", "ADJD", "APPR", "NE", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Nun grau, wie einer W\u00fcste sand'ge Tenne.", "tokens": ["Nun", "grau", ",", "wie", "ei\u00b7ner", "W\u00fcs\u00b7te", "san\u00b7d'\u00b7ge", "Ten\u00b7ne", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "PWAV", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Nun braun, von finsterm Bu\u00dfgewand umflossen,", "tokens": ["Nun", "braun", ",", "von", "fins\u00b7term", "Bu\u00df\u00b7ge\u00b7wand", "um\u00b7flos\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Goldhell, als ob dein Salz als Lava brenne,", "tokens": ["Gold\u00b7hell", ",", "als", "ob", "dein", "Salz", "als", "La\u00b7va", "bren\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOKOM", "KOUS", "PPOSAT", "NN", "KOKOM", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Milchwei\u00df, wie M\u00e4hnenflug von wei\u00dfen Rossen!", "tokens": ["Milch\u00b7wei\u00df", ",", "wie", "M\u00e4h\u00b7nen\u00b7flug", "von", "wei\u00b7\u00dfen", "Ros\u00b7sen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Antwortet drauf das Meer: \u00bbO schlauer Frager,", "tokens": ["Ant\u00b7wor\u00b7tet", "drauf", "das", "Meer", ":", "\u00bb", "O", "schlau\u00b7er", "Fra\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PAV", "ART", "NN", "$.", "$(", "NE", "ADJA"