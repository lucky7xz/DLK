{"dta.poem.3284": {"metadata": {"author": {"name": "Uz, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "Das neue Orakel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1755", "urn": "urn:nbn:de:kobv:b4-20614-6", "language": ["de:0.99"], "booktitle": "Uz, Johann Peter: Lyrische und andere Gedichte. 2. Aufl. Ansbach, 1755."}, "poem": {"stanza.1": {"line.1": {"text": "Propheten unsrer Zeit, Zigeuner, kluge Weiber!", "tokens": ["Pro\u00b7phe\u00b7ten", "uns\u00b7rer", "Zeit", ",", "Zi\u00b7geu\u00b7ner", ",", "klu\u00b7ge", "Wei\u00b7ber", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weh euch! ihr alle seyd verschm\u00e4ht!", "tokens": ["Weh", "euch", "!", "ihr", "al\u00b7le", "seyd", "ver\u00b7schm\u00e4ht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$.", "PPER", "PIS", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Seht, wie der Coffeesatz, der Neugier Zeitver-", "tokens": ["Seht", ",", "wie", "der", "Cof\u00b7fee\u00b7satz", ",", "der", "Neu\u00b7gier", "Zeit\u00b7ver"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "PWAV", "ART", "NN", "$,", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "treiber,", "tokens": ["trei\u00b7ber", ","], "token_info": ["word", "punct"], "pos": ["PAV", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Sich als Orakel bl\u00e4ht.", "tokens": ["Sich", "als", "O\u00b7ra\u00b7kel", "bl\u00e4ht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "KOUS", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die schlaue Phantasie sieht in geheimen Zeichen", "tokens": ["Die", "schlau\u00b7e", "Phan\u00b7ta\u00b7sie", "sieht", "in", "ge\u00b7hei\u00b7men", "Zei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Des weisen Schlammes Antwort stehn:", "tokens": ["Des", "wei\u00b7sen", "Schlam\u00b7mes", "Ant\u00b7wort", "stehn", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie die um Mitternacht durch \u00f6de W\u00e4lder streichen,", "tokens": ["Wie", "die", "um", "Mit\u00b7ter\u00b7nacht", "durch", "\u00f6\u00b7de", "W\u00e4l\u00b7der", "strei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "APPR", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Gespenst und Sch\u00e4tze sehn.", "tokens": ["Ge\u00b7spenst", "und", "Sch\u00e4t\u00b7ze", "sehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Auch mir verk\u00fcndigt sie, und Liebe hilft mir glauben,", "tokens": ["Auch", "mir", "ver\u00b7k\u00fcn\u00b7digt", "sie", ",", "und", "Lie\u00b7be", "hilft", "mir", "glau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "PPER", "$,", "KON", "NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df ich mein M\u00e4dchen k\u00fcssen soll.", "tokens": ["Da\u00df", "ich", "mein", "M\u00e4d\u00b7chen", "k\u00fcs\u00b7sen", "soll", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nichts kann gewisser seyn! da schn\u00e4beln sich zwo Tauben:", "tokens": ["Nichts", "kann", "ge\u00b7wis\u00b7ser", "seyn", "!", "da", "schn\u00e4\u00b7beln", "sich", "zwo", "Tau\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "ADJD", "VAINF", "$.", "ADV", "VVFIN", "PRF", "CARD", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Das ist geheimni\u00dfvoll!", "tokens": ["Das", "ist", "ge\u00b7heim\u00b7ni\u00df\u00b7voll", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Zwar sieht mein Auge nichts; doch glaub ich mei-", "tokens": ["Zwar", "sieht", "mein", "Au\u00b7ge", "nichts", ";", "doch", "glaub", "ich", "mei"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "PIS", "$.", "ADV", "VVFIN", "PPER", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "nem Gl\u00fccke:", "tokens": ["nem", "Gl\u00fc\u00b7cke", ":"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Die Tauben sind unsichtbar da:", "tokens": ["Die", "Tau\u00b7ben", "sind", "un\u00b7sicht\u00b7bar", "da", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auch Bileam sah nicht, was mit erstauntem Blicke", "tokens": ["Auch", "Bi\u00b7leam", "sah", "nicht", ",", "was", "mit", "er\u00b7staun\u00b7tem", "Bli\u00b7cke"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "NN", "VVFIN", "PTKNEG", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "--+-+-+-+-+-", "measure": "anapaest.init"}, "line.5": {"text": "Sein Thier erleuchtet sah.", "tokens": ["Sein", "Thier", "er\u00b7leuch\u00b7tet", "sah", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Sey gl\u00e4ubig, loses Kind! und komm und la\u00df dich", "tokens": ["Sey", "gl\u00e4u\u00b7big", ",", "lo\u00b7ses", "Kind", "!", "und", "komm", "und", "la\u00df", "dich"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAIMP", "ADJD", "$,", "ADJA", "NN", "$.", "KON", "VVFIN", "KON", "VVIMP", "PPER"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "k\u00fcssen!", "tokens": ["k\u00fcs\u00b7sen", "!"], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Umsonst ist alle Spr\u00f6digkeit.", "tokens": ["Um\u00b7sonst", "ist", "al\u00b7le", "Spr\u00f6\u00b7dig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dein Stolz wird endlich doch dem Schicksal weichen m\u00fcs-", "tokens": ["Dein", "Stolz", "wird", "end\u00b7lich", "doch", "dem", "Schick\u00b7sal", "wei\u00b7chen", "m\u00fcs"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ADV", "ART", "NN", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "sen:", "tokens": ["sen", ":"], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "-", "measure": "single.down"}, "line.6": {"text": "Es ist mir prophezeyt!", "tokens": ["Es", "ist", "mir", "pro\u00b7phe\u00b7zeyt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}