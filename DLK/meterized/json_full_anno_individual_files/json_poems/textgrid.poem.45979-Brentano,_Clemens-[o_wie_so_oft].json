{"textgrid.poem.45979": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "[o wie so oft]", "genre": "verse", "period": "N.A.", "pub_year": 1816, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O wie so oft", "tokens": ["O", "wie", "so", "oft"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "KOKOM", "ADV", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Hab' ich ein Zeichen erhofft,", "tokens": ["Hab'", "ich", "ein", "Zei\u00b7chen", "er\u00b7hofft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Zogen", "tokens": ["Zo\u00b7gen"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Sterne den schimmernden Bogen", "tokens": ["Ster\u00b7ne", "den", "schim\u00b7mern\u00b7den", "Bo\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.5": {"text": "Durch die himmlische Leere", "tokens": ["Durch", "die", "himm\u00b7li\u00b7sche", "Lee\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.6": {"text": "Durch die himmlische Tiefe,", "tokens": ["Durch", "die", "himm\u00b7li\u00b7sche", "Tie\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.7": {"text": "Da\u00df ich der irdischen Schwere", "tokens": ["Da\u00df", "ich", "der", "ir\u00b7di\u00b7schen", "Schwe\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Endlich auf immer entschliefe,", "tokens": ["End\u00b7lich", "auf", "im\u00b7mer", "ent\u00b7schlie\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADV", "VVFIN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.9": {"text": "Aber der Morgen", "tokens": ["A\u00b7ber", "der", "Mor\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["KON", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "L\u00f6schte die Sterne aus,", "tokens": ["L\u00f6schte", "die", "Ster\u00b7ne", "aus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.11": {"text": "Weckte die Sorgen,", "tokens": ["Weck\u00b7te", "die", "Sor\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.12": {"text": "Weckte des Herzens Haus,", "tokens": ["Weck\u00b7te", "des", "Her\u00b7zens", "Haus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.13": {"text": "Und des Allt\u00e4glichen Macht", "tokens": ["Und", "des", "All\u00b7t\u00e4g\u00b7li\u00b7chen", "Macht"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.14": {"text": "Zwang die Ahndung der Nacht.", "tokens": ["Zwang", "die", "Ahn\u00b7dung", "der", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "O wie so viel", "tokens": ["O", "wie", "so", "viel"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "KOKOM", "ADV", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Nahte der Sehnsucht das Ziel", "tokens": ["Nah\u00b7te", "der", "Sehn\u00b7sucht", "das", "Ziel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "ART", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Sanken", "tokens": ["San\u00b7ken"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "D\u00fcrstende m\u00fcde Gedanken", "tokens": ["D\u00fcrs\u00b7ten\u00b7de", "m\u00fc\u00b7de", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "word"], "pos": ["NE", "ADJA", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.5": {"text": "Hin an brennender Schwelle", "tokens": ["Hin", "an", "bren\u00b7nen\u00b7der", "Schwel\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.6": {"text": "Selig k\u00fchlender Ferne,", "tokens": ["Se\u00b7lig", "k\u00fch\u00b7len\u00b7der", "Fer\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.7": {"text": "Ach da st\u00fcrzte zum Herzen die Welle", "tokens": ["Ach", "da", "st\u00fcrz\u00b7te", "zum", "Her\u00b7zen", "die", "Wel\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "ADV", "VVFIN", "APPRART", "NN", "ART", "NN"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.8": {"text": "Und das lachende Licht in die finsteren Sterne,", "tokens": ["Und", "das", "la\u00b7chen\u00b7de", "Licht", "in", "die", "fins\u00b7te\u00b7ren", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.9": {"text": "Aber die Ebbe", "tokens": ["A\u00b7ber", "die", "Eb\u00b7be"], "token_info": ["word", "word", "word"], "pos": ["KON", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Kehrte, die Flut wich,", "tokens": ["Kehr\u00b7te", ",", "die", "Flut", "wich", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.11": {"text": "Hei\u00dfer die Steppe", "tokens": ["Hei\u00b7\u00dfer", "die", "Step\u00b7pe"], "token_info": ["word", "word", "word"], "pos": ["NN", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.12": {"text": "Umg\u00fcrtet mit Glut mich,", "tokens": ["Um\u00b7g\u00fcr\u00b7tet", "mit", "Glut", "mich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "PPER", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.13": {"text": "Und den brennenden Pfeil", "tokens": ["Und", "den", "bren\u00b7nen\u00b7den", "Pfeil"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.14": {"text": "Mahnte das fliehende Ziel zur Eil'.", "tokens": ["Mahn\u00b7te", "das", "flie\u00b7hen\u00b7de", "Ziel", "zur", "Eil'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "O wie so tief", "tokens": ["O", "wie", "so", "tief"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "KOKOM", "ADV", "ADJD"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Oft aus den Wogen mich's rief!", "tokens": ["Oft", "aus", "den", "Wo\u00b7gen", "mich's", "rief", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "NE", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Fielen", "tokens": ["Fie\u00b7len"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Um nach den Sternen zu zielen", "tokens": ["Um", "nach", "den", "Ster\u00b7nen", "zu", "zie\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "APPR", "ART", "NN", "PTKZU", "VVINF"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.5": {"text": "Tr\u00e4nen zu spiegelnden Seen", "tokens": ["Tr\u00e4\u00b7nen", "zu", "spie\u00b7geln\u00b7den", "Seen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.6": {"text": "Die zwischen blumigten Wiesen,", "tokens": ["Die", "zwi\u00b7schen", "blu\u00b7mig\u00b7ten", "Wie\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Augen der Erde, aufsehen,", "tokens": ["Au\u00b7gen", "der", "Er\u00b7de", ",", "auf\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "VVIZU", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.8": {"text": "Himmlische Kinder zu gr\u00fc\u00dfen.", "tokens": ["Himm\u00b7li\u00b7sche", "Kin\u00b7der", "zu", "gr\u00fc\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.9": {"text": "Aber die Fl\u00e4che", "tokens": ["A\u00b7ber", "die", "Fl\u00e4\u00b7che"], "token_info": ["word", "word", "word"], "pos": ["KON", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Ringelt, das Bild bricht,", "tokens": ["Rin\u00b7gelt", ",", "das", "Bild", "bricht", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.11": {"text": "Bittere B\u00e4che", "tokens": ["Bit\u00b7te\u00b7re", "B\u00e4\u00b7che"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.12": {"text": "Rinnet so wild nicht!", "tokens": ["Rin\u00b7net", "so", "wild", "nicht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "PTKNEG", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.13": {"text": "Freudig ja springet ein Fisch,", "tokens": ["Freu\u00b7dig", "ja", "sprin\u00b7get", "ein", "Fisch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.14": {"text": "Und ich mord' ihn, decke den Tisch.", "tokens": ["Und", "ich", "mord'", "ihn", ",", "de\u00b7cke", "den", "Tisch", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PPER", "$,", "VVFIN", "ART", "NN", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}}, "stanza.4": {"line.1": {"text": "O wie so rein", "tokens": ["O", "wie", "so", "rein"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "KOKOM", "ADV", "ADJD"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "W\u00e4chst in der Sch\u00f6nheit der Schein,", "tokens": ["W\u00e4chst", "in", "der", "Sch\u00f6n\u00b7heit", "der", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Scheinet", "tokens": ["Schei\u00b7net"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Sie aus der Einfalt und einet", "tokens": ["Sie", "aus", "der", "Ein\u00b7falt", "und", "ei\u00b7net"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "ART", "NN", "KON", "VVFIN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Recht in der lauteren Klarheit", "tokens": ["Recht", "in", "der", "lau\u00b7te\u00b7ren", "Klar\u00b7heit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.6": {"text": "Strahlen der himmlischen G\u00fcte", "tokens": ["Strah\u00b7len", "der", "himm\u00b7li\u00b7schen", "G\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.7": {"text": "Zum sehenden sichtbaren Auge der Wahrheit,", "tokens": ["Zum", "se\u00b7hen\u00b7den", "sicht\u00b7ba\u00b7ren", "Au\u00b7ge", "der", "Wahr\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.8": {"text": "Das das schaffet und selbst ist die Frucht und die Bl\u00fcte", "tokens": ["Das", "das", "schaf\u00b7fet", "und", "selbst", "ist", "die", "Frucht", "und", "die", "Bl\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "PDS", "VVFIN", "KON", "ADV", "VAFIN", "ART", "NN", "KON", "ART", "NN"], "meter": "--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.9": {"text": "Aber die Dichter", "tokens": ["A\u00b7ber", "die", "Dich\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["KON", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Machen die Glieder zum Leibe gern", "tokens": ["Ma\u00b7chen", "die", "Glie\u00b7der", "zum", "Lei\u00b7be", "gern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "APPRART", "NN", "ADV"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.11": {"text": "Schneiden Gesichter", "tokens": ["Schnei\u00b7den", "Ge\u00b7sich\u00b7ter"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.12": {"text": "In einen Kirschenkern", "tokens": ["In", "ei\u00b7nen", "Kir\u00b7schen\u00b7kern"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Traurig und lachend, o gebe", "tokens": ["Trau\u00b7rig", "und", "la\u00b7chend", ",", "o", "ge\u00b7be"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "KON", "ADJD", "$,", "FM", "VVFIN"], "meter": "+--+-+--", "measure": "iambic.tri.invert"}, "line.14": {"text": "Lieber der Erde ihn, da\u00df er lebe", "tokens": ["Lie\u00b7ber", "der", "Er\u00b7de", "ihn", ",", "da\u00df", "er", "le\u00b7be"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "ART", "NN", "PPER", "$,", "KOUS", "PPER", "VVFIN"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.15": {"text": "Bl\u00fctenvoll", "tokens": ["Bl\u00fc\u00b7ten\u00b7voll"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.16": {"text": "Fr\u00fcchtevoll", "tokens": ["Fr\u00fcch\u00b7te\u00b7voll"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.17": {"text": "Dir und den Deinen himmlischen Segen", "tokens": ["Dir", "und", "den", "Dei\u00b7nen", "himm\u00b7li\u00b7schen", "Se\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "KON", "ART", "PPOSAT", "ADJA", "NN"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.18": {"text": "Gebe", "tokens": ["Ge\u00b7be"], "token_info": ["word"], "pos": ["NN"], "meter": "--", "measure": "unknown.measure.zero"}, "line.19": {"text": "Auf irdischen Wegen.", "tokens": ["Auf", "ir\u00b7di\u00b7schen", "We\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.5": {"line.1": {"text": "O wie so oft", "tokens": ["O", "wie", "so", "oft"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "KOKOM", "ADV", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Hab' ich ein Zeichen erhofft,", "tokens": ["Hab'", "ich", "ein", "Zei\u00b7chen", "er\u00b7hofft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Zogen", "tokens": ["Zo\u00b7gen"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Sterne den schimmernden Bogen", "tokens": ["Ster\u00b7ne", "den", "schim\u00b7mern\u00b7den", "Bo\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.5": {"text": "Durch die himmlische Leere", "tokens": ["Durch", "die", "himm\u00b7li\u00b7sche", "Lee\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.6": {"text": "Durch die himmlische Tiefe,", "tokens": ["Durch", "die", "himm\u00b7li\u00b7sche", "Tie\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.7": {"text": "Da\u00df ich der irdischen Schwere", "tokens": ["Da\u00df", "ich", "der", "ir\u00b7di\u00b7schen", "Schwe\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+--+-", "m