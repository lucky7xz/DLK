{"dta.poem.12102": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "24.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Zeit ist kurz, wenn voll; die Zeit, wenn leer, ist lang.", "tokens": ["Die", "Zeit", "ist", "kurz", ",", "wenn", "voll", ";", "die", "Zeit", ",", "wenn", "leer", ",", "ist", "lang", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "KOUS", "ADJD", "$.", "ART", "NN", "$,", "KOUS", "ADJD", "$,", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was macht sie leer und voll? deiner Gedanken Gang.", "tokens": ["Was", "macht", "sie", "leer", "und", "voll", "?", "dei\u00b7ner", "Ge\u00b7dan\u00b7ken", "Gang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$.", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Wenn viel du siehst und h\u00f6rst, was viel dich denken macht,", "tokens": ["Wenn", "viel", "du", "siehst", "und", "h\u00f6rst", ",", "was", "viel", "dich", "den\u00b7ken", "macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "VVFIN", "KON", "VVFIN", "$,", "PRELS", "ADV", "PPER", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So ist die Stund' entflohn rascher als du gedacht.", "tokens": ["So", "ist", "die", "Stund'", "ent\u00b7flohn", "ra\u00b7scher", "als", "du", "ge\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVFIN", "ADJD", "KOKOM", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wenn du nur siehst und h\u00f6rst, was dir gibt kein Gef\u00fchl,", "tokens": ["Wenn", "du", "nur", "siehst", "und", "h\u00f6rst", ",", "was", "dir", "gibt", "kein", "Ge\u00b7f\u00fchl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "KON", "VVFIN", "$,", "PWS", "PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "So stockt die leere Zeit im leeren Weltgew\u00fchl.", "tokens": ["So", "stockt", "die", "lee\u00b7re", "Zeit", "im", "lee\u00b7ren", "Welt\u00b7ge\u00b7w\u00fchl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Wenn du auch gar nichts siehst und h\u00f6rst, nur tr\u00e4umst und sinnest,", "tokens": ["Wenn", "du", "auch", "gar", "nichts", "siehst", "und", "h\u00f6rst", ",", "nur", "tr\u00e4umst", "und", "sin\u00b7nest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "PIS", "VVFIN", "KON", "VVFIN", "$,", "ADV", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wird kurz die Zeit indem du lange F\u00e4den spinnest.", "tokens": ["Wird", "kurz", "die", "Zeit", "in\u00b7dem", "du", "lan\u00b7ge", "F\u00e4\u00b7den", "spin\u00b7nest", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ART", "NN", "KOUS", "PPER", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Doch wenn im Denken stets dich Ein Gedanke st\u00f6rt,", "tokens": ["Doch", "wenn", "im", "Den\u00b7ken", "stets", "dich", "Ein", "Ge\u00b7dan\u00b7ke", "st\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "APPRART", "NN", "VVFIN", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So hat des Denkens Zeitverk\u00fcrzung aufgeh\u00f6rt.", "tokens": ["So", "hat", "des", "Den\u00b7kens", "Zeit\u00b7ver\u00b7k\u00fcr\u00b7zung", "auf\u00b7ge\u00b7h\u00f6rt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Dann geht es dir wie mir, da, was ich auch beginne", "tokens": ["Dann", "geht", "es", "dir", "wie", "mir", ",", "da", ",", "was", "ich", "auch", "be\u00b7gin\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "PWAV", "PPER", "$,", "KOUS", "$,", "PWS", "PPER", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu denken, mir nur Ein Gedanke liegt im Sinne.", "tokens": ["Zu", "den\u00b7ken", ",", "mir", "nur", "Ein", "Ge\u00b7dan\u00b7ke", "liegt", "im", "Sin\u00b7ne", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "PPER", "ADV", "ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Was ist zu thun? wenn du nichts anders recht kannst denken?", "tokens": ["Was", "ist", "zu", "thun", "?", "wenn", "du", "nichts", "an\u00b7ders", "recht", "kannst", "den\u00b7ken", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PTKZU", "VVINF", "$.", "KOUS", "PPER", "PIS", "ADV", "ADV", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ganz in den einzigen Gedanken dich versenken.", "tokens": ["Ganz", "in", "den", "ein\u00b7zi\u00b7gen", "Ge\u00b7dan\u00b7ken", "dich", "ver\u00b7sen\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "PPER", "VVINF", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.8": {"line.1": {"text": "Ich denke, da\u00df dein Brief nun kommen mu\u00df und soll,", "tokens": ["Ich", "den\u00b7ke", ",", "da\u00df", "dein", "Brief", "nun", "kom\u00b7men", "mu\u00df", "und", "soll", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPOSAT", "NN", "ADV", "VVINF", "VMFIN", "KON", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und der Gedanke macht die leere Zeit mir voll.", "tokens": ["Und", "der", "Ge\u00b7dan\u00b7ke", "macht", "die", "lee\u00b7re", "Zeit", "mir", "voll", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Ich denke, da\u00df der Brief nun kommen soll und mu\u00df,", "tokens": ["Ich", "den\u00b7ke", ",", "da\u00df", "der", "Brief", "nun", "kom\u00b7men", "soll", "und", "mu\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ART", "NN", "ADV", "VVINF", "VMFIN", "KON", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und vor der Th\u00fcre schon h\u00f6r' ich des Boten Fu\u00df.", "tokens": ["Und", "vor", "der", "Th\u00fc\u00b7re", "schon", "h\u00f6r'", "ich", "des", "Bo\u00b7ten", "Fu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADV", "VVFIN", "PPER", "ART", "NN", "NN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}}}}}