{"dta.poem.681": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "Ix.  \n Vanitas! Vanitatum Vanitas!", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Die Herrlichkeit der Erden ", "tokens": ["Die", "Herr\u00b7lich\u00b7keit", "der", "Er\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mus rauch vnd aschen werden/", "tokens": ["Mus", "rauch", "vnd", "asc\u00b7hen", "wer\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "KON", "VVINF", "VAFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Kein fels/ kein \u00e4rtz kan stehn.", "tokens": ["Kein", "fels", "/", "kein", "\u00e4rtz", "kan", "stehn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$(", "PIAT", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Dis was vns kan ergetzen/", "tokens": ["Dis", "was", "vns", "kan", "er\u00b7get\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRELS", "PPER", "VMFIN", "VVINF", "$("], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.5": {"text": "Was wir f\u00fcr ewig sch\u00e4tzen/", "tokens": ["Was", "wir", "f\u00fcr", "e\u00b7wig", "sch\u00e4t\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "ADJD", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wird als ein leichter traum vergehn.", "tokens": ["Wird", "als", "ein", "leich\u00b7ter", "traum", "ver\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KOKOM", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "2. Was sind doch alle sachen/", "tokens": ["Was", "sind", "doch", "al\u00b7le", "sa\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "PIS", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die vns ein hertze machen/", "tokens": ["Die", "vns", "ein", "hert\u00b7ze", "ma\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Als schlechte nichtigkeit? ", "tokens": ["Als", "schlech\u00b7te", "nich\u00b7tig\u00b7keit", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wa\u00df ist der Menschen leben/ ", "tokens": ["Wa\u00df", "ist", "der", "Men\u00b7schen", "le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der jmmer vmb mus schweben/", "tokens": ["Der", "jm\u00b7mer", "vmb", "mus", "schwe\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Als eine phantasie der zeit?", "tokens": ["Als", "ei\u00b7ne", "phan\u00b7ta\u00b7sie", "der", "zeit", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "3. ", "tokens": [], "token_info": [], "pos": []}, "line.2": {"text": "Den wir vnsterblich achten/", "tokens": ["Den", "wir", "vns\u00b7terb\u00b7lich", "ach\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PRF", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ist nur ein falscher wahn.", "tokens": ["Ist", "nur", "ein", "fal\u00b7scher", "wahn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "So bald der Geist gewichen:", "tokens": ["So", "bald", "der", "Geist", "ge\u00b7wi\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Vnd dieser mundt erblichen:", "tokens": ["Vnd", "die\u00b7ser", "mundt", "er\u00b7bli\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Fragt keiner/ was man hier gethan. ", "tokens": ["Fragt", "kei\u00b7ner", "/", "was", "man", "hier", "ge\u00b7than", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$(", "PWS", "PIS", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "4. Es hilfft kein weises wissen/", "tokens": ["Es", "hilfft", "kein", "wei\u00b7ses", "wis\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "ADJA", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wir werden hingerissen/", "tokens": ["Wir", "wer\u00b7den", "hin\u00b7ge\u00b7ris\u00b7sen", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ohn einen vnterscheidt/", "tokens": ["Ohn", "ei\u00b7nen", "vn\u00b7ter\u00b7scheidt", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Was n\u00fctzt der schl\u00f6sser menge/", "tokens": ["Was", "n\u00fctzt", "der", "schl\u00f6s\u00b7ser", "men\u00b7ge", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Dem hie die Welt zu enge/ ", "tokens": ["Dem", "hie", "die", "Welt", "zu", "en\u00b7ge", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ART", "NN", "APPR", "ADJA", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Dem wird ein enges grab zu weit. ", "tokens": ["Dem", "wird", "ein", "en\u00b7ges", "grab", "zu", "weit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "5. Dis alles wirdt zerrinnen/", "tokens": ["Dis", "al\u00b7les", "wirdt", "zer\u00b7rin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "VAFIN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was m\u00fch\u2019 vnd fleis gewinnen", "tokens": ["Was", "m\u00fch'", "vnd", "fleis", "ge\u00b7win\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "ADJD", "KON", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Vnd sawrer schweis erwirbt:", "tokens": ["Vnd", "saw\u00b7rer", "schweis", "er\u00b7wirbt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Was Menschen hier besitzen/", "tokens": ["Was", "Men\u00b7schen", "hier", "be\u00b7sit\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "ADV", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Kan f\u00fcr dem todt nicht n\u00fctzen/", "tokens": ["Kan", "f\u00fcr", "dem", "todt", "nicht", "n\u00fct\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "ART", "ADJD", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Dis alles stirbt vns/ wenn man stirbt. ", "tokens": ["Dis", "al\u00b7les", "stirbt", "vns", "/", "wenn", "man", "stirbt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "VVFIN", "PPER", "$(", "KOUS", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "6. Was sind die kurtzen frewden/", "tokens": ["Was", "sind", "die", "kurt\u00b7zen", "frew\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die stets/ ach! leidt/ vnd leiden/", "tokens": ["Die", "stets", "/", "ach", "!", "leidt", "/", "vnd", "lei\u00b7den", "/"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADV", "$(", "XY", "$.", "ADJD", "$(", "KON", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Vnd hertzens angst beschwert.", "tokens": ["Vnd", "hert\u00b7zens", "angst", "be\u00b7schwert", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Das s\u00fcsse jubiliren/ ", "tokens": ["Das", "s\u00fcs\u00b7se", "ju\u00b7bi\u00b7li\u00b7ren", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Das hohe triumphiren", "tokens": ["Das", "ho\u00b7he", "tri\u00b7um\u00b7phi\u00b7ren"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wirdt oft in hohn vnd schmach verkehrt.", "tokens": ["Wirdt", "oft", "in", "hohn", "vnd", "schmach", "ver\u00b7kehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "ADJA", "KON", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "7. Du must vom ehren Throne", "tokens": ["Du", "must", "vom", "eh\u00b7ren", "Thro\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Weil keine macht noch krone", "tokens": ["Weil", "kei\u00b7ne", "macht", "noch", "kro\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PIAT", "VVFIN", "ADV", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Kan unverg\u00e4nglich seyn. ", "tokens": ["Kan", "un\u00b7ver\u00b7g\u00e4ng\u00b7lich", "seyn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Es mag vom Todten reyen/", "tokens": ["Es", "mag", "vom", "Tod\u00b7ten", "re\u00b7yen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPRART", "NN", "VVINF", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Kein Zepter dich befreyen.", "tokens": ["Kein", "Zep\u00b7ter", "dich", "be\u00b7fre\u00b7yen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PPER", "VVINF", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Kein Purpur/ Gold/ noch Edler stein.", "tokens": ["Kein", "Pur\u00b7pur", "/", "Gold", "/", "noch", "Ed\u00b7ler", "stein", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$(", "NN", "$(", "ADV", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "8. Wie eine Rose bl\u00fchet/", "tokens": ["Wie", "ei\u00b7ne", "Ro\u00b7se", "bl\u00fc\u00b7het", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wen man die Sonne sihet/ ", "tokens": ["Wen", "man", "die", "Son\u00b7ne", "si\u00b7het", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "ART", "NN", "VVFIN", "$("], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Begr\u00fcssen diese Welt: ", "tokens": ["Be\u00b7gr\u00fcs\u00b7sen", "die\u00b7se", "Welt", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PDAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die ehr der tag sich neiget/", "tokens": ["Die", "ehr", "der", "tag", "sich", "nei\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PRF", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Ehr sich der abendt zeiget/", "tokens": ["Ehr", "sich", "der", "a\u00b7bendt", "zei\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "ART", "ADJD", "VVFIN", "$("], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Verwelckt/ vnd vnversehns abf\u00e4lt. ", "tokens": ["Ver\u00b7welckt", "/", "vnd", "vn\u00b7ver\u00b7sehns", "a