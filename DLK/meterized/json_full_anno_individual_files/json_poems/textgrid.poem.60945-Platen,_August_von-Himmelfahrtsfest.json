{"textgrid.poem.60945": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "Himmelfahrtsfest", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Oft mit dem Auge des Geistes erblick ich den herrlichen Lenztag,", "tokens": ["Oft", "mit", "dem", "Au\u00b7ge", "des", "Geis\u00b7tes", "er\u00b7blick", "ich", "den", "herr\u00b7li\u00b7chen", "Lenz\u00b7tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Sehe vom Volk ringsum Meer und Lagune bedeckt;", "tokens": ["Se\u00b7he", "vom", "Volk", "ring\u00b7sum", "Meer", "und", "La\u00b7gu\u00b7ne", "be\u00b7deckt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "ADV", "NN", "KON", "NN", "VVPP", "$."], "meter": "+--+--+-+---+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Festlich erscheint der Senat in dem pr\u00e4chtigen Bucentauro,", "tokens": ["Fest\u00b7lich", "er\u00b7scheint", "der", "Se\u00b7nat", "in", "dem", "pr\u00e4ch\u00b7ti\u00b7gen", "Bu\u00b7cen\u00b7tau\u00b7ro", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+--+--++-+", "measure": "dactylic.tetra.plus"}, "line.4": {"text": "Barken zu tausend umher, voll von Musik und bekr\u00e4nzt:", "tokens": ["Bar\u00b7ken", "zu", "tau\u00b7send", "um\u00b7her", ",", "voll", "von", "Mu\u00b7sik", "und", "be\u00b7kr\u00e4nzt", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "CARD", "PTKVZ", "$,", "ADJD", "APPR", "NN", "KON", "VVPP", "$."], "meter": "+--+-+-+-+-+-+", "measure": "iambic.septa.invert"}, "line.5": {"text": "Goldschwer wogt er dahin, ihn rudern die Arsenalotten;", "tokens": ["Gold\u00b7schwer", "wogt", "er", "da\u00b7hin", ",", "ihn", "ru\u00b7dern", "die", "Ar\u00b7se\u00b7na\u00b7lot\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PAV", "$,", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.6": {"text": "Diesem entgegen, zu Schiff, eilst du heran, Patriarch!", "tokens": ["Die\u00b7sem", "ent\u00b7ge\u00b7gen", ",", "zu", "Schiff", ",", "eilst", "du", "he\u00b7ran", ",", "Pat\u00b7ri\u00b7arch", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "PTKVZ", "$,", "APPR", "NN", "$,", "VVFIN", "PPER", "PTKVZ", "$,", "NN", "$."], "meter": "+--+-+++-+-+-+", "measure": "iambic.octa.plus.invert"}, "line.7": {"text": "Gie\u00dfest ins Meer Weihwasser und streust lenzduftige Rosen,", "tokens": ["Gie\u00b7\u00dfest", "ins", "Meer", "Weih\u00b7was\u00b7ser", "und", "streust", "lenz\u00b7duf\u00b7ti\u00b7ge", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "NN", "KON", "VVFIN", "ADJA", "NN", "$,"], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.8": {"text": "Dann, in die br\u00e4utliche Flut, schleudert der Doge den Ring.", "tokens": ["Dann", ",", "in", "die", "br\u00e4ut\u00b7li\u00b7che", "Flut", ",", "schleu\u00b7dert", "der", "Do\u00b7ge", "den", "Ring", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPR", "ART", "ADJA", "NN", "$,", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "