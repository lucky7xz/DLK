{"textgrid.poem.49381": {"metadata": {"author": {"name": "Ball, Hugo", "birth": "N.A.", "death": "N.A."}, "title": "Der Schizophrene", "genre": "verse", "period": "N.A.", "pub_year": 1906, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Opfer der Zerst\u00fcckung, ganz besessen", "tokens": ["Ein", "Op\u00b7fer", "der", "Zer\u00b7st\u00fc\u00b7ckung", ",", "ganz", "be\u00b7ses\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "$,", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bin ich \u2013 wie nennt ihr's doch? \u2013 ein Schizophrene.", "tokens": ["Bin", "ich", "\u2013", "wie", "nennt", "ih\u00b7r's", "doch", "?", "\u2013", "ein", "Schi\u00b7zop\u00b7hre\u00b7ne", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$(", "PWAV", "VVFIN", "PPER", "ADV", "$.", "$(", "ART", "NN", "$."], "meter": "+--+-+--+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Ihr wollt, da\u00df ich verschwinde von der Szene,", "tokens": ["Ihr", "wollt", ",", "da\u00df", "ich", "ver\u00b7schwin\u00b7de", "von", "der", "Sze\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "KOUS", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Um euren eigenen Anblick zu vergessen.", "tokens": ["Um", "eu\u00b7ren", "ei\u00b7ge\u00b7nen", "An\u00b7blick", "zu", "ver\u00b7ges\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Ich aber werde eure Worte pressen", "tokens": ["Ich", "a\u00b7ber", "wer\u00b7de", "eu\u00b7re", "Wor\u00b7te", "pres\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VAFIN", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In des Sonettes dunkle Kantilene.", "tokens": ["In", "des", "So\u00b7net\u00b7tes", "dunk\u00b7le", "Kan\u00b7ti\u00b7le\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Es haben meine \u00e4tzenden Arsene", "tokens": ["Es", "ha\u00b7ben", "mei\u00b7ne", "\u00e4t\u00b7zen\u00b7den", "Ar\u00b7se\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Das Blut euch bis zum Herzen schon durchmessen.", "tokens": ["Das", "Blut", "euch", "bis", "zum", "Her\u00b7zen", "schon", "durch\u00b7mes\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "APPR", "APPRART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Des Tages Licht und der Gewohnheit Dauer", "tokens": ["Des", "Ta\u00b7ges", "Licht", "und", "der", "Ge\u00b7wohn\u00b7heit", "Dau\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "KON", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Beh\u00fcten euch mit einer sichern Mauer", "tokens": ["Be\u00b7h\u00fc\u00b7ten", "euch", "mit", "ei\u00b7ner", "si\u00b7chern", "Mau\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, 