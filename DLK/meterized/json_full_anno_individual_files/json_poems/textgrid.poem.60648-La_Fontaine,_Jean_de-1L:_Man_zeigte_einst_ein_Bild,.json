{"textgrid.poem.60648": {"metadata": {"author": {"name": "La Fontaine, Jean de", "birth": "N.A.", "death": "N.A."}, "title": "1L: Man zeigte einst ein Bild,", "genre": "verse", "period": "N.A.", "pub_year": 1658, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Man zeigte einst ein Bild,", "tokens": ["Man", "zeig\u00b7te", "einst", "ein", "Bild", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Auf dem war dargestellt:", "tokens": ["Auf", "dem", "war", "dar\u00b7ge\u00b7stellt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "VAFIN", "VVPP", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Ein L\u00f6we, riesenhaft und wild,", "tokens": ["Ein", "L\u00f6\u00b7we", ",", "rie\u00b7sen\u00b7haft", "und", "wild", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NE", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von einem einzigen Mann gef\u00e4llt.", "tokens": ["Von", "ei\u00b7nem", "ein\u00b7zi\u00b7gen", "Mann", "ge\u00b7f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Die das beschauten, prahlten mit der Menschenst\u00e4rke.", "tokens": ["Die", "das", "be\u00b7schau\u00b7ten", ",", "prahl\u00b7ten", "mit", "der", "Men\u00b7schen\u00b7st\u00e4r\u00b7ke", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "$,", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ein L\u00f6we kam vorbei am bu