{"dta.poem.21094": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Der Morgen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1798", "urn": "urn:nbn:de:kobv:b4-200905193300", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "J&#252;ngling, sey mir gegr&#252;sst! &#220;ber die Sch\u00f6pfungen               ", "tokens": ["J", "&#252;", "ng\u00b7ling", ",", "sey", "mir", "ge\u00b7gr", "&#252;", "sst", "!", "&#220;", "ber", "die", "Sch\u00f6p\u00b7fun\u00b7gen"], "token_info": ["word", "XML_entity", "word", "punct", "word", "word", "word", "XML_entity", "word", "punct", "XML_entity", "word", "word", "word"], "pos": ["NE", "$(", "VVFIN", "$,", "VAFIN", "PPER", "NE", "$(", "VVFIN", "$.", "$(", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Schwebst du t\u00f6nenden Schwungs freudig und stolz", "tokens": ["Schwebst", "du", "t\u00f6\u00b7nen\u00b7den", "Schwungs", "freu\u00b7dig", "und", "stolz"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJA", "NN", "ADJD", "KON", "ADJD"], "meter": "+-+--++--+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "daher!", "tokens": ["da\u00b7her", "!"], "token_info": ["word", "punct"], "pos": ["PAV", "$."], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "Deine Wange, wie gl\u00fcht sie", "tokens": ["Dei\u00b7ne", "Wan\u00b7ge", ",", "wie", "gl\u00fcht", "sie"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PWAV", "VVFIN", "PPER"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "In den Gluthen des Morgenroths!", "tokens": ["In", "den", "Glu\u00b7then", "des", "Mor\u00b7gen\u00b7roths", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Dein gelbringelndes Haar, deinen weitwallenden", "tokens": ["Dein", "gel\u00b7brin\u00b7geln\u00b7des", "Haar", ",", "dei\u00b7nen", "weit\u00b7wal\u00b7len\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PPOSAT", "ADJA"], "meter": "+-+--++--+--", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Safranmantel, ihn schwellt, siehe! der Morgenwind,", "tokens": ["Saf\u00b7ran\u00b7man\u00b7tel", ",", "ihn", "schwellt", ",", "sie\u00b7he", "!", "der", "Mor\u00b7gen\u00b7wind", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "$,", "VVIMP", "$.", "ART", "NN", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Und entbl\u00e4ttert die Rosen,", "tokens": ["Und", "ent\u00b7bl\u00e4t\u00b7tert", "die", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Die dir kr\u00e4nzen den hellen Schlaf.", "tokens": ["Die", "dir", "kr\u00e4n\u00b7zen", "den", "hel\u00b7len", "Schlaf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.3": {"line.1": {"text": "Dir, Unsterblicher, dir feyert die junge Welt,", "tokens": ["Dir", ",", "U\u00b7nsterb\u00b7li\u00b7cher", ",", "dir", "fe\u00b7yert", "die", "jun\u00b7ge", "Welt", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ADJD", "$,", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+---+-+-+-+", "measure": "dactylic.init"}, "line.2": {"text": "Dir der spiegelnde See, dir der entbrannte Wald,", "tokens": ["Dir", "der", "spie\u00b7geln\u00b7de", "See", ",", "dir", "der", "ent\u00b7brann\u00b7te", "Wald", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "$,", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Dir der sonnige H\u00fcgel,", "tokens": ["Dir", "der", "son\u00b7ni\u00b7ge", "H\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Dir die perlenbes\u00e4'te Flur.", "tokens": ["Dir", "die", "per\u00b7len\u00b7be\u00b7s\u00e4'\u00b7te", "Flur", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.4": {"line.1": {"text": "Dir, Unsterblicher, dir jubelt mein Saitenspiel.", "tokens": ["Dir", ",", "U\u00b7nsterb\u00b7li\u00b7cher", ",", "dir", "ju\u00b7belt", "mein", "Sai\u00b7ten\u00b7spiel", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ADJD", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+----+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Innig liebt dich mein Herz. Weckt nicht dein leiser", "tokens": ["In\u00b7nig", "liebt", "dich", "mein", "Herz", ".", "Weckt", "nicht", "dein", "lei\u00b7ser"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$.", "VVFIN", "PTKNEG", "PPOSAT", "ADJA"], "meter": "---+-+-+-+-", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Kuss", "tokens": ["Kuss"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Mich aus l\u00e4hmendem Schlummer", "tokens": ["Mich", "aus", "l\u00e4h\u00b7men\u00b7dem", "Schlum\u00b7mer"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "APPR", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "In des Daseyns Entz\u00fcckungen?", "tokens": ["In", "des", "Da\u00b7seyns", "Ent\u00b7z\u00fc\u00b7ckun\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+---+-", "measure": "unknown.measure.tri"}}, "stanza.5": {"line.1": {"text": "Heil dir, Strahlender, Heil! G\u00fcrte, so oft du", "tokens": ["Heil", "dir", ",", "Strah\u00b7len\u00b7der", ",", "Heil", "!", "G\u00fcr\u00b7te", ",", "so", "oft", "du"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "PPER", "$,", "NN", "$,", "NN", "$.", "NN", "$,", "ADV", "ADV", "PPER"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "kehrst,", "tokens": ["kehrst", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Meine H\u00fcfte mit Kraft. St\u00e4hle zu festem Trotz", "tokens": ["Mei\u00b7ne", "H\u00fcf\u00b7te", "mit", "Kraft", ".", "St\u00e4h\u00b7le", "zu", "fes\u00b7tem", "Trotz"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPR", "NN", "$.", "NN", "APPR", "ADJA", "NN"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.4": {"text": "Meine Schenkel. Erf\u00fclle", "tokens": ["Mei\u00b7ne", "Schen\u00b7kel", ".", "Er\u00b7f\u00fcl\u00b7le"], "token_info": ["word", "word", "punct", "word"], "pos": ["PPOSAT", "NN", "$.", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "Meine R\u00f6hren mit L\u00f6wenmark.", "tokens": ["Mei\u00b7ne", "R\u00f6h\u00b7ren", "mit", "L\u00f6\u00b7wen\u00b7mark", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.6": {"line.1": {"text": "Deine Jugend verwelkt nimmer. Die meinige", "tokens": ["Dei\u00b7ne", "Ju\u00b7gend", "ver\u00b7welkt", "nim\u00b7mer", ".", "Die", "mei\u00b7ni\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "$.", "ART", "PPOSS"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Welkt in Kurzem. Nicht lang, siehe, so suchest du", "tokens": ["Welkt", "in", "Kur\u00b7zem", ".", "Nicht", "lang", ",", "sie\u00b7he", ",", "so", "su\u00b7chest", "du"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "$.", "PTKNEG", "ADJD", "$,", "VVIMP", "$,", "ADV", "VVFIN", "PPER"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Mich vergebens im Felde,", "tokens": ["Mich", "ver\u00b7ge\u00b7bens", "im", "Fel\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPRART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Rufst vergebens dem Schlummerer.", "tokens": ["Rufst", "ver\u00b7ge\u00b7bens", "dem", "Schlum\u00b7me\u00b7rer", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$."], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "Tief im Staub' ist mein Schlaf, niedrig mein gr\u00fcnend", "tokens": ["Tief", "im", "Staub'", "ist", "mein", "Schlaf", ",", "nied\u00b7rig", "mein", "gr\u00fc\u00b7nend"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "APPRART", "NN", "VAFIN", "PPOSAT", "NN", "$,", "ADJD", "PPOSAT", "NN"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Haus.", "tokens": ["Haus", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Thaue Thr\u00e4nen darauf, Holder, und r\u00f6th' es sanft,", "tokens": ["Thau\u00b7e", "Thr\u00e4\u00b7nen", "da\u00b7rauf", ",", "Hol\u00b7der", ",", "und", "r\u00f6\u00b7th'", "es", "sanft", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PAV", "$,", "NN", "$,", "KON", "VVFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Bis dein himmlischer Bruder", "tokens": ["Bis", "dein", "himm\u00b7li\u00b7scher", "Bru\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "Mich zum ewigen Tage weckt.", "tokens": ["Mich", "zum", "e\u00b7wi\u00b7gen", "Ta\u00b7ge", "weckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}}}}