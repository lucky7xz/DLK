{"textgrid.poem.24917": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Ragnars Tod", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Geschlagen ist die blutige Schlacht;", "tokens": ["Ge\u00b7schla\u00b7gen", "ist", "die", "blu\u00b7ti\u00b7ge", "Schlacht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Zu Fall hat Ragnar die Feinde gebracht", "tokens": ["Zu", "Fall", "hat", "Rag\u00b7nar", "die", "Fein\u00b7de", "ge\u00b7bracht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VAFIN", "NE", "ART", "NN", "VVPP"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und errungen ein K\u00f6nigreich.", "tokens": ["Und", "er\u00b7run\u00b7gen", "ein", "K\u00f6\u00b7nig\u00b7reich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Des Wegs nun zieht er zur Kr\u00f6nungsstadt,", "tokens": ["Des", "Wegs", "nun", "zieht", "er", "zur", "Kr\u00f6\u00b7nungs\u00b7stadt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Um ihn, vom Kampfe des Tages matt,", "tokens": ["Um", "ihn", ",", "vom", "Kamp\u00b7fe", "des", "Ta\u00b7ges", "matt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "$,", "APPRART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Die Seinen blutend und bleich.", "tokens": ["Die", "Sei\u00b7nen", "blu\u00b7tend", "und", "bleich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "KON", "ADJD", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Stolz wallen die Banner im Abendschein;", "tokens": ["Stolz", "wal\u00b7len", "die", "Ban\u00b7ner", "im", "A\u00b7bend\u00b7schein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Doch bang hinfl\u00fcstert es durch die Reihn:", "tokens": ["Doch", "bang", "hin\u00b7fl\u00fcs\u00b7tert", "es", "durch", "die", "Reihn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "\u00bbweh, weh um Ragnar den Guten!", "tokens": ["\u00bb", "weh", ",", "weh", "um", "Rag\u00b7nar", "den", "Gu\u00b7ten", "!"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKVZ", "$,", "ADV", "APPR", "NE", "ART", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Seht, wie an der Brust ihm die Wunde klafft!", "tokens": ["Seht", ",", "wie", "an", "der", "Brust", "ihm", "die", "Wun\u00b7de", "klafft", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "APPR", "ART", "NN", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Die Rechte pre\u00dft er darauf mit Kraft,", "tokens": ["Die", "Rech\u00b7te", "pre\u00dft", "er", "da\u00b7rauf", "mit", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PAV", "APPR", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Um nicht zu fr\u00fch zu verbluten!\u00ab", "tokens": ["Um", "nicht", "zu", "fr\u00fch", "zu", "ver\u00b7blu\u00b7ten", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUI", "PTKNEG", "PTKA", "ADJD", "PTKZU", "VVINF", "$.", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Er starrt zu Boden und reitet f\u00fcrba\u00df.", "tokens": ["Er", "starrt", "zu", "Bo\u00b7den", "und", "rei\u00b7tet", "f\u00fcr\u00b7ba\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "VVFIN", "PTKVZ", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "\u00bbo Herr! Gebieter! wie schaust du so bla\u00df?\u00ab", "tokens": ["\u00bb", "o", "Herr", "!", "Ge\u00b7bie\u00b7ter", "!", "wie", "schaust", "du", "so", "bla\u00df", "?", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "FM", "NN", "$.", "NN", "$.", "PWAV", "VVFIN", "PPER", "ADV", "ADJD", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch Ragnar blickt nicht empor;", "tokens": ["Doch", "Rag\u00b7nar", "blickt", "nicht", "em\u00b7por", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Mag bluten sein Renner mit wankendem Schritt,", "tokens": ["Mag", "blu\u00b7ten", "sein", "Ren\u00b7ner", "mit", "wan\u00b7ken\u00b7dem", "Schritt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Er spornt ihn zur Eile und hemmt nicht den Ritt,", "tokens": ["Er", "spornt", "ihn", "zur", "Ei\u00b7le", "und", "hemmt", "nicht", "den", "Ritt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "NN", "KON", "VVFIN", "PTKNEG", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Bis er steht an der Hauptstadt Thor.", "tokens": ["Bis", "er", "steht", "an", "der", "Haupt\u00b7stadt", "Thor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.4": {"line.1": {"text": "Hinunter sich schwingt er vom st\u00fcrzenden Ro\u00df", "tokens": ["Hin\u00b7un\u00b7ter", "sich", "schwingt", "er", "vom", "st\u00fcr\u00b7zen\u00b7den", "Ro\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PRF", "VVFIN", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Und schreitet die Stufen hinauf zum Schlo\u00df,", "tokens": ["Und", "schrei\u00b7tet", "die", "Stu\u00b7fen", "hin\u00b7auf", "zum", "Schlo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Gest\u00fctzt auf den weinenden Sohn;", "tokens": ["Ge\u00b7st\u00fctzt", "auf", "den", "wei\u00b7nen\u00b7den", "Sohn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Hell flammen im Saale die Fackeln ringsum,", "tokens": ["Hell", "flam\u00b7men", "im", "Saa\u00b7le", "die", "Fa\u00b7ckeln", "ring\u00b7sum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPRART", "NN", "ART", "NN", "ADV", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Und durch die Reihen der Ritter stumm", "tokens": ["Und", "durch", "die", "Rei\u00b7hen", "der", "Rit\u00b7ter", "stumm"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "ADJD"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Steigt Ragnar empor zum Thron.", "tokens": ["Steigt", "Rag\u00b7nar", "em\u00b7por", "zum", "Thron", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "PTKVZ", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Und als er aufs Haupt die Krone sich dr\u00fcckt,", "tokens": ["Und", "als", "er", "aufs", "Haupt", "die", "Kro\u00b7ne", "sich", "dr\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPRART", "NN", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da steht er wie neu von Leben durchz\u00fcckt,", "tokens": ["Da", "steht", "er", "wie", "neu", "von", "Le\u00b7ben", "durch\u00b7z\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KOKOM", "ADJD", "APPR", "NN", "VVPP", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Sein Auge in Glut erglommen;", "tokens": ["Sein", "Au\u00b7ge", "in", "Glut", "er\u00b7glom\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Gebietend heischt er hin durch den Saal:", "tokens": ["Ge\u00b7bie\u00b7tend", "heischt", "er", "hin", "durch", "den", "Saal", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "\u00bbherolde! nun dient mir zum letztenmal", "tokens": ["\u00bb", "he\u00b7rol\u00b7de", "!", "nun", "dient", "mir", "zum", "letz\u00b7ten\u00b7mal"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "VVFIN", "$.", "ADV", "VVFIN", "PPER", "APPRART", "ADV"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Und k\u00fcndet den Toten mein Kommen!", "tokens": ["Und", "k\u00fcn\u00b7det", "den", "To\u00b7ten", "mein", "Kom\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.6": {"line.1": {"text": "Zu tief ist die Wunde, sie heilet nicht;", "tokens": ["Zu", "tief", "ist", "die", "Wun\u00b7de", ",", "sie", "hei\u00b7let", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "VAFIN", "ART", "NN", "$,", "PPER", "VVFIN", "PTKNEG", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Doch k\u00fchn nun kann ich vors Angesicht", "tokens": ["Doch", "k\u00fchn", "nun", "kann", "ich", "vors", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "ADV", "VMFIN", "PPER", "APPRART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Der tapferen Ahnen treten;", "tokens": ["Der", "tap\u00b7fe\u00b7ren", "Ah\u00b7nen", "tre\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Eine Krone lass' ich dem Sohne mein", "tokens": ["Ei\u00b7ne", "Kro\u00b7ne", "lass'", "ich", "dem", "Soh\u00b7ne", "mein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "ART", "NN", "PPOSAT"], "meter": "+-+----+-+", "measure": "unknown.measure.tetra"}, "line.5": {"text": "Und zieh' als K\u00f6nig in Walhall ein \u2013", "tokens": ["Und", "zieh'", "als", "K\u00f6\u00b7nig", "in", "Wal\u00b7hall", "ein", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KOUS", "NN", "APPR", "NN", "ART", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Herolde, blast die Drommeten!\u00ab", "tokens": ["He\u00b7rol\u00b7de", ",", "blast", "die", "Drom\u00b7me\u00b7ten", "!", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NN", "$,", "VVFIN", "ART", "NN", "$.", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "Von der Wunde rei\u00dft er die Hand geschwind;", "tokens": ["Von", "der", "Wun\u00b7de", "rei\u00dft", "er", "die", "Hand", "ge\u00b7schwind", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "ART", "NN", "VVPP", "$."], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Laut schmettert das Erz; sein Leben verrinnt", "tokens": ["Laut", "schmet\u00b7tert", "das", "Erz", ";", "sein", "Le\u00b7ben", "ver\u00b7rinnt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "VVFIN", "ART", "NN", "$.", "PPOSAT", "NN", "VVFIN"], "meter": "-+--+-+-