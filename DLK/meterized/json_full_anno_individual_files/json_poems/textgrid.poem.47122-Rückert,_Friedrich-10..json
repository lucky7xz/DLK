 ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "NN", "VAFIN", "PRF", "PTKNEG", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Doch, da\u00df alles steht beim Alten eben,", "tokens": ["Doch", ",", "da\u00df", "al\u00b7les", "steht", "beim", "Al\u00b7ten", "e\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PIS", "VVFIN", "APPRART", "NN", "ADV", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.11": {"text": "Dieses wissen, das sich stets vom neuen", "tokens": ["Die\u00b7ses", "wis\u00b7sen", ",", "das", "sich", "stets", "vom", "neu\u00b7en"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVINF", "$,", "PRELS", "PRF", "ADV", "APPRART", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.12": {"text": "Sagen, kann nur Liebende erfreuen.", "tokens": ["Sa\u00b7gen", ",", "kann", "nur", "Lie\u00b7ben\u00b7de", "er\u00b7freu\u00b7en", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VMFIN", "ADV", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.13": {"text": "Ja, es ist kein andrer Trost geblieben", "tokens": ["Ja", ",", "es", "ist", "kein", "an\u00b7drer", "Trost", "ge\u00b7blie\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "PPER", "VAFIN", "PIAT", "ADJA", "NN", "VVPP"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.14": {"text": "Zweien, die sich fern sind und sich lieben,", "tokens": ["Zwei\u00b7en", ",", "die", "sich", "fern", "sind", "und", "sich", "lie\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PRF", "ADJD", "VAFIN", "KON", "PRF", "VVINF", "$,"], "meter": "----+-+-+-", "measure": "unknown.measure.tri"}, "line.15": {"text": "Als der Seele Jubel und die Klagen,", "tokens": ["Als", "der", "See\u00b7le", "Ju\u00b7bel", "und", "die", "Kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "KON", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.16": {"text": "Was der Mund nicht kann dem Munde sagen,", "tokens": ["Was", "der", "Mund", "nicht", "kann", "dem", "Mun\u00b7de", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "PTKNEG", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.17": {"text": "Einem stummen Blatt es anvertrauen,", "tokens": ["Ei\u00b7nem", "stum\u00b7men", "Blatt", "es", "an\u00b7ver\u00b7trau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.18": {"text": "Schreiben es und es geschrieben schauen.", "tokens": ["Schrei\u00b7ben", "es", "und", "es", "ge\u00b7schrie\u00b7ben", "schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "KON", "PPER", "VVPP", "VVINF", "$."], "meter": "+-+---+-+-", "measure": "unknown.measure.tetra"}}}}}