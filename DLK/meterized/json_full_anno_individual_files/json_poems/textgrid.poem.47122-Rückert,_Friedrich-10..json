{"textgrid.poem.47122": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "10.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Jetzo blickt sie nach dem Abendrote,", "tokens": ["Jet\u00b7zo", "blickt", "sie", "nach", "dem", "A\u00b7ben\u00b7dro\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ob mit ihm erscheinen wird der Bote,", "tokens": ["Ob", "mit", "ihm", "er\u00b7schei\u00b7nen", "wird", "der", "Bo\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPER", "VVINF", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Ihr des Liebsten ersten Brief zu bringen:", "tokens": ["Ihr", "des", "Liebs\u00b7ten", "ers\u00b7ten", "Brief", "zu", "brin\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "\u00bbh\u00e4ttest du doch meiner Sehnsucht Schwingen!\u00ab", "tokens": ["\u00bb", "h\u00e4t\u00b7test", "du", "doch", "mei\u00b7ner", "Sehn\u00b7sucht", "Schwin\u00b7gen", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VAFIN", "PPER", "ADV", "PPOSAT", "NN", "NN", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Und es sinkt die Nacht, der Bote weilet;", "tokens": ["Und", "es", "sinkt", "die", "Nacht", ",", "der", "Bo\u00b7te", "wei\u00b7let", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$,", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Und er kommt, dem sie entgegeneilet.", "tokens": ["Und", "er", "kommt", ",", "dem", "sie", "ent\u00b7ge\u00b7ge\u00b7nei\u00b7let", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Und sie hat des Liebsten Brief erhalten,", "tokens": ["Und", "sie", "hat", "des", "Liebs\u00b7ten", "Brief", "er\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "S\u00e4umet, auseinander ihn zu falten,", "tokens": ["S\u00e4u\u00b7met", ",", "aus\u00b7ein\u00b7an\u00b7der", "ihn", "zu", "fal\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "APPR", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "Mu\u00df die Aufschrift, ihren Namen, lesen,", "tokens": ["Mu\u00df", "die", "Auf\u00b7schrift", ",", "ih\u00b7ren", "Na\u00b7men", ",", "le\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "$,", "PPOSAT", "NN", "$,", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Der ihr selber nie so sch\u00f6n gewesen.", "tokens": ["Der", "ihr", "sel\u00b7ber", "nie", "so", "sch\u00f6n", "ge\u00b7we\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADV", "ADV", "ADJD", "VAPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.11": {"text": "Und nun ruhen auf der Schrift die Augen,", "tokens": ["Und", "nun", "ru\u00b7hen", "auf", "der", "Schrift", "die", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.12": {"text": "Alle Z\u00fcge liebend einzusaugen,", "tokens": ["Al\u00b7le", "Z\u00fc\u00b7ge", "lie\u00b7bend", "ein\u00b7zu\u00b7sau\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.13": {"text": "Die f\u00fcr sie des Liebsten Hand gezogen,", "tokens": ["Die", "f\u00fcr", "sie", "des", "Liebs\u00b7ten", "Hand", "ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.14": {"text": "Jede Zeil' ein Liebesregenbogen,", "tokens": ["Je\u00b7de", "Zeil'", "ein", "Lie\u00b7bes\u00b7re\u00b7gen\u00b7bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.15": {"text": "Jedes Wort ein lichter Stern im Blaue,", "tokens": ["Je\u00b7des", "Wort", "ein", "lich\u00b7ter", "Stern", "im", "Blau\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "ADJA", "NN", "APPRART", "ADJA", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.16": {"text": "Jeder Buchstab' eine Ros' im Taue.", "tokens": ["Je\u00b7der", "Buch\u00b7stab'", "ei\u00b7ne", "Ros'", "im", "Tau\u00b7e", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.17": {"text": "So versch\u00f6nt zu einer Liebesbl\u00fcte", "tokens": ["So", "ver\u00b7sch\u00f6nt", "zu", "ei\u00b7ner", "Lie\u00b7bes\u00b7bl\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.18": {"text": "Sich das Blatt dem liebenden Gem\u00fcte.", "tokens": ["Sich", "das", "Blatt", "dem", "lie\u00b7ben\u00b7den", "Ge\u00b7m\u00fc\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.19": {"text": "Und nun sitzt sie, gleich zu schreiben, nieder.", "tokens": ["Und", "nun", "sitzt", "sie", ",", "gleich", "zu", "schrei\u00b7ben", ",", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "$,", "ADV", "PTKZU", "VVINF", "$,", "PTKVZ", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.20": {"text": "Gib, o Nacht, dein tauiges Gefieder", "tokens": ["Gib", ",", "o", "Nacht", ",", "dein", "tau\u00b7i\u00b7ges", "Ge\u00b7fie\u00b7der"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "$,", "FM", "NN", "$,", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.21": {"text": "Ihrem Blatt, da\u00df mit dem Morgenrote", "tokens": ["Ih\u00b7rem", "Blatt", ",", "da\u00df", "mit", "dem", "Mor\u00b7gen\u00b7ro\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "KOUS", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.22": {"text": "Mir zur\u00fcck gefl\u00fcgelt sei der Bote!", "tokens": ["Mir", "zu\u00b7r\u00fcck", "ge\u00b7fl\u00fc\u00b7gelt", "sei", "der", "Bo\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "VVPP", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Herz! wie soll die Ungeduld ich nennen,", "tokens": ["Herz", "!", "wie", "soll", "die", "Un\u00b7ge\u00b7duld", "ich", "nen\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PWAV", "VMFIN", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Da von ihr dich nur zwei Tage trennen,", "tokens": ["Da", "von", "ihr", "dich", "nur", "zwei", "Ta\u00b7ge", "tren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "PRF", "ADV", "CARD", "NN", "VVINF", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Da von ihr dich trennen nur zwei Meilen,", "tokens": ["Da", "von", "ihr", "dich", "tren\u00b7nen", "nur", "zwei", "Mei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "PPER", "VVFIN", "ADV", "CARD", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Da\u00df von ihrer Hand nach zweien Zeilen", "tokens": ["Da\u00df", "von", "ih\u00b7rer", "Hand", "nach", "zwei\u00b7en", "Zei\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PPOSAT", "NN", "APPR", "CARD", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Geizest so mit ungest\u00fcmem Drange?", "tokens": ["Gei\u00b7zest", "so", "mit", "un\u00b7ge\u00b7st\u00fc\u00b7mem", "Dran\u00b7ge", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Was sie schreiben wird, du wei\u00dft es lange;", "tokens": ["Was", "sie", "schrei\u00b7ben", "wird", ",", "du", "wei\u00dft", "es", "lan\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVINF", "VAFIN", "$,", "PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Und sie wei\u00df es wohl, was du wirst schreiben:", "tokens": ["Und", "sie", "wei\u00df", "es", "wohl", ",", "was", "du", "wirst", "schrei\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADV", "$,", "PWS", "PPER", "VAFIN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Und so k\u00f6nnt' es billig unterbleiben.", "tokens": ["Und", "so", "k\u00f6nnt'", "es", "bil\u00b7lig", "un\u00b7ter\u00b7blei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "Freilich, Neues hat sich nicht begeben;", "tokens": ["Frei\u00b7lich", ",", "Neu\u00b7es", "hat", "sich", "nicht", "be\u00b7ge\u00b7ben", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "NN", "VAFIN", "PRF", "PTKNEG", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Doch, da\u00df alles steht beim Alten eben,", "tokens": ["Doch", ",", "da\u00df", "al\u00b7les", "steht", "beim", "Al\u00b7ten", "e\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PIS", "VVFIN", "APPRART", "NN", "ADV", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.11": {"text": "Dieses wissen, das sich stets vom neuen", "tokens": ["Die\u00b7ses", "wis\u00b7sen", ",", "das", "sich", "stets", "vom", "neu\u00b7en"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVINF", "$,", "PRELS", "PRF", "ADV", "APPRART", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.12": {"text": "Sagen, kann nur Liebende erfreuen.", "tokens": ["Sa\u00b7gen", ",", "kann", "nur", "Lie\u00b7ben\u00b7de", "er\u00b7freu\u00b7en", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VMFIN", "ADV", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.13": {"text": "Ja, es ist kein andrer Trost geblieben", "tokens": ["Ja", ",", "es", "ist", "kein", "an\u00b7drer", "Trost", "ge\u00b7blie\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "PPER", "VAFIN", "PIAT", "ADJA", "NN", "VVPP"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.14": {"text": "Zweien, die sich fern sind und sich lieben,", "tokens": ["Zwei\u00b7en", ",", "die", "sich", "fern", "sind", "und", "sich", "lie\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PRF", "ADJD", "VAFIN", "KON", "PRF", "VVINF", "$,"], "meter": "----+-+-+-", "measure": "unknown.measure.tri"}, "line.15": {"text": "Als der Seele Jubel und die Klagen,", "tokens": ["Als", "der", "See\u00b7le", "Ju\u00b7bel", "und", "die", "Kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "KON", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.16": {"text": "Was der Mund nicht kann dem Munde sagen,", "tokens": ["Was", "der", "Mund", "nicht", "kann", "dem", "Mun\u00b7de", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "PTKNEG", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.17": {"text": "Einem stummen Blatt es anvertrauen,", "tokens": ["Ei\u00b7nem", "stum\u00b7men", "Blatt", "es", "an\u00b7ver\u00b7trau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.18": {"text": "Schreiben es und es geschrieben schauen.", "tokens": ["Schrei\u00b7ben", "es", "und", "es", "ge\u00b7schrie\u00b7ben", "schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "KON", "PPER", "VVPP", "VVINF", "$."], "meter": "+-+---+-+-", "measure": "unknown.measure.tetra"}}, "stanza.3": {"line.1": {"text": "Jetzo blickt sie nach dem Abendrote,", "tokens": ["Jet\u00b7zo", "blickt", "sie", "nach", "dem", "A\u00b7ben\u00b7dro\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ob mit ihm erscheinen wird der Bote,", "tokens": ["Ob", "mit", "ihm", "er\u00b7schei\u00b7nen", "wird", "der", "Bo\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPER", "VVINF", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Ihr des Liebsten ersten Brief zu bringen:", "tokens": ["Ihr", "des", "Liebs\u00b7ten", "ers\u00b7ten", "Brief", "zu", "brin\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "\u00bbh\u00e4ttest du doch mei