{"textgrid.poem.39979": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "7. Wer der Appenzeller Feldhauptmann ward", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Drau\u00dfen tagt die Landsgemeine", "tokens": ["Drau\u00b7\u00dfen", "tagt", "die", "Lands\u00b7ge\u00b7mei\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wieder in dem Wiesenthal,", "tokens": ["Wie\u00b7der", "in", "dem", "Wie\u00b7sent\u00b7hal", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn es sammeln sich am Rheine", "tokens": ["Denn", "es", "sam\u00b7meln", "sich", "am", "Rhei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PRF", "APPRART", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Stolze Ritter ohne Zahl.", "tokens": ["Stol\u00b7ze", "Rit\u00b7ter", "oh\u00b7ne", "Zahl", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "K\u00e4mpfen sollen sie schon morgen,", "tokens": ["K\u00e4mp\u00b7fen", "sol\u00b7len", "sie", "schon", "mor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "ADV", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Arm und Waffen sind bereit,", "tokens": ["Arm", "und", "Waf\u00b7fen", "sind", "be\u00b7reit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VAFIN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Eins nur fragen sie mit Sorgen:", "tokens": ["Eins", "nur", "fra\u00b7gen", "sie", "mit", "Sor\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wer soll F\u00fchrer sein im Streit?", "tokens": ["Wer", "soll", "F\u00fch\u00b7rer", "sein", "im", "Streit", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "NN", "PPOSAT", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Eh' sie den gefunden haben,", "tokens": ["Eh'", "sie", "den", "ge\u00b7fun\u00b7den", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "VVPP", "VAINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sehn die Rotten durch das Feld", "tokens": ["Sehn", "die", "Rot\u00b7ten", "durch", "das", "Feld"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Einen schlanken Reiter traben,", "tokens": ["Ei\u00b7nen", "schlan\u00b7ken", "Rei\u00b7ter", "tra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "R\u00fcstig wie ein Kriegesheld.", "tokens": ["R\u00fcs\u00b7tig", "wie", "ein", "Krie\u00b7ge\u00b7sheld", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Den schm\u00fcckt herrliches Geschmeide!", "tokens": ["Den", "schm\u00fcckt", "herr\u00b7li\u00b7ches", "Ge\u00b7schmei\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADJA", "NN", "$."], "meter": "-++-+-+-", "measure": "unknown.measure.tetra"}, "line.6": {"text": "M\u00e4nner, h\u00f6rt! das ist kein Hirt,", "tokens": ["M\u00e4n\u00b7ner", ",", "h\u00f6rt", "!", "das", "ist", "kein", "Hirt", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$.", "PDS", "VAFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Der in seinem Herrenkleide", "tokens": ["Der", "in", "sei\u00b7nem", "Her\u00b7ren\u00b7klei\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Sich in unsern Rat verirrt.", "tokens": ["Sich", "in", "un\u00b7sern", "Rat", "ver\u00b7irrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ei! das lie\u00df Herr Anderhalde", "tokens": ["Ei", "!", "das", "lie\u00df", "Herr", "An\u00b7der\u00b7hal\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "PDS", "VVFIN", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Doch nicht tr\u00e4umen sich im Schlaf!", "tokens": ["Doch", "nicht", "tr\u00e4u\u00b7men", "sich", "im", "Schlaf", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "VVFIN", "PRF", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dr\u00fcben aus der Burg am Walde", "tokens": ["Dr\u00fc\u00b7ben", "aus", "der", "Burg", "am", "Wal\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist's der Werdenberger Graf;", "tokens": ["Ist's", "der", "Wer\u00b7den\u00b7ber\u00b7ger", "Graf", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "NE", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.5": {"text": "H\u00e4lt und steigt von seinem Pferde,", "tokens": ["H\u00e4lt", "und", "steigt", "von", "sei\u00b7nem", "Pfer\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Naht den Hirten ohne Trutz,", "tokens": ["Naht", "den", "Hir\u00b7ten", "oh\u00b7ne", "Trutz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "An der armen Bauern Heerde", "tokens": ["An", "der", "ar\u00b7men", "Bau\u00b7ern", "Heer\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Sucht der edle Ritter Schutz.", "tokens": ["Sucht", "der", "ed\u00b7le", "Rit\u00b7ter", "Schutz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Und er sprach: \u00bbMir kam zu Ohren,", "tokens": ["Und", "er", "sprach", ":", "\u00bb", "Mir", "kam", "zu", "Oh\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$.", "$(", "PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df euch Oesterreich bekriegt,", "tokens": ["Da\u00df", "euch", "O\u00b7es\u00b7ter\u00b7reich", "be\u00b7kriegt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NE", "VVFIN", "$,"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.3": {"text": "Bin ich euch zu hochgeboren,", "tokens": ["Bin", "ich", "euch", "zu", "hoch\u00b7ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nachbarn, da\u00df ihr mir's verschwiegt?", "tokens": ["Nach\u00b7barn", ",", "da\u00df", "ihr", "mir's", "ver\u00b7schwiegt", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "NE", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wisset nur, ich bin vertrieben,", "tokens": ["Wis\u00b7set", "nur", ",", "ich", "bin", "ver\u00b7trie\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "PPER", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Bin ein arm und einsam Haupt!", "tokens": ["Bin", "ein", "arm", "und", "ein\u00b7sam", "Haupt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJD", "KON", "ADJD", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Was vom Erbe mir geblieben,", "tokens": ["Was", "vom", "Er\u00b7be", "mir", "ge\u00b7blie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPRART", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Hat der Herzog mir geraubt!", "tokens": ["Hat", "der", "Her\u00b7zog", "mir", "ge\u00b7raubt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PPER", "VVPP", "$."], "meter": "+-++--+", "measure": "iambic.tetra.chol"}}, "stanza.5": {"line.1": {"text": "Ihr seid frei und reich zu nennen,", "tokens": ["Ihr", "seid", "frei", "und", "reich", "zu", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "KON", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-++-+-+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Ich bin \u00e4rmer als ein Knecht,", "tokens": ["Ich", "bin", "\u00e4r\u00b7mer", "als", "ein", "Knecht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Eure Namen wird man kennen,", "tokens": ["Eu\u00b7re", "Na\u00b7men", "wird", "man", "ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ausgebl\u00fcht hat mein Geschlecht.", "tokens": ["Aus\u00b7ge\u00b7bl\u00fcht", "hat", "mein", "Ge\u00b7schlecht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Stolze Herren m\u00f6gt ihr hassen,", "tokens": ["Stol\u00b7ze", "Her\u00b7ren", "m\u00f6gt", "ihr", "has\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ich bin nicht des Hasses wert,", "tokens": ["Ich", "bin", "nicht", "des", "Has\u00b7ses", "wert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Nichts hat mir der Feind gelassen,", "tokens": ["Nichts", "hat", "mir", "der", "Feind", "ge\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Als mein Herz und als mein Schwert.", "tokens": ["Als", "mein", "Herz", "und", "als", "mein", "Schwert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "KON", "KOUS", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Kann ein Ritterschwert euch frommen", "tokens": ["Kann", "ein", "Rit\u00b7ter\u00b7schwert", "euch", "from\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "ART", "NN", "PPER", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und ein Herz von Zorn entbrannt,", "tokens": ["Und", "ein", "Herz", "von", "Zorn", "ent\u00b7brannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nun so hei\u00dft auch mich willkommen,", "tokens": ["Nun", "so", "hei\u00dft", "auch", "mich", "will\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ADV", "PPER", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "La\u00dft mich schirmen euer Land.", "tokens": ["La\u00dft", "mich", "schir\u00b7men", "eu\u00b7er", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wenn der Streit ist ausgestritten,", "tokens": ["Wenn", "der", "Streit", "ist", "aus\u00b7ge\u00b7strit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "G\u00f6nnt mir eures Thales Rast,", "tokens": ["G\u00f6nnt", "mir", "eu\u00b7res", "Tha\u00b7les", "Rast", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Nehmt mich auf in eure H\u00fctten,", "tokens": ["Nehmt", "mich", "auf", "in", "eu\u00b7re", "H\u00fct\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Pfropft mich auf den wilden Ast!\u00ab", "tokens": ["Pfropft", "mich", "auf", "den", "wil\u00b7den", "Ast", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "PPER", "APPR", "ART", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Spricht's und l\u00f6st die goldne Scheide", "tokens": ["Spricht's", "und", "l\u00f6st", "die", "gold\u00b7ne", "Schei\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Seines Schwertes aus dem Gurt,", "tokens": ["Sei\u00b7nes", "Schwer\u00b7tes", "aus", "dem", "Gurt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Rei\u00dft den Wappenschild vom Kleide,", "tokens": ["Rei\u00dft", "den", "Wap\u00b7pen\u00b7schild", "vom", "Klei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Vor dem Volk, das freudig murrt.", "tokens": ["Vor", "dem", "Volk", ",", "das", "freu\u00b7dig", "murrt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Pfl\u00fcckt den Federschmuck des Hutes,", "tokens": ["Pfl\u00fcckt", "den", "Fe\u00b7der\u00b7schmuck", "des", "Hu\u00b7tes", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Leget ab, was stolz und fremd,", "tokens": ["Le\u00b7get", "ab", ",", "was", "stolz", "und", "fremd", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "PRELS", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Fodert sich getrosten Mutes", "tokens": ["Fo\u00b7dert", "sich", "ge\u00b7tros\u00b7ten", "Mu\u00b7tes"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ein gemeines Hirtenhemd.", "tokens": ["Ein", "ge\u00b7mei\u00b7nes", "Hir\u00b7ten\u00b7hemd", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Und der M\u00e4nner Wohlgefallen", "tokens": ["Und", "der", "M\u00e4n\u00b7ner", "Wohl\u00b7ge\u00b7fal\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bricht mit lautem Jubel aus,", "tokens": ["Bricht", "mit", "lau\u00b7tem", "Ju\u00b7bel", "aus", ","], "token_info": ["w