{"textgrid.poem.41300": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "An Murtzuphlus", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Wolkenbruch und ganzer St\u00e4dte Brand", "tokens": ["Ein", "Wol\u00b7ken\u00b7bruch", "und", "gan\u00b7zer", "St\u00e4d\u00b7te", "Brand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wird dir zuerst, und uns durch dich, bekannt!", "tokens": ["Wird", "dir", "zu\u00b7erst", ",", "und", "uns", "durch", "dich", ",", "be\u00b7kannt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "$,", "KON", "PPER", "APPR", "PPER", "$,", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Du wei\u00dft zuerst, wo Mi\u00dfwachs, Theurung, Noth", "tokens": ["Du", "wei\u00dft", "zu\u00b7erst", ",", "wo", "Mi\u00df\u00b7wachs", ",", "Theu\u00b7rung", ",", "Noth"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PWAV", "NN", "$,", "NN", "$,", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und Krieg und Pest den sichern L\u00e4ndern droht:", "tokens": ["Und", "Krieg", "und", "Pest", "den", "si\u00b7chern", "L\u00e4n\u00b7dern", "droht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Du wei\u00dft zuerst, wo jetzt die Erde bebt,", "tokens": ["Du", "wei\u00dft", "zu\u00b7erst", ",", "wo", "jetzt", "die",