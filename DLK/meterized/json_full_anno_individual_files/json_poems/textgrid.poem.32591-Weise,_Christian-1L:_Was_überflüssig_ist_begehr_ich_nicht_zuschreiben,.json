{"textgrid.poem.32591": {"metadata": {"author": {"name": "Weise, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Was \u00fcberfl\u00fcssig ist begehr ich nicht zuschreiben,", "genre": "verse", "period": "N.A.", "pub_year": 1675, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was \u00fcberfl\u00fcssig ist begehr ich nicht zuschreiben,", "tokens": ["Was", "\u00fc\u00b7berf\u00b7l\u00fcs\u00b7sig", "ist", "be\u00b7gehr", "ich", "nicht", "zu\u00b7schrei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VAFIN", "VVFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch ist nicht unser Hertz ein steter Uberflu\u00df", "tokens": ["Doch", "ist", "nicht", "un\u00b7ser", "Hertz", "ein", "ste\u00b7ter", "U\u00b7ber\u00b7flu\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PTKNEG", "PPOSAT", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da eine Fruchtbarkeit der andern folgen mu\u00df,", "tokens": ["Da", "ei\u00b7ne", "Frucht\u00b7bar\u00b7keit", "der", "an\u00b7dern", "fol\u00b7gen", "mu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "ADJA", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und was will in der Brust vor sich gefangen bleiben?", "tokens": ["Und", "was", "will", "in", "der", "Brust", "vor", "sich", "ge\u00b7fan\u00b7gen", "blei\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VMFIN", "APPR", "ART", "NN", "APPR", "PRF", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So bald man fr\u00f6lich ist, da wachsen und bekleiben", "tokens": ["So", "bald", "man", "fr\u00f6\u00b7lich", "ist", ",", "da", "wach\u00b7sen", "und", "be\u00b7klei\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PIS", "ADJD", "VAFIN", "$,", "ADV", "VVINF", "KON", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die Rosen aus der Lust; k\u00f6mmt Jammer und Verdru\u00df,", "tokens": ["Die", "Ro\u00b7sen", "aus", "der", "Lust", ";", "k\u00f6mmt", "Jam\u00b7mer", "und", "Ver\u00b7dru\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$.", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "So gr\u00fcnt der Nessel-Strauch, der gr\u00fcndet seinen Fu\u00df", "tokens": ["So", "gr\u00fcnt", "der", "Nes\u00b7sel\u00b7Strauch", ",", "der", "gr\u00fcn\u00b7det", "sei\u00b7nen", "Fu\u00df"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "PRELS", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und l\u00e4st den scharffen Brand durch keine Macht vertreiben.", "tokens": ["Und", "l\u00e4st", "den", "scharf\u00b7fen", "Brand", "durch", "kei\u00b7ne", "Macht", "ver\u00b7trei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Will uns das Gl\u00fccke wol, so prangt die reiffe Frucht", "tokens": ["Will", "uns", "das", "Gl\u00fc\u00b7cke", "wol", ",", "s