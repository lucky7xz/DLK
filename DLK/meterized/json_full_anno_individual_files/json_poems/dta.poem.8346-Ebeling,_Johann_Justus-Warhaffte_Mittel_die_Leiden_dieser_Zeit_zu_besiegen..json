{"dta.poem.8346": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Warhaffte Mittel die Leiden  \n dieser Zeit zu besiegen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198782", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein tausendfaches Misvergn\u00fcgen,", "tokens": ["Ein", "tau\u00b7send\u00b7fa\u00b7ches", "Mis\u00b7ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das qu\u00e4lt die Menschen in der", "tokens": ["Das", "qu\u00e4lt", "die", "Men\u00b7schen", "in", "der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ART", "NN", "APPR", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und wer dasselbe kan besiegen,", "tokens": ["Und", "wer", "das\u00b7sel\u00b7be", "kan", "be\u00b7sie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PDAT", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Heist warlich ein recht grosser Held:", "tokens": ["Heist", "war\u00b7lich", "ein", "recht", "gros\u00b7ser", "Held", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Jhr Weisen gebet Mittel an,", "tokens": ["Ihr", "Wei\u00b7sen", "ge\u00b7bet", "Mit\u00b7tel", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wie man die Noth bezwingen kan.", "tokens": ["Wie", "man", "die", "Noth", "be\u00b7zwin\u00b7gen", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Jhr denket nach es zu ergt\u00fcnden,", "tokens": ["Ihr", "den\u00b7ket", "nach", "es", "zu", "erg\u00b7t\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Jhr suchet die Zufriedenheit,", "tokens": ["Ihr", "su\u00b7chet", "die", "Zu\u00b7frie\u00b7den\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Kleinod das sehr schwer zu finden,", "tokens": ["Ein", "Klei\u00b7nod", "das", "sehr", "schwer", "zu", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADV", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In dieser unvollkommen Zeit:", "tokens": ["In", "die\u00b7ser", "un\u00b7voll\u00b7kom\u00b7men", "Zeit", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Was lehret ein gesch\u00e4rfter Wiz?", "tokens": ["Was", "leh\u00b7ret", "ein", "ge\u00b7sch\u00e4rf\u00b7ter", "Wiz", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nur Mittel die f\u00fcr uns unn\u00fcz.", "tokens": ["Nur", "Mit\u00b7tel", "die", "f\u00fcr", "uns", "un\u00b7n\u00fcz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "ART", "APPR", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Der eine sagt bei Angst und Schmerze,", "tokens": ["Der", "ei\u00b7ne", "sagt", "bei", "Angst", "und", "Schmer\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Bei folternden Verdrus und Pein,", "tokens": ["Bei", "fol\u00b7tern\u00b7den", "Ver\u00b7drus", "und", "Pein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Kan ein von Grosmuth starkes Herze,", "tokens": ["Kan", "ein", "von", "Gros\u00b7muth", "star\u00b7kes", "Her\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "APPR", "NN", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein herrlich Lindrungs-Mittel seyn:", "tokens": ["Ein", "herr\u00b7lich", "Lin\u00b7drungs\u00b7Mit\u00b7tel", "seyn", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Allein er lehret nicht dabei,", "tokens": ["Al\u00b7lein", "er", "leh\u00b7ret", "nicht", "da\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "PTKNEG", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wo dieser Muth zu haben sey.", "tokens": ["Wo", "die\u00b7ser", "Muth", "zu", "ha\u00b7ben", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDAT", "NN", "PTKZU", "VAINF", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Die Noth zerbricht auch hartes Eisen,", "tokens": ["Die", "Noth", "zer\u00b7bricht", "auch", "har\u00b7tes", "Ei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wer ist in Leiden so gesinnt,", "tokens": ["Wer", "ist", "in", "Lei\u00b7den", "so", "ge\u00b7sinnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "APPR", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wo bleibt der Rath der klugen Weisen,", "tokens": ["Wo", "bleibt", "der", "Rath", "der", "klu\u00b7gen", "Wei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn Herz und Muth wie Wachs zerrinnt?", "tokens": ["Wenn", "Herz", "und", "Muth", "wie", "Wachs", "zer\u00b7rinnt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "KOKOM", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Rathschlag der gelinget nicht,", "tokens": ["Der", "Rath\u00b7schlag", "der", "ge\u00b7lin\u00b7get", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "VVFIN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wenn man bei Angst von Grosmuth spricht.", "tokens": ["Wenn", "man", "bei", "Angst", "von", "Gros\u00b7muth", "spricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Der andre sagt mit stolzen Mienen,", "tokens": ["Der", "and\u00b7re", "sagt", "mit", "stol\u00b7zen", "Mie\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df einem gut und weisen Mann,", "tokens": ["Da\u00df", "ei\u00b7nem", "gut", "und", "wei\u00b7sen", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJD", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn ihm ein Ungl\u00fcksstern erschienen,", "tokens": ["Wenn", "ihm", "ein", "Un\u00b7gl\u00fckss\u00b7tern", "er\u00b7schie\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Kein Ungl\u00fck doch begegnen kan,", "tokens": ["Kein", "Un\u00b7gl\u00fck", "doch", "be\u00b7geg\u00b7nen", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es sey nur alles Einbildung;", "tokens": ["Es", "sey", "nur", "al\u00b7les", "Ein\u00b7bil\u00b7dung", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PIAT", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Die Welt ein Land voll Aenderung.", "tokens": ["Die", "Welt", "ein", "Land", "voll", "A\u00b7en\u00b7de\u00b7rung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADJD", "NN", "$."], "meter": "-+-+-----", "measure": "unknown.measure.di"}}, "stanza.6": {"line.1": {"text": "Man mu\u00df, spricht er, nur weise denken,", "tokens": ["Man", "mu\u00df", ",", "spricht", "er", ",", "nur", "wei\u00b7se", "den\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "$,", "VVFIN", "PPER", "$,", "ADV", "ADJD", "VVINF", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Da\u00df Tr\u00fcbsal Unbequemlichkeit;", "tokens": ["Da\u00df", "Tr\u00fcb\u00b7sal", "Un\u00b7be\u00b7quem\u00b7lich\u00b7keit", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Alsdenn wird uns der Schmerz nicht kr\u00e4nken,", "tokens": ["Als\u00b7denn", "wird", "uns", "der", "Schmerz", "nicht", "kr\u00e4n\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+---+-+-", "measure": "dactylic.init"}, "line.4": {"text": "Nicht plagen ein sonst qu\u00e4lend Leid:", "tokens": ["Nicht", "pla\u00b7gen", "ein", "sonst", "qu\u00e4\u00b7lend", "Leid", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "ART", "ADV", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Jedoch was feurig ist, das brennt,", "tokens": ["Je\u00b7doch", "was", "feu\u00b7rig", "ist", ",", "das", "brennt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PWS", "ADJD", "VAFIN", "$,", "PDS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wenn man es gleich schon Wasser nennt.", "tokens": ["Wenn", "man", "es", "gleich", "schon", "Was\u00b7ser", "nennt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "ADV", "ADV", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Die Menschen die im Elend weinen,", "tokens": ["Die", "Men\u00b7schen", "die", "im", "E\u00b7lend", "wei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die klagen darum nicht so sehr,", "tokens": ["Die", "kla\u00b7gen", "da\u00b7rum", "nicht", "so", "sehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PAV", "PTKNEG", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Weil sie aus falschen Grunde meinen,", "tokens": ["Weil", "sie", "aus", "fal\u00b7schen", "Grun\u00b7de", "mei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df ihre Noth ein Uebel w\u00e4r,", "tokens": ["Da\u00df", "ih\u00b7re", "Noth", "ein", "Ue\u00b7bel", "w\u00e4r", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sie klagen weil das Herze f\u00fchlt,", "tokens": ["Sie", "kla\u00b7gen", "weil", "das", "Her\u00b7ze", "f\u00fchlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOUS", "PDS", "VVFIN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das Leiden das im Gliedern w\u00fchlt.", "tokens": ["Das", "Lei\u00b7den", "das", "im", "Glie\u00b7dern", "w\u00fchlt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Der dritte giebt stat Arzeneien,", "tokens": ["Der", "drit\u00b7te", "giebt", "stat", "Ar\u00b7ze\u00b7nei\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Man soll die Einbildung erh\u00f6hn,", "tokens": ["Man", "soll", "die", "Ein\u00b7bil\u00b7dung", "er\u00b7h\u00f6hn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "++--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Und sich darob vorhero freuen", "tokens": ["Und", "sich", "da\u00b7rob", "vor\u00b7he\u00b7ro", "freu\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PRF", "PAV", "ADV", "VVINF"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Was man ins k\u00fcnftig werde sehn:", "tokens": ["Was", "man", "ins", "k\u00fcnf\u00b7tig", "wer\u00b7de", "sehn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "APPRART", "ADJD", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Allein wird woll die Angst erstikt,", "tokens": ["Al\u00b7lein", "wird", "woll", "die", "Angst", "ers\u00b7tikt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wenn man im Traum ein Gl\u00fck erblikt?", "tokens": ["Wenn", "man", "im", "Traum", "ein", "Gl\u00fck", "er\u00b7blikt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPRART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Die alten Weisen dieser Erden,", "tokens": ["Die", "al\u00b7ten", "Wei\u00b7sen", "die\u00b7ser", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die geben Mittel an die Hand", "tokens": ["Die", "ge\u00b7ben", "Mit\u00b7tel", "an", "die", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zu lindern der Natur Beschwerden,", "tokens": ["Zu", "lin\u00b7dern", "der", "Na\u00b7tur", "Be\u00b7schwer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Jedoch es wird nicht weggebannt,", "tokens": ["Je\u00b7doch", "es", "wird", "nicht", "weg\u00b7ge\u00b7bannt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VAFIN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wenn man kein andres Mittel hat,", "tokens": ["Wenn", "man", "kein", "and\u00b7res", "Mit\u00b7tel", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PIAT", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Als diesen blos vern\u00fcnftgen Rath.", "tokens": ["Als", "die\u00b7sen", "blos", "ver\u00b7n\u00fcnft\u00b7gen", "Rath", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Wer eine Noth denkt zu bezwingen,", "tokens": ["Wer", "ei\u00b7ne", "Noth", "denkt", "zu", "be\u00b7zwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"],