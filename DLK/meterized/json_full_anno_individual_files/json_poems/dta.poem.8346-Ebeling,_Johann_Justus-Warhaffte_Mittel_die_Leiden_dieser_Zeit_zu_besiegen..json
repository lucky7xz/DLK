text": "Was ", "tokens": ["Was"], "token_info": ["word"], "pos": ["PWS"], "meter": "-", "measure": "single.down"}, "line.4": {"text": "In Salems hellgestirnten H\u00f6hn;", "tokens": ["In", "Sa\u00b7lems", "hell\u00b7ges\u00b7tirn\u00b7ten", "H\u00f6hn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So wird das bange Herz verkl\u00e4rt,", "tokens": ["So", "wird", "das", "ban\u00b7ge", "Herz", "ver\u00b7kl\u00e4rt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wenn es von Kummer wird beschwert.", "tokens": ["Wenn", "es", "von", "Kum\u00b7mer", "wird", "be\u00b7schwert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.17": {"line.1": {"text": "Man mu\u00df was Zeitlich ist bedenken,", "tokens": ["Man", "mu\u00df", "was", "Zeit\u00b7lich", "ist", "be\u00b7den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PWS", "ADJD", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie bald dasselbige vergeht,", "tokens": ["Wie", "bald", "das\u00b7sel\u00b7bi\u00b7ge", "ver\u00b7geht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PDS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das Herze auf das Ewge lenken,", "tokens": ["Das", "Her\u00b7ze", "auf", "das", "Ew\u00b7ge", "len\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "ART", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das ohne Aenderung besteht:", "tokens": ["Das", "oh\u00b7ne", "A\u00b7en\u00b7de\u00b7rung", "be\u00b7steht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "NN", "VVFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Es schwindet alle K\u00fcmmerni\u00df,", "tokens": ["Es", "schwin\u00b7det", "al\u00b7le", "K\u00fcm\u00b7mer\u00b7ni\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So bald man gl\u00e4ubt da\u00df dies gewis.", "tokens": ["So", "bald", "man", "gl\u00e4ubt", "da\u00df", "dies", "ge\u00b7wis", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PIS", "VVFIN", "KOUS", "PDS", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.18": {"line.1": {"text": "Darum ihr traurigen Gem\u00fcther,", "tokens": ["Da\u00b7rum", "ihr", "trau\u00b7ri\u00b7gen", "Ge\u00b7m\u00fc\u00b7ther", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die ihr im Gram und Schwermuth liegt,", "tokens": ["Die", "ihr", "im", "Gram", "und", "Schwer\u00b7muth", "liegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Beschaut das Reich der ewgen G\u00fcter;", "tokens": ["Be\u00b7schaut", "das", "Reich", "der", "ew\u00b7gen", "G\u00fc\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So wird das Herz in ", "tokens": ["So", "wird", "das", "Herz", "in"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "APPR"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Ruft seine Wunder-G\u00fcte an,", "tokens": ["Ruft", "sei\u00b7ne", "Wun\u00b7der\u00b7G\u00fc\u00b7te", "an", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die alle Plagen mindern kan.", "tokens": ["Die", "al\u00b7le", "Pla\u00b7gen", "min\u00b7dern", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.19": {"line.1": {"text": "Braucht diese Mittel die wir preisen,", "tokens": ["Braucht", "die\u00b7se", "Mit\u00b7tel", "die", "wir", "prei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDAT", "NN", "ART", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Es stekt darin der gute Rath,", "tokens": ["Es", "stekt", "da\u00b7rin", "der", "gu\u00b7te", "Rath", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den uns der Lehrer aller Weisen", "tokens": ["Den", "uns", "der", "Leh\u00b7rer", "al\u00b7ler", "Wei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ART", "NN", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In seinem Wort gegeben hat:", "tokens": ["In", "sei\u00b7nem", "Wort", "ge\u00b7ge\u00b7ben", "hat", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und JEsus Zeugen zeigen klar,", "tokens": ["Und", "Je\u00b7sus", "Zeu\u00b7gen", "zei\u00b7gen", "klar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Jm Beispiel ", "tokens": ["Jm", "Bei\u00b7spiel"], "token_info": ["word", "word"], "pos": ["APPRART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.20": {"line.1": {"text": "Die Mittel haben sie gest\u00e4rket,", "tokens": ["Die", "Mit\u00b7tel", "ha\u00b7ben", "sie", "ge\u00b7st\u00e4r\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dagegen sie die H\u00f6ll erbo\u00dft,", "tokens": ["Da\u00b7ge\u00b7gen", "sie", "die", "H\u00f6ll", "er\u00b7bo\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie sind im Leiden, wie man merket", "tokens": ["Sie", "sind", "im", "Lei\u00b7den", ",", "wie", "man", "mer\u00b7ket"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "$,", "PWAV", "PIS", "VVFIN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Zufriednes Herzens und getrost:", "tokens": ["Zu\u00b7fried\u00b7nes", "Her\u00b7zens", "und", "ge\u00b7trost", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wer nimmt die Arzenei nicht ein,", "tokens": ["Wer", "nimmt", "die", "Ar\u00b7ze\u00b7nei", "nicht", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "wor