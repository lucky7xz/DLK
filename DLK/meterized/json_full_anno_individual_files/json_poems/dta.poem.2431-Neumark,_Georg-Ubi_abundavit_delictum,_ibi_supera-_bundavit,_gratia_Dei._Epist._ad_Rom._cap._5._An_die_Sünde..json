{"dta.poem.2431": {"metadata": {"author": {"name": "Neumark, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Ubi abundavit delictum, ibi supera-  \n bundavit, gratia Dei.  Epist. ad Rom. cap.  5.  \n An die S\u00fcnde.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1652", "urn": "urn:nbn:de:kobv:b4-20428-0", "language": ["de:0.99"], "booktitle": "Neumark, Georg: Poetisch- und Musikalisches Lustw\u00e4ldchen. Hamburg, 1652."}, "poem": {"stanza.1": {"line.1": {"text": "Geld/ Ehr/ und alles Gut kan uns gar wenig n\u00fctzen/", "tokens": ["Geld", "/", "Ehr", "/", "und", "al\u00b7les", "Gut", "kan", "uns", "gar", "we\u00b7nig", "n\u00fct\u00b7zen", "/"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "NN", "$(", "KON", "PIS", "ADJD", "VMFIN", "PPER", "ADV", "PIS", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gett und sein heiligs Wort mu\u00df uns alleine sch\u00fctzen/", "tokens": ["Gett", "und", "sein", "hei\u00b7ligs", "Wort", "mu\u00df", "uns", "al\u00b7lei\u00b7ne", "sch\u00fct\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "PPOSAT", "ADJA", "NN", "VMFIN", "PPER", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wenn uns ein Kreutz bef\u00e4llt/ die\u00df ist die rechte Bah/", "tokens": ["Wenn", "uns", "ein", "Kreutz", "be\u00b7f\u00e4llt", "/", "die\u00df", "ist", "die", "rech\u00b7te", "Bah", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$(", "PDS", "VAFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die aus der Hellen f\u00fchrt die uns bringt Himmel- an.", "tokens": ["Die", "aus", "der", "Hel\u00b7len", "f\u00fchrt", "die", "uns", "bringt", "Him\u00b7mel", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "ART", "PPER", "VVFIN", "TRUNC", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}