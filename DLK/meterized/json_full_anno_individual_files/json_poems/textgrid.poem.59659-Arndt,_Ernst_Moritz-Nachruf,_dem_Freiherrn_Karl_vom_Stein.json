{"textgrid.poem.59659": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "Nachruf, dem Freiherrn Karl vom Stein", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der L\u00f6we schl\u00e4ft \u2013 Ihr, die ihr wachen sollt,", "tokens": ["Der", "L\u00f6\u00b7we", "schl\u00e4ft", "\u2013", "Ihr", ",", "die", "ihr", "wa\u00b7chen", "sollt", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "$(", "PPER", "$,", "PRELS", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Versteht ihr, da\u00df die Besten schlafen gehen?", "tokens": ["Ver\u00b7steht", "ihr", ",", "da\u00df", "die", "Bes\u00b7ten", "schla\u00b7fen", "ge\u00b7hen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "ART", "NN", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die, als die Welt erlag, noch stark gewollt,", "tokens": ["Die", ",", "als", "die", "Welt", "er\u00b7lag", ",", "noch", "stark", "ge\u00b7wollt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "$,", "KOUS", "ART", "NN", "VVFIN", "$,", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die werden's nur verstehen.", "tokens": ["Die", "wer\u00b7den's", "nur", "ver\u00b7ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "VVINF", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Der L\u00f6we schl\u00e4ft \u2013 Ihr, die ihr wachen sollt,", "tokens": ["Der", "L\u00f6\u00b7we", "schl\u00e4ft", "\u2013", "Ihr", ",", "die", "ihr", "wa\u00b7chen", "sollt", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "$(", "PPER", "$,", "PRELS", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Versteht ihr, welcher W\u00e4chter heimgegangen?", "tokens": ["Ver\u00b7steht", "ihr", ",", "wel\u00b7cher", "W\u00e4ch\u00b7ter", "heim\u00b7ge\u00b7gan\u00b7gen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PRELS", "NE", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sein gro\u00dfes Herz braucht keiner Klagen Sold,", "tokens": ["Sein", "gro\u00b7\u00dfes", "Herz", "braucht", "kei\u00b7ner", "Kla\u00b7gen", "Sold", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PIAT", "NN", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Nicht tr\u00e4nennasse Wangen.", "tokens": ["Nicht", "tr\u00e4\u00b7nen\u00b7nas\u00b7se", "Wan\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Es heischt den Geist heraus, den deutschen Mut,", "tokens": ["Es", "heischt", "den", "Geist", "he\u00b7raus", ",", "den", "deut\u00b7schen", "Mut", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Zu brennen hei\u00df f\u00fcr Vaterlandes Ehren,", "tokens": ["Zu", "bren\u00b7nen", "hei\u00df", "f\u00fcr", "Va\u00b7ter\u00b7lan\u00b7des", "Eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ADJD", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es heischt, wann's gilt, den letzten Tropfen Blut,", "tokens": ["Es", "heischt", ",", "wann's", "gilt", ",", "den", "letz\u00b7ten", "Trop\u00b7fen", "Blut", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "VVFIN", "$,", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Nicht weibisch eitle Z\u00e4hren.", "tokens": ["Nicht", "wei\u00b7bisch", "eit\u00b7le", "Z\u00e4h\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und schl\u00e4ngelt welsche List den Schlangenpfad", "tokens": ["Und", "schl\u00e4n\u00b7gelt", "wel\u00b7sche", "List", "den", "Schlan\u00b7gen\u00b7pfad"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PWAT", "NN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "In deutsche Gaun, dann ruft der stumme Leue", "tokens": ["In", "deut\u00b7sche", "Gaun", ",", "dann", "ruft", "der", "stum\u00b7me", "Leu\u00b7e"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mit Donners Klang \u2013 es bebet der Verrat \u2013", "tokens": ["Mit", "Don\u00b7ners", "Klang", "\u2013", "es", "be\u00b7bet", "der", "Ver\u00b7rat", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "$(", "PPER", "VVFIN", "ART", "NN", "$("], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Er rufet: Treue! Treue!", "tokens": ["Er", "ru\u00b7fet", ":", "Treu\u00b7e", "!", "Treu\u00b7e", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "NN", "$.", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und klinget die Trompete: Es ist Krieg!", "tokens": ["Und", "klin\u00b7get", "die", "Trom\u00b7pe\u00b7te", ":", "Es", "ist", "Krieg", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$.", "PPER", "VAFIN", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Und ziehen Feinde gegen Deutschlands Marken,", "tokens": ["Und", "zie\u00b7hen", "Fein\u00b7de", "ge\u00b7gen", "Deutschlands", "Mar\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPR", "NE", "NE", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Dann mahnt's aus ihm zum Kampf auf Tod und Sieg", "tokens": ["Dann", "mahnt's", "aus", "ihm", "zum", "Kampf", "auf", "Tod", "und", "Sieg"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "APPRART", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die Tapfern und die Starken.", "tokens": ["Die", "Tap\u00b7fern", "und", "die", "Star\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Der L\u00f6we schl\u00e4ft \u2013 nicht er, nur sein Gebein;", "tokens": ["Der", "L\u00f6\u00b7we", "schl\u00e4ft", "\u2013", "nicht", "er", ",", "nur", "sein", "Ge\u00b7bein", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "$(", "PTKNEG", "PPER", "$,", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Denn wann es ruft im Vaterlande: Wer da?", "tokens": ["Denn", "wann", "es", "ruft", "im", "Va\u00b7ter\u00b7lan\u00b7de", ":", "Wer", "da", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "APPRART", "NN", "$.", "PWS", "ADV", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Dann ist er wach, dann ruft der L\u00f6we Stein,", "tokens": ["Dann", "ist", "er", "wach", ",", "dann", "ruft", "der", "L\u00f6\u00b7we", "Stein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PTKVZ", "$,", "ADV", "VVFIN", "ART", "NE", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Dann ist sein Geist, ist ", "tokens": ["Dann", "ist", "sein", "Geist", ",", "ist"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "$,", "VAFIN"], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "Dann t\u00f6nt die Losung ", "tokens": ["Dann", "t\u00f6nt", "die", "Lo\u00b7sung"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Ringt jeder Deutsche f\u00fcr das Freie, Hohe.", "tokens": ["Ringt", "je\u00b7der", "Deut\u00b7sche", "f\u00fcr", "das", "Frei\u00b7e", ",", "Ho\u00b7he", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "APPR", "ART", "NN", "$,", "NE", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So schl\u00e4gt es Blitz auf Blitz in M\u00e4nner ein", "tokens": ["So", "schl\u00e4gt", "es", "Blitz", "auf", "Blitz", "in", "M\u00e4n\u00b7ner", "ein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "NN", "APPR", "NN", "APPR", "NN", "ART"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Aus ihm in heil'ger Lohe.", "tokens": ["Aus", "ihm", "in", "heil'\u00b7ger", "Lo\u00b7he", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Nein, Deutschland, nie wird dieser reinste Strahl", "tokens": ["Nein", ",", "Deutschland", ",", "nie", "wird", "die\u00b7ser", "reins\u00b7te", "Strahl"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "NE", "$,", "ADV", "VAFIN", "PDAT", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "In deiner lichten Heldenkrone bleichen,", "tokens": ["In", "dei\u00b7ner", "lich\u00b7ten", "Hel\u00b7den\u00b7kro\u00b7ne", "blei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Solang aus Alpen braust dein Rhein zu Tal", "tokens": ["So\u00b7lang", "aus", "Al\u00b7pen", "braust", "dein", "Rhein", "zu", "Tal"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "VVFIN", "PPOSAT", "NE", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und gr\u00fcnen deine Eichen.", "tokens": ["Und", "gr\u00fc\u00b7nen", "dei\u00b7ne", "Ei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9":