{"dta.poem.20512": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Verliebte Arien.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mein hertze schmeltzt in einer stummen glut/", "tokens": ["Mein", "hert\u00b7ze", "schmeltzt", "in", "ei\u00b7ner", "stum\u00b7men", "glut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Kein funcke soll aus meinen adern steigen/", "tokens": ["Kein", "fun\u00b7cke", "soll", "aus", "mei\u00b7nen", "a\u00b7dern", "stei\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dem rauche selbst verbeut man sich zu zeigen/", "tokens": ["Dem", "rau\u00b7che", "selbst", "ver\u00b7beut", "man", "sich", "zu", "zei\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "VVFIN", "PIS", "PRF", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und schweigen meistert mein erhitztes blut.", "tokens": ["Und", "schwei\u00b7gen", "meis\u00b7tert", "mein", "er\u00b7hitz\u00b7tes", "blut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Es heist mich die natur ein sch\u00f6nes auge suchen/", "tokens": ["Es", "heist", "mich", "die", "na\u00b7tur", "ein", "sch\u00f6\u00b7nes", "au\u00b7ge", "su\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und das gesetze will auff meine regung fluchen.", "tokens": ["Und", "das", "ge\u00b7set\u00b7ze", "will", "auff", "mei\u00b7ne", "re\u00b7gung", "flu\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "VMFIN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Soll denn ein ku\u00df/ ein unbefleckter schertz/", "tokens": ["Soll", "denn", "ein", "ku\u00df", "/", "ein", "un\u00b7be\u00b7fleck\u00b7ter", "schertz", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "$(", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ein s\u00fcsser blick s\u00fcnd und verbrechung heissen?", "tokens": ["Ein", "s\u00fcs\u00b7ser", "blick", "s\u00fcnd", "und", "ver\u00b7bre\u00b7chung", "heis\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Soll ich denn selber mich mir nun entreissen?", "tokens": ["Soll", "ich", "denn", "sel\u00b7ber", "mich", "mir", "nun", "en\u00b7treis\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "PPER", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der himmel kennt der menschen sinn und hertz.", "tokens": ["Der", "him\u00b7mel", "kennt", "der", "men\u00b7schen", "sinn", "und", "hertz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Lieb ist des himmels kind/ es wird ja unsre flammen/", "tokens": ["Lieb", "ist", "des", "him\u00b7mels", "kind", "/", "es", "wird", "ja", "uns\u00b7re", "flam\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "NN", "$(", "PPER", "VAFIN", "ADV", "PPOSAT", "VVINF", "$("], "meter": "++-+-+-+-+-+-", "measure": "unknown.measure.septa"}, "line.6": {"text": "Als dieberey und mord der himmel nicht verdammen.", "tokens": ["Als", "die\u00b7be\u00b7rey", "und", "mord", "der", "him\u00b7mel", "nicht", "ver\u00b7dam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wer ist doch/ der sich selbst entmenschen kan?", "tokens": ["Wer", "ist", "doch", "/", "der", "sich", "selbst", "ent\u00b7men\u00b7schen", "kan", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "$(", "PRELS", "PRF", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wir wissen uns hier nirgends zu verkl\u00e4ren/", "tokens": ["Wir", "wis\u00b7sen", "uns", "hier", "nir\u00b7gends", "zu", "ver\u00b7kl\u00e4\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Des fleisches kan das fleisch sich nicht erwehren/", "tokens": ["Des", "flei\u00b7sches", "kan", "das", "fleisch", "sich", "nicht", "er\u00b7weh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VMFIN", "PDS", "VVFIN", "PRF", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die menschlichkeit kleht menschen st\u00fcndlich an.", "tokens": ["Die", "menschlich\u00b7keit", "kleht", "men\u00b7schen", "st\u00fcnd\u00b7lich", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Die engel liessen sich im himmel abw\u00e4rts treiben/", "tokens": ["Die", "en\u00b7gel", "lies\u00b7sen", "sich", "im", "him\u00b7mel", "ab\u00b7w\u00e4rts", "trei\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPRART", "NN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wie sollen menschen doch auff erden engel bleiben?", "tokens": ["Wie", "sol\u00b7len", "men\u00b7schen", "doch", "auff", "er\u00b7den", "en\u00b7gel", "blei\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "ADV", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Soll Sylvia von mir verschlossen seyn?", "tokens": ["Soll", "Syl\u00b7via", "von", "mir", "ver\u00b7schlos\u00b7sen", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NE", "APPR", "PPER", "VVPP", "VAINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Verbotne frucht ist mehr als doppelt s\u00fcsse;", "tokens": ["Ver\u00b7bot\u00b7ne", "frucht", "ist", "mehr", "als", "dop\u00b7pelt", "s\u00fcs\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "PIS", "KOKOM", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der neben-weg reitzt mehrmahls unsre f\u00fcsse/", "tokens": ["Der", "ne\u00b7ben\u00b7weg", "reitzt", "mehr\u00b7mahls", "uns\u00b7re", "f\u00fcs\u00b7se", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "PPOSAT", "NN", "$("], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Die wollust w\u00e4chst auch aus gefahr und pein.", "tokens": ["Die", "wol\u00b7lust", "w\u00e4chst", "auch", "aus", "ge\u00b7fahr", "und", "pein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "NN", "KON", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Di\u00df ist die s\u00fcsse nu\u00df/ so schwer ist auffzubrechen/", "tokens": ["Di\u00df", "ist", "die", "s\u00fcs\u00b7se", "nu\u00df", "/", "so", "schwer", "ist", "auff\u00b7zu\u00b7bre\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$(", "ADV", "ADJD", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die rose wird geliebt/ ob gleich die d\u00f6rner stechen.", "tokens": ["Die", "ro\u00b7se", "wird", "ge\u00b7liebt", "/", "ob", "gleich", "die", "d\u00f6r\u00b7ner", "ste\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "VVPP", "$(", "KOUS", "ADV", "ART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Ach/ Sylvia! Ich wei\u00df nicht/ wo ich bin!", "tokens": ["Ach", "/", "Syl\u00b7via", "!", "Ich", "wei\u00df", "nicht", "/", "wo", "ich", "bin", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$(", "NE", "$.", "PPER", "VVFIN", "PTKNEG", "$(", "PWAV", "PPER", "VAFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Es soll kein mensch mein heisses \u00fcbel kennen/", "tokens": ["Es", "soll", "kein", "mensch", "mein", "heis\u00b7ses", "\u00fc\u00b7bel", "ken\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIAT", "NN", "PPOSAT", "ADJA", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich armer darff nicht meine kranckheit nennen/", "tokens": ["Ich", "ar\u00b7mer", "darff", "nicht", "mei\u00b7ne", "kran\u00b7ck\u00b7heit", "nen\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "VMFIN", "PTKNEG", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.4": {"text": "Die richt-sucht nimmt uns bl\u00fct und fr\u00fcchte hin.", "tokens": ["Die", "richt\u00b7sucht", "nimmt", "uns", "bl\u00fct", "und", "fr\u00fcch\u00b7te", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVFIN", "KON", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Getreue Sylvia/ hab ich genade funden/", "tokens": ["Ge\u00b7treu\u00b7e", "Syl\u00b7via", "/", "hab", "ich", "ge\u00b7na\u00b7de", "fun\u00b7den", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$(", "VAFIN", "PPER", "ADV", "VVFIN", "$("], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "So schau/ ach schaue doch! in meine tieffe wunden.", "tokens": ["So", "schau", "/", "ach", "schau\u00b7e", "doch", "!", "in", "mei\u00b7ne", "tief\u00b7fe", "wun\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$(", "ADV", "VVFIN", "ADV", "$.", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Du kennest ja die galle dieser welt/", "tokens": ["Du", "ken\u00b7nest", "ja", "die", "gal\u00b7le", "die\u00b7ser", "welt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "PDAT", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wie iedermann des andern fehler zehlet/", "tokens": ["Wie", "ie\u00b7der\u00b7mann", "des", "an\u00b7dern", "feh\u00b7ler", "zeh\u00b7let", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und fremden fall zum zeitvertreib erwehlet/", "tokens": ["Und", "frem\u00b7den", "fall", "zum", "zeit\u00b7ver\u00b7treib", "er\u00b7weh\u00b7let", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPRART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und dessen fleck vor seine schmincke h\u00e4lt.", "tokens": ["Und", "des\u00b7sen", "fleck", "vor", "sei\u00b7ne", "schmin\u00b7cke", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRELAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Du must nur dichtes garn zu meiner decken spinnen/", "tokens": ["Du", "must", "nur", "dich\u00b7tes", "garn", "zu", "mei\u00b7ner", "de\u00b7cken", "spin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Dadurch die falsche welt nicht leicht wird sehen k\u00f6nnen.", "tokens": ["Da\u00b7durch", "die", "fal\u00b7sche", "welt", "nicht", "leicht", "wird", "se\u00b7hen", "k\u00f6n\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "ADJA", "NN", "PTKNEG", "ADJD", "VAFIN", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Es soll alsdenn ein amber-reicher ku\u00df/", "tokens": ["Es", "soll", "als\u00b7denn", "ein", "am\u00b7ber\u00b7rei\u00b7cher", "ku\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der sich genetzt in moschus und rubinen/", "tokens": ["Der", "sich", "ge\u00b7netzt", "in", "mo\u00b7schus", "und", "ru\u00b7bi\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "VVPP", "APPR", "NE", "KON", "VVINF", "$("], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Vor julepp uns in dieser hitze dienen.", "tokens": ["Vor", "ju\u00b7lepp", "uns", "in", "die\u00b7ser", "hit\u00b7ze", "die\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PRF", "APPR", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wo bleibst du doch! O s\u00fcsser \u00fcberflu\u00df?", "tokens": ["Wo", "bleibst", "du", "doch", "!", "O", "s\u00fcs\u00b7ser", "\u00fc\u00b7berf\u00b7lu\u00df", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "$.", "NE", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ich wei\u00df die liebe wird zu lachen hier beginnen/", "tokens": ["Ich", "wei\u00df", "die", "lie\u00b7be", "wird", "zu", "la\u00b7chen", "hier", "be\u00b7gin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "VAFIN", "PTKZU", "VVINF", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Indem zwey zungen nicht vor liebe reden k\u00f6nnen.", "tokens": ["In\u00b7dem", "zwey", "zun\u00b7gen", "nicht", "vor", "lie\u00b7be", "re\u00b7den", "k\u00f6n\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "CARD", "NN", "PTKNEG", "APPR", "ADJA", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}