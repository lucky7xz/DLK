{"textgrid.poem.26761": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sieh in dem zarten Kind zwei liebliche Blumen vereinigt,", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sieh in dem zarten Kind zwei liebliche Blumen vereinigt,", "tokens": ["Sieh", "in", "dem", "zar\u00b7ten", "Kind", "zwei", "lieb\u00b7li\u00b7che", "Blu\u00b7men", "ver\u00b7ei\u00b7nigt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "ADJA", "NN", "CARD", "ADJA", "NN", "VVPP", "$,"], "meter": "+--+-+-+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Jungfrau und J\u00fcngling, sie deckt beide die Knospe noch zu.", "tokens": ["Jung\u00b7frau", "und", "J\u00fcng\u00b7ling", ",", "sie", "deckt", "bei\u00b7de", "die", "Knos\u00b7pe", "noch", "zu", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "PPER", "VVFIN", "PIS", "ART", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+--+---+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Leise l\u00f6st sich das Band, es entzweien sich zart die Naturen,", "tokens": ["Lei\u00b7se", "l\u00f6st", "sich", "das", "Band", ",", "es", "ent\u00b7zwei\u00b7en", "sich", "zart", "die", "Na\u00b7tu\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "ART", "NN", "$,", "PPER", "VVFIN", "PRF", "ADJD", "ART", "NN", "$,"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Und von der holden Scham trennet sich feurig die Kraft.", "tokens": ["Und", "von", "der", "hol\u00b7den", "Scham", "tren\u00b7net", "sich", "feu\u00b7rig", "die", "Kraft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VVFIN", "PRF", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.5": {"text": "G\u00f6nne dem Knaben zu spielen, in wilder Begierde zu toben:", "tokens": ["G\u00f6n\u00b7ne", "dem", "Kna\u00b7ben", "zu", "spie\u00b7len", ",", "in", "wil\u00b7der", "Be\u00b7gier\u00b7de", "zu", "to\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PTKZU", "VVINF", "$,", "APPR", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.6": {"text": "Nur die ges\u00e4ttigte Kraft kehret zur Anmut zur\u00fcck.", "tokens": ["Nur", "die", "ge\u00b7s\u00e4t\u00b7tig\u00b7te", "Kraft", "keh\u00b7ret", "zur", "An\u00b7mut", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVFIN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+--++--+--+", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Aus der Knospe beginnt die doppelte Blume zu streben,", "tokens": ["Aus", "der", "Knos\u00b7pe", "be\u00b7ginnt", "die", "dop\u00b7pel\u00b7te", "Blu\u00b7me", "zu", "stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.8": {"text": "K\u00f6stlich ist jede, doch stillt keine dein sehnendes Herz.", "tokens": ["K\u00f6st\u00b7lich", "ist", "je\u00b7de", ",", "doch", "stillt", "kei\u00b7ne", "dein", "seh\u00b7nen\u00b7des", "Herz", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PIAT", "$,", "ADV", "VVFIN", "PIAT", "PPOSAT", "ADJA", "NN", "$."], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}, "line.9": {"text": "Reizende F\u00fclle schwellt der Jungfrau bl\u00fchende Glieder,", "tokens": ["Rei\u00b7zen\u00b7de", "F\u00fcl\u00b7le", "schwellt", "der", "Jung\u00b7frau", "bl\u00fc\u00b7hen\u00b7de", "Glie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+--+-", "measure": "hexameter"}, "line.10": {"text": "Aber der Stolz bewacht streng wie der G\u00fcrtel den Reiz.", "tokens": ["A\u00b7ber", "der", "Stolz", "be\u00b7wacht", "streng", "wie", "der", "G\u00fcr\u00b7tel", "den", "Reiz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVPP", "VVFIN", "KOKOM", "ART", "NN", "ART", "NN", "$."], "meter": "+--+-+-+-+--+", "measure": "iambic.hexa.invert"}, "line.11": {"text": "Scheu wie das zitternde Reh, das ihr Horn durch die W\u00e4lder verfolget,", "tokens": ["Scheu", "wie", "das", "zit\u00b7tern\u00b7de", "Reh", ",", "das", "ihr", "Horn", "durch", "die", "W\u00e4l\u00b7der", "ver\u00b7fol\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "ADJA", "NN", "$,", "PRELS", "PPOSAT", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.12": {"text": "Flieht sie im Mann nur den Feind, hasset noch, weil sie nicht liebt.", "tokens": ["Flieht", "sie", "im", "Mann", "nur", "den", "Feind", ",", "has\u00b7set", "noch", ",", "weil", "sie", "nicht", "liebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "ADV", "ART", "NN", "$,", "VVFIN", "ADV", "$,", "KOUS", "PPER", "PTKNEG", "VVFIN", "$."], "meter": "+--+--+-+--+-+", "measure": "dactylic.di.plus"}, "line.13": {"text": "Trotzig schauet und k\u00fchn aus finstern Wimpern der J\u00fcngling,", "tokens": ["Trot\u00b7zig", "schau\u00b7et", "und", "k\u00fchn", "aus", "fins\u00b7tern", "Wim\u00b7pern", "der", "J\u00fcng\u00b7ling", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "KON", "ADJD", "APPR", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.14": {"text": "Und, geh\u00e4rtet zum Kampf, spannet die Sehne sich an.", "tokens": ["Und", ",", "ge\u00b7h\u00e4r\u00b7tet", "zum", "Kampf", ",", "span\u00b7net", "die", "Seh\u00b7ne", "sich", "an", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VVPP", "APPRART", "NN", "$,", "VVFIN", "ART", "NN", "PRF", "PTKVZ", "$."], "meter": "--+--++--+--+", "measure": "anapaest.di.plus"}, "line.15": {"text": "Fern in der Speere Gew\u00fchl und auf die st\u00e4ubende Rennbahn", "tokens": ["Fern", "in", "der", "Spee\u00b7re", "Ge\u00b7w\u00fchl", "und", "auf", "die", "st\u00e4u\u00b7ben\u00b7de", "Renn\u00b7bahn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "KON", "APPR", "ART", "ADJA", "NN"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.16": {"text": "Ruft ihn der lockende Ruhm, rei\u00dft ihn der brausende Mut.", "tokens": ["Ruft", "ihn", "der", "lo\u00b7cken\u00b7de", "Ruhm", ",", "rei\u00dft", "ihn", "der", "brau\u00b7sen\u00b7de", "Mut", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.17": {"text": "Jetzt besch\u00fctze dein Werk, Natur! Auseinander auf immer", "tokens": ["Jetzt", "be\u00b7sch\u00fct\u00b7ze", "dein", "Werk", ",", "Na\u00b7tur", "!", "Aus\u00b7ein\u00b7an\u00b7der", "auf", "im\u00b7mer"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$,", "NN", "$.", "NN", "APPR", "ADV"], "meter": "+-+--+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.18": {"text": "Fliehet, wenn du nicht vereinst, feindlich, was ewig sich sucht.", "tokens": ["Flie\u00b7het", ",", "wenn", "du", "nicht", "ver\u00b7einst", ",", "feind\u00b7lich", ",", "was", "e\u00b7wig", "sich", "sucht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PPER", "PTKNEG", "VVPP", "$,", "ADJD", "$,", "PRELS", "ADJD", "PRF", "VVFIN", "$."], "meter": "+-+-+-++--+--+", "measure": "trochaic.septa.relaxed"}, "line.19": {"text": "Aber da bist du, du M\u00e4chtige, schon, aus dem wildesten Streite", "tokens": ["A\u00b7ber", "da", "bist", "du", ",", "du", "M\u00e4ch\u00b7ti\u00b7ge", ",", "schon", ",", "aus", "dem", "wil\u00b7des\u00b7ten", "Strei\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "PPER", "$,", "PPER", "NN", "$,", "ADV", "$,", "APPR", "ART", "ADJA", "NN"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.20": {"text": "Rufst du der Harmonie g\u00f6ttlichen Frieden hervor.", "tokens": ["Rufst", "du", "der", "Har\u00b7mo\u00b7nie", "g\u00f6tt\u00b7li\u00b7chen", "Frie\u00b7den", "her\u00b7vor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-++--+--+", "measure": "iambic.hexa.relaxed"}, "line.21": {"text": "Tief verstummet die l\u00e4rmende Jagd, des rauschenden Tages", "tokens": ["Tief", "ver\u00b7stum\u00b7met", "die", "l\u00e4r\u00b7men\u00b7de", "Jagd", ",", "des", "rau\u00b7schen\u00b7den", "Ta\u00b7ges"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.22": {"text": "Tosen verhallet, und leis sinken die Sterne herab.", "tokens": ["To\u00b7sen", "ver\u00b7hal\u00b7let", ",", "und", "leis", "sin\u00b7ken", "die", "Ster\u00b7ne", "her\u00b7ab", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "KON", "ADJD", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.23": {"text": "Seufzend fl\u00fcstert das Rohr, sanft murmelnd gleiten die B\u00e4che,", "tokens": ["Seuf\u00b7zend", "fl\u00fcs\u00b7tert", "das", "Rohr", ",", "sanft", "mur\u00b7melnd", "glei\u00b7ten", "die", "B\u00e4\u00b7che", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$,", "ADJD", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.24": {"text": "Und mit melodischem Lied f\u00fcllt Philomela den Hain.", "tokens": ["Und", "mit", "me\u00b7lo\u00b7di\u00b7schem", "Lied", "f\u00fcllt", "Phi\u00b7lo\u00b7me\u00b7la", "den", "Hain", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "VVFIN", "NE", "ART", "NN", "$."], "meter": "-+-+---+-+-+-+", "measure": "unknown.measure.hexa"}, "line.25": {"text": "Was erreget zu Seufzern der Jungfrau steigenden Busen?", "tokens": ["Was", "er\u00b7re\u00b7get", "zu", "Seuf\u00b7zern", "der", "Jung\u00b7frau", "stei\u00b7gen\u00b7den", "Bu\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPR", "NN", "ART", "NN", "ADJA", "NN", "$."], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.26": {"text": "J\u00fcngling, was f\u00fcllet den Blick schwellend mit Tr\u00e4nen dir an?", "tokens": ["J\u00fcng\u00b7ling", ",", "was", "f\u00fcl\u00b7let", "den", "Blick", "schwel\u00b7lend", "mit", "Tr\u00e4\u00b7nen", "dir", "an", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWS", "VVFIN", "ART", "NN", "ADJD", "APPR", "NN", "PPER", "PTKVZ", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.27": {"text": "Ach, sie suchet umsonst, was sie sanft anschmiegend umfasse,", "tokens": ["Ach", ",", "sie", "su\u00b7chet", "um\u00b7sonst", ",", "was", "sie", "sanft", "an\u00b7schmie\u00b7gend", "um\u00b7fas\u00b7se", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VVFIN", "ADV", "$,", "PRELS", "PPER", "ADJD", "ADJD", "VVFIN", "$,"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.28": {"text": "Und die schwellende Frucht beuget zur Erde die Last.", "tokens": ["Und", "die", "schwel\u00b7len\u00b7de", "Frucht", "beu\u00b7get", "zur", "Er\u00b7de", "die", "Last", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "APPRART", "NN", "ART", "NN", "$."], "meter": "--+--++--+--+", "measure": "anapaest.di.plus"}, "line.29": {"text": "Ruhelos strebend verzehrt sich in eigenen Flammen der J\u00fcngling,", "tokens": ["Ru\u00b7he\u00b7los", "stre\u00b7bend", "ver\u00b7zehrt", "sich", "in", "ei\u00b7ge\u00b7nen", "Flam\u00b7men", "der", "J\u00fcng\u00b7ling", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "VVFIN", "PRF", "APPR", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.30": {"text": "Ach, der brennenden Glut wehet kein lindernder Hauch.", "tokens": ["Ach", ",", "der", "bren\u00b7nen\u00b7den", "Glut", "we\u00b7het", "kein", "lin\u00b7dern\u00b7der", "Hauch", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ART", "ADJA", "NN", "VVFIN", "PIAT", "ADJA", "NN", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.31": {"text": "Siehe, da finden sie sich, es f\u00fchret sie Amor zusammen,", "tokens": ["Sie\u00b7he", ",", "da", "fin\u00b7den", "sie", "sich", ",", "es", "f\u00fch\u00b7ret", "sie", "A\u00b7mor", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "ADV", "VVFIN", "PPER", "PRF", "$,", "PPER", "VVFIN", "PPER", "NE", "PTKVZ", "$,"], "meter": "-+-+--+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.32": {"text": "Und dem gefl\u00fcgelten Gott folgt der gefl\u00fcgelte Sieg.", "tokens": ["Und", "dem", "ge\u00b7fl\u00fc\u00b7gel\u00b7ten", "Gott", "folgt", "der", "ge\u00b7fl\u00fc\u00b7gel\u00b7te", "Sieg", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--++--+--+", "measure": "iambic.hexa.relaxed"}, "line.33": {"text": "G\u00f6ttliche Liebe, du bists, die der Menschheit Blumen vereinigt,", "tokens": ["G\u00f6tt\u00b7li\u00b7che", "Lie\u00b7be", ",", "du", "bists", ",", "die", "der", "Menschheit", "Blu\u00b7men", "ver\u00b7ei\u00b7nigt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPER", "ADV", "$,", "PRELS", "ART", "NN", "NN", "VVPP", "$,"], "meter": "+--+--+---+--+-", "measure": "dactylic.tri.plus"}, "line.34": {"text": "Ewig getrennt, sind sie doch ewig verbunden durch dich.", "tokens": ["E\u00b7wig", "ge\u00b7trennt", ",", "sind", "sie", "doch", "e\u00b7wig", "ver\u00b7bun\u00b7den", "durch", "dich", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,", "VAFIN", "PPER", "ADV", "ADJD", "VVPP", "APPR", "PPER", "$."], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}}, "stanza.2": {"line.1": {"text": "Sieh in dem zarten Kind zwei liebliche Blumen vereinigt,", "tokens": ["Sieh", "in", "dem", "zar\u00b7ten", "Kind", "zwei", "lieb\u00b7li\u00b7che", "Blu\u00b7men", "ver\u00b7ei\u00b7nigt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "ADJA", "NN", "CARD", "ADJA", "NN", "VVPP", "$,"], "meter": "+--+-+-+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Jungfrau und J\u00fcngling, sie deckt beide die Knospe noch zu.", "tokens": ["Jung\u00b7frau", "und", "J\u00fcng\u00b7ling", ",", "sie", "deckt", "bei\u00b7de", "die", "Knos\u00b7pe", "noch", "zu", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "PPER", "VVFIN", "PIS", "ART", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+--+---+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Leise l\u00f6st sich das Band, es entzweien sich zart die Naturen,", "tokens": ["Lei\u00b7se", "l\u00f6st", "sich", "das", "Band", ",", "es", "ent\u00b7zwei\u00b7en", "sich", "zart", "die", "Na\u00b7tu\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "ART", "NN", "$,", "PPER", 