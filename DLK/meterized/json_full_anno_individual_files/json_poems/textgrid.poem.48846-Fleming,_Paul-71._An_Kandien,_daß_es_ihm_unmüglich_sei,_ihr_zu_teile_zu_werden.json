{"textgrid.poem.48846": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "71. An Kandien, da\u00df es ihm unm\u00fcglich sei, ihr zu teile zu werden", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie bitter mir es wird, wie hart ich bin verletzet,", "tokens": ["Wie", "bit\u00b7ter", "mir", "es", "wird", ",", "wie", "hart", "ich", "bin", "ver\u00b7let\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "PPER", "VAFIN", "$,", "PWAV", "ADJD", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "da\u00df, wei\u00dfe ", "tokens": ["da\u00df", ",", "wei\u00b7\u00dfe"], "token_info": ["word", "punct", "word"], "pos": ["KOUS", "$,", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "ach, das ist viel zu schwer, als da\u00df dir der Verdru\u00df", "tokens": ["ach", ",", "das", "ist", "viel", "zu", "schwer", ",", "als", "da\u00df", "dir", "der", "Ver\u00b7dru\u00df"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["XY", "$,", "PDS", "VAFIN", "ADV", "PTKA", "ADJD", "$,", "KOKOM", "KOUS", "PPER", "ART", "NN"], "meter": "+--+-+--+--+", "measure": "iambic.penta.invert"}, "line.4": {"text": "in diesem kurzen Brief kan werden aufgesetzet.", "tokens": ["in", "die\u00b7sem", "kur\u00b7zen", "Brief", "kan", "wer\u00b7den", "auf\u00b7ge\u00b7set\u00b7zet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "VMFIN", "VAINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Mein Mund ist von der Zeit mit Tr\u00e4nen noch genetzet,", "tokens": ["Mein", "Mund", "ist", "von", "der", "Zeit", "mit", "Tr\u00e4\u00b7nen", "noch", "ge\u00b7net\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "APPR", "ART", "NN", "APPR", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "als ich zu dir sprach: Schatz, das ist der letzte Gru\u00df!", "tokens": ["als", "ich", "zu", "dir", "sprach", ":", "Schatz", ",", "das", "ist", "der", "letz\u00b7te", "Gru\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "VVFIN", "$.", "NN", "$,", "PDS", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "und du, mein s\u00fc\u00dfer Trost, mir gabest einen Ku\u00df,", "tokens": ["und", "du", ",", "mein", "s\u00fc\u00b7\u00dfer", "Trost", ",", "mir", "ga\u00b7best", "ei\u00b7nen", "Ku\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "PPOSAT", "ADJA", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "der mich auch itzund noch betr\u00fcbet und ergetzet.", "tokens": ["der", "mich", "auch", "it\u00b7zund", "noch", "be\u00b7tr\u00fc\u00b7bet", "und", "er\u00b7get\u00b7zet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADV", "ADV", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ach, Sch\u00f6ne, straf mich nicht und gib mir keine Schuld,", "tokens": ["Ach", ",", "Sch\u00f6\u00b7ne", ",", "straf", "mich", "nicht", "und", "gib", "mir", "kei\u00b7ne", "Schuld", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "NN", "$,", "VVFIN", "PPER", "PTKNEG", "KON", "VVIMP", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "du kennst mich um und an. Rat deiner Ungedult,", "tokens": ["du", "kennst", "mich", "um", "und", "an", ".", "Rat", "dei\u00b7ner", "Un\u00b7ge\u00b7dult", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "KON", "PTKVZ", "$.", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "um die ich Kranker mich zu Tode noch betr\u00fcbe.", "tokens": ["um", "die", "ich", "Kran\u00b7ker", "mich", "zu", "To\u00b7de", "noch", "be\u00b7tr\u00fc\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "NN", "PPER", "APPR", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "La\u00df mich, dieweil ich mu\u00df. Schau, was mich von dir rei\u00dft,", "tokens": ["La\u00df", "mich", ",", "die\u00b7weil", "ich", "mu\u00df", ".", "Schau", ",", "was", "mich", "von", "dir", "rei\u00dft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "KOUS", "PPER", "VMFIN", "$.", "NN", "$,", "PRELS", "PRF", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "und sei mit dem vergn\u00fcgt, in dem du warlich wei\u00dft", "tokens": ["und", "sei", "mit", "dem", "ver\u00b7gn\u00fcgt", ",", "in", "dem", "du", "war\u00b7lic