{"dta.poem.12899": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Aus eben demselben.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Vergn\u00fcgungen, die schon ein gro\u00df ger\u00e4usche geben,", "tokens": ["Ver\u00b7gn\u00fc\u00b7gun\u00b7gen", ",", "die", "schon", "ein", "gro\u00df", "ge\u00b7r\u00e4u\u00b7sche", "ge\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ADV", "ART", "ADJD", "ADJA", "VVINF", "$,"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Sind lange nicht der kern vollkommner s\u00fc\u00dfigkeit.", "tokens": ["Sind", "lan\u00b7ge", "nicht", "der", "kern", "voll\u00b7komm\u00b7ner", "s\u00fc\u00b7\u00dfig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PTKNEG", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die liebe sucht die still\u2019 und die zufriedenheit.", "tokens": ["Die", "lie\u00b7be", "sucht", "die", "still'", "und", "die", "zu\u00b7frie\u00b7den\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "ART", "ADJD", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und also mu\u00df man nur nach einem buhler streben,", "tokens": ["Und", "al\u00b7so", "mu\u00df", "man", "nur", "nach", "ei\u00b7nem", "buh\u00b7ler", "stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PIS", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der diesen punct versteht, und dessen folg\u2019 erkiest:", "tokens": ["Der", "die\u00b7sen", "punct", "ver\u00b7steht", ",", "und", "des\u00b7sen", "fol\u00b7g'", "er\u00b7kiest", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PDAT", "NN", "VVFIN", "$,", "KON", "PDS", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.6": {"text": "Da\u00df die verschwiegenheit der liebe zucker ist.", "tokens": ["Da\u00df", "die", "ver\u00b7schwie\u00b7gen\u00b7heit", "der", "lie\u00b7be", "zu\u00b7cker", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Denn solche Thyrsi\u00dfe, die viel zu pralen wissen,", "tokens": ["Denn", "sol\u00b7che", "Thyr\u00b7si\u00b7\u00dfe", ",", "die", "viel", "zu", "pra\u00b7len", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$,", "PRELS", "PIS", "PTKZU", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Nicht aber das verstehn, da\u00df sie verr\u00e4ther seyn,", "tokens": ["Nicht", "a\u00b7ber", "das", "ver\u00b7stehn", ",", "da\u00df", "sie", "ver\u00b7r\u00e4\u00b7ther", "seyn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "PDS", "VVINF", "$,", "KOUS", "PPER", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "H\u00f6rt man nur \u00fcberall von ihrem gl\u00fccke schreyn,", "tokens": ["H\u00f6rt", "man", "nur", "\u00fc\u00b7be\u00b7rall", "von", "ih\u00b7rem", "gl\u00fc\u00b7cke", "schreyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "ADV", "APPR", "PPOSAT", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und singen, was sie hier und da vor gunst genie\u00dfen.", "tokens": ["Und", "sin\u00b7gen", ",", "was", "sie", "hier", "und", "da", "vor", "gunst", "ge\u00b7nie\u00b7\u00dfen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PRELS", "PPER", "ADV", "KON", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Denn ihrer unvernufft will dieses gar nicht ein:", "tokens": ["Denn", "ih\u00b7rer", "un\u00b7ver\u00b7nufft", "will", "die\u00b7ses", "gar", "nicht", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VMFIN", "PDS", "ADV", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Da\u00df Mars und Amor nicht von gleichem sinne seyn.", "tokens": ["Da\u00df", "Mars", "und", "A\u00b7mor", "nicht", "von", "glei\u00b7chem", "sin\u00b7ne", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NE", "PTKNEG", "APPR", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Denn Amor liebt die still, und Mars liebt das get\u00fcmmel;", "tokens": ["Denn", "A\u00b7mor", "liebt", "die", "still", ",", "und", "Mars", "liebt", "das", "ge\u00b7t\u00fcm\u00b7mel", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "ART", "ADJD", "$,", "KON", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Wo man die trommel r\u00fchrt, wo der trompeten schall,", "tokens": ["Wo", "man", "die", "trom\u00b7mel", "r\u00fchrt", ",", "wo", "der", "trom\u00b7pe\u00b7ten", "schall", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ART", "NN", "VVFIN", "$,", "PWAV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Die rauhe lufft erf\u00fcllt, und der carthaunen knall", "tokens": ["Die", "rau\u00b7he", "lufft", "er\u00b7f\u00fcllt", ",", "und", "der", "car\u00b7thau\u00b7nen", "knall"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,", "KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Ras\u2019t, donnert, kracht und tobt, da baut ihm Mars den himmel;", "tokens": ["Ras't", ",", "don\u00b7nert", ",", "kracht", "und", "tobt", ",", "da", "baut", "ihm", "Mars", "den", "him\u00b7mel", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Hingegen Amor will nach stillen h\u00f6len ziehn,", "tokens": ["Hin\u00b7ge\u00b7gen", "A\u00b7mor", "will", "nach", "stil\u00b7len", "h\u00f6\u00b7len", "ziehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "VMFIN", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.18": {"text": "Und auch den schw\u00e4chsten hall wie blitz und donner fliehn.", "tokens": ["Und", "auch", "den", "schw\u00e4chs\u00b7ten", "hall", "wie", "blitz", "und", "don\u00b7ner", "fliehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "KOKOM", "ADJD", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}