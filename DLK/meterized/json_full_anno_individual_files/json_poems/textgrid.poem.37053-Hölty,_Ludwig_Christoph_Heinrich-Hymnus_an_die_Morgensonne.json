{"textgrid.poem.37053": {"metadata": {"author": {"name": "H\u00f6lty, Ludwig Christoph Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Hymnus an die Morgensonne", "genre": "verse", "period": "N.A.", "pub_year": 1771, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "D\u00e4mmrung kleidet den Hayn in ihr graues Gewand,", "tokens": ["D\u00e4mm\u00b7rung", "klei\u00b7det", "den", "Hayn", "in", "ihr", "grau\u00b7es", "Ge\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Und die Kerzen der Nacht, die den Himmel begl\u00e4nzt,", "tokens": ["Und", "die", "Ker\u00b7zen", "der", "Nacht", ",", "die", "den", "Him\u00b7mel", "be\u00b7gl\u00e4nzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "$,", "PRELS", "ART", "NN", "VVPP", "$,"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.3": {"text": "Legen den Schleyer an, den der kommende Tag", "tokens": ["Le\u00b7gen", "den", "Schle\u00b7yer", "an", ",", "den", "der", "kom\u00b7men\u00b7de", "Tag"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "PTKVZ", "$,", "PRELS", "ART", "ADJA", "NN"], "meter": "+--+-+--+--+", "measure": "iambic.penta.invert"}, "line.4": {"text": "Rings um alle Gestirne webt.", "tokens": ["Rings", "um", "al\u00b7le", "Ge\u00b7stir\u00b7ne", "webt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Schweigen herrschet umher, nur posaunet der Hahn", "tokens": ["Schwei\u00b7gen", "herr\u00b7schet", "um\u00b7her", ",", "nur", "po\u00b7sau\u00b7net", "der", "Hahn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PTKVZ", "$,", "ADV", "VVFIN", "ART", "NN"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Seinen Morgengesang, und erwecket das Dorf,", "tokens": ["Sei\u00b7nen", "Mor\u00b7gen\u00b7ge\u00b7sang", ",", "und", "er\u00b7we\u00b7cket", "das", "Dorf", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Und erwecket den Tag, der sein graulichtes Aug", "tokens": ["Und", "er\u00b7we\u00b7cket", "den", "Tag", ",", "der", "sein", "grau\u00b7lich\u00b7tes", "Aug"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "PRELS", "PPOSAT", "ADJA", "NN"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.4": {"text": "Schon allm\u00e4hlig zu \u00f6ffnen beginnt.", "tokens": ["Schon", "all\u00b7m\u00e4h\u00b7lig", "zu", "\u00f6ff\u00b7nen", "be\u00b7ginnt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}}, "stanza.3": {"line.1": {"text": "Welche Nymphe besteigt itzt das rothe Gew\u00f6lk,", "tokens": ["Wel\u00b7che", "Nym\u00b7phe", "be\u00b7steigt", "itzt", "das", "ro\u00b7the", "Ge\u00b7w\u00f6lk", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Mit der Krone von Gold? Rosen bekr\u00e4nzen ihr Haar,", "tokens": ["Mit", "der", "Kro\u00b7ne", "von", "Gold", "?", "Ro\u00b7sen", "be\u00b7kr\u00e4n\u00b7zen", "ihr", "Haar", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NN", "$.", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "--+--++--+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Wo die G\u00f6ttliche geht, keimen Blumen hervor,", "tokens": ["Wo", "die", "G\u00f6tt\u00b7li\u00b7che", "geht", ",", "kei\u00b7men", "Blu\u00b7men", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$,", "PIAT", "NN", "PTKVZ", "$,"], "meter": "--+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "F\u00fcllen Balsamger\u00fcche die Luft.", "tokens": ["F\u00fcl\u00b7len", "Bal\u00b7sam\u00b7ge\u00b7r\u00fc\u00b7che", "die", "Luft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "ART", "NN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Sie bemahlet den Ost. Ists Aurora? Sie ists,", "tokens": ["Sie", "be\u00b7mah\u00b7let", "den", "Ost", ".", "Ists", "Au\u00b7ro\u00b7ra", "?", "Sie", "ists", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$.", "NE", "NE", "$.", "PPER", "VAFIN", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Sie, die Bothin des Tags! Freude tanzet ihr vor,", "tokens": ["Sie", ",", "die", "Bo\u00b7thin", "des", "Tags", "!", "Freu\u00b7de", "tan\u00b7zet", "ihr", "vor", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ART", "NN", "ART", "NN", "$.", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "--+--++-+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Heller wirbelt der Hayn, lauter gurgelt der Bach,", "tokens": ["Hel\u00b7ler", "wir\u00b7belt", "der", "Hayn", ",", "lau\u00b7ter", "gur\u00b7gelt", "der", "Bach", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "$,", "PIAT", "NN", "ART", "NN", "$,"], "meter": "+--+-+--+--+", "measure": "iambic.penta.invert"}, "line.4": {"text": "Durchs Gewinde des Veilchenthals.", "tokens": ["Durchs", "Ge\u00b7win\u00b7de", "des", "Veil\u00b7chen\u00b7thals", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.5": {"line.1": {"text": "Sie bestreuet die Bahn, welche die Sonne betritt,", "tokens": ["Sie", "be\u00b7streu\u00b7et", "die", "Bahn", ",", "wel\u00b7che", "die", "Son\u00b7ne", "be\u00b7tritt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+--+---+--+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Schon mit r\u00f6thlichten und mit goldenen", "tokens": ["Schon", "mit", "r\u00f6th\u00b7lich\u00b7ten", "und", "mit", "gol\u00b7de\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "KON", "APPR", "ADJA"], "meter": "--+--+-+--", "measure": "anapaest.di.plus"}, "line.3": {"text": "Blumen, wandelt voran, mit dem K\u00f6rbchen am Arm,", "tokens": ["Blu\u00b7men", ",", "wan\u00b7delt", "vo\u00b7ran", ",", "mit", "dem", "K\u00f6rb\u00b7chen", "am", "Arm", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PTKVZ", "$,", "APPR", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Den ihr Flora mit Rosen gef\u00fcllt.", "tokens": ["Den", "ihr", "Flo\u00b7ra", "mit", "Ro\u00b7sen", "ge\u00b7f\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "APPR", "NN", "VVPP", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Sonne, was harrest du? Wandle der Schwester nach,", "tokens": ["Son\u00b7ne", ",", "was", "har\u00b7rest", "du", "?", "Wand\u00b7le", "der", "Schwes\u00b7ter", "nach", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWS", "VVFIN", "PPER", "$.", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.2": {"text": "Die ihr K\u00f6rbchen bereits ganz von Blumen geleert,", "tokens": ["Die", "ihr", "K\u00f6rb\u00b7chen", "be\u00b7reits", "ganz", "von", "Blu\u00b7men", "ge\u00b7leert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADV", "ADV", "APPR", "NN", "VVPP", "$,"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.3": {"text": "Geh aus deinem Gezelt, Mutter des Tags! hervor,", "tokens": ["Geh", "aus", "dei\u00b7nem", "Ge\u00b7zelt", ",", "Mut\u00b7ter", "des", "Tags", "!", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "APPR", "PPOSAT", "NN", "$,", "NN", "ART", "NN", "$.", "PTKVZ", "$,"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Und befunkle den Silberbach.", "tokens": ["Und", "be\u00b7funk\u00b7le", "den", "Sil\u00b7ber\u00b7bach", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.7": {"line.1": {"text": "Sie zerrei\u00dfet den Schleyr, der ihr Auge verh\u00fcllt,", "tokens": ["Sie", "zer\u00b7rei\u00b7\u00dfet", "den", "Schleyr", ",", "der", "ihr", "Au\u00b7ge", "ver\u00b7h\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVPP", "$,"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.2": {"text": "Zeigt die blitzende Stirn, hebt ihr funkelndes Haupt,", "tokens": ["Zeigt", "die", "blit\u00b7zen\u00b7de", "Stirn", ",", "hebt", "ihr", "fun\u00b7keln\u00b7des", "Haupt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Welches die goldenen Locken umfliegen, empor,", "tokens": ["Wel\u00b7ches", "die", "gol\u00b7de\u00b7nen", "Lo\u00b7cken", "um\u00b7flie\u00b7gen", ",", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "ART", "ADJA", "NN", "VVINF", "$,", "PTKVZ", "$,"], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.4": {"text": "Blicket Munterkeit \u00fcber die Flur.", "tokens": ["Bli\u00b7cket", "Mun\u00b7ter\u00b7keit", "\u00fc\u00b7ber", "die", "Flur", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}}, "stanza.8": {"line.1": {"text": "Heil dir, Mutter des Lichts! Sie beschimmert den Hayn,", "tokens": ["Heil", "dir", ",", "Mut\u00b7ter", "des", "Lichts", "!", "Sie", "be\u00b7schim\u00b7mert", "den", "Hayn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "NN", "ART", "NN", "$.", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Der, am Fittig des Winds, auf dem Gebirge nickt,", "tokens": ["Der", ",", "am", "Fit\u00b7tig", "des", "Winds", ",", "auf", "dem", "Ge\u00b7bir\u00b7ge", "nickt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "APPRART", "NN", "ART", "NN", "$,", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Pr\u00e4gt ihr lachendes Bild in den Spiegel des Bachs,", "tokens": ["Pr\u00e4gt", "ihr", "la\u00b7chen\u00b7des", "Bild", "in", "den", "Spie\u00b7gel", "des", "Bachs", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "R\u00f6thet die Busen des Blumenvolks.", "tokens": ["R\u00f6\u00b7thet", "die", "Bu\u00b7sen", "des", "Blu\u00b7men\u00b7volks", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.9": {"line.1": {"text": "Wie der Puls der Natur itzt so jugendlich klopft!", "tokens": ["Wie", "der", "Puls", "der", "Na\u00b7tur", "itzt", "so", "ju\u00b7gend\u00b7lich", "klopft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "ADV", "ADV", "ADJD", "VVFIN", "$."], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.2": {"text": "Wie des Waldes Musik von den Wipfeln ert\u00f6nt!", "tokens": ["Wie", "des", "Wal\u00b7des", "Mu\u00b7sik", "von", "den", "Wip\u00b7feln", "er\u00b7t\u00f6nt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.3": {"text": "Wie die Blume stolziert, und ihr seidenes Kleid", "tokens": ["Wie", "die", "Blu\u00b7me", "stol\u00b7ziert", ",", "und", "ihr", "sei\u00b7de\u00b7nes", "Kleid"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$,", "KON", "PPER", "PPOSAT", "NN"], "meter": "--+-++--+--+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "In vergoldeten Purpur taucht!", "tokens": ["In", "ver\u00b7gol\u00b7de\u00b7ten", "Pur\u00b7pur", "taucht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.10": {"line.1": {"text": "Durch dein L\u00e4cheln erweckt, Wolkenbewandlerin!", "tokens": ["Durch", "dein", "L\u00e4\u00b7cheln", "er\u00b7weckt", ",", "Wol\u00b7ken\u00b7be\u00b7wand\u00b7le\u00b7rin", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$,", "PWAV", "$."], "meter": "+-+---+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Schreitet der rege Flei\u00df durch das Aehrengefild,", "tokens": ["Schrei\u00b7tet", "der", "re\u00b7ge", "Flei\u00df", "durch", "das", "A\u00b7eh\u00b7ren\u00b7ge\u00b7fild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+--+--++--+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Mengt das Sichelger\u00e4usch, und ein fr\u00f6hliges Lied,", "tokens": ["Mengt", "das", "Si\u00b7chel\u00b7ge\u00b7r\u00e4usch", ",", "und", "ein", "fr\u00f6h\u00b7li\u00b7ges", "Lied", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "KON", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "In das Morgengefl\u00fcster des Hayns.", "tokens": ["In", "das", "Mor\u00b7gen\u00b7ge\u00b7fl\u00fcs\u00b7ter", "des", "Hayns", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}}, "stanza.11": {"line.1": {"text": "Langsam wandelt der Hirt, und der Sch\u00e4fer aufs Feld,", "tokens": ["Lang\u00b7sam", "wan\u00b7delt", "der", "Hirt", ",", "und", "der", "Sch\u00e4\u00b7fer", "aufs", "Feld", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$,", "KON", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "\u00dcber den Morgenthau, welcher den Blumen entrollt,", "tokens": ["\u00dc\u00b7ber", "den", "Mor\u00b7gent\u00b7hau", ",", "wel\u00b7cher", "den", "Blu\u00b7men", "ent\u00b7rollt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "ART", "NN", "VVPP", "$,"], "meter": "+--+-++--+--+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Spielt auf seiner Schalmey s\u00fc\u00dfe Zufriedenheit,", "tokens": ["Spielt", "auf", "sei\u00b7ner", "Schal\u00b7mey", "s\u00fc\u00b7\u00dfe", "Zu\u00b7frie\u00b7den\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Und begr\u00fc\u00dfet den jungen Tag.", "tokens": ["Und", "be\u00b7gr\u00fc\u00b7\u00dfet", "den", "jun\u00b7gen", "Tag", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.12": {"line.1": {"text": "Fr\u00f6