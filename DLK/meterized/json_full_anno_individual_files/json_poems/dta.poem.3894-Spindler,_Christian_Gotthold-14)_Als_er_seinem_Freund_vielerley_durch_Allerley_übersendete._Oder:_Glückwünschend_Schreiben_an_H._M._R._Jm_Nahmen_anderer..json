{"dta.poem.3894": {"metadata": {"author": {"name": "Spindler, Christian Gotthold", "birth": "N.A.", "death": "N.A."}, "title": "14) Als er seinem Freund vielerley durch  \n Allerley \u00fcbersendete. Oder:  \n  Gl\u00fcckw\u00fcnschend Schreiben an H. M. R.  \n Jm Nahmen anderer.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-20581-9", "language": ["de:0.99"], "booktitle": "Spindler, Christian Gotthold: Unschuldige Jugend-Fr\u00fcchte. Leipzig, 1745."}, "poem": {"stanza.1": {"line.1": {"text": "Herr Bruder, ach die Nacht wird lang,", "tokens": ["Herr", "Bru\u00b7der", ",", "ach", "die", "Nacht", "wird", "lang", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "ADV", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Winter kommt gekrochen,", "tokens": ["Der", "Win\u00b7ter", "kommt", "ge\u00b7kro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Drum geh ich auf den Vogelfang,", "tokens": ["Drum", "geh", "ich", "auf", "den", "Vo\u00b7gel\u00b7fang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hann\u00df mag sich Thee-Kraut kochen.", "tokens": ["Hann\u00df", "mag", "sich", "Thee\u00b7Kraut", "ko\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PRF", "NE", "VVFIN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.5": {"text": "Mein Engel, gieb die Nadel her,", "tokens": ["Mein", "En\u00b7gel", ",", "gieb", "die", "Na\u00b7del", "her", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVIMP", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ich neh dir eine M\u00fctze.", "tokens": ["Ich", "neh", "dir", "ei\u00b7ne", "M\u00fct\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Es kommt wohl nicht von ohngefehr,", "tokens": ["Es", "kommt", "wohl", "nicht", "von", "ohn\u00b7ge\u00b7fehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKNEG", "APPR", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wenn Jungfern nicht viel n\u00fctze.", "tokens": ["Wenn", "Jung\u00b7fern", "nicht", "viel", "n\u00fct\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PTKNEG", "ADV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Was i\u00dft der Herr zum Morgen-Brodt?", "tokens": ["Was", "i\u00dft", "der", "Herr", "zum", "Mor\u00b7gen\u00b7Brodt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Candirte L\u00e4mmer-Schw\u00e4ntze,", "tokens": ["Can\u00b7dir\u00b7te", "L\u00e4m\u00b7mer\u00b7Schw\u00e4nt\u00b7ze", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Vorm Drachen uns bewahre GOtt,", "tokens": ["Vorm", "Dra\u00b7chen", "uns", "be\u00b7wah\u00b7re", "Gott", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Die M\u00e4gdgens tragen Kr\u00e4ntze.", "tokens": ["Die", "M\u00e4gd\u00b7gens", "tra\u00b7gen", "Kr\u00e4nt\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Mein Herr, will er was neues sehn,", "tokens": ["Mein", "Herr", ",", "will", "er", "was", "neu\u00b7es", "sehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VMFIN", "PPER", "PIS", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Cupido lernet reiten,", "tokens": ["Cu\u00b7pi\u00b7do", "ler\u00b7net", "rei\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "VVFIN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.15": {"text": "Und will auch mit zu Felde gehn,", "tokens": ["Und", "will", "auch", "mit", "zu", "Fel\u00b7de", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "APPR", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Wo sie im Finstern streiten:", "tokens": ["Wo", "sie", "im", "Fins\u00b7tern", "strei\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.17": {"text": "Drum schenckt der Braut ein Ordens. Band,", "tokens": ["Drum", "schenckt", "der", "Braut", "ein", "Or\u00b7dens", ".", "Band", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "NN", "ART", "NN", "$.", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Sie darff sich nicht bedancken,", "tokens": ["Sie", "darff", "sich", "nicht", "be\u00b7dan\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.19": {"text": "Es l\u00e4st doch eben auch ", "tokens": ["Es", "l\u00e4st", "doch", "e\u00b7ben", "auch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.20": {"text": "Wer wolte darum zancken.", "tokens": ["Wer", "wol\u00b7te", "da\u00b7rum", "zan\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PAV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.21": {"text": "Bey Fetz ist eine neue Tracht", "tokens": ["Bey", "Fetz", "ist", "ei\u00b7ne", "neu\u00b7e", "Tracht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Von M\u00fctzen erst erfunden,", "tokens": ["Von", "M\u00fct\u00b7zen", "erst", "er\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.23": {"text": "Die braucht ein Ehmann in der Nacht,", "tokens": ["Die", "braucht", "ein", "Eh\u00b7mann", "in", "der", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Und sonst bey m\u00fc\u00dfgen Stunden.", "tokens": ["Und", "sonst", "bey", "m\u00fc\u00df\u00b7gen", "Stun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.25": {"text": "Die alten Ziemer f\u00e4nget man", "tokens": ["Die", "al\u00b7ten", "Zie\u00b7mer", "f\u00e4n\u00b7get", "man"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PIS"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Mit Netzen und mit Stricken.", "tokens": ["Mit", "Net\u00b7zen", "und", "mit", "Stri\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.27": {"text": "Seht doch des ", "tokens": ["Seht", "doch", "des"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ADV", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.28": {"text": "Es l\u00e4sset sich mit r\u00fccken.", "tokens": ["Es", "l\u00e4s\u00b7set", "sich", "mit", "r\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.29": {"text": "Gebratne W\u00fcrst und Sauerkraut", "tokens": ["Ge\u00b7brat\u00b7ne", "W\u00fcrst", "und", "Sau\u00b7er\u00b7kraut"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Sind gut vors kalte Fieber,", "tokens": ["Sind", "gut", "vors", "kal\u00b7te", "Fie\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.31": {"text": "Ey wie dem armen Kinde graut,", "tokens": ["Ey", "wie", "dem", "ar\u00b7men", "Kin\u00b7de", "graut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Sie h\u00e4lt die H\u00e4nde dr\u00fcber.", "tokens": ["Sie", "h\u00e4lt", "die", "H\u00e4n\u00b7de", "dr\u00fc\u00b7ber", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PAV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.33": {"text": "Es geht recht n\u00e4rrisch in der Welt,", "tokens": ["Es", "geht", "recht", "n\u00e4r\u00b7risch", "in", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Mops will Satyren schreiben.", "tokens": ["Mops", "will", "Sa\u00b7ty\u00b7ren", "schrei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.35": {"text": "Was thut der Teutsche nicht ums Geld,", "tokens": ["Was", "thut", "der", "Teut\u00b7sche", "nicht", "ums", "Geld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "PTKNEG", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "Die M\u00e4use zu vertreiben?", "tokens": ["Die", "M\u00e4u\u00b7se", "zu", "ver\u00b7trei\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.37": {"text": "B\u00fcrstinchen wei\u00df sich trefflich viel", "tokens": ["B\u00fcrs\u00b7tin\u00b7chen", "wei\u00df", "sich", "treff\u00b7lich", "viel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PRF", "ADJD", "ADV"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.38": {"text": "Mit ihrer Klapper-M\u00fchle,", "tokens": ["Mit", "ih\u00b7rer", "Klap\u00b7per\u00b7M\u00fch\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.39": {"text": "Hann\u00df Dampff gerieth ins Damen-Spiel,", "tokens": ["Hann\u00df", "Dampff", "ge\u00b7rieth", "ins", "Da\u00b7men\u00b7Spiel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.40": {"text": "Da wars verzweiffelt schw\u00fcle.", "tokens": ["Da", "wars", "ver\u00b7zweif\u00b7felt", "schw\u00fc\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.41": {"text": "In Nummer ", "tokens": ["In", "Num\u00b7mer"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.42": {"text": "Die L\u00e4use hintern Ohren,", "tokens": ["Die", "L\u00e4u\u00b7se", "hin\u00b7tern", "Oh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.43": {"text": "Herr ", "tokens": ["Herr"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.44": {"text": "Das Wasser ist gefrohren;", "tokens": ["Das", "Was\u00b7ser", "ist", "ge\u00b7froh\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.45": {"text": "Zwar steht das Wasser-H\u00e4u\u00dfgen noch,", "tokens": ["Zwar", "steht", "das", "Was\u00b7ser\u00b7H\u00b7\u00e4u\u00df\u00b7gen", "noch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.46": {"text": "Doch la\u00df dich nicht erwischen,", "tokens": ["Doch", "la\u00df", "dich", "nicht", "er\u00b7wi\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.47": {"text": "In einem ausgesch\u00f6pfften Loch", "tokens": ["In", "ei\u00b7nem", "aus\u00b7ge\u00b7sch\u00f6pff\u00b7ten", "Loch"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.48": {"text": "Giebts weiter nichts zu fischen.", "tokens": ["Giebts", "wei\u00b7ter", "nichts", "zu", "fi\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PIS", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.49": {"text": "Gebt Toback und Sartellen her,", "tokens": ["Gebt", "To\u00b7back", "und", "Sar\u00b7tel\u00b7len", "her", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "KON", "NN", "PTKVZ", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.50": {"text": "Ich will nach Haarburg reisen,", "tokens": ["Ich", "will", "nach", "Haar\u00b7burg", "rei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.51": {"text": "Ob gleich der Beutel ziemlich leer,", "tokens": ["Ob", "gleich", "der", "Beu\u00b7tel", "ziem\u00b7lich", "leer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.52": {"text": "Ist doch mein Spie\u00df von Eisen.", "tokens": ["Ist", "doch", "mein", "Spie\u00df", "von", "Ei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.53": {"text": "Ein alter Fuchs kam nach Athen", "tokens": ["Ein", "al\u00b7ter", "Fuchs", "kam", "nach", "A\u00b7then"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NE", "VVFIN", "APPR", "NE"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.54": {"text": "Und machte Querllereyen,", "tokens": ["Und", "mach\u00b7te", "Querl\u00b7le\u00b7re\u00b7yen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.55": {"text": "Er solte gar ins ", "tokens": ["Er", "sol\u00b7te", "gar", "ins"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "APPRART"], "meter": "-+-+-", "measure": "iambic.di"}, "line.56": {"text": "Um seine Schwermereyen.", "tokens": ["Um", "sei\u00b7ne", "Schwer\u00b7me\u00b7re\u00b7yen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.57": {"text": "Doch legte sich ", "tokens": ["Doch", "leg\u00b7te", "sich"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVFIN", "PRF"], "meter": "-+-+", "measure": "iambic.di"}, "line.58": {"text": "Und rieth zum ", "tokens": ["Und", "rieth", "zum"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVFIN", "APPRART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.59": {"text": "Damit er k\u00f6nnte, nebst Latein,", "tokens": ["Da\u00b7mit", "er", "k\u00f6nn\u00b7te", ",", "nebst", "La\u00b7tein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VMFIN", "$,", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.60": {"text": "Das ", "tokens": ["Das"], "token_info": ["word"], "pos": ["PDS"], "meter": "-", "measure": "single.down"}, "line.61": {"text": "Caldaunen und ein altes Weib", "tokens": ["Cal\u00b7dau\u00b7nen", "und", "ein", "al\u00b7tes", "Weib"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.62": {"text": "Sind gut vor b\u00f6se Winde,", "tokens": ["Sind", "gut", "vor", "b\u00f6\u00b7se", "Win\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.63": {"text": "Drum spiele ich zum Zeitvertreib,", "tokens": ["Drum", "spie\u00b7le", "ich", "zum", "Zeit\u00b7ver\u00b7treib", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.64": {"text": "Mit einer Nabel-Binde.", "tokens": ["Mit", "ei\u00b7ner", "Na\u00b7bel\u00b7Bin\u00b7de", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.65": {"text": "Soldaten macht der Degen kund,", "tokens": ["Sol\u00b7da\u00b7ten", "macht", "der", "De\u00b7gen", "kund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.66": {"text": "Wohl auf dem Garten-Hause,", "tokens": ["Wohl", "auf", "dem", "Gar\u00b7ten\u00b7Hau\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.67": {"text": "Ey! Bruder, mach es nicht zu bund,", "tokens": ["Ey", "!", "Bru\u00b7der", ",", "mach", "es", "nicht", "zu", "bund", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "NN", "$,", "VVFIN", "PPER", "PTKNEG", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.68": {"text": "Der Mann kommt bald vom Schmause.", "tokens": ["Der", "Mann", "kommt", "bald", "vom", "Schmau\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.69": {"text": "Das B\u00fcrsten ist gewi\u00dflich gut,", "tokens": ["Das", "B\u00fcrs\u00b7ten", "ist", "ge\u00b7wi\u00df\u00b7lich", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.70": {"text": "Zumahl vor fromme Weiber,", "tokens": ["Zu\u00b7mahl", "vor", "from\u00b7me", "Wei\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.71": {"text": "Ey! Ey! ", "tokens": ["Ey", "!", "Ey", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$.", "NN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.72": {"text": "Hier kommt der Esel-Treiber.", "tokens": ["Hier", "kommt", "der", "E\u00b7sel\u00b7Trei\u00b7ber", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.73": {"text": "Die rothe Farbe ist beliebt,", "tokens": ["Die", "ro\u00b7the", "Far\u00b7be", "ist", "be\u00b7liebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.74": {"text": "Ich trage schwartze Kleider,", "tokens": ["Ich", "tra\u00b7ge", "schwart\u00b7ze", "Klei\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.75": {"text": "Und wenn es nichts zu flicken giebt,", "tokens": ["Und", "wenn", "es", "nichts", "zu", "fli\u00b7cken", "giebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PIS", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.76": {"text": "Bin ich ein armer Schneider.", "tokens": ["Bin", "ich", "ein", "ar\u00b7mer", "Schnei\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NE", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.77": {"text": "Clorinde nahm ", "tokens": ["Clo\u00b7rin\u00b7de", "nahm"], "token_info": ["word", "word"], "pos": ["NE", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.78": {"text": "Von einem Spa\u00df-", "tokens": ["Von", "ei\u00b7nem", "Spa\u00df"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "TRUNC"], "meter": "-+-+", "measure": "iambic.di"}, "line.79": {"text": "Das sah ihr k\u00fcnfftger Ehe-Mann,", "tokens": ["Das", "sah", "ihr", "k\u00fcnfft\u00b7ger", "E\u00b7he\u00b7Mann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.80": {"text": "Drum tranck er Thee mit Sahne,", "tokens": ["Drum", "tranck", "er", "Thee", "mit", "Sah\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.81": {"text": "Damit ihm keine Ubligkeit", "tokens": ["Da\u00b7mit", "ihm", "kei\u00b7ne", "Ub\u00b7lig\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.82": {"text": "Den Magen schw\u00e4chen m\u00f6chte,", "tokens": ["Den", "Ma\u00b7gen", "schw\u00e4\u00b7chen", "m\u00f6ch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.83": {"text": "Wenn er ihr etwa Messens-Zeit", "tokens": ["Wenn", 