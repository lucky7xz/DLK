{"textgrid.poem.26507": {"metadata": {"author": {"name": "Saar, Ferdinand von", "birth": "N.A.", "death": "N.A."}, "title": "1L: K\u00e4mpfen willst du mit mir \u2013 und siehst mich im Geiste besiegt schon", "genre": "verse", "period": "N.A.", "pub_year": 1869, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "K\u00e4mpfen willst du mit mir \u2013 und siehst mich im Geiste besiegt schon", "tokens": ["K\u00e4mp\u00b7fen", "willst", "du", "mit", "mir", "\u2013", "und", "siehst", "mich", "im", "Geis\u00b7te", "be\u00b7siegt", "schon"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VMFIN", "PPER", "APPR", "PPER", "$(", "KON", "VVFIN", "PPER", "APPRART", "NN", "VVFIN", "ADV"], "meter": "+-+-+--+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Durch dein gewaltiges Lied, eh' du gesungen es noch.", "tokens": ["Durch", "dein", "ge\u00b7wal\u00b7ti\u00b7ges", "Lied", ",", "eh'", "du", "ge\u00b7sun\u00b7gen", "es", "noch", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,", "KOUS", "PPER", "VVPP", "PPER", "ADV", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.3": {"text": "W\u00e4chst dir nur dann die Kraft, wenn du sie mit meiner vergleichest?", "tokens": ["W\u00e4chst", "dir", "nur", "dann", "die", "Kraft", ",", "wenn", "du", "sie", "mit", "mei\u00b7ner", "ver\u00b7glei\u00b7chest", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "ART", "NN", "$,", "KOUS", "PPER", "PPER", "APPR", "PPOSAT", "VVFIN", "$."], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Reizt dich darum nur der Preis, weil du ihn And'ren entziehst?", "tokens": ["Reizt", "dich", "da\u00b7rum", "nur", "der", "Preis", ",", "weil", "du", "ihn", "An\u00b7d'\u00b7ren", "ent\u00b7ziehst", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PAV", "ADV", "ART", "NN", "$,", "KOUS", "PPER", "PPER", "NN", "VVFIN", "$."], "meter": "+--+--+-+--+--+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Sieh', ich verschm\u00e4h' es zu k\u00e4mpfen \u2013 es sei denn mit ehernen Waffen:", "tokens": ["Sieh'", ",", "ich", "ver\u00b7schm\u00e4h'", "es", "zu", "k\u00e4mp\u00b7fen", "\u2013", "es", "sei", "denn", "mit", "e\u00b7her\u00b7nen", "Waf\u00b7fen", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VVFIN", "PPER", "PTKZU", "VVINF", "$(", "PPER", "VAFIN", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "++-+--+--+--+--+-", "measure": "trochaic.septa.relaxed"}, "line.6": {"text": "Nimmer ein Schwert ist das Lied, nimmer ein Schlachtfeld die Kunst.", "tokens": ["Nim\u00b7mer", "ein", "Schwert", "ist", "das", "Lied", ",", "nim\u00b7mer", "ein", "Schlacht\u00b7feld", "die", "Kunst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "ART", "NN", "$,", "ADV", "ART", "NN", "ART", "NN", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.7": {"text": "Nur am eigenen Drang erprobt' ich mich stets, und umschlingen", "tokens": ["Nur", "am", "ei\u00b7ge\u00b7nen", "Drang", "er\u00b7probt'", "ich", "mich", "stets", ",", "und", "um\u00b7schlin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "APPRART", "ADJA", "NN", "VVFIN", "PPER", "PRF", "ADV", "$,", "KON", "VVINF"], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"