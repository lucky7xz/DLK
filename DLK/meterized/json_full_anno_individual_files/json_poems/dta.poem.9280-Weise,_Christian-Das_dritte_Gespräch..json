{"dta.poem.9280": {"metadata": {"author": {"name": "Weise, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Das dritte Gespr\u00e4ch.", "genre": "Lyrik; Drama; Prosa", "period": "N.A.", "pub_year": "1701", "urn": "urn:nbn:de:kobv:b4-25043-0", "language": ["de:0.99"], "booktitle": "Weise, Christian: \u00dcberfl\u00fc\u00dfige Gedancken Der gr\u00fcnenden jugend. Leipzig, 1701."}, "poem": {"stanza.1": {"line.1": {"text": "Jhr hertzen die ihr euch der s\u00fcssen eitelkeit/", "tokens": ["Ihr", "hert\u00b7zen", "die", "ihr", "euch", "der", "s\u00fcs\u00b7sen", "ei\u00b7tel\u00b7keit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "PPER", "PRF", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Durch liebe/ gunst und treu zu widmen willens seyd/", "tokens": ["Durch", "lie\u00b7be", "/", "gunst", "und", "treu", "zu", "wid\u00b7men", "wil\u00b7lens", "seyd", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "$(", "ADJD", "KON", "ADJD", "PTKZU", "VVINF", "NN", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Fangt wol und gl\u00fccklich an/ erf\u00fcllet das verlangen/", "tokens": ["Fangt", "wol", "und", "gl\u00fcck\u00b7lich", "an", "/", "er\u00b7f\u00fcl\u00b7let", "das", "ver\u00b7lan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "KON", "ADJD", "PTKVZ", "$(", "VVFIN", "PDS", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und last euch in das garn der eiteln sch\u00f6nheit fangen:", "tokens": ["Und", "last", "euch", "in", "das", "garn", "der", "ei\u00b7teln", "sch\u00f6n\u00b7heit", "fan\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ergebet euren sinn an diesen glatten schein/", "tokens": ["Er\u00b7ge\u00b7bet", "eu\u00b7ren", "sinn", "an", "die\u00b7sen", "glat\u00b7ten", "schein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "PDAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und senckt die seelen selbst in jungfer-seelen ein.", "tokens": ["Und", "senckt", "die", "see\u00b7len", "selbst", "in", "jung\u00b7fer\u00b7see\u00b7len", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADV", "APPR", "ADJA", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Man mu\u00df sich der natur auch in den liebes-sachen", "tokens": ["Man", "mu\u00df", "sich", "der", "na\u00b7tur", "auch", "in", "den", "lie\u00b7bes\u00b7sa\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VMFIN", "PRF", "ART", "NN", "ADV", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Als ein gehorsam kind recht unterw\u00fcrffig machen.", "tokens": ["Als", "ein", "ge\u00b7hor\u00b7sam", "kind", "recht", "un\u00b7ter\u00b7w\u00fcrf\u00b7fig", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJD", "NN", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Und wer nicht lieben wil der mu\u00df ein kieselstem/", "tokens": ["Und", "wer", "nicht", "lie\u00b7ben", "wil", "der", "mu\u00df", "ein", "kie\u00b7sels\u00b7tem", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PTKNEG", "VVINF", "VMFIN", "ART", "VMFIN", "ART", "ADJA", "$("], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Ein stieffkind dieser welt/ ein weicher schneeball seyn.", "tokens": ["Ein", "stieff\u00b7kind", "die\u00b7ser", "welt", "/", "ein", "wei\u00b7cher", "schnee\u00b7ball", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PDAT", "NN", "$(", "ART", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Doch eh ihr euer thun der liebes-lust ergebet/", "tokens": ["Doch", "eh", "ihr", "eu\u00b7er", "thun", "der", "lie\u00b7bes\u00b7lust", "er\u00b7ge\u00b7bet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPOSAT", "NN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und weil das hertze noch in voller freyheit lebet/", "tokens": ["Und", "weil", "das", "hert\u00b7ze", "noch", "in", "vol\u00b7ler", "frey\u00b7heit", "le\u00b7bet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "So denckt der sache nach/ wie solche schmeicheley/", "tokens": ["So", "denckt", "der", "sa\u00b7che", "nach", "/", "wie", "sol\u00b7che", "schmei\u00b7che\u00b7ley", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "APPR", "$(", "KOKOM", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Wenn zeit und stunde k\u00f6mt/ wol anzustellen sey.", "tokens": ["Wenn", "zeit", "und", "stun\u00b7de", "k\u00f6mt", "/", "wol", "an\u00b7zu\u00b7stel\u00b7len", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "ADJA", "VVFIN", "$(", "ADV", "VVIZU", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Man mu\u00df sich offt ein nest im warmen sommer bauen", "tokens": ["Man", "mu\u00df", "sich", "offt", "ein", "nest", "im", "war\u00b7men", "som\u00b7mer", "bau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VMFIN", "PRF", "ADV", "ART", "ADV", "APPRART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Das erst im winter dient: und itzo mu\u00df man schauen", "tokens": ["Das", "erst", "im", "win\u00b7ter", "dient", ":", "und", "it\u00b7zo", "mu\u00df", "man", "schau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDS", "ADV", "APPRART", "ADJA", "NN", "$.", "KON", "ADV", "VMFIN", "PIS", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Was k\u00fcnfftig werden kan. Wer erst den samen sucht", "tokens": ["Was", "k\u00fcnff\u00b7tig", "wer\u00b7den", "kan", ".", "Wer", "erst", "den", "sa\u00b7men", "sucht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADJD", "VAINF", "VMFIN", "$.", "PWS", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Indem er erndten soll/ der wird mit seiner frucht", "tokens": ["In\u00b7dem", "er", "ernd\u00b7ten", "soll", "/", "der", "wird", "mit", "sei\u00b7ner", "frucht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVINF", "VMFIN", "$(", "ART", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Vortrefflich kahl bestehn/ ein schiffmann/ wil er schiffen/", "tokens": ["Vor\u00b7treff\u00b7lich", "kahl", "be\u00b7stehn", "/", "ein", "schiff\u00b7mann", "/", "wil", "er", "schif\u00b7fen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVINF", "$(", "ART", "VVFIN", "$(", "VMFIN", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "So hat er allbereit dieselbe kunst begriffen:", "tokens": ["So", "hat", "er", "all\u00b7be\u00b7reit", "die\u00b7sel\u00b7be", "kunst", "be\u00b7grif\u00b7fen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PDAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Wer mahlt ein zierlich bild; wer heist ein lautenist?", "tokens": ["Wer", "mahlt", "ein", "zier\u00b7lich", "bild", ";", "wer", "heist", "ein", "lau\u00b7te\u00b7nist", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "ADJD", "NN", "$.", "PWS", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Der bey dem meister nicht zuvor ein sch\u00fcler ist?", "tokens": ["Der", "bey", "dem", "meis\u00b7ter", "nicht", "zu\u00b7vor", "ein", "sch\u00fc\u00b7ler", "ist", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "PTKNEG", "ADV", "ART", "ADJA", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Und also wird uns zwar die sch\u00f6ne kunst zu lieben", "tokens": ["Und", "al\u00b7so", "wird", "uns", "zwar", "die", "sch\u00f6\u00b7ne", "kunst", "zu", "lie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ADV", "ART", "ADJA", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Durch unser eltern blut urspr\u00fcnglich eingeschrieben?", "tokens": ["Durch", "un\u00b7ser", "el\u00b7tern", "blut", "ur\u00b7spr\u00fcng\u00b7lich", "ein\u00b7ge\u00b7schrie\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Doch weder halb noch gantz: die flamme f\u00fchlt man wohl:", "tokens": ["Doch", "we\u00b7der", "halb", "noch", "gantz", ":", "die", "flam\u00b7me", "f\u00fchlt", "man", "wohl", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KON", "ADJD", "ADV", "ADV", "$.", "ART", "ADJA", "VVFIN", "PIS", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Nicht aber wie man sie mit klugheit f\u00fchren sol/", "tokens": ["Nicht", "a\u00b7ber", "wie", "man", "sie", "mit", "klug\u00b7heit", "f\u00fch\u00b7ren", "sol", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "KOKOM", "PIS", "PPER", "APPR", "NN", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Man mu\u00df ein sch\u00fcler seyn: man mu\u00df die edlen gaben", "tokens": ["Man", "mu\u00df", "ein", "sch\u00fc\u00b7ler", "seyn", ":", "man", "mu\u00df", "die", "ed\u00b7len", "ga\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PIS", "VMFIN", "ART", "ADJD", "VAINF", "$.", "PIS", "VMFIN", "ART", "ADJA", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Der rechten h\u00f6fligkeit aus der erfahrung haben.", "tokens": ["Der", "rech\u00b7ten", "h\u00f6f\u00b7lig\u00b7keit", "aus", "der", "er\u00b7fah\u00b7rung", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Drum die ihr lieben wolt/ komt her und gebt euch an/", "tokens": ["Drum", "die", "ihr", "lie\u00b7ben", "wolt", "/", "komt", "her", "und", "gebt", "euch", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "PPER", "VVINF", "VMFIN", "$(", "VVFIN", "PTKVZ", "KON", "VVFIN", "PPER", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Hier tritt ein meister auff/ der euch vergn\u00fcgen kan/", "tokens": ["Hier", "tritt", "ein", "meis\u00b7ter", "auff", "/", "der", "euch", "ver\u00b7gn\u00fc\u00b7gen", "kan", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "$(", "PRELS", "PPER", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Versichert euch gewi\u00df ihr werdet solche lehren", "tokens": ["Ver\u00b7si\u00b7chert", "euch", "ge\u00b7wi\u00df", "ihr", "wer\u00b7det", "sol\u00b7che", "leh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "PPER", "VAFIN", "PIS", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Und solchen liebes-trost zu euren vortheil h\u00f6ren/", "tokens": ["Und", "sol\u00b7chen", "lie\u00b7bes\u00b7trost", "zu", "eu\u00b7ren", "vor\u00b7theil", "h\u00f6\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Mehr als ihr w\u00fcnschen m\u00f6gt. Doch wer das s\u00fcsse ziel", "tokens": ["Mehr", "als", "ihr", "w\u00fcn\u00b7schen", "m\u00f6gt", ".", "Doch", "wer", "das", "s\u00fcs\u00b7se", "ziel"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PIAT", "KOKOM", "PPER", "VVINF", "VMFIN", "$.", "KON", "PWS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "In der verfluchten lust der schande suchen will/", "tokens": ["In", "der", "ver\u00b7fluch\u00b7ten", "lust", "der", "schan\u00b7de", "su\u00b7chen", "will", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "ADJA", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Der k\u00f6mt bey mir nicht an. Ich richte mein gem\u00fcthe", "tokens": ["Der", "k\u00f6mt", "bey", "mir", "nicht", "an", ".", "Ich", "rich\u00b7te", "mein", "ge\u00b7m\u00fc\u00b7the"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPR", "PPER", "PTKNEG", "PTKVZ", "$.", "PPER", "VVFIN", "PPOSAT", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Auff die erg\u00f6tzligkeit/ darneben man die bl\u00fcte", "tokens": ["Auff", "die", "er\u00b7g\u00f6tz\u00b7lig\u00b7keit", "/", "dar\u00b7ne\u00b7ben", "man", "die", "bl\u00fc\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$(", "PAV", "PIS", "ART", "NN"], "meter": "++-+-++-+--+-", "measure": "trochaic.septa.relaxed"}, "line.37": {"text": "Der jugend nicht verletzt: die vor der erbarn welt/", "tokens": ["Der", "ju\u00b7gend", "nicht", "ver\u00b7letzt", ":", "die", "vor", "der", "er\u00b7barn", "welt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "VVPP", "$.", "ART", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Ja vor dem Cato selbst recht \u00fcberley beh\u00e4lt.", "tokens": ["Ja", "vor", "dem", "Ca\u00b7to", "selbst", "recht", "\u00fc\u00b7ber\u00b7ley", "be\u00b7h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "APPR", "ART", "NE", "ADV", "ADJD", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Zum ersten suche was: zum andern lern es kennen:", "tokens": ["Zum", "ers\u00b7ten", "su\u00b7che", "was", ":", "zum", "an\u00b7dern", "lern", "es", "ken\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "VVFIN", "PIS", "$.", "APPRART", "PIS", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Zum dritten lasse dich nicht gar zu zeitlich trennen/", "tokens": ["Zum", "drit\u00b7ten", "las\u00b7se", "dich", "nicht", "gar", "zu", "zeit\u00b7lich", "tren\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "VVFIN", "PPER", "PTKNEG", "ADV", "PTKA", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "In dieses schlie\u00df ich hier die gantze klugheit ein/", "tokens": ["In", "die\u00b7ses", "schlie\u00df", "ich", "hier", "die", "gant\u00b7ze", "klug\u00b7heit", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Und dieses soll der zweck der guten lehren seyn.", "tokens": ["Und", "die\u00b7ses", "soll", "der", "zweck", "der", "gu\u00b7ten", "leh\u00b7ren", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VMFIN", "ART", "NN", "ART", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Wer sucht/ der findet was. Der safft der reiffen trauben", "tokens": ["Wer", "sucht", "/", "der", "fin\u00b7det", "was", ".", "Der", "safft", "der", "reif\u00b7fen", "trau\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "$(", "ART", "VVFIN", "PIS", "$.", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "W\u00e4chst in der stuben nicht/ und die gebratnen tauben", "tokens": ["W\u00e4chst", "in", "der", "stu\u00b7ben", "nicht", "/", "und", "die", "ge\u00b7brat\u00b7nen", "tau\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "PTKNEG", "$(", "KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Die fliegen nicht ins maul: man mu\u00df sich drum bem\u00fchn/", "tokens": ["Die", "flie\u00b7gen", "nicht", "ins", "maul", ":", "man", "mu\u00df", "sich", "drum", "be\u00b7m\u00fchn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PTKNEG", "APPRART", "NN", "$.", "PIS", "VMFIN", "PRF", "PAV", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Und die gelegenheit fein offt zu rathe ziehn/", "tokens": ["Und", "die", "ge\u00b7le\u00b7gen\u00b7heit", "fein", "offt", "zu", "ra\u00b7the", "ziehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJD", "ADV", "PTKZU", "VVFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Die m\u00e4dgen kommen ja bald hier bald dort zusammen/", "tokens": ["Die", "m\u00e4d\u00b7gen", "kom\u00b7men", "ja", "bald", "hier", "bald", "dort", "zu\u00b7sam\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "ADV", "ADV", "ADV", "ADV", "ADV", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Da mu\u00df derselbe nach/ der seine liebes-flammen", "tokens": ["Da", "mu\u00df", "der\u00b7sel\u00b7be", "nach", "/", "der", "sei\u00b7ne", "lie\u00b7bes\u00b7flam\u00b7men"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PDAT", "APPR", "$(", "ART", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.49": {"text": "Nicht \u00fcbel hegen wil: und welcher di\u00df nicht kan/", "tokens": ["Nicht", "\u00fc\u00b7bel", "he\u00b7gen", "wil", ":", "und", "wel\u00b7cher", "di\u00df", "nicht", "kan", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "VVINF", "VMFIN", "$.", "KON", "PWAT", "PDS", "PTKNEG", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.50": {"text": "Bey diesem giebt sich auch die liebste langsam an.", "tokens": ["Bey", "die\u00b7sem", "giebt", "sich", "auch", "die", "liebs\u00b7te", "lang\u00b7sam", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "VVFIN", "PRF", "ADV", "ART", "ADJA", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.51": {"text": "Der fischer sch\u00fcttelt nicht die krebse von dem baume.", "tokens": ["Der", "fi\u00b7scher", "sch\u00fct\u00b7telt", "nicht", "die", "kreb\u00b7se", "von", "dem", "bau\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "PTKNEG", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.52": {"text": "Der vogelsteller fangt die lerchen in dem schaume", "tokens": ["Der", "vo\u00b7gel\u00b7stel\u00b7ler", "fangt", "die", "ler\u00b7chen", "in", "dem", "schau\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.53": {"text": "Des kalten wassers nicht: und der ist gar kein held/", "tokens": ["Des", "kal\u00b7ten", "was\u00b7sers", "nicht", ":", "und", "der", "ist", "gar", "kein", "held", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKNEG", "$.", "KON", "ART", "VAFIN", "ADV", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.54": {"text": "Der allzeit warten wil bi\u00df was vom himmel f\u00e4llt/", "tokens": ["Der", "all\u00b7zeit", "war\u00b7ten", "wil", "bi\u00df", "was", "vom", "him\u00b7mel", "f\u00e4llt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVINF", "VMFIN", "APPR", "PRELS", "APPRART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.55": {"text": "Es hilfft doch nichts davor: man mu\u00df ein bi\u00dfgen lauffen.", "tokens": ["Es", "hilfft", "doch", "nichts", "da\u00b7vor", ":", "man", "mu\u00df", "ein", "bi\u00df\u00b7gen", "lauf\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PIS", "PAV", "$.", "PIS", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.56": {"text": "Der himmel pflegt die lust um arbeit zu verkauffen.", "tokens": ["Der", "him\u00b7mel", "pflegt", "die", "lust", "um", "ar\u00b7beit", "zu", "ver\u00b7kauf\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.57": {"text": "Das m\u00e4dgen ist schon da man sehe sich nur um/", "tokens": ["Das", "m\u00e4d\u00b7gen", "ist", "schon", "da", "man", "se\u00b7he", "sich", "nur", "um", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "ADV", "KOUS", "PIS", "VVFIN", "PRF", "ADV", "APPR", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.58": {"text": "Offt zeigt ein fremder ort ein s\u00fcsses eigenthum/", "tokens": ["Offt", "zeigt", "ein", "frem\u00b7der", "ort", "ein", "s\u00fcs\u00b7ses", "ei\u00b7gen\u00b7thum", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.59": {"text": "Man darf de\u00dfhalben nicht auf sechs und sieben meilen", "tokens": ["Man", "darf", "de\u00df\u00b7hal\u00b7ben", "nicht", "auf", "sechs", "und", "sie\u00b7ben", "mei\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VMFIN", "PAV", "PTKNEG", "APPR", "CARD", "KON", "CARD", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.60": {"text": "Nach einer frembden zier mit pferd und kutschen eilen:", "tokens": ["Nach", "ei\u00b7ner", "fremb\u00b7den", "zier", "mit", "pferd", "und", "kut\u00b7schen", "ei\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "APPR", "NN", "KON", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.61": {"text": "Man setze nur deu fu\u00df ein wenig vor das hau\u00df/", "tokens": ["Man", "set\u00b7ze", "nur", "deu", "fu\u00df", "ein", "we\u00b7nig", "vor", "das", "hau\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "ADJD", "PTKVZ", "ART", "PIS", "APPR", "ART", "NN", "$("], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.62": {"text": "Und lese sich ein kind auch in der gassen au\u00df.", "tokens": ["Und", "le\u00b7se", "sich", "ein", "kind", "auch", "in", "der", "gas\u00b7sen", "au\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ART", "NN", "ADV", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.63": {"text": "Wiewohl es l\u00e4st sich nicht mit diesen sachen spielen:", "tokens": ["Wie\u00b7wohl", "es", "l\u00e4st", "sich", "nicht", "mit", "die\u00b7sen", "sa\u00b7chen", "spie\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "PRF", "PTKNEG", "APPR", "PDAT", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.64": {"text": "Man kan so eigentlich auf nichts gewisses zielen/", "tokens": ["Man", "kan", "so", "ei\u00b7gent\u00b7lich", "auf", "nichts", "ge\u00b7wis\u00b7ses", "zie\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "ADV", "ADV", "APPR", "PIS", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.65": {"text": "Es k\u00f6mmt zu unverhofft/ da\u00df ein geringer blick/", "tokens": ["Es", "k\u00f6mmt", "zu", "un\u00b7ver\u00b7hofft", "/", "da\u00df", "ein", "ge\u00b7rin\u00b7ger", "blick", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKA", "ADJD", "$(", "KOUS", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.66": {"text": "Ein wort/ ein sch\u00f6ner fleck/ uns einen leisen strick", "tokens": ["Ein", "wort", "/", "ein", "sch\u00f6\u00b7ner", "fleck", "/", "uns", "ei\u00b7nen", "lei\u00b7sen", "strick"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$(", "ART", "ADJA", "NN", "$(", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.67": {"text": "An das gem\u00fcthe legt. \u2012 \u2012 \u2012", "tokens": ["An", "das", "ge\u00b7m\u00fc\u00b7the", "legt", ".", "\u2012", "\u2012", "\u2012"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "VVFIN", "$.", "$(", "$(", "$("], "meter": "+--+-+--+", "m