bic.tri"}}, "stanza.9": {"line.1": {"text": "Indem es draussen st\u00fcrmt vnd schneyt,", "tokens": ["In\u00b7dem", "es", "draus\u00b7sen", "st\u00fcrmt", "vnd", "schneyt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df einem m\u00f6chte grawen,", "tokens": ["Da\u00df", "ei\u00b7nem", "m\u00f6ch\u00b7te", "gra\u00b7wen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Am besten, da\u00df wir mitler Zeit", "tokens": ["Am", "bes\u00b7ten", ",", "da\u00df", "wir", "mit\u00b7ler", "Zeit"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKA", "ADJD", "$,", "KOUS", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nach warmen Stuben schawen,", "tokens": ["Nach", "war\u00b7men", "Stu\u00b7ben", "scha\u00b7wen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Mit Freunden hie zusammen gehn,", "tokens": ["Mit", "Freun\u00b7den", "hie", "zu\u00b7sam\u00b7men", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Vom Wein vnd Tantze gl\u00fchen,", "tokens": ["Vom", "Wein", "vnd", "Tant\u00b7ze", "gl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Da\u00df Instrument last h\u00f6her stehn,", "tokens": ["Da\u00df", "Inst\u00b7ru\u00b7ment", "last", "h\u00f6\u00b7her", "stehn