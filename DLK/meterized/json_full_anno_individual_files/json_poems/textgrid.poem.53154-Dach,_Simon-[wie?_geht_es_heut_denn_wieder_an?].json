{"textgrid.poem.53154": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "[wie? geht es heut denn wieder an?]", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie? geht es heut denn wieder an?", "tokens": ["Wie", "?", "geht", "es", "heut", "denn", "wie\u00b7der", "an", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "VVFIN", "PPER", "ADV", "ADV", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein, lasset mich zufrieden,", "tokens": ["Mein", ",", "las\u00b7set", "mich", "zu\u00b7frie\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "$,", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ihr wisst, ich sey ein schwacher Mann", "tokens": ["Ihr", "wisst", ",", "ich", "sey", "ein", "schwa\u00b7cher", "Mann"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vnd gern von Lust geschieden:", "tokens": ["Vnd", "gern", "von", "Lust", "ge\u00b7schie\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Da\u00df gestrig halt ich mehr als gnug,", "tokens": ["Da\u00df", "ge\u00b7strig", "halt", "ich", "mehr", "als", "gnug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "VVFIN", "PPER", "PIAT", "KOKOM", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ich warte meiner Sachen,", "tokens": ["Ich", "war\u00b7te", "mei\u00b7ner", "Sa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Wir m\u00fcssen keinen Bier-Zech-Krug", "tokens": ["Wir", "m\u00fcs\u00b7sen", "kei\u00b7nen", "Bier\u00b7Zech\u00b7Krug"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Au\u00df diesem Hause machen.", "tokens": ["Au\u00df", "die\u00b7sem", "Hau\u00b7se", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wo sollt ihr mit der Kost hinau\u00df,", "tokens": ["Wo", "sollt", "ihr", "mit", "der", "Kost", "hin\u00b7au\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die gestern \u00fcberblieben?", "tokens": ["Die", "ge\u00b7stern", "\u00fc\u00b7berb\u00b7lie\u00b7ben", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVINF", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Last sie nur in da\u00df Pauperhau\u00df", "tokens": ["Last", "sie", "nur", "in", "da\u00df", "Pau\u00b7per\u00b7hau\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "KOUS", "NE"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Zu schicken euch belieben.", "tokens": ["Zu", "schi\u00b7cken", "euch", "be\u00b7lie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Da l\u00e4ufft man warlich nicht daf\u00fcr,", "tokens": ["Da", "l\u00e4ufft", "man", "war\u00b7lich", "nicht", "da\u00b7f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "PTKNEG", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die\u00df ist zu viel den Leuten,", "tokens": ["Die\u00df", "ist", "zu", "viel", "den", "Leu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PTKA", "PIS", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Gr\u00fctz, Erbsen, Speck ist ihr Geb\u00fchr,", "tokens": ["Gr\u00fctz", ",", "Erb\u00b7sen", ",", "Speck", "ist", "ihr", "Ge\u00b7b\u00fchr", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Nicht Wildpret vnd Pasteyten.", "tokens": ["Nicht", "Wild\u00b7pret", "vnd", "Pas\u00b7tey\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NE", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Habt ihr denn dessen Gram vnd Pein,", "tokens": ["Habt", "ihr", "denn", "des\u00b7sen", "Gram", "vnd", "Pein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Warheit auch zu sagen,", "tokens": ["Die", "War\u00b7heit", "auch", "zu", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich gebe meinen Willen drein", "tokens": ["Ich", "ge\u00b7be", "mei\u00b7nen", "Wil\u00b7len", "drein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vnd leb euch zu behagen,", "tokens": ["Vnd", "leb", "euch", "zu", "be\u00b7ha\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der Mein ist gut, klahr ist da\u00df Bier,", "tokens": ["Der", "Mein", "ist", "gut", ",", "klahr", "ist", "da\u00df", "Bier", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "ADJD", "VAFIN", "KOUS", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ich w\u00fcrd ein Unhold heissen,", "tokens": ["Ich", "w\u00fcrd", "ein", "Un\u00b7hold", "heis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Lie\u00df ich auff ewre Bitte mir", "tokens": ["Lie\u00df", "ich", "auff", "ew\u00b7re", "Bit\u00b7te", "mir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Den Mantel gro\u00df zerreissen.", "tokens": ["Den", "Man\u00b7tel", "gro\u00df", "zer\u00b7reis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Indem es draussen st\u00fcrmt vnd schneyt,", "tokens": ["In\u00b7dem", "es", "draus\u00b7sen", "st\u00fcrmt", "vnd", "schneyt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df einem m\u00f6chte grawen,", "tokens": ["Da\u00df", "ei\u00b7nem", "m\u00f6ch\u00b7te", "gra\u00b7wen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Am besten, da\u00df wir mitler Zeit", "tokens": ["Am", "bes\u00b7ten", ",", "da\u00df", "wir", "mit\u00b7ler", "Zeit"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKA", "ADJD", "$,", "KOUS", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nach warmen Stuben schawen,", "tokens": ["Nach", "war\u00b7men", "Stu\u00b7ben", "scha\u00b7wen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Mit Freunden hie zusammen gehn,", "tokens": ["Mit", "Freun\u00b7den", "hie", "zu\u00b7sam\u00b7men", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Vom Wein vnd Tantze gl\u00fchen,", "tokens": ["Vom", "Wein", "vnd", "Tant\u00b7ze", "gl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Da\u00df Instrument last h\u00f6her stehn,", "tokens": ["Da\u00df", "Inst\u00b7ru\u00b7ment", "last", "h\u00f6\u00b7her", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die Fidel da\u00df beziehen.", "tokens": ["Die", "Fi\u00b7del", "da\u00df", "be\u00b7zie\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KOUS", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ein solch giest-Kindelbier ist gut,", "tokens": ["Ein", "solch", "giest\u00b7Kin\u00b7del\u00b7bier", "ist", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wer deutet es zum b\u00f6sen?", "tokens": ["Wer", "deu\u00b7tet", "es", "zum", "b\u00f6\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPRART", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wir haben hierbey vnsern Hut", "tokens": ["Wir", "ha\u00b7ben", "hier\u00b7bey", "vn\u00b7sern", "Hut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie sonst nicht au\u00dfzul\u00f6sen;", "tokens": ["Wie", "sonst", "nicht", "au\u00df\u00b7zu\u00b7l\u00f6\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Herr Bierwolff, gebt vn\u00df offters raum", "tokens": ["Herr", "Bier\u00b7wolff", ",", "gebt", "vn\u00df", "off\u00b7ters", "raum"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "NN", "$,", "VVFIN", "PPER", "ADV", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Bey Euch vn\u00df zu ergetzen,", "tokens": ["Bey", "Euch", "vn\u00df", "zu", "er\u00b7get\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Wir wollen einen sch\u00f6nen Baum", "tokens": ["Wir", "wol\u00b7len", "ei\u00b7nen", "sch\u00f6\u00b7nen", "Baum"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Euch auff Johannes setzen.", "tokens": ["Euch", "auff", "Jo\u00b7han\u00b7nes", "set\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NE", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Wie? geht es heut denn wieder an?", "tokens": ["Wie", "?", "geht", "es", "heut", "denn", "wie\u00b7der", "an", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "VVFIN", "PPER", "ADV", "ADV", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein, lasset mich zufrieden,", "tokens": ["Mein", ",", "las\u00b7set", "mich", "zu\u00b7frie\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "$,", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ihr wisst, ich sey ein schwacher Mann", "tokens": ["Ihr", "wisst", ",", "ich", "sey", "ein", "schwa\u00b7cher", "Mann"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vnd gern von Lust geschieden:", "tokens": ["Vnd", "gern", "von", "Lust", "ge\u00b7schie\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Da\u00df gestrig halt ich mehr als gnug,", "tokens": ["Da\u00df", "ge\u00b7strig", "halt", "ich", "mehr", "als", "gnug", ","]