{"textgrid.poem.43361": {"metadata": {"author": {"name": "Leopardi, Giacomo", "birth": "N.A.", "death": "N.A."}, "title": "XxVIII. An mich selbst.", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun wirst du ruhn f\u00fcr immer,", "tokens": ["Nun", "wirst", "du", "ruhn", "f\u00fcr", "im\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVINF", "APPR", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mein m\u00fcdes Herz. Es schwand der letzte Wahn,", "tokens": ["Mein", "m\u00fc\u00b7des", "Herz", ".", "Es", "schwand", "der", "letz\u00b7te", "Wahn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$.", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der ewig schien. Er schwand. Ich f\u00fchl' es tief:", "tokens": ["Der", "e\u00b7wig", "schien", ".", "Er", "schwand", ".", "Ich", "f\u00fchl'", "es", "tief", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "$.", "PPER", "VVFIN", "$.", "PPER", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Auf holde T\u00e4uschung, auch der ", "tokens": ["Auf", "hol\u00b7de", "T\u00e4u\u00b7schung", ",", "auch", "der"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "ADV", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "So ruh f\u00fcr immer. Lange", "tokens": ["So", "ruh", "f\u00fcr", "im\u00b7mer", ".", "Lan\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "NN", "APPR", "ADV", "$.", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Genug hast du geklopft. Nichts hier verdient", "tokens": ["Ge\u00b7nug", "hast", "du", "ge\u00b7klopft", ".", "Nichts", "hier", "ver\u00b7dient"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "VVPP", "$.", "PIS", "ADV", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Dein reges Schlagen, keines Seufzers ist", "tokens": ["Dein", "re\u00b7ges", "Schla\u00b7gen", ",", "kei\u00b7nes", "Seuf\u00b7zers", "ist"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PIAT", "NN", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Die Erde werth. Nur Schmerz und Langweil bietet", "tokens": ["Die", "Er\u00b7de", "werth", ".", "Nur", "Schmerz", "und", "Lang\u00b7weil", "bie\u00b7tet"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "$.", "ADV", "NN", "KON", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Das Leben, Andres nicht. Die Welt ist Koth.", "tokens": ["Das", "Le\u00b7ben", ",", "And\u00b7res", "nicht", ".", "Die", "Welt", "ist", "Koth", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PIS", "PTKNEG", "$.", "ART", "NN", "VAFIN", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Ergieb dich denn! Verzweifle", "tokens": ["Er\u00b7gieb", "dich", "denn", "!", "Ver\u00b7zweif\u00b7le"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["VVIMP", "PPER", "ADV", "$.", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Zum letzten Mal! Uns Menschen hat das Schicksal", "tokens": ["Zum", "letz\u00b7ten", "Mal", "!", "Uns", "Men\u00b7schen", "hat", "d