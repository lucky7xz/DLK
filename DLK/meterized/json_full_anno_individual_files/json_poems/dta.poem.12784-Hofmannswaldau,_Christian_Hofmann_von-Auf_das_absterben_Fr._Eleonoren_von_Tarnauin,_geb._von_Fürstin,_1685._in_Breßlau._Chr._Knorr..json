tztes kind ein sohn der schmertzen seyn,", "tokens": ["Drum", "must'", "ihr", "letz\u00b7tes", "kind", "ein", "sohn", "der", "schmert\u00b7zen", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPOSAT", "ADJA", "NN", "ART", "NN", "ART", "ADJA", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.87": {"text": "Du aber, um ihr noch den letzten dienst zu geben,", "tokens": ["Du", "a\u00b7ber", ",", "um", "ihr", "noch", "den", "letz\u00b7ten", "dienst", "zu", "ge\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "KOUI", "PPER", "ADV", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.88": {"text": "Schreib auch mit Jacob dort auf ihren grabe-stein:", "tokens": ["Schreib", "auch", "mit", "Ja\u00b7cob", "dort", "auf", "ih\u00b7ren", "gra\u00b7be\u00b7stein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "NE", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+---+-+-+", "measure": "unknown.measure.penta"}, "line.89": {"text": "Hier ruht mein augen-trost, ein weib von seltnen gaben,", "tokens": ["Hier", "ruht", "mein", "au\u00b7gen\u00b7trost", ",", "ein", "weib", "von", "selt\u00b7nen", "ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$,", "ART", "NN", "APPR", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.90": {"text": "Mein leit-stern, meine lust, so mich allein vergn\u00fcgt;", "tokens": ["Mein", "leit\u00b7stern", ",", "mei\u00b7ne", "lust", ",", "so", "mich", "al\u00b7lein", "ver\u00b7gn\u00fcgt", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "$,", "PPOSAT", "NN", "$,", "ADV", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.91": {"text": "Zwey leichen sind allhier in einer grufft begraben,", "tokens": ["Zwey", "lei\u00b7chen", "sind", "all\u00b7hier", "in", "ei\u00b7ner", "grufft", "be\u00b7gra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "VVINF", "VAFIN", "ADV", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.92": {"text": "Weil neben ihr zugleich mein treues hertze liegt.", "tokens": ["Weil", "ne\u00b7ben", "ihr", "zu\u00b7gleich", "mein", "treu\u00b7es", "hert\u00b7ze", "liegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPER", "ADV", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}