word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ADV", "ART", "NN", "PTKNEG", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.73": {"text": "Hochwerther! dessen aug\u2019 in hei\u00dfen thr\u00e4nen schwimmet,", "tokens": ["Hoch\u00b7wert\u00b7her", "!", "des\u00b7sen", "aug'", "in", "hei\u00b7\u00dfen", "thr\u00e4\u00b7nen", "schwim\u00b7met", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PRELAT", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.74": {"text": "Und den die rauhe qvaal fast selbst zur leiche macht,", "tokens": ["Und", "den", "die", "rau\u00b7he", "qvaal", "fast", "selbst", "zur", "lei\u00b7che", "macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ART", "ADJA", "NN", "ADV", "ADV", "APPRART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.75": {"text": "Ich wei\u00df, da\u00df itzt dein hertz in tausend funcken glimmet,", "tokens": ["Ich", "wei\u00df", ",", "da\u00df", "itzt", "dein", "hertz", "in", "tau\u00b7send", "fun\u00b7cken", "glim\u00b7met", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ADV", "PPOSAT", "NN", "APPR", "CARD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.76": {"text": "Und alles dis bejaht, was ich hier vorgebracht.", "tokens": ["Und", "al\u00b7les", "dis", "be\u00b7jaht", ",", "was", "ich", "hier", "vor\u00b7ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "PDS", "VVFIN", "$,", "PWS", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.77": {"text": "Dir ist am besten kund ihr ungemeines wesen,", "tokens": ["Dir", "ist", "am", "bes\u00b7ten", "kund", "ihr", "un\u00b7ge\u00b7mei\u00b7nes", "we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "ADJA", "NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.78": {"text": "Wie sie an tugenden recht eine F\u00fcrstin war.", "tokens": ["Wie", "sie", "an", "tu\u00b7gen\u00b7den", "recht", "ei\u00b7ne", "F\u00fcrs\u00b7tin", "war", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "NN", "ADV", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.79": {"text": "Drum kan man auch an dir nichts als betr\u00fcbni\u00df lesen,", "tokens": ["Drum", "kan", "man", "auch", "an", "dir", "nichts", "als", "be\u00b7tr\u00fcb\u00b7ni\u00df", "le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PIS", "ADV", "APPR", "PPER", "PIS", "KOKOM", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.80": {"text": "Denn das, was du geliebt, liegt auf der todten-bahr.", "tokens": ["Denn", "das", ",", "was", "du", "ge\u00b7liebt", ",", "liegt", "auf", "der", "tod\u00b7ten\u00b7bahr", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "$,", "PWS", "PPER", "VVPP", "$,", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.81": {"text": "Jedoch was wilst du viel den schlu\u00df des himmels \u00e4ndern?", "tokens": ["Je\u00b7doch", "was", "wilst", "du", "viel", "den", "schlu\u00df", "des", "him\u00b7mels", "\u00