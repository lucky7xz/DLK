{"dta.poem.12784": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf das absterben  \n Fr. Eleonoren von Tarnauin,  \n geb. von F\u00fcrstin, 1685. in Bre\u00dflau.  \n Chr. Knorr.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Verzeihe, Seelige! da\u00df ich bey deinem grabe,", "tokens": ["Ver\u00b7zei\u00b7he", ",", "See\u00b7li\u00b7ge", "!", "da\u00df", "ich", "bey", "dei\u00b7nem", "gra\u00b7be", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$.", "KOUS", "PPER", "APPR", "PPOSAT", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dein bildni\u00df noch einmal darff aus dem sarge ziehn,", "tokens": ["Dein", "bild\u00b7ni\u00df", "noch", "ein\u00b7mal", "darff", "aus", "dem", "sar\u00b7ge", "ziehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "ADV", "VMFIN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Der vorsatz, den hierbey ich in dem hertzen habe,", "tokens": ["Der", "vor\u00b7satz", ",", "den", "hier\u00b7bey", "ich", "in", "dem", "hert\u00b7zen", "ha\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "PPER", "APPR", "ART", "NN", "VAFIN", "$,"], "meter": "-++--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Ist, da\u00df dein hoher ruhm soll nach dem tode bl\u00fchn.", "tokens": ["Ist", ",", "da\u00df", "dein", "ho\u00b7her", "ruhm", "soll", "nach", "dem", "to\u00b7de", "bl\u00fchn", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "KOUS", "PPOSAT", "ADJA", "NN", "VMFIN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die asche soll von dir in blumen sich verkehren,", "tokens": ["Die", "asc\u00b7he", "soll", "von", "dir", "in", "blu\u00b7men", "sich", "ver\u00b7keh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VMFIN", "APPR", "PPER", "APPR", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Von denen meine faust dir einen crantz verspricht,", "tokens": ["Von", "de\u00b7nen", "mei\u00b7ne", "faust", "dir", "ei\u00b7nen", "crantz", "ver\u00b7spricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPOSAT", "NN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Den keiner raute gifft, kein schimmel kan verzehren:", "tokens": ["Den", "kei\u00b7ner", "rau\u00b7te", "gifft", ",", "kein", "schim\u00b7mel", "kan", "ver\u00b7zeh\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ADJA", "NN", "$,", "PIAT", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Denn wahre tugend stirbt auch nach dem tode nicht.", "tokens": ["Denn", "wah\u00b7re", "tu\u00b7gend", "stirbt", "auch", "nach", "dem", "to\u00b7de", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "ADV", "APPR", "ART", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ich wei\u00df zwar, da\u00df ein thor hat in die welt geschrieben:", "tokens": ["Ich", "wei\u00df", "zwar", ",", "da\u00df", "ein", "thor", "hat", "in", "die", "welt", "ge\u00b7schrie\u00b7ben", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "KOUS", "ART", "NN", "VAFIN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Die tugend w\u00e4re nur der m\u00e4nner eigenthum;", "tokens": ["Die", "tu\u00b7gend", "w\u00e4\u00b7re", "nur", "der", "m\u00e4n\u00b7ner", "ei\u00b7gen\u00b7thum", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Es k\u00f6nte dieses bild kein frauen-hertze lieben,", "tokens": ["Es", "k\u00f6n\u00b7te", "die\u00b7ses", "bild", "kein", "frau\u00b7en\u00b7hert\u00b7ze", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PDAT", "NN", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Denn gro\u00dfmuth zeuge blo\u00df den wahren ehren-ruhm;", "tokens": ["Denn", "gro\u00df\u00b7muth", "zeu\u00b7ge", "blo\u00df", "den", "wah\u00b7ren", "eh\u00b7ren\u00b7ruhm", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Allein welch kluger geist wird solchen wahnwitz hegen,", "tokens": ["Al\u00b7lein", "welch", "klu\u00b7ger", "geist", "wird", "sol\u00b7chen", "wahn\u00b7witz", "he\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWAT", "ADJA", "NN", "VAFIN", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Als ziere dieses gold nicht auch der frauen haupt?", "tokens": ["Als", "zie\u00b7re", "die\u00b7ses", "gold", "nicht", "auch", "der", "frau\u00b7en", "haupt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PDAT", "NN", "PTKNEG", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Gewi\u00df, man solte den bey schlang und ottern legen,", "tokens": ["Ge\u00b7wi\u00df", ",", "man", "sol\u00b7te", "den", "bey", "schlang", "und", "ot\u00b7tern", "le\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PIS", "VMFIN", "ART", "APPR", "ADJD", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Der dis geschlechte schimpfft, und dessen ehre raubt.", "tokens": ["Der", "dis", "ge\u00b7schlech\u00b7te", "schimpfft", ",", "und", "des\u00b7sen", "eh\u00b7re", "raubt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PDS", "VVFIN", "VVFIN", "$,", "KON", "PDS", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Ist gleich das erste weib aus Adams ribbe kommen,", "tokens": ["Ist", "gleich", "das", "ers\u00b7te", "weib", "aus", "A\u00b7dams", "rib\u00b7be", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "APPR", "NE", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "So war sie doch, wie er, des H\u00f6chsten ebenbild,", "tokens": ["So", "war", "sie", "doch", ",", "wie", "er", ",", "des", "H\u00f6chs\u00b7ten", "e\u00b7ben\u00b7bild", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "$,", "PWAV", "PPER", "$,", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Der fleisch zu ihrem bau, zu jenem leim genommen,", "tokens": ["Der", "fleisch", "zu", "ih\u00b7rem", "bau", ",", "zu", "je\u00b7nem", "leim", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$,", "APPR", "PDAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Und beyd\u2019 auf einen schlag mit seiner krafft erf\u00fcllt.", "tokens": ["Und", "beyd'", "auf", "ei\u00b7nen", "schlag", "mit", "sei\u00b7ner", "krafft", "er\u00b7f\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Die schrifften lassen uns viel solcher weiber schauen,", "tokens": ["Die", "schriff\u00b7ten", "las\u00b7sen", "uns", "viel", "sol\u00b7cher", "wei\u00b7ber", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "PPER", "ADV", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Die es an tugenden den m\u00e4nnern vorgethan;", "tokens": ["Die", "es", "an", "tu\u00b7gen\u00b7den", "den", "m\u00e4n\u00b7nern", "vor\u00b7ge\u00b7than", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "ART", "ADJA", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Es darff sie kein Bernin in festen marmel hauen,", "tokens": ["Es", "darff", "sie", "kein", "Ber\u00b7nin", "in", "fes\u00b7ten", "mar\u00b7mel", "hau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PIAT", "NN", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.24": {"text": "Wer ihre tempel sucht, der sehe b\u00fccher an.", "tokens": ["Wer", "ih\u00b7re", "tem\u00b7pel", "sucht", ",", "der", "se\u00b7he", "b\u00fc\u00b7cher", "an", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "VVFIN", "$,", "PRELS", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.25": {"text": "Des H\u00f6chsten finger selbst hat ihren ruhm beschrieben.", "tokens": ["Des", "H\u00f6chs\u00b7ten", "fin\u00b7ger", "selbst", "hat", "ih\u00b7ren", "ruhm", "be\u00b7schrie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VAFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Jhr lob steht oben an in Sirachs cancelley.", "tokens": ["Ihr", "lob", "steht", "o\u00b7ben", "an", "in", "Si\u00b7rachs", "can\u00b7cel\u00b7ley", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "APPR", "APPR", "NE", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Hier hat er seinen kiel den sternen gleich, getrieben,", "tokens": ["Hier", "hat", "er", "sei\u00b7nen", "kiel", "den", "ster\u00b7nen", "gleich", ",", "ge\u00b7trie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "NN", "ART", "ADJA", "ADV", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Zu zeigen, da\u00df ihr glantz nicht zu erreichen sey.", "tokens": ["Zu", "zei\u00b7gen", ",", "da\u00df", "ihr", "glantz", "nicht", "zu", "er\u00b7rei\u00b7chen", "sey", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "KOUS", "PPOSAT", "NN", "PTKNEG", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Gewi\u00df, ein weibes-bild von englischen geberden,", "tokens": ["Ge\u00b7wi\u00df", ",", "ein", "wei\u00b7bes\u00b7bild", "von", "eng\u00b7li\u00b7schen", "ge\u00b7ber\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Die wahre gottesfurcht in reiner seelen hegt.", "tokens": ["Die", "wah\u00b7re", "got\u00b7tes\u00b7furcht", "in", "rei\u00b7ner", "see\u00b7len", "hegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Bey der vernunfft und zucht vertraute schwestern werden,", "tokens": ["Bey", "der", "ver\u00b7nunfft", "und", "zucht", "ver\u00b7trau\u00b7te", "schwes\u00b7tern", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "NN", "VVFIN", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Die aller worte gold auf kluge schaalen legt,", "tokens": ["Die", "al\u00b7ler", "wor\u00b7te", "gold", "auf", "klu\u00b7ge", "schaa\u00b7len", "legt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "NN", "APPR", "ADJA", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Die ihren eh-gemahl von gantzem hertzen liebet,", "tokens": ["Die", "ih\u00b7ren", "eh\u00b7ge\u00b7mahl", "von", "gant\u00b7zem", "hert\u00b7zen", "lie\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Den keuschen atlas nicht durch geilen ku\u00df befleckt,", "tokens": ["Den", "keu\u00b7schen", "at\u00b7las", "nicht", "durch", "gei\u00b7len", "ku\u00df", "be\u00b7fleckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKNEG", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Die sich in stetem lauff rechtschaffner tugend \u00fcbet,", "tokens": ["Die", "sich", "in", "ste\u00b7tem", "lauff", "recht\u00b7schaff\u00b7ner", "tu\u00b7gend", "\u00fc\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "ADJA", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Und alle kost verschm\u00e4ht, so nach der wohllust schmeckt.", "tokens": ["Und", "al\u00b7le", "kost", "ver\u00b7schm\u00e4ht", ",", "so", "nach", "der", "wohl\u00b7lust", "schmeckt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVPP", "$,", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Die durch der weisheit mast das wirthschaffts-schiff regieret,", "tokens": ["Die", "durch", "der", "weis\u00b7heit", "mast", "das", "wirth\u00b7schaffts\u00b7schiff", "re\u00b7gie\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Und manchen sorgen-sturm durch kluge sanfftmuth stillt.", "tokens": ["Und", "man\u00b7chen", "sor\u00b7gen\u00b7sturm", "durch", "klu\u00b7ge", "sanfft\u00b7muth", "stillt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Die kinder und gesind auf tugend-pfaden f\u00fchret,", "tokens": ["Die", "kin\u00b7der", "und", "ge\u00b7sind", "auf", "tu\u00b7gen\u00b7dpfa\u00b7den", "f\u00fch\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ADJD", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Und durch verborgnen witz das hau\u00df mit segen f\u00fcllt.", "tokens": ["Und", "durch", "ver\u00b7borg\u00b7nen", "witz", "das", "hau\u00df", "mit", "se\u00b7gen", "f\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Ein so beschaffnes weib ist ihres mannes sonne:", "tokens": ["Ein", "so",