 "pos": ["PWS", "VVFIN", "PTKNEG", "ADV", "VVFIN", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.64": {"text": "Ob iede gleich davon nicht allzeit fr\u00fcchte tr\u00e4gt?", "tokens": ["Ob", "ie\u00b7de", "gleich", "da\u00b7von", "nicht", "all\u00b7zeit", "fr\u00fcch\u00b7te", "tr\u00e4gt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "PAV", "PTKNEG", "ADV", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.65": {"text": "Ist wo ein weib nicht gut, so will ich tausend zehlen,", "tokens": ["Ist", "wo", "ein", "weib", "nicht", "gut", ",", "so", "will", "ich", "tau\u00b7send", "zeh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PWAV", "ART", "NN", "PTKNEG", "ADJD", "$,", "ADV", "VMFIN", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.66": {"text": "Die an vortreffligkeit den engeln gleiche gehn,", "tokens": ["Die", "an", "vor\u00b7tref\u00b7flig\u00b7keit", "den", "en\u00b7geln", "glei\u00b7che", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "ART", "ADJA", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.67": {"text": "Gewi\u00df, es w\u00fcrde mir gar bald an worten fehlen,", "tokens": ["Ge\u00b7wi\u00df", ",", "es", "w\u00fcr\u00b7de", "mir", "gar", "bald", "an", "wor\u00b7ten", "feh\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VAFIN", "PPER", "ADV", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.68": {"text": "Wenn ich ihr hohes lob zur gn\u00fcge