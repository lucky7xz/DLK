{"dta.poem.18522": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf Sr. K\u00f6nigl. Maj. in Polen  \n Augusti  II  ersten einzug in  \n Dantzig.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1703", "urn": "urn:nbn:de:kobv:b4-200905199360", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Gro\u00dfm\u00e4chtigster August/ und S\u00e4chsischer Piast/", "tokens": ["Gro\u00df\u00b7m\u00e4ch\u00b7tigs\u00b7ter", "Au\u00b7gust", "/", "und", "S\u00e4ch\u00b7si\u00b7scher", "Pi\u00b7ast", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$(", "KON", "NN", "NE", "$("], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Der du zur seiten ruhm/ das gl\u00fccke zu den f\u00fcssen/", "tokens": ["Der", "du", "zur", "sei\u00b7ten", "ruhm", "/", "das", "gl\u00fc\u00b7cke", "zu", "den", "f\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "ADJA", "NN", "$(", "PDS", "VVFIN", "APPR", "ART", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Den seegen ob dem haupt/ die macht in h\u00e4nden hast/", "tokens": ["Den", "see\u00b7gen", "ob", "dem", "haupt", "/", "die", "macht", "in", "h\u00e4n\u00b7den", "hast", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "KOUS", "ART", "NN", "$(", "ART", "VVFIN", "APPR", "ADJA", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und Elb- und Weichselstrom als schwestern heissest fliessen/", "tokens": ["Und", "Elb", "und", "Weich\u00b7sel\u00b7strom", "als", "schwes\u00b7tern", "heis\u00b7sest", "flies\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "TRUNC", "KON", "NN", "KOKOM", "VVFIN", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Willkommen tausendmal! dein einzug sey begl\u00fcckt/", "tokens": ["Will\u00b7kom\u00b7men", "tau\u00b7send\u00b7mal", "!", "dein", "ein\u00b7zug", "sey", "be\u00b7gl\u00fcckt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$.", "PPOSAT", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+---+-+-+", "measure": "unknown.measure.penta"}, "line.6": {"text": "Der himmel sey mit dir auf deinen K\u00f6nigs-wegen!", "tokens": ["Der", "him\u00b7mel", "sey", "mit", "dir", "auf", "dei\u00b7nen", "K\u00f6\u00b7nigs\u00b7we\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Schau/ wie dir unsre stadt die treu entgegen schickt/", "tokens": ["Schau", "/", "wie", "dir", "uns\u00b7re", "stadt", "die", "treu", "ent\u00b7ge\u00b7gen", "schickt", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "PWAV", "PPER", "PPOSAT", "NN", "ART", "ADJD", "PTKVZ", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wie sich auf deiner bahn die hertzen unterlegen:", "tokens": ["Wie", "sich", "auf", "dei\u00b7ner", "bahn", "die", "hert\u00b7zen", "un\u00b7ter\u00b7le\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "APPR", "PPOSAT", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Man l\u00e4st umb dich zu sehn gantz sicher hoff und hau\u00df/", "tokens": ["Man", "l\u00e4st", "umb", "dich", "zu", "sehn", "gantz", "si\u00b7cher", "hoff", "und", "hau\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "PPER", "PTKZU", "VVINF", "ADV", "ADJD", "VVFIN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Weil du/ als Schutz-Gott/ hier den eintritt hast genommen/", "tokens": ["Weil", "du", "/", "als", "Schutz\u00b7Gott", "/", "hier", "den", "ein\u00b7tritt", "hast", "ge\u00b7nom\u00b7men", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$(", "KOUS", "NN", "$(", "ADV", "ART", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Das donnernde gesch\u00fctz rufft in den wolcken au\u00df/", "tokens": ["Das", "don\u00b7nern\u00b7de", "ge\u00b7sch\u00fctz", "rufft", "in", "den", "wol\u00b7cken", "au\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Es sey ein Krieges-Gott in unsre mauren kommen:", "tokens": ["Es", "sey", "ein", "Krie\u00b7ge\u00b7sGott", "in", "uns\u00b7re", "mau\u00b7ren", "kom\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Was wunder? m\u00f6chte doch das vierdte theil der welt", "tokens": ["Was", "wun\u00b7der", "?", "m\u00f6ch\u00b7te", "doch", "das", "vierd\u00b7te", "theil", "der", "welt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "NN", "$.", "VMFIN", "ADV", "ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Dich/ grosse Majest\u00e4t/ mit freuden-festen ehren;", "tokens": ["Dich", "/", "gros\u00b7se", "Ma\u00b7jes\u00b7t\u00e4t", "/", "mit", "freu\u00b7den\u00b7fes\u00b7ten", "eh\u00b7ren", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$(", "ADJA", "NN", "$(", "APPR", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Den jubelschall/ der itzt durch unsre gassen f\u00e4llt/", "tokens": ["Den", "ju\u00b7bel\u00b7schall", "/", "der", "itzt", "durch", "uns\u00b7re", "gas\u00b7sen", "f\u00e4llt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Den solte man mit recht in gantz Europa h\u00f6ren.", "tokens": ["Den", "sol\u00b7te", "man", "mit", "recht", "in", "gantz", "Eu\u00b7ro\u00b7pa", "h\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PIS", "APPR", "ADJD", "APPR", "ADV", "NE", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Dein strahl pflegt ja nicht blo\u00df die Sachsen anzugehn/", "tokens": ["Dein", "strahl", "pflegt", "ja", "nicht", "blo\u00df", "die", "Sach\u00b7sen", "an\u00b7zu\u00b7gehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "PTKNEG", "ADV", "ART", "NN", "VVIZU", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Nicht nur Sarmatien wird sich durch dich verneuen/", "tokens": ["Nicht", "nur", "Sar\u00b7ma\u00b7ti\u00b7en", "wird", "sich", "durch", "dich", "ver\u00b7neu\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "NE", "VAFIN", "PRF", "APPR", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Gott heist nach sonnen-art dich in der h\u00f6he stehn/", "tokens": ["Gott", "heist", "nach", "son\u00b7nen\u00b7art", "dich", "in", "der", "h\u00f6\u00b7he", "stehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPR", "NE", "PRF", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Damit dein glantz zugleich viel Reiche m\u00f6g erfreuen.", "tokens": ["Da\u00b7mit", "dein", "glantz", "zu\u00b7gleich", "viel", "Rei\u00b7che", "m\u00f6g", "er\u00b7freu\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPOSAT", "NN", "ADV", "ADV", "NE", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Du bist der Christenheit ein rechter Salomon;", "tokens": ["Du", "bist", "der", "Chris\u00b7ten\u00b7heit", "ein", "rech\u00b7ter", "Sa\u00b7lo\u00b7mon", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Denn deines Scepters gold bringt goldne zeiten wieder/", "tokens": ["Denn", "dei\u00b7nes", "Scep\u00b7ters", "gold", "bringt", "gold\u00b7ne", "zei\u00b7ten", "wie\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "VVFIN", "ADJA", "NN", "ADV", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Es setzt mit dir zugleich auf deinen G\u00f6tter-thron", "tokens": ["Es", "setzt", "mit", "dir", "zu\u00b7gleich", "auf", "dei\u00b7nen", "G\u00f6t\u00b7ter\u00b7thron"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Sich Deutschlands sicherheit/ Europens ruhe nieder;", "tokens": ["Sich", "Deutschlands", "si\u00b7cher\u00b7heit", "/", "Eu\u00b7ro\u00b7pens", "ru\u00b7he", "nie\u00b7der", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PRF", "NE", "NN", "$(", "NE", "NN", "PTKVZ", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.25": {"text": "Ach weltber\u00fchmter Held! dich hat die ewigkeit", "tokens": ["Ach", "welt\u00b7be\u00b7r\u00fchm\u00b7ter", "Held", "!", "dich", "hat", "die", "e\u00b7wig\u00b7keit"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "PWAT", "NN", "$.", "PPER", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Noch ehe du regiert/ noch ehe du gebohren/", "tokens": ["Noch", "e\u00b7he", "du", "re\u00b7giert", "/", "noch", "e\u00b7he", "du", "ge\u00b7boh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "VVFIN", "$(", "ADV", "KOUS", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Zum wunder dieser welt/ zum seegen unsrer zeit/", "tokens": ["Zum", "wun\u00b7der", "die\u00b7ser", "welt", "/", "zum", "see\u00b7gen", "uns\u00b7rer", "zeit", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PDAT", "NN", "$(", "APPRART", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Mit kurtzem/ wie du siehst/ zum K\u00f6nig au\u00dferkohren.", "tokens": ["Mit", "kurt\u00b7zem", "/", "wie", "du", "siehst", "/", "zum", "K\u00f6\u00b7nig", "au\u00b7\u00dfer\u00b7koh\u00b7ren", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$(", "PWAV", "PPER", "VVFIN", "$(", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Wer dir sich wiedersetzt/ der trotzt des h\u00f6chsten macht;", "tokens": ["Wer", "dir", "sich", "wie\u00b7der\u00b7setzt", "/", "der", "trotzt", "des", "h\u00f6chs\u00b7ten", "macht", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PRF", "VVPP", "$(", "ART", "VVFIN", "ART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Der purpur/ den du tr\u00e4gst/ ist himmlisches gewebe/", "tokens": ["Der", "pur\u00b7pur", "/", "den", "du", "tr\u00e4gst", "/", "ist", "himm\u00b7li\u00b7sches", "ge\u00b7we\u00b7be", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "$(", "ART", "PPER", "VVFIN", "$(", "VAFIN", "ADJA", "NN", "$("], "meter": "-+---+-+-+-+-", "measure": "dactylic.init"}, "line.31": {"text": "Du hast den Chur-hut zwar dir niemals zugedacht/", "tokens": ["Du", "hast", "den", "Chur\u00b7hut", "zwar", "dir", "nie\u00b7mals", "zu\u00b7ge\u00b7dacht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "PPER", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Doch sieht man/ wie GOtt gar zu cronen dich erhebe.", "tokens": ["Doch", "sieht", "man", "/", "wie", "Gott", "gar", "zu", "cro\u00b7nen", "dich", "er\u00b7he\u00b7be", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "$(", "KOKOM", "NN", "ADV", "APPR", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Dein Chur-hau\u00df reichte dir zwey schwerdter in die hand;", "tokens": ["Dein", "Chur\u00b7hau\u00df", "reich\u00b7te", "dir", "zwey", "schwerd\u00b7ter", "in", "die", "hand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "CARD", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Nun hastu beyde zwar mit l\u00f6wen-muth gef\u00fchret/", "tokens": ["Nun", "has\u00b7tu", "bey\u00b7de", "zwar", "mit", "l\u00f6\u00b7wen\u00b7muth", "ge\u00b7f\u00fch\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "ADV", "APPR", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Des einen sch\u00e4rffe f\u00fchlt das T\u00fcrcksche morgenl and/", "tokens": ["Des", "ei\u00b7nen", "sch\u00e4rf\u00b7fe", "f\u00fchlt", "das", "T\u00fcrck\u00b7sche", "mor\u00b7genl", "and", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "VVFIN", "ART", "NN", "VMFIN", "PIS", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Des andern st\u00e4rcke hat ein abendland ger\u00fchret.", "tokens": ["Des", "an\u00b7dern", "st\u00e4r\u00b7cke", "hat", "ein", "a\u00b7bend\u00b7land", "ge\u00b7r\u00fch\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Jedoch der ausgang stellt uns noch was anders dar/", "tokens": ["Je\u00b7doch", "der", "aus\u00b7gang", "stellt", "uns", "noch", "was", "an\u00b7ders", "dar", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "PPER", "ADV", "PWS", "ADV", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Du solst damit theils uns/ theils deinen Sachsen n\u00fctzen/", "tokens": ["Du", "solst", "da\u00b7mit", "theils", "uns", "/", "theils", "dei\u00b7nen", "Sach\u00b7sen", "n\u00fct\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PAV", "ADV", "PPER", "$(", "ADV", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Drum lu\u00df mir zu/ da\u00df ich auf deiner schwerdter paar", "tokens": ["Drum", "lu\u00df", "mir", "zu", "/", "da\u00df", "ich", "auf", "dei\u00b7ner", "schwerd\u00b7ter", "paar"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "NN", "PPER", "PTKZU", "$(", "KOUS", "PPER", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Die bey-schrifft setzen darff: Zwey reiche zu besch\u00fctzen!", "tokens": ["Die", "bey\u00b7schrifft", "set\u00b7zen", "darff", ":", "Zwey", "rei\u00b7che", "zu", "be\u00b7sch\u00fct\u00b7zen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VMFIN", "$.", "CARD", "ADJA", "PTKZU",