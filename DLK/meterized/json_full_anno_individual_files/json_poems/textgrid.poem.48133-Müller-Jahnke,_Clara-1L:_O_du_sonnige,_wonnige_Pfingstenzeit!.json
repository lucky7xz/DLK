{"textgrid.poem.48133": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "1L: O du sonnige, wonnige Pfingstenzeit!", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O du sonnige, wonnige Pfingstenzeit!", "tokens": ["O", "du", "son\u00b7ni\u00b7ge", ",", "won\u00b7ni\u00b7ge", "Pfings\u00b7ten\u00b7zeit", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Der Himmel ist blau und das Herz so weit,", "tokens": ["Der", "Him\u00b7mel", "ist", "blau", "und", "das", "Herz", "so", "weit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "in der Brust ein freudiges Gl\u00fchen \u2013", "tokens": ["in", "der", "Brust", "ein", "freu\u00b7di\u00b7ges", "Gl\u00fc\u00b7hen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN", "$("], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "und die Knospe springt und die H\u00fclle f\u00e4llt;", "tokens": ["und", "die", "Knos\u00b7pe", "springt", "und", "die", "H\u00fcl\u00b7le", "f\u00e4llt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "KON", "ART", "NN", "VVFIN", "$."], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "der Odem des Fr\u00fchlings durchweht die Welt,", "tokens": ["der", "O\u00b7dem", "des", "Fr\u00fch\u00b7lings", "durch\u00b7weht", "die", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "und die Rosen wollen bl\u00fchen.", "tokens": ["und", "die", "Ro\u00b7sen", "wol\u00b7len", "bl\u00fc\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Vom ragenden Dome der Glockenschall,", "tokens": ["Vom", "ra\u00b7gen\u00b7den", "Do\u00b7me", "der", "Glo\u00b7cken\u00b7schall", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "im knospenden Flieder die Nachtigall,", "tokens": ["im", "knos\u00b7pen\u00b7den", "Flie\u00b7der", "die", "Nach\u00b7ti\u00b7gall", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "auf der Lippe ein Lied entglommen,", "tokens": ["auf", "der", "Lip\u00b7pe", "ein", "Lied", "ent\u00b7glom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "das dich, du br\u00e4utliche Erde preist,", "tokens": ["das", "dich", ",", "du", "br\u00e4ut\u00b7li\u00b7che", "Er\u00b7de", "preist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "$,", "PPER", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "und dich, der Pfingsten heiligen Geist,", "tokens": ["und", "dich", ",", "der", "Pfings\u00b7ten", "hei\u00b7li\u00b7gen", "Geist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "der \u00fcber die Welt gekommen!", "tokens": ["der", "\u00fc\u00b7ber", "die", "Welt", "ge\u00b7kom\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Und was auch die Seelen in Bande schlug,", "tokens": ["Und", "was", "auch", "die", "See\u00b7len", "in", "Ban\u00b7de", "schlug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "der Dogmen Zwang und der Formeln Fluch,", "tokens": ["der", "Dog\u00b7men", "Zwang", "und", "der", "For\u00b7meln", "Fluch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "ART", "NN", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "zerschmilzt in lodernden Flammen:", "tokens": ["zer\u00b7schmilzt", "in", "lo\u00b7dern\u00b7den", "Flam\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "vom ragenden Dome der Glockenklang,", "tokens": ["vom", "ra\u00b7gen\u00b7den", "Do\u00b7me", "der", "Glo\u00b7cken\u00b7klang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "im bl\u00fchenden Laube der Vogelsang, \u2013", "tokens": ["im", "bl\u00fc\u00b7hen\u00b7den", "Lau\u00b7be", "der", "Vo\u00b7gel\u00b7sang", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "$,", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "wie stimmt das so k\u00f6stlich zusammen!", "tokens": ["wie", "stimmt", "das", "so", "k\u00f6st\u00b7lich", "zu\u00b7sam\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PDS", "ADV", "ADJD", "PTKVZ", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.4": {"line.1": {"text": "Der Lenzwind braust; doch woher er weht,", "tokens": ["Der", "Lenz\u00b7wind", "braust", ";", "doch", "wo\u00b7her", "er", "weht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "KON", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "ihr wi\u00dft es nicht, noch wohin er geht.", "tokens": ["ihr", "wi\u00dft", "es", "nicht", ",", "noch", "wo\u00b7hin", "er", "geht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$,", "KON", "PWAV", "PPER", "VVFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Frei st\u00fcrmt er daher und von dannen \u2013", "tokens": ["Frei", "st\u00fcrmt", "er", "da\u00b7her", "und", "von", "dan\u00b7nen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PAV", "KON", "APPR", "ADV", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "und ihn, der jegliche Fessel rei\u00dft,", "tokens": ["und", "ihn", ",", "der", "jeg\u00b7li\u00b7che", "Fes\u00b7sel", "rei\u00dft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "PRELS", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "den starken, freien, den heiligen Geist,", "tokens": ["den", "star\u00b7ken", ",", "frei\u00b7en", ",", "den", "hei\u00b7li\u00b7gen", "Geist", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "ihn wolltet ihr halten und bannen?!", "tokens": ["ihn", "woll\u00b7tet", "ihr", "hal\u00b7ten", "und", "ban\u00b7nen", "?!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVINF", "KON", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.5": {"line.1": {"text": "Verlorene M\u00fch! Er ist frei, ist frei! \u2013", "tokens": ["Ver\u00b7lo\u00b7re\u00b7ne", "M\u00fch", "!", "Er", "ist", "frei", ",", "ist", "frei", "!", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ADJA", "NN", "$.", "PPER", "VAFIN", "ADJD", "$,", "VAFIN", "ADJD", "$.", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Den Harten und Stolzen geht er vorbei,", "tokens": ["Den", "Har\u00b7ten", "und", "Stol\u00b7zen", "geht", "er", "vor\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "die lieblos, was schwach ist, verdammen;", "tokens": ["die", "lieb\u00b7los", ",", "was", "schwach", "ist", ",", "ver\u00b7dam\u00b7men", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJD", "$,", "PRELS", "ADJD", "VAFIN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "unsterbliches Leben entlockt er der Gruft, \u2013", "tokens": ["uns\u00b7terb\u00b7li\u00b7ches", "Le\u00b7ben", "ent\u00b7lockt", "er", "der", "Gruft", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "ART", "NN", "$,", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "und wo ihn ein Herz in Demut ruft,", "tokens": ["und", "wo", "ihn", "ein", "Herz", "in", "De\u00b7mut", "ruft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "da f\u00fcllt er's mit heiligen Flammen.", "tokens": ["da", "f\u00fcllt", "er's", "mit", "hei\u00b7li\u00b7gen", "Flam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.6": {"line.1": {"text": "Die duftenden Birken tragt herbei,", "tokens": ["Die", "duf\u00b7ten\u00b7den", "Bir\u00b7ken", "tragt", "her\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "da\u00df br\u00e4utlich geschm\u00fcckt eure Wohnung sei", "tokens": ["da\u00df", "br\u00e4ut\u00b7lich", "ge\u00b7schm\u00fcckt", "eu\u00b7re", "Woh\u00b7nung", "sei"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "VVPP", "PPOSAT", "NN", "VAFIN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "und in Liebe das Herz entglommen, \u2013", "tokens": ["und", "in", "Lie\u00b7be", "das", "Herz", "ent\u00b7glom\u00b7men", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "APPR", "NN", "ART", "NN", "VVPP", "$,", "$("], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "die T\u00fcr macht auf und die Tore weit:", "tokens": ["die", "T\u00fcr", "macht", "auf", "und", "die", "To\u00b7re", "weit", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "KON", "ART", "NN", "ADJD", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "du trostvoll heilige Pfingstenzeit,", "tokens": ["du", "trost\u00b7voll", "hei\u00b7li\u00b7ge", "Pfings\u00b7ten\u00b7zeit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "sei aller Welt willkommen!", "tokens": ["sei", "al\u00b7ler", "Welt", "will\u00b7kom\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "O du sonnige, wonnige Pfingstenzeit!", "tokens": ["O", "du", "son\u00b7ni\u00b7ge", ",", "won\u00b7ni\u00b7ge", "Pfings\u00b7ten\u00b7zeit", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Der Himmel ist blau und das Herz so weit,", "tokens": ["Der", "Him\u00b7mel", "ist", "blau", "und", "das", "Herz", "so", "weit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "in d