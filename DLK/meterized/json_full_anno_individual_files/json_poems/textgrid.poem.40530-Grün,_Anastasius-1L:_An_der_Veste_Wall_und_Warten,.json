{"textgrid.poem.40530": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: An der Veste Wall und Warten,", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "An der Veste Wall und Warten,", "tokens": ["An", "der", "Ves\u00b7te", "Wall", "und", "War\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die dich z\u00fcgeln soll, Florenz,", "tokens": ["Die", "dich", "z\u00fc\u00b7geln", "soll", ",", "Flo\u00b7renz", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PPER", "VVINF", "VMFIN", "$,", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Lehnt sich deines F\u00fcrsten Garten,", "tokens": ["Lehnt", "sich", "dei\u00b7nes", "F\u00fcrs\u00b7ten", "Gar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bl\u00fcthenvoll im sonn'gen Lenz.", "tokens": ["Bl\u00fc\u00b7then\u00b7voll", "im", "sonn'\u00b7gen", "Lenz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Doch des Schlummers s\u00fc\u00dfe Schlinge", "tokens": ["Doch", "des", "Schlum\u00b7mers", "s\u00fc\u00b7\u00dfe", "Schlin\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "H\u00e4lt die Wacht am Wall umfahn,", "tokens": ["H\u00e4lt", "die", "Wacht", "am", "Wall", "um\u00b7fahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPRART", "NE", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Rost zerfra\u00df des Kriegers Klinge,", "tokens": ["Rost", "zer\u00b7fra\u00df", "des", "Krie\u00b7gers", "Klin\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Seiner Flinte fehlt der Hahn.", "tokens": ["Sei\u00b7ner", "Flin\u00b7te", "fehlt", "der", "Hahn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Tief wohl schl\u00e4ft er; ihn umd\u00fcstert", "tokens": ["Tief", "wohl", "schl\u00e4ft", "er", ";", "ihn", "um\u00b7d\u00fcs\u00b7tert"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "ADV", "VVFIN", "PPER", "$.", "PPER", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Keine Ahnung der Gefahr.", "tokens": ["Kei\u00b7ne", "Ah\u00b7nung", "der", "Ge\u00b7fahr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "H\u00f6rt er's nicht, wie's unten fl\u00fcstert", "tokens": ["H\u00f6rt", "er's", "nicht", ",", "wie's", "un\u00b7ten", "fl\u00fcs\u00b7tert"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PIS", "PTKNEG", "$,", "VVFIN", "ADV", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Droh'nd aus der Belag'rer Schaar?", "tokens": ["Droh'nd", "aus", "der", "Be\u00b7lag'\u00b7rer", "Schaar", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Sieht er nicht im Thale blinken", "tokens": ["Sieht", "er", "nicht", "im", "Tha\u00b7le", "blin\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKNEG", "APPRART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Federb\u00fcsche aller Art,", "tokens": ["Fe\u00b7der\u00b7b\u00fc\u00b7sche", "al\u00b7ler", "Art", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hundertfarb'ge F\u00e4hnlein winken,", "tokens": ["Hun\u00b7dert\u00b7fa\u00b7rb'\u00b7ge", "F\u00e4hn\u00b7lein", "win\u00b7ken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Denen, Lenz, dein Heer sich schaart?", "tokens": ["De\u00b7nen", ",", "Lenz", ",", "dein", "Heer", "sich", "schaart", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "NN", "$,", "PPOSAT", "NN", "PRF", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Und doch blasen aus den Beeten", "tokens": ["Und", "doch", "bla\u00b7sen", "aus", "den", "Bee\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVINF", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie ein Janitscharenchor", "tokens": ["Wie", "ein", "Ja\u00b7nit\u00b7scha\u00b7ren\u00b7chor"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Tausend bl\u00fchende Trompeten", "tokens": ["Tau\u00b7send", "bl\u00fc\u00b7hen\u00b7de", "Trom\u00b7pe\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["CARD", "ADJA", "NN"], "meter": "+-+--++-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Schon zum Sturm, zum Sturm empor!", "tokens": ["Schon", "zum", "Sturm", ",", "zum", "Sturm", "em\u00b7por", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$,", "APPRART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Und doch schwebt schon ob der Veste", "tokens": ["Und", "doch", "schwebt", "schon", "ob", "der", "Ves\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ADV", "KOUS", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Eine Lerch' als Luftballon,", "tokens": ["Ei\u00b7ne", "Ler\u00b7ch'", "als", "Luft\u00b7bal\u00b7lon", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KOUS", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Die vom Feindesheer die beste", "tokens": ["Die", "vom", "Fein\u00b7des\u00b7heer", "die", "bes\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPRART", "NN", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kundschaft bringt als dein Spion!", "tokens": ["Kund\u00b7schaft", "bringt", "als", "dein", "Spi\u00b7on", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "KOKOM", "PPOSAT", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.7": {"line.1": {"text": "Schwert- und Feuerlilie schwingen", "tokens": ["Schwer\u00b7t", "und", "Feu\u00b7er\u00b7li\u00b7lie", "schwin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["TRUNC", "KON", "NN", "VVINF"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Waffen hoch im Zornesmuth,", "tokens": ["Waf\u00b7fen", "hoch", "im", "Zor\u00b7nes\u00b7muth", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Jene scharfe breite Klingen,", "tokens": ["Je\u00b7ne", "schar\u00b7fe", "brei\u00b7te", "Klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Diese rothe Luntengluth.", "tokens": ["Die\u00b7se", "ro\u00b7the", "Lun\u00b7ten\u00b7gluth", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Mit den breiten gr\u00fcnen Tatzen", "tokens": ["Mit", "den", "brei\u00b7ten", "gr\u00fc\u00b7nen", "Tat\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Haut der Feigenbaum die Wand;", "tokens": ["Haut", "der", "Fei\u00b7gen\u00b7baum", "die", "Wand", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Tausend Blumenknospen platzen,", "tokens": ["Tau\u00b7send", "Blu\u00b7men\u00b7knos\u00b7pen", "plat\u00b7zen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie im Peloton entbrannt!", "tokens": ["Wie", "im", "Pe\u00b7lo\u00b7ton", "ent\u00b7brannt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Bravo! Wie ein Hagelschauer", "tokens": ["Bra\u00b7vo", "!", "Wie", "ein", "Ha\u00b7gel\u00b7schau\u00b7er"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ITJ", "$.", "PWAV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schwarzer Flintenkugeln h\u00e4ngt", "tokens": ["Schwar\u00b7zer", "Flin\u00b7ten\u00b7ku\u00b7geln", "h\u00e4ngt"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Rings entlang der Veste Mauer", "tokens": ["Rings", "ent\u00b7lang", "der", "Ves\u00b7te", "Mau\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPO", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Traub' an Traube dicht gedr\u00e4ngt!", "tokens": ["Traub'", "an", "Trau\u00b7be", "dicht", "ge\u00b7dr\u00e4ngt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Goldorangenbomben stecken", "tokens": ["Gol\u00b7do\u00b7ran\u00b7gen\u00b7bom\u00b7ben", "ste\u00b7cken"], "token_info": ["word", "word"], "pos": ["NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Allerw\u00e4rts im Mauernritz;", "tokens": ["Al\u00b7ler\u00b7w\u00e4rts", "im", "Mau\u00b7ern\u00b7ritz", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Lenz, du f\u00fchrst gar tapfre Recken,", "tokens": ["Lenz", ",", "du", "f\u00fchrst", "gar", "tapf\u00b7re", "Re\u00b7cken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lenz, du f\u00fchrst gar gut Gesch\u00fctz!", "tokens": ["Lenz", ",", "du", "f\u00fchrst", "gar", "gut", "Ge\u00b7sch\u00fctz", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "ADV", "ADJD", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Legst Spaliere und Stacketen", "tokens": ["Legst", "Spa\u00b7lie\u00b7re", "und", "Sta\u00b7cke\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "NN", "KON", "NN"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.2": {"text": "Als Sturmleitern an den Wall,", "tokens": ["Als", "Sturm\u00b7lei\u00b7tern", "an", "den", "Wall", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "ART", "NE", "$,"], "meter": "-++-+-+", "measure": "unknown.measure.tetra"}, "line.3": {"text": "In die luft'gen Sprossen treten", "tokens": ["In", "die", "luft'\u00b7gen", "Spros\u00b7sen", "tre\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Deine bl\u00fch'nden St\u00fcrmer all!", "tokens": ["Dei\u00b7ne", "bl\u00fch'n\u00b7den", "St\u00fcr\u00b7mer", "all", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PIAT", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Ha, Verrath selbst in der Veste!", "tokens": ["Ha", ",", "Ver\u00b7rath", "selbst", "in", "der", "Ves\u00b7te", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "NN", "ADV", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Helfend reicht am Wallesrand", "tokens": ["Hel\u00b7fend", "reicht", "am", "Wal\u00b7les\u00b7rand"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Eine Rose, froh der G\u00e4ste,", "tokens": ["Ei\u00b7ne", "Ro\u00b7se", ",", "froh", "der", "G\u00e4s\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Rasch den Klimmern ihre Hand!", "tokens": ["Rasch", "den", "Klim\u00b7mern", "ih\u00b7re", "Hand", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Bl\u00fcthenrank' und Epheu standen", "tokens": ["Bl\u00fcthen\u00b7rank'", "und", "E\u00b7pheu", "stan\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Schon am Walle bei der Wacht',", "tokens": ["Schon", "am", "Wal\u00b7le", "bei", "der", "Wacht'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die sie knebelten und banden,", "tokens": ["Die", "sie", "kne\u00b7bel\u00b7ten", "und", "ban\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "KON", "ADJD", "$,"], "meter": "+-+---+-", "measure": "unknown.measure.tri"}, "line.4": {"text": "Als sie noch zu tr\u00e4umen dacht'.", "tokens": ["Als", "sie", "noch", "zu", "tr\u00e4u\u00b7men", "dacht'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Solchem Sieg zum Ehrenbogen", "tokens": ["Sol\u00b7chem", "Sieg", "zum", "Eh\u00b7ren\u00b7bo\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "W\u00f6lbt aus Silbers\u00e4ulen hell,", "tokens": ["W\u00f6lbt", "aus", "Sil\u00b7ber\u00b7s\u00e4u\u00b7len", "hell", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Von Demantenstaub umflogen,", "tokens": ["Von", "De\u00b7man\u00b7ten\u00b7staub", "um\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sich des Gartens Springequell.", "tokens": ["Sich", "des", "Gar\u00b7tens", "Sprin\u00b7ge\u00b7quell", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "Deiner Truppen Banner ragen,", "tokens": ["Dei\u00b7ner", "Trup\u00b7pen", "Ban\u00b7ner", "ra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lenz, nun auf den Wellen dort;", "tokens": ["Lenz", ",", "nun", "auf", "den", "Wel\u00b7len", "dort", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "APPR", "ART", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ha, wer wagt's, die zu verjagen?", "tokens": ["Ha", ",", "wer", "wagt's", ",", "die", "zu", "ver\u00b7ja\u00b7gen", "?"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWS", "VVFIN", "$,", "PRELS", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.