{"dta.poem.10265": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "\u201eich habe dich so sehr geliebet.\u201c", "genre": "Lyrik", "period": "N.A.", "pub_year": "1883", "urn": "urn:nbn:de:kobv:b4-200905197184", "language": ["de:0.99"], "booktitle": "Liliencron, Detlev von: Adjutantenritte und andere Gedichte. Leipzig, [1883]."}, "poem": {"stanza.1": {"line.1": {"text": "Ich war bei hellem Sommerlicht             ", "tokens": ["Ich", "war", "bei", "hel\u00b7lem", "Som\u00b7mer\u00b7licht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In eine D\u00e4mmergruft gestiegen,", "tokens": ["In", "ei\u00b7ne", "D\u00e4m\u00b7mer\u00b7gruft", "ge\u00b7stie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wo Sarkophage, dicht an dicht,", "tokens": ["Wo", "Sar\u00b7ko\u00b7pha\u00b7ge", ",", "dicht", "an", "dicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "$,", "ADJD", "APPR", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie Denker in Gedanken, schwiegen.", "tokens": ["Wie", "Den\u00b7ker", "in", "Ge\u00b7dan\u00b7ken", ",", "schwie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der S\u00e4rge Silberschilderei,", "tokens": ["Der", "S\u00e4r\u00b7ge", "Sil\u00b7ber\u00b7schil\u00b7de\u00b7rei", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wo Nam\u2019 und Wappen eingeschnitten,", "tokens": ["Wo", "Nam'", "und", "Wap\u00b7pen", "ein\u00b7ge\u00b7schnit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Umzog barocke Schn\u00f6rkelei,", "tokens": ["Um\u00b7zog", "ba\u00b7ro\u00b7cke", "Schn\u00f6r\u00b7ke\u00b7lei", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nach l\u00e4ngst verj\u00e4hrten alten Sitten.", "tokens": ["Nach", "l\u00e4ngst", "ver\u00b7j\u00e4hr\u00b7ten", "al\u00b7ten", "Sit\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Es traf mein Blick auf einen Sarg,", "tokens": ["Es", "traf", "mein", "Blick", "auf", "ei\u00b7nen", "Sarg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Aus all den andern Schmerzerrettern.", "tokens": ["Aus", "all", "den", "an\u00b7dern", "Schmer\u00b7zer\u00b7ret\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ich wu\u00dfte, wen die Truhe barg,", "tokens": ["Ich", "wu\u00df\u00b7te", ",", "wen", "die", "Tru\u00b7he", "barg", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Aus einer Chronik gelben Bl\u00e4ttern:", "tokens": ["Aus", "ei\u00b7ner", "Chro\u00b7nik", "gel\u00b7ben", "Bl\u00e4t\u00b7tern", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ein Jahr nach ihrer Hochzeit schied", "tokens": ["Ein", "Jahr", "nach", "ih\u00b7rer", "Hoch\u00b7zeit", "schied"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die junge Frau mit ihrem Knaben.", "tokens": ["Die", "jun\u00b7ge", "Frau", "mit", "ih\u00b7rem", "Kna\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und der, der nun die Sonne mied,", "tokens": ["Und", "der", ",", "der", "nun", "die", "Son\u00b7ne", "mied", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "$,", "PRELS", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sein einzig Gl\u00fcck war hier begraben.", "tokens": ["Sein", "ein\u00b7zig", "Gl\u00fcck", "war", "hier", "be\u00b7gra\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Schnee fiel in seine Sommerflur,", "tokens": ["Schnee", "fiel", "in", "sei\u00b7ne", "Som\u00b7mer\u00b7flur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er war zu tief, zu tief \u201ebetr\u00fcbet.\u201c", "tokens": ["Er", "war", "zu", "tief", ",", "zu", "tief", "\u201e", "be\u00b7tr\u00fc\u00b7bet", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "PTKA", "ADJD", "$,", "PTKA", "ADJD", "$(", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ich las auf ihrem Sarge nur:", "tokens": ["Ich", "las", "auf", "ih\u00b7rem", "Sar\u00b7ge", "nur", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u201eich habe dich so sehr geliebet.\u201c", "tokens": ["\u201e", "ich", "ha\u00b7be", "dich", "so", "sehr", "ge\u00b7lie\u00b7bet", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VAFIN", "PPER", "ADV", "ADV", "VVPP", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}