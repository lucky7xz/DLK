{"dta.poem.21376": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Caput XVII.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-30602-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich habe mich mit dem Kaiser gezankt", "tokens": ["Ich", "ha\u00b7be", "mich", "mit", "dem", "Kai\u00b7ser", "ge\u00b7zankt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "APPR", "ART", "NN", "VVPP"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Im Traum, im Traum versteht sich, \u2013", "tokens": ["Im", "Traum", ",", "im", "Traum", "ver\u00b7steht", "sich", ",", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "NN", "$,", "APPRART", "NN", "VVFIN", "PRF", "$,", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Im wachenden Zustand sprechen wir nicht", "tokens": ["Im", "wa\u00b7chen\u00b7den", "Zu\u00b7stand", "spre\u00b7chen", "wir", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "PPER", "PTKNEG"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Mit F\u00fcrsten so widersetzig.", "tokens": ["Mit", "F\u00fcrs\u00b7ten", "so", "wi\u00b7der\u00b7set\u00b7zig", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "ADJD", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}}}}