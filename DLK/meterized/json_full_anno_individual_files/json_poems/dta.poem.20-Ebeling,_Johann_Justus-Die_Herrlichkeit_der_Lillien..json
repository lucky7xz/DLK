{"dta.poem.20": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Die Herrlichkeit der Lillien.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198797", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Holde Lillie, sch\u00f6ne Blume!", "tokens": ["Hol\u00b7de", "Lil\u00b7lie", ",", "sch\u00f6\u00b7ne", "Blu\u00b7me", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Prange zu des Sch\u00f6pfers Ruhme,", "tokens": ["Pran\u00b7ge", "zu", "des", "Sch\u00f6p\u00b7fers", "Ruh\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Der dein weises Unschuldskleid,", "tokens": ["Der", "dein", "wei\u00b7ses", "Un\u00b7schulds\u00b7kleid", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit so grosser Herrligkeit,", "tokens": ["Mit", "so", "gros\u00b7ser", "Herr\u00b7lig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wundernsw\u00fcrdig ausgeschm\u00fckket;", "tokens": ["Wun\u00b7derns\u00b7w\u00fcr\u00b7dig", "aus\u00b7ge\u00b7schm\u00fck\u00b7ket", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wenn ich dich mit regen Sinn", "tokens": ["Wenn", "ich", "dich", "mit", "re\u00b7gen", "Sinn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "APPR", "ADJA", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Aller Blumen K\u00f6nigin!", "tokens": ["Al\u00b7ler", "Blu\u00b7men", "K\u00f6\u00b7ni\u00b7gin", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Auf dem gr\u00fcnen Thron erblikket:", "tokens": ["Auf", "dem", "gr\u00fc\u00b7nen", "Thron", "er\u00b7blik\u00b7ket", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "So deucht mir, es kan dein Funkeln,", "tokens": ["So", "deucht", "mir", ",", "es", "kan", "dein", "Fun\u00b7keln", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PPER", "VMFIN", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "Aller F\u00fcrsten Glanz verdunkeln.", "tokens": ["Al\u00b7ler", "F\u00fcrs\u00b7ten", "Glanz", "ver\u00b7dun\u00b7keln", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ja die Schrift das Buch der Warheit,", "tokens": ["Ja", "die", "Schrift", "das", "Buch", "der", "War\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ART", "NN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "R\u00fchmet deinen Glanz und Klarheit,", "tokens": ["R\u00fch\u00b7met", "dei\u00b7nen", "Glanz", "und", "Klar\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und des H\u00f6chsten Lehrers Mund,", "tokens": ["Und", "des", "H\u00f6chs\u00b7ten", "Leh\u00b7rers", "Mund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Macht uns deine Sch\u00f6nheit kund.", "tokens": ["Macht", "uns", "dei\u00b7ne", "Sch\u00f6n\u00b7heit", "kund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Unser Sinn wird \u00fcberf\u00fchret,", "tokens": ["Un\u00b7ser", "Sinn", "wird", "\u00fc\u00b7berf\u00b7\u00fch\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Da\u00df der K\u00f6nig Salomon,", "tokens": ["Da\u00df", "der", "K\u00f6\u00b7nig", "Sa\u00b7lo\u00b7mon", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Der auf einem g\u00fcldnen Thron,", "tokens": ["Der", "auf", "ei\u00b7nem", "g\u00fcld\u00b7nen", "Thron", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Mehr als K\u00f6niglich gezieret,", "tokens": ["Mehr", "als", "K\u00f6\u00b7nig\u00b7lich", "ge\u00b7zie\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "KOKOM", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Keine solche Pracht gewiesen,", "tokens": ["Kei\u00b7ne", "sol\u00b7che", "Pracht", "ge\u00b7wie\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "PIAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Als uns wird an dir gepriesen.", "tokens": ["Als", "uns", "wird", "an", "dir", "ge\u00b7prie\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VAFIN", "APPR", "PPER", "VVPP", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.3": {"line.1": {"text": "Dieses K\u00f6nigliche Prangen", "tokens": ["Die\u00b7ses", "K\u00f6\u00b7nig\u00b7li\u00b7che", "Pran\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["PDAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "War mit manchen Schmuk behangen;", "tokens": ["War", "mit", "man\u00b7chen", "Schmuk", "be\u00b7han\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PIAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Salomonis Purpur-Kleid,", "tokens": ["Sa\u00b7lo\u00b7mo\u00b7nis", "Pur\u00b7pur\u00b7Kleid", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Schimmerte mit Herrlichkeit:", "tokens": ["Schim\u00b7mer\u00b7te", "mit", "Herr\u00b7lich\u00b7keit", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und sein majest\u00e4tisch Wesen,", "tokens": ["Und", "sein", "ma\u00b7jes\u00b7t\u00e4\u00b7tisch", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Borgte Ansehn, Pracht und Schein,", "tokens": ["Borg\u00b7te", "An\u00b7sehn", ",", "Pracht", "und", "Schein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Von so manchen Edelstein:", "tokens": ["Von", "so", "man\u00b7chen", "E\u00b7del\u00b7stein", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Alles war recht auserlesen,", "tokens": ["Al\u00b7les", "war", "recht", "au\u00b7ser\u00b7le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Heitre Blizze, bunte Strahlen,", "tokens": ["Heit\u00b7re", "Bliz\u00b7ze", ",", "bun\u00b7te", "Strah\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "K\u00f6nnten ihn recht herrlich mahlen.", "tokens": ["K\u00f6nn\u00b7ten", "ihn", "recht", "herr\u00b7lich", "mah\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Wie seid ihr, ihr Erdenkinder,", "tokens": ["Wie", "seid", "ihr", ",", "ihr", "Er\u00b7den\u00b7kin\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "$,", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Salomonis Ueberwinder,", "tokens": ["Sa\u00b7lo\u00b7mo\u00b7nis", "Ue\u00b7ber\u00b7win\u00b7der", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Uebertrift eur Attlaskleid,", "tokens": ["Ue\u00b7bert\u00b7rift", "eur", "Att\u00b7las\u00b7kleid", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Den bestrahlten Purpur weit?", "tokens": ["Den", "be\u00b7strahl\u00b7ten", "Pur\u00b7pur", "weit", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ja! wir m\u00fcssen es gestehen,", "tokens": ["Ja", "!", "wir", "m\u00fcs\u00b7sen", "es", "ge\u00b7ste\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "PPER", "VMFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wer euch, wenn ihr bl\u00fcht, beschaut,", "tokens": ["Wer", "euch", ",", "wenn", "ihr", "bl\u00fcht", ",", "be\u00b7schaut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "PPER", "$,", "KOUS", "PPER", "VVFIN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Kan den, der euch auferbaut,", "tokens": ["Kan", "den", ",", "der", "euch", "au\u00b7fer\u00b7baut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Und eur Meisterst\u00fck besehen:", "tokens": ["Und", "eur", "Meis\u00b7ter\u00b7st\u00fck", "be\u00b7se\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Denn eur Schmuk ist auserlesen,", "tokens": ["Denn", "eur", "Schmuk", "ist", "au\u00b7ser\u00b7le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Und ein angebohrnes Wesen.", "tokens": ["Und", "ein", "an\u00b7ge\u00b7bohr\u00b7nes", "We\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Eurer Bl\u00e4tter zart Gespinste,", "tokens": ["Eu\u00b7rer", "Bl\u00e4t\u00b7ter", "zart", "Ge\u00b7spins\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Offenbahrt uns Wunderk\u00fcnste;", "tokens": ["Of\u00b7fen\u00b7bahrt", "uns", "Wun\u00b7der\u00b7k\u00fcns\u00b7te", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Uebertrift noch den Attlas;", "tokens": ["Ue\u00b7bert\u00b7rift", "noch", "den", "Att\u00b7las", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "$."], "meter": "+-+--++", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Wenn man durchs Vergr\u00f6\u00dfrungs-Gla\u00df", "tokens": ["Wenn", "man", "durchs", "Ver\u00b7gr\u00f6\u00df\u00b7rungs\u00b7Gla\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PIS", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Jhre Z\u00e4rtlichkeit besiehet,", "tokens": ["Ih\u00b7re", "Z\u00e4rt\u00b7lich\u00b7keit", "be\u00b7sie\u00b7het", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und die Faden dran erwegt,", "tokens": ["Und", "die", "Fa\u00b7den", "dran", "er\u00b7wegt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PAV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wie sie wunderbar gelegt;", "tokens": ["Wie", "sie", "wun\u00b7der\u00b7bar", "ge\u00b7legt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Wie dadurch ein Lichtstrahl gl\u00fchet:", "tokens": ["Wie", "da\u00b7durch", "ein", "Licht\u00b7strahl", "gl\u00fc\u00b7het", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PAV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "So mu\u00df man ger\u00fchrt erkennen,", "tokens": ["So", "mu\u00df", "man", "ge\u00b7r\u00fchrt", "er\u00b7ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "VVPP", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "Da\u00df ihr herrlich seid zu nennen.", "tokens": ["Da\u00df", "ihr", "herr\u00b7lich", "seid", "zu", "nen\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VAFIN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Man wird auf den glatten Fl\u00e4chen,", "tokens": ["Man", "wird", "auf", "den", "glat\u00b7ten", "Fl\u00e4\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Wenn sich dran die Strahlen brechen,", "tokens": ["Wenn", "sich", "dran", "die", "Strah\u00b7len", "bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "PAV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Viele K\u00fcgelchen gewahr,", "tokens": ["Vie\u00b7le", "K\u00fc\u00b7gel\u00b7chen", "ge\u00b7wahr", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Die durchsichtig, schimmernd klar:", "tokens": ["Die", "durch\u00b7sich\u00b7tig", ",", "schim\u00b7mernd", "klar", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJD", "$,", "ADJD", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wer dies sieht, der solte meinen,", "tokens": ["Wer", "dies", "sieht", ",", "der", "sol\u00b7te", "mei\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PDS", "VVFIN", "$,", "PRELS", "VMFIN", "PPOSAT", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Da\u00df auf einem Lillien-Blat,", "tokens": ["Da\u00df", "auf", "ei\u00b7nem", "Lil\u00b7li\u00b7en\u00b7Blat", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "So viel als es H\u00f6hen hat,", "tokens": ["So", "viel", "als", "es", "H\u00f6\u00b7hen", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KOUS", "PPER", "NN", "VAFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "So viel Edelsteine scheinen;", "tokens": ["So", "viel", "E\u00b7del\u00b7stei\u00b7ne", "schei\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Und da\u00df es mit Regenbogen,", "tokens": ["Und", "da\u00df", "es", "mit", "Re\u00b7gen\u00b7bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "NN", "$,"], 