{"dta.poem.11816": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "102.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "In L\u00fcften schwebt die Lerch' und \u00fcber ihr der Aar,", "tokens": ["In", "L\u00fcf\u00b7ten", "schwebt", "die", "Ler\u00b7ch'", "und", "\u00fc\u00b7ber", "ihr", "der", "Aar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "KON", "APPR", "PPOSAT", "ART", "NN", "$,"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Nicht ahnt die S\u00e4ngerinn die schweigende Gefahr.", "tokens": ["Nicht", "ahnt", "die", "S\u00e4n\u00b7ge\u00b7rinn", "die", "schwei\u00b7gen\u00b7de", "Ge\u00b7fahr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Nicht ihr droht die Gefahr, der fr\u00fchwach aufgeschwungnen,", "tokens": ["Nicht", "ihr", "droht", "die", "Ge\u00b7fahr", ",", "der", "fr\u00fch\u00b7wach", "auf\u00b7ge\u00b7schwung\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PPER", "VVFIN", "ART", "NN", "$,", "PRELS", "ADJD", "VVINF", "$,"], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Sie droht den unten tief vom Schlummer noch umschlungnen,", "tokens": ["Sie", "droht", "den", "un\u00b7ten", "tief", "vom", "Schlum\u00b7mer", "noch", "um\u00b7schlung\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADV", "ADJD", "APPRART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Den jetzt vom Lerchenschall erst aufgesungenen,", "tokens": ["Den", "jetzt", "vom", "Ler\u00b7chen\u00b7schall", "erst", "auf\u00b7ge\u00b7sun\u00b7ge\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPRART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.2": {"text": "Dann von der Adlerkrall' im Nu bezwungenen.", "tokens": ["Dann", "von", "der", "Ad\u00b7ler\u00b7krall'", "im", "Nu", "be\u00b7zwun\u00b7ge\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "APPRART", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}}}}}