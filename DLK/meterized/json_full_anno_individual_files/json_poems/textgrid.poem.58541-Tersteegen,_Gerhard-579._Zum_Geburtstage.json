{"textgrid.poem.58541": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "579. Zum Geburtstage", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du gabst mir, Herr, den Atem meines Lebens,", "tokens": ["Du", "gabst", "mir", ",", "Herr", ",", "den", "A\u00b7tem", "mei\u00b7nes", "Le\u00b7bens", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "NN", "$,", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du gabst mir Gnad' und Kraft und viel dabei;", "tokens": ["Du", "gabst", "mir", "Gnad'", "und", "Kraft", "und", "viel", "da\u00b7bei", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "KON", "NN", "KON", "ADV", "PAV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Hab' ich's nicht dir, so hab' ich's all' vergebens,", "tokens": ["Hab'", "ich's", "nicht", "dir", ",", "so", "hab'", "ich's", "all'", "ver\u00b7ge\u00b7bens", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "PTKNEG", "PPER", "$,", "ADV", "VAFIN", "PIS", "PIS", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Drum gib mir's heut zum neuen Jahr aufs neu!", "tokens": ["Drum", "gib", "mir's", "heut", "zum", "neu\u00b7en", "Jahr", "aufs", "neu", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVIMP", "NE", "ADV", "APPRART", "ADJA", "NN", "APPRART", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ach, bleibe doch beim H\u00e4uflein deiner Kinder,", "tokens": ["Ach", ",", "blei\u00b7be", "doch", "beim", "H\u00e4uf\u00b7lein", "dei\u00b7ner", "Kin\u00b7der", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVFIN", "ADV", "APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der Abend kommt, der Frommen werden minder;", "tokens": ["Der", "A\u00b7bend", "kommt", ",", "der", "From\u00b7men", "wer\u00b7den", "min\u00b7der", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VAFIN", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Willst du, soll ich noch wallen in der H\u00fctte,", "tokens": ["Willst", "du", ",", "soll", "ich", "noch", "wal\u00b7len", "in", "der", "H\u00fct\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "VMFIN", "PPER", "ADV", "VVINF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Mach mich zum Licht und Salz in ihrer Mitte!", "tokens": ["Mach", "mich", "zum", "Licht", "und", "Salz", "in", "ih\u00b7rer", "Mit\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "KON", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Du gabst mir, Herr, den Atem meines Lebens,", "tokens": ["Du", "gabst", "mir", ",", "Herr", ",", "den", "A\u00b7tem", "mei\u00b7nes", "Le\u00b7bens", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "NN", "$,", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambi