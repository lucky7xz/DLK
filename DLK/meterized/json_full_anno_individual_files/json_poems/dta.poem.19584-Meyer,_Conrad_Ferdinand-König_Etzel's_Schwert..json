{"dta.poem.19584": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "K\u00f6nig Etzel's Schwert.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "Der Kaiser spricht zu Ritter Hug:", "tokens": ["Der", "Kai\u00b7ser", "spricht", "zu", "Rit\u00b7ter", "Hug", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NE", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u201edu hast f\u00fcr mich dein Schwert verspellt,", "tokens": ["\u201e", "du", "hast", "f\u00fcr", "mich", "dein", "Schwert", "ver\u00b7spellt", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "APPR", "PPER", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Des Eisens ist bei mir genug,", "tokens": ["Des", "Ei\u00b7sens", "ist", "bei", "mir", "ge\u00b7nug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Geh, w\u00e4hl' dir eins, das dir gef\u00e4llt!\u201c", "tokens": ["Geh", ",", "w\u00e4hl'", "dir", "eins", ",", "das", "dir", "ge\u00b7f\u00e4llt", "!", "\u201c"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NE", "$,", "VVIMP", "PPER", "PIS", "$,", "PRELS", "PPER", "VVPP", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Hug schreitet durch den Waffensaal,", "tokens": ["Hug", "schrei\u00b7tet", "durch", "den", "Waf\u00b7fen\u00b7saal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wo stets der graue Schaffner sitzt.", "tokens": ["Wo", "stets", "der", "grau\u00b7e", "Schaff\u00b7ner", "sitzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u201eder Kaiser giebt mir freie Wahl", "tokens": ["\u201e", "der", "Kai\u00b7ser", "giebt", "mir", "frei\u00b7e", "Wahl"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "ART", "NN", "VVFIN", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Aus Allem was da hangt und blitzt!\u201c", "tokens": ["Aus", "Al\u00b7lem", "was", "da", "hangt", "und", "blitzt", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PIS", "PWS", "ADV", "VVFIN", "KON", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Er pr\u00fcft und w\u00e4gt. Von ihrem Ort", "tokens": ["Er", "pr\u00fcft", "und", "w\u00e4gt", ".", "Von", "ih\u00b7rem", "Ort"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$.", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Langt er die Schwerter mannigfalt \u2014", "tokens": ["Langt", "er", "die", "Schwer\u00b7ter", "man\u00b7nig\u00b7falt"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "VVFIN", "$("], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u201esprich, wessen ist das gro\u00dfe dort,", "tokens": ["\u201e", "sprich", ",", "wes\u00b7sen", "ist", "das", "gro\u00b7\u00dfe", "dort", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "$,", "PWAV", "VAFIN", "ART", "ADJA", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gewaltig, heidnisch, ungestalt?\u201c", "tokens": ["Ge\u00b7wal\u00b7tig", ",", "heid\u00b7nisch", ",", "un\u00b7ge\u00b7stalt", "?", "\u201c"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "punct"], "pos": ["ADJD", "$,", "ADJD", "$,", "ADJD", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "\u201edes W\u00fcrgers Etzel!\u201c fl\u00fcstert scheu", "tokens": ["\u201e", "des", "W\u00fcr\u00b7gers", "Et\u00b7zel", "!", "\u201c", "fl\u00fcs\u00b7tert", "scheu"], "token_info": ["punct", "word", "word", "word", "punct", "punct", "word", "word"], "pos": ["$(", "ART", "NN", "NE", "$.", "$(", "VVFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Graue, der es h\u00e4lt in Hut.", "tokens": ["Der", "Grau\u00b7e", ",", "der", "es", "h\u00e4lt", "in", "Hut", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u201edes Hunnenk\u00f6nigs! Meiner Treu,", "tokens": ["\u201e", "des", "Hun\u00b7nen\u00b7k\u00f6\u00b7nigs", "!", "Mei\u00b7ner", "Treu", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "$.", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So lechzt und d\u00fcrstet es nach Blut!\u201c", "tokens": ["So", "lechzt", "und", "d\u00fcrs\u00b7tet", "es", "nach", "Blut", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "PPER", "APPR", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "\u201ela\u00df ruhn. Es hat genug gew\u00fcrgt!", "tokens": ["\u201e", "la\u00df", "ruhn", ".", "Es", "hat", "ge\u00b7nug", "ge\u00b7w\u00fcrgt", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "VVINF", "$.", "PPER", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die todte Wuth erwecke nicht!\u201c", "tokens": ["Die", "tod\u00b7te", "Wuth", "er\u00b7we\u00b7cke", "nicht", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKNEG", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u201egieb her! Dem ist der Sieg verb\u00fcrgt,", "tokens": ["\u201e", "gieb", "her", "!", "Dem", "ist", "der", "Sieg", "ver\u00b7b\u00fcrgt", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "PTKVZ", "$.", "PDS", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der mit dem Schwert des Hunnen ficht!\u201c", "tokens": ["Der", "mit", "dem", "Schwert", "des", "Hun\u00b7nen", "ficht", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ART", "NN", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Und wieder sprengt er in den Kampf.", "tokens": ["Und", "wie\u00b7der", "sprengt", "er", "in", "den", "Kampf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u201edu hast dich lange nicht geletzt,", "tokens": ["\u201e", "du", "hast", "dich", "lan\u00b7ge", "nicht", "ge\u00b7letzt", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "PPER", "ADV", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Schwert Etzel's, an des Blutes Dampf!", "tokens": ["Schwert", "Et\u00b7zel's", ",", "an", "des", "Blu\u00b7tes", "Dampf", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Drum freue dich und trinke jetzt!\u201c", "tokens": ["Drum", "freu\u00b7e", "dich", "und", "trin\u00b7ke", "jetzt", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PAV", "VVFIN", "PPER", "KON", "VVFIN", "ADV", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Er schwingt es weit, er m\u00e4ht und m\u00e4ht", "tokens": ["Er", "schwingt", "es", "weit", ",", "er", "m\u00e4ht", "und", "m\u00e4ht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "$,", "PPER", "ADJD", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und Etzel's Schwert, es schwelgt und trinkt,", "tokens": ["Und", "Et\u00b7zel's", "Schwert", ",", "es", "schwelgt", "und", "trinkt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "NE", "$,", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bis m\u00fcd die Sonne niedergeht", "tokens": ["Bis", "m\u00fcd", "die", "Son\u00b7ne", "nie\u00b7der\u00b7geht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJD", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und hinter rothe Wolken sinkt.", "tokens": ["Und", "hin\u00b7ter", "ro\u00b7the", "Wol\u00b7ken", "sinkt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Als l\u00e4ngst er schon im Mondlicht braust,", "tokens": ["Als", "l\u00e4ngst", "er", "schon", "im", "Mond\u00b7licht", "braust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPER", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wird ihm der Arm vom Schlagen matt.", "tokens": ["Wird", "ihm", "der", "Arm", "vom", "Schla\u00b7gen", "matt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er fr\u00e4gt das Schwert in seiner Faust:", "tokens": ["Er", "fr\u00e4gt", "das", "Schwert", "in", "sei\u00b7ner", "Faust", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u201eschwert Etzel's, bist noch nicht du satt?", "tokens": ["\u201e", "schwert", "Et\u00b7zel's", ",", "bist", "noch", "nicht", "du", "satt", "?"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "NE", "$,", "VAFIN", "ADV", "PTKNEG", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "La\u00df ab! Heut ist genug gethan!\u201c", "tokens": ["La\u00df", "ab", "!", "Heut", "ist", "ge\u00b7nug", "ge\u00b7than", "!", "\u201c"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVIMP", "PTKVZ", "$.", "ADV", "VAFIN", "ADV", "VVPP", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Doch weh, es wei\u00df von keiner Rast,", "tokens": ["Doch", "weh", ",", "es", "wei\u00df", "von", "kei\u00b7ner", "Rast", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "PPER", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es hebt ein neues Morden an", "tokens": ["Es", "hebt", "ein", "neu\u00b7es", "Mor\u00b7den", "an"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und trifft und fri\u00dft was es erfa\u00dft.", "tokens": ["Und", "trifft", "und", "fri\u00dft", "was", "es", "er\u00b7fa\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "PWS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "\u201ela\u00df ab!\u201c Es zuckt in grauser Lust,", "tokens": ["\u201e", "la\u00df", "ab", "!", "\u201c", "Es", "zuckt", "in", "grau\u00b7ser", "Lust", ","], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "PTKVZ", "$.", "$(", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Ritter st\u00fcrzt mit seinem Pferd", "tokens": ["Der", "Rit\u00b7ter", "st\u00fcrzt", "mit", "sei\u00b7nem", "Pferd"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und jubelnd sticht ihn durch die Brust", "tokens": ["Und", "ju\u00b7belnd", "sticht", "ihn", "durch", "die", "Brust"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des Hunnen uners\u00e4ttlich Schwert.", "tokens": ["Des", "Hun\u00b7nen", "un\u00b7er\u00b7s\u00e4tt\u00b7lich", "Schwert", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}