{"textgrid.poem.48214": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "6. Wie man's machen mu\u00df", "genre": "verse", "period": "N.A.", "pub_year": 1858, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zwei- oder dreimal mu\u00dft' er vor's Messer,", "tokens": ["Zwei", "o\u00b7der", "drei\u00b7mal", "mu\u00dft'", "er", "vor's", "Mes\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["TRUNC", "KON", "ADV", "VMFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Dann war er durch und ein Durchschnittsassesser.", "tokens": ["Dann", "war", "er", "durch", "und", "ein", "Durch\u00b7schnitt\u00b7sas\u00b7ses\u00b7ser", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Im \u00fcbrigen war er ein Pfiffikus:", "tokens": ["Im", "\u00fcb\u00b7ri\u00b7gen", "war", "er", "ein", "Pfif\u00b7fi\u00b7kus", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "VAFIN", "PPER", "ART", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "\u00bbeine Spezialit\u00e4t man w\u00e4hlen mu\u00df.\u00ab", "tokens": ["\u00bb", "ei\u00b7ne", "Spe\u00b7zi\u00b7a\u00b7li\u00b7t\u00e4t", "man", "w\u00e4h\u00b7len", "mu\u00df", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "NN", "PIS", "VVINF", "VMFIN", "$.", "$("], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.3": {"line.1": {"text": "Und endlich hat er sich entschieden:", "tokens": ["Und", "end\u00b7lich", "hat", "er", "sich", "ent\u00b7schie\u00b