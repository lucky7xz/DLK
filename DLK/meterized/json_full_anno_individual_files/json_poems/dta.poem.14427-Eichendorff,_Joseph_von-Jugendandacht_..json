{"dta.poem.14427": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Jugendandacht .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Wann Lenzesstrahlen golden niederrinnen,", "tokens": ["Wann", "Len\u00b7zes\u00b7strah\u00b7len", "gol\u00b7den", "nie\u00b7der\u00b7rin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sieht man die Schaaren losgebunden ziehen,", "tokens": ["Sieht", "man", "die", "Schaa\u00b7ren", "los\u00b7ge\u00b7bun\u00b7den", "zie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ART", "NN", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Im Waldrevier, dem neu der Schmuck geliehen,", "tokens": ["Im", "Wald\u00b7re\u00b7vier", ",", "dem", "neu", "der", "Schmuck", "ge\u00b7lie\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "ADJD", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die lust'ge Jagd nach Lieb' und Scherz beginnen.", "tokens": ["Die", "lust'\u00b7ge", "Jagd", "nach", "Lieb'", "und", "Scherz", "be\u00b7gin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}