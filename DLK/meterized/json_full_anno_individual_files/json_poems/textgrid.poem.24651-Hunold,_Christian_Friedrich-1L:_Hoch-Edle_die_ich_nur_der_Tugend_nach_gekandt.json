{"textgrid.poem.24651": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Hoch-Edle/ die ich nur der Tugend nach gekandt/", "genre": "verse", "period": "N.A.", "pub_year": 1701, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hoch-Edle/ die ich nur der Tugend nach gekandt/", "tokens": ["Hoch\u00b7Ed\u00b7le", "/", "die", "ich", "nur", "der", "Tu\u00b7gend", "nach", "ge\u00b7kandt", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "PRELS", "PPER", "ADV", "ART", "NN", "APPR", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die ein so gut Ger\u00fccht/ als Ruth in Salems Auen", "tokens": ["Die", "ein", "so", "gut", "Ge\u00b7r\u00fccht", "/", "als", "Ruth", "in", "Sa\u00b7lems", "Au\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ART", "ADV", "ADJD", "NN", "$(", "KOUS", "NN", "APPR", "NE", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "An Tugend hat gehabt/ die das gelobte Land", "tokens": ["An", "Tu\u00b7gend", "hat", "ge\u00b7habt", "/", "die", "das", "ge\u00b7lob\u00b7te", "Land"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VAFIN", "VAPP", "$(", "PRELS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Auf Erden nicht so wohl/ als wie anitzt kan bauen/", "tokens": ["Auf", "Er\u00b7den", "nicht", "so", "wohl", "/", "als", "wie", "a\u00b7nitzt", "kan", "bau\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKNEG", "ADV", "ADV", "$(", "KOUS", "KOKOM", "ADV", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ihr hat der H\u00f6chste viel vom Leiden auferlegt/", "tokens": ["Ihr", "hat", "der", "H\u00f6chs\u00b7te", "viel", "vom", "Lei\u00b7den", "auf\u00b7er\u00b7legt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "APPRART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und Ihr den Liebes-Ku\u00df der Frommen auch gegeben:", "tokens": ["Und", "Ihr", "den", "Lie\u00b7bes\u00b7Ku\u00df", "der", "From\u00b7men", "auch", "ge\u00b7ge\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "NN", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Allein ihr Glaube ward zu keiner Zeit bewegt;", "tokens": ["Al\u00b7lein", "ihr", "Glau\u00b7be", "ward", "zu", "kei\u00b7ner", "Zeit", "be\u00b7wegt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VAFIN", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Bey diesem allen blieb ihr Hertz am Himmel kleben.", "tokens": ["Bey", "die\u00b7sem", "al\u00b7len", "blieb", "ihr", "Hertz", "am", "Him\u00b7mel", "kle\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "PIS", "VVFIN", "PPOSAT", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Vor edle Seelen ist die Welt ein Jammerthal/", "tokens": ["Vor", "ed\u00b7le", "See\u00b7len", "ist", "die", "Welt", "ein", "Jam\u00b7mer\u00b7thal", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "ART", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und wenn sie gleich den Fu\u00df auf lauter Rosen setzen.", "tokens": ["Und", "wenn", "sie", "gleich", "den", "Fu\u00df", "auf", "lau\u00b7ter", "Ro\u00b7sen", "set\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "ART", "NN", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und diesen giebt der Herr den gr\u00f6sten Gnaden-Strahl/", "tokens": ["Und", "die\u00b7sen", "giebt", "der", "Herr", "den", "gr\u00f6s\u00b7ten", "Gna\u00b7den\u00b7Strahl", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Die an dem Irdischen sich nie zu sehr ergetzen.", "tokens": ["Die", "an", "dem", "Ir\u00b7di\u00b7schen", "sich", "nie", "zu", "sehr", "er\u00b7get\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "PRF", "ADV", "PTKA", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Wer kennt das edle Blut der ", "tokens": ["Wer", "kennt", "das", "ed\u00b7le", "Blut", "der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ART", "ADJA", "NN", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Sie hat? Hochseelige den Ursprung draus genommen/", "tokens": ["Sie", "hat", "?", "Hoch\u00b7see\u00b7li\u00b7ge", "den", "Ur\u00b7sprung", "draus", "ge\u00b7nom\u00b7men", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$.", "NN", "ART", "NN", "PAV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Und zeigte was die Schrift von frommen Assaph spricht:", "tokens": ["Und", "zeig\u00b7te", "was", "die", "Schrift", "von", "from\u00b7men", "As\u00b7sa\u00b7ph", "spricht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PWS", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.16": {"text": "Von edlen Eltern mu\u00df was edles wieder kommen.", "tokens": ["Von", "ed\u00b7len", "El\u00b7tern", "mu\u00df", "was", "ed\u00b7les", "wie\u00b7der", "kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VMFIN", "PWS", "ADJA", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Ein Zweig von sch\u00f6ner Art tr\u00e4gt allzeit gute Frucht:", "tokens": ["Ein", "Zweig", "von", "sch\u00f6\u00b7ner", "Art", "tr\u00e4gt", "all\u00b7zeit", "gu\u00b7te", "Frucht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "So ist Hochseelige ihr Lebens-Baum gewesen", "tokens": ["So", "ist", "Hoch\u00b7see\u00b7li\u00b7ge", "ihr", "Le\u00b7bens\u00b7Baum", "ge\u00b7we\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "NN", "PPOSAT", "NN", "VAPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Ihr Tugend-Wachsthum hat der H\u00f6chste selbst gesucht/", "tokens": ["Ihr", "Tu\u00b7gen\u00b7dWachst\u00b7hum", "hat", "der", "H\u00f6chs\u00b7te", "selbst", "ge\u00b7sucht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Die Frucht des Glaubens war bey Ihnen auserlesen.", "tokens": ["Die", "Frucht", "des", "Glau\u00b7bens", "war", "bey", "Ih\u00b7nen", "au\u00b7ser\u00b7le\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Unm\u00f6glich da\u00df ein Blat in wenig Zeilen bringt/", "tokens": ["Un\u00b7m\u00f6g\u00b7lich", "da\u00df", "ein", "Blat", "in", "we\u00b7nig", "Zei\u00b7len", "bringt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ART", "NN", "APPR", "PIAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Wie rein ihr Christenthum; Es war so wie mich deuchtet/", "tokens": ["Wie", "rein", "ihr", "Chris\u00b7ten\u00b7thum", ";", "Es", "war", "so", "wie", "mich", "deuch\u00b7tet", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPOSAT", "NN", "$.", "PPER", "VAFIN", "ADV", "KOKOM", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Ein Licht/ davon der Glantz in fremde Gr\u00e4ntzen dringt/", "tokens": ["Ein", "Licht", "/", "da\u00b7von", "der", "Glantz", "in", "frem\u00b7de", "Gr\u00e4nt\u00b7zen", "dringt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PAV", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Das vor den Leuten hat nach Christi Wort geleuchtet.", "tokens": ["Das", "vor", "den", "Leu\u00b7ten", "hat", "nach", "Chris\u00b7ti", "Wort", "ge\u00b7leuch\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "VAFIN", "APPR", "NE", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Ein edles Weib bekomt auch einen edlen Mann/", "tokens": ["Ein", "ed\u00b7les", "Weib", "be\u00b7komt", "auch", "ei\u00b7nen", "ed\u00b7len", "Mann", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "So ward an Ihr erf\u00fcllt was Frommen wird verhei\u00dfen.", "tokens": ["So", "ward", "an", "Ihr", "er\u00b7f\u00fcllt", "was", "From\u00b7men", "wird", "ver\u00b7hei\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "PPER", "VVFIN", "PIS", "NN", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Wer/ theurer Heincke/ dich noch wurdig nennen kan/", "tokens": ["Wer", "/", "theu\u00b7rer", "Hein\u00b7cke", "/", "dich", "noch", "wur\u00b7dig", "nen\u00b7nen", "kan", "/"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "$(", "ADJD", "NN", "$(", "PPER", "ADV", "ADJD", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Mu\u00df deine Tugenden und Wissenschafften preisen.", "tokens": ["Mu\u00df", "dei\u00b7ne", "Tu\u00b7gen\u00b7den", "und", "Wis\u00b7sen\u00b7schaff\u00b7ten", "prei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Der Leib vermodert zwar/ doch deiner Ehren-Krantz", "tokens": ["Der", "Leib", "ver\u00b7mo\u00b7dert", "zwar", "/", "doch", "dei\u00b7ner", "Eh\u00b7ren\u00b7Krantz"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "$(", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Frist weder Zeit noch Wurm. Gelehrsamkeit und Gaben/", "tokens": ["Frist", "we\u00b7der", "Zeit", "noch", "Wurm", ".", "Ge\u00b7lehr\u00b7sam\u00b7keit", "und", "Ga\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ADV", "NN", "$.", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Die dich ber\u00fchmt gemacht/ die werden steten Glantz/", "tokens": ["Die", "dich", "be\u00b7r\u00fchmt", "ge\u00b7macht", "/", "die", "wer\u00b7den", "ste\u00b7ten", "Glantz", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "VVPP", "$(", "PDS", "VAFIN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "So/ wie Wohlseelige/ auch Ihre Tugend haben.", "tokens": ["So", "/", "wie", "Wohl\u00b7see\u00b7li\u00b7ge", "/", "auch", "Ih\u00b7re", "Tu\u00b7gend", "ha\u00b7ben", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "KOKOM", "NN", "$(", "ADV", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Bey allem Kummer hat der H\u00f6chste sie geliebt/", "tokens": ["Bey", "al\u00b7lem", "Kum\u00b7mer", "hat", "der", "H\u00f6chs\u00b7te", "sie", "ge\u00b7liebt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "NN", "VAFIN", "ART", "ADJA", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Und in sein Vater-Hertz sie inniglich geschlossen;", "tokens": ["Und", "in", "sein", "Va\u00b7ter\u00b7Hertz", "sie", "in\u00b7nig\u00b7lich", "ge\u00b7schlos\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Davon ihr sch\u00f6nes End ein herrlich Zeichen giebt/", "tokens": ["Da\u00b7von", "ihr", "sch\u00f6\u00b7nes", "End", "ein", "herr\u00b7lich", "Zei\u00b7chen", "giebt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPOSAT", "ADJA", "NN", "ART", "ADJD", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "So wie ihr Leben auch die Proben stets genossen.", "tokens": ["So", "wie", "ihr", "Le\u00b7ben", "auch", "die", "Pro\u00b7ben", "stets", "ge\u00b7nos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "PPOSAT", "NN", "ADV", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Wir schauen ihren Tod mit solchen Augen an/", "tokens": ["Wir", "schau\u00b7en", "ih\u00b7ren", "Tod", "mit", "sol\u00b7chen", "Au\u00b7gen", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "PIAT", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "In welchen/ wenn vorher die Thr\u00e4nen sind gewesen/", "tokens": ["In", "wel\u00b7chen", "/", "wenn", "vor\u00b7her", "die", "Thr\u00e4\u00b7nen", "sind", "ge\u00b7we\u00b7sen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "$(", "KOUS", "ADV", "ART", "NN", "VAFIN", "VAPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Nun Ehrerbietung steht/ woraus ein jeder kan", "tokens": ["Nun", "Ehr\u00b7er\u00b7bie\u00b7tung", "steht", "/", "wo\u00b7raus", "ein", "je\u00b7der", "kan"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "NN", "VVFIN", "$(", "PWAV", "ART", "PIS", "VMFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "So ihre Herrlichkeit/ als ihr Ged\u00e4chtni\u00df lesen.", "tokens": ["So", "ih\u00b7re", "Herr\u00b7lich\u00b7keit", "/", "als", "ihr", "Ge\u00b7d\u00e4cht\u00b7ni\u00df", "le\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "$(", "KOUS", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "So gr\u00fcnt Ihr Ruhm allhier/ und auch ihr edles Blut", "tokens": ["So", "gr\u00fcnt", "Ihr", "Ruhm", "all\u00b7hier", "/", "und", "auch", "ihr", "ed\u00b7les", "Blut"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "ADV", "$(", "KON", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Wir ihrer Eltern Glantz durch gleiche Tugend nehren.", "tokens": ["Wir", "ih\u00b7rer", "El\u00b7tern", "Glantz", "durch", "glei\u00b7che", "Tu