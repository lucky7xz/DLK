{"textgrid.poem.53012": {"metadata": {"author": {"name": "Seume, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: Als ich Euniken lieblich zu k\u00fcssen mich nahte, lachte", "genre": "verse", "period": "N.A.", "pub_year": 1786, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als ich Euniken lieblich zu k\u00fcssen mich nahte, lachte", "tokens": ["Als", "ich", "Eu\u00b7ni\u00b7ken", "lieb\u00b7lich", "zu", "k\u00fcs\u00b7sen", "mich", "nah\u00b7te", ",", "lach\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["KOUS", "PPER", "NN", "ADJD", "PTKZU", "VVINF", "PPER", "VVFIN", "$,", "VVFIN"], "meter": "---+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Sie herzschneidend mich aus und sagte: r\u00fchre mich nicht an!", "tokens": ["Sie", "herz\u00b7schnei\u00b7dend", "mich", "aus", "und", "sag\u00b7te", ":", "r\u00fch\u00b7re", "mich", "nicht", "an", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "PPER", "PTKVZ", "KON", "VVFIN", "$.", "VVFIN", "PPER", "PTKNEG", "PTKVZ", "$."], "meter": "-+---+-+-+-+-+", "measure": "dactylic.init"}, "line.3": {"text": "Willst du Ochsentreiber mich schmatzen? Verwegner ich lernte", "tokens": ["Willst", "du", "Och\u00b7sen\u00b7trei\u00b7ber", "mich", "schmat\u00b7zen", "?", "Ver\u00b7weg\u00b7ner", "ich", "lern\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VMFIN", "PPER", "NN", "PPER", "VVINF", "$.", "NN", "PPER", "VVFIN"], "meter": "+-+-+--+--+--+-", "measure": "hexameter"}, "line.4": {"text": "B\u00e4urische K\u00fcsse nicht, sondern feiner die Lippen zu schmiegen,", "tokens": ["B\u00e4u\u00b7ri\u00b7sche", "K\u00fcs\u00b7se", "nicht", ",", "son\u00b7dern", "fei\u00b7ner", "die", "Lip\u00b7pen", "zu", "schmie\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKNEG", "$,", "KON", "ADJD", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.5": {"text": "Auch im Traume nicht sollst du ber\u00fchren mein niedliches M\u00e4ulchen,", "tokens": ["Auch", "im", "Trau\u00b7me", "nicht", "sollst", "du", "be\u00b7r\u00fch\u00b7ren", "mein", "nied\u00b7li\u00b7ches", "M\u00e4ul\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "PTKNEG", "VMFIN", "PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.6": {"text": "Wie du blickst und sprichst wie grob und widrig dein Scherz ist!", "tokens": ["Wie", "du", "blickst", "und", "sprichst", "wie", "grob", "und", "wid\u00b7rig", "dein", "Scherz", "ist", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "KON", "VVFIN", "KOKOM", "ADJD", "KON", "ADJD", "PPOSAT", "NN", "VAFIN", "$."], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.7": {"text": "Ey, wie fein du redest in zierlich gedrechselten Worten,", "tokens": ["Ey", ",", "wie", "fein", "du", "re\u00b7dest", "in", "zier\u00b7lich", "ge\u00b7drech\u00b7sel\u00b7ten", "Wor\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ADJD", "PPER", "VVFIN", "APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+--+--+--+-", "measure": "hexameter"}, "line.8": {"text": "Z\u00e4rtlich und lieblich ist dir das Kinn, s\u00fc\u00df duftend das Haupthaar;", "tokens": ["Z\u00e4rt\u00b7lich", "und", "lieb\u00b7lich", "ist", "dir", "das", "Kinn", ",", "s\u00fc\u00df", "duf\u00b7tend", "das", "Haupt\u00b7haar", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "VAFIN", "PPER", "ART", "NN", "$,", "ADJD", "VVPP", "ART", "NN", "$."], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.9": {"text": "Gehe du Sudelmaul, geh; sieh wie die H\u00e4nde dir schwarz sind!", "tokens": ["Ge\u00b7he", "du", "Su\u00b7del\u00b7maul", ",", "geh", ";", "sieh", "wie", "die", "H\u00e4n\u00b7de", "dir", "schwarz", "sind", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NE", "$,", "VVFIN", "$.", "VVFIN", "KOKOM", "ART", "NN", "PPER", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Wie abscheulich du riechst! Geh, du beschmutzest mir alles.", "tokens": ["Wie", "ab\u00b7scheu\u00b7lich", "du", "riechst", "!", "Geh", ",", "du", "be\u00b7schmut\u00b7zest", "mir", "al\u00b7les", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "VVFIN", "$.", "NE", "$,", "PPER", "VVFIN", "PPER", "PIS", "$."], "meter": "-+-+--+--+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Also sprach sie und bitter, und spie in den Busen sich drey Mahl,", "tokens": ["Al\u00b7so", "sprach", "sie", "und", "bit\u00b7ter", ",", "und", "spie", "in", "den", "Bu\u00b7sen", "sich", "drey", "Mahl", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KON", "ADJD", "$,", "KON", "VVFIN", "APPR", "ART", "NN", "PRF", "CARD", "NN", "$,"], "meter": "+-+--+--+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Sahe best\u00e4ndig mich an vom Kopf herab zu den F\u00fc\u00dfen,", "tokens": ["Sa\u00b7he", "be\u00b7st\u00e4n\u00b7dig", "mich", "an", "vom", "Kopf", "her\u00b7ab", "zu", "den", "F\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PRF", "APPR", "APPRART", "NN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.3": {"text": "Und verzog die Lippen, und blickte ver\u00e4chtlich und schief an.", "tokens": ["Und", "ver\u00b7zog", "die", "Lip\u00b7pen", ",", "und", "blick\u00b7te", "ver\u00b7\u00e4cht\u00b7lich", "und", "schief", "an", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "KON", "VVFIN", "ADJD", "KON", "VVFIN", "PTKVZ", "$."], "meter": "--+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Stolz auf die feine zarte Gestalt verlachte sie schn\u00f6de", "tokens": ["Stolz", "auf", "die", "fei\u00b7ne", "zar\u00b7te", "Ge\u00b7stalt", "ver\u00b7lach\u00b7te", "sie", "schn\u00f6\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "ADJA", "NN", "VVFIN", "PPER", "VVFIN"], "meter": "---+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Mich und spottend, es schwoll mir aber blutend das Herzblut,", "tokens": ["Mich", "und", "spot\u00b7tend", ",", "es", "schwoll", "mir", "a\u00b7ber", "blu\u00b7tend", "das", "Herz\u00b7blut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KON", "ADJD", "$,", "PPER", "ADJD", "PPER", "ADV", "ADJD", "ART", "NN", "$,"], "meter": "+-+--+-+-+--++", "measure": "trochaic.septa.relaxed"}, "line.6": {"text": "Und vor Schmerz war die Farbe wie der thauigen Rose.", "tokens": ["Und", "vor", "Schmerz", "war", "die", "Far\u00b7be", "wie", "der", "thau\u00b7i\u00b7gen", "Ro\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VAFIN", "ART", "NN", "KOKOM", "ART", "ADJA", "NN", "$."], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.7": {"text": "Sie verlie\u00df mich und ging, mich griff herzbrennender Zorn tief,", "tokens": ["Sie", "ver\u00b7lie\u00df", "mich", "und", "ging", ",", "mich", "griff", "herz\u00b7bren\u00b7nen\u00b7der", "Zorn", "tief", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "KON", "VVFIN", "$,", "PPER", "VVFIN", "ADJA", "NN", "ADJD", "$,"], "meter": "--+--+-+-+-+-+", "measure": "anapaest.di.plus"}, "line.8": {"text": "Da\u00df das M\u00e4dchen mich lieblichen Burschen so b\u00f6slich verscheuchte.", "tokens": ["Da\u00df", "das", "M\u00e4d\u00b7chen", "mich", "lieb\u00b7li\u00b7chen", "Bur\u00b7schen", "so", "b\u00f6s\u00b7lich", "ver\u00b7scheuch\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "ADJA", "NN", "ADV", "ADJD", "VVFIN", "$."], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.9": {"text": "Sagt mir, ihr Hirten, die Wahrheit, sagt mir ob ich nicht sch\u00f6n bin?", "tokens": ["Sagt", "mir", ",", "ihr", "Hir\u00b7ten", ",", "die", "Wahr\u00b7heit", ",", "sagt", "mir", "ob", "ich", "nicht", "sch\u00f6n", "bin", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPOSAT", "NN", "$,", "ART", "NN", "$,", "VVFIN", "PPER", "KOUS", "PPER", "PTKNEG", "ADJD", "VAFIN", "$."], "meter": "-+-+--+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.10": {"text": "Oder hat pl\u00f6tzlich ein neidischer Gott die Gestalt mir zerst\u00f6ret?", "tokens": ["O\u00b7der", "hat", "pl\u00f6tz\u00b7lich", "ein", "nei\u00b7di\u00b7scher", "Gott", "die", "Ge\u00b7stalt", "mir", "zer\u00b7st\u00f6\u00b7ret", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADJD", "ART", "ADJA", "NN", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.11": {"text": "K\u00fcrzlich bl\u00fchte doch noch mir etwas liebliche Sch\u00f6nheit,", "tokens": ["K\u00fcrz\u00b7lich", "bl\u00fch\u00b7te", "doch", "noch", "mir", "et\u00b7was", "lieb\u00b7li\u00b7che", "Sch\u00f6n\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ADV", "ADV", "PPER", "ADV", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.12": {"text": "Und wie der Epheu am Baumstamm kr\u00e4uselte mir sich das Milchhaar,", "tokens": ["Und", "wie", "der", "E\u00b7pheu", "am", "Baum\u00b7stamm", "kr\u00e4u\u00b7sel\u00b7te", "mir", "sich", "das", "Milch\u00b7haar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "APPRART", "NN", "VVFIN", "PPER", "PRF", "ART", "NN", "$,"], "meter": "-+-+--+-+-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.13": {"text": "Locken waren wie Kresse mir um die Schl\u00e4fe gegossen,", "tokens": ["Lo\u00b7cken", "wa\u00b7ren", "wie", "Kres\u00b7se", "mir", "um", "die", "Schl\u00e4\u00b7fe", "ge\u00b7gos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "KOKOM", "NN", "PPER", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.14": {"text": "Und die Stirne gl\u00e4nzte mir wei\u00df auf schw\u00e4rzlichen Braunen.", "tokens": ["Und", "die", "Stir\u00b7ne", "gl\u00e4nz\u00b7te", "mir", "wei\u00df", "auf", "schw\u00e4rz\u00b7li\u00b7chen", "Brau\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "--+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.15": {"text": "Freundlicher war mein Aug' als das Auge der goldnen Athene,", "tokens": ["Freund\u00b7li\u00b7cher", "war", "mein", "Aug'", "als", "das", "Au\u00b7ge", "der", "gold\u00b7nen", "A\u00b7the\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "KOKOM", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}, "line.16": {"text": "Z\u00e4rter als frischer K\u00e4se war mir der Mund, von dem Munde", "tokens": ["Z\u00e4r\u00b7ter", "als", "fri\u00b7scher", "K\u00e4\u00b7se", "war", "mir", "der", "Mund", ",", "von", "dem", "Mun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "KOUS", "ADJA", "NN", "VAFIN", "PPER", "ART", "NN", "$,", "APPR", "ART", "NN"], "meter": "+--+-+-+--+--+-", "measure": "hexameter"}, "line.17": {"text": "Flo\u00df mir s\u00fc\u00dfer als Honigscheiben die liebliche Rede.", "tokens": ["Flo\u00df", "mir", "s\u00fc\u00b7\u00dfer", "als", "Ho\u00b7nig\u00b7schei\u00b7ben", "die", "lieb\u00b7li\u00b7che", "Re\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "KOKOM", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.18": {"text": "Zauberisch ist mein Gesang, wenn ich das Haberrohr spiele,", "tokens": ["Zau\u00b7be\u00b7risch", "ist", "mein", "Ge\u00b7sang", ",", "wenn", "ich", "das", "Ha\u00b7berr\u00b7ohr", "spie\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPOSAT", "NN", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "+---+-+-+-+--+-", "measure": "trochaic.hexa.relaxed"}, "line.19": {"text": "Wenn ich die Fl\u00f6te sprech' und die Pfeif' und das hallende Zwerchrohr;", "tokens": ["Wenn", "ich", "die", "Fl\u00f6\u00b7te", "sprech'", "und", "die", "Pfeif'", "und", "das", "hal\u00b7len\u00b7de", "Zwer\u00b7chrohr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "KON", "ART", "NN", "KON", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.20": {"text": "Und auf der Bergtrift gelt' ich f\u00fcr sch\u00f6n bey unseren Weibern:", "tokens": ["Und", "auf", "der", "Berg\u00b7trift", "gelt'", "ich", "f\u00fcr", "sch\u00f6n", "bey", "un\u00b7se\u00b7ren", "Wei\u00b7bern", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PPER", "APPR", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.21": {"text": "Alle k\u00fcssen mich oft, und nur die St\u00e4dterinn str\u00e4ubt sich.", "tokens": ["Al\u00b7le", "k\u00fcs\u00b7sen", "mich", "oft", ",", "und", "nur", "die", "St\u00e4d\u00b7te\u00b7rinn", "str\u00e4ubt", "sich", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADV", "$,", "KON", "ADV", "ART", "NN", "VVFIN", "PRF", "$."], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.22": {"text": "Weil ich ein Hirt bin lief sie davon, und wei\u00df denn die Stolze", "tokens": ["Weil", "ich", "ein", "Hirt", "bin", "lief", "sie", "da\u00b7von", ",", "und", "wei\u00df", "denn", "die", "Stol\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "VAFIN", "VVFIN", "PPER", "PAV", "$,", "KON", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.23": {"text": "Nicht, da\u00df Bachus, der Sch\u00f6ne, die K\u00fche trieb in dem Thale?", "tokens": ["Nicht", ",", "da\u00df", "Ba\u00b7chus", ",", "der", "Sch\u00f6\u00b7ne", ",", "die", "K\u00fc\u00b7he", "trieb", "in", "dem", "Tha\u00b7le", "?"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "$,", "KOUS", "NE", "$,", "ART", "NN", "$,", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.24": {"text": "Nicht wie Kypris entbrannte? War nicht ihr Liebling ein Kuhhirt?", "tokens": ["Nicht", "wie", "Kyp\u00b7ris", "ent\u00b7brann\u00b7te", "?", "War", "nicht", "ihr", "Lieb\u00b7ling", "ein", "Kuh\u00b7hirt", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "KOKOM", "NE", "VVFIN", "$.", "VAFIN", "PTKNEG", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.25": {"text": "Weidete nicht die G\u00f6ttinn auf Phrygiens Bergen und k\u00fc\u00dfte", "tokens": ["Wei\u00b7de\u00b7te", "nicht", "die", "G\u00f6t\u00b7tinn", "auf", "Phry\u00b7gi\u00b7ens", "Ber\u00b7gen", "und", "k\u00fc\u00df\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKNEG", "ART", "NN", "APPR", "NE", "NN", "KON", "VVFIN"], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}, "line.26": {"text": "Ihren Adonis im Hain, und klagte Adonis im Haine?", "tokens": ["Ih\u00b7ren", "A\u00b7do\u00b7nis", "im", "Hain", ",", "und", "klag\u00b7te", "A\u00b7do\u00b7nis", "im", "Hai\u00b7ne", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "$,", "KON", "VVFIN", "NE", "APPRART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.27": {"text": "Auch Endymion war er nicht Kuhhirt? Eitle Selene", "tokens": ["Auch", "En\u00b7dy\u00b7mi\u00b7on", "war", "er", "nicht", "Kuh\u00b7hirt", "?", "Eit\u00b7le", "Se\u00b7le\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "NN", "VAFIN", "PPER", "PTKNEG", "NN", "$.", "NN", "NE"], "meter": "-+-+-+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.28": {"text": "Nicht den Hirten zu k\u00fcssen? und kam von dem hohen Olympus", "tokens": ["Nicht", "den", "Hir\u00b7ten", "zu", "k\u00fcs\u00b7sen", "?", "und", "kam", "von", "dem", "ho\u00b7hen", "O\u00b7lym\u00b7pus"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ART", "NN", "PTKZU", "VVINF", "$.", "KON", "VVFIN", "APPR", "ART", "ADJA", "NE"], "meter": "--+--+--+--+-+--", "measure": "anapaest.tetra.plus"}, "line.29": {"text": "Auf die Triften des Latmus daselbst bey dem J\u00fcngling zu ruhen?", "tokens": ["Auf", "die", "Trif\u00b7ten", "des", "Lat\u00b7mus", "da\u00b7selbst", "bey", "dem", "J\u00fcng\u00b7ling", "zu", "ru\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "PAV", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.30": {"text": "Rhea beweinst den Hirten, durchirrtest du, hoher Kronion,", "tokens": ["Rhea", "be\u00b7weinst", "den", "Hir\u00b7ten", ",", "durch\u00b7irr\u00b7test", "du", ",", "ho\u00b7her", "Kro\u00b7ni\u00b7on", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "$,", "VVFIN", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+--+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.31": {"text": "Wegen des Hirtenknaben nicht als Adler das Luftmeer?", "tokens": ["We\u00b7gen", "des", "Hir\u00b7ten\u00b7kna\u00b7ben", "nicht", "als", "Ad\u00b7ler", "das", "Luft\u00b7meer", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKNEG", "KOUS", "NN", "ART", "NN", "$."], "meter": "+--+-+-+-+--+-", "measure": "hexameter"}, "line.32": {"text": "Nur Eunike verweigert dem Hirten das K\u00fc\u00dfchen, und d\u00fcnkt sich", "tokens": ["Nur", "Eu\u00b7ni\u00b7ke", "ver\u00b7wei\u00b7gert", "dem", "Hir\u00b7ten", "das", "K\u00fc\u00df\u00b7chen", ",", "und", "d\u00fcnkt", "sich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "NE", "VVFIN", "ART", "NN", "ART", "NN", "$,", "KON", "VVFIN", "PRF"], "meter": "-----+--+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.33": {"text": "Besser als Kybele, Kypris, nicht in der Stadt, in der Trift nicht", "tokens": ["Bes\u00b7ser", "als", "Ky\u00b7be\u00b7le", ",", "Kyp\u00b7ris", ",", "nicht", "in", "der", "Stadt", ",", "in", "der", "Trift", "nicht"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "KOKOM", "NE", "$,", "NE", "$,", "PTKNEG", "APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "PTKNEG"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.34": {"text": "Auch den Z\u00e4rtling nicht k\u00fcssen, und wenn du auch ewig allein schliefst.", "tokens": ["Auch", "den", "Z\u00e4rt\u00b7ling", "nicht", "k\u00fcs\u00b7sen", ",", "und", "wenn", "du", "auch", "e\u00b7wig", "al\u00b7lein", "schliefst", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PTKNEG", "VVINF", "$,", "KON", "KOUS", "PPER", "ADV", "ADJD", "ADV", "VVFIN", "$."], "meter": "+-+--+-+-+-+-+-+", "measure": "trochaic.octa.plus.relaxed"}}, "stanza.3": {"line.1": {"text": "Als ich Euniken lieblich zu k\u00fcssen mich nahte, lachte", "tokens": ["Als", "ich", "Eu\u00b7ni\u00b7ken", "lieb\u00b7lich", "zu", "k\u00fcs\u00b7sen", "mich", "nah\u00b7te", ",", "lach\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["KOUS", "PPER", "NN", "ADJD", "PTKZU", "VVINF", "PPER", "VVFIN", "$,", "VVFIN"], "meter": "---+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Sie herzschneidend mich aus und sagte: r\u00fchre mich nicht an!", "tokens": ["Sie", "herz\u00b7schnei\u00b7dend", "mich", "aus", "und", "sag\u00b7te", ":", "r\u00fch\u00b7re", "mich", "nicht", "an", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "PPER", "PTKVZ", "KON", "VVFIN", "$.", "VVFIN", "PPER", "PTKNEG", "PTKVZ", "$."], "meter": "-+---+-+-+-+-+", "measure": "dactylic.init"}, "line.3": {"text": "Willst du Ochsentreiber mich schmatzen? Verwegner ich lernte", "tokens": ["Willst", "du", "Och\u00b7sen\u00b7trei\u00b7ber", "mich", "schmat\u00b7zen", "?", "Ver\u00b7weg\u00b7ner", "ich", "lern\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VMFIN", "PPER", "NN", "PPER", "VVINF", "$.", "NN", "PPER", "VVFIN"], "meter": "+-+-+--+--+--+-", "measure": "hexameter"}, "line.4": {"text": "B\u00e4urische K\u00fcsse nicht, sondern feiner die Lippen zu schmiegen,", "tokens": ["B\u00e4u\u00b7ri\u00b7sche", "K\u00fcs\u00b7se", "nicht", ",", "son\u00b7dern", "fei\u00b7ner", "die", "Lip\u00b7pen", "zu", "schmie\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKNEG", "$,", "KON", "ADJD", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.5": {"text": "Auch im Traume nicht sollst du ber\u00fchren mein niedliches M\u00e4ulchen,", "tokens": ["Auch", "im", "Trau\u00b7me", "nicht", "sollst", "du", "be\u00b7r\u00fch\u00b7ren", "mein", "nied\u00b7li\u00b7ches", "M\u00e4ul\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "PTKNEG", "VMFIN", "PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.6": {"text": "Wie du blickst und sprichst wie grob und widrig dein Scherz ist!", "tokens": ["Wie", "du", "blickst", "und", "sprichst", "wie", "grob", "und", "wid\u00b7rig", "dein", "Scherz", "ist", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "KON", "VVFIN", "KOKOM", "ADJD", "KON", "ADJD", "PPOSAT", "NN", "VAFIN", "$."], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.7": {"text": "Ey, wie fein du redest in zierlich gedrechselten Worten,", "tokens": ["Ey", ",", "wie", "fein", "du", "re\u00b7dest", "in", "zier\u00b7lich", "ge\u00b7drech\u00b7sel\u00b7ten", "Wor\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ADJD", "PPER", "VVFIN", "APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+--+--+--+-", "measure": "hexameter"}, "line.8": {"text": "Z\u00e4rtlich und lieblich ist dir das Kinn, s\u00fc\u00df duftend das Haupthaar;", "tokens": ["Z\u00e4rt\u00b7lich", "und", "lieb\u00b7lich", "ist", "dir", "das", "Kinn", ",", "s\u00fc\