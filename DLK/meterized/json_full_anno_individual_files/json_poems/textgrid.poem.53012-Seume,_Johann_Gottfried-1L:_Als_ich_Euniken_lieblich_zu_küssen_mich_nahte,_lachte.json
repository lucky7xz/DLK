sen", ",", "und", "wenn", "du", "auch", "e\u00b7wig", "al\u00b7lein", "schliefst", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PTKNEG", "VVINF", "$,", "KON", "KOUS", "PPER", "ADV", "ADJD", "ADV", "VVFIN", "$."], "meter": "+-+--+-+-+-+-+-+", "measure": "trochaic.octa.plus.relaxed"}}}}}