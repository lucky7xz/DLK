ct"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Bald m\u00e4chtiger, bald leise,", "tokens": ["Bald", "m\u00e4ch\u00b7ti\u00b7ger", ",", "bald", "lei\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJA", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit wunderbarem Klang,", "tokens": ["Mit", "wun\u00b7der\u00b7ba\u00b7rem", "Klang", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zogst du Gesangeskreise,", "tokens": ["Zogst", "du", "Ge\u00b7san\u00b7ges\u00b7krei\u00b7se", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$,"], "meter": "++-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df eine tiefe Weise", "tokens": ["Da\u00df", "ei\u00b7ne", "tie\u00b7fe", "Wei\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Das wilde Meer bezwang.", "tokens": ["Das", "wil\u00b7de", "Meer", "be\u00b7zwang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Und Sturm und Nacht verzogen,", "tokens": ["Und", "Sturm", "und", "Nacht", "ver\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Schon blitzt' es hier und da,", "tokens": ["Schon", "blitzt'", "es", "hier", "und", "da", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "KON", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das Land stieg aus den Wogen,", "tokens": ["Das", "Land", "stieg", "aus", "den", "Wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und unter dem Friedensbogen", "tokens": ["Und", "un\u00b7ter", "dem", "Frie\u00b7dens\u00b7bo\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Die alte Victoria. \u2013", "tokens": ["Die", "al\u00b7te", "Vic\u00b7to\u00b7ria", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NE", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.16": {"line.1": {"text": "Fahr wohl! wie Adlerschwingen", "tokens": ["Fahr", "wohl", "!", "wie", "Ad\u00b7ler\u00b7schwin\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADV", "ADV", "$.", "PWAV", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wird in der Zeiten Schwung", "tokens": ["Wird", "in", "der", "Zei\u00b7ten", "Schwung"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dein Ringen und dein Singen", "tokens": ["Dein", "Rin\u00b7gen", "und", "dein", "Sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Durch deutsche Herzen klingen,", "tokens": ["Durch", "deut\u00b7sche", "Her\u00b7zen", "klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "So bleibst du ewig jung!", "tokens": ["So", "bleibst", "du", "e\u00b7wig", "jung", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}