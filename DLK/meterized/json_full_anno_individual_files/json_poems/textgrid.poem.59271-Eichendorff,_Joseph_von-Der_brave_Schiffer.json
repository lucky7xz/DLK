punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "Der leuchtet aus der Nacht.", "tokens": ["Der", "leuch\u00b7tet", "aus", "der", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und ob sie Nebel decken,", "tokens": ["Und", "ob", "sie", "Ne\u00b7bel", "de\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was gro\u00df und heilig war,", "tokens": ["Was", "gro\u00df", "und", "hei\u00b7lig", "war", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "KON", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Lenkten da aus den Schrecken", "tokens": ["Lenk\u00b7ten", "da", "aus", "den", "Schre\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "APPR", "ART", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Gewaltig die treuen Recken \u2013", "tokens": ["Ge\u00b7wal\u00b7tig", "die", "treu\u00b7en", "Re\u00b7cken", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "ADJA", "NN", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Du mitten in dieser Schar.", "tokens": ["Du", "mit\u00b7ten", "in", "die\u00b7ser", "Schar", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "PDAT", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Da sah man wohl den schlanken", "tokens": ["Da", "sah", "man", "wohl", "den", "schlan\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "ART", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wald k\u00fchner Masten sich", "tokens": ["Wald", "k\u00fch\u00b7ner", "Mas\u00b7ten", "sich"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADJA", "NN", "PRF"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Zum Himmel pfeilernd ranken!", "tokens": ["Zum", "Him\u00b7mel", "pfei\u00b7lernd", "ran\u00b7ken", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Du lehntest voll Gedanken", "tokens": ["Du", "lehn\u00b7test", "voll", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Auf deine Harfe dich.", "tokens": ["Auf", "dei\u00b7ne", "Har\u00b7fe", "dich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Bald m\u00e4chtiger, bald leise,", "tokens": ["Bald", "m\u00e4ch\u00b7ti\u00b7ger", ",", "bald", "lei\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJA", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit wunderbarem Klang,", "tokens": ["Mit", "wun\u00b7der\u00b7ba\u00b7rem", "Klang", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zogst du Gesangeskreise,", "tokens": ["Zogst", "du", "Ge\u00b7san\u00b7ges\u00b7krei\u00b7se", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$,"], "meter": "++-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df eine tiefe Weise", "