{"textgrid.poem.26092": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die einfachen Sterne haben sich hoch \u00fcber die B\u00e4ume geschoben.", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die einfachen Sterne haben sich hoch \u00fcber die B\u00e4ume geschoben.", "tokens": ["Die", "ein\u00b7fa\u00b7chen", "Ster\u00b7ne", "ha\u00b7ben", "sich", "hoch", "\u00fc\u00b7ber", "die", "B\u00e4u\u00b7me", "ge\u00b7scho\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PRF", "ADJD", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+--+-+-+-+--+--+-", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Manchen, der nie tags sein Auge vom Boden gehoben,", "tokens": ["Man\u00b7chen", ",", "der", "nie", "tags", "sein", "Au\u00b7ge", "vom", "Bo\u00b7den", "ge\u00b7ho\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PRELS", "ADV", "ADV", "PPOSAT", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "+--+--+--+--+-", "measure": "dactylic.penta"}, "line.3": {"text": "Den machen nachts kopfhoch die blauen Lampen droben,", "tokens": ["Den", "ma\u00b7chen", "nachts", "kopf\u00b7hoch", "die", "blau\u00b7en", "Lam\u00b7pen", "dro\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "ADV", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die urewig gleichm\u00e4\u00dfig Wandelnden,", "tokens": ["Die", "u\u00b7re\u00b7wig", "gleich\u00b7m\u00e4\u00b7\u00dfig", "Wan\u00b7deln\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJD", "NN", "$,"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.5": {"text": "Die ewig fernen und nie laut Handelnden,", "tokens": ["Die", "e\u00b7wig", "fer\u00b7nen", "und", "nie", "laut", "Han\u00b7deln\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "KON", "ADV", "APPR", "NN", "$,"], "meter": "-+-+--+-+--", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Die Sterngeister, die blauen, der gro\u00dfen Ruhe leuchtende Meister,", "tokens": ["Die", "Stern\u00b7geis\u00b7ter", ",", "die", "blau\u00b7en", ",", "der", "gro\u00b7\u00dfen", "Ru\u00b7he", "leuch\u00b7ten\u00b7de", "Meis\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "$,", "ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-++--+--+-+-+--+-", "measure": "iambic.septa.relaxed"}, "line.7": {"text": "Die dem Weisen Zeichen und Weglicht geben,", "tokens": ["Die", "dem", "Wei\u00b7sen", "Zei\u00b7chen", "und", "Weg\u00b7licht", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "NN", "KON", "NN", "VVINF", "$,"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Die alle Erdenk\u00f6nige samt allen K\u00f6nigreichen \u00fcberleben,", "tokens": ["Die", "al\u00b7le", "Er\u00b7den\u00b7k\u00f6\u00b7ni\u00b7ge", "samt", "al\u00b7len", "K\u00f6\u00b7nig\u00b7rei\u00b7chen", "\u00fc\u00b7berl\u00b7e\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "APPR", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.9": {"text": "Die wie feurige Liebesgedanken \u00fcber den n\u00e4chtlichen D\u00e4chern schweben.", "tokens": ["Die", "wie", "feu\u00b7ri