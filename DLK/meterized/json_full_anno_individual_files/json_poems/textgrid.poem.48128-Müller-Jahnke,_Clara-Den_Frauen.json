{"textgrid.poem.48128": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "Den Frauen", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Den Frauen einen Fr\u00fchlingsgru\u00df!", "tokens": ["Den", "Frau\u00b7en", "ei\u00b7nen", "Fr\u00fch\u00b7lings\u00b7gru\u00df", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Euch allen, die in Fron und M\u00fchen", "tokens": ["Euch", "al\u00b7len", ",", "die", "in", "Fron", "und", "M\u00fc\u00b7hen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "PIS", "$,", "PRELS", "APPR", "NE", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "ihr dornenreiche Pfade geht,", "tokens": ["ihr", "dor\u00b7nen\u00b7rei\u00b7che", "Pfa\u00b7de", "geht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "euch sollen Maienrosen bl\u00fchen!", "tokens": ["euch", "sol\u00b7len", "Mai\u00b7en\u00b7ro\u00b7sen", "bl\u00fc\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Greift lachend in die rote Pracht:", "tokens": ["Greift", "la\u00b7chend", "in", "die", "ro\u00b7te", "Pracht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "ein Morgen gl\u00fcht, den keine Wolke", "tokens": ["ein", "Mor\u00b7gen", "gl\u00fcht", ",", "den", "kei\u00b7ne", "Wol\u00