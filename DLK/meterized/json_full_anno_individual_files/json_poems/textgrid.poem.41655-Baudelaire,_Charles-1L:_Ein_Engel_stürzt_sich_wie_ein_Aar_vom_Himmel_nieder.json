{"textgrid.poem.41655": {"metadata": {"author": {"name": "Baudelaire, Charles", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein Engel st\u00fcrzt sich wie ein Aar vom Himmel nieder", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Engel st\u00fcrzt sich wie ein Aar vom Himmel nieder", "tokens": ["Ein", "En\u00b7gel", "st\u00fcrzt", "sich", "wie", "ein", "Aar", "vom", "Him\u00b7mel", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PRF", "KOKOM", "ART", "NN", "APPRART", "NN", "PTKVZ"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und rauft mit grimmer Faust des Glaubenslosen Haar.", "tokens": ["Und", "rauft", "mit", "grim\u00b7mer", "Faust", "des", "Glau\u00b7bens\u00b7lo\u00b7sen", "Haar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Lern deine Pflicht! ruft er, und schl\u00e4gt ihm Haupt und Glieder,", "tokens": ["Lern", "dei\u00b7ne", "Pflicht", "!", "ruft", "er", ",", "und", "schl\u00e4gt", "ihm", "Haupt", "und", "Glie\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$.", "VVFIN", "PPER", "$,", "KON", "VVFIN", "PPER", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ich will es. Und ich bin dein Engel in Gefahr.", "tokens": ["Ich", "will", "es", ".", "Und", "ich", "bin", "dein", "En\u00b7gel", "in", "Ge\u00b7fahr", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "$.", "KON", "PPER", "VAFIN", "PPOSAT", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Denn lieben sollst du und sollst nicht dein Herz versperren", "tokens": ["Denn", "lie\u00b7ben", "sollst", "du", "und", "sollst", "nicht", "dein", "Herz", "ver\u00b7sper\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVINF", "VMFIN", "PPER", "KON", "VMFIN", "PTKNEG", "PPOSAT", "NN", "VVINF"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "All dem, was h\u00e4\u00dflich, siech, verirrt auf b\u00f6sem Pfad,", "tokens": ["All", "dem", ",", "was", "h\u00e4\u00df\u00b7lich", ",", "siech", ",", "ver\u00b7irrt", "auf", "b\u00f6\u00b7sem", "Pfad", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "$,", "PRELS", "ADJD", "$,", "PPER", "$,", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da\u00df einen Teppich du bei seinem Nahn dem Herren", "tokens": ["Da\u00df", "ei\u00b7nen", "Tep\u00b7pich", "du", "bei", "sei\u00b7nem", "Nahn", "dem", "Her\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "PPER", "APPR", "PPOSAT", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Siegjubelnd breiten kannst mit mildbarmherzger Tat.", "tokens": ["Sieg\u00b7ju\u00b7belnd", "brei\u00b7ten", "kannst", "mit", "mild\u00b7barm\u00b7herz\u00b7ger", "Tat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVINF", "VMFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanz