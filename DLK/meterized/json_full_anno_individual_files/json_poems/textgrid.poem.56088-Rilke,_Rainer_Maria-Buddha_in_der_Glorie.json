{"textgrid.poem.56088": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Buddha in der Glorie", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mitte aller Mitten, Kern der Kerne,", "tokens": ["Mit\u00b7te", "al\u00b7ler", "Mit\u00b7ten", ",", "Kern", "der", "Ker\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "$,", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Mandel, die sich einschlie\u00dft und vers\u00fc\u00dft, \u2013", "tokens": ["Man\u00b7del", ",", "die", "sich", "ein\u00b7schlie\u00dft", "und", "ver\u00b7s\u00fc\u00dft", ",", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "$,", "PRELS", "PRF", "VVFIN", "KON", "VVFIN", "$,", "$("], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "dieses Alles bis an alle Sterne", "tokens": ["die\u00b7ses", "Al\u00b7les", "bis", "an", "al\u00b7le", "Ster\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDAT", "PIS", "ADV", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "ist dein Fruchtfleisch: Sei gegr\u00fc\u00dft.", "tokens": ["ist", "dein", "Frucht\u00b7fleisch", ":", "Sei", "ge\u00b7gr\u00fc\u00dft", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "$.", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Sieh, du f\u00fchlst, wie nichts mehr an dir h\u00e4ngt;", "tokens": ["Sieh", ",", "du", "f\u00fchlst", ",", "wie", "nichts", "mehr", "an", "dir", "h\u00e4ngt", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VVFIN", "$,", "PWAV", "PIS", "ADV", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "im Unendlichen ist deine Schale,", "tokens": ["im", "Un\u00b7end\u00b7li\u00b7chen", "ist", "dei\u00b7ne", "Scha\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "und dort steht der starke Saft und dr\u00e4ngt.", "tokens": ["und", "dort", "steht", "der", "star\u00b7ke", "Saft", "und", "dr\u00e4ngt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "ADJA", "NN", "KON", "VVFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und von au\u00dfen hilft ihm ein Gestrahle,", "tokens": ["Und", "von", "au\u00b7\u00dfen", "hilft", "ihm", "ein", "Ge\u00b7strah\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "denn ganz oben werden deine Sonnen", "tokens": ["denn", "ganz", "o\u00b7ben", "wer\u00b7den", "dei\u00b7ne", "Son\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "VAFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "voll und gl\u00fchend umgedreht.", "tokens": ["voll", "und", "gl\u00fc\u00b7hend", "um\u00b7ge\u00b7dreht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch in dir ist schon begonnen,", "tokens": ["Doch", "in", "dir", "ist", "schon", "be\u00b7gon\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VAFIN", "ADV", "VVPP", "$,"], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.4": {"text": "was die Sonnen \u00fcbersteht.", "tokens": ["was", "die", "Son\u00b7nen", "\u00fc\u00b7bers\u00b7teht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Mitte aller Mitten, Kern der Kerne,", "tokens": ["Mit\u00b7te", "al\u00b7ler", "Mit\u00b7ten", ",", "Kern", "der", "Ker\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "$,", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Mandel, die sich einschlie\u00dft und vers\u00fc\u00dft, \u2013", "tokens": ["Man\u00b7del", ",", "die", "sich", "ein\u00b7schlie\u00dft", "und", "ver\u00b7s\u00fc\u00dft", ",", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "$,", "PRELS", "PRF", "VVFIN", "KON", "VVFIN", "$,", "$("], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "dieses Alles bis an alle Sterne", "tokens": ["die\u00b7ses", "Al\u00b7les", "bis", "an", "al\u00b7le", "Ster\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDAT", "PIS", "ADV", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "ist dein Fruchtfleisch: Sei gegr\u00fc\u00dft.", "tokens": ["ist", "dein", "Frucht\u00b7fleisch", ":", "Sei", "ge\u00b7gr\u00fc\u00dft", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "$.", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text":