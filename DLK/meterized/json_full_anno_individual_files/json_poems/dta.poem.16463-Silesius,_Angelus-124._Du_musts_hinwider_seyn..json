{"dta.poem.16463": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "124.  Du musts hinwider seyn.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Gott ist dir worden Mensch/ wirstu nicht wieder Gott/", "tokens": ["Gott", "ist", "dir", "wor\u00b7den", "Mensch", "/", "wirs\u00b7tu", "nicht", "wie\u00b7der", "Gott", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "VAPP", "NN", "$(", "VAFIN", "PTKNEG", "ADV", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So schm\u00e4hstu die Geburt/ und h\u00f6nest seinen Tod.", "tokens": ["So", "schm\u00e4h\u00b7stu", "die", "Ge\u00b7burt", "/", "und", "h\u00f6\u00b7nest", "sei\u00b7nen", "Tod", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$(", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}