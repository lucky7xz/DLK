{"dta.poem.12877": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Als den Thyrsis eine biene ge-  \n stochen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die sch\u00f6ne Doris schlief, als eine biene flog", "tokens": ["Die", "sch\u00f6\u00b7ne", "Do\u00b7ris", "schlief", ",", "als", "ei\u00b7ne", "bie\u00b7ne", "flog"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NE", "VVFIN", "$,", "KOUS", "ART", "ADJA", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und s\u00fc\u00dfen honig-safft aus ihren rosen sog,", "tokens": ["Und", "s\u00fc\u00b7\u00dfen", "ho\u00b7nig\u00b7\u00b7safft", "aus", "ih\u00b7ren", "ro\u00b7sen", "sog", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Darauf in stiller eil sich zu dem Thyrsis machte", "tokens": ["Da\u00b7rauf", "in", "stil\u00b7ler", "eil", "sich", "zu", "dem", "Thyr\u00b7sis", "mach\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "APPR", "ADJA", "NN", "PRF", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und einen bittern stich in seine lippen brachte,", "tokens": ["Und", "ei\u00b7nen", "bit\u00b7tern", "stich", "in", "sei\u00b7ne", "lip\u00b7pen", "brach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "ADJD", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Es ist dem faulen kerl auch recht dadurch geschehn.", "tokens": ["Es", "ist", "dem", "fau\u00b7len", "kerl", "auch", "recht", "da\u00b7durch", "ge\u00b7schehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "ADV", "ADJD", "PAV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Denn warum hat er sich nicht besser vorgesehn.", "tokens": ["Denn", "wa\u00b7rum", "hat", "er", "sich", "nicht", "bes\u00b7ser", "vor\u00b7ge\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "VAFIN", "PPER", "PRF", "PTKNEG", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Er h\u00e4tte, wenn er selbst nur honig haben wollen,", "tokens": ["Er", "h\u00e4t\u00b7te", ",", "wenn", "er", "selbst", "nur", "ho\u00b7nig", "ha\u00b7ben", "wol\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "KOUS", "PPER", "ADV", "ADV", "ADJD", "VAINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Der muntern bien allhier die m\u00fch ersparen sollen.", "tokens": ["Der", "mun\u00b7tern", "bi\u00b7en", "all\u00b7hier", "die", "m\u00fch", "er\u00b7spa\u00b7ren", "sol\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+----+-+-+-", "measure": "unknown.measure.penta"}}}}}