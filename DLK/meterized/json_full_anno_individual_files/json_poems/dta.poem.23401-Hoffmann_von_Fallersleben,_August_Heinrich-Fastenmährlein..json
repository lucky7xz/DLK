{"dta.poem.23401": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Fastenm\u00e4hrlein.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Herr am gr\u00fcnen Donnerstag", "tokens": ["Ein", "Herr", "am", "gr\u00fc\u00b7nen", "Don\u00b7ners\u00b7tag"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "A\u00df Fleisch, denn Fisch bekam ihm schlecht:", "tokens": ["A\u00df", "Fleisch", ",", "denn", "Fisch", "be\u00b7kam", "ihm", "schlecht", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "KON", "NN", "VVFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das ist ein Essen, wie ich's mag!", "tokens": ["Das", "ist", "ein", "Es\u00b7sen", ",", "wie", "ich's", "mag", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "PWAV", "PIS", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sprach er: nun i\u00df auch du, mein guter Knecht!", "tokens": ["Sprach", "er", ":", "nun", "i\u00df", "auch", "du", ",", "mein", "gu\u00b7ter", "Knecht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$.", "ADV", "VVFIN", "ADV", "PPER", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Da sprach zum Herrn der gute Knecht:", "tokens": ["Da", "sprach", "zum", "Herrn", "der", "gu\u00b7te", "Knecht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "O Herr, f\u00fcrwahr, das thu' ich nicht!", "tokens": ["O", "Herr", ",", "f\u00fcr\u00b7wahr", ",", "das", "thu'", "ich", "nicht", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ADV", "$,", "PDS", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "O Herr, es ist f\u00fcrwahr nicht recht,", "tokens": ["O", "Herr", ",", "es", "ist", "f\u00fcr\u00b7wahr", "nicht", "recht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PPER", "VAFIN", "ADV", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df man die strenge Fasten also bricht.", "tokens": ["Da\u00df", "man", "die", "stren\u00b7ge", "Fas\u00b7ten", "al\u00b7so", "bricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "ADJA", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "I\u00df! sprach der Herr, that's Christus nicht?", "tokens": ["I\u00df", "!", "sprach", "der", "Herr", ",", "that's", "Chris\u00b7tus", "nicht", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "ART", "NN", "$,", "VVFIN", "NE", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und was ", "tokens": ["Und", "was"], "token_info": ["word", "word"], "pos": ["KON", "PWS"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Ja, Herr, er that's, doch wi\u00dft ihr nicht \u2014", "tokens": ["Ja", ",", "Herr", ",", "er", "that's", ",", "doch", "wi\u00dft", "ihr", "nicht"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "NN", "$,", "PPER", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "PTKNEG", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es ging ihm auch des andern Tags recht schlecht.", "tokens": ["Es", "ging", "ihm", "auch", "des", "an\u00b7dern", "Tags", "recht", "schlecht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}