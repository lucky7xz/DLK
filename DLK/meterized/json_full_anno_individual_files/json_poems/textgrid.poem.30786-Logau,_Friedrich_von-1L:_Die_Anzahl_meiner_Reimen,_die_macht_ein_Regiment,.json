{"textgrid.poem.30786": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Anzahl meiner Reimen, die macht ein Regiment,", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Anzahl meiner Reimen, die macht ein Regiment,", "tokens": ["Die", "An\u00b7zahl", "mei\u00b7ner", "Rei\u00b7men", ",", "die", "macht", "ein", "Re\u00b7gi\u00b7ment", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,", "PRELS", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Das weiland auff drey tausent gericht ward und genennt.", "tokens": ["Das", "wei\u00b7land", "auff", "drey", "tau\u00b7sent", "ge\u00b7richt", "ward", "und", "ge\u00b7nennt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "CARD", "NN", "VVPP", "VAFIN", "KON", "VVPP", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Wo dieses wird gemustert, und viel von Mannschafft fehlt,", "tokens": ["Wo", "die\u00b7ses", "wird", "ge\u00b7mus\u00b7tert", ",", "und", "viel", "von", "Mann\u00b7schafft", "fehlt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDS", "VAFIN", "VVPP", "$,", "KON", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "So bleibts vielleicht bey deme, wie man sie ietzo zehlt.", "tokens": ["So", "bleibts", "viel\u00b7leicht", "bey", "de\u00b7me", ",", "wie", "man", "sie", "iet\u00b7zo", "zehlt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "APPR", "ART", "$,", "PWAV", "PIS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "So aber so sie t\u00fcgen, das wenig abegeh,", "tokens": ["So", "a\u00b7ber", "so", "sie", "t\u00fc\u00b7gen", ",", "das", "we\u00b7nig", "a\u00b7be\u00b7geh", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "PPER", "VVINF", "$,", "PRELS", "PIS", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "So kan vielleicht ich richten ins k\u00fcnfftig ein Arme.", "tokens": ["So", "kan", "viel\u00b7leicht", "ich", "rich\u00b7ten", "ins", "k\u00fcnff\u00b7tig", "ein", "Ar\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ADV", "PPER", "VVFIN", "APPRART", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Die Anzahl meiner Reimen, die macht ein Regiment,", "tokens": ["Die", "An\u00b7zahl", "mei\u00b7ner", "Rei\u00b7men", ",", "die", "macht", "ein", "Re\u00b7gi\u00b7ment", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,", "PRELS", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Das weiland auff drey tausent gericht ward und genennt.", "tokens": ["Das", "wei\u00b7land", "auff", "drey", "tau\u00b7sent", "ge\u00b7richt", "ward", "und", "ge\u00b7nennt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "CARD", "NN", "VVPP", "VAFIN", "KON", "VVPP", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Wo dieses wird gemustert, und viel von Mannschaff