{"textgrid.poem.39237": {"metadata": {"author": {"name": "Strachwitz, Moritz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich bring' Euch wieder ein altes Lied", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich bring' Euch wieder ein altes Lied", "tokens": ["Ich", "bring'", "Euch", "wie\u00b7der", "ein", "al\u00b7tes", "Lied"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Von schwerem Liebesleid:", "tokens": ["Von", "schwe\u00b7rem", "Lie\u00b7be\u00b7sleid", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es liebte der D\u00e4ne Walafried", "tokens": ["Es", "lieb\u00b7te", "der", "D\u00e4\u00b7ne", "Wa\u00b7la\u00b7fried"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Eine Norwegs-Fischermaid,", "tokens": ["Ei\u00b7ne", "Nor\u00b7wegs\u00b7Fi\u00b7scher\u00b7maid", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Am Kreidegeklipp, wo sich b\u00e4umt die Flut", "tokens": ["Am", "Krei\u00b7de\u00b7ge\u00b7klipp", ",", "wo", "sich", "b\u00e4umt", "die", "Flut"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "PWAV", "PRF", "VVFIN", "ART", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "In sch\u00e4umender Ungeduld,", "tokens": ["In", "sch\u00e4u\u00b7men\u00b7der", "Un\u00b7ge\u00b7duld", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Da k\u00fc\u00dft' er sie oft mit falschem Mut", "tokens": ["Da", "k\u00fc\u00dft'", "er", "sie", "oft", "mit", "fal\u00b7schem", "Mut"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Und schwur ihr ewige Huld.", "tokens": ["Und", "schwur", "ihr", "e\u00b7wi\u00b7ge", "Huld", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Er schwur bei seines Schwertes Griff,", "tokens": ["Er", "schwur", "bei", "sei\u00b7nes", "Schwer\u00b7tes", "Griff", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bei seines Mantels Kreuz,", "tokens": ["Bei", "sei\u00b7nes", "Man\u00b7tels", "Kreuz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bei dem Sturm, der die heulende See durchpfiff,", "tokens": ["Bei", "dem", "Sturm", ",", "der", "die", "heu\u00b7len\u00b7de", "See", "durch\u00b7pfiff", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Bei der Dirne eigenem Reiz.", "tokens": ["Bei", "der", "Dir\u00b7ne", "ei\u00b7ge\u00b7nem", "Reiz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Er schwur ihr bei dem heiligen Meer,", "tokens": ["Er", "schwur", "ihr", "bei", "dem", "hei\u00b7li\u00b7gen", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Bei seines Vaters Bart,", "tokens": ["Bei", "sei\u00b7nes", "Va\u00b7ters", "Bart", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Bei Rittertreu' und Ritterehr'", "tokens": ["Bei", "Rit\u00b7ter\u00b7treu'", "und", "Rit\u00b7ter\u00b7ehr'"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NE", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Nach falscher Ritter Art:", "tokens": ["Nach", "fal\u00b7scher", "Rit\u00b7ter", "Art", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "\u00bbeh' schlinge mich ein der Woge Wut,", "tokens": ["\u00bb", "eh'", "schlin\u00b7ge", "mich", "ein", "der", "Wo\u00b7ge", "Wut", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "ART", "ART", "NN", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Eh' meine Treu zertaut!\u00ab", "tokens": ["Eh'", "mei\u00b7ne", "Treu", "zer\u00b7taut", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es h\u00f6rte den Schwur die Meeresflut,", "tokens": ["Es", "h\u00f6r\u00b7te", "den", "Schwur", "die", "Mee\u00b7res\u00b7flut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sie br\u00fcllte wild und laut.", "tokens": ["Sie", "br\u00fcll\u00b7te", "wild", "und", "laut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der Fant die Maid in die Arme schlo\u00df,", "tokens": ["Der", "Fant", "die", "Maid", "in", "die", "Ar\u00b7me", "schlo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Fort ritt er mit leichtem Sinn,", "tokens": ["Fort", "ritt", "er", "mit", "leich\u00b7tem", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Er ritt hinan auf das Felsenschlo\u00df,", "tokens": ["Er", "ritt", "hi\u00b7nan", "auf", "das", "Fel\u00b7sen\u00b7schlo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Zu der jungen K\u00f6nigin.", "tokens": ["Zu", "der", "jun\u00b7gen", "K\u00f6\u00b7ni\u00b7gin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Es ruhe mein Lied an dieser Stell',", "tokens": ["Es", "ru\u00b7he", "mein", "Lied", "an", "die\u00b7ser", "Stell'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "PDAT", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die doch ein jeder wei\u00df.", "tokens": ["Die", "doch", "ein", "je\u00b7der", "wei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "PIS", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Markgraf war ein junger Gesell,", "tokens": ["Der", "Mark\u00b7graf", "war", "ein", "jun\u00b7ger", "Ge\u00b7sell", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Der K\u00f6nig war ein Greis! \u2013", "tokens": ["Der", "K\u00f6\u00b7nig", "war", "ein", "Greis", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "\u00bbauf der hohen See, in den Wind hinaus,", "tokens": ["\u00bb", "auf", "der", "ho\u00b7hen", "See", ",", "in", "den", "Wind", "hin\u00b7aus", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "ART", "ADJA", "NN", "$,", "APPR", "ART", "NN", "APZR", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Da liegt mein Schiff zur Wacht;", "tokens": ["Da", "liegt", "mein", "Schiff", "zur", "Wacht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "In J\u00fctland in meines Vaters Haus,", "tokens": ["In", "J\u00fct\u00b7land", "in", "mei\u00b7nes", "Va\u00b7ters", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Da schlafen wir morgen Nacht.\u00ab", "tokens": ["Da", "schla\u00b7fen", "wir", "mor\u00b7gen", "Nacht", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "NN", "$.", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Es senkt auf die Wasser K\u00f6nig Schlaf", "tokens": ["Es", "senkt", "auf", "die", "Was\u00b7ser", "K\u00f6\u00b7nig", "Schlaf"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "NN", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sein Zepter schwer und matt,", "tokens": ["Sein", "Zep\u00b7ter", "schwer", "und", "matt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit der F\u00fcrstin f\u00e4hrt der D\u00e4nengraf", "tokens": ["Mit", "der", "F\u00fcrs\u00b7tin", "f\u00e4hrt", "der", "D\u00e4\u00b7nen\u00b7graf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "In das brausende Kattegat.", "tokens": ["In", "das", "brau\u00b7sen\u00b7de", "Kat\u00b7te\u00b7gat", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.5": {"text": "Eine Fischerdirn' mit braunem Gesicht,", "tokens": ["Ei\u00b7ne", "Fi\u00b7scherdirn'", "mit", "brau\u00b7nem", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Die rudert den Kahn mit Macht;", "tokens": ["Die", "ru\u00b7dert", "den", "Kahn", "mit", "Macht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Der falsche Ritter kennt sie nicht,", "tokens": ["Der", "fal\u00b7sche", "Rit\u00b7ter", "kennt", "sie", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Zu finster ist die Nacht.", "tokens": ["Zu", "fins\u00b7ter", "ist", "die", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Sie sieht nicht auf ihn, nicht auf die Dam',", "tokens": ["Sie", "sieht", "nicht", "auf", "ihn", ",", "nicht", "auf", "die", "Dam'", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPR", "PPER", "$,", "PTKNEG", "APPR", "ART", "NN", "$,"], "meter": "-+----+-+", "measure": "dactylic.init"}, "line.2": {"text": "Sie rudert f\u00fcr und f\u00fcr,", "tokens": ["Sie", "ru\u00b7dert", "f\u00fcr", "und", "f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "KON", "APPR", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie stiert mit Blicken wundersam", "tokens": ["Sie", "stiert", "mit", "Bli\u00b7cken", "wun\u00b7der\u00b7sam"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auf das Kreidegeklipp vor ihr.", "tokens": ["Auf", "das", "Krei\u00b7de\u00b7ge\u00b7klipp", "vor", "ihr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "PPOSAT", "$."], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.5": {"text": "Und n\u00e4her r\u00fcckt die Felsengestalt,", "tokens": ["Und", "n\u00e4\u00b7her", "r\u00fcckt", "die", "Fel\u00b7sen\u00b7ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Wie ein Norwegs-Gletschergeist.", "tokens": ["Wie", "ein", "Nor\u00b7wegs\u00b7Glet\u00b7scher\u00b7geist", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Des D\u00e4nen Arm mit s\u00fc\u00dfer Gewalt", "tokens": ["Des", "D\u00e4\u00b7nen", "Arm", "mit", "s\u00fc\u00b7\u00dfer", "Ge\u00b7walt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Sein k\u00f6niglich Lieb umkreist:", "tokens": ["Sein", "k\u00f6\u00b7nig\u00b7lich", "Lieb", "um\u00b7kreist", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "\u00bbsei ruhig, mein Lieb, dort liegt mein Schiff,", "tokens": ["\u00bb", "sei", "ru\u00b7hig", ",", "mein", "Lieb", ",", "dort", "liegt", "mein", "Schiff", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "ADJD", "$,", "PPOSAT", "NN", "$,", "ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Sei ruhig, bald ist's getan!\u00ab", "tokens": ["Sei", "ru\u00b7hig", ",", "bald", "ist's", "ge\u00b7tan", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["VAIMP", "ADJD", "$,", "ADV", "VAFIN", "VVPP", "$.", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und n\u00e4her kam das Felsenriff,", "tokens": ["Und", "n\u00e4\u00b7her", "kam", "das", "Fel\u00b7sen\u00b7riff", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und rascher scho\u00df der Kahn.", "tokens": ["Und", "ra\u00b7scher", "scho\u00df", "der", "Kahn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Zwei Ruderschl\u00e4ge mit wilder Eil',", "tokens": ["Zwei", "Ru\u00b7der\u00b7schl\u00e4\u00b7ge", "mit", "wil\u00b7der", "Eil'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Die tat die braune Dirn',", "tokens": ["Die", "tat", "die", "brau\u00b7ne", "Dirn'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Da st\u00fcrmte der Nachen wie ein Pfeil", "tokens": ["Da", "st\u00fcrm\u00b7te", "der", "Na\u00b7chen", "wie", "ein", "Pfeil"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "KOKOM", "ART", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Nach der wei\u00dfen Felsenstirn.", "tokens": ["Nach", "der", "wei\u00b7\u00dfen",