{"dta.poem.14822": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Die verlorene Braut.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Ich wei\u00df nicht, was mir grauset,", "tokens": ["Ich", "wei\u00df", "nicht", ",", "was", "mir", "grau\u00b7set", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Blick' nicht so fremd auf mich!", "tokens": ["Blick'", "nicht", "so", "fremd", "auf", "mich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ADV", "ADJD", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich wollt', ich w\u00e4r' zu Hause,", "tokens": ["Ich", "wollt'", ",", "ich", "w\u00e4r'", "zu", "Hau\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "PPER", "VAFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nach Hause f\u00fchr' ich Dich.\u201c", "tokens": ["Nach", "Hau\u00b7se", "f\u00fchr'", "ich", "Dich", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "PRF", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}