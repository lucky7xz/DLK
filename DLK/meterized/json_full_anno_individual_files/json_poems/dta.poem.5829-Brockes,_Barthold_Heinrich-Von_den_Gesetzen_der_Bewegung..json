{"dta.poem.5829": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Gesetzen der Bewegung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "H\u00f6rt gleich, gehemmt in seinem Lauf,", "tokens": ["H\u00f6rt", "gleich", ",", "ge\u00b7hemmt", "in", "sei\u00b7nem", "Lauf", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$,", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Druck des harten C\u00f6rpers auf,", "tokens": ["Der", "Druck", "des", "har\u00b7ten", "C\u00f6r\u00b7pers", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn ein noch h\u00e4rterer den Gang ihn unterbricht;", "tokens": ["Wenn", "ein", "noch", "h\u00e4r\u00b7te\u00b7rer", "den", "Gang", "ihn", "un\u00b7ter\u00b7bricht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADV", "ADJD", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Verliert er zwar den Strich, doch sein Bewegen nicht.", "tokens": ["Ver\u00b7liert", "er", "zwar", "den", "Strich", ",", "doch", "sein", "Be\u00b7we\u00b7gen", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "$,", "ADV", "PPOSAT", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}