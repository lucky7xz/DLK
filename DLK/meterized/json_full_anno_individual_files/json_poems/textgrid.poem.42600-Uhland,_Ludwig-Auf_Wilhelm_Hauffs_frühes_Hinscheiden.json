{"textgrid.poem.42600": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Auf Wilhelm Hauffs fr\u00fches Hinscheiden", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dem jungen, frischen, farbenhellen Leben,", "tokens": ["Dem", "jun\u00b7gen", ",", "fri\u00b7schen", ",", "far\u00b7ben\u00b7hel\u00b7len", "Le\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dem reichen Fr\u00fchling, dem kein Herbst gegeben,", "tokens": ["Dem", "rei\u00b7chen", "Fr\u00fch\u00b7ling", ",", "dem", "kein", "Herbst", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ihm lasset uns zum Totenopfer zollen", "tokens": ["Ihm", "las\u00b7set", "uns", "zum", "To\u00b7ten\u00b7op\u00b7fer", "zol\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den abgeknickten Zweig \u2013 den bl\u00fctevollen!", "tokens": ["Den", "ab\u00b7ge\u00b7knick\u00b7ten", "Zweig", "\u2013", "den", "bl\u00fc\u00b7te\u00b7vol\u00b7len", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Noch eben war von dieses Fr\u00fchlings Scheine", "tokens": ["Noch", "e\u00b7ben", "war", "von", "die\u00b7ses", "Fr\u00fch\u00b7lings", "Schei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VAFIN", "APPR", "PDAT", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das Vaterland begl\u00e4nzt. \u2013 Auf schroffem Steine,", "tokens": ["Das", "Va\u00b7ter\u00b7land", "be\u00b7gl\u00e4nzt", ".", "\u2013", "Auf", "schrof\u00b7fem", "Stei\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$.", "$(", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dem man die Burg gebrochen, hob sich neu", "tokens": ["Dem", "man", "die", "Burg", "ge\u00b7bro\u00b7chen", ",", "hob", "sich", "neu"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "PIS", "ART", "NN", "VVPP", "$,", "VVFIN", "PRF", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ein Wolkenschlo\u00df, ein zauberhaft Geb\u00e4u.", "tokens": ["Ein", "Wol\u00b7ken\u00b7schlo\u00df", ",", "ein", "zau\u00b7ber\u00b7haft", "Ge\u00b7b\u00e4u", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Doch in der H\u00f6hle, wo die stille Kraft", "tokens": ["Doch", "in", "der", "H\u00f6h\u00b7le", ",", "wo", "die", "stil\u00b7le", "Kraft"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "$,", "PWAV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Des Erdgeists r\u00e4tselhafte Formen schafft:", "tokens": ["Des", "Erd\u00b7geists", "r\u00e4t\u00b7sel\u00b7haf\u00b7te", "For\u00b7men", "schafft", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Am Fackellicht der Phantasie entfaltet,", "tokens": ["Am", "Fa\u00b7ckel\u00b7licht", "der", "Phan\u00b7ta\u00b7sie", "ent\u00b7fal\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sahn wir zu Heldenbildern sie gestaltet;", "tokens": ["Sahn", "wir", "zu", "Hel\u00b7den\u00b7bil\u00b7dern", "sie", "ge\u00b7stal\u00b7tet", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und jeder Hall, in Spalt' und Kluft versteckt,", "tokens": ["Und", "je\u00b7der", "Hall", ",", "in", "Spalt'", "und", "Kluft", "ver\u00b7steckt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$,", "APPR", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ward zu beseeltem Menschenwort erweckt.", "tokens": ["Ward", "zu", "be\u00b7seel\u00b7tem", "Men\u00b7schen\u00b7wort", "er\u00b7weckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Mit Heldenfahrten und mit Festest\u00e4nzen,", "tokens": ["Mit", "Hel\u00b7den\u00b7fahr\u00b7ten", "und", "mit", "Fes\u00b7te\u00b7st\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit Satyrlarven und mit Blumenkr\u00e4nzen", "tokens": ["Mit", "Sa\u00b7tyr\u00b7lar\u00b7ven", "und", "mit", "Blu\