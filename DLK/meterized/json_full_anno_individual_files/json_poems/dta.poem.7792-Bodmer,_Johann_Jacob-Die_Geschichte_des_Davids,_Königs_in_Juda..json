{"dta.poem.7792": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Die Geschichte des Davids,  \n K\u00f6nigs in Juda.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-200905198429", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das Hau\u00df drinn Suri wohnt, ein Wald umher beschlosse,", "tokens": ["Das", "Hau\u00df", "drinn", "Su\u00b7ri", "wohnt", ",", "ein", "Wald", "um\u00b7her", "be\u00b7schlos\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "NE", "VVFIN", "$,", "ART", "NN", "PTKVZ", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es lag auf einer Klipp, von deren sich ergosse,", "tokens": ["Es", "lag", "auf", "ei\u00b7ner", "Klipp", ",", "von", "de\u00b7ren", "sich", "er\u00b7gos\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,", "APPR", "PRELAT", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ein ungest\u00fcmer Bach, der von dem Regen war", "tokens": ["Ein", "un\u00b7ge\u00b7st\u00fc\u00b7mer", "Bach", ",", "der", "von", "dem", "Re\u00b7gen", "war"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VAFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So aufgeschwellt da\u00df er bedrohte mit Gefahr", "tokens": ["So", "auf\u00b7ge\u00b7schwellt", "da\u00df", "er", "be\u00b7droh\u00b7te", "mit", "Ge\u00b7fahr"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "KOUS", "PPER", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Den der sich wollt zu leicht in seine Wellen wagen.", "tokens": ["Den", "der", "sich", "wollt", "zu", "leicht", "in", "sei\u00b7ne", "Wel\u00b7len", "wa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRELS", "PRF", "VMFIN", "PTKA", "ADJD", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Drum wollt der F\u00fchrer nicht durch diesen Bach durchjagen,", "tokens": ["Drum", "wollt", "der", "F\u00fch\u00b7rer", "nicht", "durch", "die\u00b7sen", "Bach", "durch\u00b7ja\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "ART", "NN", "PTKNEG", "APPR", "PDAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Besondern nahm den Weg weit durch den Wald herum,", "tokens": ["Be\u00b7son\u00b7dern", "nahm", "den", "Weg", "weit", "durch", "den", "Wald", "he\u00b7rum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "ADJD", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und kame an den Ort, allwo des L\u00f6wen Grimm", "tokens": ["Und", "ka\u00b7me", "an", "den", "Ort", ",", "all\u00b7wo", "des", "L\u00f6\u00b7wen", "Grimm"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$,", "PWAV", "ART", "NN", "NE"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Von David war gestillt. Die Michal sich erschrecket,", "tokens": ["Von", "Da\u00b7vid", "war", "ge\u00b7stillt", ".", "Die", "Mic\u00b7hal", "sich", "er\u00b7schre\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VAFIN", "VVPP", "$.", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Als sie schaut auf der Erd den L\u00f6wen ausgestrecket.", "tokens": ["Als", "sie", "schaut", "auf", "der", "Erd", "den", "L\u00f6\u00b7wen", "aus\u00b7ge\u00b7stre\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.11": {"text": "Sie meinet da\u00df er leb, drum spricht ihr David zu,", "tokens": ["Sie", "mei\u00b7net", "da\u00df", "er", "leb", ",", "drum", "spricht", "ihr", "Da\u00b7vid", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOUS", "PPER", "VVFIN", "$,", "PAV", "VVFIN", "PPER", "NE", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und stillet ihre Furcht da\u00df sie nimmt wieder Ruh.", "tokens": ["Und", "stil\u00b7let", "ih\u00b7re", "Furcht", "da\u00df", "sie", "nimmt", "wie\u00b7der", "Ruh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "KOUS", "PPER", "VVFIN", "ADV", "NN", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "Er kan die\u00df hohe Werck nicht so gering beschreiben,", "tokens": ["Er", "kan", "die\u00df", "ho\u00b7he", "Werck", "nicht", "so", "ge\u00b7ring", "be\u00b7schrei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PDS", "ADJA", "NN", "PTKNEG", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Da\u00df Michal sollte nicht darob verwundert bleiben,", "tokens": ["Da\u00df", "Mic\u00b7hal", "soll\u00b7te", "nicht", "da\u00b7rob", "ver\u00b7wun\u00b7dert", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "VMFIN", "PTKNEG", "PAV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Die zu ihm sagt: F\u00fcrwahr, der David hat verdient", "tokens": ["Die", "zu", "ihm", "sagt", ":", "F\u00fcr\u00b7wahr", ",", "der", "Da\u00b7vid", "hat", "ver\u00b7dient"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "APPR", "PPER", "VVFIN", "$.", "NN", "$,", "ART", "NE", "VAFIN", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Nach dieser Helden That, da\u00df sein Ruhm ewig gr\u00fcnt.", "tokens": ["Nach", "die\u00b7ser", "Hel\u00b7den", "That", ",", "da\u00df", "sein", "Ruhm", "e\u00b7wig", "gr\u00fcnt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "NN", "$,", "KOUS", "PPOSAT", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Ward sie f\u00fcrhin err\u00f6th, als er ihr Lob ausbreit\u2019te,", "tokens": ["Ward", "sie", "f\u00fcr\u00b7hin", "er\u00b7r\u00f6th", ",", "als", "er", "ihr", "Lob", "aus\u00b7breit'\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "VVFIN", "$,", "KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "So gehet es iezt auch an dieses Helden Seite;", "tokens": ["So", "ge\u00b7het", "es", "iezt", "auch", "an", "die\u00b7ses", "Hel\u00b7den", "Sei\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "APPR", "PDAT", "NN", "NN", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.19": {"text": "Die Wangen feuren an, die Augen bergen sich.", "tokens": ["Die", "Wan\u00b7gen", "feu\u00b7ren", "an", ",", "die", "Au\u00b7gen", "ber\u00b7gen", "sich", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,", "ART", "NN", "VVFIN", "PRF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Er w\u00f6llt sie w\u00fcst es nicht, doch spricht sie emsiglich", "tokens": ["Er", "w\u00f6llt", "sie", "w\u00fcst", "es", "nicht", ",", "doch", "spricht", "sie", "em\u00b7sig\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "VVFIN", "PPER", "PTKNEG", "$,", "ADV", "VVFIN", "PPER", "ADJD"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.21": {"text": "Von dieser grossen That bis sie nach Suri kommen.", "tokens": ["Von", "die\u00b7ser", "gros\u00b7sen", "That", "bis", "sie", "nach", "Su\u00b7ri", "kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "APPR", "PPER", "APPR", "NE", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Allda Zeruja kaum die Freudenpost vernommen,", "tokens": ["All\u00b7da", "Ze\u00b7ru\u00b7ja", "kaum", "die", "Freu\u00b7den\u00b7post", "ver\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.23": {"text": "Da\u00df Michal kommen an, Ahinoam ihr Kind,", "tokens": ["Da\u00df", "Mic\u00b7hal", "kom\u00b7men", "an", ",", "A\u00b7hi\u00b7no\u00b7am", "ihr", "Kind", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "VVFIN", "PTKVZ", "$,", "NE", "PPOSAT", "NN", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.24": {"text": "Die sie allzeit geliebt, sie sich von Freuden findt", "tokens": ["Die", "sie", "all\u00b7zeit", "ge\u00b7liebt", ",", "sie", "sich", "von", "Freu\u00b7den", "findt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADV", "VVPP", "$,", "PPER", "PRF", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "So \u00fcberh\u00e4uft, da\u00df sie nicht wei\u00df was sie beginnet.", "tokens": ["So", "\u00fc\u00b7berh\u00b7\u00e4uft", ",", "da\u00df", "sie", "nicht", "wei\u00df", "was", "sie", "be\u00b7gin\u00b7net", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "KOUS", "PPER", "PTKNEG", "VVFIN", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Sie l\u00e4uft bald hin bald her, indeme die gewinnet", "tokens": ["Sie", "l\u00e4uft", "bald", "hin", "bald", "her", ",", "in\u00b7de\u00b7me", "die", "ge\u00b7win\u00b7net"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ADV", "PTKVZ", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Den hohen Windelsteig und eilet was sie kan,", "tokens": ["Den", "ho\u00b7hen", "Win\u00b7del\u00b7steig", "und", "ei\u00b7let", "was", "sie", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "VVFIN", "PWS", "PPER", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Da\u00df sie Zeruja m\u00f6g im Bett noch treffen an.", "tokens": ["Da\u00df", "sie", "Ze\u00b7ru\u00b7ja", "m\u00f6g", "im", "Bett", "noch", "tref\u00b7fen", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NE", "NE", "APPRART", "NN", "ADV", "VVFIN", "PTKVZ", "$."], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.29": {"text": "Die aber schon ger\u00fcst und sie mit beyden Armen", "tokens": ["Die", "a\u00b7ber", "schon", "ge\u00b7r\u00fcst", "und", "sie", "mit", "bey\u00b7den", "Ar\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ADV", "VVPP", "KON", "PPER", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Umf\u00e4ht und hertzlich k\u00fc\u00dft: Sie spricht, wo dein Erbarmen", "tokens": ["Um\u00b7f\u00e4ht", "und", "hertz\u00b7lich", "k\u00fc\u00dft", ":", "Sie", "spricht", ",", "wo", "dein", "Er\u00b7bar\u00b7men"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VVPP", "KON", "ADJD", "VVFIN", "$.", "PPER", "VVFIN", "$,", "PWAV", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Und deine G\u00fctigkeit, o Michal nicht bey dir,", "tokens": ["Und", "dei\u00b7ne", "G\u00fc\u00b7tig\u00b7keit", ",", "o", "Mic\u00b7hal", "nicht", "bey", "dir", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "FM", "NE", "PTKNEG", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Sch\u00e4mt ich mich da\u00df du mich also solltst finden hier.", "tokens": ["Sch\u00e4mt", "ich", "mich", "da\u00df", "du", "mich", "al\u00b7so", "solltst", "fin\u00b7den", "hier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "KOUS", "PPER", "PRF", "ADV", "ADV", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Ich war mir diese Ehr wohl heute nicht versehen,", "tokens": ["Ich", "war", "mir", "die\u00b7se", "Ehr", "wohl", "heu\u00b7te", "nicht", "ver\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PDAT", "NN", "ADV", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Und wei\u00df in Wahrheit nicht recht wie mir ist geschehen.", "tokens": ["Und", "wei\u00df", "in", "Wahr\u00b7heit", "nicht", "recht", "wie", "mir", "ist", "ge\u00b7sche\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "PTKNEG", "ADJD", "KOKOM", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Des K\u00f6nigs Tochter kommt hie in ein schlechtes Hau\u00df,", "tokens": ["Des", "K\u00f6\u00b7nigs", "Toch\u00b7ter", "kommt", "hie", "in", "ein", "schlech\u00b7tes", "Hau\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Es siehet hie, wie man lebt auf dem Lande, aus.", "tokens": ["Es", "sie\u00b7het", "hie", ",", "wie", "man", "lebt", "auf", "dem", "Lan\u00b7de", ",", "aus", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PWAV", "PIS", "VVFIN", "APPR", "ART", "NN", "$,", "PTKVZ", "$."], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.37": {"text": "Wo ich Zeruja find, die Michal hiezu saget,", "tokens": ["Wo", "ich", "Ze\u00b7ru\u00b7ja", "find", ",", "die", "Mic\u00b7hal", "hie\u00b7zu", "sa\u00b7get", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "NE", "VVFIN", "$,", "ART", "NN", "PAV", "VVFIN", "$,"], "meter": "-+---+-+-+-+-", "measure": "dactylic.init"}, "line.38": {"text": "Da ist es eben recht, und alles mir behaget,", "tokens": ["Da", "ist", "es", "e\u00b7ben", "recht", ",", "und", "al\u00b7les", "mir", "be\u00b7ha\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADJD", "$,", "KON", "PIS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Weil ich von David h\u00f6rt da\u00df dein Hau\u00df also nah,", "tokens": ["Weil", "ich", "von", "Da\u00b7vid", "h\u00f6rt", "da\u00df", "dein", "Hau\u00df", "al\u00b7so", "nah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NE", "VVFIN", "KOUS", "PPOSAT", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.40": {"text": "Konnt ich mich zwingen nicht bevor ich dich hie sah.", "tokens": ["Konnt", "ich", "mich", "zwin\u00b7gen", "nicht", "be\u00b7vor", "ich", "dich", "hie", "sah", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "VVINF", "PTKNEG", "KOUS", "PPER", "PRF", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Nun ist mir aber wohl. Hiemit sie sich umfassen,", "tokens": ["Nun", "ist", "mir", "a\u00b7ber", "wohl", ".", "Hie\u00b7mit", "sie", "sich", "um\u00b7fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "$.", "ADV", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Und David der sie will etwas alleine lassen,", "tokens": ["Und", "Da\u00b7vid", "der", "sie", "will", "et\u00b7was", "al\u00b7lei\u00b7ne", "las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "ART", "PPER", "VMFIN", "PIS", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Bestellet unterde\u00df da\u00df man die Spei\u00df bereit,", "tokens": ["Be\u00b7stel\u00b7let", "un\u00b7ter\u00b7de\u00df", "da\u00df", "man", "die", "Spei\u00df", "be\u00b7reit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "KOUS", "PIS", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Der kleine Joab kommt mit seinen Br\u00fcdern beyd", "tokens": ["Der", "klei\u00b7ne", "Joab", "kommt", "mit", "sei\u00b7nen", "Br\u00fc\u00b7dern", "beyd"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NE", "VVFIN", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.45": {"text": "Um ihren Vetter her und ihn gar fr\u00f6lich gr\u00fcssen.", "tokens": ["Um", "ih\u00b7ren", "Vet\u00b7ter", "her", "und", "ihn", "gar", "fr\u00f6\u00b7lich", "gr\u00fcs\u00b7s