laxed"}, "line.50": {"text": "Die sollen Spei\u00df und Tranck, wie es sich will geb\u00fchren,", "tokens": ["Die", "sol\u00b7len", "Spei\u00df", "und", "Tranck", ",", "wie", "es", "sich", "will", "ge\u00b7b\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "KON", "NN", "$,", "PWAV", "PPER", "PRF", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.51": {"text": "Hintragen nach der H\u00f6l, und Joab bittet sehr,", "tokens": ["Hin\u00b7tra\u00b7gen", "nach", "der", "H\u00f6l", ",", "und", "Joab", "bit\u00b7tet", "sehr", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,", "KON", "NE", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.52": {"text": "Er m\u00f6g ihn nehmen mit, er wollte auch die Ehr", "tokens": ["Er", "m\u00f6g", "ihn", "neh\u00b7men", "mit", ",", "er", "woll\u00b7te", "auch", "die", "Ehr"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "VVFIN", "PTKVZ", "$,", "PPER", "VMFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.53": {"text": "Geniessen da\u00df er k\u00f6nnt des K\u00f6nigs T\u00f6chter sehen.", "tokens": ["Ge\u00b7nies\u00b7sen", "da\u00df", "er", "k\u00f6nnt", "des", "K\u00f6\u00b7nigs", "T\u00f6ch\u00b7ter", "se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOUS", "PPER", "VVFIN", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.54": {"text": "Der David sagt ihm zu, er sollte mit hingehen,", "tokens": ["Der", "Da\u00b7vid", "sagt", "ihm", "zu", ",", "er", "soll\u00b7te", "mit", "hin\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "PPER", "PTKVZ", "$,", "PPER", "VMFIN", "APPR", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.55": {"text": "Darauf schm\u00fcckt er sich aus, weil schon in seinem Blut", "tokens": ["Da\u00b7rauf", "schm\u00fcckt", "er", "sich", "aus", ",", "weil", "schon", "in", "sei\u00b7nem", "Blut"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "PRF", "PTKVZ", "$,", "KOUS", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+---+-+-+-+", "measure": "dactylic.init"}, "line.56": {"text": "Sich zeigt ein stoltzer Sinn und ein erhobner Muth.", "tokens": ["Sich", "zeigt", "ein", "stolt\u00b7zer", "Sinn", "und", "ein", "er\u00b7hob\u00b7ner", "Muth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "ART", "ADJA", "NN", "KON", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}