{"textgrid.poem.39695": {"metadata": {"author": {"name": "G\u00fcnderode, Karoline von", "birth": "N.A.", "death": "N.A."}, "title": "Eine persische Erz\u00e4hlung", "genre": "verse", "period": "N.A.", "pub_year": 1793, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Rasend am Altar des Feuers", "tokens": ["Ra\u00b7send", "am", "Al\u00b7tar", "des", "Feu\u00b7ers"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPRART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ormuzd Priester war geworden;", "tokens": ["Or\u00b7muzd", "Pries\u00b7ter", "war", "ge\u00b7wor\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VAFIN", "VAPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber als der Morgen helle", "tokens": ["A\u00b7ber", "als", "der", "Mor\u00b7gen", "hel\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "G\u00fclden aus dem Osten blickte,", "tokens": ["G\u00fcl\u00b7den", "aus", "dem", "Os\u00b7ten", "blick\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Kehrte Ruh in seine Seele.", "tokens": ["Kehr\u00b7te", "Ruh", "in", "sei\u00b7ne", "See\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Laut rief er dem Opferknaben:", "tokens": ["Laut", "rief", "er", "dem", "Op\u00b7fer\u00b7kna\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "\u00bbsiehe wie der Morgen pranget.", "tokens": ["\u00bb", "sie\u00b7he", "wie", "der", "Mor\u00b7gen", "pran\u00b7get", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "KOKOM", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Licht hat endlich obgesieget,", "tokens": ["Licht", "hat", "end\u00b7lich", "ob\u00b7ge\u00b7sie\u00b7get", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Siegend werden nie zur Erde", "tokens": ["Sie\u00b7gend", "wer\u00b7den", "nie", "zur", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "ADV", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Wieder sich die Schatten senken.\u00ab", "tokens": ["Wie\u00b7der", "sich", "die", "Schat\u00b7ten", "sen\u00b7ken", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "PRF", "ART", "NN", "VVINF", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Trost erf\u00fcllet sprachs der Alte,", "tokens": ["Trost", "er\u00b7f\u00fcl\u00b7let", "sprachs", "der", "Al\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PIS", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Kniete nieder am Altare,", "tokens": ["Knie\u00b7te", "nie\u00b7der", "am", "Al\u00b7ta\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Betend auf zum Gott de Lichtes", "tokens": ["Be\u00b7tend", "auf", "zum", "Gott", "de", "Lich\u00b7tes"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "APPR", "APPRART", "NN", "NE", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Prei\u00dfend ihn, des frohen Sieges,", "tokens": ["Prei\u00b7\u00dfend", "ihn", ",", "des", "fro\u00b7hen", "Sie\u00b7ges", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Angethan in hellen Kleidern", "tokens": ["An\u00b7ge\u00b7than", "in", "hel\u00b7len", "Klei\u00b7dern"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Zw\u00f6lf der Stunden t\u00e4glich feiern.", "tokens": ["Zw\u00f6lf", "der", "Stun\u00b7den", "t\u00e4g\u00b7lich", "fei\u00b7ern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Aber als die Zw\u00f6lf im Weste", "tokens": ["A\u00b7ber", "als", "die", "Zw\u00f6lf", "im", "Wes\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Tr\u00fcbe sich begunt zu f\u00e4rben,", "tokens": ["Tr\u00fc\u00b7be", "sich", "be\u00b7gunt", "zu", "f\u00e4r\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Leis verglomm im Abendstrahle,", "tokens": ["Leis", "ver\u00b7glomm", "im", "A\u00b7bend\u00b7strah\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Ormuzd Priester ward da stille,", "tokens": ["Or\u00b7muzd", "Pries\u00b7ter", "ward", "da", "stil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VAFIN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Sorgend blickt er auf zum Himmel", "tokens": ["Sor\u00b7gend", "blickt", "er", "auf", "zum", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Forschend was die Zeit gew\u00e4hre. \u2013", "tokens": ["For\u00b7schend", "was", "die", "Zeit", "ge\u00b7w\u00e4h\u00b7re", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVPP", "PRELS", "ART", "NN", "VVFIN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "Dunkel kam heran geschritten,", "tokens": ["Dun\u00b7kel", "kam", "he\u00b7ran", "ge\u00b7schrit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PTKVZ", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Zagend streift es, bla\u00df und \u00e4ngstlich,", "tokens": ["Za\u00b7gend", "streift", "es", ",", "bla\u00df", "und", "\u00e4ngst\u00b7lich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "Muthig ward's dann, dehnt sich m\u00e4chtig,", "tokens": ["Mut\u00b7hig", "ward's", "dann", ",", "dehnt", "sich", "m\u00e4ch\u00b7tig", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ADV", "$,", "VVFIN", "PRF", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "Wuchs und deckt mit Riesengliedern", "tokens": ["Wuchs", "und", "deckt", "mit", "Rie\u00b7sen\u00b7glie\u00b7dern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KON", "VVFIN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.27": {"text": "Siegreich bald die niedren Th\u00e4ler,", "tokens": ["Sieg\u00b7reich", "bald", "die", "nied\u00b7ren", "Th\u00e4\u00b7ler", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Reiht sich um den Stern des Tages,", "tokens": ["Reiht", "sich", "um", "den", "Stern", "des", "Ta\u00b7ges", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.29": {"text": "Dr\u00e4ngt ihn hastig hin zum Weste. \u2013", "tokens": ["Dr\u00e4ngt", "ihn", "has\u00b7tig", "hin", "zum", "Wes\u00b7te", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "ADV", "APPRART", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.30": {"text": "Ormuzd Priester rief der Sonne,", "tokens": ["Or\u00b7muzd", "Pries\u00b7ter", "rief", "der", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.31": {"text": "Tapfer sich im Kampf zu zeigen,", "tokens": ["Tap\u00b7fer", "sich", "im", "Kampf", "zu", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.32": {"text": "Heftig rief er, Wahnsinn betend.", "tokens": ["Hef\u00b7tig", "rief", "er", ",", "Wahn\u00b7sinn", "be\u00b7tend", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "$,", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.33": {"text": "Aber das Gestirn des Lichtes", "tokens": ["A\u00b7ber", "das", "Ge\u00b7stirn", "des", "Lich\u00b7tes"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.34": {"text": "Bettet sich im Weste stille.", "tokens": ["Bet\u00b7tet", "sich", "im", "Wes\u00b7te", "stil\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.35": {"text": "Rasend, zitternd, sah's der Alte", "tokens": ["Ra\u00b7send", ",", "zit\u00b7ternd", ",", "sah's", "der", "Al\u00b7te"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "$,", "VVPP", "$,", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.36": {"text": "Raffte sich empor vom Boden", "tokens": ["Raff\u00b7te", "sich", "em\u00b7por", "vom", "Bo\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "PTKVZ", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.37": {"text": "Eilte nach dem nahen Meere. \u2013", "tokens": ["Eil\u00b7te", "nach", "dem", "na\u00b7hen", "Mee\u00b7re", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.38": {"text": "Gl\u00e4nzend aus der Fluthen Spiegel", "tokens": ["Gl\u00e4n\u00b7zend", "aus", "der", "Flut\u00b7hen", "Spie\u00b7gel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.39": {"text": "Luna kam heraufgeschritten;", "tokens": ["Lu\u00b7na", "kam", "her\u00b7auf\u00b7ge\u00b7schrit\u00b7ten", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "VVIZU", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.40": {"text": "Feucht ihr Haar, vom Meer noch tr\u00e4uflend,", "tokens": ["Feucht", "ihr", "Haar", ",", "vom", "Meer", "noch", "tr\u00e4uf\u00b7lend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,", "APPRART", "NN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.41": {"text": "Thaubegl\u00e4nzet ihre Wange,", "tokens": ["Thau\u00b7be\u00b7gl\u00e4n\u00b7zet", "ih\u00b7re", "Wan\u00b7ge", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.42": {"text": "Blickte sie zur Erde nieder.", "tokens": ["Blick\u00b7te", "sie", "zur", "Er\u00b7de", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.43": {"text": "Da ergrimmte Ormuzd Priester,", "tokens": ["Da", "er\u00b7grimm\u00b7te", "Or\u00b7muzd", "Pries\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.44": {"text": "Nahm den Bogen, nahm die Pfeile,", "tokens": ["Nahm", "den", "Bo\u00b7gen", ",", "nahm", "die", "Pfei\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.45": {"text": "Eilte zu des Felsen Gipfel,", "tokens": ["Eil\u00b7te", "zu", "des", "Fel\u00b7sen", "Gip\u00b7fel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.46": {"text": "Achtet nicht der schroffen H\u00f6he,", "tokens": ["Ach\u00b7tet", "nicht", "der", "schrof\u00b7fen", "H\u00f6\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.47": {"text": "Drunten nicht des Meeres Brausen,", "tokens": ["Drun\u00b7ten", "nicht", "des", "Mee\u00b7res", "Brau\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.48": {"text": "Nimmt der Pfeile sch\u00e4rfsten, zielet", "tokens": ["Nimmt", "der", "Pfei\u00b7le", "sch\u00e4rfs\u00b7ten", ",", "zie\u00b7let"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "ART", "NN", "VVFIN", "$,", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.49": {"text": "Hoch zum Mond, dem Herz der N\u00e4chte;", "tokens": ["Hoch", "zum", "Mond", ",", "dem", "Herz", "der", "N\u00e4ch\u00b7te", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "$,", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.50": {"text": "Schwirrend rei\u00dft ihn da die Senne", "tokens": ["Schwir\u00b7rend", "rei\u00dft", "ihn", "da", "die", "Sen\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.51": {"text": "Seines Bogens hin zur Tiefe,", "tokens": ["Sei\u00b7nes", "Bo\u00b7gens", "hin", "zur", "Tie\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.52": {"text": "Sterbend b\u00fc\u00dft er sein Erk\u00fchnen. \u2013", "tokens": ["Ster\u00b7bend", "b\u00fc\u00dft", "er", "sein", "Er\u00b7k\u00fch\u00b7nen", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PPOSAT", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.53": {"text": "Mitleidsvoll ihm Mitra l\u00e4chlet;", "tokens": ["Mit\u00b7le\u00b7ids\u00b7voll", "ihm", "Mit\u00b7ra", "l\u00e4ch\u00b7let", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "NN", "VVFIN", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.54": {"text": "Aber g\u00fctig nimmt das Dunkel", "tokens": ["A\u00b7ber", "g\u00fc\u00b7tig", "nimmt", "das", "Dun\u00b7kel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.55": {"text": "Auf in seinem heil'gen Schoo\u00dfe", "tokens": ["Auf", "in", "sei\u00b7nem", "heil'\u00b7gen", "Schoo\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.56": {"text": "Freundlich den verirrten Kranken,", "tokens": ["Freund\u00b7lich", "den", "ver\u00b7irr\u00b7ten", "Kran\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.57": {"text": "Da\u00df im Arm der Mittern\u00e4chte", "tokens": ["Da\u00df", "im", "Arm", "der", "Mit\u00b7ter\u00b7n\u00e4ch\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPRART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.58": {"text": "Schweren Wahnsinns er genese.", "tokens": ["Schwe\u00b7ren", "Wahn\u00b7sinns", "er", "ge\u00b7ne\u00b7se", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.2": {"line.1": {"text": "Rasend am Altar des Feuers", "tokens": ["Ra\u00b7send", "am", "Al\u00b7tar", "des", "Feu\u00b7ers"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPRART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ormuzd Priester war geworden;", "tokens": ["Or\u00b7muzd", "Pries\u00b7ter", "war", "ge\u00b7wor\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VAFIN", "VAPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber als der Morgen helle", "tokens": ["A\u00b7ber", "als", "der", "Mor\u00b7gen", "hel\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "G\u00fclden aus dem Osten blickte,", "tokens": ["G\u00fcl\u00b7den", "aus", "dem", "Os\u00b7ten", "blick\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Kehrte Ruh in seine Seele.", "tokens": ["Kehr\u00b7te", "Ruh", "in", "sei\u00b7ne", "See\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Laut rief er dem Opferknaben:", "tokens": ["Laut", "rief", "er", "dem", "Op\u00b7fer\u00b7kna\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "\u00bbsiehe wie der Morgen pranget.", "tokens": ["\u00bb", "sie\u00b7he", "wie", "der", "Mor\u00b7gen", "pran\u00b7get", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "KOKOM", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Licht hat endlich obgesieget,", "tokens": ["Licht", "hat", "end\u00b7lich", "ob\u00b7ge\u00b7sie\u00b7get", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Siegend werden nie zur Erde", "tokens": ["Sie\u00b7gend", "wer\u00b7den", "nie", "zur", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "ADV", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Wieder sich die Schatten senken.\u00ab", "tokens": ["Wie\u00b7der", "sich", "die", "Schat\u00b7ten", "sen\u00b7ken", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "PRF", "ART", "NN", "VVINF", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Trost erf\u00fcllet sprachs der Alte,", "tokens": ["Trost", "er\u00b7f\u00fcl\u00b7let", "sprachs", "der", "Al\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PIS", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Kniete nieder am Altare,", "tokens": ["Knie\u00b7te", "nie\u00b7der", "am", "Al\u00b7ta\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Betend auf zum Gott de Lichtes", "tokens": ["Be\u00b7tend", "auf", "zum", "Gott", "de", "Lich\u00b7tes"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "APPR", "APPRART", "NN", "NE", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Prei\u00dfend ihn, des frohen Sieges,", "tokens": ["Prei\u00b7\u00dfend", "ihn", ",", "des", "fro\u00b7hen", "Sie\u00b7ges", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Angethan in hellen Kleidern", "tokens": ["An\u00b7ge\u00b7than", "in", "hel\u00b7len", "Klei\u00b7dern"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Zw\u00f6lf der Stunden t\u00e4glich feiern.", "tokens": ["Zw\u00f6lf", "der", "Stun\u00b7den", "t\u00e4g\u00b7lich", "fei\u00b7ern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Aber als die Zw\u00f6lf im Weste", "tokens": ["A\u00b7ber", "als", "die", "Zw\u00f6lf", "im", "Wes\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Tr\u00fcbe sich begunt zu f\u00e4rben,", "tokens": ["Tr\u00fc\u00b7be", "sich", "be\u00b7gunt", "zu", "f\u00e4r\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Leis verglomm im Abendstrahle,", "tokens": ["Leis", "ver\u00b7glomm", "im", "A\u00b7bend\u00b7strah\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Ormuzd Priester ward da stille,", "tokens": ["Or\u00b7muzd", "Pries\u00b7ter", "ward", "da", "stil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VAFIN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Sorgend blickt er auf zum Himmel", "tokens": ["Sor\u00b7gend", "blickt", "er", "auf", "zum", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Forschend was die Zeit gew\u00e4hre. \u2013", "tokens": ["For\u00b7schend", "was", "die", "Zeit", "ge\u00b7w\u00e4h\u00b7re", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVPP", "PRELS", "ART", "NN", "VVFIN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "Dunkel kam heran geschritten,", "tokens": ["Dun\u00b7kel", "kam", "he\u00b7ran", "ge\u00b7schrit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PTKVZ", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Zagend streift es, bla\u00df und \u00e4ngstlich,", "tokens": ["Za\u00b7gend", "streift", "es", ",", "bla\u00df", "und", "\u00e4ngst\u00b7lich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "Muthig ward's dann, dehnt sich m\u00e4chtig,", "tokens": ["Mut\u00b7hig", "ward's", "dann", ",", "dehnt", "sich", "m\u00e4ch\u00b7tig", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ADV", "$,", "VVFIN", "PRF", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "Wuchs und deckt mit Riesengliedern", "tokens": ["Wuchs", "und", "deckt", "mit", "Rie\u00b7sen\u00b7glie\u00b7dern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KON", "VVFIN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.27": {"text": "Siegreich bald die niedren Th\u00e4ler,", "tokens": ["Sieg\u00b7reich", "bald", "die", "nied\u00b7ren", "Th\u00e4\u00b7ler", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Reiht sich um den Stern des Tages,", "tokens": ["Reiht", "sich", "um", "den", "Stern", "des", "Ta\u00b7ges", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.29": {"text": "Dr\u00e4ngt ihn hastig hin zum Weste. \u2013", "tokens": ["Dr\u00e4ngt", "ihn", "has\u00b7tig", "hin", "zum", "Wes\u00b7te", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "ADV", "APPRART", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.30": {"text": "Ormuzd Priester rief der Sonne,", "tokens": ["Or\u00b7muzd", "Pries\u00b7ter", "rief", "der", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.31": {"text": "Tapfer sich im Kampf zu zeigen,", "tokens": ["Tap\u00b7fer", "sich", "im", "Kampf", "zu", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.32": {"text": "Heftig rief er, Wahnsinn betend.", "tokens": ["Hef\u00b7tig", "r