 "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.39": {"text": "Luna kam heraufgeschritten;", "tokens": ["Lu\u00b7na", "kam", "her\u00b7auf\u00b7ge\u00b7schrit\u00b7ten", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "VVIZU", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.40": {"text": "Feucht ihr Haar, vom Meer noch tr\u00e4uflend,", "tokens": ["Feucht", "ihr", "Haar", ",", "vom", "Meer", "noch", "tr\u00e4uf\u00b7lend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,", "APPRART", "NN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.41": {"text": "Thaubegl\u00e4nzet ihre Wange,", "tokens": ["Thau\u00b7be\u00b7gl\u00e4n\u00b7zet", "ih\u00b7re", "Wan\u00b7ge", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.42": {"text": "Blickte sie zur Erde nieder.", "tokens": ["Blick\u00b7te", "sie", "zur", "Er\u00b7de", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.43": {"text": "Da ergrimmte Ormuzd Priester,", "tokens": ["Da", "er\u00b7grimm\u00b7te", "Or\u00b7muzd", "Pries\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.44": {"text": "Nahm den Bogen, nahm die Pfeile,", "tokens": ["Nahm", "den", "Bo\u00b7gen", ",", "nahm", "die", "Pfei\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.45": {"text": "Eilte zu des Felsen Gipfel,", "tokens": ["Eil\u00b7te", "zu", "des", "