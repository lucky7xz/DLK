{"dta.poem.6281": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der H\u00e4rte und von der  \n Fl\u00fc\u00dfigkeit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Theilung ist gar leicht zu fassen;", "tokens": ["Die", "Thei\u00b7lung", "ist", "gar", "leicht", "zu", "fas\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Man darf in rohten Wein nur Wasser giessen lassen;", "tokens": ["Man", "darf", "in", "roh\u00b7ten", "Wein", "nur", "Was\u00b7ser", "gies\u00b7sen", "las\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "APPR", "ADJA", "NN", "ADV", "NN", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So wird so gleich ein\u2019 andre Farb entstehn.", "tokens": ["So", "wird", "so", "gleich", "ein'", "and\u00b7re", "Farb", "ent\u00b7stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ADV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Von Zweyen mischet sich die Blasse, wie wir sehn,", "tokens": ["Von", "Zwe\u00b7yen", "mi\u00b7schet", "sich", "die", "Blas\u00b7se", ",", "wie", "wir", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PRF", "ART", "NN", "$,", "PWAV", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Das Wasser mischt sich gantz mit Reben-Safft,", "tokens": ["Das", "Was\u00b7ser", "mischt", "sich", "gantz", "mit", "Re\u00b7ben\u00b7\u00b7S\u00b7afft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.6": {"text": "Der Wein verliehrt die vor\u2019ge Krafft,", "tokens": ["Der", "Wein", "ver\u00b7liehrt", "die", "vor'\u00b7ge", "Krafft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Das Gantze ist gemischt von unterschiednen Theilen.", "tokens": ["Das", "Gant\u00b7ze", "ist", "ge\u00b7mischt", "von", "un\u00b7ter\u00b7schied\u00b7nen", "Thei\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Begreifft man nun nicht sichtbarlich,", "tokens": ["Be\u00b7greifft", "man", "nun", "nicht", "sicht\u00b7bar\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}