{"dta.poem.1144": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "30. Die Sch\u00f6ne Unbest\u00e4ndige.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Mir thut/ du Wechsel-Kind der Lieb'/ an dir nicht weh ", "tokens": ["Mir", "thut", "/", "du", "Wech\u00b7sel\u00b7Kind", "der", "Lieb'", "/", "an", "dir", "nicht", "weh"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$(", "PPER", "NN", "ART", "NN", "$(", "APPR", "PPER", "PTKNEG", "PTKVZ"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "die Unbest\u00e4ndigkeit der fl\u00fcchtigen Gedancken:", "tokens": ["die", "Un\u00b7be\u00b7st\u00e4n\u00b7dig\u00b7keit", "der", "fl\u00fcch\u00b7ti\u00b7gen", "Ge\u00b7dan\u00b7cken", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Denn/ woltestu niemahls von deinem Sinne wancken/", "tokens": ["Denn", "/", "wol\u00b7tes\u00b7tu", "nie\u00b7mahls", "von", "dei\u00b7nem", "Sin\u00b7ne", "wan\u00b7cken", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$(", "VMFIN", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "So w\u00e4restu ein Fel\u00df in meiner Thr\u00e4nen-See.", "tokens": ["So", "w\u00e4\u00b7res\u00b7tu", "ein", "Fel\u00df", "in", "mei\u00b7ner", "Thr\u00e4\u00b7nen\u00b7See", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die heisse Sommers-Zeit zerschmelzt den h\u00e4rtsten Schnee;", "tokens": ["Die", "heis\u00b7se", "Som\u00b7mer\u00b7sZeit", "zer\u00b7schmelzt", "den", "h\u00e4rts\u00b7ten", "Schnee", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Man wird dich gegen mir noch schn vor Liebe krancken.", "tokens": ["Man", "wird", "dich", "ge\u00b7gen", "mir", "noch", "schn", "vor", "Lie\u00b7be", "kran\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PRF", "APPR", "PPER", "ADV", "ADJD", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Bleibt Sonn und Monde doch nicht stets in gleichen Schran-", "tokens": ["Bleibt", "Sonn", "und", "Mon\u00b7de", "doch", "nicht", "stets", "in", "glei\u00b7chen", "Schran"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "NN", "KON", "NE", "ADV", "PTKNEG", "ADV", "APPR", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "cken/ ", "tokens": ["cken", "/"], "token_info": ["word", "punct"], "pos": ["VVINF", "$("], "meter": "-", "measure": "single.down"}, "line.9": {"text": "Sibyllens kluges Blatt f\u00fchrt Zephir in der H\u00f6h.", "tokens": ["Si\u00b7byl\u00b7lens", "klu\u00b7ges", "Blatt", "f\u00fchrt", "Ze\u00b7phir", "in", "der", "H\u00f6h", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+---+", "measure": "unknown.measure.penta"}, "line.10": {"text": "Peleens Wunder-Braut ward bald zu einem Drachen/", "tokens": ["Pe\u00b7leens", "Wun\u00b7der\u00b7Braut", "ward", "bald", "zu", "ei\u00b7nem", "Dra\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VAFIN", "ADV", "APPR", "ART", "NN", "$("], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.11": {"text": "Bald ward sie mit der Haut des Leuen \u00fcberdeckt/", "tokens": ["Bald", "ward", "sie", "mit", "der", "Haut", "des", "Leu\u00b7en", "\u00fc\u00b7berd\u00b7eckt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "ART", "NN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Das Feuer konte sie doch endlich z\u00e4hmer machen.", "tokens": ["Das", "Feu\u00b7er", "kon\u00b7te", "sie", "doch", "end\u00b7lich", "z\u00e4h\u00b7mer", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "ADV", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Du/ die du hast in mir den heissen Brandt erweckt/", "tokens": ["Du", "/", "die", "du", "hast", "in", "mir", "den", "heis\u00b7sen", "Brandt", "er\u00b7weckt", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$(", "PRELS", "PPER", "VAFIN", "APPR", "PPER", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "W\u00fcrdstu nicht auch einmahl best\u00e4ndig/ w\u00e4r\u2019 es viel:", "tokens": ["W\u00fcrd\u00b7stu", "nicht", "auch", "ein\u00b7mahl", "be\u00b7st\u00e4n\u00b7dig", "/", "w\u00e4r'", "es", "viel", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ADV", "ADV", "ADJD", "$(", "VAFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Das B\u00f6se hat so wohl/ als Guttes/ Ma\u00df und Ziel.", "tokens": ["Das", "B\u00f6\u00b7se", "hat", "so", "wohl", "/", "als", "Gut\u00b7tes", "/", "Ma\u00df", "und", "Ziel", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADV", "$(", "KOUS", "NN", "$(", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}