{"dta.poem.21077": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Ellwina an Erwin.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1798", "urn": "urn:nbn:de:kobv:b4-200905193300", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "O Erwin, welche Wehmuth, welch Entz&#252;cken,               ", "tokens": ["O", "Er\u00b7win", ",", "wel\u00b7che", "Weh\u00b7muth", ",", "welch", "Entz", "&#252;", "cken", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "XML_entity", "word", "punct"], "pos": ["NE", "NE", "$,", "PWAT", "NN", "$,", "PWAT", "NN", "$(", "VVINF", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Durchbebt mich seit den selgen Augenblicken,", "tokens": ["Durch\u00b7bebt", "mich", "seit", "den", "sel\u00b7gen", "Au\u00b7gen\u00b7bli\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die mir unl\u00e4ngst, von deinem Arm umschlossen,", "tokens": ["Die", "mir", "un\u00b7l\u00e4ngst", ",", "von", "dei\u00b7nem", "Arm", "um\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "$,", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So hell verflossen!", "tokens": ["So", "hell", "ver\u00b7flos\u00b7sen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Als du so flehend mir ins Auge blicktest,", "tokens": ["Als", "du", "so", "fle\u00b7hend", "mir", "ins", "Au\u00b7ge", "blick\u00b7test", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als du so bl\u00f6d' und bang' mich an dich dr\u00fccktest,", "tokens": ["Als", "du", "so", "bl\u00f6d'", "und", "bang'", "mich", "an", "dich", "dr\u00fcck\u00b7test", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "KON", "VVFIN", "PRF", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Als mir zum erstenmal, was in dir brannte,", "tokens": ["Als", "mir", "zum", "ers\u00b7ten\u00b7mal", ",", "was", "in", "dir", "brann\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "ADV", "$,", "PRELS", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Dein Mund bekannte.", "tokens": ["Dein", "Mund", "be\u00b7kann\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Noch immer w\u00e4hn' ich, Bester, dich zu sehen.", "tokens": ["Noch", "im\u00b7mer", "w\u00e4hn'", "ich", ",", "Bes\u00b7ter", ",", "dich", "zu", "se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVIMP", "PPER", "$,", "NN", "$,", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich h\u00f6re noch dein seeler\u00fchrend Flehen,", "tokens": ["Ich", "h\u00f6\u00b7re", "noch", "dein", "see\u00b7le\u00b7r\u00fch\u00b7rend", "Fle\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPOSAT", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie Lieblingsmelodieen um uns singen,", "tokens": ["Wie", "Lieb\u00b7lings\u00b7me\u00b7lo\u00b7di\u00b7e\u00b7en", "um", "uns", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Tief in mir klingen.", "tokens": ["Tief", "in", "mir", "klin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPER", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.4": {"line.1": {"text": "Ich f\u00fchle noch der Pulse rasches Jagen,", "tokens": ["Ich", "f\u00fch\u00b7le", "noch", "der", "Pul\u00b7se", "ra\u00b7sches", "Ja\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dem deinigen mein Herz entgegen schlagen,", "tokens": ["Dem", "dei\u00b7ni\u00b7gen", "mein", "Herz", "ent\u00b7ge\u00b7gen", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSS", "PPOSAT", "NN", "APPO", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dem deinigen des Busens rege Wellen", "tokens": ["Dem", "dei\u00b7ni\u00b7gen", "des", "Bu\u00b7sens", "re\u00b7ge", "Wel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPOSS", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Entgegenschwellen.", "tokens": ["Ent\u00b7ge\u00b7gen\u00b7schwel\u00b7len", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.5": {"line.1": {"text": "Ich weiss es noch, und ich vergess' es nimmer,", "tokens": ["Ich", "weiss", "es", "noch", ",", "und", "ich", "ver\u00b7ge\u00b7ss'", "es", "nim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "KON", "PPER", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wie du, umgossen von des Sp\u00e4troths Schimmer,", "tokens": ["Wie", "du", ",", "um\u00b7gos\u00b7sen", "von", "des", "Sp\u00e4t\u00b7roths", "Schim\u00b7mer", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "VVPP", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In deiner schlanken Sch\u00f6nheit vor mit standest,", "tokens": ["In", "dei\u00b7ner", "schlan\u00b7ken", "Sch\u00f6n\u00b7heit", "vor", "mit", "stan\u00b7dest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "APPR", "APPR", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dann mich umwandest,", "tokens": ["Dann", "mich", "um\u00b7wan\u00b7dest", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Dann wieder los mich liessest, dann es wagtest,", "tokens": ["Dann", "wie\u00b7der", "los", "mich", "lies\u00b7sest", ",", "dann", "es", "wag\u00b7test", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "PPER", "VVFIN", "$,", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das Unaussprechliche mir stammelnd sagtest,", "tokens": ["Das", "Un\u00b7aus\u00b7sprech\u00b7li\u00b7che", "mir", "stam\u00b7melnd", "sag\u00b7test", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und w\u00e4hrend du es auszusprechen rangest,", "tokens": ["Und", "w\u00e4h\u00b7rend", "du", "es", "aus\u00b7zu\u00b7spre\u00b7chen", "ran\u00b7gest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPER", "VVIZU", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mich heiss umschlangest \u2014", "tokens": ["Mich", "heiss", "um\u00b7schlan\u00b7gest"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "NE", "VVFIN", "$("], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "O Gott! wie ward der Armen da zu Muthe!", "tokens": ["O", "Gott", "!", "wie", "ward", "der", "Ar\u00b7men", "da", "zu", "Mu\u00b7the", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "PWAV", "VAFIN", "ART", "NN", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wohl zehnmal floh und kam in der Minute", "tokens": ["Wohl", "zehn\u00b7mal", "floh", "und", "kam", "in", "der", "Mi\u00b7nu\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "KON", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das Blut mir ins Gesicht. Der Boden wankte,", "tokens": ["Das", "Blut", "mir", "ins", "Ge\u00b7sicht", ".", "Der", "Bo\u00b7den", "wank\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "APPRART", "NN", "$.", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Feste schwankte,", "tokens": ["Die", "Fes\u00b7te", "schwank\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.8": {"line.1": {"text": "Und um mich rollten rings die hohen Sph\u00e4ren.", "tokens": ["Und", "um", "mich", "roll\u00b7ten", "rings", "die", "ho\u00b7hen", "Sph\u00e4\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dem Aug' entquollen wollustreiche Z\u00e4hren.", "tokens": ["Dem", "Aug'", "ent\u00b7quol\u00b7len", "wol\u00b7lust\u00b7rei\u00b7che", "Z\u00e4h\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich stand die seligste der Viertelstunden,", "tokens": ["Ich", "stand", "die", "se\u00b7ligs\u00b7te", "der", "Vier\u00b7tel\u00b7stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "ART", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Von dir umwunden.", "tokens": ["Von", "dir", "um\u00b7wun\u00b7den", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.9": {"line.1": {"text": "O Erwin, welche Wehmuth, welch Entz\u00fccken", "tokens": ["O", "Er\u00b7win", ",", "wel\u00b7che", "Weh\u00b7muth", ",", "welch", "Ent\u00b7z\u00fc\u00b7cken"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NE", "NE", "$,", "PWAT", "NN", "$,", "PWAT", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Durchstr\u00f6mt seit jenen schw\u00fclen Augenblicken", "tokens": ["Durch\u00b7str\u00f6mt", "seit", "je\u00b7nen", "schw\u00fc\u00b7len", "Au\u00b7gen\u00b7bli\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dein armes M\u00e4dchen! Welches s\u00fcsse W\u00e4hnen,", "tokens": ["Dein", "ar\u00b7mes", "M\u00e4d\u00b7chen", "!", "Wel\u00b7ches", "s\u00fcs\u00b7se", "W\u00e4h\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$.", "PWAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und welches Sehnen!", "tokens": ["Und", "wel\u00b7ches", "Seh\u00b7nen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PWAT", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.10": {"line.1": {"text": "Wie trunken wandl' ich in der Meinen Mitte!", "tokens": ["Wie", "trun\u00b7ken", "wandl'", "ich", "in", "der", "Mei\u00b7nen", "Mit\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "PPER", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es irrt mein Fuss, es taumeln meine Tritte.", "tokens": ["Es", "irrt", "mein", "Fuss", ",", "es", "tau\u00b7meln", "mei\u00b7ne", "Trit\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Tr\u00e4umenden verwehen, wie Sekunden,", "tokens": ["Der", "Tr\u00e4u\u00b7men\u00b7den", "ver\u00b7we\u00b7hen", ",", "wie", "Se\u00b7kun\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "PWAV", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Tagesstunden.", "tokens": ["Die", "Ta\u00b7gess\u00b7tun\u00b7den", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.11": {"line.1": {"text": "Willkommen naht die Nacht. Zu Bette legen", "tokens": ["Will\u00b7kom\u00b7men", "naht", "die", "Nacht", ".", "Zu", "Bet\u00b7te", "le\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN", "$.", "APPR", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Andern sich, um s\u00fcsser Ruh zu pflegen.", "tokens": ["Die", "An\u00b7dern", "sich", ",", "um", "s\u00fcs\u00b7ser", "Ruh", "zu", "pfle\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "PRF", "$,", "KOUI", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Gedankenvoll sitz' ich bey Mondenschimmer", "tokens": ["Ge\u00b7dan\u00b7ken\u00b7voll", "sitz'", "ich", "bey", "Mon\u00b7den\u00b7schim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Im engen Zimmer.", "tokens": ["Im", "en\u00b7gen", "Zim\u00b7mer", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.12": {"line.1": {"text": "Es gaukeln um mich holde Fantasieen;", "tokens": ["Es", "gau\u00b7keln", "um", "mich", "hol\u00b7de", "Fan\u00b7ta\u00b7si\u00b7e\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+----+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Mein Ohr umt\u00f6nen ferne Melodieen;", "tokens": ["Mein", "Ohr", "um\u00b7t\u00f6\u00b7nen", "fer\u00b7ne", "Me\u00b7lo\u00b7di\u00b7e\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Mein Aug' umschweben himmlische Gesichte", "tokens": ["Mein", "Aug'", "um\u00b7schwe\u00b7ben", "himm\u00b7li\u00b7sche", "Ge\u00b7sich\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Im D\u00e4mmerlichte.", "tokens": ["Im", "D\u00e4m\u00b7mer\u00b7lich\u00b7te", "."], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.13": {"line.1": {"text": "Ich schaudre auf. Und um mich ists so stille.", "tokens": ["Ich", "schaud\u00b7re", "auf", ".", "Und", "um", "mich", "ists", "so", "stil\u00b7le", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$.", "KON", "APPR", "PPER", "VAFIN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Aus Duftgew\u00f6lken weint des Mondes F\u00fclle;", "tokens": ["Aus", "Duft\u00b7ge\u00b7w\u00f6l\u00b7ken", "weint", "des", "Mon\u00b7des", "F\u00fcl\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dann droht es, mir mit ungest\u00fcmen Dr\u00e4ngen", "tokens": ["Dann", "droht", "es", ",", "mir", "mit", "un\u00b7ge\u00b7st\u00fc\u00b7men", "Dr\u00e4n\u00b7gen"], "token_info": ["word",