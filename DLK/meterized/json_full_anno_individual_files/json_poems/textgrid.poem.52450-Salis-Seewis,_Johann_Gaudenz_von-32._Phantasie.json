{"textgrid.poem.52450": {"metadata": {"author": {"name": "Salis-Seewis, Johann Gaudenz von", "birth": "N.A.", "death": "N.A."}, "title": "32. Phantasie", "genre": "verse", "period": "N.A.", "pub_year": 1798, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie sie wandelt auf den Birkenh\u00f6hen,", "tokens": ["Wie", "sie", "wan\u00b7delt", "auf", "den", "Bir\u00b7ken\u00b7h\u00f6\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Thymian und Skabiosen pfl\u00fcckt,", "tokens": ["Thy\u00b7mi\u00b7an", "und", "Ska\u00b7bio\u00b7sen", "pfl\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Wie sie sich zur Blumenlese b\u00fcckt,", "tokens": ["Wie", "sie", "sich", "zur", "Blu\u00b7men\u00b7le\u00b7se", "b\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Wie sich ihres Schleiers Falten bl\u00e4hen,", "tokens": ["Wie", "sich", "ih\u00b7res", "Schlei\u00b7ers", "Fal\u00b7ten", "bl\u00e4\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "PPOSAT", "NN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Wie die apfelgr\u00fcnen B\u00e4nder wehen,", "tokens": ["Wie", "die", "ap\u00b7fel\u00b7gr\u00fc\u00b7nen", "B\u00e4n\u00b7der", "we\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "troch