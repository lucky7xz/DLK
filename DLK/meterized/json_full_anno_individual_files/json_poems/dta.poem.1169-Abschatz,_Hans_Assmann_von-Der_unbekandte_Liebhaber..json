{"dta.poem.1169": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Der unbekandte Liebhaber.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Schau die K\u00fcnheit fremder Hand/ ", "tokens": ["Schau", "die", "K\u00fcn\u00b7heit", "frem\u00b7der", "Hand", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Welche/ sonder dich zu kennen/", "tokens": ["Wel\u00b7che", "/", "son\u00b7der", "dich", "zu", "ken\u00b7nen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "$(", "KON", "PPER", "PTKZU", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Macht durch diese Schrifft bekant", "tokens": ["Macht", "durch", "die\u00b7se", "Schrifft", "be\u00b7kant"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PDAT", "NN", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihrer treuen Seele Brennen/", "tokens": ["Ih\u00b7rer", "treu\u00b7en", "See\u00b7le", "Bren\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Welche dich nicht kennen will", "tokens": ["Wel\u00b7che", "dich", "nicht", "ken\u00b7nen", "will"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "PTKNEG", "VVINF", "VMFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Und nur kennet allzuviel.", "tokens": ["Und", "nur", "ken\u00b7net", "all\u00b7zu\u00b7viel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PIS", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Fordre meinen Nahmen nicht", "tokens": ["Ford\u00b7re", "mei\u00b7nen", "Nah\u00b7men", "nicht"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKNEG"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Bi\u00df ihn wird die Zeit entdecken/", "tokens": ["Bi\u00df", "ihn", "wird", "die", "Zeit", "ent\u00b7de\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "ART", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der treuen Dienste Pflicht", "tokens": ["Und", "der", "treu\u00b7en", "Diens\u00b7te", "Pflicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Gleiche Flamm in dir erwecken/", "tokens": ["Glei\u00b7che", "Flamm", "in", "dir", "er\u00b7we\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "PPER", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Bi\u00df man mich auch ungenennt", "tokens": ["Bi\u00df", "man", "mich", "auch", "un\u00b7ge\u00b7nennt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "PRF", "ADV", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Gleich wie deine Tugend kennt.", "tokens": ["Gleich", "wie", "dei\u00b7ne", "Tu\u00b7gend", "kennt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Mehr ich deiner Sclaven Zahl/", "tokens": ["Mehr", "ich", "dei\u00b7ner", "Scla\u00b7ven", "Zahl", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PPER", "PPOSAT", "NN", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Du bist drum nicht mehr geplaget;", "tokens": ["Du", "bist", "drum", "nicht", "mehr", "ge\u00b7pla\u00b7get", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PAV", "PTKNEG", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn ein andrer seine Qual", "tokens": ["Wenn", "ein", "an\u00b7drer", "sei\u00b7ne", "Qual"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Dir mit langen Worten klaget/", "tokens": ["Dir", "mit", "lan\u00b7gen", "Wor\u00b7ten", "kla\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sollen stumme Dienst allein", "tokens": ["Sol\u00b7len", "stum\u00b7me", "Dienst", "al\u00b7lein"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Meiner Liebe Zeugen seyn.", "tokens": ["Mei\u00b7ner", "Lie\u00b7be", "Zeu\u00b7gen", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}