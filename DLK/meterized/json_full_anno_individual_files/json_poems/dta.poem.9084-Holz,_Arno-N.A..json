{"dta.poem.9084": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1898", "urn": "urn:nbn:de:kobv:b4-200905192685", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Fern auf der Insel Nurapu", "tokens": ["Fern", "auf", "der", "In\u00b7sel", "Nu\u00b7ra\u00b7pu"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "bl\u00fcht der Baum Bo.", "tokens": ["bl\u00fcht", "der", "Baum", "Bo", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NE", "NE", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "In seinen Wurzeln singt die See,", "tokens": ["In", "sei\u00b7nen", "Wur\u00b7zeln", "singt", "die", "See", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "durch seine Zweige ziehn die Sterne.", "tokens": ["durch", "sei\u00b7ne", "Zwei\u00b7ge", "ziehn", "die", "Ster\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Auf einem langen Ast, mein Gott: die Hirtin und der \u2014 Schornsteinfeger?", "tokens": ["Auf", "ei\u00b7nem", "lan\u00b7gen", "Ast", ",", "mein", "Gott", ":", "die", "Hir\u00b7tin", "und", "der", "Schorn\u00b7stein\u00b7fe\u00b7ger", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "PPOSAT", "NN", "$.", "ART", "NN", "KON", "ART", "$(", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.3": {"line.1": {"text": "Die niedlichen kleinen Schuhe, der goldne Hut,", "tokens": ["Die", "nied\u00b7li\u00b7chen", "klei\u00b7nen", "Schu\u00b7he", ",", "der", "gold\u00b7ne", "Hut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "die schwarze Leiter, der Hirtenstab . . .", "tokens": ["die", "schwar\u00b7ze", "Lei\u00b7ter", ",", "der", "Hir\u00b7ten\u00b7stab", ".", ".", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "$.", "$.", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Ihr habt also doch nicht zur\u00fcckgefunden?", "tokens": ["Ihr", "habt", "al\u00b7so", "doch", "nicht", "zu\u00b7r\u00fcck\u00b7ge\u00b7fun\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Ach Gott, ja:", "tokens": ["Ach", "Gott", ",", "ja", ":"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ITJ", "NN", "$,", "ADV", "$."], "meter": "+--", "measure": "dactylic.init"}, "line.2": {"text": "wenn man aus Porzellan ist!", "tokens": ["wenn", "man", "aus", "Por\u00b7zel\u00b7lan", "ist", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "NE", "VAFIN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.6": {"line.1": {"text": "Das alte St\u00fcbchen mit dem Spiegeltischchen,", "tokens": ["Das", "al\u00b7te", "St\u00fcb\u00b7chen", "mit", "dem", "Spie\u00b7gel\u00b7ti\u00b7schchen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "das verschn\u00f6rkelte Spind aus Mahagoniholz,", "tokens": ["das", "ver\u00b7schn\u00f6r\u00b7kel\u00b7te", "Spind", "aus", "Ma\u00b7ha\u00b7go\u00b7ni\u00b7holz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "der blaue, gem\u00fctliche Kachelofen!", "tokens": ["der", "blau\u00b7e", ",", "ge\u00b7m\u00fct\u00b7li\u00b7che", "Ka\u00b7chel\u00b7o\u00b7fen", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.7": {"line.1": {"text": "Grossmutters Tulpen!", "tokens": ["Gross\u00b7mut\u00b7ters", "Tul\u00b7pen", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.8": {"line.1": {"text": "Das waren noch Zeiten!", "tokens": ["Das", "wa\u00b7ren", "noch", "Zei\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.9": {"line.1": {"text": "Hier ruft keine Kukuksuhr,", "tokens": ["Hier", "ruft", "kei\u00b7ne", "Ku\u00b7kuk\u00b7suhr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "hier duftet kein Lawendeltopf;", "tokens": ["hier", "duf\u00b7tet", "kein", "La\u00b7wen\u00b7del\u00b7topf", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "hier braust die See,", "tokens": ["hier", "braust", "die", "See", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "hier fliehn die Sterne.", "tokens": ["hier", "fliehn", "die", "Ster\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.10": {"line.1": {"text": "Und ich sitze und weine bitterlich!", "tokens": ["Und", "ich", "sit\u00b7ze", "und", "wei\u00b7ne", "bit\u00b7ter\u00b7lich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "KON", "VVFIN", "ADJD", "$."], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}}}}}