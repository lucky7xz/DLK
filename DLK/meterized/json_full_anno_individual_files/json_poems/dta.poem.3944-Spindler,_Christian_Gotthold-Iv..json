{"dta.poem.3944": {"metadata": {"author": {"name": "Spindler, Christian Gotthold", "birth": "N.A.", "death": "N.A."}, "title": "Iv.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-20581-9", "language": ["de:0.99"], "booktitle": "Spindler, Christian Gotthold: Unschuldige Jugend-Fr\u00fcchte. Leipzig, 1745."}, "poem": {"stanza.1": {"line.1": {"text": "Mein G\u00f6nner, hoher Freund, es kommt ein", "tokens": ["Mein", "G\u00f6n\u00b7ner", ",", "ho\u00b7her", "Freund", ",", "es", "kommt", "ein"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "ADJA", "NN", "$,", "PPER", "VVFIN", "ART"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "w\u00fcnschend Schreiben,", "tokens": ["w\u00fcn\u00b7schend", "Schrei\u00b7ben", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Ich, da dirs gl\u00fccklich geht, darf dirs nicht schul-", "tokens": ["Ich", ",", "da", "dirs", "gl\u00fcck\u00b7lich", "geht", ",", "darf", "dirs", "nicht", "schul"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "$,", "KOUS", "PIS", "ADJD", "VVFIN", "$,", "VMFIN", "PIS", "PTKNEG", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "dig bleiben,", "tokens": ["dig", "blei\u00b7ben", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVINF", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Denn du verwechselst heut den bangen Wittwer-", "tokens": ["Denn", "du", "ver\u00b7wech\u00b7selst", "heut", "den", "ban\u00b7gen", "Witt\u00b7wer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADV", "ART", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Stand", "tokens": ["Stand"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.7": {"text": "Mit einem gl\u00fccklichen und neuen Ehestand.", "tokens": ["Mit", "ei\u00b7nem", "gl\u00fcck\u00b7li\u00b7chen", "und", "neu\u00b7en", "E\u00b7hes\u00b7tand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}