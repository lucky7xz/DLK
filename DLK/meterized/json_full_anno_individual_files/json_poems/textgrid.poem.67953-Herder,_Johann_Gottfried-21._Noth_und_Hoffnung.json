{"textgrid.poem.67953": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "21. Noth und Hoffnung", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "1:", "tokens": [":"], "token_info": ["punct"], "pos": ["$."]}, "line.2": {"text": "Ihr G\u00f6tter, weh mir, da\u00df ich Noth und Gram", "tokens": ["Ihr", "G\u00f6t\u00b7ter", ",", "weh", "mir", ",", "da\u00df", "ich", "Noth", "und", "Gram"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "PPER", "$,", "KOUS", "PPER", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Zu Lebensf\u00fchrerinnen mitbekam!", "tokens": ["Zu", "Le\u00b7bens\u00b7f\u00fch\u00b7re\u00b7rin\u00b7nen", "mit\u00b7be\u00b7kam", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ge\u00e4ngstiget von aussen und von innen,", "tokens": ["Ge\u00b7\u00e4ngs\u00b7ti\u00b7get", "von", "aus\u00b7sen", "und", "von", "in\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "VVINF", "KON", "APPR", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wenn werd ich Ruh im Spiel der Welt gewinnen?", "tokens": ["Wenn", "werd", "ich", "Ruh", "im", "Spiel", "der", "Welt", "ge\u00b7win\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "NN", "APPRART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "2:", "tokens": [":"], "token_info": ["punct"], "pos": ["$."]}, "line.2": {"text": "Ihr G\u00f6tter, wohl mir, da\u00df ihr Noth mir gabt,", "tokens": ["Ihr", "G\u00f6t\u00b7ter", ",", "wohl", "mir", ",", "da\u00df", "ihr", "Noth", "mir", "gabt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADV", "PPER", "$,", "KOUS", "PPOSAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und mit der Hoffnung Liebeku\u00df mich labt.", "tokens": ["Und", "mit", "der", "Hoff\u00b7nung", "Lie\u00b7be\u00b7ku\u00df", "mich", "labt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Von aussen soll die Eine fort mich dringen,", "tokens": ["Von", "aus\u00b7sen", "soll", "die", "Ei\u00b7ne", "fort", "mich", "drin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVINF", "VMFIN", "ART", "NN", "PTKVZ", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Von innen machts die andre mir gelingen.", "tokens": ["Von", "in\u00b7nen", "machts", "die", "and\u00b7re", "mir", "ge\u00b7lin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VVFIN", "ART", "ADJA", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "1:", "tokens": [":"], "token_info": ["punct"], "pos": ["$."]}, "line.2": {"text": "Der Fels des Sisyphus ist unsre M\u00fch,", "tokens": ["Der", "Fels", "des", "Si\u00b7syp\u00b7hus", "ist", "uns\u00b7re", "M\u00fch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NE", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sie steiget schwer, und schwerer sinket sie.", "tokens": ["Sie", "stei\u00b7get", "schwer", ",", "und", "schwe\u00b7rer", "sin\u00b7ket", "sie", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$,", "KON", "ADJD", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ixions Rad, es brennt in unserm Herzen,", "tokens": ["I\u00b7xions", "Rad", ",", "es", "brennt", "in", "un\u00b7serm", "Her\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Auch wenn mit Wolken wir und Hoffnung scherzen.", "tokens": ["Auch", "wenn", "mit", "Wol\u00b7ken", "wir", "und", "Hoff\u00b7nung", "scher\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "APPR", "NN", "PPER", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "2:", "tokens": [":"], "token_info": ["punct"], "pos": ["$."]}, "line.2": {"text": "Der Erde Saat ist unsre kurze M\u00fch,", "tokens": ["Der", "Er\u00b7de", "Saat", "ist", "uns\u00b7re", "kur\u00b7ze", "M\u00fch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sie sinket leicht und frisch erstehet sie.", "tokens": ["Sie", "sin\u00b7ket", "leicht", "und", "frisch", "er\u00b7ste\u00b7het", "sie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "KON", "ADJD", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wie junges Gr\u00fcn soll unsre Hoffnung gr\u00fcnen;", "tokens": ["Wie", "jun\u00b7ges", "Gr\u00fcn", "soll", "uns\u00b7re", "Hoff\u00b7nung", "gr\u00fc\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "VMFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Bald ist es Frucht, wo Bl\u00fcthen nur erschienen.", "tokens": ["Bald", "ist", "es", "Frucht", ",", "wo", "Bl\u00fc\u00b7then", "nur", "er\u00b7schie\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "$,", "PWAV", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "1:", "tokens": [":"], "token_info": ["punct"], "pos": ["$."]}, "line.2": {"text": "Der Herbst entlaubt das Leben und den Hain,", "tokens": ["Der", "Herbst", "ent\u00b7laubt", "das", "Le\u00b7ben", "und", "den", "Hain", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und Winterfrost wird deine Hoffnung seyn.", "tokens": ["Und", "Win\u00b7ter\u00b7frost", "wird", "dei\u00b7ne", "Hoff\u00b7nung", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "2:", "tokens": [":"], "token_info": ["punct"], "pos": ["$."]}, "line.2": {"text": "Der Fr\u00fchling kommt mit Hain und Hoffnung wieder,", "tokens": ["Der", "Fr\u00fch\u00b7ling", "kommt", "mit", "Hain", "und", "Hoff\u00b7nung", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "KON", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und s\u00fcsse Noth besingen alle Lieder.", "tokens": ["Und", "s\u00fcs\u00b7se", "Noth", "be\u00b7sin\u00b7gen", "al\u00b7le", "Lie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Ihr Nachtigallen, klaget s\u00fcsse Pein,", "tokens": ["Ihr", "Nach\u00b7ti\u00b7gal\u00b7len", ",", "kla\u00b7get", "s\u00fcs\u00b7se", "Pein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ihr Turteltauben, girret Liebe drein,", "tokens": ["Ihr", "Tur\u00b7tel\u00b7tau\u00b7ben", ",", "gir\u00b7ret", "Lie\u00b7be", "drein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ihr Knospen spro\u00dft der M\u00fche s\u00fcsses Streben,", "tokens": ["Ihr", "Knos\u00b7pen", "spro\u00dft", "der", "M\u00fc\u00b7he", "s\u00fcs\u00b7ses", "Stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ihr Lerchen singt der Hoffnung Fr\u00fchlingsleben.", "tokens": ["Ihr", "Ler\u00b7chen", "singt", "der", "Hoff\u00b7nung", "Fr\u00fch\u00b7lings\u00b7le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "1:", "tokens": [":"], "token_info": ["punct"], "pos": ["$."]}, "line.2": {"text": "So will ich denn, des Lebens mich zu freun,", "tokens": ["So", "will", "ich", "denn", ",", "des", "Le\u00b7bens", "mich", "zu", "freun", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "$,", "ART", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "In Noth getrost, in Hoffnung gl\u00fccklich seyn.", "tokens": ["In", "Noth", "ge\u00b7trost", ",", "in", "Hoff\u00b7nung", "gl\u00fcck\u00b7lich", "seyn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,", "APPR", "NN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wenn unter Rosen oft auch Dornen stechen,", "tokens": ["Wenn", "un\u00b7ter", "Ro\u00b7sen", "oft", "auch", "Dor\u00b7nen", "ste\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NN", "ADV", "ADV", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Von Dornen will ich meine Rosen brechen.", "tokens": ["Von", "Dor\u00b7nen", "will", "ich", "mei\u00b7ne", "Ro\u00b7sen", "bre\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "1:", "tokens": [":"], "token_info": ["punct"], "pos": ["$."]}, "line.2": {"text": "Ihr G\u00f6tter, weh mir, da\u00df ich Noth und Gram", "tokens": ["Ihr", "G\u00f6t\u00b7ter", ",", "weh", "mir", ",", "da\u00df", "ich", "Noth", "und", "Gram"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["P