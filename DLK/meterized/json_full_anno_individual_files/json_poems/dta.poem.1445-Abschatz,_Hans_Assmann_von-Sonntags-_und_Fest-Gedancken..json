{"dta.poem.1445": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Sonntags- und Fest-Gedancken.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Freude wird der gantzen Welt von dem Engel angetragen/", "tokens": ["Freu\u00b7de", "wird", "der", "gant\u00b7zen", "Welt", "von", "dem", "En\u00b7gel", "an\u00b7ge\u00b7tra\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "ADJA", "NN", "APPR", "ART", "NN", "VVPP", "$("], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Freude l\u00e4st auch dir/ mein Hertz/ dein gebohrner Heyland", "tokens": ["Freu\u00b7de", "l\u00e4st", "auch", "dir", "/", "mein", "Hertz", "/", "dein", "ge\u00b7bohr\u00b7ner", "Hey\u00b7land"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "ADV", "PPER", "$(", "PPOSAT", "NN", "$(", "PPOSAT", "ADJA", "NN"], "meter": "+-+---+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "sagen:", "tokens": ["sa\u00b7gen", ":"], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Eile seiner Krippe zu/ la\u00df in deinen Glaubens-Armen/", "tokens": ["Ei\u00b7le", "sei\u00b7ner", "Krip\u00b7pe", "zu", "/", "la\u00df", "in", "dei\u00b7nen", "Glau\u00b7bens\u00b7Ar\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "PTKZU", "$(", "VVIMP", "APPR", "PPOSAT", "NN", "$("], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.5": {"text": "Heisser Brunst und Liebe voll/ dieses edle Kind erwarmen/", "tokens": ["Heis\u00b7ser", "Brunst", "und", "Lie\u00b7be", "voll", "/", "die\u00b7ses", "ed\u00b7le", "Kind", "er\u00b7war\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "ADJD", "$(", "PDAT", "ADJA", "NN", "VVINF", "$("], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.6": {"text": "Stimme Wiegen-Lieder an/ la\u00df mit frohem Mund erschal-", "tokens": ["Stim\u00b7me", "Wie\u00b7gen\u00b7Lie\u00b7der", "an", "/", "la\u00df", "mit", "fro\u00b7hem", "Mund", "er\u00b7scha\u00b7l"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "NN", "PTKVZ", "$(", "VVIMP", "APPR", "ADJA", "NN", "TRUNC"], "meter": "+-+-+--+-+-+--+", "measure": "trochaic.septa.relaxed"}, "line.7": {"text": "len:", "tokens": ["len", ":"], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "-", "measure": "single.down"}, "line.8": {"text": "Ehre sey GOtt in der H\u00f6h/ Fried auff Erd und Wohlgefal-", "tokens": ["Eh\u00b7re", "sey", "Gott", "in", "der", "H\u00f6h", "/", "Fried", "auff", "Erd", "und", "Wohl\u00b7ge\u00b7fa\u00b7l"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "NN", "APPR", "ART", "NN", "$(", "NN", "APPR", "NN", "KON", "TRUNC"], "meter": "+--+--+--+-+--+", "measure": "dactylic.tri.plus"}, "line.9": {"text": "len!", "tokens": ["len", "!"], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "-", "measure": "single.down"}}}}}