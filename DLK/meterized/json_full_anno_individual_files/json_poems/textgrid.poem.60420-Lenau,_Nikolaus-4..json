{"textgrid.poem.60420": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Redlich, Schatten, kannst du heben", "tokens": ["Red\u00b7lich", ",", "Schat\u00b7ten", ",", "kannst", "du", "he\u00b7ben"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "$,", "NN", "$,", "VMFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Den Pokal, mich lassen leben;", "tokens": ["Den", "Po\u00b7kal", ",", "mich", "las\u00b7sen", "le\u00b7ben", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "VVINF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn sie meinen Leib bestatten,", "tokens": ["Wenn", "sie", "mei\u00b7nen", "Leib", "be\u00b7stat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bist du mitvergangen, Schatten!", "tokens": ["Bist", "du", "mit\u00b7ver\u00b7gan\u00b7gen", ",", "Schat\u00b7ten", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "PPER", "VVINF", "$,", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Manches Auge m\u00f6chte weinen;", "tokens": ["Man\u00b7ches", "Au\u00b7ge", "m\u00f6ch\u00b7te", "wei\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schatten, doch ich w\u00fc\u00dfte keinen", "tokens": ["Schat\u00b7ten", ",", "doch", "ich", "w\u00fc\u00df\u00b7te", "kei\