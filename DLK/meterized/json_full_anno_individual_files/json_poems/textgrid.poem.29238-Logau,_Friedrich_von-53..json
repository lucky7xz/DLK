{"textgrid.poem.29238": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "53.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wiewol manch andre Kunst ist sp\u00f6ttisch blieben liegen,", "tokens": ["Wie\u00b7wol", "manch", "and\u00b7re", "Kunst", "ist", "sp\u00f6t\u00b7tisch", "blie\u00b7ben", "lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "ADJA", "NN", "VAFIN", "ADJD", "VVFIN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ist Rechen-Kunst doch hoch im Krieg ietzund gestiegen.", "tokens": ["Ist", "Re\u00b7chen\u00b7Kunst", "doch", "hoch", "im", "Krieg", "ie\u00b7tzund", "ge\u00b7stie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ADV", "ADJD", "APPRART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da\u00df f\u00fcnffzehn funffzig gab, da\u00df funffzig hundert war,", "tokens": ["Da\u00df", "f\u00fcnff\u00b7zehn", "funff\u00b7zig", "gab", ",", "da\u00df", "funff\u00b7zig", "hun\u00b7dert", "war", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "CARD", "CARD", "VVFIN", "$,", "KOUS", "CARD", "CARD", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df hundert tausend galt, kam her durch ihre Lahr.", "tokens": ["Da\u00df", "hun\u00b7dert", "tau\u00b7send", "galt", ",", "kam", "her", "durch", "ih\u00b7re", "Lahr", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "CARD", "CARD", "VVFIN", "$,", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Sie machte noch wol gar au\u00df Nullen starcke Summen", "tokens": ["Sie", "mach\u00b7te", "noch", "wol", "gar", "au\u00df", "Nul\u00b7len", "star\u00b7cke", "Sum\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ADV", "APPR", "NN", "VVFIN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und kunte k\u00fcnstlich drauff auch gar darhinter kummen,", "tokens": ["Und", "kun\u00b7te", "k\u00fcnst\u00b7lich", "drauff", "auch", "gar", "dar\u00b7hin\u00b7ter", "kum\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADJD", "PAV", "ADV", "ADV", "PAV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Was offt ein gantzes Land in seinem Beutel trug;", "tokens": ["Was", "offt", "ein", "gant\u00b7zes", "Land", "in", "sei\u00b7nem", "Beu\u00b7tel", "trug", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Drau\u00df manchem reich zu seyn, kam gar ein schneller Fug.", "tokens": ["Drau\u00df", "man\u00b7chem", "reich", "zu", "seyn", ",", "kam", "gar", "ein", "schnel\u00b7ler", "Fug", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PIAT", "ADJD", "PTKZU", "VAINF", "$,", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wiewol manch andre Kunst ist sp\u00f6ttisch blieben liegen,", "tokens": ["Wie\u00b7wol", "ma