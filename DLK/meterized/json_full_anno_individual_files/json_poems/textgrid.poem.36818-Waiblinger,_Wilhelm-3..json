{"textgrid.poem.36818": {"metadata": {"author": {"name": "Waiblinger, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Komm, Freund, Geleiter bin ich und F\u00fchrer dir,", "tokens": ["Komm", ",", "Freund", ",", "Ge\u00b7lei\u00b7ter", "bin", "ich", "und", "F\u00fch\u00b7rer", "dir", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "NN", "VAFIN", "PPER", "KON", "NN", "PPER", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Komm nach Pompeji. Willig hast du mir stets", "tokens": ["Komm", "nach", "Pom\u00b7pe\u00b7ji", ".", "Wil\u00b7lig", "hast", "du", "mir", "stets"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "NE", "$.", "ADJD", "VAFIN", "PPER", "PPER", "ADV"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Ge\u00f6ffnet manchen Quell der Sch\u00f6nheit,", "tokens": ["Ge\u00b7\u00f6ff\u00b7net", "man\u00b7chen", "Quell", "der", "Sch\u00f6n\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Manchen Gedanken von h\u00f6h'rer Weisheit", "tokens": ["Man\u00b7chen", "Ge\u00b7dan\u00b7ken", "von", "h\u00f6h'\u00b7rer", "Weis\u00b7heit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "APPR", "ADJA", "NN"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Enth\u00fcllt vor mir; drum ladet der Dankbare", "tokens": ["Ent\u00b7h\u00fcllt", "vor", "mir", ";", "drum", "la\u00b7det", "der", "Dank\u00b7ba\u00b7re"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PPER", "$.", "PAV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-++--", "measure": "unknown.measure.penta"}, "line.2": {"text": "Dich ein zum Weinberg. Hoch an der Ulme rankt", "tokens": ["Dich", "ein", "zum", "Wein\u00b7berg", ".", "Hoch", "an", "der", "Ul\u00b7me", "rankt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "ART", "APPRART", "NN", "$.", "ADJD", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Viel\u00e4st'ge fruchtbelad'ne Rebe,", "tokens": ["Vie\u00b7l\u00e4st'\u00b7ge", "frucht\u00b7be\u00b7la\u00b7d'\u00b7ne", "Re\u00b7be", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "Wurzelnd und bl\u00fchend aus tausendj\u00e4hr'ger", "tokens": ["Wur\u00b7zelnd", "und", "bl\u00fc\u00b7hend", "aus", "tau\u00b7send\u00b7j\u00e4hr'\u00b7ger"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "KON", "ADJD", "APPR", "ADJA"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Vulkan'scher Asche. Drunten im gro\u00dfen Grab", "tokens": ["Vul\u00b7kan'\u00b7scher", "A\u00b7sche", ".", "Drun\u00b7ten", "im", "gro\u00b7\u00dfen", "Grab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "NN", "APPRART", "ADJA", "NN"], "meter": "---+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Schlief eine Stadt, der G\u00f6tter und Menschen voll,", "tokens": ["Schlief", "ei\u00b7ne", "Stadt", ",", "der", "G\u00f6t\u00b7ter", "und", "Men\u00b7schen", "voll", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "ART", "NN", "KON", "NN", "ADJD", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Als noch die Sonn' ihr schien; verlassen", "tokens": ["Als", "noch", "die", "Sonn'", "ihr", "schien", ";", "ver\u00b7las\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "PPER", "VVFIN", "$.", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Aber von beiden, da sie des hei\u00dfen", "tokens": ["A\u00b7ber", "von", "bei\u00b7den", ",", "da", "sie", "des", "hei\u00b7\u00dfen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PIAT", "$,", "KOUS", "PPER", "ART", "ADJA"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "Schreckbaren Regens t\u00f6dtlich Gew\u00f6lk bedeckt,", "tokens": ["Schreck\u00b7ba\u00b7ren", "Re\u00b7gens", "t\u00f6dt\u00b7lich", "Ge\u00b7w\u00f6lk", "be\u00b7deckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "NN", "VVPP", "$,"], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Aus dessen Graus nun wieder der Tempel steigt,", "tokens": ["Aus", "des\u00b7sen", "Graus", "nun", "wie\u00b7der", "der", "Tem\u00b7pel", "steigt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "ADV", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und heit're S\u00e4ulen, und das farb'ge", "tokens": ["Und", "heit'\u00b7re", "S\u00e4u\u00b7len", ",", "und", "das", "fa\u00b7rb'\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "$,", "KON", "ART", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Kleine Gemach, die gemalte Hausflur,", "tokens": ["Klei\u00b7ne", "Ge\u00b7mach", ",", "die", "ge\u00b7mal\u00b7te", "Haus\u00b7flur", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.5": {"line.1": {"text": "Und selbst des Forums tempelumragter Platz,", "tokens": ["Und", "selbst", "des", "Fo\u00b7rums", "tem\u00b7pe\u00b7lum\u00b7rag\u00b7ter", "Platz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Da l\u00e4ngst gest\u00fcrzt ist fr\u00fcherer G\u00f6tter Dienst", "tokens": ["Da", "l\u00e4ngst", "ge\u00b7st\u00fcrzt", "ist", "fr\u00fc\u00b7he\u00b7rer", "G\u00f6t\u00b7ter", "Dienst"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVPP", "VAFIN", "ADJA", "NN", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und jene, die des Donn'rers Adler", "tokens": ["Und", "je\u00b7ne", ",", "die", "des", "Donn'\u00b7rers", "Ad\u00b7ler"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PDS", "$,", "PRELS", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und Amathusiens Rosen ehrten,", "tokens": ["Und", "A\u00b7mat\u00b7hu\u00b7siens", "Ro\u00b7sen", "ehr\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Des Heidenthums holdsinniger Name schm\u00fcckt", "tokens": ["Des", "Hei\u00b7den\u00b7thums", "hold\u00b7sin\u00b7ni\u00b7ger", "Na\u00b7me", "schm\u00fcckt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die Gl\u00fccklichen! Der kalte Gedanke, wie", "tokens": ["Die", "Gl\u00fcck\u00b7li\u00b7chen", "!", "Der", "kal\u00b7te", "Ge\u00b7dan\u00b7ke", ",", "wie"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "$.", "ART", "ADJA", "NN", "$,", "PWAV"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Empfindung, Wunsch, und Schmerz und Sehnsucht,", "tokens": ["Emp\u00b7fin\u00b7dung", ",", "Wunsch", ",", "und", "Schmerz", "und", "Sehn\u00b7sucht", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "KON", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Alles zum heitern Bild verkl\u00e4rte", "tokens": ["Al\u00b7les", "zum", "hei\u00b7tern", "Bild", "ver\u00b7kl\u00e4r\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "APPRART", "ADJA", "NN", "VVFIN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}, "stanza.7": {"line.1": {"text": "Sich ihrem frischen sch\u00f6pfrischen Geist. O Freund,", "tokens": ["Sich", "ih\u00b7rem", "fri\u00b7schen", "sch\u00f6pf\u00b7ri\u00b7schen", "Geist", ".", "O", "Freund", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PRF", "PPOSAT", "ADJA", "ADJA", "NN", "$.", "NE", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Komm, sieh und f\u00fchl's hier, offen ist Th\u00fcr' und Haus,", "tokens": ["Komm", ",", "sieh", "und", "f\u00fchl's", "hier", ",", "of\u00b7fen", "ist", "Th\u00fcr'", "und", "Haus", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVIMP", "KON", "VVFIN", "ADV", "$,", "ADJD", "VAFIN", "NE", "KON", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Komm, dich umf\u00e4ngt der S\u00e4ulen Anmuth,", "tokens": ["Komm", ",", "dich", "um\u00b7f\u00e4ngt", "der", "S\u00e4u\u00b7len", "An\u00b7muth", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPER", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dich des verschwiegnen Gemaches Sch\u00f6nheit.", "tokens": ["Dich", "des", "ver\u00b7schwieg\u00b7nen", "Ge\u00b7ma\u00b7ches", "Sch\u00f6n\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.8": {"line.1": {"text": "Sagt dir's nicht selbst die bunte gemalte Wand,", "tokens": ["Sagt", "dir's", "nicht", "selbst", "die", "bun\u00b7te", "ge\u00b7mal\u00b7te", "Wand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PTKNEG", "ADV", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der Arabesken schw\u00e4rmende Phantasie,", "tokens": ["Der", "A\u00b7ra\u00b7bes\u00b7ken", "schw\u00e4r\u00b7men\u00b7de", "Phan\u00b7ta\u00b7sie", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und all' der Bilder Lieblichkeit nicht,", "tokens": ["Und", "all'", "der", "Bil\u00b7der", "Lieb\u00b7lich\u00b7keit", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "ART", "NN", "NN", "PTKNEG", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Wie sie gef\u00fchlt und gedacht, die Vorwelt?", "tokens": ["Wie", "sie", "ge\u00b7f\u00fchlt", "und", "ge\u00b7dacht", ",", "die", "Vor\u00b7welt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVPP", "KON", "VVPP", "$,", "ART", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.9": {"line.1": {"text": "O Freund, was w\u00e4ren wir, wenn Jahrtausende", "tokens": ["O", "Freund", ",", "was", "w\u00e4\u00b7ren", "wir", ",", "wenn", "Jahr\u00b7tau\u00b7sen\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "NN", "$,", "PWS", "VAFIN", "PPER", "$,", "KOUS", "NN"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Zuvor uns dieses Himmels Azur gebl\u00fcht,", "tokens": ["Zu\u00b7vor", "uns", "die\u00b7ses", "Him\u00b7mels", "A\u00b7zur", "ge\u00b7bl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PDAT", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "D\u00e4ucht mir doch, jener bessern Zeit ist", "tokens": ["D\u00e4ucht", "mir", "doch", ",", "je\u00b7ner", "bes\u00b7sern", "Zeit", "ist"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ADV", "$,", "PDAT", "ADJA", "NN", "VAFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wenigstens unsere Freundschaft w\u00fcrdig.", "tokens": ["We\u00b7nigs\u00b7tens", "un\u00b7se\u00b7re", "Freund\u00b7schaft", "w\u00fcr\u00b7dig", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "ADJD", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.10": {"line.1": {"text": "Komm, Freund, Geleiter bin ich und F\u00fchrer dir,", "tokens": ["Komm", ",", "Freund", ",", "Ge\u00b7lei\u00b7ter", "bin", "ich", "und", "F\u00fch\u00b7rer", "dir", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word"