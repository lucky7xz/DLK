{"textgrid.poem.62954": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mit einem Blick voll heitrer Ruh,", "genre": "verse", "period": "N.A.", "pub_year": 1784, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit einem Blick voll heitrer Ruh,", "tokens": ["Mit", "ei\u00b7nem", "Blick", "voll", "hei\u00b7trer", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Erschien die junge Theonise", "tokens": ["Er\u00b7schien", "die", "jun\u00b7ge", "Theo\u00b7ni\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.3": {"text": "Auf einer bunt gestickten Wiese", "tokens": ["Auf", "ei\u00b7ner", "bunt", "ge\u00b7stick\u00b7ten", "Wie\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und schnitt sich Gras f\u00fcr ihre Kuh.", "tokens": ["Und", "schnitt", "sich", "Gras", "f\u00fcr", "ih\u00b7re", "Kuh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Voll Reitz, wie Hebe, kniet sie hier", "tokens": ["Voll", "Reitz", ",", "wie", "He\u00b7be", ",", "kniet", "sie", "hier"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "NN", "$,", "PWAV", "NE", "$,", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und singt. Schnell wand sich eine Schlange", "tokens": ["Und", "singt", ".", "Schnell", "wand", "sich", "ei\u00b7ne", "Schlan\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$.", "ADJD", "VVFIN", "PRF", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Um ihren Arm. Ihr war nicht bange,", "tokens": ["Um", "ih\u00b7ren", "Arm", ".", "Ihr", "war", "nicht", "ban\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "$.", "PPER", "VAFIN", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sie schwang die Sichel nach dem Thier.", "tokens": ["Sie", "schwang", "die", "Si\u00b7chel", "nach", "dem", "Thier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Da sprach die Schlange: t\u00f6dtst du mich,", "tokens": ["Da", "sprach", "die", "Schlan\u00b7ge", ":", "t\u00f6dtst", "du", "mich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "VVFIN", "PPER", "PRF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So lebst du zwar, doch deine Mutter", "tokens": ["So", "lebst", "du", "zwar", ",", "doch", "dei\u00b7ne", "Mut\u00b7ter"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Erbla\u00dft. Ha, rief sie, meine Mutter!", "tokens": ["Er\u00b7bla\u00dft", ".", "Ha", ",", "rief", "sie", ",", "mei\u00b7ne", "Mut\u00b7ter", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVPP", "$.", "ITJ", "$,", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und ihre Brust schlug f\u00fcrchterlich.", "tokens": ["Und", "ih\u00b7re", "Brust", "schlug", "f\u00fcrch\u00b7ter\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Sie wirft noch einen nassen Blick", "tokens": ["Sie", "wirft", "noch", "ei\u00b7nen", "nas\u00b7sen", "Blick"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nach ihrem Dach. Nun sauge, sauge!", "tokens": ["Nach", "ih\u00b7rem", "Dach", ".", "Nun", "sau\u00b7ge", ",", "sau\u00b7ge", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$.", "ADV", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Spricht sie zur Natter, schlie\u00dft ihr Auge", "tokens": ["Spricht", "sie", "zur", "Nat\u00b7ter", ",", "schlie\u00dft", "ihr", "Au\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$,", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und sinket starr ins Gras zur\u00fcck.", "tokens": ["Und", "sin\u00b7ket", "starr", "ins", "Gras", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Doch schnell erwacht sie; ihre Hand", "tokens": ["Doch", "schnell", "er\u00b7wacht", "sie", ";", "ih\u00b7re", "Hand"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "$.", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ergreift ein J\u00fcngling. Gleich den S\u00f6hnen", "tokens": ["Er\u00b7greift", "ein", "J\u00fcng\u00b7ling", ".", "Gleich", "den", "S\u00f6h\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$.", "ADV", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Des Aethers, l\u00e4chelt er, der Sch\u00f6nen,", "tokens": ["Des", "A\u00b7e\u00b7thers", ",", "l\u00e4\u00b7chelt", "er", ",", "der", "Sch\u00f6\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "PPER", "$,", "ART", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die bebend ihm zur Seite stand.", "tokens": ["Die", "be\u00b7bend", "ihm", "zur", "Sei\u00b7te", "stand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Der Spruch des Schicksals ist erf\u00fcllt;", "tokens": ["Der", "Spruch", "des", "Schick\u00b7sals", "ist", "er\u00b7f\u00fcllt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das fr\u00f6mmste Kind, so war sein Wille,", "tokens": ["Das", "fr\u00f6mms\u00b7te", "Kind", ",", "so", "war", "sein", "Wil\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADV", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Befreyt mich von der Schlangenh\u00fclle,", "tokens": ["Be\u00b7freyt", "mich", "von", "der", "Schlan\u00b7gen\u00b7h\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die lange mich gefangen hielt.", "tokens": ["Die", "lan\u00b7ge", "mich", "ge\u00b7fan\u00b7gen", "hielt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Ich bin ein Prinz, fuhr Idamant", "tokens": ["Ich", "bin", "ein", "Prinz", ",", "fuhr", "I\u00b7da\u00b7mant"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "VVFIN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu reden fort, die blauen Wellen", "tokens": ["Zu", "re\u00b7den", "fort", ",", "die", "blau\u00b7en", "Wel\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "PTKVZ", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Des Euphrats nagen an den Schwellen", "tokens": ["Des", "Eu\u00b7ph\u00b7rats", "na\u00b7gen", "an", "den", "Schwel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Des Throns, den ich nun wieder fand.", "tokens": ["Des", "Throns", ",", "den", "ich", "nun", "wie\u00b7der", "fand", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Komm, Edle, weihe mir ihn ein;", "tokens": ["Komm", ",", "Ed\u00b7le", ",", "wei\u00b7he", "mir", "ihn", "ein", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJA", "$,", "VVFIN", "PPER", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Durch dich erst kann ich gl\u00fccklich werden.", "tokens": ["Durch", "dich", "erst", "kann", "ich", "gl\u00fcck\u00b7lich", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "VMFIN", "PPER", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Heil mir! das beste Kind auf Erden", "tokens": ["Heil", "mir", "!", "das", "bes\u00b7te", "Kind", "auf", "Er\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "$.", "ART", "ADJA", "NN", "APPR", "NN"], "meter": "---+-+-+-", "measure": "unknown.measure.tri"}, "line.4": {"text": "Mu\u00df auch die beste Gattin seyn.", "tokens": ["Mu\u00df", "auch", "die", "bes\u00b7te", "Gat\u00b7tin", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Ja, Lina, Tugend darbet nie:", "tokens": ["Ja", ",", "Li\u00b7na", ",", "Tu\u00b7gend", "dar\u00b7bet", "nie", ":"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "NE", "$,", "NN", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und hat ein Gatte keine Kronen,", "tokens": ["Und", "hat", "ein", "Gat\u00b7te", "kei\u00b7ne", "Kro\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "