{"dta.poem.6314": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Hitze und von der K\u00e4lte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.85", "af:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Gluht, die, wenn sie nah', in uns die Spitzen senckt,", "tokens": ["Die", "Gluht", ",", "die", ",", "wenn", "sie", "nah'", ",", "in", "uns", "die", "Spit\u00b7zen", "senckt", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "$,", "KOUS", "PPER", "PTKVZ", "$,", "APPR", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und, da sie auf uns w\u00fcrckt, wie scharffes Eisen pfleget,", "tokens": ["Und", ",", "da", "sie", "auf", "uns", "w\u00fcrckt", ",", "wie", "scharf\u00b7fes", "Ei\u00b7sen", "pfle\u00b7get", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPER", "APPR", "PPER", "VVFIN", "$,", "PWAV", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Uns einen herben Schmertz erreget;", "tokens": ["Uns", "ei\u00b7nen", "her\u00b7ben", "Schmertz", "er\u00b7re\u00b7get", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Erregt uns, wenn sie nicht so weit von uns gelenckt,", "tokens": ["Er\u00b7regt", "uns", ",", "wenn", "sie", "nicht", "so", "weit", "von", "uns", "ge\u00b7lenckt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "PPER", "PTKNEG", "ADV", "ADJD", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Durch ihrer Hitze sanfftes Spiel,", "tokens": ["Durch", "ih\u00b7rer", "Hit\u00b7ze", "sanff\u00b7tes", "Spiel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein k\u00fctzelnd angenehm Gef\u00fchl\u2019.", "tokens": ["Ein", "k\u00fct\u00b7zelnd", "an\u00b7ge\u00b7nehm", "Ge\u00b7f\u00fchl'", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}