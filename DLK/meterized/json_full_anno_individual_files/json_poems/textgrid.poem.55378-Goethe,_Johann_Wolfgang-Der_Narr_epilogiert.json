{"textgrid.poem.55378": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Der Narr epilogiert", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Manch gutes Werk hab ich verricht',", "tokens": ["Manch", "gu\u00b7tes", "Werk", "hab", "ich", "ver\u00b7richt'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr nehmt das Lob, das kr\u00e4nkt mich nicht:", "tokens": ["Ihr", "nehmt", "das", "Lob", ",", "das", "kr\u00e4nkt", "mich", "nicht", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PDS", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich denke, da\u00df sich in der Welt", "tokens": ["Ich", "den\u00b7ke", ",", "da\u00df", "sich", "in", "der", "Welt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PRF", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Alles bald wieder ins gleiche stellt.", "tokens": ["Al\u00b7les", "bald", "wie\u00b7der", "ins", "glei\u00b7che", "stellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "ADV", "APPRART", "ADJA", "VVFIN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Lobt man mich, weil ich was Dummes gemacht,", "tokens": ["Lobt", "man", "mich", ",", "weil", "ich", "was", "Dum\u00b7mes", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PPER", "$,", 