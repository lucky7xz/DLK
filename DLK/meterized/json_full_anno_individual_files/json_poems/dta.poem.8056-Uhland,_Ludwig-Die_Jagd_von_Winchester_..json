{"dta.poem.8056": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Die Jagd von Winchester .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "K\u00f6nig Wilhelm hatt\u2019 ein\u2019 schweren Traum,", "tokens": ["K\u00f6\u00b7nig", "Wil\u00b7helm", "hatt'", "ein'", "schwe\u00b7ren", "Traum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Vom Lager sprang er auf,", "tokens": ["Vom", "La\u00b7ger", "sprang", "er", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wollt\u2019 jagen dort in Winchesters Wald,", "tokens": ["Wollt'", "ja\u00b7gen", "dort", "in", "Win\u00b7ches\u00b7ters", "Wald", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "ADV", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Rief seine Herrn zuhauf.", "tokens": ["Rief", "sei\u00b7ne", "Herrn", "zu\u00b7hauf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und als sie kamen vor den Wald,", "tokens": ["Und", "als", "sie", "ka\u00b7men", "vor", "den", "Wald", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da h\u00e4lt der K\u00f6nig still,", "tokens": ["Da", "h\u00e4lt", "der", "K\u00f6\u00b7nig", "still", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gibt Jedem einen guten Pfeil,", "tokens": ["Gibt", "Je\u00b7dem", "ei\u00b7nen", "gu\u00b7ten", "Pfeil", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wer jagen und birschen will.", "tokens": ["Wer", "ja\u00b7gen", "und", "bir\u00b7schen", "will", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVINF", "KON", "VVINF", "VMFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Der K\u00f6nig k\u00f6mmt zur hohen Eich\u2019,", "tokens": ["Der", "K\u00f6\u00b7nig", "k\u00f6mmt", "zur", "ho\u00b7hen", "Eich'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da springt ein Hirsch vorbei,", "tokens": ["Da", "springt", "ein", "Hirsch", "vor\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der K\u00f6nig spannt den Bogen schnell,", "tokens": ["Der", "K\u00f6\u00b7nig", "spannt", "den", "Bo\u00b7gen", "schnell", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch die Sehne rei\u00dft entzwei.", "tokens": ["Doch", "die", "Seh\u00b7ne", "rei\u00dft", "ent\u00b7zwei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Herr Titan besser treffen will,", "tokens": ["Herr", "Ti\u00b7tan", "bes\u00b7ser", "tref\u00b7fen", "will", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "ADJD", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Herr Titan dr\u00fcckt wohl ab,", "tokens": ["Herr", "Ti\u00b7tan", "dr\u00fcckt", "wohl", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VVFIN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er schie\u00dft dem K\u00f6nig mitten in\u2019s Herz", "tokens": ["Er", "schie\u00dft", "dem", "K\u00f6\u00b7nig", "mit\u00b7ten", "in's", "Herz"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "APPRART", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Den Pfeil, den der ihm gab.", "tokens": ["Den", "Pfeil", ",", "den", "der", "ihm", "gab", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Herr Titan fliehet durch den Wald,", "tokens": ["Herr", "Ti\u00b7tan", "flie\u00b7het", "durch", "den", "Wald", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Flieht \u00fcber Land und Meer,", "tokens": ["Flieht", "\u00fc\u00b7ber", "Land", "und", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er flieht wie ein gescheuchtes Wild,", "tokens": ["Er", "flieht", "wie", "ein", "ge\u00b7scheuch\u00b7tes", "Wild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Findt nirgends Ruhe mehr.", "tokens": ["Findt", "nir\u00b7gends", "Ru\u00b7he", "mehr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Prinz Heinrich ritt im Wald umher,", "tokens": ["Prinz", "Hein\u00b7rich", "ritt", "im", "Wald", "um\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VVFIN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Viel Reh\u2019 und Hasen er fand:", "tokens": ["Viel", "Reh'", "und", "Ha\u00b7sen", "er", "fand", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "\u201ewohl tr\u00e4f\u2019 ich gern ein edler Wild", "tokens": ["\u201e", "wohl", "tr\u00e4f'", "ich", "gern", "ein", "ed\u00b7ler", "Wild"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "ADV", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit dem Pfeil von K\u00f6nigs Hand.\u201c", "tokens": ["Mit", "dem", "Pfeil", "von", "K\u00f6\u00b7nigs", "Hand", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NN", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Da reiten schon in ernstem Zug", "tokens": ["Da", "rei\u00b7ten", "schon", "in", "erns\u00b7tem", "Zug"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die hohen Lords heran,", "tokens": ["Die", "ho\u00b7hen", "Lords", "he\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie melden ihm des K\u00f6nigs Tod,", "tokens": ["Sie", "mel\u00b7den", "ihm", "des", "K\u00f6\u00b7nigs", "Tod", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie tragen die Kron\u2019 ihm an.", "tokens": ["Sie", "tra\u00b7gen", "die", "Kron'", "ihm", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.8": {"line.1": {"text": "\u201eauf dieser trauervollen Jagd", "tokens": ["\u201e", "auf", "die\u00b7ser", "trau\u00b7er\u00b7vol\u00b7len", "Jagd"], "token_info": ["punct", "word", "word", "word", "word"], "pos": ["$(", "APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Euch reiche Beute ward,", "tokens": ["Euch", "rei\u00b7che", "Beu\u00b7te", "ward", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihr habt erjagt, gewalt\u2019ger Herr!", "tokens": ["Ihr", "habt", "er\u00b7jagt", ",", "ge\u00b7walt'\u00b7ger", "Herr", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den edeln Leopard.\u201c", "tokens": ["Den", "e\u00b7deln", "Le\u00b7o\u00b7pard", ".", "\u201c"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NE", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}