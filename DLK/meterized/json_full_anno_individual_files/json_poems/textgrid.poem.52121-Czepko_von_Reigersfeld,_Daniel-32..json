{"textgrid.poem.52121": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "32.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Schweidnitz, die du dich so ehrlich hast gewehrt,", "tokens": ["O", "Schweid\u00b7nitz", ",", "die", "du", "dich", "so", "ehr\u00b7lich", "hast", "ge\u00b7wehrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PRELS", "PPER", "PRF", "ADV", "ADJD", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und \u00f6d' und w\u00fcste bist, und keinen Arm begehrt", "tokens": ["Und", "\u00f6d'", "und", "w\u00fcs\u00b7te", "bist", ",", "und", "kei\u00b7nen", "Arm", "be\u00b7gehrt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "KON", "VVFIN", "VAFIN", "$,", "KON", "PIAT", "NN", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Zum Nachtheil deiner Pflicht die Zeit nicht auffzuheben,", "tokens": ["Zum", "Nacht\u00b7heil", "dei\u00b7ner", "Pflicht", "die", "Zeit", "nicht", "auff\u00b7zu\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wie haben mich und dich Verr\u00e4ther angegeben.", "tokens": ["Wie", "ha\u00b7ben", "mich", "und", "dich", "Ver\u00b7r\u00e4\u00b7ther", "an\u00b7ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "KON", "PPER", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Hart ist der Schlu\u00df, und schwer mu\u00df dein Verh\u00e4ngn\u00fc\u00df seyn,", "tokens": ["Hart", "ist", "der", "Schlu\u00df", ",", "und", "schwer", "mu\u00df", "dein", "Ver\u00b7h\u00e4ng\u00b7n\u00fc\u00df", "seyn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "$,", "KON", "ADJD", "VMFIN", "PPOSAT", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ihr Herren, die ihr wollt die G\u00fcter ziehen ein:", "tokens": ["Ihr", "Her\u00b7ren", ",", "die", "ihr", "wollt", "die", "G\u00fc\u00b7ter", "zie\u00b7hen", "ein", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PPER", "VMFIN", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das Schwein kan nicht gewehrn, nachdem euch so sol d\u00fcrsten,", "tokens": ["Das", "Schwein", "kan", "nicht", "ge\u00b7wehrn", ",", "nach\u00b7dem", "euch", "so", "sol", "d\u00fcrs\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PTKNEG", "VVINF", "$,", "KOUS", "PPER", "ADV", "PIAT", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Denn wie ich glaub' ihr sehnt euch nicht nach seinen W\u00fcrsten.", "tokens": ["Denn", "wie", "ich", "glaub'", "ihr", "sehnt", "euch", "nicht", "nach", "sei\u00b7nen", "W\u00fcrs\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "PPER", "VVFIN", "PPER", "PTKNEG", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "meas