{"dta.poem.21088": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "An  \n meine Schwester Marie Luise  \n zum Abschiede.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1798", "urn": "urn:nbn:de:kobv:b4-200905193300", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es schl&#228;gt! es schl&#228;gt! mein Herz ist voll,               ", "tokens": ["Es", "schl", "&#228;", "gt", "!", "es", "schl", "&#228;", "gt", "!", "mein", "Herz", "ist", "voll", ","], "token_info": ["word", "word", "XML_entity", "word", "punct", "word", "word", "XML_entity", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "VVFIN", "$.", "PPER", "ADJD", "$(", "VVFIN", "$.", "PPOSAT", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Dein Auge nebeltr\u00fcbe!", "tokens": ["Dein", "Au\u00b7ge", "ne\u00b7belt\u00b7r\u00fc\u00b7be", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Nun, s\u00fcsse Schwester, lebe wohl,", "tokens": ["Nun", ",", "s\u00fcs\u00b7se", "Schwes\u00b7ter", ",", "le\u00b7be", "wohl", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "ADJA", "NN", "$,", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und denke mein in Liebe!", "tokens": ["Und", "den\u00b7ke", "mein", "in", "Lie\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Gedenke mein, wenn Lunens Licht,", "tokens": ["Ge\u00b7den\u00b7ke", "mein", ",", "wenn", "Lu\u00b7nens", "Licht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "$,", "KOUS", "NE", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In deine Fenster flimmert,", "tokens": ["In", "dei\u00b7ne", "Fens\u00b7ter", "flim\u00b7mert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und wenn dein klares Angesicht", "tokens": ["Und", "wenn", "dein", "kla\u00b7res", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im Glanz Aurorens schimmert.", "tokens": ["Im", "Glanz", "Au\u00b7ro\u00b7rens", "schim\u00b7mert", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NE", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Gedenke mein, wenn du empor", "tokens": ["Ge\u00b7den\u00b7ke", "mein", ",", "wenn", "du", "em\u00b7por"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PPOSAT", "$,", "KOUS", "PPER", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zum Morgenhimmel blickest,", "tokens": ["Zum", "Mor\u00b7gen\u00b7him\u00b7mel", "bli\u00b7ckest", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und in des Abends grauem Flor", "tokens": ["Und", "in", "des", "A\u00b7bends", "grau\u00b7em", "Flor"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In s\u00fcssen Schlummer nickest.", "tokens": ["In", "s\u00fcs\u00b7sen", "Schlum\u00b7mer", "ni\u00b7ckest", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Denk \u00f6fter an mein fernes Land,", "tokens": ["Denk", "\u00f6f\u00b7ter", "an", "mein", "fer\u00b7nes", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "An meine Hochgestade,", "tokens": ["An", "mei\u00b7ne", "Hoch\u00b7ge\u00b7sta\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "An Wittow's meerumrauschten Strand", "tokens": ["An", "Wit\u00b7tow's", "mee\u00b7rum\u00b7rauschten", "Strand"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NE", "ADJA", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und traute Uferpfade.", "tokens": ["Und", "trau\u00b7te", "U\u00b7fer\u00b7pfa\u00b7de", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Denk \u00f6fter an den Schattengang", "tokens": ["Denk", "\u00f6f\u00b7ter", "an", "den", "Schat\u00b7ten\u00b7gang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In meinem stillen Garten,", "tokens": ["In", "mei\u00b7nem", "stil\u00b7len", "Gar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wo wir manch einsam St\u00fcndlein lang", "tokens": ["Wo", "wir", "manch", "ein\u00b7sam", "St\u00fcnd\u00b7lein", "lang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PIAT", "ADJD", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In holder D\u00e4mmrung harrten.", "tokens": ["In", "hol\u00b7der", "D\u00e4mm\u00b7rung", "harr\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Denk, wie wir \u00f6fter Hand in Hand", "tokens": ["Denk", ",", "wie", "wir", "\u00f6f\u00b7ter", "Hand", "in", "Hand"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PWAV", "PPER", "ADV", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hoch auf Arkona ruhten.", "tokens": ["Hoch", "auf", "Ar\u00b7ko\u00b7na", "ruh\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NE", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wie klang der hochgebirgte Strand!", "tokens": ["Wie", "klang", "der", "hoch\u00b7ge\u00b7birg\u00b7te", "Strand", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie donnerten die Fluthen!", "tokens": ["Wie", "don\u00b7ner\u00b7ten", "die", "Flut\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Gedenk' an mich. Doch \u00f6fter noch", "tokens": ["Ge\u00b7denk'", "an", "mich", ".", "Doch", "\u00f6f\u00b7ter", "noch"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "APPR", "PPER", "$.", "KON", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gedenk' an Freundin Tugend;", "tokens": ["Ge\u00b7denk'", "an", "Freun\u00b7din", "Tu\u00b7gend", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ihr Arm ist sanft, und s\u00fcss ihr Joch,", "tokens": ["Ihr", "Arm", "ist", "sanft", ",", "und", "s\u00fcss", "ihr", "Joch", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,", "KON", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und hold ist sie der Jugend.", "tokens": ["Und", "hold", "ist", "sie", "der", "Ju\u00b7gend", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Sey kalt, wie Eis! Sey rein, wie Schnee!", "tokens": ["Sey", "kalt", ",", "wie", "Eis", "!", "Sey", "rein", ",", "wie", "Schnee", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "$,", "PWAV", "NN", "$.", "VAIMP", "ADJD", "$,", "PWAV", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie Veilchen sey dem\u00fcthig!", "tokens": ["Wie", "Veil\u00b7chen", "sey", "de\u00b7m\u00fct\u00b7hig", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sey mild, wie Milch, rasch, wie ein Reh,", "tokens": ["Sey", "mild", ",", "wie", "Milch", ",", "rasch", ",", "wie", "ein", "Reh", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "$,", "PWAV", "NN", "$,", "ADJD", "$,", "PWAV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie L\u00e4mmchen fromm und g\u00fctig!", "tokens": ["Wie", "L\u00e4mm\u00b7chen", "fromm", "und", "g\u00fc\u00b7tig", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Sey r\u00fcstig, deine Pflicht zu thun,", "tokens": ["Sey", "r\u00fcs\u00b7tig", ",", "dei\u00b7ne", "Pflicht", "zu", "thun", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "$,", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu handeln, dulden, lieben!", "tokens": ["Zu", "han\u00b7deln", ",", "dul\u00b7den", ",", "lie\u00b7ben", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "VVINF", "$,", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Auch M\u00e4gdlein m\u00f6gen Thaten thun,", "tokens": ["Auch", "M\u00e4gd\u00b7lein", "m\u00f6\u00b7gen", "Tha\u00b7ten", "thun", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und heilge Pflichten \u00fcben!", "tokens": ["Und", "heil\u00b7ge", "Pflich\u00b7ten", "\u00fc\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Sey fromm, sey fromm und f\u00fcrchte Gott!", "tokens": ["Sey", "fromm", ",", "sey", "fromm", "und", "f\u00fcrch\u00b7te", "Gott", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "$,", "VAFIN", "ADJD", "KON", "VVFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und halt, halt' an im Glauben,", "tokens": ["Und", "halt", ",", "halt'", "an", "im", "Glau\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "VVFIN", "APPR", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und lass nicht frecher Frevler Spott", "tokens": ["Und", "lass", "nicht", "fre\u00b7cher", "Frev\u00b7ler", "Spott"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PTKNEG", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dir deine Krone rauben!", "tokens": ["Dir", "dei\u00b7ne", "Kro\u00b7ne", "rau\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Und wird dir eng und bang ums Herz", "tokens": ["Und", "wird", "dir", "eng", "und", "bang", "ums", "Herz"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "ADJD", "KON", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von Ahnung, Sehnsucht, Reue,", "tokens": ["Von", "Ah\u00b7nung", ",", "Sehn\u00b7sucht", ",", "Reu\u00b7e", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und presst dich tiefverschwiegner Schmerz,", "tokens": ["Und", "presst", "dich", "tief\u00b7ver\u00b7schwieg\u00b7ner", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So tritt hinaus ins Freye!", "tokens": ["So", "tritt", "hin\u00b7aus", "ins", "Frey\u00b7e", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Schau auf! schau auf ins Vaterland!", "tokens": ["Schau", "auf", "!", "schau", "auf", "ins", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$.", "ADJD", "APPR", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Vaterland ist droben!", "tokens": ["Das", "Va\u00b7ter\u00b7land", "ist", "dro\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Was irdisch ist, ist Traum und Tand;", "tokens": ["Was", "ir\u00b7disch", "ist", ",", "ist", "Traum", "und", "Tand", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VAFIN", "$,", "VAFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Was bleibend letzt, ist oben.", "tokens": ["Was", "blei\u00b7bend", "letzt", ",", "ist", "o\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "ADJD", "ADV", "$,", "VAFIN", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Und oben ist auch Wiedersehn \u2014", "tokens": ["Und", "o\u00b7ben", "ist", "auch", "Wie\u00b7der\u00b7sehn"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "ADV", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Drum weine nicht, Luise!", "tokens": ["Drum", "wei\u00b7ne", "nicht", ",", "Lu\u00b7i\u00b7se", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PAV", "VVFIN", "PTKNEG", "$,", "NE", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Uns winkt, uns l\u00e4chelt Wiedersehn", "tokens": ["Uns", "winkt", ",", "uns", "l\u00e4\u00b7chelt", "Wie\u00b7der\u00b7sehn"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im sch\u00f6nern Paradiese!", "tokens": ["Im", "sch\u00f6\u00b7nern", "Pa\u00b7ra\u00b7die\u00b7se", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}