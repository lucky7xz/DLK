{"textgrid.poem.58602": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "34.", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gib Herz und Willen ganz, la\u00df Gott als Herrn drin schalten,", "tokens": ["Gib", "Herz", "und", "Wil\u00b7len", "ganz", ",", "la\u00df", "Gott", "als", "Herrn", "drin", "schal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "KON", "NN", "ADV", "$,", "VVIMP", "NN", "KOUS", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Man mu\u00df nicht raten mit, nein, Gott ist Herr allein!", "tokens": ["Man", "mu\u00df", "nicht", "ra\u00b7ten", "mit", ",", "nein", ",", "Gott", "ist", "Herr", "al\u00b7lein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PTKNEG", "VVFIN", "PTKVZ", "$,", "PTKANT", "$,", "NN", "VAFIN", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Welt, du verlierst dein Recht und die es mit dir halten,", "tokens": ["Welt", ",", "du", "ver\u00b7lierst", "dein", "Recht", "und", "die", "es", "mit", "dir", "hal\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "KON", "PRELS", "PPER", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mein Herr, befehle nur, ich will dein Sklave sein!", "tokens": ["Mein", "Herr", ",", "be\u00b7feh\u00b7le", "nur", ",", "ich", "will", "dein", "Skla\u00b7ve", "sein", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "ADV", "$,", "PPER", "VMFIN", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ach, la\u00df mich unverr\u00fcckt von dir abh\u00e4ngig leben", "tokens": ["Ach", ",", "la\u00df", "mich", "un\u00b7ver\u00b7r\u00fcckt", "von", "dir", "ab\u00b7h\u00e4n\u00b7gig", "le\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "VVIMP", "PPER", "ADJD", "APPR", "PPER", "ADJD", "VVINF"], "meter": "-+-+-+-+++-+-", "measure": "unknown.measure.septa"}, "line.6": {"text": "Und Licht und Finsternis von dir nur nehmen an!", "tokens": ["Und", "Licht", "und", "Fins\u00b7ter\u00b7nis", "von", "dir", "nur", "neh\u00b7men", "an", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "APPR", "PPER", "ADV", "VVINF", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Das \u00dcbel l\u00e4\u00dft du zu, den Frieden willst du geben,", "tokens": ["Das", "\u00dc\u00b7bel", "l\u00e4\u00dft", "du", "zu", ",", "den", "Frie\u00b7den", "willst", "du", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ", "$,", "ART", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Drum spricht mein Glaube stets: Der Herr, der hat's getan.", "tokens": ["Drum", "spricht", "mein", "Glau\u00b7be", "stets", ":", "Der", "Herr", ",", "der", "hat's", "ge\u00b7tan", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPOSAT", "NN", "AD