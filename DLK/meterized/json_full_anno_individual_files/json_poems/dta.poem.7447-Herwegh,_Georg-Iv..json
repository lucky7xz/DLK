{"dta.poem.7447": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Iv.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192430", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Manch b\u00f6ser Geist haust in Helvetiens Schl\u00fcnden,", "tokens": ["Manch", "b\u00f6\u00b7ser", "Geist", "haust", "in", "Hel\u00b7ve\u00b7ti\u00b7ens", "Schl\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "APPR", "NE", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Manch schlimmer Pfaffe keucht den Berg hinan,", "tokens": ["Manch", "schlim\u00b7mer", "Pfaf\u00b7fe", "keucht", "den", "Berg", "hi\u00b7nan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Teufel bricht sich mit dem Kreuze Bahn,", "tokens": ["Der", "Teu\u00b7fel", "bricht", "sich", "mit", "dem", "Kreu\u00b7ze", "Bahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der Teufel in den frommen Thalesgr\u00fcnden.", "tokens": ["Der", "Teu\u00b7fel", "in", "den", "from\u00b7men", "Tha\u00b7les\u00b7gr\u00fcn\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Doch lieb' ich sie mit allen ihren S\u00fcnden.", "tokens": ["Doch", "lieb'", "ich", "sie", "mit", "al\u00b7len", "ih\u00b7ren", "S\u00fcn\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "APPR", "PIAT", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ha! klebt nicht Winkelriedens Blut daran?", "tokens": ["Ha", "!", "klebt", "nicht", "Win\u00b7kel\u00b7rie\u00b7dens", "Blut", "da\u00b7ran", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVFIN", "PTKNEG", "NN", "NN", "PAV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Hier ist die W\u00fcste und das Canaan,", "tokens": ["Hier", "ist", "die", "W\u00fcs\u00b7te", "und", "das", "Ca\u00b7naan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "KON", "ART", "NE", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Um ein Profet der Welt das Heil zu k\u00fcnden.", "tokens": ["Um", "ein", "Pro\u00b7fet", "der", "Welt", "das", "Heil", "zu", "k\u00fcn\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Hier ", "tokens": ["Hier"], "token_info": ["word"], "pos": ["ADV"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Hier ", "tokens": ["Hier"], "token_info": ["word"], "pos": ["ADV"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Was willst Du tot sie und gefangen sehn?", "tokens": ["Was", "willst", "Du", "tot", "sie", "und", "ge\u00b7fan\u00b7gen", "sehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "VVFIN", "PPER", "KON", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "O la\u00df den Traum, an den der J\u00fcngling glaubte,", "tokens": ["O", "la\u00df", "den", "Traum", ",", "an", "den", "der", "J\u00fcng\u00b7ling", "glaub\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "$,", "APPR", "ART", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vergi\u00df, wo frische Alpenrosen stehn,", "tokens": ["Ver\u00b7gi\u00df", ",", "wo", "fri\u00b7sche", "Al\u00b7pen\u00b7ro\u00b7sen", "stehn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "PWAV", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der deutschen Freiheit Rose, die bestaubte!", "tokens": ["Der", "deut\u00b7schen", "Frei\u00b7heit", "Ro\u00b7se", ",", "die", "be\u00b7staub\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NE", "$,", "PRELS", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}