{"dta.poem.9966": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "75.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Vielmals mu\u00df ich in das wasser/ wen\u0303 dem menschen es gef\u00e4llt/", "tokens": ["Viel\u00b7mals", "mu\u00df", "ich", "in", "das", "was\u00b7ser", "/", "we\u00f1", "dem", "men\u00b7schen", "es", "ge\u00b7f\u00e4llt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "ART", "ADJA", "$(", "KOUS", "ART", "ADJA", "PPER", "VVPP", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Jhrer viele\u0303 ists ein wunder/ da\u00df das schw\u00e4ch das st\u00e4rckste h\u00e4lt.", "tokens": ["Ih\u00b7rer", "viel\u1ebd", "ists", "ein", "wun\u00b7der", "/", "da\u00df", "das", "schw\u00e4ch", "das", "st\u00e4rcks\u00b7te", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "$(", "KOUS", "PDS", "VVFIN", "ART", "ADJA", "VVFIN", "$."], "meter": "+--+-+-+-+-+-+", "measure": "iambic.septa.invert"}, "line.3": {"text": "Wenn den feind ich fassen kan/ wird er mir nichts abgewin\u0303en/", "tokens": ["Wenn", "den", "feind", "ich", "fas\u00b7sen", "kan", "/", "wird", "er", "mir", "nichts", "ab\u00b7ge\u00b7wi\u00f1en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "VVINF", "VMFIN", "$(", "VAFIN", "PPER", "PPER", "PIS", "VVPP", "$("], "meter": "--+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und er f\u00e4sselt sich nur mehr/ wenn er dencket zu entrinnen.", "tokens": ["Und", "er", "f\u00e4s\u00b7selt", "sich", "nur", "mehr", "/", "wenn", "er", "den\u00b7cket", "zu", "ent\u00b7rin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PRF", "ADV", "ADV", "$(", "KOUS", "PPER", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-++--+-+-+-+-+-", "measure": "iambic.septa.relaxed"}}}}}