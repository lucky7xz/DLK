{"dta.poem.4065": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905197104", "language": ["de:0.99"], "booktitle": "H\u00f6lderlin, Friedrich: Gedichte. Stuttgart u. a., 1826."}, "poem": {"stanza.1": {"line.1": {"text": "Wieder ein Gl\u00fcck erlebt! Die gef\u00e4hrliche D\u00fcrre", "tokens": ["Wie\u00b7der", "ein", "Gl\u00fcck", "er\u00b7lebt", "!", "Die", "ge\u00b7f\u00e4hr\u00b7li\u00b7che", "D\u00fcr\u00b7re"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VVPP", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "geneset,", "tokens": ["ge\u00b7ne\u00b7set", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Und die Sch\u00e4rfe des Lichts senget die Bl\u00fcthe", "tokens": ["Und", "die", "Sch\u00e4r\u00b7fe", "des", "Lichts", "sen\u00b7get", "die", "Bl\u00fc\u00b7the"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ART", "NN", "VVFIN", "ART", "NN"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "nicht mehr,", "tokens": ["nicht", "mehr", ","], "token_info": ["word", "word", "punct"], "pos": ["PTKNEG", "ADV", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "Offen steht jetzt wieder ein Saal, und gesund ist", "tokens": ["Of\u00b7fen", "steht", "jetzt", "wie\u00b7der", "ein", "Saal", ",", "und", "ge\u00b7sund", "ist"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "ADV", "ADV", "ART", "NN", "$,", "KON", "ADJD", "VAFIN"], "meter": "+-+-+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "der Garten,", "tokens": ["der", "Gar\u00b7ten", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Und von Regen erfrischt rauschet das gl\u00e4nzende", "tokens": ["Und", "von", "Re\u00b7gen", "er\u00b7frischt", "rau\u00b7schet", "das", "gl\u00e4n\u00b7zen\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "VVFIN", "VVFIN", "ART", "ADJA"], "meter": "--+--++--+--", "measure": "anapaest.di.plus"}, "line.8": {"text": "Thal", "tokens": ["Thal"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.9": {"text": "Hoch von Gew\u00e4chsen, es schwellen die B\u00e4ch', und", "tokens": ["Hoch", "von", "Ge\u00b7w\u00e4ch\u00b7sen", ",", "es", "schwel\u00b7len", "die", "B\u00e4ch'", ",", "und"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["ADJD", "APPR", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "$,", "KON"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.10": {"text": "alle gebund'nen", "tokens": ["al\u00b7le", "ge\u00b7bun\u00b7d'\u00b7nen"], "token_info": ["word", "word"], "pos": ["PIAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.11": {"text": "Fittige wagen sich wieder in's Reich des Gesangs.", "tokens": ["Fit\u00b7ti\u00b7ge", "wa\u00b7gen", "sich", "wie\u00b7der", "in's", "Reich", "des", "Ge\u00b7sangs", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "ADV", "APPRART", "NN", "ART", "NN", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.12": {"text": "Voll ist die Luft von Fr\u00f6hlichen jetzt, und die Stadt", "tokens": ["Voll", "ist", "die", "Luft", "von", "Fr\u00f6h\u00b7li\u00b7chen", "jetzt", ",", "und", "die", "Stadt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "ART", "NN", "APPR", "NN", "ADV", "$,", "KON", "ART", "NN"], "meter": "+--+-+--+--+", "measure": "iambic.penta.invert"}, "line.13": {"text": "und der Hain ist", "tokens": ["und", "der", "Hain", "ist"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VAFIN"], "meter": "--+-", "measure": "anapaest.init"}, "line.14": {"text": "Rings von zufriedenen Kindern des Himmels", "tokens": ["Rings", "von", "zu\u00b7frie\u00b7de\u00b7nen", "Kin\u00b7dern", "des", "Him\u00b7mels"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN", "ART", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.15": {"text": "erf\u00fcllt.", "tokens": ["er\u00b7f\u00fcllt", "."], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-+", "measure": "iambic.single"}, "line.16": {"text": "Gerne begegnen sie sich und irren unter einander,", "tokens": ["Ger\u00b7ne", "be\u00b7geg\u00b7nen", "sie", "sich", "und", "ir\u00b7ren", "un\u00b7ter", "ein\u00b7an\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "KON", "VVFIN", "APPR", "PRF", "$,"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.17": {"text": "Sorgenlos und es scheint keines zu wenig, zu", "tokens": ["Sor\u00b7gen\u00b7los", "und", "es", "scheint", "kei\u00b7nes", "zu", "we\u00b7nig", ",", "zu"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADJD", "KON", "PPER", "VVFIN", "PIS", "APPR", "PIS", "$,", "APPR"], "meter": "+-+---+--+-+", "measure": "trochaic.penta.relaxed"}, "line.18": {"text": "viel.", "tokens": ["viel", "."], "token_info": ["word", "punct"], "pos": ["ADV", "$."], "meter": "+", "measure": "single.up"}, "line.19": {"text": "Denn so ordnet das Herz es an, und zu athmen", "tokens": ["Denn", "so", "ord\u00b7net", "das", "Herz", "es", "an", ",", "und", "zu", "ath\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "PPER", "PTKVZ", "$,", "KON", "PTKZU", "VVINF"], "meter": "--+--+-+--+-", "measure": "anapaest.di.plus"}, "line.20": {"text": "die Anmuth,", "tokens": ["die", "An\u00b7muth", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.21": {"text": "Sie, die geschickliche, schenkt ihnen ein g\u00f6ttlicher", "tokens": ["Sie", ",", "die", "ge\u00b7schick\u00b7li\u00b7che", ",", "schenkt", "ih\u00b7nen", "ein", "g\u00f6tt\u00b7li\u00b7cher"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "$,", "ART", "ADJA", "$,", "VVFIN", "PPER", "ART", "ADJA"], "meter": "-+-+---+--+--", "measure": "iambic.tetra.relaxed"}, "line.22": {"text": "Geist.", "tokens": ["Geist", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.23": {"text": "Aber die Wanderer auch sind wohl geleitet und", "tokens": ["A\u00b7ber", "die", "Wan\u00b7de\u00b7rer", "auch", "sind", "wohl", "ge\u00b7lei\u00b7tet", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADV", "VAFIN", "ADV", "VVPP", "KON"], "meter": "+--+--+-+-+-+", "measure": "dactylic.di.plus"}, "line.24": {"text": "haben", "tokens": ["ha\u00b7ben"], "token_info": ["word"], "pos": ["VAFIN"], "meter": "+-", "measure": "trochaic.single"}, "line.25": {"text": "Kr\u00e4nze genug und Gesang, haben den heiligen Stab", "tokens": ["Kr\u00e4n\u00b7ze", "ge\u00b7nug", "und", "Ge\u00b7sang", ",", "ha\u00b7ben", "den", "hei\u00b7li\u00b7gen", "Stab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "ADV", "KON", "NN", "$,", "VAFIN", "ART", "ADJA", "NN"], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.26": {"text": "Voll geschm\u00fcckt mit Trauben und Laub, bei sich,", "tokens": ["Voll", "ge\u00b7schm\u00fcckt", "mit", "Trau\u00b7ben", "und", "Laub", ",", "bei", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "APPR", "NN", "KON", "NN", "$,", "APPR", "PRF", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.27": {"text": "und der Fichte", "tokens": ["und", "der", "Fich\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["KON", "ART", "NN"], "meter": "--+-", "measure": "anapaest.init"}, "line.28": {"text": "Schatten; von Dorfe zu Dorf jauchzt es, von", "tokens": ["Schat\u00b7ten", ";", "von", "Dor\u00b7fe", "zu", "Dorf", "jauchzt", "es", ",", "von"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["NN", "$.", "APPR", "NN", "APPR", "NN", "VVFIN", "PPER", "$,", "APPR"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.29": {"text": "Tage zu Tag,", "tokens": ["Ta\u00b7ge", "zu", "Tag", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.30": {"text": "Und wie Wagen, bespannt mit freiem Wilde, so", "tokens": ["Und", "wie", "Wa\u00b7gen", ",", "be\u00b7spannt", "mit", "frei\u00b7em", "Wil\u00b7de", ",", "so"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "PWAV", "NN", "$,", "VVFIN", "APPR", "ADJA", "NN", "$,", "ADV"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.31": {"text": "ziehn die", "tokens": ["ziehn", "die"], "token_info": ["word", "word"], "pos": ["VVFIN", "ART"], "meter": "+-", "measure": "trochaic.single"}, "line.32": {"text": "Berge voran, und so tr\u00e4get und eilet der Pfad.", "tokens": ["Ber\u00b7ge", "vo\u00b7ran", ",", "und", "so", "tr\u00e4\u00b7get", "und", "ei\u00b7let", "der", "Pfad", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "KON", "ADV", "VVFIN", "KON", "VVFIN", "ART", "NN", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}}}}}