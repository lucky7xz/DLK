{"dta.poem.2581": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Cupido.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es sey/ ein Kauffmann mu\u00df durch Hitze Frost und Schnee", "tokens": ["Es", "sey", "/", "ein", "Kauf\u00b7fmann", "mu\u00df", "durch", "Hit\u00b7ze", "Frost", "und", "Schnee"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$(", "ART", "NN", "VMFIN", "APPR", "NN", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Durch Unruh und Gefahr in frembde L\u00e4nder reisen.", "tokens": ["Durch", "Un\u00b7ruh", "und", "Ge\u00b7fahr", "in", "fremb\u00b7de", "L\u00e4n\u00b7der", "rei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ist mein ergebnes Volck nicht seliger zu preisen?", "tokens": ["Ist", "mein", "er\u00b7geb\u00b7nes", "Volck", "nicht", "se\u00b7li\u00b7ger", "zu", "prei\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN", "PTKNEG", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Das ruht bey einem Baum/ das schl\u00e4fft in bundtem Klee/", "tokens": ["Das", "ruht", "bey", "ei\u00b7nem", "Baum", "/", "das", "schl\u00e4fft", "in", "bund\u00b7tem", "Klee", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "ART", "NN", "$(", "PDS", "VVFIN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Treibt kein Gewerbe nicht/ wei\u00df Wechsel nicht zu schl\u00fcssen/", "tokens": ["Treibt", "kein", "Ge\u00b7wer\u00b7be", "nicht", "/", "wei\u00df", "Wech\u00b7sel", "nicht", "zu", "schl\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "PTKNEG", "$(", "VVFIN", "NN", "PTKNEG", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Das beste ", "tokens": ["Das", "bes\u00b7te"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}}}}}