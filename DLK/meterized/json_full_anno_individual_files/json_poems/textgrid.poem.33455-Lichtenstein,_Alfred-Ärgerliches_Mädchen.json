{"textgrid.poem.33455": {"metadata": {"author": {"name": "Lichtenstein, Alfred", "birth": "N.A.", "death": "N.A."}, "title": "\u00c4rgerliches M\u00e4dchen", "genre": "verse", "period": "N.A.", "pub_year": 1913, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es ist schon sp\u00e4t. Ich mu\u00df verdienen.", "tokens": ["Es", "ist", "schon", "sp\u00e4t", ".", "Ich", "mu\u00df", "ver\u00b7die\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$.", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Aber die gehn heute alle vorbei mit blasierten Mienen.", "tokens": ["A\u00b7ber", "die", "gehn", "heu\u00b7te", "al\u00b7le", "vor\u00b7bei", "mit", "bla\u00b7sier\u00b7ten", "Mie\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "ADV", "PIS", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "+--+--+-+-+-+-+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "Nicht einen Gl\u00fccksgroschen wolln sie mir geben.", "tokens": ["Nicht", "ei\u00b7nen", "Gl\u00fccks\u00b7gro\u00b7schen", "wolln", "sie", "mir", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "VMFIN", "PPER", "PPER", "VVINF", "$."], "meter": "-+-++-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Es ist ein j\u00e4mmerliches Leben.", "tokens": ["Es", "ist", "ein", "j\u00e4m\u00b7mer\u00b7li\u00b7ches", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Komme ich ohne Geld nach Haus,", "tokens": ["Kom\u00b7me", "ich", "oh\u00b7ne", "Geld", "nach", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wirft mich die Alte hinaus.", "tokens": ["Wirft", "mich", "die", "Al\u00b7te", "hin\u00b7aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.7": {"text": "Fast kein Mensch ist auf der Stra\u00dfe mehr.", "tokens": ["Fast", "kein", "Mensch", "ist", "auf", "der", "Stra\u00b7\u00dfe", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VAFIN", "APPR", "ART", "NN", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Ich bin todm\u00fcde und friere sehr.", "tokens": ["Ich", "bin", "tod\u00b7m\u00fc\u00b7de", "und", "frie\u00b7re", "sehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "KON", "ADJA", "ADV", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "So elend zumute war mir noch nie.", "tokens": ["So", "e\u00b7lend", "zu\u00b7mu\u00b7te", "war", "mir", "noch", "nie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "VAFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ich laufe umher wie ein St\u00fcck Vieh.", "tokens": ["Ich", "lau\u00b7fe", "um\u00b7her", "wie", "ein", "St\u00fcck", "Vieh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "KOKOM", "ART", "NN", "NN", "$."], "meter": "-+--+-+++", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Da endlich kommt dr\u00fcben einer an:", "tokens": ["Da", "end\u00b7lich", "kommt", "dr\u00fc\u00b7ben", "ei\u00b7ner", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ADV", "PIS", "PTKVZ", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ein ganz anst\u00e4ndig angezogener Mann \u2013", "tokens": ["Ein", "ganz", "an\u00b7st\u00e4n\u00b7dig", "an\u00b7ge\u00b7zo\u00b7ge\u00b7ner", "Mann", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "ADJA", "NN", "$("], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.5": {"text": "Doch auf das \u00c4u\u00dfere darf man in diesem Leben", "tokens": ["Doch", "auf", "das", "\u00c4u\u00b7\u00dfe\u00b7re", "darf", "man", "in", "die\u00b7sem", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VMFIN", "PIS", "APPR", "PDAT", "NN"], "meter": "-+-+-++--+-+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Nicht viel geben.", "tokens": ["Nicht", "viel", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Er ist auch schon \u00e4lter. [Die haben mehr Geld,", "tokens": ["Er", "ist", "auch", "schon", "\u00e4l\u00b7ter", ".", "Die", "ha\u00b7ben", "mehr", "Geld", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ADJD", "$.", "$(", "PDS", "VAFIN", "PIAT", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Von den Jungen wird man eher geprellt.]", "tokens": ["Von", "den", "Jun\u00b7gen", "wird", "man", "e\u00b7her", "ge\u00b7prellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "PIS", "ADV", "VVPP", "$.", "$("], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}, "line.9": {"text": "Er ist mir vis-\u00e0-vis.", "tokens": ["Er", "ist", "mir", "vis\u00b7\u00e0\u00b7vis", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Ich heb die Kleddage bis \u00fcber das Knie.", "tokens": ["Ich", "heb", "die", "Kled\u00b7da\u00b7ge", "bis", "\u00fc\u00b7ber", "das", "Knie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.11": {"text": "Ich kann mir dies leisten.", "tokens": ["Ich", "kann", "mir", "dies", "leis\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PDS", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.12": {"text": "Es zieht am meisten.", "tokens": ["Es", "zieht", "am", "meis\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "PIS", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.13": {"text": "Die Kerle kommen wie Fliegen", "tokens": ["Die", "Ker\u00b7le", "kom\u00b7men", "wie", "Flie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVINF", "KOKOM", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.14": {"text": "Ins Licht zu uns Ziegen ...", "tokens": ["Ins", "Licht", "zu", "uns", "Zie\u00b7gen", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "PPER", "NN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.15": {"text": "Der Kavalier bleibt wirklich dr\u00fcben stehen.", "tokens": ["Der", "Ka\u00b7va\u00b7lier", "bleibt", "wirk\u00b7lich", "dr\u00fc\u00b7ben", "ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Er glotzt. Er winkt. Ich will schon bei ihm hingehn ...", "tokens": ["Er", "glotzt", ".", "Er", "winkt", ".", "Ich", "will", "schon", "bei", "ihm", "hin\u00b7gehn", "..."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VVFIN", "$.", "PPER", "VMFIN", "ADV", "APPR", "PPER", "VVINF", "$("], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.17": {"text": "Ich denke: der wird mir ein gro\u00dfes Goldst\u00fcck schenken.", "tokens": ["Ich", "den\u00b7ke", ":", "der", "wird", "mir", "ein", "gro\u00b7\u00dfes", "Gold\u00b7st\u00fcck", "schen\u00b7ken", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "ART", "VAFIN", "PPER", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.18": {"text": "Dann besauf ich mich heimlich mit teuren Getr\u00e4nken.", "tokens": ["Dann", "be\u00b7sauf", "ich", "mich", "heim\u00b7lich", "mit", "teu\u00b7ren", "Ge\u00b7tr\u00e4n\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.19": {"text": "Das ist noch das sch\u00f6nste: einmal \u2013 allein", "tokens": ["Das", "ist", "noch", "das", "sch\u00f6ns\u00b7te", ":", "ein\u00b7mal", "\u2013", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["PDS", "VAFIN", "ADV", "ART", "ADJA", "$.", "ADV", "$(", "ADV"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.20": {"text": "Still f\u00fcr sich besoffen sein \u2013", "tokens": ["Still", "f\u00fcr", "sich", "be\u00b7sof\u00b7fen", "sein", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PRF", "VVPP", "VAINF", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Oder ich kann neue Schuhe kaufen ...", "tokens": ["O\u00b7der", "ich", "kann", "neu\u00b7e", "Schu\u00b7he", "kau\u00b7fen", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "ADJA", "NN", "VVINF", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Mu\u00df nicht mehr in gestopften Str\u00fcmpfen laufen \u2013", "tokens": ["Mu\u00df", "nicht", "mehr", "in", "ge\u00b7stopf\u00b7ten", "Str\u00fcmp\u00b7fen", "lau\u00b7fen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PTKNEG", "ADV", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Oder ... ich geh einmal nicht auf den Bummel hinaus.", "tokens": ["O\u00b7der", "...", "ich", "geh", "ein\u00b7mal", "nicht", "auf", "den", "Bum\u00b7mel", "hin\u00b7aus", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$(", "PPER", "VVFIN", "ADV", "PTKNEG", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.4": {"text": "Und ruhe mich von den Kerlen aus \u2013", "tokens": ["Und", "ru\u00b7he", "mich", "von", "den", "Ker\u00b7len", "aus", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ART", "NN", "PTKVZ", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Oder ... ach, ich freu mich schon so ...", "tokens": ["O\u00b7der", "...", "ach", ",", "ich", "freu", "mich", "schon", "so", "..."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$(", "ITJ", "$,", "PPER", "VVFIN", "PPER", "ADV", "ADV", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.6": {"text": "Ich bin so froh \u2013", "tokens": ["Ich", "bin", "so", "froh", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Da kommt die Kitti an.", "tokens": ["Da", "kommt", "die", "Kit\u00b7ti", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Und versaut den Mann.", "tokens": ["Und", "ver\u00b7saut", "den", "Mann", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Es ist schon sp\u00e4t. Ich mu\u00df verdienen.", "tokens": ["Es", "ist", "schon", "sp\u00e4t", ".", "Ich", "mu\u00df", "ver\u00b7die\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$.", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Aber die gehn heute alle vorbei mit blasierten Mienen.", "tokens": ["A\u00b7ber", "die", "gehn", "heu\u00b7te", "al\u00b7le", "vor\u00b7bei", "mit", "bla\u00b7sier\u00b7ten", "Mie\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "ADV", "PIS", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "+--+--+-+-+-+-+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "Nicht einen Gl\u00fccksgroschen wolln sie mir gebe