{"dta.poem.21138": {"metadata": {"author": {"name": "Zachariae, Justus Friedrich Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "An die Liebe .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-20676-2", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Liebe, du G\u00f6ttin z\u00e4rtlicher Schmerzen", "tokens": ["Lie\u00b7be", ",", "du", "G\u00f6t\u00b7tin", "z\u00e4rt\u00b7li\u00b7cher", "Schmer\u00b7zen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PPER", "NN", "ADJA", "NN"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "In unsern jungen f\u00fchlenden Herzen,", "tokens": ["In", "un\u00b7sern", "jun\u00b7gen", "f\u00fch\u00b7len\u00b7den", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "La\u00df mir, holde Liebe,", "tokens": ["La\u00df", "mir", ",", "hol\u00b7de", "Lie\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Meine Traurigkeit.", "tokens": ["Mei\u00b7ne", "Trau\u00b7rig\u00b7keit", "."], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Wenn ich mich betr\u00fcbe,", "tokens": ["Wenn", "ich", "mich", "be\u00b7tr\u00fc\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Ehret dich mein Leid.", "tokens": ["Eh\u00b7ret", "dich", "mein", "Leid", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Einsame Thr\u00e4nen liebender Jugend", "tokens": ["Ein\u00b7sa\u00b7me", "Thr\u00e4\u00b7nen", "lie\u00b7ben\u00b7der", "Ju\u00b7gend"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Sind oft die Zeichen h\u00f6herer Tugend,", "tokens": ["Sind", "oft", "die", "Zei\u00b7chen", "h\u00f6\u00b7he\u00b7rer", "Tu\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Als des Weisen Lehren,", "tokens": ["Als", "des", "Wei\u00b7sen", "Leh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Der in W\u00fcsten flieht;", "tokens": ["Der", "in", "W\u00fcs\u00b7ten", "flieht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Und das Schwerdt vor Heeren,", "tokens": ["Und", "das", "Schwerdt", "vor", "Hee\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Das zum Siege zieht.", "tokens": ["Das", "zum", "Sie\u00b7ge", "zieht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Liebe, du bildest Herzen von neuen.", "tokens": ["Lie\u00b7be", ",", "du", "bil\u00b7dest", "Her\u00b7zen", "von", "neu\u00b7en", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "NN", "APPR", "ADJA", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Z\u00e4rtliche T\u00f6ne will ich dir weihen.", "tokens": ["Z\u00e4rt\u00b7li\u00b7che", "T\u00f6\u00b7ne", "will", "ich", "dir", "wei\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VMFIN", "PPER", "PPER", "VVINF", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Da\u00df mein Herz empfunden,", "tokens": ["Da\u00df", "mein", "Herz", "emp\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Das verdank ich dir.", "tokens": ["Das", "ver\u00b7dank", "ich", "dir", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PPER", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Und auch tr\u00fcbe Stunden,", "tokens": ["Und", "auch", "tr\u00fc\u00b7be", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Liebe, sende mir!", "tokens": ["Lie\u00b7be", ",", "sen\u00b7de", "mir", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}