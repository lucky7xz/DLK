{"dta.poem.9108": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1898", "urn": "urn:nbn:de:kobv:b4-200905192685", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dann losch das Licht,", "tokens": ["Dann", "losch", "das", "Licht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "und durch die Stille", "tokens": ["und", "durch", "die", "Stil\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "nur noch dein Herzschlag . . .", "tokens": ["nur", "noch", "dein", "Herz\u00b7schlag", ".", ".", "."], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ADV", "ADV", "PPOSAT", "NN", "$.", "$.", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Seligkeit!", "tokens": ["Se\u00b7lig\u00b7keit", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "Im Garten, fr\u00fchauf, pfiff ein Vogel,", "tokens": ["Im", "Gar\u00b7ten", ",", "fr\u00fch\u00b7auf", ",", "pfiff", "ein", "Vo\u00b7gel", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "ADJD", "$,", "VVFIN", "ART", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "von tausend Gr\u00e4sern troff der Thau,", "tokens": ["von", "tau\u00b7send", "Gr\u00e4\u00b7sern", "troff", "der", "Thau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "der ganze Himmel stand in Rosen.", "tokens": ["der", "gan\u00b7ze", "Him\u00b7mel", "stand", "in", "Ro\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Lieber! Liebe!", "tokens": ["Lie\u00b7ber", "!", "Lie\u00b7be", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["ADJD", "$.", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Und wieder: Kuss auf Kuss . . .", "tokens": ["Und", "wie\u00b7der", ":", "Kuss", "auf", "Kuss", ".", ".", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct", "punct"], "pos": ["KON", "ADV", "$.", "NN", "APPR", "NN", "$.", "$.", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Was kann die Welt uns jetzt noch bieten!", "tokens": ["Was", "kann", "die", "Welt", "uns", "jetzt", "noch", "bie\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "NN", "PPER", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}