{"textgrid.poem.28162": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "77.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "F\u00fcr sp\u00f6ttisch Ding h\u00e4lt Mars, quid juris etwa k\u00fcnnen;", "tokens": ["F\u00fcr", "sp\u00f6t\u00b7tisch", "Ding", "h\u00e4lt", "Mars", ",", "quid", "ju\u00b7ris", "et\u00b7wa", "k\u00fcn\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "NN", "VVFIN", "NN", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Quid furis aber ist ein ehrenreich Beginnen.", "tokens": ["Quid", "fu\u00b7ris", "a\u00b7ber", "ist", "ein", "eh\u00b7ren\u00b7reich", "Be\u00b7gin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "ADV", "VAFIN", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Noch dannoch wirds geschehn, da\u00df Mars um einen Bissen", "tokens": ["Noch", "dan\u00b7noch", "wirds", "ge\u00b7schehn", ",", "da\u00df", "Mars", "um", "ei\u00b7nen", "Bis\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VAFIN", "VVPP", "$,", "KOUS", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wird endlich noch wol gar quid ruris lernen m\u00fcssen", "tokens": ["Wird", "end\u00b7lich", "noch", "wol", "gar", "quid", "ru\u00b7ris", "ler\u00b7nen", "m\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ADV", "ADV", "ADV", "FM", "FM", "VVINF", "VMFIN"]