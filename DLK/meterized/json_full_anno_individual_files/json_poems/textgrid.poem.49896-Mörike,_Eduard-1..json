{"textgrid.poem.49896": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1841, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Tochter des Walds, du Lilienverwandte,", "tokens": ["Toch\u00b7ter", "des", "Walds", ",", "du", "Li\u00b7li\u00b7en\u00b7ver\u00b7wand\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "PPER", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "So lang von mir gesuchte, unbekannte,", "tokens": ["So", "lang", "von", "mir", "ge\u00b7such\u00b7te", ",", "un\u00b7be\u00b7kann\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "PPER", "VVFIN", "$,", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Im fremden Kirchhof, \u00f6d und winterlich,", "tokens": ["Im", "frem\u00b7den", "Kirch\u00b7hof", ",", "\u00f6d", "und", "win\u00b7ter\u00b7lich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Zum erstenmal, o sch\u00f6ne, find ich dich!", "tokens": ["Zum", "ers\u00b7ten\u00b7mal", ",", "o", "sch\u00f6\u00b7ne", ",", "find", "ich", "dich", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "ADV", "$,", "FM", "ADJA", "$,", "VVFIN", "PPER", "PRF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Von welcher Hand gepflegt du hier erbl\u00fchtest,", "tokens": ["Von", "wel\u00b7cher", "Hand", "ge\u00b7pflegt", "du", "hier", "er\u00b7bl\u00fch\u00b7test", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "VVPP", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich wei\u00df es nicht, noch wessen Grab du h\u00fctest;", "tokens": ["Ich", "wei\u00df", "es", "nicht", ",", "noch", "wes\u00b7sen", "Grab", "du", "h\u00fc\u00b7test", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$,", "ADV", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ist es ein J\u00fcngling, so geschah ihm Heil,", "tokens": ["Ist", "es", "ein", "J\u00fcng\u00b7ling", ",", "so", "ge\u00b7schah", "ihm", "Heil", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "$,", "ADV", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ist's eine Jungfrau, lieblich fiel ihr Teil.", "tokens": ["Ist's", "ei\u00b7ne", "Jung\u00b7frau", ",", "lieb\u00b7lich", "fiel", "ihr", "Teil", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$,", "ADJD", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Im n\u00e4chtgen Hain, von Schneelicht \u00fcberbreitet,", "tokens": ["Im", "n\u00e4cht\u00b7gen", "Hain", ",", "von", "Schnee\u00b7licht", "\u00fc\u00b7berb\u00b7rei\u00b7tet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wo fromm das Reh an dir vor\u00fcberweidet,", "tokens": ["Wo", "fromm", "das", "Reh", "an", "dir", "vor\u00b7\u00fc\u00b7berw\u00b7ei\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Bei der Kapelle, am kristallnen Teich,", "tokens": ["Bei", "der", "Ka\u00b7pel\u00b7le", ",", "am", "kris\u00b7tall\u00b7nen", "Teich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "Dort sucht ich deiner Heimat Zauberreich.", "tokens": ["Dort", "sucht", "ich", "dei\u00b7ner", "Hei\u00b7mat", "Zau\u00b7ber\u00b7reich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Sch\u00f6n bist du, Kind des Mondes, nicht der Sonne;", "tokens": ["Sch\u00f6n", "bist", "du", ",", "Kind", "des", "Mon\u00b7des", ",", "nicht", "der", "Son\u00b7ne", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "$,", "NN", "ART", "NN", "$,", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dir w\u00e4re t\u00f6dlich andrer Blumen Wonne,", "tokens": ["Dir", "w\u00e4\u00b7re", "t\u00f6d\u00b7lich", "an\u00b7drer", "Blu\u00b7men", "Won\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dich n\u00e4hrt, den keuschen Leib voll Reif und Duft,", "tokens": ["Dich", "n\u00e4hrt", ",", "den", "keu\u00b7schen", "Leib", "voll", "Reif", "und", "Duft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ART", "ADJA", "NN", "ADJD", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Himmlischer K\u00e4lte balsams\u00fc\u00dfe Luft.", "tokens": ["Himm\u00b7li\u00b7scher", "K\u00e4l\u00b7te", "bal\u00b7sam\u00b7s\u00fc\u00b7\u00dfe", "Luft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.5": {"line.1": {"text": "In deines Busens goldner F\u00fclle gr\u00fcndet", "tokens": ["In", "dei\u00b7nes", "Bu\u00b7sens", "gold\u00b7ner", "F\u00fcl\u00b7le", "gr\u00fcn\u00b7det"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein Wohlgeruch, der sich nur kaum verk\u00fcndet;", "tokens": ["Ein", "Wohl\u00b7ge\u00b7ruch", ",", "der", "sich", "nur", "kaum", "ver\u00b7k\u00fcn\u00b7det", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PRF", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So duftete, ber\u00fchrt von Engelshand,", "tokens": ["So", "duf\u00b7te\u00b7te", ",", "be\u00b7r\u00fchrt", "von", "En\u00b7gels\u00b7hand", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "VVFIN", "APPR", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der benedeiten Mutter Brautgewand.", "tokens": ["Der", "be\u00b7ne\u00b7dei\u00b7ten", "Mut\u00b7ter", "Braut\u00b7ge\u00b7wand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NE", "$."], "meter": "---+-+-+-+", "measure": "zehnsilber"}}, "stanza.6": {"line.1": {"text": "Dich w\u00fcrden, mahnend an das heilge Leiden,", "tokens": ["Dich", "w\u00fcr\u00b7den", ",", "mah\u00b7nend", "an", "das", "heil\u00b7ge", "Lei\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "F\u00fcnf Purpurtropfen sch\u00f6n und einzig kleiden:", "tokens": ["F\u00fcnf", "Pur\u00b7pur\u00b7trop\u00b7fen", "sch\u00f6n", "und", "ein\u00b7zig", "klei\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "ADJD", "KON", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch kindlich zierst du, um die Weihnachtszeit,", "tokens": ["Doch", "kind\u00b7lich", "zierst", "du", ",", "um", "die", "Weih\u00b7nachts\u00b7zeit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "$,", "KOUI", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Lichtgr\u00fcn mit einem Hauch dein wei\u00dfes Kleid.", "tokens": ["Licht\u00b7gr\u00fcn", "mit", "ei\u00b7nem", "Hauch", "dein", "wei\u00b7\u00dfes", "Kleid", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "++-+-+-+-+", "measure": "iambic.penta.spondeus"}}, "stanza.7": {"line.1": {"text": "Der Elfe, der in mittern\u00e4chtger Stunde", "tokens": ["Der", "El\u00b7fe", ",", "der", "in", "mit\u00b7ter\u00b7n\u00e4cht\u00b7ger", "Stun\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zum Tanze geht im lichterhellen Grunde,", "tokens": ["Zu