{"dta.poem.13519": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Letzte Heimkehr .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.71", "sv:0.28"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Der Wintermorgen gl\u00e4nzt so klar,", "tokens": ["Der", "Win\u00b7ter\u00b7mor\u00b7gen", "gl\u00e4nzt", "so", "klar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Wandrer kommt von ferne,", "tokens": ["Ein", "Wand\u00b7rer", "kommt", "von", "fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ihn sch\u00fcttelt Frost, es starrt sein Haar,", "tokens": ["Ihn", "sch\u00fct\u00b7telt", "Frost", ",", "es", "starrt", "sein", "Haar", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihm log die sch\u00f6ne Ferne,", "tokens": ["Ihm", "log", "die", "sch\u00f6\u00b7ne", "Fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Nun endlich will er rasten hier,", "tokens": ["Nun", "end\u00b7lich", "will", "er", "ras\u00b7ten", "hier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VMFIN", "PPER", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Er klopft an seines Vaters Th\u00fcr.", "tokens": ["Er", "klopft", "an", "sei\u00b7nes", "Va\u00b7ters", "Th\u00fcr."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Doch todt sind, die sonst aufgethan,", "tokens": ["Doch", "todt", "sind", ",", "die", "sonst", "auf\u00b7ge\u00b7than", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "$,", "PRELS", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Verwandelt Hof und Habe,", "tokens": ["Ver\u00b7wan\u00b7delt", "Hof", "und", "Ha\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und fremde Leute seh'n ihn an", "tokens": ["Und", "frem\u00b7de", "Leu\u00b7te", "seh'n", "ihn", "an"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "VVFIN", "PPER", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als k\u00e4m' er aus dem Grabe;", "tokens": ["Als", "k\u00e4m'", "er", "aus", "dem", "Gra\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ihn schauert tief im Herzensgrund,", "tokens": ["Ihn", "schau\u00b7ert", "tief", "im", "Her\u00b7zens\u00b7grund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ins Feld eilt er zur selben Stund.", "tokens": ["Ins", "Feld", "eilt", "er", "zur", "sel\u00b7ben", "Stund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Da sang kein V\u00f6glein weit und breit,", "tokens": ["Da", "sang", "kein", "V\u00f6\u00b7glein", "weit", "und", "breit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er lehnt' an einem Baume,", "tokens": ["Er", "lehnt'", "an", "ei\u00b7nem", "Bau\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der sch\u00f6ne Garten lag verschneit,", "tokens": ["Der", "sch\u00f6\u00b7ne", "Gar\u00b7ten", "lag", "ver\u00b7schneit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es war ihm wie im Traume,", "tokens": ["Es", "war", "ihm", "wie", "im", "Trau\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "KOKOM", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und wie die Morgenglocke klingt,", "tokens": ["Und", "wie", "die", "Mor\u00b7gen\u00b7glo\u00b7cke", "klingt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Im stillen Feld er niedersinkt.", "tokens": ["Im", "stil\u00b7len", "Feld", "er", "nie\u00b7der\u00b7sinkt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und als er aufsteht vom Gebet,", "tokens": ["Und", "als", "er", "auf\u00b7steht", "vom", "Ge\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Nicht wei\u00df, wohin sich wenden,", "tokens": ["Nicht", "wei\u00df", ",", "wo\u00b7hin", "sich", "wen\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "$,", "PWAV", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein sch\u00f6ner J\u00fcngling bei ihm steht,", "tokens": ["Ein", "sch\u00f6\u00b7ner", "J\u00fcng\u00b7ling", "bei", "ihm", "steht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Fa\u00dft mild ihn bei den H\u00e4nden:", "tokens": ["Fa\u00dft", "mild", "ihn", "bei", "den", "H\u00e4n\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "\u201ekomm' mit, sollst ruhn nach kurzem Gang.\u201c \u2014", "tokens": ["\u201e", "komm'", "mit", ",", "sollst", "ruhn", "nach", "kur\u00b7zem", "Gang", ".", "\u201c"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "VVFIN", "PTKVZ", "$,", "VMFIN", "VVINF", "APPR", "ADJA", "NN", "$.", "$(", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Er folgt, ihn r\u00fchrt der Stimme Klang.", "tokens": ["Er", "folgt", ",", "ihn", "r\u00fchrt", "der", "Stim\u00b7me", "Klang", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Nun durch die Bergeseinsamkeit", "tokens": ["Nun", "durch", "die", "Ber\u00b7ge\u00b7sein\u00b7sam\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie wie zum Himmel steigen,", "tokens": ["Sie", "wie", "zum", "Him\u00b7mel", "stei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KOKOM", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Kein Glockenklang mehr reicht so weit,", "tokens": ["Kein", "Glo\u00b7cken\u00b7klang", "mehr", "reicht", "so", "weit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie sehn im \u00f6den Schweigen", "tokens": ["Sie", "sehn", "im", "\u00f6\u00b7den", "Schwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die L\u00e4nder hinter sich verbl\u00fchn,", "tokens": ["Die", "L\u00e4n\u00b7der", "hin\u00b7ter", "sich", "ver\u00b7bl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Schon Sterne durch die Wipfel gl\u00fchn.", "tokens": ["Schon", "Ster\u00b7ne", "durch", "die", "Wip\u00b7fel", "gl\u00fchn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Der F\u00fchrer jetzt die Fackel sacht", "tokens": ["Der", "F\u00fch\u00b7rer", "jetzt", "die", "Fa\u00b7ckel", "sacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Erhebt und schweigend schreitet,", "tokens": ["Er\u00b7hebt", "und", "schwei\u00b7gend", "schrei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Bei ihrem Schein die stille Nacht", "tokens": ["Bei", "ih\u00b7rem", "Schein", "die", "stil\u00b7le", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gleichwie ein Dom sich weitet,", "tokens": ["Gleich\u00b7wie", "ein", "Dom", "sich", "wei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wo unsichtbare H\u00e4nde baun \u2014", "tokens": ["Wo", "un\u00b7sicht\u00b7ba\u00b7re", "H\u00e4n\u00b7de", "baun"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den Wandrer fa\u00dft ein heimlich Graun.", "tokens": ["Den", "Wand\u00b7rer", "fa\u00dft", "ein", "heim\u00b7lich", "Graun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Er sprach: was bringt der Wind herauf", "tokens": ["Er", "sprach", ":", "was", "bringt", "der", "Wind", "her\u00b7auf"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "PWS", "VVFIN", "ART", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So fremden Laut getragen,", "tokens": ["So", "frem\u00b7den", "Laut", "ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Als h\u00f6rt' ich ferner Str\u00f6me Lauf,", "tokens": ["Als", "h\u00f6rt'", "ich", "fer\u00b7ner", "Str\u00f6\u00b7me", "Lauf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "ADV", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dazwischen Glocken schlagen?", "tokens": ["Da\u00b7zwi\u00b7schen", "Glo\u00b7cken", "schla\u00b7gen", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PAV", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "\u201edas ist des Nachtgesanges Wehn,", "tokens": ["\u201e", "das", "ist", "des", "Nacht\u00b7ge\u00b7san\u00b7ges", "Wehn", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PDS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sie loben Gott in stillen H\u00f6h'n.\u201c", "tokens": ["Sie", "lo\u00b7ben", "Gott", "in", "stil\u00b7len", "H\u00f6h'", "n.", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "abbreviation", "punct"], "pos": ["PPER", "VVFIN", "NN", "APPR", "ADJA", "NN", "NE", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Der Wandrer drauf: ich kann nicht mehr \u2014", "tokens": ["Der", "Wand\u00b7rer", "drauf", ":", "ich", "kann", "nicht", "mehr"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$.", "PPER", "VMFIN", "PTKNEG", "ADV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ist's Morgen, der so blendet?", "tokens": ["Ist's", "Mor\u00b7gen", ",", "der", "so", "blen\u00b7det", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PRELS", "ADV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Was leuchten dort f\u00fcr L\u00e4nder her? \u2014", "tokens": ["Was", "leuch\u00b7ten", "dort", "f\u00fcr", "L\u00e4n\u00b7der", "her", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "VVFIN", "ADV", "APPR", "NN", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sein Freund die Fackel wendet:", "tokens": ["Sein", "Freund", "die", "Fa\u00b7ckel", "wen\u00b7det", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "\u201enun ruh zum letztenmale aus,", "tokens": ["\u201e", "nun", "ruh", "zum", "letz\u00b7ten\u00b7ma\u00b7le", "aus", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "NN", "APPRART", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wenn du erwachst, sind wir zu Haus.\u201c", "tokens": ["Wenn", "du", "er\u00b7wachst", ",", "sind", "wir", "zu", "Haus", ".", "\u201c"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "VAFIN", "PPER", "APPR", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}