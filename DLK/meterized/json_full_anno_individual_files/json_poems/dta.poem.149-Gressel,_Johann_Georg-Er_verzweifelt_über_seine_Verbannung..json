{"dta.poem.149": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Er verzweifelt \u00fcber seine Verbannung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Ich bin nun hin! ich bin/ ach Schmertz! nun gantz verlohren/", "tokens": ["Ich", "bin", "nun", "hin", "!", "ich", "bin", "/", "ach", "Schmertz", "!", "nun", "gantz", "ver\u00b7loh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PTKVZ", "$.", "PPER", "VAFIN", "$(", "XY", "NN", "$.", "ADV", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Lysette hat mich von sich weggebannt/", "tokens": ["Ly\u00b7set\u00b7te", "hat", "mich", "von", "sich", "weg\u00b7ge\u00b7bannt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "APPR", "PRF", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sie hat sich gantz und gar auf meinen Fall verschwohren/", "tokens": ["Sie", "hat", "sich", "gantz", "und", "gar", "auf", "mei\u00b7nen", "Fall", "ver\u00b7schwoh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PRF", "ADV", "KON", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und ihre Liebe umgewandt.", "tokens": ["Und", "ih\u00b7re", "Lie\u00b7be", "um\u00b7ge\u00b7wandt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich mu\u00df wie ", "tokens": ["Ich", "mu\u00df", "wie"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VMFIN", "KOKOM"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Die vorgeno\u00dfne Lust qu\u00e4lt mich herben Schmertzen.", "tokens": ["Die", "vor\u00b7ge\u00b7no\u00df\u00b7ne", "Lust", "qu\u00e4lt", "mich", "her\u00b7ben", "Schmert\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-++-+-+-", "measure": "unknown.measure.hexa"}, "line.7": {"text": "Die Seele ist in Alabast verkehrt/", "tokens": ["Die", "See\u00b7le", "ist", "in", "A\u00b7la\u00b7bast", "ver\u00b7kehrt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "NE", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Das keine Klagen h\u00f6rt.", "tokens": ["Das", "kei\u00b7ne", "Kla\u00b7gen", "h\u00f6rt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "So jagt mich ", "tokens": ["So", "jagt", "mich"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER"], "meter": "-+-", "measure": "amphibrach.single"}, "line.10": {"text": "Und schl\u00e4gt mein Hoffnungs-Schiff vor seinen Port vorbey/", "tokens": ["Und", "schl\u00e4gt", "mein", "Hoff\u00b7nungs\u00b7Schiff", "vor", "sei\u00b7nen", "Port", "vor\u00b7bey", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Die Seegel reissen ab/ der Ancker ist entzwey", "tokens": ["Die", "See\u00b7gel", "reis\u00b7sen", "ab", "/", "der", "An\u00b7cker", "ist", "ent\u00b7zwey"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$(", "ART", "NN", "VAFIN", "NE"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Ich mu\u00df im Sturme untergehen.", "tokens": ["Ich", "mu\u00df", "im", "Stur\u00b7me", "un\u00b7ter\u00b7ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Es kehret mein Magnet jetzt sich nach S\u00fcden/", "tokens": ["Es", "keh\u00b7ret", "mein", "Mag\u00b7net", "jetzt", "sich", "nach", "S\u00fc\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ADV", "PRF", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Sonst ging er nach den Nord-", "tokens": ["Sonst", "ging", "er", "nach", "den", "Nord"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "TRUNC"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.15": {"text": "Und zeigte die erw\u00fcnschte Ruh/", "tokens": ["Und", "zeig\u00b7te", "die", "er\u00b7w\u00fcnschte", "Ruh", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.16": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.17": {"text": "Die Hoffnung kan die Segel nicht mehr spannen/", "tokens": ["Die", "Hoff\u00b7nung", "kan", "die", "Se\u00b7gel", "nicht", "mehr", "span\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ART", "NN", "PTKNEG", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Sie trifft gar keinen Haven an/", "tokens": ["Sie", "trifft", "gar", "kei\u00b7nen", "Ha\u00b7ven", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PIAT", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Es ist mit mir gethan.", "tokens": ["Es", "ist", "mit", "mir", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.20": {"text": "Der Jammer will mich \u00fcbermannen.", "tokens": ["Der", "Jam\u00b7mer", "will", "mich", "\u00fc\u00b7berm\u00b7an\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Jhr Wolcken schlagt nur her/", "tokens": ["Ihr", "Wol\u00b7cken", "schlagt", "nur", "her", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "PTKVZ", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.22": {"text": "Mit Blitz und Donner-Schl\u00e4gen/", "tokens": ["Mit", "Blitz", "und", "Don\u00b7ner\u00b7Schl\u00e4\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.23": {"text": "Die mich zur Erden niederlegen/", "tokens": ["Die", "mich", "zur", "Er\u00b7den", "nie\u00b7der\u00b7le\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Fort eilet/ das ist mein Begehr.", "tokens": ["Fort", "ei\u00b7let", "/", "das", "ist", "mein", "Be\u00b7gehr", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$(", "PDS", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Komm ", "tokens": ["Komm"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "+", "measure": "single.up"}, "line.26": {"text": "Hin nach dem Schlund der Jammer-vollen H\u00f6llen.", "tokens": ["Hin", "nach", "dem", "Schlund", "der", "Jam\u00b7mer\u00b7vol\u00b7len", "H\u00f6l\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Fort fahr nur hin/", "tokens": ["Fort", "fahr", "nur", "hin", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "PTKVZ", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.28": {"text": "Ich seh schon wo ich bin/", "tokens": ["Ich", "seh", "schon", "wo", "ich", "bin", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PWAV", "PPER", "VAFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.29": {"text": "Da ist der grosse ", "tokens": ["Da", "ist", "der", "gros\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "ADJA"], "meter": "-+-+-", "measure": "iambic.di"}, "line.30": {"text": "Mit seinen weiten Rachen", "tokens": ["Mit", "sei\u00b7nen", "wei\u00b7ten", "Ra\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.31": {"text": "Ha Cammerad ich mu\u00df", "tokens": ["Ha", "Cam\u00b7me\u00b7rad", "ich", "mu\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "NE", "PPER", "VMFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.32": {"text": "Mit dir dis Loch bewachen/", "tokens": ["Mit", "dir", "dis", "Loch", "be\u00b7wa\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PDS", "NN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.33": {"text": "Jhr Furien seyd nur getrost", "tokens": ["Ihr", "Fu\u00b7ri\u00b7en", "seyd", "nur", "ge\u00b7trost"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Ich bringe Feur und Flammen", "tokens": ["Ich", "brin\u00b7ge", "Feur", "und", "Flam\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.35": {"text": "Jhr Geister flieht zusammen", "tokens": ["Ihr", "Geis\u00b7ter", "flieht", "zu\u00b7sam\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKVZ"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.36": {"text": "Ich bin ein ", "tokens": ["Ich", "bin", "ein"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VAFIN", "ART"], "meter": "+-+", "measure": "trochaic.di"}}}}}