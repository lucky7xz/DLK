{"textgrid.poem.53019": {"metadata": {"author": {"name": "Pyra, Jakob Immanuel", "birth": "N.A.", "death": "N.A."}, "title": "1L: Als noch dein freyer Geist sein Wachen", "genre": "verse", "period": "N.A.", "pub_year": 1729, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als noch dein freyer Geist sein Wachen", "tokens": ["Als", "noch", "dein", "frey\u00b7er", "Geist", "sein", "Wa\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "PPOSAT", "ADJA", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nicht v\u00f6llig deinem Fleisch und Blute schuldig war,", "tokens": ["Nicht", "v\u00f6l\u00b7lig", "dei\u00b7nem", "Fleisch", "und", "Blu\u00b7te", "schul\u00b7dig", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "PPOSAT", "NN", "KON", "NN", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da a\u00df ich froh, getrost und ohne Sorgen,", "tokens": ["Da", "a\u00df", "ich", "froh", ",", "ge\u00b7trost", "und", "oh\u00b7ne", "Sor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$,", "VVPP", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Von deinen mir mit Lust gereichten Bissen,", "tokens": ["Von", "dei\u00b7nen", "mir", "mit", "Lust", "ge\u00b7reich\u00b7ten", "Bis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "PPER", "APPR", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Da stillt ein Becher unsern Durst,", "tokens": ["Da", "stillt", "ein", "Be\u00b7cher", "un\u00b7sern", "Durst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da tauchten wir zugleich in eine blancke Schaale,", "tokens": ["Da", "tauch\u00b7ten", "wir", "zu\u00b7gleich", "in", "ei\u00b7ne", "blan\u00b7cke", "Schaa\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Dein Arm trug meines Kummers Last,", "tokens": ["Dein", "Arm", "trug", "mei\u00b7nes", "Kum\u00b7mers", "Last", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPOSAT", "NN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und mein versiegelt Hertz dein Sorgen.", "tokens": ["Und", "mein", "ver\u00b7sie\u00b7gelt", "Hertz", "dein", "Sor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "VVFIN", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Da lebten wir mit Lust beysammen,", "tokens": ["Da", "leb\u00b7ten", "wir", "mit", "Lust", "bey\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und spotteten sowohl des Gl\u00fcckes, als des Grams.", "tokens": ["Und", "spot\u00b7te\u00b7ten", "so\u00b7wohl", "des", "Gl\u00fc\u00b7ckes", ",", "als", "des", "Grams", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "ART", "NN", "$,", "KOUS", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich sang, du sangst; du sangst und Doris horchte,", "tokens": ["Ich", "sang", ",", "du", "sangst", ";", "du", "sangst", "und", "Do\u00b7ris", "horch\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "$.", "PPER", "VVFIN", "KON", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und nahm dich k\u00fcssend in die Arme.", "tokens": ["Und", "nahm", "dich", "k\u00fcs\u00b7send", "in", "die", "Ar\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der Stimme spielend Ebenbild,", "tokens": ["Der", "Stim\u00b7me", "spie\u00b7lend", "E\u00b7ben\u00b7bild", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das in durchschallten Wald aus holen Felsen rufet,", "tokens": ["Das", "in", "durch\u00b7schall\u00b7ten", "Wald", "aus", "ho\u00b7len", "Fel\u00b7sen", "ru\u00b7fet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die Echo trug der Doris Lob", "tokens": ["Die", "E\u00b7cho", "trug", "der", "Do\u00b7ris", "Lob"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NE", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und Namen tausendf\u00e4ltig weiter.", "tokens": ["Und", "Na\u00b7men", "tau\u00b7send\u00b7f\u00e4l\u00b7tig", "wei\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Von p\u00f6belhaftigen Begierden", "tokens": ["Von", "p\u00f6\u00b7bel\u00b7haf\u00b7ti\u00b7gen", "Be\u00b7gier\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "W\u00fcrd ich und du befreyt, mit uns allein vergn\u00fcgt,", "tokens": ["W\u00fcrd", "ich", "und", "du", "be\u00b7freyt", ",", "mit", "uns", "al\u00b7lein", "ver\u00b7gn\u00fcgt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "KON", "PPER", "VVPP", "$,", "APPR", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Du mit der scharffen Fl\u00f6t, ich mit der Leyer,", "tokens": ["Du", "mit", "der", "scharf\u00b7fen", "Fl\u00f6t", ",", "ich", "mit", "der", "Le\u00b7yer", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "ADJA", "NN", "$,", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-++-+", "measure": "unknown.measure.hexa"}, "line.4": {"text": "Die letzten Wildnisse besuchet haben,", "tokens": ["Die", "letz\u00b7ten", "Wild\u00b7nis\u00b7se", "be\u00b7su\u00b7chet", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wo die unfruchtbare Natur", "tokens": ["Wo", "die", "un\u00b7frucht\u00b7ba\u00b7re", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.6": {"text": "Kein gr\u00fcner Busch erfreut, kein frischer Schatten k\u00fchlet,", "tokens": ["Kein", "gr\u00fc\u00b7ner", "Busch", "er\u00b7freut", ",", "kein", "fri\u00b7scher", "Schat\u00b7ten", "k\u00fch\u00b7let", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "$,", "PIAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wo \u00fcber unser Haupt das Rad", "tokens": ["Wo", "\u00fc\u00b7ber", "un\u00b7ser", "Haupt", "das", "Rad"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "PPOSAT", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Des Sonnenwagens tiefer schwebte.", "tokens": ["Des", "Son\u00b7nen\u00b7wa\u00b7gens", "tie\u00b7fer", "schweb\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ich w\u00fcrd in einem Sch\u00e4ferkleide,", "tokens": ["Ich", "w\u00fcrd", "in", "ei\u00b7nem", "Sch\u00e4\u00b7fer\u00b7klei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dort um des reissenden und breiten Tygris Strand", "tokens": ["Dort", "um", "des", "reis\u00b7sen\u00b7den", "und", "brei\u00b7ten", "Tyg\u00b7ris", "Strand"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "KON", "ADJA", "NE", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Von eurem Ruhm mit Lust gespielet haben.", "tokens": ["Von", "eu\u00b7rem", "Ruhm", "mit", "Lust", "ge\u00b7spie\u00b7let", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "NN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Es h\u00e4tten mich der holden Namen wegen", "tokens": ["Es", "h\u00e4t\u00b7ten", "mich", "der", "hol\u00b7den", "Na\u00b7men", "we\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ART", "ADJA", "NN", "APPR"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Die L\u00f6wen gantz entz\u00fcckt gescheut.", "tokens": ["Die", "L\u00f6\u00b7wen", "gantz", "ent\u00b7z\u00fcckt", "ge\u00b7scheut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ja, ja, es h\u00e4tten selbst die rauhen regen Felsen", "tokens": ["Ja", ",", "ja", ",", "es", "h\u00e4t\u00b7ten", "selbst", "die", "rau\u00b7hen", "re\u00b7gen", "Fel\u00b7sen"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "PTKANT", "$,", "PPER", "VAFIN", "ADV", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Bewegt von eurem Ruhm erklungen.", "tokens": ["Be\u00b7wegt", "von", "eu\u00b7rem", "Ruhm", "er\u00b7klun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Da h\u00e4tten uns die falschen Freunde,", "tokens": ["Da", "h\u00e4t\u00b7ten", "uns", "die", "fal\u00b7schen", "Freun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Durch Ph\u00f6bus Pfeil verjagt, im Singen nie gest\u00f6hrt;", "tokens": ["Durch", "Ph\u00f6\u00b7bus", "Pfeil", "ver\u00b7jagt", ",", "im", "Sin\u00b7gen", "nie", "ge\u00b7st\u00f6hrt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "VVPP", "$,", "APPRART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Du h\u00e4ttest selbst der Barbarn Hertz erweichet;", "tokens": ["Du", "h\u00e4t\u00b7test", "selbst", "der", "Bar\u00b7barn", "Hertz", "er\u00b7wei\u00b7chet", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und ihre Grausamkeit uns nie getrennet.", "tokens": ["Und", "ih\u00b7re", "Grau\u00b7sam\u00b7keit", "uns", "nie", "ge\u00b7tren\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und wie! du l\u00e4ngst verlachter Neid,", "tokens": ["Und", "wie", "!", "du", "l\u00e4ngst", "ver\u00b7lach\u00b7ter", "Neid", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "$.", "PPER", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Du denckst das heilge Band der Freundschaft aufzul\u00f6sen?", "tokens": ["Du", "denckst", "das", "heil\u00b7ge", "Band", "der", "Freund\u00b7schaft", "auf\u00b7zu\u00b7l\u00f6\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Hast du in deiner Bosheit Frist,", "tokens": ["Hast", "du", "in", "dei\u00b7ner", "Bos\u00b7heit", "Frist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Nicht Damon, Thirsis, kennen lernen?", "tokens": ["Nicht", "Da\u00b7mon", ",", "Thir\u00b7sis", ",", "ken\u00b7nen", "ler\u00b7nen", "?"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "NE", "$,", "NE", "$,", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Als noch dein freyer Geist sein Wachen", "tokens": ["Als", "noch", "dein", "frey\u00b7er", "Geist", "sein", "Wa\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "PPOSAT", "ADJA", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nicht v\u00f6llig deinem Fleisch und Blute schuldig war,", "tokens": ["Nicht", "v\u00f6l\u00b7lig", "dei\u00b7nem", "Fleisch", "und", "Blu\u00b7te", "schul\u00b7dig", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "PPOSAT", "NN", "KON", "NN", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da a\u00df ich froh, getrost und ohne Sorgen,", "tokens": ["Da", "a\u00df", "ich", "froh", ",", "ge\u00b7trost", "und", "oh\u00b7ne", "Sor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$,", "VVPP", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Von deinen mir mit Lust gereichten Bissen,", "tokens": ["Von", "dei\u00b7nen", "mir", "mit", "Lust", "ge\u00b7reich\u00b7ten", "Bis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punc