{"textgrid.poem.42148": {"metadata": {"author": {"name": "Wedekind, Frank", "birth": "N.A.", "death": "N.A."}, "title": "Der Gefangene", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Oftmals hab ich nachts im Bette", "tokens": ["Oft\u00b7mals", "hab", "ich", "nachts", "im", "Bet\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schon gegr\u00fcbelt hin und her,", "tokens": ["Schon", "ge\u00b7gr\u00fc\u00b7belt", "hin", "und", "her", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Was es denn geschadet h\u00e4tte,", "tokens": ["Was", "es", "denn", "ge\u00b7scha\u00b7det", "h\u00e4t\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "VVPP", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn mein Ich ein andrer w\u00e4r.", "tokens": ["Wenn", "mein", "Ich", "ein", "an\u00b7drer", "w\u00e4r", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "PPER", "ART", "ADJA", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "H\u00f6hnisch raunten meine Zweifel", "tokens": ["H\u00f6h\u00b7nisch", "raun\u00b7ten", "mei\u00b7ne", "Zwei\u00b7fel"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mir die tolle Antwort zu:", "tokens": ["Mir", "die", "tol\u00b7le", "Ant\u00b7wort", "zu", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nichts geschadet, dummer Teufel,", "tokens": ["Nichts", "ge\u00b7scha\u00b7det", ",", "dum\u00b7mer", "Teu\u00b7fel", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "VVPP", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Denn der andre w\u00e4rest du!", "tokens": ["Denn", "der", "and\u00b7re", "w\u00e4\u00b7rest", "du", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "VAFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Hilflos w\u00e4lzt ich mich im Bette", "tokens": ["Hil\u00b7flos", "w\u00e4lzt", "ich", "mich", "im", "Bet\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "PRF", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und entrang mir dies Gedicht,", "tokens": ["Und", "ent\u00b7rang", "mir", "dies", "Ge\u00b7dicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PDS", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Rasselnd mit der Sklavenkette,", "tokens": ["Ras\u00b7selnd", "mit", "der", "Skla\u00b7ven\u00b7ket\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die kein Denker je zerbricht.", "tokens": ["Die", "kein", "Den\u00b7ker", "je", "zer\u00b7bricht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Oftmals hab ich nachts im Bette", "tokens": ["Oft\u00b7mals", "