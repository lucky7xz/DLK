{"textgrid.poem.58471": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "509. Siehe, ich bin bei euch!", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Herr ist nahe, der dich will", "tokens": ["Der", "Herr", "ist", "na\u00b7he", ",", "der", "dich", "will"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "PRELS", "PPER", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Durch seine Nahheit machen still;", "tokens": ["Durch", "sei\u00b7ne", "Nah\u00b7heit", "ma\u00b7chen", "still", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dein Herz und alle dein Begehren", "tokens": ["Dein", "Herz", "und", "al\u00b7le", "dein", "Be\u00b7geh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "KON", "PIS", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "La\u00df sanfte sich zu ihm einkehren,", "tokens": ["La\u00df", "sanf\u00b7te", "sich", "zu", "ihm", "ein\u00b7keh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "VVFIN", "PRF", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Gib ihm im Grunde wahrlich Platz", "tokens": ["Gib", "ihm", "im", "Grun\u00b7de", "wahr\u00b7lich", "Platz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "APPRART", "NN", "ADV", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Als deinem ganz' und einen Schatz!", "tokens": ["Als", "dei\u00b7nem", "ganz'", "und"