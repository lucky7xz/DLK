{"dta.poem.23668": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Bei einem Faschingsball 1839.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1840", "urn": "urn:nbn:de:kobv:b4-200905192626", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die uns wie ein Kranz im Fr\u00fchling gewunden", "tokens": ["Die", "uns", "wie", "ein", "Kranz", "im", "Fr\u00fch\u00b7ling", "ge\u00b7wun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "KOKOM", "ART", "NN", "APPRART", "NN", "VAPP"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "umgeben,", "tokens": ["um\u00b7ge\u00b7ben", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Und wie ein Tanz von fr\u00f6hlichen Stunden", "tokens": ["Und", "wie", "ein", "Tanz", "von", "fr\u00f6h\u00b7li\u00b7chen", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "umschweben,", "tokens": ["um\u00b7schwe\u00b7ben", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Und Freude in unser Leben weben,", "tokens": ["Und", "Freu\u00b7de", "in", "un\u00b7ser", "Le\u00b7ben", "we\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Und Leben unserm Streben geben,", "tokens": ["Und", "Le\u00b7ben", "un\u00b7serm", "Stre\u00b7ben", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und unser Leben zum Leben erheben,", "tokens": ["Und", "un\u00b7ser", "Le\u00b7ben", "zum", "Le\u00b7ben", "er\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Die unser Herzweh", "tokens": ["Die", "un\u00b7ser", "Herz\u00b7weh"], "token_info": ["word", "word", "word"], "pos": ["ART", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "Und unsere Plagen,", "tokens": ["Und", "un\u00b7se\u00b7re", "Pla\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Wie die Sonne den M\u00e4rzschnee,", "tokens": ["Wie", "die", "Son\u00b7ne", "den", "M\u00e4rz\u00b7schnee", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Wissen zu verjagen;", "tokens": ["Wis\u00b7sen", "zu", "ver\u00b7ja\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.12": {"text": "Die den Becher", "tokens": ["Die", "den", "Be\u00b7cher"], "token_info": ["word", "word", "word"], "pos": ["ART", "ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.13": {"text": "der schlimmen Laune f\u00fcr sich behalten,", "tokens": ["der", "schlim\u00b7men", "Lau\u00b7ne", "f\u00fcr", "sich", "be\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PRF", "VVINF", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "Und uns nur den F\u00e4cher", "tokens": ["Und", "uns", "nur", "den", "F\u00e4\u00b7cher"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "ADV", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.15": {"text": "der Fr\u00f6hlichkeit entfalten;", "tokens": ["der", "Fr\u00f6h\u00b7lich\u00b7keit", "ent\u00b7fal\u00b7ten", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.16": {"text": "Die besser ", "tokens": ["Die", "bes\u00b7ser"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.17": {"text": "Und mehr f\u00fcr den Herrscher als die Herrschaft entbrennen;", "tokens": ["Und", "mehr", "f\u00fcr", "den", "Herr\u00b7scher", "als", "die", "Herr\u00b7schaft", "ent\u00b7bren\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "KOKOM", "ART", "NN", "VVINF", "$."], "meter": "-+--+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.18": {"text": "Die nicht grollen und schmollen,", "tokens": ["Die", "nicht", "grol\u00b7len", "und", "schmol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "VVINF", "KON", "VVFIN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.19": {"text": "Wenn wir trinken sollen und wollen,", "tokens": ["Wenn", "wir", "trin\u00b7ken", "sol\u00b7len", "und", "wol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVINF", "VMFIN", "KON", "VMFIN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.20": {"text": "Die unsern hei\u00dfen Durst zu w\u00fcrdigen", "tokens": ["Die", "un\u00b7sern", "hei\u00b7\u00dfen", "Durst", "zu", "w\u00fcr\u00b7di\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "immer bereit sind,", "tokens": ["im\u00b7mer", "be\u00b7reit", "sind", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.22": {"text": "Und denen unsere leeren Flaschen und Taschen", "tokens": ["Und", "de\u00b7nen", "un\u00b7se\u00b7re", "lee\u00b7ren", "Fla\u00b7schen", "und", "Ta\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PDS", "PPOSAT", "ADJA", "NN", "KON", "NN"], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.23": {"text": "nimmer leid sind:", "tokens": ["nim\u00b7mer", "leid", "sind", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Es leben ", "tokens": ["Es", "le\u00b7ben"], "token_info": ["word", "word"], "pos": ["PPER", "VVINF"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Die uns wie ein Kranz im Fr\u00fchling gewunden", "tokens": ["Die", "uns", "wie", "ein", "Kranz", "im", "Fr\u00fch\u00b7ling", "ge\u00b7wun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "KOKOM", "ART", "NN", "APPRART", "NN", "VAPP"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "umgeben,", "tokens": ["um\u00b7ge\u00b7ben", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Und wie ein Tanz von fr\u00f6hlichen Stunden", "tokens": ["Und", "wie", "ein", "Tanz", "von", "fr\u00f6h\u00b7li\u00b7chen", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "APPR", "ADJ