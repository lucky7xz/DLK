{"textgrid.poem.33821": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "Zweiflers Nachtgedanken", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "F\u00fcr Traum in Traum soll ich dies Dasein halten,", "tokens": ["F\u00fcr", "Traum", "in", "Traum", "soll", "ich", "dies", "Da\u00b7sein", "hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "VMFIN", "PPER", "PDS", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "F\u00fcr eines Schemens bleichen Widerschein,", "tokens": ["F\u00fcr", "ei\u00b7nes", "Sche\u00b7mens", "blei\u00b7chen", "Wi\u00b7der\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und wie mit Herbstlaub wilde St\u00fcrme schalten,", "tokens": ["Und", "wie", "mit", "Herbst\u00b7laub", "wil\u00b7de", "St\u00fcr\u00b7me", "schal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "APPR", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So soll's verweht vom Hauch der Zukunft sein?", "tokens": ["So", "soll's", "ver\u00b7weht", "vom", "Hauch", "der", "Zu\u00b7kunft", "sein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "APPRART", "NN", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Warum sind wir verbannt in Endlichkeit", "tokens": ["Wa\u00b7rum", "sind", "wir", "ver\u00b7bannt", "in", "End\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPER", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und in ein Leben, so von Nacht umh\u00fcllt,", "tokens": ["Und", "in", "ein", "Le\u00b7ben", ",", "so", "von", "Nacht", "um\u00b7h\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Da\u00df uns entrei\u00dfen d\u00fcrfen Tod und Zeit", "tokens": ["Da\u00df", "uns", "ent\u00b7rei\u00b7\u00dfen", "d\u00fcr\u00b7fen", "Tod", "und", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVINF", "VMFIN", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Selbst das, was unser bessres Sein erf\u00fcllt?", "tokens": ["Selbst", "das", ",", "was", "un\u00b7ser", "bess\u00b7res", "Sein", "er\u00b7f\u00fcllt", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDS", "$,", "PRELS", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Gott oder Weltgeist, allerschaffend Wesen", "tokens": ["Gott", "o\u00b7der", "Welt\u00b7geist", ",", "al\u00b7ler\u00b7schaf\u00b7fend", "We\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "KON", "NN", "$,", "ADJD", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Und aller Wesen erst' und letzter Grund,", "tokens": ["Und", "al\u00b7ler", "We\u00b7sen", "er\u00b7st'", "und", "letz\u00b7ter", "Grund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Wird unsres Daseins Klagschrift erst gelesen,", "tokens": ["Wird", "uns\u00b7res", "Da\u00b7seins", "Klagsc\u00b7hrift", "erst", "ge\u00b7le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wird uns erst Antwort, wenn verstummt der Mund?", "tokens": ["Wird", "uns", "erst", "Ant\u00b7wort", ",", "wenn", "ver\u00b7stummt", "der", "Mund", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "NN", "$,", "KOUS", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Warum, wenn unser Geist aus deinem Geist,", "tokens": ["Wa\u00b7rum", ",", "wenn", "un\u00b7ser", "Geist", "aus", "dei\u00b7nem", "Geist", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "KOUS", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Warum ein unab\u00e4nderliches Mu\u00df,", "tokens": ["Wa\u00b7rum", "ein", "un\u00b7ab\u00b7\u00e4n\u00b7der\u00b7li\u00b7ches", "Mu\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Das f\u00fchllos unser Erdengl\u00fcck zerrei\u00dft", "tokens": ["Das", "f\u00fchl\u00b7los", "un\u00b7ser", "Er\u00b7den\u00b7gl\u00fcck", "zer\u00b7rei\u00dft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und nur Entsagung f\u00fchrt uns zum Entschlu\u00df?", "tokens": ["Und", "nur", "Ent\u00b7sa\u00b7gung", "f\u00fchrt", "uns", "zum", "Ent\u00b7schlu\u00df", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "NN", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Versuch' ich's, diese R\u00e4tsel auszukl\u00fcgeln,", "tokens": ["Ver\u00b7such'", "ich's", ",", "die\u00b7se", "R\u00e4t\u00b7sel", "aus\u00b7zu\u00b7kl\u00fc\u00b7geln", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$,", "PDAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Um meine Seele wehn in fl\u00fccht'ger Spur,", "tokens": ["Um", "mei\u00b7ne", "See\u00b7le", "wehn", "in", "fl\u00fccht'\u00b7ger", "Spur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "In scheuem Flug, wie mit Libellenfl\u00fcgeln,", "tokens": ["In", "scheu\u00b7em", "Flug", ",", "wie", "mit", "Li\u00b7bel\u00b7len\u00b7fl\u00fc\u00b7geln", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "PWAV", "APPR", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die Urgedanken, D\u00e4mmrungsfalter nur;", "tokens": ["Die", "Ur\u00b7ge\u00b7dan\u00b7ken", ",", "D\u00e4mm\u00b7rungs\u00b7fal\u00b7ter", "nur", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und jetzt, da alles rings um mich verstummt,", "tokens": ["Und", "jetzt", ",", "da", "al\u00b7les", "rings", "um", "mich", "ver\u00b7stummt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "KOUS", "PIS", "ADV", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "T\u00f6nt an mein Herz ein Schauder der Natur,", "tokens": ["T\u00f6nt", "an", "mein", "Herz", "ein", "Schau\u00b7der", "der", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Im K\u00e4fer, der noch melancholisch summt,", "tokens": ["Im", "K\u00e4\u00b7fer", ",", "der", "noch", "me\u00b7lan\u00b7cho\u00b7lisch", "summt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Im Flutgemurmel und im Gang der Uhr.", "tokens": ["Im", "Flut\u00b7ge\u00b7mur\u00b7mel", "und", "im", "Gang", "der", "Uhr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Die Wasser brausen fort ins Bodenlose,", "tokens": ["Die", "Was\u00b7ser", "brau\u00b7sen", "fort", "ins", "Bo\u00b7den\u00b7lo\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Sterne fort zum fernsten \u00c4therreich;", "tokens": ["Die", "Ster\u00b7ne", "fort", "zum", "ferns\u00b7ten", "\u00c4\u00b7ther\u00b7reich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch Sturz und Sturm ist Ruh' in deinem Scho\u00dfe,", "tokens": ["Doch", "Sturz", "und", "Sturm", "ist", "Ruh'", "in", "dei\u00b7nem", "Scho\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VAFIN", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dein Antlitz sieht in Tag und Nacht zugleich;", "tokens": ["Dein", "Ant\u00b7litz", "sieht", "in", "Tag", "und", "Nacht", "zu\u00b7gleich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "NN", "KON", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Aus tiefsten Tiefen des Gebirges schiebt,", "tokens": ["Aus", "tiefs\u00b7ten", "Tie\u00b7fen", "des", "Ge\u00b7bir\u00b7ges", "schiebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Jahrtausend' alt, sich Urgestein empor", "tokens": ["Jahr\u00b7tau\u00b7send'", "alt", ",", "sich", "Ur\u00b7ge\u00b7stein", "em\u00b7por"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ADJD", "$,", "PRF", "NN", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und strebt zum freien \u00c4ther und verstiebt", "tokens": ["Und", "strebt", "zum", "frei\u00b7en", "\u00c4\u00b7ther", "und", "ver\u00b7stiebt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPRART", "ADJA", "NN", "KON", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Verwitternd in Atome wie zuvor.", "tokens": ["Ver\u00b7wit\u00b7ternd", "in", "A\u00b7to\u00b7me", "wie", "zu\u00b7vor", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NN", "KOKOM", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Es graben, irrend zwischen Krieg und Frieden,", "tokens": ["Es", "gra\u00b7ben", ",", "ir\u00b7rend", "zwi\u00b7schen", "Krieg", "und", "Frie\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "$,", "VVPP", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die V\u00f6lker ihres Ruhmes Testament", "tokens": ["Die", "V\u00f6l\u00b7ker", "ih\u00b7res", "Ruh\u00b7mes", "Tes\u00b7ta\u00b7ment"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "In Todesangst auf stolze Pyramiden,", "tokens": ["In", "To\u00b7de\u00b7sangst", "auf", "stol\u00b7ze", "Py\u00b7ra\u00b7mi\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df eine Nachwelt ihre Namen kennt.", "tokens": ["Da\u00df", "ei\u00b7ne", "Nach\u00b7welt", "ih\u00b7re", "Na\u00b7men", "kennt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die Fr\u00fcchte reisen ab und werden Staub,", "tokens": ["Die", "Fr\u00fcch\u00b7te", "rei\u00b7sen", "ab", "und", "wer\u00b7den", "Staub", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "KON", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Heroen schreiten durch der Zeiten Furt,", "tokens": ["He\u00b7roen", "schrei\u00b7ten", "durch", "der", "Zei\u00b7ten", "Furt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Doch Bl\u00fcte, Wachstum, Frucht und fallend Laub", "tokens": ["Doch", "Bl\u00fc\u00b7te", ",", "Wachs\u00b7tum", ",", "Frucht", "und", "fal\u00b7lend", "Laub"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "NN", "$,", "PWAV", "$,", "NN", "KON", "ADJD", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Ist Eines dir, Geschichte, Grab, Geburt.", "tokens": ["Ist", "Ei\u00b7nes", "dir", ",", "Ge\u00b7schich\u00b7te", ",", "Grab", ",", "Ge\u00b7burt", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VAFIN", "PIS", "PPER", "$,", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Und wir, die all des herrlichen Phantomes", "tokens": ["Und", "wir", ",", "die", "all", "des", "herr\u00b7li\u00b7chen", "Phan\u00b7to\u00b7mes"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "$,", "PRELS", "PIAT", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Erhabnen Anblick hochentz\u00fcckt erschau'n,", "tokens": ["Er\u00b7hab\u00b7nen", "An\u00b7blick", "ho\u00b7chent\u00b7z\u00fcckt", "er\u00b7schau'n", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wir m\u00fcssen wie in Wogen eines Stromes,", "tokens": ["Wir", "m\u00fcs\u00b7sen", "wie", "in", "Wo\u00b7gen", "ei\u00b7nes", "Stro\u00b7mes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "KOKOM", "APPR", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "All unser Gl\u00fcck, der Enkel Erbe bau'n.", "tokens": ["All", "un\u00b7ser", "Gl\u00fcck", ",", "der", "En\u00b7kel", "Er\u00b7be", "bau'", "n."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "abbreviation"], "pos": ["PIAT", "PPOSAT", "NN", "$,", "ART", "NN", "NN", "VVFIN", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Nur so entsteht des Lebens Wichtigkeit,", "tokens": ["Nur", "so", "ent\u00b7steht", "des", "Le\u00b7bens", "Wich\u00b7tig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Nur so erbl\u00fcht des Staubes Unterschied.", "tokens": ["Nur", "so", "er\u00b7bl\u00fcht", "des", "Stau\u00b7bes", "Un\u00b7ter\u00b7schied", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "O, wer durchwandeln jeden Geist der Zeit", "tokens": ["O", ",", "wer", "durch\u00b7wan\u00b7deln", "je\u00b7den", "Geist", "der", "Zeit"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "PWS", "VVFIN", "PIAT", "NN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und leben k\u00f6nnte, wie ein ewig Lied!", "tokens": ["Und", "le\u00b7ben", "k\u00f6nn\u00b7te", ",", "wie", "ein", "e\u00b7wig", "Lied", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VMFIN", "$,", "PWAV", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Doch du nur quillst lebendig jeder Quelle,", "tokens": ["D