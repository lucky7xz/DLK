{"textgrid.poem.33085": {"metadata": {"author": {"name": "B\u00fcchner, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nichts r\u00fchrt die Seele an so g\u00f6ttlich sch\u00f6n,", "genre": "verse", "period": "N.A.", "pub_year": 1849, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nichts r\u00fchrt die Seele an so g\u00f6ttlich sch\u00f6n,", "tokens": ["Nichts", "r\u00fchrt", "die", "See\u00b7le", "an", "so", "g\u00f6tt\u00b7lich", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "APPR", "ADV", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Als sich in einem Andern selbst zu f\u00fchlen,", "tokens": ["Als", "sich", "in", "ei\u00b7nem", "An\u00b7dern", "selbst", "zu", "f\u00fch\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "APPR", "ART", "ADJA", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Gedanken, die gestaltlos in uns w\u00fchlen,", "tokens": ["Ge\u00b7dan\u00b7ken", ",", "die", "ge\u00b7stalt\u00b7los", "in", "uns", "w\u00fch\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ADJD", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In edler Form verk\u00f6rpert vor uns seh'n.", "tokens": ["In", "ed\u00b7ler", "Form", "ver\u00b7k\u00f6r\u00b7pert", "vor", "uns", "seh'", "n."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "APPR", "PPER", "VVFIN", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Den Dichter hat dein Auge nie erblickt,", "tokens": ["Den", "Dich\u00b7ter", "hat", "dein", "Au\u00b7ge", "nie", "er\u00b7blickt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und pl\u00f6tzlich steht, ein Freund, er dir zur Seite,", "tokens": ["Und", "pl\u00f6tz\u00b7lich", "steht", ",", "ein", "Freund", ",", "er", "dir", "zur", "Sei\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "$,", "ART", "NN", "$,", "PPER", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und manchem Zweifel, manchem stillen Leide", "tokens": ["Und", "man\u00b7chem", "Zwei\u00b7fel", ",", "man\u00b7chem", "stil\u00b7len", "Lei\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "$,", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Hat deinen Geist auf einmal er entr\u00fcckt.", "tokens": ["Hat", "dei\u00b7nen", "Geist", "auf", "ein\u00b7mal", "er", "ent\u00b7r\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "APPR", "ADV", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Du irrest nicht \u2013 denn sieh! so denkt er auch,", "tokens": ["Du", "ir\u00b7rest", "nicht", "\u2013", "denn", "sieh", "!", "so", "denkt", "er", "auch", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$(", "KON", "VVIMP", "$.", "ADV", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Dein Herz spricht wahr \u2013 im Seinen ist erklung