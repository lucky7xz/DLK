{"textgrid.poem.66079": {"metadata": {"author": {"name": "Falke, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Ein Fr\u00fchlingslied", "genre": "verse", "period": "N.A.", "pub_year": 1884, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein V\u00f6glein fliegt im Winde", "tokens": ["Ein", "V\u00f6\u00b7glein", "fliegt", "im", "Win\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit seinen bunten Fl\u00fcgeln", "tokens": ["Mit", "sei\u00b7nen", "bun\u00b7ten", "Fl\u00fc\u00b7geln"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und kann sich gar nicht z\u00fcgeln.", "tokens": ["Und", "kann", "sich", "gar", "nicht", "z\u00fc\u00b7geln", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PRF", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie fliegt es so geschwinde,", "tokens": ["Wie", "fliegt", "es", "so", "ge\u00b7schwin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Das kleine V\u00f6glein Hoffen;", "tokens": ["Das", "klei\u00b7ne", "V\u00f6\u00b7glein", "Hof\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die Welt steht ihm ja offen.", "tokens": ["Die", "Welt", "steht", "ihm", "ja", "of\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ein M\u00e4gdlein geht im Garten,", "tokens": ["Ein", "M\u00e4gd\u00b7lein", "geht", "im", "Gar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Jungfr\u00e4ulich schon zu nennen.", "tokens": ["Jung\u00b7fr\u00e4u\u00b7lich", "schon", "zu", "nen\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die ersten Tulpen brennen,", "tokens": ["Die", "ers\u00b7ten", "Tul\u00b7pen", "bren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die ersten Veilchen warten.", "tokens": ["Die", "ers\u00b7ten", "Veil\u00b7chen", "war\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Es lugt nach einem Kr\u00e4nzlein", "tokens": ["Es", "lugt", "nach", "ei\u00b7nem", "Kr\u00e4nz\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und sehnt nach einem T\u00e4nzlein.", "tokens": ["Und", "sehnt", "nach", "ei\u00b7nem", "T\u00e4nz\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und wo willst du denn tanzen gehn,", "tokens": ["Und", "wo", "willst", "du", "denn", "tan\u00b7zen", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "VMFIN", "PPER", "ADV", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du junge Freude, du?", "tokens": ["Du", "jun\u00b7ge", "Freu\u00b7de", ",", "du", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Soweit die weichen Winde wehn,", "tokens": ["So\u00b7weit", "die", "wei\u00b7chen", "Win\u00b7de", "wehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Soweit in hohem Bogen", "tokens": ["So\u00b7weit", "in", "ho\u00b7hem", "Bo\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Das Hoffen ist geflogen,", "tokens": ["Das", "Hof\u00b7fen", "ist", "ge\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Das liebe schnelle V\u00f6gelein,", "tokens": ["Das", "lie\u00b7be", "schnel\u00b7le", "V\u00f6\u00b7ge\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So weit \u2013 willst du mein T\u00e4nzer sein? \u2013", "tokens": ["So", "weit", "\u2013", "willst", "du", "mein", "T\u00e4n\u00b7zer", "sein", "?", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADJD", "$(", "VMFIN", "PPER", "PPOSAT", "NN", "VAINF", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Tr\u00e4gt mich mein goldner Schuh.", "tokens": ["Tr\u00e4gt", "mich", "mein", "gold\u00b7ner", "Schuh", "."],