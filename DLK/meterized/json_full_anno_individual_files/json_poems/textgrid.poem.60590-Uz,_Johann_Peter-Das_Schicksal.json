{"textgrid.poem.60590": {"metadata": {"author": {"name": "Uz, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "Das Schicksal", "genre": "verse", "period": "N.A.", "pub_year": 1758, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nicht immer wird das Gl\u00fcck den Schaaren Oestreichs lachen:", "tokens": ["Nicht", "im\u00b7mer", "wird", "das", "Gl\u00fcck", "den", "Schaa\u00b7ren", "O\u00b7e\u00b7streichs", "la\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "VAFIN", "ART", "NN", "ART", "NN", "NE", "VVINF", "$."], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Bald, bald siegt wieder Preu\u00dfens Held.", "tokens": ["Bald", ",", "bald", "siegt", "wie\u00b7der", "Preu\u00b7\u00dfens", "Held", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "VVFIN", "ADV", "NE", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der gro\u00dfe Friedrich wird schrecklicher erwachen,", "tokens": ["Der", "gro\u00b7\u00dfe", "Fried\u00b7rich", "wird", "schreck\u00b7li\u00b7cher", "er\u00b7wa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "VAFIN", "ADJD", "VVINF", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Im waffenvollen Feld.", "tokens": ["Im", "waf\u00b7fen\u00b7vol\u00b7len", "Feld", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der Krieg erhob ietzt ihn, ietzt seine Feinde wieder,", "tokens": ["Der", "Krieg", "er\u00b7hob", "ietzt", "ihn", ",", "ietzt", "sei\u00b7ne", "Fein\u00b7de", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "PPER", "$,", "ADV", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der unentschiedne lange Krieg:", "tokens": ["Der", "un\u00b7ent\u00b7schied\u00b7ne", "lan\u00b7ge", "Krieg", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Noch fliegt von Heer zu Heer mit blutigem Gefieder", "tokens": ["Noch", "fliegt", "von", "Heer", "zu", "Heer", "mit", "blu\u00b7ti\u00b7gem", "Ge\u00b7fie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "NN", "APPR", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der zweifelhafte Sieg.", "tokens": ["Der", "zwei\u00b7fel\u00b7haf\u00b7te", "Sieg", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Bewaffnet mit dem Blitz, zieht auf gebahnten Wegen", "tokens": ["Be\u00b7waff\u00b7net", "mit", "dem", "Blitz", ",", "zieht", "auf", "ge\u00b7bahn\u00b7ten", "We\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Noch einmal Friedrich gegen Wien:", "tokens": ["Noch", "ein\u00b7mal", "Fried\u00b7rich", "ge\u00b7gen", "Wi\u00b7en", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "NE", "APPR", "NE", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Das Schicksal stellet ihm ein Olm\u00fctz nun entgegen,", "tokens": ["Das", "Schick\u00b7sal", "stel\u00b7let", "ihm", "ein", "Ol\u00b7m\u00fctz", "nun", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ART", "NN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wie vormals ein Collin.", "tokens": ["Wie", "vor\u00b7mals", "ein", "Col\u00b7lin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wir sahen Friedrichs Stadt bedroht auf allen Seiten", "tokens": ["Wir", "sa\u00b7hen", "Fried\u00b7richs", "Stadt", "be\u00b7droht", "auf", "al\u00b7len", "Sei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NE", "NN", "VVFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von Ueberschwemmungen der Wuth:", "tokens": ["Von", "Ue\u00b7ber\u00b7schwem\u00b7mun\u00b7gen", "der", "Wuth", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Doch legte nicht ein Gott bey Ro\u00dfbach und bey Leuthen", "tokens": ["Doch", "leg\u00b7te", "nicht", "ein", "Gott", "bey", "Ro\u00df\u00b7bach", "und", "bey", "Leu\u00b7then"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PTKNEG", "ART", "NN", "APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die aufgeschwollne Fluth?", "tokens": ["Die", "auf\u00b7ge\u00b7schwoll\u00b7ne", "Fluth", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Den stolzesten Entwurf, der schon Verderben br\u00fctet,", "tokens": ["Den", "stol\u00b7zes\u00b7ten", "Ent\u00b7wurf", ",", "der", "schon", "Ver\u00b7der\u00b7ben", "br\u00fc\u00b7tet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ADV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und loszubrechen fertig steht,", "tokens": ["Und", "los\u00b7zu\u00b7bre\u00b7chen", "fer\u00b7tig", "steht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hat oft der Ewige, der K\u00f6nigen gebiethet,", "tokens": ["Hat", "oft", "der", "E\u00b7wi\u00b7ge", ",", "der", "K\u00f6\u00b7ni\u00b7gen", "ge\u00b7bie\u00b7thet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Durch einen Hauch verweht.", "tokens": ["Durch", "ei\u00b7nen", "Hauch", "ver\u00b7weht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Dem Thun der Sterblichen hat er ein Ziel gestecket,", "tokens": ["Dem", "Thun", "der", "Sterb\u00b7li\u00b7chen", "hat", "er", "ein", "Ziel", "ge\u00b7ste\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Nach welchem, still und unbemerkt,", "tokens": ["Nach", "wel\u00b7chem", ",", "still", "und", "un\u00b7be\u00b7merkt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Selbst fliehend, alles geht, eh uns die Furcht erschrecket,", "tokens": ["Selbst", "flie\u00b7hend", ",", "al\u00b7les", "geht", ",", "eh", "uns", "die", "Furcht", "er\u00b7schre\u00b7cket", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "$,", "PIS", "VVFIN", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Eh uns die Hoffnung st\u00e4rkt.", "tokens": ["Eh", "uns", "die", "Hoff\u00b7nung", "st\u00e4rkt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.7": {"line.1": {"text": "Ich sollt' erz\u00fcrntem Gl\u00fcck ein thr\u00e4nend Antlitz zeigen,", "tokens": ["Ich", "sollt'", "er\u00b7z\u00fcrn\u00b7tem", "Gl\u00fcck", "ein", "thr\u00e4\u00b7nend", "Ant\u00b7litz", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADJA", "NN", "ART", "ADJD", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mich frech erheben, wann es lacht?", "tokens": ["Mich", "frech", "er\u00b7he\u00b7ben", ",", "wann", "es", "lacht", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVINF", "$,", "PWAV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich Sterblicher will nicht den stolzen Nacken beugen,", "tokens": ["Ich", "Sterb\u00b7li\u00b7cher", "will", "nicht", "den", "stol\u00b7zen", "Na\u00b7cken", "beu\u00b7gen", 