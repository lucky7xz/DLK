{"dta.poem.13001": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Florette an Leandern.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Leander solte mir zuvor von hoffnung singen,", "tokens": ["Le\u00b7an\u00b7der", "sol\u00b7te", "mir", "zu\u00b7vor", "von", "hoff\u00b7nung", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPER", "ADV", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Noch eh mein stumpffer kiel die antwort aufgesetzt.", "tokens": ["Noch", "eh", "mein", "stumpf\u00b7fer", "kiel", "die", "ant\u00b7wort", "auf\u00b7ge\u00b7setzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPOSAT", "ADJA", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Denn eben diese k\u00f6nt\u2019 ihn zur ", "tokens": ["Denn", "e\u00b7ben", "die\u00b7se", "k\u00f6nt'", "ihn", "zur"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "PDS", "VMFIN", "PPER", "APPRART"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und also w\u00fcrde ja mein hoffnungs-lied verletzt.", "tokens": ["Und", "al\u00b7so", "w\u00fcr\u00b7de", "ja", "mein", "hoff\u00b7nungs\u00b7lied", "ver\u00b7letzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}