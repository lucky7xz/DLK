"alexandrine.iambic.hexa"}, "line.61": {"text": "Da\u00df den erbla\u00dften leib ein enger sarg umschr\u00e4nckt.", "tokens": ["Da\u00df", "den", "er\u00b7bla\u00df\u00b7ten", "leib", "ein", "en\u00b7ger", "sarg", "um\u00b7schr\u00e4nckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "ART", "ADJD", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.62": {"text": "Doch wie den reinen geist das paradies umf\u00e4ngt;", "tokens": ["Doch", "wie", "den", "rei\u00b7nen", "geist", "das", "pa\u00b7ra\u00b7dies", "um\u00b7f\u00e4ngt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "ADJA", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.63": {"text": "So soll dein ehren-ruf auch hier unsterblich bleiben.", "tokens": ["So", "soll", "dein", "eh\u00b7ren\u00b7ruf", "auch", "hier", "uns\u00b7terb\u00b7lich", "blei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPOSAT", "NN", "ADV", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.64": {"text": "Die wahrheit selber wird auf deinen leich-stein schreiben:", "tokens": ["Die", "wahr\u00b7heit", "sel\u00b7ber", "wird", "auf", "dei\u00b7nen", "leich\u00b7stein", "schrei\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.65": {"text": "Hier lieget, was die welt nicht ohne schmertz vermist,", "tokens": ["Hier", "lie\u00b7get", ",", "was", "die", "welt", "nicht", "oh\u00b7ne", "schmertz", "ver\u00b7mist", ","], "token_info": ["word", "word", "pun