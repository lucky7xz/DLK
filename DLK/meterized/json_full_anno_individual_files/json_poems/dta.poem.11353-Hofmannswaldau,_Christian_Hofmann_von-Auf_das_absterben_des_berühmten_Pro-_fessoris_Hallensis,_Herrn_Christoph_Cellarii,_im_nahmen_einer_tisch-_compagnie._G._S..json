{"dta.poem.11353": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf das absterben des ber\u00fchmten  Pro-  \n fessoris Hallensis,  Herrn Christoph  Cellarii,  \n im nahmen einer tisch-  \n compagnie.  \n  G. S.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sich in dem labyrinth der alten nicht vergehn:", "tokens": ["Sich", "in", "dem", "la\u00b7by\u00b7rinth", "der", "al\u00b7ten", "nicht", "ver\u00b7gehn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "ART", "ADJA", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die wunder von Athen und Latium verstehn:", "tokens": ["Die", "wun\u00b7der", "von", "A\u00b7then", "und", "La\u00b7ti\u00b7um", "ver\u00b7stehn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "KON", "NN", "VVINF", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Der sprachen zierlichkeit recht aus dem grunde wissen:", "tokens": ["Der", "spra\u00b7chen", "zier\u00b7lich\u00b7keit", "recht", "aus", "dem", "grun\u00b7de", "wis\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der k\u00fcnste weiten kreis in das gehirne schliessen:", "tokens": ["Der", "k\u00fcns\u00b7te", "wei\u00b7ten", "kreis", "in", "das", "ge\u00b7hir\u00b7ne", "schlies\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "APPR", "ART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Aus finstrer schrifften nacht ein licht der wahrheit ziehn:", "tokens": ["Aus", "finst\u00b7rer", "schriff\u00b7ten", "nacht", "ein", "licht", "der", "wahr\u00b7heit", "ziehn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Sich um den grauen witz vergangner zeit bem\u00fchn:", "tokens": ["Sich", "um", "den", "grau\u00b7en", "witz", "ver\u00b7gang\u00b7ner", "zeit", "be\u00b7m\u00fchn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "ADJA", "NN", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Den halb-verfallnen schacht des alterthums durchkriechen:", "tokens": ["Den", "halb\u00b7ver\u00b7fall\u00b7nen", "schacht", "des", "al\u00b7ter\u00b7thums", "durch\u00b7krie\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "ART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Das, was der nasse schwamm der jahre weggestrichen,", "tokens": ["Das", ",", "was", "der", "nas\u00b7se", "schwamm", "der", "jah\u00b7re", "weg\u00b7ge\u00b7stri\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "ART", "NN", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Und sonst zerst\u00fccket liegt, wie ein zerschmettert glas,", "tokens": ["Und", "sonst", "zer\u00b7st\u00fc\u00b7cket", "liegt", ",", "wie", "ein", "zer\u00b7schmet\u00b7tert", "glas", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "VVFIN", "$,", "PWAV", "ART", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Da man die st\u00fccke kaum mit m\u00fch zusammen las,", "tokens": ["Da", "man", "die", "st\u00fc\u00b7cke", "kaum", "mit", "m\u00fch", "zu\u00b7sam\u00b7men", "las", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "VVFIN", "ADV", "APPR", "ADJD", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Durch hurtigen verstand ersetzen und erg\u00e4ntzen:", "tokens": ["Durch", "hur\u00b7ti\u00b7gen", "ver\u00b7stand", "er\u00b7set\u00b7zen", "und", "er\u00b7g\u00e4nt\u00b7zen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Sich \u00fcber den bezirck und die gemeine grentzen", "tokens": ["Sich", "\u00fc\u00b7ber", "den", "be\u00b7zirck", "und", "die", "ge\u00b7mei\u00b7ne", "grent\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PRF", "APPR", "ART", "NN", "KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Des p\u00f6vels selbst erh\u00f6hn: Was Socrates gefragt:", "tokens": ["Des", "p\u00f6\u00b7vels", "selbst", "er\u00b7h\u00f6hn", ":", "Was", "So\u00b7cra\u00b7tes", "ge\u00b7fragt", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVINF", "$.", "PWS", "NE", "VVPP", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "Democritus belacht: Heraclitus beklagt:", "tokens": ["De\u00b7mo\u00b7cri\u00b7tus", "be\u00b7lacht", ":", "Her\u00b7a\u00b7cli\u00b7tus", "be\u00b7klagt", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVPP", "$.", "NE", "VVFIN", "$."], "meter": "-+-+-++--+-+", "measure": "iambic.hexa.relaxed"}, "line.15": {"text": "Der ruhig\u2019 Epicur nach seiner lust ersonnen;", "tokens": ["Der", "ru\u00b7hig'", "E\u00b7pi\u00b7cur", "nach", "sei\u00b7ner", "lust", "er\u00b7son\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Was Pyrrho vor ein garn des zweifels angesponnen,", "tokens": ["Was", "Pyrr\u00b7ho", "vor", "ein", "garn", "des", "zwei\u00b7fels", "an\u00b7ge\u00b7spon\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NE", "APPR", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Und wie ein seiden-wurm sich selbst darein verwebt:", "tokens": ["Und", "wie", "ein", "sei\u00b7den\u00b7wurm", "sich", "selbst", "da\u00b7rein", "ver\u00b7webt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "PRF", "ADV", "PAV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Wie Aristoteles nach dem bewei\u00df gestrebt:", "tokens": ["Wie", "A\u00b7ris\u00b7to\u00b7te\u00b7les", "nach", "dem", "be\u00b7wei\u00df", "ge\u00b7strebt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "APPR", "ART", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Gewissen grund gesucht: Das g\u00f6ldne mittel funden,", "tokens": ["Ge\u00b7wis\u00b7sen", "grund", "ge\u00b7sucht", ":", "Das", "g\u00f6ld\u00b7ne", "mit\u00b7tel", "fun\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "$.", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "In dem die tugend ruht: Warum sein ruhm verschwunden?", "tokens": ["In", "dem", "die", "tu\u00b7gend", "ruht", ":", "Wa\u00b7rum", "sein", "ruhm", "ver\u00b7schwun\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "VVFIN", "$.", "PWAV", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Was in der \u00e4ltern welt der sieben weisen mund", "tokens": ["Was", "in", "der", "\u00e4l\u00b7tern", "welt", "der", "sie\u00b7ben", "wei\u00b7sen", "mund"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "ART", "ADJA", "NN", "ART", "CARD", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Vor r\u00e4thsel aufgel\u00f6st: Des Hermes tieffen grund:", "tokens": ["Vor", "r\u00e4th\u00b7sel", "auf\u00b7ge\u00b7l\u00f6st", ":", "Des", "Her\u00b7mes", "tief\u00b7fen", "grund", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$.", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Und was Pythagoras aus stoltz und neid verschwiegen:", "tokens": ["Und", "was", "Py\u00b7tha\u00b7go\u00b7ras", "aus", "stoltz", "und", "neid", "ver\u00b7schwie\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "NE", "APPR", "NE", "KON", "NN", "VVPP", "$."], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.24": {"text": "Wie sich Empedocles durch abgeschmackte l\u00fcgen", "tokens": ["Wie", "sich", "Em\u00b7pe\u00b7do\u00b7cles", "durch", "ab\u00b7ge\u00b7schmack\u00b7te", "l\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PRF", "NE", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Durchaus verg\u00f6ttern wolln: Was Plato speculirt,", "tokens": ["Durc\u00b7haus", "ver\u00b7g\u00f6t\u00b7tern", "wolln", ":", "Was", "Pla\u00b7to", "spe\u00b7cu\u00b7lirt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "VMFIN", "$.", "PWS", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Und vor ideen einst als wei\u00dfheit eingef\u00fchrt:", "tokens": ["Und", "vor", "i\u00b7deen", "einst", "als", "wei\u00df\u00b7heit", "ein\u00b7ge\u00b7f\u00fchrt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NE", "ADV", "KOUS", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.27": {"text": "Wie grob und h\u00fcndisch sich der Cynicus vergangen:", "tokens": ["Wie", "grob", "und", "h\u00fcn\u00b7disch", "sich", "der", "Cy\u00b7ni\u00b7cus", "ver\u00b7gan\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "KON", "ADJD", "PRF", "ART", "NE", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Wie Zeno sich gesch\u00e4mt: Womit die redner prangen,", "tokens": ["Wie", "Ze\u00b7no", "sich", "ge\u00b7sch\u00e4mt", ":", "Wo\u00b7mit", "die", "red\u00b7ner", "pran\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "PRF", "VVPP", "$.", "PWAV", "ART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Die Rom, die Griechenland erstaunend angeh\u00f6rt:", "tokens": ["Die", "Rom", ",", "die", "Grie\u00b7chen\u00b7land", "er\u00b7stau\u00b7nend", "an\u00b7ge\u00b7h\u00f6rt", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "$,", "ART", "NN", "VVPP", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Was ein Quintilian, ein Tullius gelehrt:", "tokens": ["Was", "ein", "Quin\u00b7ti\u00b7li\u00b7an", ",", "ein", "Tul\u00b7li\u00b7us", "ge\u00b7lehrt", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NE", "$,", "ART", "NN", "VVPP", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.31": {"text": "Der pr\u00e4chtige poet in seinen vers geschlossen:", "tokens": ["Der", "pr\u00e4ch\u00b7ti\u00b7ge", "poet", "in", "sei\u00b7nen", "vers", "ge\u00b7schlos\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.32": {"text": "Was Livius erzehlt: Der alten sch\u00f6ne glossen:", "tokens": ["Was", "Li\u00b7vius", "er\u00b7zehlt", ":", "Der", "al\u00b7ten", "sch\u00f6\u00b7ne", "glos\u00b7sen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NE", "VVFIN", "$.", "ART", "ADJA", "ADJA", "VVINF", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.33": {"text": "Des Strabo ri\u00df der welt: Und was allhier der flei\u00df", "tokens": ["Des", "Stra\u00b7bo", "ri\u00df", "der", "welt", ":", "Und", "was", "all\u00b7hier", "der", "flei\u00df"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$.", "KON", "PWS", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Auf dieses enge blat nicht zu entwerssen wei\u00df:", "tokens": ["Auf", "die\u00b7ses", "en\u00b7ge", "blat", "nicht", "zu", "ent\u00b7wers\u00b7sen", "wei\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "PTKNEG", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Di\u00df alles, und noch mehr in seinem kopff verfassen,", "tokens": ["Di\u00df", "al\u00b7les", ",", "und", "noch", "mehr", "in", "sei\u00b7nem", "kopff", "ver\u00b7fas\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "$,", "KON", "ADV", "ADV", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Und doch den hochmuth sich nicht \u00fcbermeistern lassen:", "tokens": ["Und", "doch", "den", "hoch\u00b7muth", "sich", "nicht", "\u00fc\u00b7ber\u00b7meis\u00b7tern", "las\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "PRF", "PTKNEG", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Di\u00df heist ein ", "tokens": ["Di\u00df", "heist", "ein"], "token_info": ["word", "word", "word"], "pos": ["PDS", "VAFIN", "ART"], "meter": "+-+", "measure": "trochaic.di"}, "line.38": {"text": "Wie rar ist dieses lob! Die wissenschafft allein", "tokens": ["Wie", "rar", "ist", "die\u00b7ses", "lob", "!", "Die", "wis\u00b7sen\u00b7schafft", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VAFIN", "PDAT", "NN", "$.", "ART", "NN", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Findt jezuweilen wohl ein f\u00e4higes gehirne:", "tokens": ["Findt", "je\u00b7zu\u00b7wei\u00b7len", "wohl", "ein", "f\u00e4\u00b7hi\u00b7ges", "ge\u00b7hir\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "So steigt Salmasius vor vielen zum gestirne,", "tokens": ["So", "steigt", "Sal\u00b7ma\u00b7sius", "vor", "vie\u00b7len", "zum", "ge\u00b7stir\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "APPR", "PIAT", "APPRART", "ADJA", "$,"], "meter": "--+-+-+-+-+-", "measure": "anapaest.init"}, "line.41": {"text": "F\u00e4llt aber durch den stoltz weit tieffer, als er steigt.", "tokens": ["F\u00e4llt", "a\u00b7ber", "durch", "den", "stoltz", "weit", "tief\u00b7fer", ",", "als", "er", "steigt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ART", "NN", "ADJD", "ADJD", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Die unersch\u00f6pffte m\u00fch, so ein Menage zeigt:", "tokens": ["Die", "un\u00b7er\u00b7sch\u00f6pff\u00b7te", "m\u00fch", ",", "so", "ein", "Me\u00b7na\u00b7ge", "zeigt", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJD", "$,", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Sein critischer geschmack, sein angebrachtes lesen,", "tokens": ["Sein", "cri\u00b7ti\u0