{"textgrid.poem.44429": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[so war es nicht gemeint!]", "genre": "verse", "period": "N.A.", "pub_year": 1828, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So war es nicht gemeint!", "tokens": ["So", "war", "es", "nicht", "ge\u00b7meint", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Die Sch\u00f6nheit war das Gut der Geistesschwachen,", "tokens": ["Die", "Sch\u00f6n\u00b7heit", "war", "das", "Gut", "der", "Geis\u00b7tes\u00b7schwa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Uns andern, die kein Anmutsstrahl bescheint,", "tokens": ["Uns", "an\u00b7dern", ",", "die", "kein", "An\u00b7muts\u00b7strahl", "be\u00b7scheint", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "$,", "PRELS", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Verlieh ein Gott, zu bilden und zu machen;", "tokens": ["Ver\u00b7lieh", "ein", "Gott", ",", "zu", "bil\u00b7den", "und", "zu", "ma\u00b7chen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Doch beider Gunst auf einem Haupt vereint?", "tokens": ["Doch", "bei\u00b7der", "Gunst", "auf", "ei\u00b7nem", "Haupt", "ver\u00b7eint", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["