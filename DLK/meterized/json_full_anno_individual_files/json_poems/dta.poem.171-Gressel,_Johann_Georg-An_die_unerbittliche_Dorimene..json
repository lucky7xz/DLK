{"dta.poem.171": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "An die unerbittliche  Dorimene.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Will Dorimene nicht dein harter Sinn erweichen?", "tokens": ["Will", "Do\u00b7ri\u00b7me\u00b7ne", "nicht", "dein", "har\u00b7ter", "Sinn", "er\u00b7wei\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NE", "PTKNEG", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und gibst du keinen Blick zu einem Gnaden-Zeichen?", "tokens": ["Und", "gibst", "du", "kei\u00b7nen", "Blick", "zu", "ei\u00b7nem", "Gna\u00b7den\u00b7Zei\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PIAT", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mu\u00df stets dein Augen-Licht ein Zorn ", "tokens": ["Mu\u00df", "stets", "dein", "Au\u00b7gen\u00b7Licht", "ein", "Zorn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "PPOSAT", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Schau deine Grausamkeit benimmt ihm Glantz und Schein!", "tokens": ["Schau", "dei\u00b7ne", "Grau\u00b7sam\u00b7keit", "be\u00b7nimmt", "ihm", "Glantz", "und", "Schein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "VVFIN", "PPER", "NN", "KON", "NN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.5": {"text": "Wohnt in dem sch\u00f6nen Leib so eine schwartze Seele?", "tokens": ["Wohnt", "in", "dem", "sch\u00f6\u00b7nen", "Leib", "so", "ei\u00b7ne", "schwart\u00b7ze", "See\u00b7le", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Verwirfft dein Paradies in solche Kummer-H\u00f6le?", "tokens": ["Ver\u00b7wirfft", "dein", "Pa\u00b7ra\u00b7dies", "in", "sol\u00b7che", "Kum\u00b7mer\u00b7H\u00f6le", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Lacht dein verliebt Gesicht mir zum Verderben an!", "tokens": ["Lacht", "dein", "ver\u00b7liebt", "Ge\u00b7sicht", "mir", "zum", "Ver\u00b7der\u00b7ben", "an", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "VVFIN", "NN", "PPER", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ja! der beliebte Weg f\u00fchrt auf die Todes-Bahn.", "tokens": ["Ja", "!", "der", "be\u00b7lieb\u00b7te", "Weg", "f\u00fchrt", "auf", "die", "To\u00b7des\u00b7Bahn", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ist die Erbarmni\u00df denn so gar aus dir verbannet/", "tokens": ["Ist", "die", "Er\u00b7barm\u00b7ni\u00df", "denn", "so", "gar", "aus", "dir", "ver\u00b7ban\u00b7net", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "KON", "ADV", "ADV", "APPR", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Da\u00df deine Freundlichkeit der Stoltz stets \u00fcbermannet?", "tokens": ["Da\u00df", "dei\u00b7ne", "Freund\u00b7lich\u00b7keit", "der", "Stoltz", "stets", "\u00fc\u00b7berm\u00b7an\u00b7net", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Mitleiden zieht bey dir das trotze Seegel ein/", "tokens": ["Mit\u00b7lei\u00b7den", "zieht", "bey", "dir", "das", "trot\u00b7ze", "See\u00b7gel", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PPER", "ART", "ADJA", "NN", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Du wilt ergrimmeter als L\u00f6w und Tyger seyn", "tokens": ["Du", "wilt", "er\u00b7grim\u00b7me\u00b7ter", "als", "L\u00f6w", "und", "Ty\u00b7ger", "seyn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADJD", "KOKOM", "NE", "KON", "NN", "VAINF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Mich soll dein lachend Aug mit Lust zu Grabe leuchten/", "tokens": ["Mich", "soll", "dein", "la\u00b7chend", "Aug", "mit", "Lust", "zu", "Gra\u00b7be", "leuch\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "ADJD", "NN", "APPR", "NN", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Du wilt den glimmen Tocht vor Oel mit Wasser feuchten/", "tokens": ["Du", "wilt", "den", "glim\u00b7men", "Tocht", "vor", "O\u00b7el", "mit", "Was\u00b7ser", "feuch\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN", "APPR", "NN", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.15": {"text": "Du reichst vor Msndeln-Milch mir Pilsen-S\u00e4ffte dar/", "tokens": ["Du", "reichst", "vor", "Msn\u00b7deln\u00b7Milch", "mir", "Pil\u00b7sen\u00b7S\u00e4ff\u00b7te", "dar", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "PPER", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und deine Freundlichkeit bereitet mir die Bahr.", "tokens": ["Und", "dei\u00b7ne", "Freund\u00b7lich\u00b7keit", "be\u00b7rei\u00b7tet", "mir", "die", "Bahr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Du machst dich gro\u00df damit die Menschen zu ert\u00f6dten/", "tokens": ["Du", "machst", "dich", "gro\u00df", "da\u00b7mit", "die", "Men\u00b7schen", "zu", "er\u00b7t\u00f6d\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "PAV", "ART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Und r\u00fchmst die Tyranney ohn eintziges Err\u00f6hten/", "tokens": ["Und", "r\u00fchmst", "die", "Ty\u00b7ran\u00b7ney", "ohn", "eint\u00b7zi\u00b7ges", "Er\u00b7r\u00f6h\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Der Stahl und Kieselstein hegt mehr Empfindlichkeit", "tokens": ["Der", "Stahl", "und", "Kie\u00b7sel\u00b7stein", "hegt", "mehr", "Emp\u00b7find\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Als du/ der die Natur sonst alles eingeweiht.", "tokens": ["Als", "du", "/", "der", "die", "Na\u00b7tur", "sonst", "al\u00b7les", "ein\u00b7ge\u00b7weiht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$(", "ART", "ART", "NN", "ADV", "PIS", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Mit Sch\u00f6nheit ist dein Leib gar pr\u00e4chtig ausgezieret/", "tokens": ["Mit", "Sch\u00f6n\u00b7heit", "ist", "dein", "Leib", "gar", "pr\u00e4ch\u00b7tig", "aus\u00b7ge\u00b7zie\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPOSAT", "NN", "ADV", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Nur schade! da\u00df der Leib ein solches Hertze f\u00fchret/", "tokens": ["Nur", "scha\u00b7de", "!", "da\u00df", "der", "Leib", "ein", "sol\u00b7ches", "Hert\u00b7ze", "f\u00fch\u00b7ret", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$.", "KOUS", "ART", "NN", "ART", "PIAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Dem Demant und ", "tokens": ["Dem", "De\u00b7mant", "und"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "KON"], "meter": "+-+-", "measure": "trochaic.di"}, "line.24": {"text": "Das blo\u00df zur Quaal und Pein mit Anmuth an sich zeucht.", "tokens": ["Das", "blo\u00df", "zur", "Qua\u00b7al", "und", "Pein", "mit", "An\u00b7muth", "an", "sich", "zeucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "APPRART", "NN", "KON", "NN", "APPR", "NN", "APPR", "PRF", "VVFIN", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.25": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.26": {"text": "Das Hertze aber will sich nicht mit ihm verbr\u00fcdern/", "tokens": ["Das", "Hert\u00b7ze", "a\u00b7ber", "will", "sich", "nicht", "mit", "ihm", "ver\u00b7br\u00fc\u00b7dern", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "VMFIN", "PRF", "PTKNEG", "APPR", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.28": {"text": "Der Diamant zerspringt wenn Bockes-Blut ihn t\u00e4ufft.", "tokens": ["Der", "Di\u00b7a\u00b7mant", "zer\u00b7springt", "wenn", "Bo\u00b7ckes\u00b7Blut", "ihn", "t\u00e4ufft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KOUS", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Du aber bleibest hart bey meinem Tod und Sterben/", "tokens": ["Du", "a\u00b7ber", "blei\u00b7best", "hart", "bey", "mei\u00b7nem", "Tod", "und", "Ster\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ADJD", "APPR", "PPOSAT", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Du siehst mit Freuden an mein kl\u00e4gliches Verderben/", "tokens": ["Du", "siehst", "mit", "Freu\u00b7den", "an", "mein", "kl\u00e4g\u00b7li\u00b7ches", "Ver\u00b7der\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Mein Leben f\u00e4llt dahin wie eine ", "tokens": ["Mein", "Le\u00b7ben", "f\u00e4llt", "da\u00b7hin", "wie", "ei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PAV", "KOKOM", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.32": {"text": "Wenn ich den harten Sinn nicht bald erreichen kan.", "tokens": ["Wenn", "ich", "den", "har\u00b7ten", "Sinn", "nicht", "bald", "er\u00b7rei\u00b7chen", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "PTKNEG", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "So h\u00f6re doch nun auf mein kranckes Hertz zu plagen/", "tokens": ["So", "h\u00f6\u00b7re", "doch", "nun", "auf", "mein", "kran\u00b7ckes", "Hertz", "zu", "pla\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "APPR", "PPOSAT", "ADJA", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Was n\u00fctzt es deiner Brust? was hilfft dir mein Verzagen?", "tokens": ["Was", "n\u00fctzt", "es", "dei\u00b7ner", "Brust", "?", "was", "hilfft", "dir", "mein", "Ver\u00b7za\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PPOSAT", "NN", "$.", "PWS", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Wenn du mitleidig bist/ so bist du doppelt sch\u00f6n/", "tokens": ["Wenn", "du", "mit\u00b7lei\u00b7dig", "bist", "/", "so", "bist", "du", "dop\u00b7pelt", "sch\u00f6n", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VAFIN", "$(", "ADV", "VAFIN", "PPER", "ADJD", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Sonst bist du als ein Bild von Ertze anzusehn.", "tokens": ["Sonst", "bist", "du", "als", "ein", "Bild", "von", "Ert\u00b7ze", "an\u00b7zu\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "KOUS", "ART", "NN", "APPR", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}