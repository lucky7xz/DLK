{"textgrid.poem.67739": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Eine Theodicee", "genre": "verse", "period": "N.A.", "pub_year": 1763, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bberwache!\u00ab sprach ein Gott, und sieh! ich sah,", "tokens": ["\u00bb", "er\u00b7wa\u00b7che", "!", "\u00ab", "sprach", "ein", "Gott", ",", "und", "sieh", "!", "ich", "sah", ","], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$.", "$(", "VVFIN", "ART", "NN", "$,", "KON", "VVIMP", "$.", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und weite Nacht war um mich da,", "tokens": ["Und", "wei\u00b7te", "Nacht", "war", "um", "mich", "da", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VAFIN", "APPR", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und \u00fcber mir ein Heer ges\u00e4'ter Sterne", "tokens": ["Und", "\u00fc\u00b7ber", "mir", "ein", "Heer", "ge\u00b7s\u00e4'\u00b7ter", "Ster\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Erhob mein Aug'! Wie der, dem Orkus nah", "tokens": ["Er\u00b7hob", "mein", "Aug'", "!", "Wie", "der", ",", "dem", "Or\u00b7kus", "nah"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "$.", "PWAV", "ART", "$,", "ART", "NN", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "In Cimmers Kluft nur H\u00f6llenstimmen ferne", "tokens": ["In", "Cim\u00b7mers", "Kluft", "nur", "H\u00f6l\u00b7len\u00b7stim\u00b7men", "fer\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "NN", "ADV", "NN", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Herrauschen h\u00f6rt, nur ew'ges Schwarze sah", "tokens": ["Her\u00b7rau\u00b7schen", "h\u00f6rt", ",", "nur", "ew'\u00b7ges", "Schwar\u00b7ze", "sah"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "$,", "ADV", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und, schnell ins Meer des Lichts entz\u00fcckt,", "tokens": ["Und", ",", "schnell", "ins", "Meer", "des", "Lichts", "ent\u00b7z\u00fcckt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADJD", "APPRART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Weit\u00e4ugig starrt \u2013 halb sieht \u2013 nur Zauberei erblickt \u2013", "tokens": ["Wei\u00b7t\u00e4u\u00b7gig", "starrt", "\u2013", "halb", "sieht", "\u2013", "nur", "Zau\u00b7be\u00b7rei", "er\u00b7blickt", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$(", "ADJD", "VVFIN", "$(", "ADV", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Schon sehen lernt \u2013 und sieht, sieht Alles Pracht!", "tokens": ["Schon", "se\u00b7hen", "lernt", "\u2013", "und", "sieht", ",", "sieht", "Al\u00b7les", "Pracht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "VVFIN", "$(", "KON", "VVFIN", "$,", "VVFIN", "PIAT", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.10": {"text": "Er f\u00fchlt's, nennt, stammelt: \u00bb", "tokens": ["Er", "f\u00fchlt's", ",", "nennt", ",", "stam\u00b7melt", ":", "\u00bb"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$.", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.11": {"text": "So stand ich, staunte, griff nach Sternen, staunte mehr,", "tokens": ["So", "stand", "ich", ",", "staun\u00b7te", ",", "griff", "nach", "Ster\u00b7nen", ",", "staun\u00b7te", "mehr", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "VVFIN", "$,", "VVFIN", "APPR", "NN", "$,", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und Wirbelwind ward um mich her.", "tokens": ["Und", "Wir\u00b7bel\u00b7wind", "ward", "um", "mich", "her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Schnell bin ich hoch \u2013 tief unter mir die Erde,", "tokens": ["Schnell", "bin", "ich", "hoch", "\u2013", "tief", "un\u00b7ter", "mir", "die", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "ADJD", "$(", "ADJD", "APPR", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Bei mir ein Gott, Mensch an Geberde,", "tokens": ["Bei", "mir", "ein", "Gott", ",", "Mensch", "an", "Ge\u00b7ber\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "$,", "NN", "APPR", "NN", "$,"], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}, "line.15": {"text": "Vor mir der Sonnenkreis.", "tokens": ["Vor", "mir", "der", "Son\u00b7nen\u00b7kreis", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.16": {"text": "Ich sah Unendliches \u2013 ich f\u00fchl und seh' und h\u00f6re", "tokens": ["Ich", "sah", "Un\u00b7end\u00b7li\u00b7ches", "\u2013", "ich", "f\u00fchl", "und", "seh'", "und", "h\u00f6\u00b7re"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJA", "$(", "PPER", "VVFIN", "KON", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Die Harmonie der ganzen Sph\u00e4re,", "tokens": ["Die", "Har\u00b7mo\u00b7nie", "der", "gan\u00b7zen", "Sph\u00e4\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Was Newton z\u00e4hlt, der Seraph wei\u00df", "tokens": ["Was", "New\u00b7ton", "z\u00e4hlt", ",", "der", "Se\u00b7raph", "wei\u00df"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "NE", "VVFIN", "$,", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Und Gott erschuf. \u2013 Gott, Du bist hier;", "tokens": ["Und", "Gott", "er\u00b7schuf", ".", "\u2013", "Gott", ",", "Du", "bist", "hier", ";"], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "$.", "$(", "NN", "$,", "PPER", "VAFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Der Seraph singt Dir, Newton forscht dort Sterne,", "tokens": ["Der", "Se\u00b7raph", "singt", "Dir", ",", "New\u00b7ton", "forscht", "dort", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "NE", "VVFIN", "ADV", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Die ich von ihm einst, meinem Seraph, lerne,", "tokens": ["Die", "ich", "von", "ihm", "einst", ",", "mei\u00b7nem", "Se\u00b7raph", ",", "ler\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PPER", "ADV", "$,", "PPOSAT", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Und ich \u2013 hier knie' ich Dir.", "tokens": ["Und", "ich", "\u2013", "hier", "knie'", "ich", "Dir", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$(", "ADV", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.23": {"text": "O Du, von dem einst Funken, Sonnen, troffen,", "tokens": ["O", "Du", ",", "von", "dem", "einst", "Fun\u00b7ken", ",", "Son\u00b7nen", ",", "trof\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "NE", "$,", "APPR", "ART", "ADV", "NN", "$,", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Der von dem Chaos Kl\u00f6\u00dfe ri\u00df \u2013", "tokens": ["Der", "von", "dem", "Chaos", "Kl\u00f6\u00b7\u00dfe", "ri\u00df", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "NN", "VVFIN", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.25": {"text": "Noch f\u00fchlen sie den Wurf; sie laufen dort, sie brennen,", "tokens": ["Noch", "f\u00fch\u00b7len", "sie", "den", "Wurf", ";", "sie", "lau\u00b7fen", "dort", ",", "sie", "bren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$.", "PPER", "VVFIN", "ADV", "$,", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Bis einst Dein Wink ins Nichts sie stie\u00df.", "tokens": ["Bis", "einst", "Dein", "Wink", "ins", "Nichts", "sie", "stie\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "PPOSAT", "NN", "APPRART", "PIS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Noch gl\u00e4nzt im Mittelthron die Sonn', Dein Bild,", "tokens": ["Noch", "gl\u00e4nzt", "im", "Mit\u00b7tel\u00b7thron", "die", "Sonn'", ",", "Dein", "Bild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "ART", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "Die um sich Welten ewig ohne Ruh", "tokens": ["Die", "um", "sich", "Wel\u00b7ten", "e\u00b7wig", "oh\u00b7ne", "Ruh"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "PRF", "NN", "ADJD", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.29": {"text": "An goldnen Seilen lenkt. Die stelltest Du!", "tokens": ["An", "gold\u00b7nen", "Sei\u00b7len", "lenkt", ".", "Die", "stell\u00b7test", "Du", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$.", "ART", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.30": {"text": "Wer, wo bist Du? \u2013 K\u00f6nnt' ich Dich, ach, nur sehn, nicht nennen!", "tokens": ["Wer", ",", "wo", "bist", "Du", "?", "\u2013", "K\u00f6nnt'", "ich", "Dich", ",", "ach", ",", "nur", "sehn", ",", "nicht", "nen\u00b7nen", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "$,", "PWAV", "VAFIN", "PPER", "$.", "$(", "VMFIN", "PPER", "PPER", "$,", "ITJ", "$,", "ADV", "VVINF", "$,", "PTKNEG", "VVINF", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.31": {"text": "Wen? \u2013 Dich, in Sonnen und ins Nichts verh\u00fcllt!", "tokens": ["Wen", "?", "\u2013", "Dich", ",", "in", "Son\u00b7nen", "und", "ins", "Nichts", "ver\u00b7h\u00fcllt", "!"], "token_info": ["word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "$.", "$(", "PPER", "$,", "APPR", "NN", "KON", "APPRART", "PIS", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.32": {"text": "Du schufst da Was und Wenn und Wo?", "tokens": ["Du", "schufst", "da", "Was", "und", "Wenn", "und", "Wo", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PWS", "KON", "KOUS", "KON", "PWAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Und bist Du \u2013?", "tokens": ["Und", "bist", "Du", "\u2013", "?"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["KON", "VAFIN", "PPER", "$(", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.34": {"text": "Ach, Erde, Mutter, der ich bin,", "tokens": ["Ach", ",", "Er\u00b7de", ",", "Mut\u00b7ter", ",", "der", "ich", "bin", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "NN", "$,", "NN", "$,", "PRELS", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Was bist Du? mir schon! was dem Erdengeist,", "tokens": ["Was", "bist", "Du", "?", "mir", "schon", "!", "was", "dem", "Er\u00b7den\u00b7geist", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "$.", "PPER", "ADV", "$.", "PWS", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.36": {"text": "Der von dem h\u00f6chsten irdischen Gedanken hin", "tokens": ["Der", "von", "dem", "h\u00f6chs\u00b7ten", "ir\u00b7di\u00b7schen", "Ge\u00b7dan\u00b7ken", "hin"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "ADJA", "ADJA", "NN", "PTKVZ"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "In Deine Tiefe blickt und Engel wird?", "tokens": ["In", "Dei\u00b7ne", "Tie\u00b7fe", "blickt", "und", "En\u00b7gel", "wird", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "KON", "NE", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.38": {"text": "Und was denn Gott? O Gott, mein Auge irrt", "tokens": ["Und", "was", "denn", "Gott", "?", "O", "Gott", ",", "mein", "Au\u00b7ge", "irrt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PWS", "KON", "NN", "$.", "NE", "NN", "$,", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.39": {"text": "Ueber und unter mir umher \u2013 mir wird", "tokens": ["Ue\u00b7ber", "und", "un\u00b7ter", "mir", "um\u00b7her", "\u2013", "mir", "wird"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "KON", "APPR", "PPER", "PTKVZ", "$(", "PPER", "VAFIN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.40": {"text": "Das All zum Nichts, das Nichts zum Allen!", "tokens": ["Das", "All", "zum", "Nichts", ",", "das", "Nichts", "zum", "Al\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "PIS", "$,", "PRELS", "PIS", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.41": {"text": "Du bist im Nichts das All. \u2013 Wer rei\u00dft", "tokens": ["Du", "bist", "im", "Nichts", "das", "All", ".", "\u2013", "Wer", "rei\u00dft"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "APPRART", "PIS", "ART", "NN", "$.", "$(", "PWS", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.42": {"text": "Mich los von Erd' und Sonne? dort sind Sonnen!", "tokens": ["Mich", "los", "von", "Erd'", "und", "Son\u00b7ne", "?", "dort", "sind", "Son\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "APPR", "NN", "KON", "NN", "$.", "ADV", "VAFIN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.43": {"text": "Sonnen, wo ist die, um die Ihr Erden seid?", "tokens": ["Son\u00b7nen", ",", "wo", "ist", "die", ",", "um", "die", "Ihr", "Er\u00b7den", "seid", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "VAFIN", "ART", "$,", "KOUI", "ART", "PPOSAT", "NN", "VAFIN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.44": {"text": "Wo ist des Allen Kraft? wo hat das ", "tokens": ["Wo", "ist", "des", "Al\u00b7len", "Kraft", "?", "wo", "hat", "das"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "ART", "ADJA", "NN", "$.", "PWAV", "VAFIN", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "\u00bberwache!\u00ab sprach ein Gott, und sieh! ich sah,", "tokens": ["\u00bb", "er\u00b7wa\u00b7che", "!", "\u00ab", "sprach", "ein", "Gott", ",", "und", "sieh", "!", "ich", "sah", ","], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$.", "$(", "VVFI