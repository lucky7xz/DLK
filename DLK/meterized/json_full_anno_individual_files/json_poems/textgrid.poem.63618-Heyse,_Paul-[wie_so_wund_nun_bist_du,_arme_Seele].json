{"textgrid.poem.63618": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "[wie so wund nun bist du, arme Seele]", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie so wund nun bist du, arme Seele,", "tokens": ["Wie", "so", "wund", "nun", "bist", "du", ",", "ar\u00b7me", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADJD", "ADV", "VAFIN", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Blutest, ach, verblutest dich nach innen!", "tokens": ["Blu\u00b7test", ",", "ach", ",", "ver\u00b7blu\u00b7test", "dich", "nach", "in\u00b7nen", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ITJ", "$,", "VVFIN", "PRF", "APPR", "ADV", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Gleich der Taube, der das Rohr des J\u00e4gers", "tokens": ["Gleich", "der", "Tau\u00b7be", ",", "der", "das", "Rohr", "des", "J\u00e4\u00b7gers"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "$,", "PRELS", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Ihren Nestling in die Brust getroffen,", "tokens": ["Ih\u00b7ren", "Nest\u00b7ling", "in", "die", "Brust", "ge\u00b7trof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Ihn durchs Herz und sie mit gleichem Schusse", "tokens": ["Ihn", "durchs", "Herz", "und", "sie", "mit", "glei\u00b7chem", "Schus\u00b7se"], "token_info": ["word", "word", "