{"dta.poem.10907": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "No.  12.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.85", "da:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Auf diesem blumigt-flach-und lustigem Gefilde,", "tokens": ["Auf", "die\u00b7sem", "blu\u00b7mig\u00b7tflach\u00b7\u00b7und", "lus\u00b7ti\u00b7gem", "Ge\u00b7fil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wovon das Auge kaum die Fern ergr\u00fcnden kann,", "tokens": ["Wo\u00b7von", "das", "Au\u00b7ge", "kaum", "die", "Fern", "er\u00b7gr\u00fcn\u00b7den", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "ART", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Blicke den Verschie\u00df kaum zu erreichen taugen,", "tokens": ["Die", "Bli\u00b7cke", "den", "Ver\u00b7schie\u00df", "kaum", "zu", "er\u00b7rei\u00b7chen", "tau\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADV", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sieht man verwunderlich fast die Natur im Bilde,", "tokens": ["Sieht", "man", "ver\u00b7wun\u00b7der\u00b7lich", "fast", "die", "Na\u00b7tur", "im", "Bil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADJD", "ADV", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Jm hier versammelten ganz unterschiednen Wilde.", "tokens": ["Jm", "hier", "ver\u00b7sam\u00b7mel\u00b7ten", "ganz", "un\u00b7ter\u00b7schied\u00b7nen", "Wil\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADV", "VVFIN", "ADV", "ADJA", "NN", "$."], "meter": "-+-+---+-+-+-", "measure": "unknown.measure.penta"}}}}}