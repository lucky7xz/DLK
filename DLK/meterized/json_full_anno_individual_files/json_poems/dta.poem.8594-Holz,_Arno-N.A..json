{"dta.poem.8594": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1899", "urn": "urn:nbn:de:kobv:b4-200905192690", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Auf meinen Probiertisch,", "tokens": ["Auf", "mei\u00b7nen", "Pro\u00b7bier\u00b7tisch", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "unter die Schusterkugel,", "tokens": ["un\u00b7ter", "die", "Schus\u00b7ter\u00b7ku\u00b7gel", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "schleppen die jungen, t\u00e4ppischen Riesen mir ihre Missgeburten.", "tokens": ["schlep\u00b7pen", "die", "jun\u00b7gen", ",", "t\u00e4p\u00b7pi\u00b7schen", "Rie\u00b7sen", "mir", "ih\u00b7re", "Miss\u00b7ge\u00b7bur\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "$,", "ADJA", "NN", "PPER", "PPOSAT", "NN", "$."], "meter": "+--+-++-+--+-+-+-", "measure": "iambic.octa.plus.invert"}}, "stanza.2": {"line.1": {"text": "Die leblosen Gliederchen h\u00e4ngen schief, die Aeuglein drehn sich nicht,", "tokens": ["Die", "leb\u00b7lo\u00b7sen", "Glie\u00b7der\u00b7chen", "h\u00e4n\u00b7gen", "schief", ",", "die", "A\u00b7e\u00b7u\u00b7glein", "drehn", "sich", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "VVFIN", "$,", "ART", "NN", "CARD", "PRF", "PTKNEG", "$,"], "meter": "-+--+--+-+-+-+-+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "lauter Alr\u00e4unchen!", "tokens": ["lau\u00b7ter", "Al\u00b7r\u00e4un\u00b7chen", "!"], "token_info": ["word", "word", "punct"], "pos": ["PIAT", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Hier renke ich ein R\u00fcckgrat ein,", "tokens": ["Hier", "ren\u00b7ke", "ich", "ein", "R\u00fcck\u00b7grat", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "dort trepaniere ich eine Sch\u00e4deldecke,", "tokens": ["dort", "tre\u00b7pa\u00b7nie\u00b7re", "ich", "ei\u00b7ne", "Sch\u00e4\u00b7del\u00b7de\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "mit einem Zwirnsfaden, kunstvoll, knipse ich ein Bein ab.", "tokens": ["mit", "ei\u00b7nem", "Zwirns\u00b7fa\u00b7den", ",", "kunst\u00b7voll", ",", "knip\u00b7se", "ich", "ein", "Bein", "ab", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ADJD", "$,", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+--+-+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "Dann nehme ich ein Prieschen,", "tokens": ["Dann", "neh\u00b7me", "ich", "ein", "Prie\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "r\u00fccke die schwarze Hornbrille und stelle die Lampe zurecht.", "tokens": ["r\u00fc\u00b7cke", "die", "schwar\u00b7ze", "Horn\u00b7bril\u00b7le", "und", "stel\u00b7le", "die", "Lam\u00b7pe", "zu\u00b7recht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "KON", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+--+-+---+--+--+", "measure": "iambic.hexa.invert"}}, "stanza.5": {"line.1": {"text": "So.", "tokens": ["So", "."], "token_info": ["word", "punct"], "pos": ["ADV", "$."], "meter": "-", "measure": "single.down"}}, "stanza.6": {"line.1": {"text": "Nun stippe ich in den Farbentopf.", "tokens": ["Nun", "stip\u00b7pe", "ich", "in", "den", "Far\u00b7ben\u00b7topf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "Polichinell, der noch zu gebildet aussieht, kriegt als Nase eine Leberwurst,", "tokens": ["Po\u00b7li\u00b7chi\u00b7nell", ",", "der", "noch", "zu", "ge\u00b7bil\u00b7det", "aus\u00b7sieht", ",", "kriegt", "als", "Na\u00b7se", "ei\u00b7ne", "Le\u00b7ber\u00b7wurst", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "ADV", "PTKZU", "VVPP", "VVFIN", "$,", "VVFIN", "KOUS", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Colombinchen, noch immer nicht sch\u00f6n genug, ein Zinnoberm\u00e4ulchen,", "tokens": ["Co\u00b7lom\u00b7bin\u00b7chen", ",", "noch", "im\u00b7mer", "nicht", "sch\u00f6n", "ge\u00b7nug", ",", "ein", "Zin\u00b7no\u00b7ber\u00b7m\u00e4ul\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "ADV", "PTKNEG", "ADJD", "ADV", "$,", "ART", "NN", "$,"], "meter": "+-+--+--+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "ein qu\u00e4kendes Engelsk\u00fcken, hilft ihm Alles nichts, einen Perlmutterpopo!", "tokens": ["ein", "qu\u00e4\u00b7ken\u00b7des", "En\u00b7gels\u00b7k\u00fc\u00b7ken", ",", "hilft", "ihm", "Al\u00b7les", "nichts", ",", "ei\u00b7nen", "Perl\u00b7mut\u00b7ter\u00b7po\u00b7po", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "VVFIN", "PPER", "PIS", "PIS", "$,", "ART", "NN", "$."], "meter": "-+--+-+-+-+-++-++--+", "measure": "iambic.octa.plus.relaxed"}}}}}