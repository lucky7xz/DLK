{"dta.poem.7723": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "In einem Garten sind drei ungebetne G\u00e4ste;", "tokens": ["In", "ei\u00b7nem", "Gar\u00b7ten", "sind", "drei", "un\u00b7ge\u00b7bet\u00b7ne", "G\u00e4s\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "CARD", "ADJA", "NN", "$."], "meter": "-+-+--++-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Die Aepfel fressen sie und brechen noch die Aeste.", "tokens": ["Die", "A\u00b7e\u00b7pfel", "fres\u00b7sen", "sie", "und", "bre\u00b7chen", "noch", "die", "A\u00b7es\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "KON", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+--+-+-+-+-+-+", "measure": "iambic.septa.relaxed"}}, "stanza.2": {"line.1": {"text": "Der G\u00e4rtner wehrlos ist gewachsen nicht den drein,", "tokens": ["Der", "G\u00e4rt\u00b7ner", "wehr\u00b7los", "ist", "ge\u00b7wach\u00b7sen", "nicht", "den", "drein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VAFIN", "VAPP", "PTKNEG", "ART", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch klug besinnt er sich die Eintracht zu entzwein.", "tokens": ["Doch", "klug", "be\u00b7sinnt", "er", "sich", "die", "Ein\u00b7tracht", "zu", "ent\u00b7zwein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "PRF", "ART", "NN", "PTKZU", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Mit Neigen naht er sich und gr\u00fc\u00dft: ich w\u00fc\u00dfte gern,", "tokens": ["Mit", "Nei\u00b7gen", "naht", "er", "sich", "und", "gr\u00fc\u00dft", ":", "ich", "w\u00fc\u00df\u00b7te", "gern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "PRF", "KON", "VVFIN", "$.", "PPER", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wer sind, die des Besuchs mich w\u00fcrdigen, die Herrn?", "tokens": ["Wer", "sind", ",", "die", "des", "Be\u00b7suchs", "mich", "w\u00fcr\u00b7di\u00b7gen", ",", "die", "Herrn", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "$,", "PRELS", "ART", "NN", "PPER", "VVINF", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Ich bin ein Mann vom Schwert. Ich bin des Rechts gelehrt.", "tokens": ["Ich", "bin", "ein", "Mann", "vom", "Schwert", ".", "Ich", "bin", "des", "Rechts", "ge\u00b7lehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPRART", "NN", "$.", "PPER", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich, sprach der dritte, bin ein Kaufmann ehrenwerth.", "tokens": ["Ich", ",", "sprach", "der", "drit\u00b7te", ",", "bin", "ein", "Kauf\u00b7mann", "eh\u00b7ren\u00b7werth", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "VVFIN", "ART", "ADJA", "$,", "VAFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "\u201eein Schurke bist du wol, die beiden Herrn in Ehren,", "tokens": ["\u201e", "ein", "Schur\u00b7ke", "bist", "du", "wol", ",", "die", "bei\u00b7den", "Herrn", "in", "Eh\u00b7ren", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VAFIN", "PPER", "ADV", "$,", "PRELS", "PIAT", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die mir die Ehre thun im Garten einzukehren.", "tokens": ["Die", "mir", "die", "Eh\u00b7re", "thun", "im", "Gar\u00b7ten", "ein\u00b7zu\u00b7keh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVINF", "APPRART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Der eine mit dem Schwert, der andre mit der Feder,", "tokens": ["Der", "ei\u00b7ne", "mit", "dem", "Schwert", ",", "der", "and\u00b7re", "mit", "der", "Fe\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "APPR", "ART", "NN", "$,", "PRELS", "PIS", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Besch\u00fctzen Eigenthum und Recht, gleichtapfer jeder.", "tokens": ["Be\u00b7sch\u00fct\u00b7zen", "Ei\u00b7gen\u00b7thum", "und", "Recht", ",", "gleich\u00b7tap\u00b7fer", "je\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "NN", "$,", "ADV", "PIS", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Wenn sie f\u00fcr ihren Schutz von meinen Aepfeln speisen,", "tokens": ["Wenn", "sie", "f\u00fcr", "ih\u00b7ren", "Schutz", "von", "mei\u00b7nen", "A\u00b7e\u00b7pfeln", "spei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "So wollen sie mir ganz besondre Gunst erweisen.", "tokens": ["So", "wol\u00b7len", "sie", "mir", "ganz", "be\u00b7sond\u00b7re", "Gunst", "er\u00b7wei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "ADV", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Du aber, hast du hier gehandelt und gekauft?", "tokens": ["Du", "a\u00b7ber", ",", "hast", "du", "hier", "ge\u00b7han\u00b7delt", "und", "ge\u00b7kauft", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "VAFIN", "PPER", "ADV", "VVPP", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bezahlt zum mindsten nicht; nun zahlst du mir's gehauft.\u201c", "tokens": ["Be\u00b7zahlt", "zum", "minds\u00b7ten", "nicht", ";", "nun", "zahlst", "du", "mir's", "ge\u00b7hauft", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "APPRART", "ADJA", "PTKNEG", "$.", "ADV", "VVFIN", "PPER", "NE", "VVPP", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Der G\u00e4rtner r\u00fcstig fa\u00dft den Kr\u00e4mer an im Nu,", "tokens": ["Der", "G\u00e4rt\u00b7ner", "r\u00fcs\u00b7tig", "fa\u00dft", "den", "Kr\u00e4\u00b7mer", "an", "im", "Nu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "ART", "NN", "APPR", "APPRART", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und wirft zu Boden ihn, die beiden sehen zu.", "tokens": ["Und", "wirft", "zu", "Bo\u00b7den", "ihn", ",", "die", "bei\u00b7den", "se\u00b7hen", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "PPER", "$,", "PRELS", "PIS", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Sie sehn unschl\u00fcssig zu, wie er ihn t\u00fcchtig pre\u00dft,", "tokens": ["Sie", "sehn", "un\u00b7schl\u00fcs\u00b7sig", "zu", ",", "wie", "er", "ihn", "t\u00fcch\u00b7tig", "pre\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PTKVZ", "$,", "PWAV", "PPER", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In Weidenstricken ihn geknebelt liegen l\u00e4\u00dft.", "tokens": ["In", "Wei\u00b7den\u00b7stri\u00b7cken", "ihn", "ge\u00b7kne\u00b7belt", "lie\u00b7gen", "l\u00e4\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "VVPP", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.11": {"line.1": {"text": "Und als er ausgeschnauft, wandt' er sich zu den beiden:", "tokens": ["Und", "als", "er", "aus\u00b7ge\u00b7schnauft", ",", "wandt'", "er", "sich", "zu", "den", "bei\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "PRF", "APPR", "ART", "PIAT", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "\u201enun la\u00dft uns ferner Recht und Unrecht unterscheiden.", "tokens": ["\u201e", "nun", "la\u00dft", "uns", "fer\u00b7ner", "Recht", "und", "Un\u00b7recht", "un\u00b7ter\u00b7schei\u00b7den", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "ADV", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.12": {"line.1": {"text": "Der edle Kriegsmann ist gewohnt an Kriegesbeute;", "tokens": ["Der", "ed\u00b7le", "Kriegs\u00b7mann", "ist", "ge\u00b7wohnt", "an", "Krie\u00b7ges\u00b7beu\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es freut mich, wenn er heut sich meiner Fr\u00fcchte freute.", "tokens": ["Es", "freut", "mich", ",", "wenn", "er", "heut", "sich", "mei\u00b7ner", "Fr\u00fcch\u00b7te", "freu\u00b7te", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KOUS", "PPER", "ADV", "PRF", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.13": {"line.1": {"text": "Du aber, welchen Anspruch hast du oder Titel?", "tokens": ["Du", "a\u00b7ber", ",", "wel\u00b7chen", "An\u00b7spruch", "hast", "du", "o\u00b7der", "Ti\u00b7tel", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "PWAT", "NN", "VAFIN", "PPER", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Schwebt hier ein Rechtstreit ob, da\u00df du dich schl\u00e4gst ins Mittel,", "tokens": ["Schwebt", "hier", "ein", "Rechts\u00b7treit", "ob", ",", "da\u00df", "du", "dich", "schl\u00e4gst", "ins", "Mit\u00b7tel", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "KOUS", "$,", "KOUS", "PPER", "PRF", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.14": {"line.1": {"text": "Und nimmst in Voraus dir die Sporteln und Geb\u00fchren?", "tokens": ["Und", "nimmst", "in", "Vo\u00b7raus", "dir", "die", "Spor\u00b7teln", "und", "Ge\u00b7b\u00fch\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "PPER", "ART", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "La\u00df sehn, ob ich nicht selbst kann meinen Rechtstreit f\u00fchren!\u201c \u2014", "tokens": ["La\u00df", "sehn", ",", "ob", "ich", "nicht", "selbst", "kann", "mei\u00b7nen", "Rechts\u00b7treit", "f\u00fch\u00b7ren", "!", "\u201c"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["VVIMP", "VVINF", "$,", "KOUS", "PPER", "PTKNEG", "ADV", "VMFIN", "PPOSAT", "NN", "VVINF", "$.", "$(", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.15": {"line.1": {"text": "Er packt ihn wacker an, dem zweiten ist gethan", "tokens": ["Er", "packt", "ihn", "wa\u00b7cker", "an", ",", "dem", "zwei\u00b7ten", "ist", "ge\u00b7than"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,", "ART", "ADJA", "VAFIN", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Alswie dem ersten, und der dritte siehts mit an.", "tokens": ["Als\u00b7wie", "dem", "ers\u00b7ten", ",", "und", "der", "drit\u00b7te", "siehts", "mit", "an", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "$,", "KON", "ART", "ADJA", "NN", "APPR", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.16": {"line.1": {"text": "Dann kehrt er ausgeschnauft zum dritten sich zuletzt:", "tokens": ["Dann", "kehrt", "er", "aus\u00b7ge\u00b7schnauft", "zum", "drit\u00b7ten", "sich", "zu\u00b7letzt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "APPRART", "ADJA", "PRF", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "\u201emeinst du, ein R\u00e4uber sei dem Krieger gleich gesetzt?", "tokens": ["\u201e", "meinst", "du", ",", "ein", "R\u00e4u\u00b7ber", "sei", "dem", "Krie\u00b7ger", "gleich", "ge\u00b7setzt", "?"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "$,", "ART", "NN", "VAFIN", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.17": {"line.1": {"text": "Wenn du ein Krieger bist, ist hier denn Feindesland?", "tokens": ["Wenn", "du", "ein", "Krie\u00b7ger", "bist", ",", "ist", "hier", "denn", "Fein\u00b7des\u00b7land", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VAFIN", "$,", "VAFIN", "ADV", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nun, wenn du dieses meinst, so f\u00fchl' auch Feindeshand!\u201c \u2014", "tokens": ["Nun", ",", "wenn", "du", "die\u00b7ses", "meinst", ",", "so", "f\u00fchl'", "auch", "Fein\u00b7des\u00b7hand", "!", "\u201c"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "PDS", "VVFIN", "$,", "ADV", "VVFIN", "ADV", "NN", "$.", "$(", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.18": {"line.1": {"text": "Er greift ihn tapfer an, und thut ihm wie den beiden;", "tokens": ["Er", "greift", "ihn", "tap\u00b7fer", "an", ",", "und", "thut", "ihm", "wie", "den", "bei\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,", "KON", "VVFIN", "PPER", "KOKOM", "ART", "PIAT", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Nachbarn ruft er dann, den Handel zu entscheiden.", "tokens": ["Die", "Nach\u00b7barn", "ruft", "er", "dann", ",", "den", "Han\u00b7del", "zu", "ent\u00b7schei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "$,", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.19": {"line.1": {"text": "Und als die Sch\u00e4diger den abgesch\u00e4tzten Schaden", "tokens": ["Und", "als", "die", "Sch\u00e4\u00b7di\u00b7ger", "den", "ab\u00b7ge\u00b7sch\u00e4tz\u00b7ten", "Scha\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Geg\u00fctet, l\u00e4\u00dft er sie aus ihrer Haft in Gnaden. \u2014", "tokens": ["Ge\u00b7g\u00fc\u00b7tet", ",", "l\u00e4\u00dft", "er", "sie", "aus", "ih\u00b7rer", "Haft", "in", "Gna\u00b7den", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVPP", "$,", "VVFIN", "PPER", "PPER", "APPR", "PPOSAT", "NN", "APPR", "NN", "$.", "$("], "meter": "-+-+