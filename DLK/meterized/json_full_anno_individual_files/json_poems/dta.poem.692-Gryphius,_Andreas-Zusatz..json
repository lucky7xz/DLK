{"dta.poem.692": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "Zusatz.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Der/ der vns sch\u00fctzt\u2019 in noth/", "tokens": ["Der", "/", "der", "vns", "sch\u00fctzt'", "in", "noth", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$(", "PRELS", "PPER", "VVFIN", "APPR", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Erweist an mir die Allmacht seiner Ehren!", "tokens": ["Er\u00b7weist", "an", "mir", "die", "All\u00b7macht", "sei\u00b7ner", "Eh\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mein ach mein Todt ist todt.", "tokens": ["Mein", "ach", "mein", "Todt", "ist", "todt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADV", "PPOSAT", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Es m\u00fcsse di\u00df was et", "tokens": ["Es", "m\u00fcs\u00b7se", "di\u00df", "was", "et"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PDS", "PRELS", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Den/ den was athem holt/ veracht/", "tokens": ["Den", "/", "den", "was", "at\u00b7hem", "holt", "/", "ver\u00b7acht", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "$(", "ART", "PWS", "ADJA", "VVFIN", "$(", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Schm\u00fcckt er mit seiner g\u00fctte pracht!", "tokens": ["Schm\u00fcckt", "er", "mit", "sei\u00b7ner", "g\u00fct\u00b7te", "pracht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der/ der mir vor den Rucken wandte:", "tokens": ["Der", "/", "der", "mir", "vor", "den", "Ru\u00b7cken", "wand\u00b7te", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$(", "PRELS", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der mich in seinem grim verbannte: ", "tokens": ["Der", "mich", "in", "sei\u00b7nem", "grim", "ver\u00b7bann\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Kehret mir den s\u00fcssen Mund/ vnd die lieben Augen zu", "tokens": ["Keh\u00b7ret", "mir", "den", "s\u00fcs\u00b7sen", "Mund", "/", "vnd", "die", "lie\u00b7ben", "Au\u00b7gen", "zu"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$(", "KON", "ART", "ADJA", "NN", "PTKZU"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.10": {"text": "Er erquickt mein ", "tokens": ["Er", "er\u00b7quickt", "mein"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT"], "meter": "+-+-", "measure": "trochaic.di"}, "line.11": {"text": "Keine pein ist dem ergetzen", "tokens": ["Kei\u00b7ne", "pein", "ist", "dem", "er\u00b7get\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VAFIN", "ART", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Das ich f\u00fchle gleich zu sch\u00e4tzen. ", "tokens": ["Das", "ich", "f\u00fch\u00b7le", "gleich", "zu", "sch\u00e4t\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "VVFIN", "ADV", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}