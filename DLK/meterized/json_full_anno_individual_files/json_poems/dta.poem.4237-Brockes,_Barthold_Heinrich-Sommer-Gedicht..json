{"dta.poem.4237": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Sommer-Gedicht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Segen-schwangeren Alleen", "tokens": ["Die", "Se\u00b7gen\u00b7schwan\u00b7ge\u00b7ren", "Al\u00b7leen"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der h\u00e4ufig aufgeth\u00fcrmten Hocken,", "tokens": ["Der", "h\u00e4u\u00b7fig", "auf\u00b7ge\u00b7th\u00fcrm\u00b7ten", "Ho\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Von Gersten, Haber, Weizen, Rocken,", "tokens": ["Von", "Gers\u00b7ten", ",", "Ha\u00b7ber", ",", "Wei\u00b7zen", ",", "Ro\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sind itzt, wie sie so zierlich stehen,", "tokens": ["Sind", "itzt", ",", "wie", "sie", "so", "zier\u00b7lich", "ste\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "$,", "PWAV", "PPER", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "In langen Linien zu sehen.", "tokens": ["In", "lan\u00b7gen", "Li\u00b7ni\u00b7en", "zu", "se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wenn wir darinn spatzieren gehen,", "tokens": ["Wenn", "wir", "da\u00b7rinn", "spat\u00b7zie\u00b7ren", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PAV", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So la\u00dft uns, mit ger\u00fchrter Brust,", "tokens": ["So", "la\u00dft", "uns", ",", "mit", "ge\u00b7r\u00fchr\u00b7ter", "Brust", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch Dessen Macht und Lieb\u2019 erh\u00f6hen,", "tokens": ["Doch", "Des\u00b7sen", "Macht", "und", "Lieb'", "er\u00b7h\u00f6\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der, uns zum Nutzen und zur Lust,", "tokens": ["Der", ",", "uns", "zum", "Nut\u00b7zen", "und", "zur", "Lust", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PPER", "APPRART", "NN", "KON", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Um Seine Lieb\u2019 und Macht zu fassen,", "tokens": ["Um", "Sei\u00b7ne", "Lieb'", "und", "Macht", "zu", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "KON", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "So sch\u00f6n Getrayde wachsen lassen.", "tokens": ["So", "sch\u00f6n", "Ge\u00b7tray\u00b7de", "wach\u00b7sen", "las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "NN", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ach GOtt! hier seh\u2019 ich, Dir zum Preise,", "tokens": ["Ach", "Gott", "!", "hier", "seh'", "ich", ",", "Dir", "zum", "Prei\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$.", "ADV", "VVFIN", "PPER", "$,", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie, aus der Erden, unsre Speise", "tokens": ["Wie", ",", "aus", "der", "Er\u00b7den", ",", "uns\u00b7re", "Spei\u00b7se"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "$,", "APPR", "ART", "NN", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Durch Deine Macht und Liebe quillt!", "tokens": ["Durch", "Dei\u00b7ne", "Macht", "und", "Lie\u00b7be", "quillt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ach GOtt! wie ist von Deinem Segen,", "tokens": ["Ach", "Gott", "!", "wie", "ist", "von", "Dei\u00b7nem", "Se\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$.", "PWAV", "VAFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Durch Saamen, Sonnen-Schein und Regen,", "tokens": ["Durch", "Saa\u00b7men", ",", "Son\u00b7nen\u00b7Schein", "und", "Re\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der Erden obre Fl\u00e4ch\u2019 erf\u00fcllt!", "tokens": ["Der", "Er\u00b7den", "ob\u00b7re", "Fl\u00e4ch'", "er\u00b7f\u00fcllt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wohin wir unsre Blicke kehren,", "tokens": ["Wo\u00b7hin", "wir", "uns\u00b7re", "Bli\u00b7cke", "keh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ist alles voller schwehren Aehren,", "tokens": ["Ist", "al\u00b7les", "vol\u00b7ler", "schweh\u00b7ren", "A\u00b7eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Die, blo\u00df allein um uns zu n\u00e4hren,", "tokens": ["Die", ",", "blo\u00df", "al\u00b7lein", "um", "uns", "zu", "n\u00e4h\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "ADV", "ADV", "APPR", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Von Fleisch-formiernden K\u00f6rnern voll.", "tokens": ["Von", "Fleischfor\u00b7miern\u00b7den", "K\u00f6r\u00b7nern", "voll", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "ADJD", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Ach w\u00e4r, f\u00fcr so viel Gnad\u2019 und G\u00fcte,", "tokens": ["Ach", "w\u00e4r", ",", "f\u00fcr", "so", "viel", "Gnad'", "und", "G\u00fc\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VAFIN", "$,", "APPR", "ADV", "PIAT", "NN", "KON", "NN", "$,"], "meter": "-+--++-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Doch unser fr\u00f6hliches Gem\u00fchte", "tokens": ["Doch", "un\u00b7ser", "fr\u00f6h\u00b7li\u00b7ches", "Ge\u00b7m\u00fch\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Auch voll Erkenntlichkeit und Dank!", "tokens": ["Auch", "voll", "Er\u00b7kennt\u00b7lich\u00b7keit", "und", "Dank", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ach, s\u00fcngen wir, f\u00fcr so viel Gaben,", "tokens": ["Ach", ",", "s\u00fcn\u00b7gen", "wir", ",", "f\u00fcr", "so", "viel", "Ga\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVFIN", "PPER", "$,", "APPR", "ADV", "PIAT", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Die wir, allein von Dir, nur haben,", "tokens": ["Die", "wir", ",", "al\u00b7lein", "von", "Dir", ",", "nur", "ha\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PPER", "$,", "ADV", "APPR", "PPER", "$,", "ADV", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Dir manchen sch\u00f6nen Lob-Gesang!", "tokens": ["Dir", "man\u00b7chen", "sch\u00f6\u00b7nen", "Lob\u00b7Ge\u00b7sang", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}