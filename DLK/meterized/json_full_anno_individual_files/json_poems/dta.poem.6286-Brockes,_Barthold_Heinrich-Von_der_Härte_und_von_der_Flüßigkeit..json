{"dta.poem.6286": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der H\u00e4rte und von der  \n Fl\u00fc\u00dfigkeit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Jmgleichen, wenn man Saltz im Wasser schmeltzen l\u00e4sst,", "tokens": ["Jm\u00b7glei\u00b7chen", ",", "wenn", "man", "Saltz", "im", "Was\u00b7ser", "schmelt\u00b7zen", "l\u00e4sst", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "PIS", "NN", "APPRART", "NN", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So sind die C\u00f6rperchen, so aufgel\u00f6set seyn,", "tokens": ["So", "sind", "die", "C\u00f6r\u00b7per\u00b7chen", ",", "so", "auf\u00b7ge\u00b7l\u00f6\u00b7set", "seyn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "$,", "ADV", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mit jeden Wasser-C\u00f6rperlein,", "tokens": ["Mit", "je\u00b7den", "Was\u00b7ser\u00b7C\u00f6\u00b7rper\u00b7lein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die sich so leicht vermengen, fest", "tokens": ["Die", "sich", "so", "leicht", "ver\u00b7men\u00b7gen", ",", "fest"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "PRF", "ADV", "ADJD", "VVINF", "$,", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und \u00fcberall verbunden,", "tokens": ["Und", "\u00fc\u00b7be\u00b7rall", "ver\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Es wird so hoch als tieff das Wasser saltzig funden.", "tokens": ["Es", "wird", "so", "hoch", "als", "tieff", "das", "Was\u00b7ser", "salt\u00b7zig", "fun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "KOKOM", "ADJD", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und was man endlich auch sonst in das Wasser leget,", "tokens": ["Und", "was", "man", "end\u00b7lich", "auch", "sonst", "in", "das", "Was\u00b7ser", "le\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PIS", "ADV", "ADV", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Es mag dasselbige geschehn", "tokens": ["Es", "mag", "das\u00b7sel\u00b7bi\u00b7ge", "ge\u00b7schehn"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PDS", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Von C\u00f6rpern, welche gantz darinn zergehn;", "tokens": ["Von", "C\u00f6r\u00b7pern", ",", "wel\u00b7che", "gantz", "da\u00b7rinn", "zer\u00b7gehn", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "PRELS", "ADV", "PAV", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Wie, oder da\u00df davon sich kleine Theilchen trennen,", "tokens": ["Wie", ",", "o\u00b7der", "da\u00df", "da\u00b7von", "sich", "klei\u00b7ne", "Theil\u00b7chen", "tren\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "KON", "KOUS", "PAV", "PRF", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und allgemach entfernen k\u00f6nnen;", "tokens": ["Und", "all\u00b7ge\u00b7mach", "ent\u00b7fer\u00b7nen", "k\u00f6n\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "So wird es den Geschmack, Geruch und Farb empfangen.", "tokens": ["So", "wird", "es", "den", "Ge\u00b7schmack", ",", "Ge\u00b7ruch", "und", "Farb", "emp\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "$,", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Es wird fest an einander hangen,", "tokens": ["Es", "wird", "fest", "an", "ein\u00b7an\u00b7der", "han\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "APPR", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Und allenthalben mit ihm fl\u00fc\u00dfen.", "tokens": ["Und", "al\u00b7len\u00b7thal\u00b7ben", "mit", "ihm", "fl\u00fc\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Und dies beweget uns zu schl\u00fc\u00dfen:", "tokens": ["Und", "dies", "be\u00b7we\u00b7get", "uns", "zu", "schl\u00fc\u00b7\u00dfen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Da\u00df \u00fcberall die kleinen C\u00f6rperlein", "tokens": ["Da\u00df", "\u00fc\u00b7be\u00b7rall", "die", "klei\u00b7nen", "C\u00f6r\u00b7per\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Von jedem Wasser-Theil mit fortgef\u00fchret seyn.", "tokens": ["Von", "je\u00b7dem", "Was\u00b7ser\u00b7T\u00b7heil", "mit", "fort\u00b7ge\u00b7f\u00fch\u00b7ret", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "APPR", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}