{"dta.poem.553": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Neu-Jahrs- Gratulation  an einen vorneh-  \n men  Minister.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Das kluge Alterthum/ von dessen guten Sitten", "tokens": ["Das", "klu\u00b7ge", "Al\u00b7ter\u00b7thum", "/", "von", "des\u00b7sen", "gu\u00b7ten", "Sit\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$(", "APPR", "PRELAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Noch keine dieser Zeit so gar verloschen ist/", "tokens": ["Noch", "kei\u00b7ne", "die\u00b7ser", "Zeit", "so", "gar", "ver\u00b7lo\u00b7schen", "ist", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "PDAT", "NN", "ADV", "ADV", "VVPP", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ist recht mit Vorbedacht ins neue Jahr geschritten/", "tokens": ["Ist", "recht", "mit", "Vor\u00b7be\u00b7dacht", "ins", "neu\u00b7e", "Jahr", "ge\u00b7schrit\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "NN", "APPRART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wenn ", "tokens": ["Wenn"], "token_info": ["word"], "pos": ["KOUS"], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Und vor den holden West die rauhen Winde-wehen/", "tokens": ["Und", "vor", "den", "hol\u00b7den", "West", "die", "rau\u00b7hen", "Win\u00b7de\u00b7we\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da\u00df jene mu\u00df betr\u00fcbt nach ihrem Buhlen sehen.", "tokens": ["Da\u00df", "je\u00b7ne", "mu\u00df", "be\u00b7tr\u00fcbt", "nach", "ih\u00b7rem", "Buh\u00b7len", "se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "VMFIN", "ADJD", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die Felder sind dann leer von Fr\u00fcchten und von Leuten", "tokens": ["Die", "Fel\u00b7der", "sind", "dann", "leer", "von", "Fr\u00fcch\u00b7ten", "und", "von", "Leu\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Der ungest\u00fchme ", "tokens": ["Der", "un\u00b7ge\u00b7st\u00fch\u00b7me"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "Und seine Ritter-Schaar will nach den harten Streiten", "tokens": ["Und", "sei\u00b7ne", "Rit\u00b7ter\u00b7Schaar", "will", "nach", "den", "har\u00b7ten", "Strei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VMFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "So wie ihr F\u00fchrer still/ und nicht blut-gierig seyn.", "tokens": ["So", "wie", "ihr", "F\u00fch\u00b7rer", "still", "/", "und", "nicht", "blut\u00b7gie\u00b7rig", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "PPOSAT", "NN", "PTKVZ", "$(", "KON", "PTKNEG", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Es ruht zu solcher Zeit/ was diese Welt umschliesset", "tokens": ["Es", "ruht", "zu", "sol\u00b7cher", "Zeit", "/", "was", "die\u00b7se", "Welt", "um\u00b7schlies\u00b7set"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN", "$(", "PWS", "PDAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Was ", "tokens": ["Was"], "token_info": ["word"], "pos": ["PWS"], "meter": "-", "measure": "single.down"}, "line.13": {"text": "In diese frohe Zeit da Dolch und Degen liegen", "tokens": ["In", "die\u00b7se", "fro\u00b7he", "Zeit", "da", "Dolch", "und", "De\u00b7gen", "lie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "NN", "ADV", "NN", "KON", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Da lauter Lieblichkeit die stoltze Ruhe reicht/", "tokens": ["Da", "lau\u00b7ter", "Lieb\u00b7lich\u00b7keit", "die", "stolt\u00b7ze", "Ru\u00b7he", "reicht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Da hat das Alterthum zu jedermanns Vergn\u00fcgen/", "tokens": ["Da", "hat", "das", "Al\u00b7ter\u00b7thum", "zu", "je\u00b7der\u00b7manns", "Ver\u00b7gn\u00fc\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "APPR", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Das neue Jahr gesetzt/ und dadurch angezeigt/", "tokens": ["Das", "neu\u00b7e", "Jahr", "ge\u00b7setzt", "/", "und", "da\u00b7durch", "an\u00b7ge\u00b7zeigt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$(", "KON", "PAV", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Da\u00df denn mit bestem Recht die W\u00fcnsche k\u00f6nnen fliessen/", "tokens": ["Da\u00df", "denn", "mit", "bes\u00b7tem", "Recht", "die", "W\u00fcn\u00b7sche", "k\u00f6n\u00b7nen", "flies\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPR", "ADJA", "NN", "ART", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Weil ", "tokens": ["Weil"], "token_info": ["word"], "pos": ["KOUS"], "meter": "+", "measure": "single.up"}, "line.19": {"text": "Jhr Absehn war dabey den G\u00f6ttern Danck zu geben", "tokens": ["Ihr", "Ab\u00b7sehn", "war", "da\u00b7bey", "den", "G\u00f6t\u00b7tern", "Danck", "zu", "ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "PAV", "ART", "NN", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Vor die verliehne Ruh/ sie w\u00fcnscheten zu gleich", "tokens": ["Vor", "die", "ver\u00b7lieh\u00b7ne", "Ruh", "/", "sie", "w\u00fcn\u00b7sche\u00b7ten", "zu", "gleich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$(", "PPER", "VVFIN", "APPR", "ADV"], "meter": "---+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.21": {"text": "Da\u00df ferner ihre Huld sie friedlich liesse leben/", "tokens": ["Da\u00df", "fer\u00b7ner", "ih\u00b7re", "Huld", "sie", "fried\u00b7lich", "lies\u00b7se", "le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "PPER", "ADJD", "VVFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "So w\u00e4ren sie so wol am Gl\u00fcck als G\u00fctern reich.", "tokens": ["So", "w\u00e4\u00b7ren", "sie", "so", "wol", "am", "Gl\u00fcck", "als", "G\u00fc\u00b7tern", "reich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "APPRART", "NN", "KOUS", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Am meisten liessen sich dieselbe w\u00fcnschend h\u00f6ren", "tokens": ["Am", "meis\u00b7ten", "lies\u00b7sen", "sich", "die\u00b7sel\u00b7be", "w\u00fcn\u00b7schend", "h\u00f6\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "PIS", "VVFIN", "PRF", "PDAT", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Die vorgeno\u00dfne Gunst die G\u00f6nner musten ehren.", "tokens": ["Die", "vor\u00b7ge\u00b7no\u00df\u00b7ne", "Gunst", "die", "G\u00f6n\u00b7ner", "mus\u00b7ten", "eh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Weil nun die neue Welt auch diesen Brauch beliebet/", "tokens": ["Weil", "nun", "die", "neu\u00b7e", "Welt", "auch", "die\u00b7sen", "Brauch", "be\u00b7lie\u00b7bet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "ADV", "PDAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "So stellt sich meine Pflicht Hoch-Wohlgebohrner ein/", "tokens": ["So", "stellt", "sich", "mei\u00b7ne", "Pflicht", "Hoch\u00b7Wohl\u00b7ge\u00b7bohr\u00b7ner", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "PPOSAT", "NN", "NN", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Mein Arm-seyn aber nichts von Gold und Silber giebet/", "tokens": ["Mein", "Ar\u00b7mseyn", "a\u00b7ber", "nichts", "von", "Gold", "und", "Sil\u00b7ber", "gie\u00b7bet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "PIS", "APPR", "NN", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Mein Unverm\u00f6gen l\u00e4st mich nicht frey-gebig seyn.", "tokens": ["Mein", "Un\u00b7ver\u00b7m\u00f6\u00b7gen", "l\u00e4st", "mich", "nicht", "frey\u00b7ge\u00b7big", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "PTKNEG", "ADJD", "VAINF", "$."], "meter": "-+-+-+-++--+", "measure": "iambic.hexa.chol"}, "line.29": {"text": "Der Wille ist zwar da/ doch das Verm\u00f6gen fehlet/", "tokens": ["Der", "Wil\u00b7le", "ist", "zwar", "da", "/", "doch", "das", "Ver\u00b7m\u00f6\u00b7gen", "feh\u00b7let", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADV", "$(", "ADV", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Ohn das der Wille todt und gleichsahm ist entseelet.", "tokens": ["Ohn", "das", "der", "Wil\u00b7le", "todt", "und", "gleich\u00b7sahm", "ist", "ent\u00b7see\u00b7let", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ART", "NN", "ADJD", "KON", "ADJD", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Doch seiner Gro\u00dfmuth wird der Wille auch gefallen/", "tokens": ["Doch", "sei\u00b7ner", "Gro\u00df\u00b7muth", "wird", "der", "Wil\u00b7le", "auch", "ge\u00b7fal\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ART", "NN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Denn hohe Leute sind mit solchem offt vergn\u00fcgt", "tokens": ["Denn", "ho\u00b7he", "Leu\u00b7te", "sind", "mit", "sol\u00b7chem", "offt", "ver\u00b7gn\u00fcgt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "VAFIN", "APPR", "PIAT", "ADV", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Ein ", "tokens": ["Ein"], "token_info": ["word"], "pos": ["ART"], "meter": "+", "measure": "single.up"}, "line.34": {"text": "Wenn ein getreuer Knecht mit Wasser vor ihm liegt/", "tokens": ["Wenn", "ein", "ge\u00b7treu\u00b7er", "Knecht", "mit", "Was\u00b7ser", "vor", "ihm", "liegt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "APPR", "NN", "APPR", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Er schaute an das Hertz/ und nicht Geschenck und Gaben/", "tokens": ["Er", "schau\u00b7te", "an", "das", "Hertz", "/", "und", "nicht", "Ge\u00b7schenck", "und", "Ga\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$(", "KON", "PTKNEG", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Weil unter solchen offt das sch\u00e4rffste Gifft vergraben.", "tokens": ["Weil", "un\u00b7ter", "sol\u00b7chen", "offt", "das", "sch\u00e4rffs\u00b7te", "Gifft", "ver\u00b7gra\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PIAT", "ADV", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Ich lege mich nun selbst zu seinen F\u00fcssen nieder/", "tokens": ["Ich", "le\u00b7ge", "mich", "nun", "selbst", "zu", "sei\u00b7nen", "F\u00fcs\u00b7sen", "nie\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "APPR", "PPOSAT", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Und stimme meinen Wunsch mit diesen Worten an:", "tokens": ["Und", "stim\u00b7me", "mei\u00b7nen", "Wunsch", "mit", "die\u00b7sen", "Wor\u00b7ten", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "APPR", "PDAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Der Himmel schencket uns den frohen Tag jetzt wieder/", "tokens": ["Der", "Him\u00b7mel", "schen\u00b7cket", "uns", "den", "fro\u00b7hen", "Tag", "jetzt", "wie\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "ADV", "ADV", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "An dem ein jeder jauchtzt/ der nur zwey Worte kan.", "tokens": ["An", "dem", "ein", "je\u00b7der", "jauchtzt", "/", "der", "nur", "zwey", "Wor\u00b7te", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "PIS", "VVFIN", "$(", "ART", "ADV", "CARD", "NN", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Was von Vergn\u00fcgen ist in dieser Welt zu finden/", "tokens": ["Was", "von", "Ver\u00b7gn\u00fc\u00b7gen", "ist", "in", "die\u00b7ser", "Welt", "zu", "fin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "NN", "VAFIN", "APPR", "PDAT", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Hoch-Wohlgebohrner Herr/ das mu\u00df sich ihm verbinden.", "tokens": ["Hoch\u00b7Wohl\u00b7ge\u00b7bohr\u00b7ner", "Herr", "/", "das", "mu\u00df", "sich", "ihm", "ver\u00b7bin\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$(", "PDS", "VMFIN", "PRF", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Sein Alter steige so wie Noahs seine Jahre/", "tokens": ["Sein", "Al\u00b7ter", "stei\u00b7ge", "so", "wie", "No\u00b7ahs", "sei\u00b7ne", "Jah\u00b7re", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "KOKOM", "NE", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Husai beuge sich vor seinen klugen Raht/", "tokens": ["Hu\u00b7sai", "beu\u00b7ge", "sich", "vor", "sei\u00b7nen", "klu\u00b7gen", "Raht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PRF", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.45": {"text": "Sein Reichthum wachse so wie Jacobs Haabe ware", "tokens": ["Sein", "Reicht\u00b7hum", "wach\u00b7se", "so", "wie", "Ja\u00b7cobs", "Haa\u00b7be", "wa\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "KOKOM", "NE", "NN", "VAFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Der F\u00fcrst mach\u2019 ihn so gro\u00df wie man dem Joseph that.", "tokens": ["Der", "F\u00fcrst", "mach'", "ihn", "so", "gro\u00df", "wie", "man", "dem", "Jo\u00b7se\u00b7ph", "that", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "ADJD", "KOKOM", "PIS", "ART", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.47": {"text": "Kein Unfall m\u00fcsse ihm niemahls beschwerlich werden/", "tokens": ["Kein", "Un\u00b7fall", "m\u00fcs\u00b7se", "ihm", "nie\u00b7mahls", "be\u00b7schwer\u00b7lich", "wer\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "PPER", "ADV", "ADJD", "VAINF", "$("], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.48": {"text": "Bis ihm der Himmel wird gegeben vor die Erden.", "tokens": ["Bis", "ihm", "der", "Him\u00b7mel", "wird", "ge\u00b7ge\u00b7ben", "vor", "die", "Er\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "VAFIN", "VVPP", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.49": {"text": "Zwar da die Gaben schlecht und kein Geschenck zu nennen/", "tokens": ["Zwar", "da", "die", "Ga\u00b7ben", "schlecht", "und", "kein", "Ge\u00b7schenck", "zu", "nen\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "ADJD", "KON", "PIAT", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.50": {"text": "So m\u00fcste wol die Pracht der Worte gr\u00f6sser seyn/", "tokens": ["So", "m\u00fcs\u00b7te", "wol", "die", "Pracht", "der", "Wor\u00b7te", "gr\u00f6s\u00b7ser", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ADV", "ART", "NN", "ART", "NN", "ADJD", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.51": {"text": "Doch was verstellet ist/ ist leichtlich zu erkennen", "tokens": ["Doch", "was", "ver\u00b7stel\u00b7let", "ist", "/", "ist", "leicht\u00b7lich", "zu", "er\u00b7ken\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PWS", "VVPP", "VAFIN", "$(", "VAFIN", "ADJD", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.52": {"text": "Prahl-Worte geben nichts als \u00fcber-t\u00fcnchten Schein/", "tokens": ["Prahl\u00b7Wor\u00b7te", "ge\u00b7ben", "nichts", "als", "\u00fc\u00b7ber\u00b7t\u00fcnch\u00b7ten", "Schein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PIS", "KOKOM", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.53": {"text": "Ein steinern Schau-Gericht und Wind-erf\u00fcllte Torten", "tokens": ["Ein", "stei\u00b7nern", "Schau\u00b7Ge\u00b7richt", "und", "Win\u00b7der\u00b7f\u00fcll\u00b7te", "Tor\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.54": {"text": "Reicht der/ der seine Pflicht bringt in geschminckten Worten.", "tokens": ["Reicht", "der", "/", "der", "sei\u00b7ne", "Pflicht", "bringt", "in", "ge\u00b7schminck\u00b7ten", "Wor\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "$(", "ART", "PPOSAT", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.55": {"text": "Ich bring ein treues Hertz/ das heisse W\u00fcnsche reichet/", "tokens": ["Ich", "bring", "ein", "treu\u00b7es", "Hertz", "/", "das", "heis\u00b7se", "W\u00fcn\u00b7sche", "rei\u00b7chet", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$(", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.56": {"text": "Das/ grosser G\u00f6nner/ ihm auf ewig ist geschenckt/", "tokens": ["Das", "/", "gros\u00b7ser", "G\u00f6n\u00b7ner", "/", "ihm", "auf", "e\u00b7wig", "ist", "ge\u00b7schenckt", "/"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$(", "ADJA", "NN", "$(", "PPER", "APPR", "ADJD", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.57": {"text": "Wenn seine hohe Gunst nur niemahls von mir weichet/", "tokens": ["Wenn", "sei\u00b7ne", "ho\u00b7he", "Gunst", "nur", "nie\u00b7mahls", "von", "mir", "wei\u00b7chet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "ADV", "ADV", "APPR", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.58": {"text": "So bin ich stets vergn\u00fcgt/ und bleibe ungekr\u00e4nckt.", "tokens": ["So", "bin", "ich", "stets", "ver\u00b7gn\u00fcgt", "/", "und", "blei\u00b7be", "un\u00b7ge\u00b7kr\u00e4nckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "VVPP", "$(", "KON", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.59": {"text": "Nun Himmel mache wahr was meine W\u00fcnsche haben/", "tokens": ["Nun", "Him\u00b7mel", "ma\u00b7che", "wahr", "was", "mei\u00b7ne", "W\u00fcn\u00b7sche", "ha\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "ADJD", "PRELS", "PPOSAT", "NN", "VAFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.60": {"text": "So wird mit froher Lust sein hohes Hertz sich laben.", "tokens": ["So", "wird", "mit", "fro\u00b7her", "Lust", "sein", "ho\u00b7hes", "Hertz", "sich", "la\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "ADJA", "NN", "PPOSAT", "ADJA", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}