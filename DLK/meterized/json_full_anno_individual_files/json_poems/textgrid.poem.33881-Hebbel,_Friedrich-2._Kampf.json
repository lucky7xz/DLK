{"textgrid.poem.33881": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "2. Kampf", "genre": "verse", "period": "N.A.", "pub_year": 1856, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Oft, wenn sie still an mir vor\u00fcberschwebt", "tokens": ["Oft", ",", "wenn", "sie", "still", "an", "mir", "vor\u00b7\u00fc\u00b7bersc\u00b7hwebt"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "PPER", "ADJD", "APPR", "PPER", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und l\u00e4chelnd beut des holden Gru\u00dfes Segen", "tokens": ["Und", "l\u00e4\u00b7chelnd", "beut", "des", "hol\u00b7den", "Gru\u00b7\u00dfes", "Se\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und mild und treu den frommen Blick erhebt,", "tokens": ["Und", "mild", "und", "treu", "den", "from\u00b7men", "Blick", "er\u00b7hebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KON", "ADJD", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Da tr\u00e4ume ich, beseligt und verwegen,", "tokens": ["Da", "tr\u00e4u\u00b7me", "ich", ",", "be\u00b7se\u00b7ligt", "und", "ver\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "VVPP", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Die Liebe sei's, die Gru\u00df und Blick durchwebt,", "tokens": ["Die", "Lie\u00b7be", "sei's", ",", "die", "Gru\u00df", "und", "Blick", "durch\u00b7webt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$,", "ART", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und auch die k\u00fchnste Hoffnung will sich regen.", "tokens": ["Und", "auch", "die", "k\u00fchns\u00b7te", "Hoff\u00b7nung", "will", "sich", "re\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "VMFIN", "PRF", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Doch bange Zweifel kehren bald zur\u00fcck,", "tokens": ["Doch", "ban\u00b7ge", "Zwei\u00b7fel", "keh\u00b7ren", "bald", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "NN", "VVFIN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und zu mir selber sprech' ich dann mit Reue:", "tokens": ["Und", "zu", "mir", "sel\u00b7ber", "sprech'", "ich", "dann", "mit", "Reu\u00b7e", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "ADV", "VVFIN", "PPER", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie w\u00e4r' nicht mild und treu ihr Gru\u00df und Blick?", "tokens": ["Wie", "w\u00e4r'", "nicht", "mild", "und", "treu", "ihr", "Gru\u00df", "und", "Blick", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PTKNEG", "ADJD", "KON", "ADJD", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sie ist ja selbst die Milde und die Treue!", "tokens": ["Sie", "ist", "ja", "selbst", "die", "Mil\u00b7de", "und", "die", "Treu\u00b7e", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und schneller, als es kam, verweht mein Gl\u00fcck,", "tokens": ["Und", "schnel\u00b7ler", ",", "als", "es", "kam", ",", "ver\u00b7weht", "mein", "Gl\u00fcck", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "KOUS", "PPER", "VVFIN", "$,", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und alle Wunden bluten mir auf's Neue.", "tokens": ["Und", "al\u00b7le", "Wun\u00b7den", "blu\u00b7ten", "mir", "auf's", "Neu\u00b7e", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "PPER", "APPRART", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Oft, wenn sie still an mir vor\u00fcberschwebt", "tokens": ["Oft", ",", "wenn", "sie", "still", "an", "mir", "vor\u00b7\u00fc\u00b7bersc\u00b7hwebt"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "PPER", "ADJD", "APPR", "PPER", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und l\u00e4chelnd beut des holden Gru\u00dfes Segen", "tokens": ["Und", "l\u00e4\u00b7chelnd", "beut", "des", "hol\u00b7den", "Gru\u00b7\u00dfes", "Se\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und mild und treu den frommen Blick erhebt,", "tokens": ["Und", "mild", "und", "treu", "den", "from\u00b7men", "Blick", "er\u00b7hebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON",