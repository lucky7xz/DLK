{"textgrid.poem.46215": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "An Brissach von helden Bernharden eingenommen", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ja, Brissach, dein verlust ist dein gewin und preis,", "tokens": ["Ja", ",", "Bris\u00b7sach", ",", "dein", "ver\u00b7lust", "ist", "dein", "ge\u00b7win", "und", "preis", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "NE", "$,", "PPOSAT", "NN", "VAFIN", "PPOSAT", "NN", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "du hast, indem du dich verloren, dich gefunden,", "tokens": ["du", "hast", ",", "in\u00b7dem", "du", "dich", "ver\u00b7lo\u00b7ren", ",", "dich", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "KOUS", "PPER", "PRF", "VVPP", "$,", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "du hast, von diesem schwert erobert, \u00fcberwunden,", "tokens": ["du", "hast", ",", "von", "die\u00b7sem", "schwert", "er\u00b7o\u00b7bert", ",", "\u00fc\u00b7berw\u00b7un\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "APPR", "PDAT", "ADJD", "VVPP", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "und uneinnehmlich nu wirst du auf dise weis;", "tokens": ["und", "un\u00b7ein\u00b7nehm\u00b7lich", "nu", "wirst", "du", "auf", "di\u00b7se", "weis", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "VAFIN", "PPER", "APPR", "PDAT", "PTKVZ", "$."], "meter": "-+-+-++-+--+", "measure": "iambic.hexa.chol"}, "line.5": {"text": "Dan diser f\u00fcrst, held, Mars, dein siger, ist so weis,", "tokens": ["Dan", "di\u00b7ser", "f\u00fcrst", ",", "held", ",", "Mars", ",", "dein", "si\u00b7ger", ",", "ist", "so", "weis", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PDS", "VVFIN", "$,", "VVFIN", "$,", "NN", "$,", "PPOSAT", "ADJA", "$,", "VAFIN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "so g\u00fctig, m\u00e4chtig, gro\u00df, da\u00df dein verdru\u00df verschwunden,", "tokens": ["so", "g\u00fc\u00b7tig", ",", "m\u00e4ch\u00b7tig", ",", "gro\u00df", ",", "da\u00df", "dein", "ver\u00b7dru\u00df", "ver\u00b7schwun\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADJD", "$,", "ADJD", "$,", "KOUS", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "alsbald dich seine faust zu seinem dienst verbunden,", "tokens": ["als\u00b7bald", "dich", "sei\u00b7ne", "faust", "zu", "sei\u00b7nem", "dienst", "ver\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "darum mit freud und dank gehorsam dich erweis.", "tokens": ["da\u00b7rum", "mit", "freud", "und", "dank", "ge\u00b7hor\u00b7sam", "dich", "er\u00b7weis", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "APPR", "VVFIN", "KON", "APPR", "ADJD", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Sih doch, bedenk und merk, wie herrlich er dich zieret,", "tokens": ["Sih", "doch", ",", "be\u00b7denk", "und", "merk", ",", "wie", "herr\u00b7lich", "er", "dich", "zie\u00b7ret", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$,", "VVFIN", "KON", "VVFIN", "$,", "PWAV", "ADJD", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "und du mit nichten ihn, wie durch ihn got in dir,", "tokens": ["und", "du", "mit", "nich\u00b7ten", "ihn", ",", "wie", "durch", "ihn", "got", "in", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "PIS", "PPER", "$,", "PWAV", "APPR", "PPER", "ADJD", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "als durch got \u00fcber dich er sigreich triumfieret!", "tokens": ["als", "durch", "got", "\u00fc\u00b7ber", "dich", "er", "sig\u00b7reich", "tri\u00b7um\u00b7fie\u00b7ret", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NN", "APPR", "PPER", "PPER", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "So lern nu dise lehr und gib ihm dank daf\u00fcr,", "tokens": ["So", "lern", "nu", "di\u00b7se", "lehr", "und", "gib", "ihm", "dank", "da\u00b7f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PDAT", "NN", "KON", "VVIMP", "PPER", "APPR", "PAV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "weil got den f\u00fcrsten selbs, wie der f\u00fcrst dich, regieret,", "tokens":