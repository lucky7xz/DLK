{"dta.poem.14612": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Die Zauberin im Walde.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "\u201eund wie ich so sinnend athme", "tokens": ["\u201e", "und", "wie", "ich", "so", "sin\u00b7nend", "ath\u00b7me"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "KON", "PWAV", "PPER", "ADV", "ADJD", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Stromesk\u00fchl' und Waldesd\u00fcfte,", "tokens": ["Stro\u00b7mes\u00b7k\u00fchl'", "und", "Wal\u00b7des\u00b7d\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ein wundersam Gel\u00fcsten", "tokens": ["Und", "ein", "wun\u00b7der\u00b7sam", "Ge\u00b7l\u00fcs\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mich hinabzog nach den Kl\u00fcften:\u201c", "tokens": ["Mich", "hin\u00b7ab\u00b7zog", "nach", "den", "Kl\u00fcf\u00b7ten", ":", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}