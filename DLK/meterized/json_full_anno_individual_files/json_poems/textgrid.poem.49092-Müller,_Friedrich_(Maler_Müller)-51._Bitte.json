{"textgrid.poem.49092": {"metadata": {"author": {"name": "M\u00fcller, Friedrich (Maler M\u00fcller)", "birth": "N.A.", "death": "N.A."}, "title": "51. Bitte", "genre": "verse", "period": "N.A.", "pub_year": 1787, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Er\u00f6ffnet fr\u00fch dem Mitleid eure Brust, ihr", "tokens": ["Er\u00b7\u00f6ff\u00b7net", "fr\u00fch", "dem", "Mit\u00b7leid", "eu\u00b7re", "Brust", ",", "ihr"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "ADJD", "ART", "NN", "PPOSAT", "NN", "$,", "PPOSAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "G\u00fcnstlinge des Gl\u00fccks, da\u00df ungewaffnet euch nicht", "tokens": ["G\u00fcnst\u00b7lin\u00b7ge", "des", "Gl\u00fccks", ",", "da\u00df", "un\u00b7ge\u00b7waff\u00b7net", "euch", "nicht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "ART", "NN", "$,", "KOUS", "ADJD", "PPER", "PTKNEG"], "meter": "-+--+-+-+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Selbst das Elend \u00fcberf\u00e4llt; das immer", "tokens": ["Selbst", "das", "E\u00b7lend", "\u00fc\u00b7berf\u00b7\u00e4llt", ";", "das", "im\u00b7mer"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ART", "NN", "VVPP", "$.", "ART", "ADV"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Heile Herz es trotzt so leicht; ihm ekelt", "tokens": ["Hei\u00b7le", "Herz", "es", "trotzt", "so", "leicht", ";", "ihm", "e\u00b7kelt"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "PPER", "VVFIN", "ADV", "ADJD", "$.", "PPER", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Andrer Weh; es wendet seitw\u00e4rts trotzig", "tokens": ["A\u00b7ndrer", "Weh", ";", "es", "wen\u00b7det", "seit\u00b7w\u00e4rts", "trot\u00b7zig"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "PPER", "VVFIN", "ADV", "ADJD"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Ab das Haupt; doch unverwarnet packt das", "tokens": ["Ab", "das", "Haupt", ";", "doch", "un\u00b7ver\u00b7war\u00b7net", "packt", "das"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$.", "ADV", "ADJD", "VVFIN", "ART"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Schicksal, knirrscht's; da sinkt auf einmal aller", "tokens": ["Schick\u00b7sal", ",", "knirrscht's", ";", "da", "sinkt", "auf", "ein\u00b7mal", "al\u00b7ler"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "$.", "ADV", "VVFIN", "APPR", "ADV", "PIAT"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.8": {"text": "Uebermut. Unge\u00fcbt im Leid beugt sich die", "tokens": ["Ue\u00b7ber\u00b7mut", ".", "Un\u00b7ge\u00b7\u00fcbt", "im", "Leid", "beugt", "sich", "die"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "NN", "APPRART", "NN", "VVFIN", "PRF", "ART"], "meter": "+--+-+-++-+", "measure": "iambic.hexa.invert"}, "line.9": {"text": "Stolze Seele, dreifach stark getroffen.", "tokens": ["Stol\u00b7ze", "See\u00b7le", ",", "drei\u00b7fach", "stark", "ge\u00b7trof\u00b7fen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADV", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Ha! Dann r\u00e4cht sich bitter, schroff, die lange", "tokens": ["Ha", "!", "Dann", "r\u00e4cht", "sich", "bit\u00b7ter", ",", "schroff", ",", "die", "lan\u00b7ge"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word"