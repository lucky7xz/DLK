{"dta.poem.3414": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "An Ida .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1798", "urn": "urn:nbn:de:kobv:b4-200905193292", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Schwellen nicht Seufzer, meine s&#252;sse Ida,", "tokens": ["Schwel\u00b7len", "nicht", "Seuf\u00b7zer", ",", "mei\u00b7ne", "s", "&#252;", "sse", "I\u00b7da", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "XML_entity", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "NN", "$,", "PPOSAT", "FM", "FM", "FM", "FM", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Deinen gew\u00f6lbten Busen, wann das Fr\u00fchroth", "tokens": ["Dei\u00b7nen", "ge\u00b7w\u00f6lb\u00b7ten", "Bu\u00b7sen", ",", "wann", "das", "Fr\u00fch\u00b7roth"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PWAV", "ART", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Deines Gemaches weisse W\u00e4nde r\u00f6thet,", "tokens": ["Dei\u00b7nes", "Ge\u00b7ma\u00b7ches", "weis\u00b7se", "W\u00e4n\u00b7de", "r\u00f6\u00b7thet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Oder das Sp\u00e4troth?", "tokens": ["O\u00b7der", "das", "Sp\u00e4t\u00b7roth", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Weht es nicht dann um dich, wie Geistgeflister?", "tokens": ["Weht", "es", "nicht", "dann", "um", "dich", ",", "wie", "Geist\u00b7ge\u00b7flis\u00b7ter", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ADV", "APPR", "PPER", "$,", "PWAV", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Haucht es nicht warm dich an, wie Kussgelispel?", "tokens": ["Haucht", "es", "nicht", "warm", "dich", "an", ",", "wie", "Kuss\u00b7ge\u00b7lis\u00b7pel", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "PTKNEG", "ADJD", "PPER", "PTKVZ", "$,", "PWAV", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Zittern nicht helle Thr\u00e4nen in des Auges", "tokens": ["Zit\u00b7tern", "nicht", "hel\u00b7le", "Thr\u00e4\u00b7nen", "in", "des", "Au\u00b7ges"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PTKNEG", "ADJA", "NN", "APPR", "ART", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Goldenen Wimpern?", "tokens": ["Gol\u00b7de\u00b7nen", "Wim\u00b7pern", "?"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Dieses Schauern, Geliebte, dieses Zittern,", "tokens": ["Die\u00b7ses", "Schau\u00b7ern", ",", "Ge\u00b7lieb\u00b7te", ",", "die\u00b7ses", "Zit\u00b7tern", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "NN", "$,", "PDAT", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Dieses Wehen und Rauschen lass dich mahnen,", "tokens": ["Die\u00b7ses", "We\u00b7hen", "und", "Rau\u00b7schen", "lass", "dich", "mah\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "KON", "NN", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Dass dein Treuer dein denkt, dass dein Verlassner", "tokens": ["Dass", "dein", "Treu\u00b7er", "dein", "denkt", ",", "dass", "dein", "Ver\u00b7lass\u00b7ner"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "PPOSAT", "VVFIN", "$,", "KOUS", "PPOSAT", "NN"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Fern um dich trauert.", "tokens": ["Fern", "um", "dich", "trau\u00b7ert", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "VVFIN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.4": {"line.1": {"text": "Ja, ich gedenk' an dich, mein s\u00fcsses M\u00e4dchen,", "tokens": ["Ja", ",", "ich", "ge\u00b7denk'", "an", "dich", ",", "mein", "s\u00fcs\u00b7ses", "M\u00e4d\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "APPR", "PPER", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ja, ich trauer' um dich, wann Gottes Fr\u00fchroth", "tokens": ["Ja", ",", "ich", "trau\u00b7e\u00b7r'", "um", "dich", ",", "wann", "Got\u00b7tes", "Fr\u00fch\u00b7roth"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "APPR", "PPER", "$,", "PWAV", "NN", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Meine Fenster r\u00f6thet, und wann die Thauflur", "tokens": ["Mei\u00b7ne", "Fens\u00b7ter", "r\u00f6\u00b7thet", ",", "und", "wann", "die", "Thauf\u00b7lur"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "KON", "PWAV", "ART", "NN"], "meter": "+-+-+--+---", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Schimmert im Sp\u00e4troth.", "tokens": ["Schim\u00b7mert", "im", "Sp\u00e4t\u00b7roth", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.5": {"line.1": {"text": "Ida, wann werd' ich dich wiedersehen,", "tokens": ["I\u00b7da", ",", "wann", "werd'", "ich", "dich", "wie\u00b7der\u00b7se\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "VAFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wann, um den Schwanenhals dir br\u00fcnstig fallend,", "tokens": ["Wann", ",", "um", "den", "Schwa\u00b7nen\u00b7hals", "dir", "br\u00fcns\u00b7tig", "fal\u00b7lend", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "KOUI", "ART", "NN", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wann, mit des Busens Fluthen steigend, sinkend,", "tokens": ["Wann", ",", "mit", "des", "Bu\u00b7sens", "Flut\u00b7hen", "stei\u00b7gend", ",", "sin\u00b7kend", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "$,", "APPR", "ART", "NN", "NN", "VVPP", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Liebe dir wimmern?", "tokens": ["Lie\u00b7be", "dir", "wim\u00b7mern", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}}}}