{"dta.poem.7407": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Arndt's Wiedereinsetzung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192430", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "O Jubelbotschaft, die zu uns gekommen!", "tokens": ["O", "Ju\u00b7bel\u00b7bot\u00b7schaft", ",", "die", "zu", "uns", "ge\u00b7kom\u00b7men", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PRELS", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "O selten, selten Gl\u00fcck!", "tokens": ["O", "sel\u00b7ten", ",", "sel\u00b7ten", "Gl\u00fcck", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADJD", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihr hattet einen starken Mann genommen,", "tokens": ["Ihr", "hat\u00b7tet", "ei\u00b7nen", "star\u00b7ken", "Mann", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und gebt uns einen Greis zur\u00fcck!", "tokens": ["Und", "gebt", "uns", "ei\u00b7nen", "Greis", "zu\u00b7r\u00fcck", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Als einst gehemmet ihr des Schwertes Blitze", "tokens": ["Als", "einst", "ge\u00b7hem\u00b7met", "ihr", "des", "Schwer\u00b7tes", "Blit\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bei diesem Sohne Teut's,", "tokens": ["Bei", "die\u00b7sem", "Soh\u00b7ne", "Teut's", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da in das Land stie\u00df fluchend er die Spitze,", "tokens": ["Da", "in", "das", "Land", "stie\u00df", "flu\u00b7chend", "er", "die", "Spit\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "ADJD", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und kniete vor dem stumpfen Kreuz.", "tokens": ["Und", "knie\u00b7te", "vor", "dem", "stump\u00b7fen", "Kreuz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "De\u00df Lied man sich erfreut in S\u00fcd und Norden,", "tokens": ["De\u00df", "Lied", "man", "sich", "er\u00b7freut", "in", "S\u00fcd", "und", "Nor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIS", "PRF", "ADJD", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im Feld, am stillen Herd,", "tokens": ["Im", "Feld", ",", "am", "stil\u00b7len", "Herd", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Durch Eure Ruthen ist verwandelt worden", "tokens": ["Durch", "Eu\u00b7re", "Ru\u00b7then", "ist", "ver\u00b7wan\u00b7delt", "wor\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "VVPP", "VAPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sein Pegasus zum Steckenpferd.", "tokens": ["Sein", "Pe\u00b7ga\u00b7sus", "zum", "Ste\u00b7cken\u00b7pferd", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und nun, da 's Zeit, da\u00df man sie wieder z\u00fccke,", "tokens": ["Und", "nun", ",", "da", "'s", "Zeit", ",", "da\u00df", "man", "sie", "wie\u00b7der", "z\u00fc\u00b7cke", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "KOUS", "PPER", "NN", "$,", "KOUS", "PIS", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Die Flammberg' allzumal,", "tokens": ["Die", "Flamm\u00b7ber\u00b7g'", "all\u00b7zu\u00b7mal", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Nun schickt Ihr uns den Alten mit der Kr\u00fccke,", "tokens": ["Nun", "schickt", "Ihr", "uns", "den", "Al\u00b7ten", "mit", "der", "Kr\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Alt \u2014 nicht blos durch der Jahre Zahl.", "tokens": ["Alt", "nicht", "blos", "durch", "der", "Jah\u00b7re", "Zahl", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$(", "PTKNEG", "ADV", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Wohl m\u00f6cht' er stehn, wie wir noch, und nicht wanken", "tokens": ["Wohl", "m\u00f6cht'", "er", "stehn", ",", "wie", "wir", "noch", ",", "und", "nicht", "wan\u00b7ken"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "VVINF", "$,", "PWAV", "PPER", "ADV", "$,", "KON", "PTKNEG", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im heissen Pulverdampf,", "tokens": ["Im", "heis\u00b7sen", "Pul\u00b7ver\u00b7dampf", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch rufen ", "tokens": ["Doch", "ru\u00b7fen"], "token_info": ["word", "word"], "pos": ["KON", "VVINF"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}}, "stanza.6": {"line.1": {"text": "Die Kugel blieb dieselbe allerwegen", "tokens": ["Die", "Ku\u00b7gel", "blieb", "die\u00b7sel\u00b7be", "al\u00b7ler\u00b7we\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PDAT", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vom alten guten Blei,", "tokens": ["Vom", "al\u00b7ten", "gu\u00b7ten", "Blei", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch tr\u00e4gt man ihr ein ander Haubt entgegen,", "tokens": ["Doch", "tr\u00e4gt", "man", "ihr", "ein", "an\u00b7der", "Haubt", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "PPER", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sie rei\u00dft ein stolzer Herz entzwei.", "tokens": ["Sie", "rei\u00dft", "ein", "stol\u00b7zer", "Herz", "ent\u00b7zwei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Vor Einem Altar, dem der Freiheit, reichen", "tokens": ["Vor", "Ei\u00b7nem", "Al\u00b7tar", ",", "dem", "der", "Frei\u00b7heit", ",", "rei\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "ART", "NN", "$,", "ADJA"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Sich V\u00f6lker nun die Hand,", "tokens": ["Sich", "V\u00f6l\u00b7ker", "nun", "die", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "NN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und weiter, als die Lorbern und die Eichen,", "tokens": ["Und", "wei\u00b7ter", ",", "als", "die", "Lor\u00b7bern", "und", "die", "Ei\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "KOUS", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dehnt sich des Deutschen Vaterland.", "tokens": ["Dehnt", "sich", "des", "Deut\u00b7schen", "Va\u00b7ter\u00b7land", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Die Sterne blassen, wenn die Sonnen funkeln,", "tokens": ["Die", "Ster\u00b7ne", "blas\u00b7sen", ",", "wenn", "die", "Son\u00b7nen", "fun\u00b7keln", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "KOUS", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und Sonne ist er nicht;", "tokens": ["Und", "Son\u00b7ne", "ist", "er", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er ist ein sch\u00f6ner Stern, la\u00dft ihn im Dunkeln!", "tokens": ["Er", "ist", "ein", "sch\u00f6\u00b7ner", "Stern", ",", "la\u00dft", "ihn", "im", "Dun\u00b7keln", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,", "VVIMP", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Was rei\u00dft Ihr ihn an's Morgenlicht?", "tokens": ["Was", "rei\u00dft", "Ihr", "ihn", "an's", "Mor\u00b7gen\u00b7licht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Er ist ein Abendrot und mag noch feuchten", "tokens": ["Er", "ist", "ein", "A\u00b7ben\u00b7drot", "und", "mag", "noch", "feuch\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "KON", "VMFIN", "ADV", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Manch Auge, kummerschwer,", "tokens": ["Manch", "Au\u00b7ge", ",", "kum\u00b7mer\u00b7schwer", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PIAT", "NN", "$,", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "All", "tokens": ["All"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "nn er die junge Welt nicht mehr.", "tokens": ["nn", "er", "die", "jun\u00b7ge", "Welt", "nicht", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Es zieht durch sie ein frischer schaffend Wehen", "tokens": ["Es", "zieht", "durch", "sie", "ein", "fri\u00b7scher", "schaf\u00b7fend", "We\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "ART", "ADJA", "ADJD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In ungehemmtem Lauf,", "tokens": ["In", "un\u00b7ge\u00b7hemm\u00b7tem", "Lauf", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und mit des Fr\u00fchlings neuen Blumen gehen", "tokens": ["Und", "mit", "des", "Fr\u00fch\u00b7lings", "neu\u00b7en", "Blu\u00b7men", "ge\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Auch neue gro\u00dfe Herzen auf!", "tokens": ["Auch", "neu\u00b7e", "gro\u00b7\u00dfe", "Her\u00b7zen", "auf", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}