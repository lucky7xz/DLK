{"textgrid.poem.58524": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "562. Aufmunterung zum Kampf", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer sollt' bei so viel Feinden schlafen", "tokens": ["Wer", "sollt'", "bei", "so", "viel", "Fein\u00b7den", "schla\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "APPR", "ADV", "PIAT", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und sitzen still in Sicherheit?", "tokens": ["Und", "sit\u00b7zen", "still", "in", "Si\u00b7cher\u00b7heit", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wer leben will, ergreif' die Waffen", "tokens": ["Wer", "le\u00b7ben", "will", ",", "er\u00b7greif'", "die", "Waf\u00b7fen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "VVINF", "VMFIN", "$,", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und g\u00fcrte sich getrost zum Streit!", "tokens": ["Und", "g\u00fcr\u00b7te", "sich", "ge\u00b7trost", "zum", "Streit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "La\u00df Welt und alle Last dahinten,", "tokens": ["La\u00df", "Welt", "und", "al\u00b7le", "Last", "da\u00b7hin\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "KON", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ehr', Hoheit, Wollust, Geld und Gut,", "to