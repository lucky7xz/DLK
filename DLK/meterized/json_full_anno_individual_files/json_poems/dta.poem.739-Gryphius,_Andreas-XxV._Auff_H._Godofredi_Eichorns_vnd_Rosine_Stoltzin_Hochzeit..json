{"dta.poem.739": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "XxV.  Auff H. Godofredi Eichorns vnd  \n Rosine Stoltzin Hochzeit.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Ob gleich der weisse Schnee jtzt Thal' vnd Berge decket/ ", "tokens": ["Ob", "gleich", "der", "weis\u00b7se", "Schnee", "jtzt", "Thal'", "vnd", "Ber\u00b7ge", "de\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "ADV", "NN", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vnd manch geschwinder ", "tokens": ["Vnd", "manch", "ge\u00b7schwin\u00b7der"], "token_info": ["word", "word", "word"], "pos": ["KON", "PIAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "In dem er sich des zorns der grimmen k\u00e4lt\u2019 erwehrt/", "tokens": ["In", "dem", "er", "sich", "des", "zorns", "der", "grim\u00b7men", "k\u00e4lt'", "er\u00b7wehrt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PRF", "ART", "NN", "ART", "ADJA", "VVFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Vor welcher jeder Baum steht bis in tod erschrecket;", "tokens": ["Vor", "wel\u00b7cher", "je\u00b7der", "Baum", "steht", "bis", "in", "tod", "er\u00b7schre\u00b7cket", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PIAT", "NN", "VVFIN", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ob gleich der bleiche frost die scharffe ", "tokens": ["Ob", "gleich", "der", "blei\u00b7che", "frost", "die", "scharf\u00b7fe"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vnd alle Blumen raubt die ", "tokens": ["Vnd", "al\u00b7le", "Blu\u00b7men", "raubt", "die"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "VVFIN", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Hat doch der Liebe flamm euch s\u00fcsser zeit beschert", "tokens": ["Hat", "doch", "der", "Lie\u00b7be", "flamm", "euch", "s\u00fcs\u00b7ser", "zeit", "be\u00b7schert"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ART", "NN", "VVFIN", "PPER", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Als wol die Sonne selbst/ vnd hitz vnd lust erwecket/", "tokens": ["Als", "wol", "die", "Son\u00b7ne", "selbst", "/", "vnd", "hitz", "vnd", "lust", "er\u00b7we\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "ADV", "$(", "KON", "NN", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Sie hat Herr Gottfried euch die sch\u00f6ne Rose bracht/", "tokens": ["Sie", "hat", "Herr", "Gott\u00b7fried", "euch", "die", "sch\u00f6\u00b7ne", "Ro\u00b7se", "bracht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "NE", "PPER", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+---+-+-+", "measure": "unknown.measure.penta"}, "line.2": {"text": "Bey der jhr fr\u00fcling habt/ vnd aller winter lacht/", "tokens": ["Bey", "der", "jhr", "fr\u00fc\u00b7ling", "habt", "/", "vnd", "al\u00b7ler", "win\u00b7ter", "lacht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADJD", "VAFIN", "$(", "KON", "PIAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wol euch/ vnd mehr denn wol! was m\u00f6gt jhr noch erdenck&#7869;? ", "tokens": ["Wol", "euch", "/", "vnd", "mehr", "denn", "wol", "!", "was", "m\u00f6gt", "jhr", "noch", "er\u00b7denck", "&#7869;", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "XML_entity", "punct"], "pos": ["ADV", "PPER", "$(", "KON", "ADV", "ADV", "ADV", "$.", "PWS", "VMFIN", "PPER", "ADV", "ADJD", "$(", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}}, "stanza.4": {"line.1": {"text": "Wol euch vnd mehr denn wol! wenn diese rawe zeit", "tokens": ["Wol", "euch", "vnd", "mehr", "denn", "wol", "!", "wenn", "die\u00b7se", "ra\u00b7we", "zeit"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "KON", "ADV", "ADV", "ADV", "$.", "KOUS", "PDAT", "ADJA", "NN"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "So sch\u00f6ne Blumen gibt vnd solche lust bereit", "tokens": ["So", "sch\u00f6\u00b7ne", "Blu\u00b7men", "gibt", "vnd", "sol\u00b7che", "lust", "be\u00b7reit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "VVFIN", "KON", "PIAT", "NN", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Was wird\u2019 euch nicht der Herbst f\u00fcr s\u00fcsse Fr\u00fcchte schencken.", "tokens": ["Was", "wird'", "euch", "nicht", "der", "Herbst", "f\u00fcr", "s\u00fcs\u00b7se", "Fr\u00fcch\u00b7te", "schen\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "PTKNEG", "ART", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}