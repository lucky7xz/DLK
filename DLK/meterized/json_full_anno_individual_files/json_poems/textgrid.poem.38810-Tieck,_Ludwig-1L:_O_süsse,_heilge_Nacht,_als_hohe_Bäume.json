{"textgrid.poem.38810": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: O s\u00fcsse, heilge Nacht, als hohe B\u00e4ume", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O s\u00fcsse, heilge Nacht, als hohe B\u00e4ume", "tokens": ["O", "s\u00fcs\u00b7se", ",", "heil\u00b7ge", "Nacht", ",", "als", "ho\u00b7he", "B\u00e4u\u00b7me"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VVFIN", "$,", "ADJA", "NN", "$,", "KOUS", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit Geisterstimmen durch das Dunkel rauschten,", "tokens": ["Mit", "Geis\u00b7ters\u00b7tim\u00b7men", "durch", "das", "Dun\u00b7kel", "rauschten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Gespr\u00e4ch und Wort dort mit dem Strome tauschten,", "tokens": ["Ge\u00b7spr\u00e4ch", "und", "Wort", "dort", "mit", "dem", "Stro\u00b7me", "tauschten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der Mond aussandte sanfte Liebestr\u00e4ume.", "tokens": ["Der", "Mond", "aus\u00b7sand\u00b7te", "sanf\u00b7te", "Lie\u00b7be\u00b7str\u00e4u\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Da f\u00fchlt' ich euch ihr hohen Sternenr\u00e4ume,", "tokens": ["Da", "f\u00fchlt'", "ich", "euch", "ihr", "ho\u00b7hen", "Ster\u00b7nen\u00b7r\u