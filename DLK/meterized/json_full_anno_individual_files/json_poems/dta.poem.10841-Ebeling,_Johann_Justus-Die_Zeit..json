{"dta.poem.10841": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Die Zeit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198774", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Zeit ist wie ein Flus, der unverr\u00fckt\nfortflie\u00dft,", "tokens": ["Die", "Zeit", "ist", "wie", "ein", "Flus", ",", "der", "un\u00b7ver\u00b7r\u00fckt", "fort\u00b7flie\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "KOKOM", "ART", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bis sie sich in das Meer der Ewig-", "tokens": ["Bis", "sie", "sich", "in", "das", "Meer", "der", "E\u00b7wig"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "APPR", "ART", "NN", "ART", "TRUNC"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die Schiffer sind wir selbst; die mit den Flus fort", "tokens": ["Die", "Schif\u00b7fer", "sind", "wir", "selbst", ";", "die", "mit", "den", "Flus", "fort"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "$.", "ART", "APPR", "ART", "NN", "PTKVZ"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Hofnung ist der Kahn; worauf was wir er-", "tokens": ["Die", "Hof\u00b7nung", "ist", "der", "Kahn", ";", "wo\u00b7rauf", "was", "wir", "er"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$.", "PWAV", "PWS", "PPER", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Mit uns wird fort gebracht. So wie offt Nord", "tokens": ["Mit", "uns", "wird", "fort", "ge\u00b7bracht", ".", "So", "wie", "offt", "Nord"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VAFIN", "PTKVZ", "VVPP", "$.", "ADV", "KOKOM", "ADV", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Wenn man auf Fl\u00fcssen schifft, ins Seegel st\u00fcrmmt", "tokens": ["Wenn", "man", "auf", "Fl\u00fcs\u00b7sen", "schifft", ",", "ins", "See\u00b7gel", "st\u00fcrmmt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PIS", "APPR", "NN", "VVFIN", "$,", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Das Schiff an Klippen schmeist; das die Gefahr", "tokens": ["Das", "Schiff", "an", "Klip\u00b7pen", "schmeist", ";", "das", "die", "Ge\u00b7fahr"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN", "VVFIN", "$.", "PDS", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Man werde auf der Fluth mit seinen Waaren stran-", "tokens": ["Man", "wer\u00b7de", "auf", "der", "Fluth", "mit", "sei\u00b7nen", "Waa\u00b7ren", "stran"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VAFIN", "APPR", "ART", "NN", "APPR", "PPOSAT", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "So gehts den Menschen auch. Es sind im Flus der", "tokens": ["So", "gehts", "den", "Men\u00b7schen", "auch", ".", "Es", "sind", "im", "Flus", "der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "$.", "PPER", "VAFIN", "APPRART", "NN", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Viel Klippen aufgestellt, woran die Festigkeit", "tokens": ["Viel", "Klip\u00b7pen", "auf\u00b7ge\u00b7stellt", ",", "wo\u00b7ran", "die", "Fes\u00b7tig\u00b7keit"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIAT", "NN", "VVPP", "$,", "PWAV", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Des Muthes \u00f6ffters bricht; wenn Ungl\u00fckswinde", "tokens": ["Des", "Mu\u00b7thes", "\u00f6ff\u00b7ters", "bricht", ";", "wenn", "Un\u00b7gl\u00fcks\u00b7win\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ADV", "VVFIN", "$.", "KOUS", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "So mu\u00df man in der Zeit offt viele Noth ausste-", "tokens": ["So", "mu\u00df", "man", "in", "der", "Zeit", "offt", "vie\u00b7le", "Noth", "a\u00b7usste"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PIS", "APPR", "ART", "NN", "ADV", "PIAT", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Allein so lange noch der Hofnungs-Kahn sich h\u00e4lt,", "tokens": ["Al\u00b7lein", "so", "lan\u00b7ge", "noch", "der", "Hof\u00b7nungs\u00b7Kahn", "sich", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "ADV", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Der Anker nicht zerbricht, und in dem Sturm zer-", "tokens": ["Der", "An\u00b7ker", "nicht", "zer\u00b7bricht", ",", "und", "in", "dem", "Sturm", "zer"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PTKNEG", "VVFIN", "$,", "KON", "APPR", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Gedenkt man immer noch zum Haffen zu gelan-", "tokens": ["Ge\u00b7denkt", "man", "im\u00b7mer", "noch", "zum", "Haf\u00b7fen", "zu", "ge\u00b7lan"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "ADV", "ADV", "APPRART", "NN", "APPR", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Man rudert wieder fort; ein m\u00fchsam Unterfangen", "tokens": ["Man", "ru\u00b7dert", "wie\u00b7der", "fort", ";", "ein", "m\u00fch\u00b7sam", "Un\u00b7ter\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIS", "VVFIN", "ADV", "PTKVZ", "$.", "ART", "ADJD", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Wird offt mit Gl\u00fck bekr\u00f6nt. Wer auf dem Flus-", "tokens": ["Wird", "offt", "mit", "Gl\u00fck", "be\u00b7kr\u00f6nt", ".", "Wer", "auf", "dem", "Flus"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "APPR", "NN", "VVFIN", "$.", "PWS", "APPR", "ART", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Und nicht den rechten Weg zum Meeres Eingang", "tokens": ["Und", "nicht", "den", "rech\u00b7ten", "Weg", "zum", "Mee\u00b7res", "Ein\u00b7gang"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PTKNEG", "ART", "ADJA", "NN", "APPRART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Der st\u00fcrzet in den Schlund, und da ist kein er-", "tokens": ["Der", "st\u00fcr\u00b7zet", "in", "den", "Schlund", ",", "und", "da", "ist", "kein", "er"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPR", "ART", "NN", "$,", "KON", "ADV", "VAFIN", "PIAT", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Da helffen Anker nicht, noch Seegel, Mast und", "tokens": ["Da", "helf\u00b7fen", "An\u00b7ker", "nicht", ",", "noch", "See\u00b7gel", ",", "Mast", "und"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "NN", "PTKNEG", "$,", "ADV", "NN", "$,", "NN", "KON"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Die gr\u00f6sseste Gefahr ist auf dem Flus der", "tokens": ["Die", "gr\u00f6s\u00b7ses\u00b7te", "Ge\u00b7fahr", "ist", "auf", "dem", "Flus", "der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "APPR", "ART", "NN", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Wo sich derselbe giest ins Meer der Ewig-", "tokens": ["Wo", "sich", "der\u00b7sel\u00b7be", "giest", "ins", "Meer", "der", "E\u00b7wig"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PRF", "PDAT", "VVFIN", "APPRART", "NN", "ART", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Da ist ein Scheideweg; wer da nicht kl\u00fcglich", "tokens": ["Da", "ist", "ein", "Schei\u00b7de\u00b7weg", ";", "wer", "da", "nicht", "kl\u00fcg\u00b7lich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "$.", "PWS", "ADV", "PTKNEG", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Der st\u00fcrzet in den Schlund, draus keiner wieder-", "tokens": ["Der", "st\u00fcr\u00b7zet", "in", "den", "Schlund", ",", "draus", "kei\u00b7ner", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPR", "ART", "NN", "$,", "PAV", "PIAT", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "Wer aber recht erlernt; so lang er auf dem", "tokens": ["Wer", "a\u00b7ber", "recht", "er\u00b7lernt", ";", "so", "lang", "er", "auf", "dem"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "ADJD", "VVPP", "$.", "ADV", "ADJD", "PPER", "APPR", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "Wie er zum rechten Weg ohnfehlbar kommen", "tokens": ["Wie", "er", "zum", "rech\u00b7ten", "Weg", "ohn\u00b7fehl\u00b7bar", "kom\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPRART", "ADJA", "NN", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Stets auf den Compas sieht, der ihm die Stras-", "tokens": ["Stets", "auf", "den", "Com\u00b7pas", "sieht", ",", "der", "ihm", "die", "Stras"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "$,", "PRELS", "PPER", "ART", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "Und darnach seinen Kahn mit kluger Vorsicht", "tokens": ["Und", "dar\u00b7nach", "sei\u00b7nen", "Kahn", "mit", "klu\u00b7ger", "Vor\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PAV", "PPOSAT", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "Der schiffet gl\u00fcklich ein, und nach getroffner", "tokens": ["Der", "schif\u00b7fet", "gl\u00fck\u00b7lich", "ein", ",", "und", "nach", "ge\u00b7troff\u00b7ner"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ADJD", "PTKVZ", "$,", "KON", "APPR", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.30": {"text": "Kommt er zu seiner Ruh in sichren Haffen an.", "tokens": ["Kommt", "er", "zu", "sei\u00b7ner", "Ruh", "in", "sich\u00b7ren", "Haf\u00b7fen", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Ein jeder wird verstehn, was dieses Lehrbild-", "tokens": ["Ein", "je\u00b7der", "wird", "ver\u00b7stehn", ",", "was", "die\u00b7ses", "Lehr\u00b7bild"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "PIS", "VAFIN", "VVINF", "$,", "PWS", "PDAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.32": {"text": "Wer mit der schnellen Zeit zur Ewigkeit fort-", "tokens": ["Wer", "mit", "der", "schnel\u00b7len", "Zeit", "zur", "E\u00b7wig\u00b7keit", "for\u00b7t"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "ART", "ADJA", "NN", "APPRART", "NN", "TRUNC"], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.33": {"text": "Jm Todt nicht gl\u00fcklich f\u00e4hrt, der sinkt zum Schlund", "tokens": ["Jm", "Todt", "nicht", "gl\u00fck\u00b7lich", "f\u00e4hrt", ",", "der", "sinkt", "zum", "Schlund"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "PTKNEG", "ADJD", "VVFIN", "$,", "PRELS", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.34": {"text": "Wo ihn die Angst und Noth, wie Wirbel \u00fcber-", "tokens": ["Wo", "ihn", "die", "Angst", "und", "Noth", ",", "wie", "Wir\u00b7bel", "\u00fc\u00b7ber"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "PPER", "ART", "NN", "KON", "NN", "$,", "PWAV", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.35": {"text": "Wer auf der rechten Bahn, die ", "tokens": ["Wer", "auf", "der", "rech\u00b7ten", "Bahn", ",", "die"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["PWS", "APPR", "ART", "ADJA", "NN", "$,", "PRELS"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.36": {"text": "Die uns der Heiland zeigt und auch ge\u00f6ffnet,", "tokens": ["Die", "uns", "der", "Hei\u00b7land", "zeigt", "und", "auch", "ge\u00b7\u00f6ff\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVFIN", "KON", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.37": {"text": "Der kommt begl\u00fckt hindurch, wird in den Haffen", "tokens": ["Der", "kommt", "be\u00b7gl\u00fckt", "hin\u00b7durch", ",", "wird", "in", "den", "Haf\u00b7fen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "VVFIN", "PAV", "$,", "VAFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.38": {"text": "In jener Seeligkeit; wo g\u00fcldne Ehrenkro-", "tokens": ["In", "je\u00b7ner", "See\u00b7lig\u00b7keit", ";", "wo", "g\u00fcld\u00b7ne", "Eh\u00b7ren\u00b7kro"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "$.", "PWAV", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Und andre Sch\u00e4ze sind. Mein GOtt! der Flus", "tokens": ["Und", "and\u00b7re", "Sch\u00e4\u00b7ze", "sind", ".", "Mein", "Gott", "!", "der", "Flus"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADJA", "NN", "VAFIN", "$.", "PPOSAT", "NN", "$.", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.40": {"text": "F\u00fchrt mich stets weiter fort, zum Meer der Ewig-", "tokens": ["F\u00fchrt", "mich", "stets", "wei\u00b7ter", "fort", ",", "zum", "Meer", "der", "E\u00b7wig"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "PTKVZ", "$,", "APPRART", "NN", "ART", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.41": {"text": "Ich weis nicht wenn ich werd, zu dieser Scheidung", "tokens": ["Ich", "weis", "nicht", "wenn", "ich", "werd", ",", "zu", "die\u00b7ser", "Schei\u00b7dung"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "PTKVZ", "PTKNEG", "KOUS", "PPER", "VAFIN", "$,", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.42": {"text": "Da mein entbundner Geist wird endlich aufgenom-", "tokens": ["Da", "mein", "ent\u00b7bund\u00b7ner", "Geist", "wird", "end\u00b7lich", "auf\u00b7ge\u00b7nom"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "VAFIN", "ADV", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "In jene Ewigkeit. Mach mich in dieser Zeit,", "tokens": ["In", "je\u00b7ne", "E\u00b7wig\u00b7keit", ".", "Mach", "mich", "in", "die\u00b7ser", "Zeit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$.", "VVFIN", "PRF", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Durch deinen Gnadengeist, zu dieser Fahrt bereit.", "tokens": ["Durch", "dei\u00b7nen", "Gna\u00b7den\u00b7geist", ",", "zu", "die\u00b7ser", "Fahrt", "be\u00b7reit", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "APPR", "PDAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Ich finde hie und da verf\u00fcrische Sirenen,", "tokens": ["Ich", "fin\u00b7de", "hie", "und", "da", "ver\u00b7f\u00fc\u00b7ri\u00b7sche", "Si\u00b7re\u00b7nen", ","], "token_info": ["word"