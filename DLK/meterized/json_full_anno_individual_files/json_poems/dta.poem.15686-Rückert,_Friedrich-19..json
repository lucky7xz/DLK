{"dta.poem.15686": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "19.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Den Gegner setze nicht herab, dem vorgezogen", "tokens": ["Den", "Geg\u00b7ner", "set\u00b7ze", "nicht", "her\u00b7ab", ",", "dem", "vor\u00b7ge\u00b7zo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "ADV", "$,", "ART", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du hoffest einst zu seyn, wenn dir das Gl\u00fcck gewogen.", "tokens": ["Du", "hof\u00b7fest", "einst", "zu", "seyn", ",", "wenn", "dir", "das", "Gl\u00fcck", "ge\u00b7wo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKZU", "VAINF", "$,", "KOUS", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wenn \u00fcber ihm den Platz sie dir erkennen zu,", "tokens": ["Wenn", "\u00fc\u00b7ber", "ihm", "den", "Platz", "sie", "dir", "er\u00b7ken\u00b7nen", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPER", "ART", "NN", "PPER", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Je h\u00f6her selbst er stand, je h\u00f6her stehest du.", "tokens": ["Je", "h\u00f6\u00b7her", "selbst", "er", "stand", ",", "je", "h\u00f6\u00b7her", "ste\u00b7hest", "du", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "PPER", "VVFIN", "$,", "ADV", "ADJD