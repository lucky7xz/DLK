{"dta.poem.9048": {"metadata": {"author": {"name": "Schmolck, Benjamin", "birth": "N.A.", "death": "N.A."}, "title": "I.  \n  M orgen- A ndacht  \n am Sonntage.  \n Melod. Des Morgens/ wenn ich fr\u00fch auffsich.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1712", "urn": "urn:nbn:de:kobv:b4-20543-0", "language": ["de:0.99"], "booktitle": "Schmolck, Benjamin: Der Lustige Sabbath. Jauer u. a., 1712."}, "poem": {"stanza.1": {"line.1": {"text": "1. (herf\u00fcr/", "tokens": ["(", "her\u00b7f\u00fcr", "/"], "token_info": ["punct", "word", "punct"], "pos": ["$(", "ADV", "$("], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "Mein GOTt/ die Sonne geht", "tokens": ["Mein", "GoTt", "/", "die", "Son\u00b7ne", "geht"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$(", "ART", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sey du die So\u00f1e selbst in mir/", "tokens": ["Sey", "du", "die", "So\u00f1e", "selbst", "in", "mir", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "ADV", "APPR", "PPER", "$("], "meter": "+--++-+", "measure": "dactylic.init"}, "line.4": {"text": "Du So\u00f1e der Gerechtigkeit/", "tokens": ["Du", "So\u00f1e", "der", "Ge\u00b7rech\u00b7tig\u00b7keit", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ART", "NN", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Vertreib der S\u00fcnden Dunckelheit.", "tokens": ["Ver\u00b7treib", "der", "S\u00fcn\u00b7den", "Dun\u00b7ckel\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "2.", "tokens": [], "token_info": [], "pos": []}, "line.2": {"text": "Mein erstes Opffer ist dein Ruhm/", "tokens": ["Mein", "ers\u00b7tes", "Opf\u00b7fer", "ist", "dein", "Ruhm", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mein Hertz ist selbst dein Eigenthum/", "tokens": ["Mein", "Hertz", "ist", "selbst", "dein", "Ei\u00b7gen\u00b7thum", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ach kehre gn\u00e4dig bey mir ein/", "tokens": ["Ach", "keh\u00b7re", "gn\u00e4\u00b7dig", "bey", "mir", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "ADJD", "APPR", "PPER", "ART", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du must dir selbst den Tempel weihn.", "tokens": ["Du", "must", "dir", "selbst", "den", "Tem\u00b7pel", "weihn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "3.", "tokens": [], "token_info": [], "pos": []}, "line.2": {"text": "Gieb/ da\u00df ich meinen Fu\u00df bewahr/", "tokens": ["Gieb", "/", "da\u00df", "ich", "mei\u00b7nen", "Fu\u00df", "be\u00b7wahr", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$(", "KOUS", "PPER", "PPOSAT", "NN", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Eh ich mit deiner Kirchen-Schaar", "tokens": ["Eh", "ich", "mit", "dei\u00b7ner", "Kir\u00b7chen\u00b7Schaar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zum Hause GOttes wallen geh/", "tokens": ["Zum", "Hau\u00b7se", "Got\u00b7tes", "wal\u00b7len", "geh", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NN", "VVINF", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da\u00df ich auch heilig f\u00fcr dir steh.", "tokens": ["Da\u00df", "ich", "auch", "hei\u00b7lig", "f\u00fcr", "dir", "steh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "4.", "tokens": [], "token_info": [], "pos": []}, "line.2": {"text": "Bereite Hertze/ Mund und Hand/", "tokens": ["Be\u00b7rei\u00b7te", "Hert\u00b7ze", "/", "Mund", "und", "Hand", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$(", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und gib mir Wei\u00dfheit u\u00f1 Berstand", "tokens": ["Und", "gib", "mir", "Wei\u00df\u00b7heit", "u\u00f1", "Ber\u00b7stand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "PPER", "NN", "KON", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Da\u00df ich dein Wort mit Andacht h\u00f6r/", "tokens": ["Da\u00df", "ich", "dein", "Wort", "mit", "An\u00b7dacht", "h\u00f6r", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Zu deines grossen Nahmens Ehr.", "tokens": ["Zu", "dei\u00b7nes", "gros\u00b7sen", "Nah\u00b7mens", "Ehr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "5.", "tokens": [], "token_info": [], "pos": []}, "line.2": {"text": "Schreib alles fest in meinen Sinn/", "tokens": ["Schreib", "al\u00b7les", "fest", "in", "mei\u00b7nen", "Sinn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIS", "ADJD", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df ich nicht nur ein H\u00f6rer bin/", "tokens": ["Da\u00df", "ich", "nicht", "nur", "ein", "H\u00f6\u00b7rer", "bin", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADV", "ART", "NN", "VAFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Verleihe deine Krafft dabey/", "tokens": ["Ver\u00b7lei\u00b7he", "dei\u00b7ne", "Krafft", "da\u00b7bey", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PAV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da\u00df ich zugleich ein Th\u00e4ter sey.", "tokens": ["Da\u00df", "ich", "zu\u00b7gleich", "ein", "Th\u00e4\u00b7ter", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "6.", "tokens": [], "token_info": [], "pos": []}, "line.2": {"text": "Hilff da\u00df ich diesen gantzen Tag", "tokens": ["Hilff", "da\u00df", "ich", "die\u00b7sen", "gant\u00b7zen", "Tag"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KOUS", "PPER", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit Leib und Seele feyren mag.", "tokens": ["Mit", "Leib", "und", "See\u00b7le", "fey\u00b7ren", "mag."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bewahr mich f\u00fcr der argen Welt/", "tokens": ["Be\u00b7wahr", "mich", "f\u00fcr", "der", "ar\u00b7gen", "Welt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die deinen Sabbath s\u00fcndlich h\u00e4lt.", "tokens": ["Die", "dei\u00b7nen", "Sab\u00b7ba\u00b7th", "s\u00fcnd\u00b7lich", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "7.", "tokens": [], "token_info": [], "pos": []}, "line.2": {"text": "So geh ich denn mit Freuden hin/", "tokens": ["So", "geh", "ich", "denn", "mit", "Freu\u00b7den", "hin", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wo ich bey dir zu Hause bin/", "tokens": ["Wo", "ich", "bey", "dir", "zu", "Hau\u00b7se", "bin", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "PPER", "APPR", "NN", "VAFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mein Hertz ist willig und bereit/", "tokens": ["Mein", "Hertz", "ist", "wil\u00b7lig", "und", "be\u00b7reit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "O Heilige Dreyfaltigkeit!", "tokens": ["O", "Hei\u00b7li\u00b7ge", "Drey\u00b7fal\u00b7tig\u00b7keit", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}