{"dta.poem.12739": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Der weiber grausamkeit.  \n L. R. B.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was den weibern angebohren,", "tokens": ["Was", "den", "wei\u00b7bern", "an\u00b7ge\u00b7boh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wird wol meistens schwachheit seyn;", "tokens": ["Wird", "wol", "meis\u00b7tens", "schwach\u00b7heit", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ADJD", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dennoch haben sie geschworen,", "tokens": ["Den\u00b7noch", "ha\u00b7ben", "sie", "ge\u00b7schwo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df dem floh kein gantzes bein", "tokens": ["Da\u00df", "dem", "floh", "kein", "gant\u00b7zes", "bein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "VVFIN", "PIAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "An dem leibe bleiben solte,", "tokens": ["An", "dem", "lei\u00b7be", "blei\u00b7ben", "sol\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Der sich an sie reiben wolte.", "tokens": ["Der", "sich", "an", "sie", "rei\u00b7ben", "wol\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "PPER", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}