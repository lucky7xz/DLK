{"dta.poem.9901": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "10.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Meine farb dem feuer gleicht/ ohne bein bin ich gemachet/", "tokens": ["Mei\u00b7ne", "farb", "dem", "feu\u00b7er", "gleicht", "/", "oh\u00b7ne", "bein", "bin", "ich", "ge\u00b7ma\u00b7chet", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVFIN", "ART", "NN", "VVFIN", "$(", "APPR", "ADV", "VAFIN", "PPER", "VVPP", "$("], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Werde wie der h\u00f6chste f\u00fcrst von soldaten wohl bewachet/", "tokens": ["Wer\u00b7de", "wie", "der", "h\u00f6chs\u00b7te", "f\u00fcrst", "von", "sol\u00b7da\u00b7ten", "wohl", "be\u00b7wa\u00b7chet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KOKOM", "ART", "ADJA", "ADV", "APPR", "VMFIN", "ADV", "VVFIN", "$("], "meter": "--+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Ich hab frieden in den h\u00e4nden/ und kan machen krieg der welt/", "tokens": ["Ich", "hab", "frie\u00b7den", "in", "den", "h\u00e4n\u00b7den", "/", "und", "kan", "ma\u00b7chen", "krieg", "der", "welt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "APPR", "ART", "ADJA", "$(", "KON", "VMFIN", "VVINF", "VVFIN", "ART", "NN", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Weil man mich f\u00fcrs allerbeste/ wie auch f\u00fcr das schlimste h\u00e4lt.", "tokens": ["Weil", "man", "mich", "f\u00fcrs", "al\u00b7ler\u00b7bes\u00b7te", "/", "wie", "auch", "f\u00fcr", "das", "schlims\u00b7te", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PRF", "APPRART", "ADJA", "$(", "KOKOM", "ADV", "APPR", "ART", "ADJA", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}}}}