{"textgrid.poem.48950": {"metadata": {"author": {"name": "Arnold, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "10.", "genre": "verse", "period": "N.A.", "pub_year": 1690, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr Salems-t\u00f6chter, h\u00f6rt, die ihr an stein und mauren", "tokens": ["Ihr", "Sa\u00b7lems\u00b7t\u00f6ch\u00b7ter", ",", "h\u00f6rt", ",", "die", "ihr", "an", "stein", "und", "mau\u00b7ren"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "$,", "PRELS", "PPER", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bindt Gottes dienst und lieb und nur auffs \u00e4u\u00dfre gafft,", "tokens": ["Bindt", "Got\u00b7tes", "dienst", "und", "lieb", "und", "nur", "auffs", "\u00e4u\u00df\u00b7re", "gafft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ADV", "KON", "ADJD", "KON", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Aus irdisch-grobem sinn: Ihr d\u00fcrfft mich wol bedauren,", "tokens": ["Aus", "ir\u00b7dischgro\u00b7bem", "sinn", ":", "Ihr", "d\u00fcrfft", "mich", "wol", "be\u00b7dau\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$.", "PPER", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "Als h\u00e4tt mir diese schw\u00e4rtz die einsamkeit verschafft.", "tokens": ["Als", "h\u00e4tt", "mir", "die\u00b7se", "schw\u00e4rtz", "die", "ein\u00b7sam\u00b7keit", "ver\u00b7schafft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "PDS", "VVFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ihr seht hier keine pracht, nicht kirchen-pomp noch schreyen,", "tokens": ["Ihr", "seht", "hier", "kei\u00b7ne", "pracht", ",", "nicht", "kir\u00b7chen\u00b7pomp", "noch", "schre\u00b7yen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PIAT", "NN", "$,", "PTKNEG", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Kein opffer noch altar, kein bild, kein schatten werck.", "tokens": ["Kein", "opf\u00b7fer", "noch", "al\u00b7tar", ",", "kein", "bild", ",", "kein", "schat\u00b7ten", "werck", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "ADJD", "$,", "PIAT", "NN", "$,", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wir sind einf\u00e4ltig schlecht, und heissen arme layen,", "tokens": ["Wir", "sind", "ein\u00b7f\u00e4l\u00b7tig", "schlecht", ",", "und", "heis\u00b7sen", "ar\u00b7me", "la\u00b7yen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "VVFIN", "$,", "KON", "ADJA", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.8": {"text": "Die nicht zum Priesterthum von andern gunst und st\u00e4rck", "tokens": ["Die", "nicht", "zum", "Pries\u00b7ter\u00b7thum", "von", "an\u00b7dern", "gunst", "und", "st\u00e4rck"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PTKNEG", "APPRART", "NN", "APPR", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Zu holen sind gewohnt. Je schw\u00e4rtzer dir von ferne", "tokens": ["Zu", "ho\u00b7len", "sind", "ge\u00b7wohnt", ".", "Je", "schw\u00e4rt\u00b7zer", "dir", "von", "fer\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "VAFIN", "VVPP", "$.", "ADV", "ADJD", "PPER", "APPR", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Scheint Christi braut zu seyn, o blinde unvernunfft", "tokens": ["Scheint", "Chris\u00b7ti", "braut", "zu", "seyn", ",", "o", "blin\u00b7de", "un\u00b7ver\u00b7nunfft"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "NE", "VVFIN", "PTKZU", "VAINF", "$,", "FM", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Je mehr erblickt der Geist an diesem himmel sterne,", "tokens": ["Je", "mehr", "er\u00b7blickt", "der", "Geist", "an", "die\u00b7sem", "him\u00b7mel", "ster\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ART", "NN", "APPR", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Wie wol noch gantz verdeckt. Wer diese Jesus zunfft,", "tokens": ["Wie", "wol", "noch", "gantz", "ver\u00b7deckt", ".", "Wer", "die\u00b7se", "Je\u00b7sus", "zunfft", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADV", "ADV", "VVPP", "$.", "PWS", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Und ihre Glori wei\u00df, der sucht sie nicht im schw\u00e4tzen", "tokens": ["Und", "ih\u00b7re", "Glo\u00b7ri", "wei\u00df", ",", "der", "sucht", "sie", "nicht", "im", "schw\u00e4t\u00b7zen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "$,", "PRELS", "VVFIN", "PPER", "PTKNEG", "APPRART", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und in gelehrsamkeit, so die verf\u00fchrerin", "tokens": ["Und", "in", "ge\u00b7lehr\u00b7sam\u00b7keit", ",", "so", "die", "ver\u00b7f\u00fch\u00b7re\u00b7rin"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "$,", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Die schlang hat bey den baum des wissens wollen setzen,", "tokens": ["Die", "schlang", "hat", "bey", "den", "baum", "des", "wis\u00b7sens", "wol\u00b7len", "set\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VAFIN", "APPR", "ART", "NN", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Das der gehorsam nicht den lebens-baum gewinn.", "tokens": ["Das", "der", "ge\u00b7hor\u00b7sam", "nicht", "den", "le\u00b7bens\u00b7baum", "ge\u00b7winn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "ADJD", "PTKNEG", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Nein, hier gilt keine schminck der falsch-ber\u00fchmten k\u00fcnste", "tokens": ["Nein", ",", "hier", "gilt", "kei\u00b7ne", "schminck", "der", "falschbe\u00b7r\u00fchm\u00b7ten", "k\u00fcns\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "ADV", "VVFIN", "PIAT", "NN", "ART", "ADJA", "ADJA"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.18": {"text": "Und keine deuteley, ob mans auch predigt nennt,", "tokens": ["Und", "kei\u00b7ne", "deu\u00b7te\u00b7ley", ",", "ob", "mans", "auch", "pre\u00b7digt", "nennt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$,", "KOUS", "PIS", "ADV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Auch nicht der hohe ruhm der falschen wei\u00dfheits-d\u00fcnste:", "tokens": ["Auch", "nicht", "der", "ho\u00b7he", "ruhm", "der", "fal\u00b7schen", "wei\u00df\u00b7heits\u00b7d\u00fcns\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "ART", "ADJA", "NN", "ART", "ADJA", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Was reich und wei\u00df will seyn, wird nicht allhier erkennt.", "tokens": ["Was", "reich", "und", "wei\u00df", "will", "seyn", ",", "wird", "nicht", "all\u00b7hier", "er\u00b7kennt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "KON", "VVFIN", "VMFIN", "VAINF", "$,", "VAFIN", "PTKNEG", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Drum wi\u00dft, da\u00df ich so schwartz und d\u00fcrr und traurig sey", "tokens": ["Drum", "wi\u00dft", ",", "da\u00df", "ich", "so", "schwartz", "und", "d\u00fcrr", "und", "trau\u00b7rig", "sey"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "$,", "KOUS", "PPER", "ADV", "ADJD", "KON", "ADJD", "KON", "ADJD", "VAFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Ob eurer frechheit stoltz und groben heucheley,", "tokens": ["Ob", "eu\u00b7rer", "frech\u00b7heit", "stoltz", "und", "gro\u00b7ben", "heu\u00b7che\u00b7ley", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADJD", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Sonst k\u00f6nt ich wol so wei\u00df von au\u00df- als innen scheinen,", "tokens": ["Sonst", "k\u00f6nt", "ich", "wol", "so", "wei\u00df", "von", "au\u00df", "als", "in\u00b7nen", "schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "VVFIN", "APPR", "TRUNC", "KOKOM", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Wo nicht die trauer-zeit mich zw\u00fcng, euch zu beweinen.", "tokens": ["Wo", "nicht", "die", "trau\u00b7e\u00b7rzeit", "mich", "zw\u00fcng", ",", "euch", "zu", "be\u00b7wei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PTKNEG", "ART", "NN", "PPER", "VVFIN", "$,", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Doch \u00e4rgert euch nicht mehr an meiner kleinigkeit,", "tokens": ["Doch", "\u00e4r\u00b7gert", "euch", "nicht", "mehr", "an", "mei\u00b7ner", "klei\u00b7nig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Ihr sollt mich sch\u00f6n genug sehn bey der hochzeit-freud.", "tokens": ["Ihr", "sollt", "mich", "sch\u00f6n", "ge\u00b7nug", "sehn", "bey", "der", "hoch\u00b7zeit\u00b7freud", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "ADJD", "ADV", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+