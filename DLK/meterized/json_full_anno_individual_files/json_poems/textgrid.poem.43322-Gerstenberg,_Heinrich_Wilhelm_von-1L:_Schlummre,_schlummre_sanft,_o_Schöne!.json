{"textgrid.poem.43322": {"metadata": {"author": {"name": "Gerstenberg, Heinrich Wilhelm von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Schlummre, schlummre sanft, o Sch\u00f6ne!", "genre": "verse", "period": "N.A.", "pub_year": 1780, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schlummre, schlummre sanft, o Sch\u00f6ne!", "tokens": ["Schlumm\u00b7re", ",", "schlumm\u00b7re", "sanft", ",", "o", "Sch\u00f6\u00b7ne", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "ADJD", "$,", "FM", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "St\u00f6hrt sie nicht, der Nachtigallen T\u00f6ne!", "tokens": ["St\u00f6hrt", "sie", "nicht", ",", "der", "Nach\u00b7ti\u00b7gal\u00b7len", "T\u00f6\u00b7ne", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "$,", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Sterblich ist sie nicht: ach nein!", "tokens": ["Sterb\u00b7lich", "ist", "sie", "nicht", ":", "ach", "nein", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "PTKNEG", "$.", "ADV", "PTKANT", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Eine G\u00f6ttinn mu\u00df sie seyn.", "tokens": ["Ei\u00b7ne", "G\u00f6t\u00b7tinn", "mu\u00df", "sie", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "O ich will auf diesen Auen", "tokens": ["O", "ich", "will", "auf", "die\u00b7sen", "Au\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "PPER", "VMFIN", "APPR", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Gleich ihr einen Altar bauen;", "tokens": ["Gleich", "ihr", "ei\u00b7nen", "Al\u00b7tar", "bau\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Weihrauch will ich auf ihn streun:", "tokens": ["Weih\u00b7rauch", "will", "ich", "auf", "ihn", "streun", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "APPR", "PPER", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Ja! \u2013 sie kann nicht sterblich seyn.", "tokens": ["Ja", "!", "\u2013", "sie", "kann", "nicht", "sterb\u00b7lich", "seyn", "."], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "$(", "PPER", "VMFIN", "PTKNEG", "ADJD", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Aber wenn sie nun erwachet;", "tokens": ["A\u00b7ber", "wenn", "sie", "nun", "er\u00b7wa\u00b7chet", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Freundlich diese Wange lachet \u2013", "tokens": ["Freund\u00b7lich", "die\u00b7se", "Wan\u00b7ge", "la\u00b7chet", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PDAT", "NN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Armes Herz! wie wird dirs gehn!", "tokens": ["Ar\u00b7mes", "Herz", "!", "wie", "wird", "dirs", "gehn", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "PWAV", "VAFIN", "PIS", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "O wie schlummert sie so sch\u00f6