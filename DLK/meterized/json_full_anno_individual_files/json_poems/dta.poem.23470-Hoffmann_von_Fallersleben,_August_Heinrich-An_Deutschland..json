{"dta.poem.23470": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "An Deutschland.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Zerbrich das schwere Joch, darunter du gebunden!", "tokens": ["Zer\u00b7brich", "das", "schwe\u00b7re", "Joch", ",", "da\u00b7run\u00b7ter", "du", "ge\u00b7bun\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "ADJA", "NN", "$,", "PAV", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "O Deutschland, wach doch auf, fa\u00df wieder einen Muth!", "tokens": ["O", "Deutschland", ",", "wach", "doch", "auf", ",", "fa\u00df", "wie\u00b7der", "ei\u00b7nen", "Muth", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "VVFIN", "ADV", "PTKVZ", "$,", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Gebrauch dein altes Herz und widersteh der Wuth,", "tokens": ["Ge\u00b7brauch", "dein", "al\u00b7tes", "Herz", "und", "wi\u00b7der\u00b7steh", "der", "Wuth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die dich und die Freiheit durch dich selbst \u00fcberwunden!", "tokens": ["Die", "dich", "und", "die", "Frei\u00b7heit", "durch", "dich", "selbst", "\u00fc\u00b7berw\u00b7un\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "KON", "ART", "NN", "APPR", "PPER", "ADV", "VVPP", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.2": {"line.1": {"text": "Straf nu die Tyrannei, die dich schier gar geschunden,", "tokens": ["Straf", "nu", "die", "Ty\u00b7ran\u00b7nei", ",", "die", "dich", "schier", "gar", "ge\u00b7schun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "NN", "$,", "PRELS", "PRF", "ADJD", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+++-+-", "measure": "unknown.measure.septa"}, "line.2": {"text": "Und l\u00f6sch doch endlich aus die dich verzehrend Glut!", "tokens": ["Und", "l\u00f6sch", "doch", "end\u00b7lich", "aus", "die", "dich", "ver\u00b7zeh\u00b7rend", "Glut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "ADV", "APPR", "PRELS", "PPER", "VVPP", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nicht mit deim eignen Schwei\u00df, sondern dem b\u00f6sen Blut", "tokens": ["Nicht", "mit", "deim", "eig\u00b7nen", "Schwei\u00df", ",", "son\u00b7dern", "dem", "b\u00f6\u00b7sen", "Blut"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKNEG", "APPR", "ART", "ADJA", "NN", "$,", "KON", "ART", "ADJA", "NN"], "meter": "-+-+-+---+-+", "measure": "unknown.measure.penta"}, "line.4": {"text": "Flie\u00dfend aus deiner Feind und falschen Br\u00fcder Wunden.", "tokens": ["Flie\u00b7\u00dfend", "aus", "dei\u00b7ner", "Feind", "und", "fal\u00b7schen", "Br\u00fc\u00b7der", "Wun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "KON", "ADJA", "NN", "NN", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}}, "stanza.3": {"line.1": {"text": "Verlassend dich auf Gott, folg denen F\u00fcrsten nach,", "tokens": ["Ver\u00b7las\u00b7send", "dich", "auf", "Gott", ",", "folg", "de\u00b7nen", "F\u00fcrs\u00b7ten", "nach", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "PPER", "APPR", "NN", "$,", "VVFIN", "PDS", "NN", "PTKVZ", "$,"], "meter": "-+-+-+---+-+", "measure": "unknown.measure.penta"}, "line.2": {"text": "Die sein gerechte Hand will, so du willst, bewahren", "tokens": ["Die", "sein", "ge\u00b7rech\u00b7te", "Hand", "will", ",", "so", "du", "willst", ",", "be\u00b7wah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "VMFIN", "$,", "ADV", "PPER", "VMFIN", "$,", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Zu der Getreuen Trost, zu der Treulosen Rach.", "tokens": ["Zu", "der", "Ge\u00b7treu\u00b7en", "Trost", ",", "zu", "der", "Treu\u00b7lo\u00b7sen", "Rach", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "So la\u00df nu alle Furcht und nicht die Zeit hinfahren,", "tokens": ["So", "la\u00df", "nu", "al\u00b7le", "Furcht", "und", "nicht", "die", "Zeit", "hin\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "ADV", "PIAT", "NN", "KON", "PTKNEG", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und Gott wird aller Welt, da\u00df nichts dann Schand und", "tokens": ["Und", "Gott", "wird", "al\u00b7ler", "Welt", ",", "da\u00df", "nichts", "dann", "Schand", "und"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VAFIN", "PIAT", "NN", "$,", "KOUS", "PIS", "ADV", "NN", "KON"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Des Feinds Meineid und Stolz gezeuget, offenbaren.", "tokens": ["Des", "Feinds", "Mei\u00b7neid", "und", "Stolz", "ge\u00b7zeu\u00b7get", ",", "of\u00b7fen\u00b7ba\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "NN", "VVPP", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}