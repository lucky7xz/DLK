{"textgrid.poem.56232": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "Zur Antwort", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dir ist sonst der Mund verschlossen,", "tokens": ["Dir", "ist", "sonst", "der", "Mund", "ver\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Du antwortest mir ja kaum,", "tokens": ["Du", "ant\u00b7wor\u00b7test", "mir", "ja", "kaum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nur zu Liedern s\u00fc\u00dfen Klanges", "tokens": ["Nur", "zu", "Lie\u00b7dern", "s\u00fc\u00b7\u00dfen", "Klan\u00b7ges"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "\u00d6ffnest du ihn, wie im Traum.", "tokens": ["\u00d6ff\u00b7nest", "du", "ihn", ",", "wie", "im", "Traum", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "$,", "PWAV", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "K\u00f6nnt ich auch so dichten, w\u00fcrden", "tokens": ["K\u00f6nnt", "ich", "auch", "so", "dich\u00b7ten", ",", "w\u00fcr\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "ADJA", "$,", "VAFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "H\u00fcbsch auch meine Lieder sein,", "tokens": ["H\u00fcbsch", "auch", "mei\u00b7ne", "Lie\u0