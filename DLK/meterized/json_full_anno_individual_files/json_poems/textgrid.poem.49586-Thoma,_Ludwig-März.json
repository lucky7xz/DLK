{"textgrid.poem.49586": {"metadata": {"author": {"name": "Thoma, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "M\u00e4rz", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ah! Wie die buttergelbe Sonne", "tokens": ["Ah", "!", "Wie", "die", "but\u00b7ter\u00b7gel\u00b7be", "Son\u00b7ne"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "PWAV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Uns w\u00e4rmend durch die Poren dringt!", "tokens": ["Uns", "w\u00e4r\u00b7mend", "durch", "die", "Po\u00b7ren", "dringt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie neu erwachte Fr\u00fchlingswonne", "tokens": ["Wie", "neu", "er\u00b7wach\u00b7te", "Fr\u00fch\u00b7lings\u00b7won\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Uns das vergr\u00e4mte Herz beschwingt!", "tokens": ["Uns", "das", "ver\u00b7gr\u00e4m\u00b7te", "Herz", "be\u00b7schwingt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Dem winterm\u00fcden Menschentume", "tokens": ["Dem", "win\u00b7ter\u00b7m\u00fc\u00b7den", "Men\u00b7schen\u00b7tu\u00b7me"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Erheitert ihr die Phantasie,", "tokens": ["Er\u00b7hei\u00b7tert", "ihr", "die", "Phan\u00b7ta\u00b7sie", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Schneegl\u00f6ckchen, Veilchen, Schl\u00fcsselblume", "tokens": ["Schnee\u00b7gl\u00f6ck\u00b7chen", ",", "Veil\u00b7chen", ",", "Schl\u00fcs\u00b7sel\u00b7blu\u00b7me"], "token_info": ["word", "punct", "word", "punct", "word"], "pos": ["NN", "$,", "NN", "$,", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Und was auf Wiesen sonst gedieh!", "tokens": ["Und", "was", "auf", "Wie\u00b7sen", "sonst", "ge\u00b7dieh", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "APPR", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Im Mistbeet herrscht ein reges Leben;", "tokens": ["Im", "Mist\u00b7beet", "herrscht", "ein", "re\u00b7ges", "Le\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das dr\u00e4ngt sich an das helle Licht", "tokens": ["Das", "dr\u00e4ngt", "sich", "an", "das", "hel\u00b7le", "Licht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PRF", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und will uns bald Gem\u00fcse geben,", "tokens": ["Und", "will", "uns", "bald", "Ge\u00b7m\u00fc\u00b7se", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Will Zutat sein zum Leibgericht.", "tokens": ["Will", "Zu\u00b7tat", "sein", "zum", "Leib\u00b7ge\u00b7richt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "PPOSAT", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und wie sich froh den H\u00fchnerstei\u00dfen", "tokens": ["Und", "wie", "sich", "froh", "den", "H\u00fch\u00b7ner\u00b7stei\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PRF", "ADJD", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Entringt das liebe Osterei!", "tokens": ["Ent\u00b7ringt", "das", "lie\u00b7be", "Os\u00b7te\u00b7rei", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So mag sich die Natur beflei\u00dfen,", "tokens": ["So", "mag", "sich", "die", "Na\u00b7tur", "be\u00b7flei\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PRF", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df sie nebst sch\u00f6n auch schmackhaft sei.", "tokens": ["Da\u00df", "sie", "nebst", "sch\u00f6n", "auch", "schmack\u00b7haft", "sei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Das Starkbier regelt dann die St\u00fchle,", "tokens": ["Das", "Stark\u00b7bier", "re\u00b7gelt", "dann", "die", "St\u00fch\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn Hertling spricht, ist's ebenso,", "tokens": ["Wenn", "Hert\u00b7ling", "spricht", ",", "ist's", "e\u00b7ben\u00b7so", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "$,", "