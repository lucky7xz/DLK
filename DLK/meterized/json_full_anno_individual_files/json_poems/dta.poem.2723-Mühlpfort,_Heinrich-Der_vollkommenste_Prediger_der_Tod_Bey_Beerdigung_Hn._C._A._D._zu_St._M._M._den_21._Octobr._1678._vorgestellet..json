{"dta.poem.2723": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der vollkommenste Prediger der Tod/  \n  Bey Beerdigung Hn.   C. A. D.   zu St. M. M.  \n den 21. Octobr. 1678. vorgestellet.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "E Ntweich/ Ehrw\u00fcrdiger/ der Tod will itzt auftreten", "tokens": ["E", "Nt\u00b7weich", "/", "Ehr\u00b7w\u00fcr\u00b7di\u00b7ger", "/", "der", "Tod", "will", "itzt", "auf\u00b7tre\u00b7ten"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "$(", "NN", "$(", "ART", "NN", "VMFIN", "ADV", "VVINF"], "meter": "+-+-+-+-+--++-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Und allen Sterblichen die letzte Predigt thun.", "tokens": ["Und", "al\u00b7len", "Sterb\u00b7li\u00b7chen", "die", "letz\u00b7te", "Pre\u00b7digt", "thun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Du hast dein Ambt erf\u00fcllt mit Lehren/ Singen/ Beten;", "tokens": ["Du", "hast", "dein", "Ambt", "er\u00b7f\u00fcllt", "mit", "Leh\u00b7ren", "/", "Sin\u00b7gen", "/", "Be\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "VVPP", "APPR", "NN", "$(", "NN", "$(", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Man lasse doch den Leib/ die m\u00fcden Knochen/ ruhn.", "tokens": ["Man", "las\u00b7se", "doch", "den", "Leib", "/", "die", "m\u00fc\u00b7den", "Kno\u00b7chen", "/", "ruhn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "ART", "NN", "$(", "ART", "ADJA", "NN", "$(", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Arbeiter in dem Wort/ Hau\u00dfhalter derer G\u00fcter/", "tokens": ["Ar\u00b7bei\u00b7ter", "in", "dem", "Wort", "/", "Hau\u00df\u00b7hal\u00b7ter", "de\u00b7rer", "G\u00fc\u00b7ter", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$(", "NN", "PDS", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So uns in Ewigkeit aus ihren Sch\u00e4tzen weist/", "tokens": ["So", "uns", "in", "E\u00b7wig\u00b7keit", "aus", "ih\u00b7ren", "Sch\u00e4t\u00b7zen", "weist", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "APPR", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Du hast genug versorgt den Hunger der Gem\u00fcther/", "tokens": ["Du", "hast", "ge\u00b7nug", "ver\u00b7sorgt", "den", "Hun\u00b7ger", "der", "Ge\u00b7m\u00fc\u00b7ther", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVFIN", "ART", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und mit dem Lebens-Brod die D\u00fcrfftigen gespeist.", "tokens": ["Und", "mit", "dem", "Le\u00b7bens\u00b7Brod", "die", "D\u00fcr\u00b7ff\u00b7ti\u00b7gen", "ge\u00b7speist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Du Brunnen Jsraels/ der quell-reich ist geflossen/", "tokens": ["Du", "Brun\u00b7nen", "Js\u00b7raels", "/", "der", "quell\u00b7reich", "ist", "ge\u00b7flos\u00b7sen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "NE", "$(", "ART", "ADJD", "VAFIN", "VVPP", "$("], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Und in viel B\u00e4che sich durch Lehren hat zertheilt:", "tokens": ["Und", "in", "viel", "B\u00e4\u00b7che", "sich", "durch", "Leh\u00b7ren", "hat", "zer\u00b7theilt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIAT", "NN", "PRF", "APPR", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Du Regen/ der mit Trost ein mattes Hertz begossen/", "tokens": ["Du", "Re\u00b7gen", "/", "der", "mit", "Trost", "ein", "mat\u00b7tes", "Hertz", "be\u00b7gos\u00b7sen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$(", "ART", "APPR", "NN", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und offt verfaultes Fleisch vom S\u00fcnden-Brand geheilt:", "tokens": ["Und", "offt", "ver\u00b7faul\u00b7tes", "Fleisch", "vom", "S\u00fcn\u00b7den\u00b7Brand", "ge\u00b7heilt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJA", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Die Kirche wird nicht mehr von deiner Stimm erschallen/", "tokens": ["Die", "Kir\u00b7che", "wird", "nicht", "mehr", "von", "dei\u00b7ner", "Stimm", "er\u00b7schal\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKNEG", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Dein Donner schl\u00e4get nicht in Fels der Hertzen ein.", "tokens": ["Dein", "Don\u00b7ner", "schl\u00e4\u00b7get", "nicht", "in", "Fels", "der", "Hert\u00b7zen", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKNEG", "APPR", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Es wird kein Feur hinfort von deiner Zunge fallen/", "tokens": ["Es", "wird", "kein", "Feur", "hin\u00b7fort", "von", "dei\u00b7ner", "Zun\u00b7ge", "fal\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Die Worte werden nicht mit Blitz gefiedert seyn.", "tokens": ["Die", "Wor\u00b7te", "wer\u00b7den", "nicht", "mit", "Blitz", "ge\u00b7fie\u00b7dert", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKNEG", "APPR", "NN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Du bist/ als wie die Sonn\u2019/ in deinem Krei\u00df gelauffen.", "tokens": ["Du", "bist", "/", "als", "wie", "die", "Sonn'", "/", "in", "dei\u00b7nem", "Krei\u00df", "ge\u00b7lauf\u00b7fen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$(", "KOUS", "KOKOM", "ART", "NN", "$(", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Wie ihrer Fackel Licht bestrahlt die gantze Welt:", "tokens": ["Wie", "ih\u00b7rer", "Fa\u00b7ckel", "Licht", "be\u00b7strahlt", "die", "gant\u00b7ze", "Welt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "So scheinestu ein Stern in der Zuh\u00f6rer Hauffen/", "tokens": ["So", "schei\u00b7nes\u00b7tu", "ein", "Stern", "in", "der", "Zu\u00b7h\u00f6\u00b7rer", "Hauf\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Der Licht und Finstern\u00fc\u00df hat deutlich vorgestellt.", "tokens": ["Der", "Licht", "und", "Fins\u00b7ter\u00b7n\u00fc\u00df", "hat", "deut\u00b7lich", "vor\u00b7ge\u00b7stellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VAFIN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Der Sonne ", "tokens": ["Der", "Son\u00b7ne"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.22": {"text": "Wie hat dein Glaubens-Stral nicht manchen angebrant?", "tokens": ["Wie", "hat", "dein", "Glau\u00b7bens\u00b7S\u00b7tral", "nicht", "man\u00b7chen", "an\u00b7ge\u00b7brant", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPOSAT", "NN", "PTKNEG", "VVINF", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Denn Priester m\u00fcssen vor vom Himmel feurig werden/", "tokens": ["Denn", "Pries\u00b7ter", "m\u00fcs\u00b7sen", "vor", "vom", "Him\u00b7mel", "feu\u00b7rig", "wer\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VMFIN", "APPR", "APPRART", "NN", "ADJD", "VAINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Eh\u2019 die zertheilte Zung entdeckt/ wer sie gesand/", "tokens": ["Eh'", "die", "zer\u00b7theil\u00b7te", "Zung", "ent\u00b7deckt", "/", "wer", "sie", "ge\u00b7sand", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVPP", "$(", "PWS", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Sah\u2019 Jacob in dem Traum die Boten Gottes steigen/", "tokens": ["Sah'", "Ja\u00b7cob", "in", "dem", "Traum", "die", "Bo\u00b7ten", "Got\u00b7tes", "stei\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "APPR", "ART", "NN", "ART", "NN", "NN", "VVINF", "$("], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.26": {"text": "Und auff der Leiter ab zu ihm hernieder gehn/", "tokens": ["Und", "auff", "der", "Lei\u00b7ter", "ab", "zu", "ihm", "her\u00b7nie\u00b7der", "gehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "PTKVZ", "APPR", "PPER", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "So hat ihm di\u00df Gesicht ja satsam wollen zeigen!", "tokens": ["So", "hat", "ihm", "di\u00df", "Ge\u00b7sicht", "ja", "sat\u00b7sam", "wol\u00b7len", "zei\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PDS", "NN", "ADV", "ADJD", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Da\u00df Engel/ eh sie sich zu lehren unterstehn/", "tokens": ["Da\u00df", "En\u00b7gel", "/", "eh", "sie", "sich", "zu", "leh\u00b7ren", "un\u00b7ter\u00b7stehn", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "$(", "KOUS", "PPER", "PRF", "PTKZU", "VVINF", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Den Nachdruck und Befehl vom Himmel holen m\u00fcssen/", "tokens": ["Den", "Nach\u00b7druck", "und", "Be\u00b7fehl", "vom", "Him\u00b7mel", "ho\u00b7len", "m\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "APPRART", "NN", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Und vor das grosse Reich der Herrligkeit beschaun:", "tokens": ["Und", "vor", "das", "gros\u00b7se", "Reich", "der", "Herr\u00b7lig\u00b7keit", "be\u00b7schaun", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Denn k\u00f6nnen sie bey uns sich Wolcken-gleich ergiessen/", "tokens": ["Denn", "k\u00f6n\u00b7nen", "sie", "bey", "uns", "sich", "Wol\u00b7cken\u00b7gleich", "er\u00b7gies\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "APPR", "PPER", "PRF", "NE", "VVPP", "$("], "meter": "-+-++--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.32": {"text": "Und mit dem Perlen-Safft die Unter-Welt bethaun.", "tokens": ["Und", "mit", "dem", "Per\u00b7len\u00b7\u00b7S\u00b7afft", "die", "Un\u00b7ter\u00b7Welt", "be\u00b7thaun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "+--+--+-+-+-+", "measure": "dactylic.di.plus"}, "line.33": {"text": "Bist du nicht einem Berg/ ", "tokens": ["Bist", "du", "nicht", "ei\u00b7nem", "Berg", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "ART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.34": {"text": "Der stets von oben ab befeuchtet Grund und Thal?", "tokens": ["Der", "stets", "von", "o\u00b7ben", "ab", "be\u00b7feuch\u00b7tet", "Grund", "und", "Thal", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "ADV", "PTKVZ", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Es kan die Biene nicht so ihren Honig lesen/", "tokens": ["Es", "kan", "die", "Bie\u00b7ne", "nicht", "so", "ih\u00b7ren", "Ho\u00b7nig", "le\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "PTKNEG", "ADV", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Als du den Kern der Schrifft durchsucht durch kluge Wahl.", "tokens": ["Als", "du", "den", "Kern", "der", "Schrifft", "durch\u00b7sucht", "durch", "klu\u00b7ge", "Wahl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Und kam es wiederumb/ die Laster zuerschrecken/", "tokens": ["Und", "kam", "es", "wie\u00b7de\u00b7rumb", "/", "die", "Las\u00b7ter", "zu\u00b7er\u00b7schre\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "$(", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Erschienstu deinem Volck nicht als ein feurig L\u00f6w?", "tokens": ["Er\u00b7schiens\u00b7tu", "dei\u00b7nem", "Volck", "nicht", "als", "ein", "feu\u00b7rig", "L\u00f6w", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "PTKNEG", "KOUS", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Du lie\u00dft kein r\u00e4udig Schaf in deiner Heerde stecken/", "tokens": ["Du", "lie\u00dft", "kein", "r\u00e4u\u00b7dig", "Schaf", "in", "dei\u00b7ner", "Heer\u00b7de", "ste\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "ADJD", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Weil die Erfahrung lehrt/ da\u00df eines sch\u00e4dlich sey.", "tokens": ["Weil", "die", "Er\u00b7fah\u00b7rung", "lehrt", "/", "da\u00df", "ei\u00b7nes", "sch\u00e4d\u00b7lich", "sey", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$(", "KOUS", "ART", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Ach Laute/ die so wohl von Gottes Hand gestimmet/", "tokens": ["Ach", "Lau\u00b7te", "/", "die", "so", "wohl", "von", "Got\u00b7tes", "Hand", "ge\u00b7stim\u00b7met", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$(", "ART", "ADV", "ADV", "APPR", "NN", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Mehr als des Orpheus Klang die Menschen zu sich rieff!", "tokens": ["Mehr", "als", "des", "Or\u00b7pheus", "Klan