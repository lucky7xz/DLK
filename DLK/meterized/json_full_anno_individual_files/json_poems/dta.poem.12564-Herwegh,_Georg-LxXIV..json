{"dta.poem.12564": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "LxXIV.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1843", "urn": "urn:nbn:de:kobv:b4-200905192440", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Auf dem Wege von Damasko", "tokens": ["Auf", "dem", "We\u00b7ge", "von", "Da\u00b7mas\u00b7ko"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "NE"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Machte Saulus einst ", "tokens": ["Mach\u00b7te", "Sau\u00b7lus", "einst"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "NE", "ADV"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Doch, das ihn bekehrt, das Licht,", "tokens": ["Doch", ",", "das", "ihn", "be\u00b7kehrt", ",", "das", "Licht", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "$,", "PRELS", "PPER", "VVFIN", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "\u2014 Ein Berliner war es nicht!", "tokens": ["Ein", "Ber\u00b7li\u00b7ner", "war", "es", "nicht", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADJA", "VAFIN", "PPER", "PTKNEG", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}}}}