{"dta.poem.18654": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "Talismane,  \n Amulete, Abraxas, Inschriften  \n und Siegel .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Gottes ist der Orient!", "tokens": ["Got\u00b7tes", "ist", "der", "O\u00b7rient", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Gottes ist der Occident!", "tokens": ["Got\u00b7tes", "ist", "der", "Oc\u00b7ci\u00b7dent", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nord- und s\u00fcdliches Gel\u00e4nde", "tokens": ["Nord", "und", "s\u00fcd\u00b7li\u00b7ches", "Ge\u00b7l\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["TRUNC", "KON", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ruht im Frieden seiner H\u00e4nde.", "tokens": ["Ruht", "im", "Frie\u00b7den", "sei\u00b7ner", "H\u00e4n\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Er, der einzige Gerechte,", "tokens": ["Er", ",", "der", "ein\u00b7zi\u00b7ge", "Ge\u00b7rech\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Will f\u00fcr jedermann das Rechte.", "tokens": ["Will", "f\u00fcr", "je\u00b7der\u00b7mann", "das", "Rech\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "PIS", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sey, von seinen hundert Namen,", "tokens": ["Sey", ",", "von", "sei\u00b7nen", "hun\u00b7dert", "Na\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "APPR", "PPOSAT", "CARD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dieser hochgelobet! Amen.", "tokens": ["Die\u00b7ser", "hoch\u00b7ge\u00b7lo\u00b7bet", "!", "A\u00b7men", "."], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PDS", "VVFIN", "$.", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Mich verwirren will das Irren;", "tokens": ["Mich", "ver\u00b7wir\u00b7ren", "will", "das", "Ir\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "VMFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Doch du weisst mich zu entwirren.", "tokens": ["Doch", "du", "weisst", "mich", "zu", "ent\u00b7wir\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn ich handle, wenn ich dichte", "tokens": ["Wenn", "ich", "hand\u00b7le", ",", "wenn", "ich", "dich\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "KOUS", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gieb du meinem Weg die Richte.", "tokens": ["Gieb", "du", "mei\u00b7nem", "Weg", "die", "Rich\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Ob ich Ird\u2019sches denk\u2019 und sinne,", "tokens": ["Ob", "ich", "Ird'\u00b7sches", "denk'", "und", "sin\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Das gereicht zu h\u00f6herem Gewinne.", "tokens": ["Das", "ge\u00b7reicht", "zu", "h\u00f6\u00b7he\u00b7rem", "Ge\u00b7win\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVPP", "PTKZU", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Mit dem Staube nicht der Geist zerstoben", "tokens": ["Mit", "dem", "Stau\u00b7be", "nicht", "der", "Geist", "zer\u00b7sto\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PTKNEG", "ART", "NN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Dringet, in sich selbst gedr\u00e4ngt, nach oben.", "tokens": ["Drin\u00b7get", ",", "in", "sich", "selbst", "ge\u00b7dr\u00e4ngt", ",", "nach", "o\u00b7ben", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "APPR", "PRF", "ADV", "VVPP", "$,", "APPR", "ADV", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Im Athemholen sind zweyerley Gnaden:", "tokens": ["Im", "A\u00b7them\u00b7ho\u00b7len", "sind", "zwey\u00b7er\u00b7ley", "Gna\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PIAT", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die Luft einziehn, sich ihrer entladen.", "tokens": ["Die", "Luft", "ein\u00b7ziehn", ",", "sich", "ih\u00b7rer", "ent\u00b7la\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "PRF", "PPOSAT", "VVPP", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Jenes bedr\u00e4ngt, dieses erfrischt;", "tokens": ["Je\u00b7nes", "be\u00b7dr\u00e4ngt", ",", "die\u00b7ses", "er\u00b7frischt", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "PDS", "VVFIN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "So wunderbar ist das Leben gemischt.", "tokens": ["So", "wun\u00b7der\u00b7bar", "ist", "das", "Le\u00b7ben", "ge\u00b7mischt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Du danke Gott, wenn er dich presst,", "tokens": ["Du", "dan\u00b7ke", "Gott", ",", "wenn", "er", "dich", "presst", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,", "KOUS", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und dank\u2019 ihm, wenn er dich wieder entl\u00e4sst.", "tokens": ["Und", "dank'", "ihm", ",", "wenn", "er", "dich", "wie\u00b7der", "ent\u00b7l\u00e4sst", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "KOUS", "PPER", "PRF", "ADV", "VVFIN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}}}}