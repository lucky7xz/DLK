{"textgrid.poem.33740": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Altenglands frohe Zeit nach blut'gen Schlachten", "tokens": ["Al\u00b7ten\u00b7glands", "fro\u00b7he", "Zeit", "nach", "blut'\u00b7gen", "Schlach\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADJA", "NN", "APPR", "ADJA", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Taucht lachend vor mir auf, die feine Sitte", "tokens": ["Taucht", "la\u00b7chend", "vor", "mir", "auf", ",", "die", "fei\u00b7ne", "Sit\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "APPR", "PPER", "PTKVZ", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Bringt bunte Wortspiel' auf, und wie Granite", "tokens": ["Bringt", "bun\u00b7te", "Wort\u00b7spiel'", "auf", ",", "und", "wie", "Gra\u00b7ni\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ADJA", "NN", "PTKVZ", "$,", "KON", "PWAV", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Gedanken aus den tiefsten Geistesschachten.", "tokens": ["Ge\u00b7dan\u00b7ken", "aus", "den", "tiefs\u00b7ten", "Geis\u00b7tes\u00b7schach\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Noch schmettert, w\u00e4hrend Maskenz\u00fcge lachten,", "tokens": ["Noch", "schmet\u00b7tert", ",", "w\u00e4h\u00b7rend", "Mas\u00b7ken\u00b7z\u00fc\u00b7ge", "lach\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "KOUS", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Trompetenschall, noch gilt es k\u00fchne Ritte,", "tokens": ["Trom\u00b7pe\u00b7ten\u00b7schall", ",", "noch", "gilt", "es", "k\u00fch\u00b7ne", "Rit\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und k\u00fchne Meerfahrt lockt; mit ernstem Schritte", "tokens": ["Und", "k\u00fch\u00b7ne", "Meer\u00b7fahrt", "lockt", ";", "mit", "erns\u00b7tem", "Schrit\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "VVFIN", "$.", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Reckt sich die Vorzeit in das neue Trachten.", "tokens": ["Reckt", "sich", "die", "Vor\u00b7zeit", "in", "das", "neu\u00b7e", "Trach\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Um einen Dichter schwebt vom Meeresstrande", "tokens": ["Um", "ei\u00b7nen", "Dich\u00b7ter", "schwebt", "vom", "Mee\u00b7res\u00b7stran\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "ART", "NN", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Elfenschwarm und zaubert seinen Tr\u00e4umen", "tokens": ["Der", "El\u00b7fen\u00b7schwarm", "und", "zau\u00b7bert", "sei\u00b7nen", "Tr\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambi