{"dta.poem.20100": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Cromwell\u2019s letzte Nacht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1851", "urn": "urn:nbn:de:kobv:b4-200905191321", "language": ["de:0.99"], "booktitle": "Fontane, Theodor: Gedichte. Berlin, 1851."}, "poem": {"stanza.1": {"line.1": {"text": "Mir sagt\u2019s nicht nur des Arztes ernste Miene,", "tokens": ["Mir", "sagt's", "nicht", "nur", "des", "Arz\u00b7tes", "erns\u00b7te", "Mie\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Selbst f\u00fchl\u2019 ich\u2019s, meine Stunden sind gez\u00e4hlt.", "tokens": ["Selbst", "f\u00fchl'", "ich's", ",", "mei\u00b7ne", "Stun\u00b7den", "sind", "ge\u00b7z\u00e4hlt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "$,", "PPOSAT", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Der tolle Traum, der mich vom Lager schreckte,", "tokens": ["Der", "tol\u00b7le", "Traum", ",", "der", "mich", "vom", "La\u00b7ger", "schreck\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Er war nicht Ausgeburt des hei\u00dfen Hirns,", "tokens": ["Er", "war", "nicht", "Aus\u00b7ge\u00b7burt", "des", "hei\u00b7\u00dfen", "Hirns", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Auch Stimme nicht des mahnenden Gewissens,", "tokens": ["Auch", "Stim\u00b7me", "nicht", "des", "mah\u00b7nen\u00b7den", "Ge\u00b7wis\u00b7sens", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Er war ein Ruf aus einer andren Welt", "tokens": ["Er", "war", "ein", "Ruf", "aus", "ei\u00b7ner", "an\u00b7dren", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Zum Hintritt vor den Richter mich zu r\u00fcsten.", "tokens": ["Zum", "Hin\u00b7tritt", "vor", "den", "Rich\u00b7ter", "mich", "zu", "r\u00fcs\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ein toller Traum! w\u00fc\u00dft ich, in n\u00e4chster Nacht", "tokens": ["Ein", "tol\u00b7ler", "Traum", "!", "w\u00fc\u00dft", "ich", ",", "in", "n\u00e4chs\u00b7ter", "Nacht"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "VVFIN", "PPER", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wird dir der Schlaf ein gleiches Schreckni\u00df", "tokens": ["Wird", "dir", "der", "Schlaf", "ein", "glei\u00b7ches", "Schrec\u00b7kni\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So m\u00f6chte diese Stunde noch der Tod", "tokens": ["So", "m\u00f6ch\u00b7te", "die\u00b7se", "Stun\u00b7de", "noch", "der", "Tod"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PDAT", "NN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Statt jenes Stuart an mein Lager treten.", "tokens": ["Statt", "je\u00b7nes", "Stu\u00b7art", "an", "mein", "La\u00b7ger", "tre\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PDAT", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ernst stand er vor mir; um den nackten Hals", "tokens": ["Ernst", "stand", "er", "vor", "mir", ";", "um", "den", "nack\u00b7ten", "Hals"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "APPR", "PPER", "$.", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Trug, statt des Schmucks, er einen rothen Streifen,", "tokens": ["Trug", ",", "statt", "des", "Schmucks", ",", "er", "ei\u00b7nen", "ro\u00b7then", "Strei\u00b7fen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUI", "ART", "NN", "$,", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und als er, wie vordem, zu leichtem Gru\u00df", "tokens": ["Und", "als", "er", ",", "wie", "vor\u00b7dem", ",", "zu", "leich\u00b7tem", "Gru\u00df"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "$,", "PWAV", "ADV", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Nach dem Barett auf seinem Haupte fasste,", "tokens": ["Nach", "dem", "Ba\u00b7rett", "auf", "sei\u00b7nem", "Haup\u00b7te", "fass\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Nahm er den Kopf von seinem blutgen Rumpf,", "tokens": ["Nahm", "er", "den", "Kopf", "von", "sei\u00b7nem", "blut\u00b7gen", "Rumpf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Mein Auge schlo\u00df sich; als ich\u2019s scheu ge\u00f6ffnet", "tokens": ["Mein", "Au\u00b7ge", "schlo\u00df", "sich", ";", "als", "ich's", "scheu", "ge\u00b7\u00f6ff\u00b7net"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "$.", "KOUS", "PIS", "ADJD", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Sah wieder ich den purpurfarbnen Streifen,", "tokens": ["Sah", "wie\u00b7der", "ich", "den", "pur\u00b7purf\u00b7arb\u00b7nen", "Strei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Er winkte mit dem Finger mir, zu folgen,", "tokens": ["Er", "wink\u00b7te", "mit", "dem", "Fin\u00b7ger", "mir", ",", "zu", "fol\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "PPER", "$,", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Und schwand dann, r\u00fcckw\u00e4rts schreitend, in der", "tokens": ["Und", "schwand", "dann", ",", "r\u00fcck\u00b7w\u00e4rts", "schrei\u00b7tend", ",", "in", "der"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "$,", "ADV", "ADJD", "$,", "APPR", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Was schreckt das Traumbild mich des todten", "tokens": ["Was", "schreckt", "das", "Traum\u00b7bild", "mich", "des", "tod\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ART", "NN", "PRF", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und weckt in mir den alten Aberglauben", "tokens": ["Und", "weckt", "in", "mir", "den", "al\u00b7ten", "A\u00b7berg\u00b7lau\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "An eines K\u00f6nigs Unverletzlichkeit?", "tokens": ["An", "ei\u00b7nes", "K\u00f6\u00b7nigs", "Un\u00b7ver\u00b7letz\u00b7lich\u00b7keit", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Das Schwert des Henkers w\u00e4r\u2019 wie Glas zer-", "tokens": ["Das", "Schwert", "des", "Hen\u00b7kers", "w\u00e4r'", "wie", "Glas", "zer"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "KOKOM", "NN", "TRUNC"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wenn Gottes Will\u2019 ihn unverletzlich schuf.", "tokens": ["Wenn", "Got\u00b7tes", "Will'", "ihn", "un\u00b7ver\u00b7letz\u00b7lich", "schuf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VMFIN", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Was ist die Unantastbarkeit des K\u00f6nigs?", "tokens": ["Was", "ist", "die", "Un\u00b7an\u00b7tast\u00b7bar\u00b7keit", "des", "K\u00f6\u00b7nigs", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Nichts als ein Vorrecht, das die Zeit ihm leiht:", "tokens": ["Nichts", "als", "ein", "Vor\u00b7recht", ",", "das", "die", "Zeit", "ihm", "leiht", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "KOKOM", "ART", "NN", "$,", "PRELS", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Sein Urahn, ein Eroberer und M\u00f6rder", "tokens": ["Sein", "Ur\u00b7ahn", ",", "ein", "Er\u00b7o\u00b7be\u00b7rer", "und", "M\u00f6r\u00b7der"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "ART", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Ist der Begr\u00fcnder all der Heiligkeit.", "tokens": ["Ist", "der", "Be\u00b7gr\u00fcn\u00b7der", "all", "der", "Hei\u00b7lig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PIAT", "ART", "NN", "$."], "meter": "++-+-+-+-+", "measure": "iambic.penta.spondeus"}, "line.10": {"text": "Der k\u00fchne Normann, der bei Hastingsfield", "tokens": ["Der", "k\u00fch\u00b7ne", "Nor\u00b7mann", ",", "der", "bei", "Has\u00b7tings\u00b7field"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Den K\u00f6nig Harald in den Staub geworfen,", "tokens": ["Den", "K\u00f6\u00b7nig", "Ha\u00b7rald", "in", "den", "Staub", "ge\u00b7wor\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Was war er Bessres als der Cromwell heut,", "tokens": ["Was", "war", "er", "Bess\u00b7res", "als", "der", "Crom\u00b7well", "heut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "PIS", "KOKOM", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Der jenen Carl bei Marston-Moor geschlagen?", "tokens": ["Der", "je\u00b7nen", "Carl", "bei", "Mar\u00b7ston\u00b7Moor", "ge\u00b7schla\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PDAT", "NE", "APPR", "NE", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Es soll nicht mehr sein blutig Haupt mich", "tokens": ["Es", "soll", "nicht", "mehr", "sein", "blu\u00b7tig", "Haupt", "mich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PTKNEG", "ADV", "PPOSAT", "ADJD", "NN", "PPER"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Es lebt in mir: ich war ein Gotteswerkzeug,", "tokens": ["Es", "lebt", "in", "mir", ":", "ich", "war", "ein", "Got\u00b7tes\u00b7werk\u00b7zeug", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "$.", "PPER", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und auserw\u00e4hlt zu retten und zu strafen.", "tokens": ["Und", "au\u00b7ser\u00b7w\u00e4hlt", "zu", "ret\u00b7ten", "und", "zu", "stra\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ich sah das Schiff, vom Sturm umhergeschlagen,", "tokens": ["Ich", "sah", "das", "Schiff", ",", "vom", "Sturm", "um\u00b7her\u00b7ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Der Klippe nah, dran es zerschellen mu\u00dfte:", "tokens": ["Der", "Klip\u00b7pe", "nah", ",", "dran", "es", "zer\u00b7schel\u00b7len", "mu\u00df\u00b7te", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,", "PAV", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Ich sprang hinzu, von seinem Platze dr\u00e4ngt ich", "tokens": ["Ich", "sprang", "hin\u00b7zu", ",", "von", "sei\u00b7nem", "Plat\u00b7ze", "dr\u00e4ngt", "ich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "APPR", "PPOSAT", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Den schwachen Steurer, und mit sichrer Hand", "tokens": ["Den", "schwa\u00b7chen", "Steu\u00b7rer", ",", "und", "mit", "sich\u00b7rer", "Hand"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "KON", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Lenkt ich das Schiff, als Lootse, in den Hafen.", "tokens": ["Lenkt", "ich", "das", "Schiff", ",", "als", "Loot\u00b7se", ",", "in", "den", "Ha\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "KOUS", "NN", "$,", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Es war noch immer, galt\u2019s ein Volk zu retten,", "tokens": ["Es", "war", "noch", "im\u00b7mer", ",", "galt's", "ein", "Volk", "zu", "ret\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "$,", "VVFIN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Das Recht des St\u00e4rkern nicht das schlechtste", "tokens": ["Das", "Recht", "des", "St\u00e4r\u00b7kern", "nicht", "das", "schlechts\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "PTKNEG", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Da\u00df ich mein Thun mit seinem Tod besiegelt,", "tokens": ["Da\u00df", "ich", "mein", "Thun", "mit", "sei\u00b7nem", "Tod", "be\u00b7sie\u00b7gelt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Es war Nothwendigkeit; er mu\u00dfte sterben,", "tokens": ["Es", "war", "Noth\u00b7wen\u00b7dig\u00b7keit", ";", "er", "mu\u00df\u00b7te", "ster\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "$.", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es war sein Blut der M\u00f6rtel meines Bau\u2019s.", "tokens": ["Es", "war", "sein", "Blut", "der", "M\u00f6r\u00b7tel", "mei\u00b7nes", "Bau'", "s."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "ART", "NN", "PPOSAT", "NN", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Wenn in die Sendung, die an mich