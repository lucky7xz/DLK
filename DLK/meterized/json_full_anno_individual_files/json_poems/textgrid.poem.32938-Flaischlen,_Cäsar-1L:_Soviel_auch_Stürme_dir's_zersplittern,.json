{"textgrid.poem.32938": {"metadata": {"author": {"name": "Flaischlen, C\u00e4sar", "birth": "N.A.", "death": "N.A."}, "title": "1L: Soviel auch St\u00fcrme dir's zersplittern,", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Soviel auch St\u00fcrme dir's zersplittern,", "tokens": ["So\u00b7viel", "auch", "St\u00fcr\u00b7me", "dir's", "zer\u00b7split\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "NN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "hoch halte, freudig, ohne Zittern,", "tokens": ["hoch", "hal\u00b7te", ",", "freu\u00b7dig", ",", "oh\u00b7ne", "Zit\u00b7tern", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$,", "ADJD", "$,", "KOUI", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "das stolze Banner deiner Kunst", "tokens": ["das", "stol\u00b7ze", "Ban\u00b7ner", "dei\u00b7ner", "Kunst"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "und fordere furchtlos ohne Wanken", "tokens": ["und", "for\u00b7de\u00b7re", "furcht\u00b7los", "oh\u00b7ne", "Wan\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "APPR", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "kampffroh dein Schicksal in die Schranken,", "tokens": ["kampf\u00b7froh", "dein", "Schick\u00b7sal", "in", "die", "Schran\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "ein Feigling nur erbuhlt sich Gunst.", "tokens": ["ein", "Feig\u00b7ling", "nur", "er\u00b7buhlt", "sich", "Gunst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "PRF", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wohl mag's ja sch\u00f6n sein: ohne Gr\u00e4men", "tokens": ["Wohl", "mag's", "ja", "sch\u00f6n", "sein", ":", "oh\u00b7ne", "Gr\u00e4\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VMFIN", "ADV", "ADJD", "VAINF", "$.", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "das Leben, wie sich's gibt, zu nehmen,", "tokens": ["das", "Le\u00b7ben", ",", "wie", "sich's", "gibt", ",", "zu", "neh\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "PIS", "VVFIN", "$,", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "rasch zu genie\u00dfen, eh's verrinnt,", "tokens": ["rasch", "zu", "ge\u00b7nie\u00b7\u00dfen", ",", "eh's", "ver\u00b7rinnt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "PTKZU", "VVINF", "$,", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "und seelenruhig abzuwarten,", "tokens": ["und", "see\u00b7len\u00b7ru\u00b7hig", "ab\u00b7zu\u00b7war\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVIZU", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "ob Gl\u00fcck, ob Ungl\u00fcck mischt die Karten,", "tokens": ["ob", "Gl\u00fcck", ",", "ob", "Un\u00b7gl\u00fcck", "mischt", "die", "Kar\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "KOUS", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "ob man verliert, ob man gewinnt!", "tokens": ["ob", "man", "ver\u00b7liert", ",", "ob", "man", "ge\u00b7winnt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "$,", "KOUS", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Doch gr\u00f6\u00dfer ist: sich aufzuraffen", "tokens": ["Doch", "gr\u00f6\u00b7\u00dfer", "ist", ":", "sich", "auf\u00b7zu\u00b7raf\u00b7fen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADJD", "VAFIN", "$.", "PRF", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "und selber sein Geschick zu schaffen", "tokens": ["und", "sel\u00b7ber", "sein", "Ge\u00b7schick", "zu", "schaf\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "PPOSAT", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "mit kampf- und trotzgemuter Kraft", "tokens": ["mit", "kampf", "und", "trotz\u00b7ge\u00b7mu\u00b7ter", "Kraft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "TRUNC", "KON", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "und sich mit ungebrochenen Schwingen", "tokens": ["und", "sich", "mit", "un\u00b7ge\u00b7bro\u00b7che\u00b7nen", "Schwin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PRF", "APPR", "ADJA", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "den Niederungen zu entringen", "tokens": ["den", "Nie\u00b7de\u00b7run\u00b7gen", "zu", "ent\u00b7rin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "und ihres Werktags dumpfer Haft.", "tokens": ["und", "ih\u00b7res", "Werk\u00b7tags", "dum\u00b7pfer", "Haft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und ist auch mancher Flug vergebens,", "tokens": ["Und", "ist", "auch", "man\u00b7cher", "Flug", "ver\u00b7ge\u00b7bens", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "PIAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "du doch bist Herr dann deines Lebens", "tokens": ["du", "doch", "bist", "Herr", "dann", "dei\u00b7nes", "Le\u00b7bens"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VAFIN", "NN", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "und nicht ein wetterlaunisch Gl\u00fcck,", "tokens": ["und", "nicht", "ein", "wet\u00b7ter\u00b7lau\u00b7nisch", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "du in den H\u00e4nden h\u00e4ltst die Z\u00fcgel", "tokens": ["du", "in", "den", "H\u00e4n\u00b7den", "h\u00e4ltst", "die", "Z\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "ART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "und gibst ihm Unterschrift und Siegel,", "tokens": ["und", "gibst", "ihm", "Un\u00b7ter\u00b7schrift", "und", "Sie\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "und nicht ein zufallblind Geschick.", "tokens": ["und", "nicht", "ein", "zu\u00b7fall\u00b7blind", "Ge\u00b7schick", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ART", "ADJD", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.5": {"line.1": {"text": "Und nennen Sp\u00f6tter drob dich Schw\u00e4rmer,", "tokens": ["Und", "nen\u00b7nen", "Sp\u00f6t\u00b7ter", "drob", "dich", "Schw\u00e4r\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "was liegt daran! sie sind doch \u00e4rmer", "tokens": ["was", "liegt", "da\u00b7ran", "!", "sie", "sind", "doch", "\u00e4r\u00b7mer"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PAV", "$.", "PPER", "VAFIN", "ADV", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "als du, trotz Geld und Gold und Glanz,", "tokens": ["als", "du", ",", "trotz", "Geld", "und", "Gold", "und", "Glanz", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "APPR", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "und ob sie alles sich erf\u00fcllen,", "tokens": ["und", "ob", "sie", "al\u00b7les", "sich", "er\u00b7f\u00fcl\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PIS", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "es wird sich ihnen nie enth\u00fcllen,", "tokens": ["es", "wird", "sich", "ih\u00b7nen", "nie", "ent\u00b7h\u00fcl\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PRF", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "wie sch\u00f6n ein selbsterrungener Kranz.", "tokens": ["wie", "sch\u00f6n", "ein", "selbs\u00b7ter\u00b7run\u00b7ge\u00b7ner", "Kranz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.6": {"line.1": {"text": "Sie f\u00fchlen nie, durch Ebnen schreitend,", "tokens": ["Sie", "f\u00fch\u00b7len", "nie", ",", "durch", "Eb\u00b7nen", "schrei\u00b7tend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "im gro\u00dfen Tro\u00df ihr Leben reitend,", "tokens": ["im", "gro\u00b7\u00dfen", "Tro\u00df", "ihr", "Le\u00b7ben", "rei\u00b7tend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "wie froh sich's rastet im Gebirg,", "tokens": ["wie", "froh", "sich's", "ras\u00b7tet", "im", "Ge\u00b7birg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PIS", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "der Sonne nahe und tief unten", "tokens": ["der", "Son\u00b7ne", "na\u00b7he", "und", "tief", "un\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PTKVZ", "KON", "ADJD", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "zur\u00fcckgek\u00e4mpft und \u00fcberwunden", "tokens": ["zu\u00b7r\u00fcck\u00b7ge\u00b7k\u00e4mpft", "und", "\u00fc\u00b7berw\u00b7un\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["VVPP", "KON", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "des Alltags dunstiger Bezirk.", "tokens": ["des", "All\u00b7tags", "duns\u00b7ti\u00b7ger", "Be\u00b7zirk", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Nicht bl\u00f6der Diener bl\u00f6der G\u00f6tzen ...", "tokens": ["Nicht", "bl\u00f6\u00b7der", "Die\u00b7ner", "bl\u00f6\u00b7der", "G\u00f6t\u00b7zen", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJA", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "sei stolz, Freund, und zertritt die Fetzen,", "tokens": ["sei", "stolz", ",", "Freund", ",", "und", "zer\u00b7tritt", "die", "Fet\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$,", "NN", "$,", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "mit denen Leere sich versch\u00f6nt!", "tokens": ["mit", "de\u00b7nen", "Lee\u00b7re", "sich", "ver\u00b7sch\u00f6nt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PIS", "PRF", "VVPP", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Und solltest du im Kampf erliegen,", "tokens": ["Und", "soll\u00b7test", "du", "im", "Kampf", "er\u00b7lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "was du gewollt, wird dennoch siegen ...", "tokens": ["was", "du", "ge\u00b7wollt", ",", "wird", "den\u00b7noch", "sie\u00b7gen", "..."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VMPP", "$,", "VAFIN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6"