{"dta.poem.853": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "Xl.  Auff den Sontag des suchenden Hirtens/  \n oder 3. Sontag nach der H. Dreyeinigkeit  \n Luc. 15.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Der Engelscharen F\u00fcrst/ den Gott jhmb gleich gebohren. ", "tokens": ["Der", "En\u00b7gel\u00b7scha\u00b7ren", "F\u00fcrst", "/", "den", "Gott", "jhmb", "gleich", "ge\u00b7boh\u00b7ren", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$(", "ART", "NN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Durch den das weite Schlo\u00df der wunder sch\u00f6nen", "tokens": ["Durch", "den", "das", "wei\u00b7te", "Schlo\u00df", "der", "wun\u00b7der", "sch\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ART", "ADJA", "NN", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Welt ", "tokens": ["Welt"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Gegr\u00fcndet/ steigt vom Thron vnd seiner Himmel zelt", "tokens": ["Ge\u00b7gr\u00fcn\u00b7det", "/", "steigt", "vom", "Thron", "vnd", "sei\u00b7ner", "Him\u00b7mel", "zelt"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "$(", "VVFIN", "APPRART", "NN", "KON", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Vnd suchet was sich selbst auff Erden hat verlohren.", "tokens": ["Vnd", "su\u00b7chet", "was", "sich", "selbst", "auff", "Er\u00b7den", "hat", "ver\u00b7loh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PWS", "PRF", "ADV", "APPR", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Der K\u00f6nig den zur Lust der Vater jhm erkohren:", "tokens": ["Der", "K\u00f6\u00b7nig", "den", "zur", "Lust", "der", "Va\u00b7ter", "jhm", "er\u00b7koh\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "APPRART", "NN", "ART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hat seine lust an vns/ der alles gibt vnd h\u00e4lt", "tokens": ["Hat", "sei\u00b7ne", "lust", "an", "vns", "/", "der", "al\u00b7les", "gibt", "vnd", "h\u00e4lt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "NN", "APPR", "PPER", "$(", "ART", "PIS", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Kreucht seinen Schaffen nach vnd wird das L\u00f6segelt", "tokens": ["Kreucht", "sei\u00b7nen", "Schaf\u00b7fen", "nach", "vnd", "wird", "das", "L\u00f6\u00b7se\u00b7gelt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "NN", "APPR", "KON", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der auff die angst vnd todt/ vnd H\u00f6lle sich verschworen.", "tokens": ["Der", "auff", "die", "angst", "vnd", "todt", "/", "vnd", "H\u00f6l\u00b7le", "sich", "ver\u00b7schwo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "KON", "ADJD", "$(", "KON", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "H\u00f6rt Schaffe die jhr steckt verjrr\u2019t in mancher klufft:", "tokens": ["H\u00f6rt", "Schaf\u00b7fe", "die", "jhr", "steckt", "ver\u00b7jrr't", "in", "man\u00b7cher", "klufft", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "ART", "PPER", "VVFIN", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die ewig trewe Trew/ der Lebens Hirte rufft", "tokens": ["Die", "e\u00b7wig", "tre\u00b7we", "Trew", "/", "der", "Le\u00b7bens", "Hir\u00b7te", "rufft"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "ADJA", "NN", "$(", "ART", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Folgt seiner Stim\u2019 vnd Hand eh\u2019 euch der Wolff zureisse.", "tokens": ["Folgt", "sei\u00b7ner", "Stim'", "vnd", "Hand", "eh'", "euch", "der", "Wolff", "zu\u00b7reis\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "KON", "NN", "KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Welch Groschen jtzt nicht klingt/ wenn ", "tokens": ["Welch", "Gro\u00b7schen", "jtzt", "nicht", "klingt", "/", "wenn"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["PIAT", "NN", "ADV", "PTKNEG", "VVFIN", "$(", "KOUS"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "kehrt ", "tokens": ["kehrt"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Wird vnter dickem staub von scharffen rost verzehrt", "tokens": ["Wird", "vn\u00b7ter", "di\u00b7ckem", "staub", "von", "scharf\u00b7fen", "rost", "ver\u00b7zehrt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "ADJA", "NN", "APPR", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "V\u00f1 taug nichts al\u00df da\u00df man jhn mit dem kott weg schmeisse.", "tokens": ["V\u00f1", "taug", "nichts", "al\u00df", "da\u00df", "man", "jhn", "mit", "dem", "kott", "weg", "schmeis\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PIS", "KOKOM", "KOUS", "PIS", "PPER", "APPR", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}}}}}