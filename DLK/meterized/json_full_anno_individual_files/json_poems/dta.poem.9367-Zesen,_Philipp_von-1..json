 "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "PPER", "ADV", "VVINF", "$("], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "dasz ich deinen Rosen-mund/", "tokens": ["dasz", "ich", "dei\u00b7nen", "Ro\u00b7sen\u00b7mund", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "den ich vormahls k\u00fcssen kunt/", "tokens": ["den", "ich", "vor\u00b7mahls", "k\u00fcs\u00b7sen", "kunt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVFIN", "PTKVZ", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "fort nicht mehr kann h\u00f6ren singen/", "tokens": ["fort", "nicht", "mehr", "kann", "h\u00f6\u00b7ren", "sin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKVZ", "PTKNEG", "ADV", "VMFIN", "VVINF", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "solche s\u00fcsze Melodey/", "tokens": ["sol\u00b7che", "s\u00fcs\u00b7ze", "Me\u00b7lo\u00b7dey", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "die mich machte traurens frey?", "tokens": ["die", "mich", "mach\u00b7te", "trau\u00b7rens", "frey", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Wiltu denn so gar verlaszen", "tokens": ["Wil\u00b7tu", "denn", "so", "gar", "ver\u00b7las\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "ADV", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "deinen Liebsten/ der sich Dier", "tokens": ["dei\u00b7nen