{"dta.poem.23521": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Nie ohne dieses!", "genre": "Lyrik", "period": "N.A.", "pub_year": "1840", "urn": "urn:nbn:de:kobv:b4-200905192626", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "O gl\u00fccklich wer noch Vettern hat,", "tokens": ["O", "gl\u00fcck\u00b7lich", "wer", "noch", "Vet\u00b7tern", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "PWS", "ADV", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem gl\u00e4nzet noch ein Morgenroth:", "tokens": ["Dem", "gl\u00e4n\u00b7zet", "noch", "ein", "Mor\u00b7gen\u00b7roth", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er wird, wenn nicht Geheimerath,", "tokens": ["Er", "wird", ",", "wenn", "nicht", "Ge\u00b7hei\u00b7me\u00b7rath", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "KOUS", "PTKNEG", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch Etwas noch vor seinem Tod.", "tokens": ["Doch", "Et\u00b7was", "noch", "vor", "sei\u00b7nem", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}