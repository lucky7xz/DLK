{"textgrid.poem.36122": {"metadata": {"author": {"name": "Kosegarten, Gotthard Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Kommt, holdselige Frau'n, in meinen sikelischen Garten,", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kommt, holdselige Frau'n, in meinen sikelischen Garten,", "tokens": ["Kommt", ",", "hold\u00b7se\u00b7li\u00b7ge", "Frau'n", ",", "in", "mei\u00b7nen", "si\u00b7ke\u00b7li\u00b7schen", "Gar\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJA", "NN", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Welchen f\u00fcr euch ich gepflanzt, welchen f\u00fcr euch ich gepflegt.", "tokens": ["Wel\u00b7chen", "f\u00fcr", "euch", "ich", "ge\u00b7pflanzt", ",", "wel\u00b7chen", "f\u00fcr", "euch", "ich", "ge\u00b7pflegt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "APPR", "PPER", "PPER", "VVPP", "$,", "PWAT", "APPR", "PPER", "PPER", "VVPP", "$."], "meter": "+--+--+--+-+-+", "measure": "dactylic.tri.plus"}, "line.3": {"text": "Freundliche Frauen und Fr\u00e4ulein, in meine bukolische Wirtschaft", "tokens": ["Freund\u00b7li\u00b7che", "Frau\u00b7en", "und", "Fr\u00e4u\u00b7lein", ",", "in", "mei\u00b7ne", "bu\u00b7ko\u00b7li\u00b7sche", "Wirt\u00b7schaft"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "NN", "$,", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Folgt mir gef\u00e4llig; f\u00fcr euch ward sie beschickt und geschm\u00fcckt.", "tokens": ["Folgt", "mir", "ge\u00b7f\u00e4l\u00b7lig", ";", "f\u00fcr", "euch", "ward", "sie", "be\u00b7schickt", "und", "ge\u00b7schm\u00fcckt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "$.", "APPR", "PPER", "VAFIN", "PPER", "VVPP", "KON", "VVPP", "$."], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}, "line.5": {"text": "Welches der M\u00e4gdlein mir hilft die Beete s\u00e4ubern und w\u00e4ssern,", "tokens": ["Wel\u00b7ches", "der", "M\u00e4gd\u00b7lein", "mir", "hilft", "die", "Bee\u00b7te", "s\u00e4u\u00b7bern", "und", "w\u00e4s\u00b7sern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "PPER", "VVFIN", "ART", "NN", "VVINF", "KON", "VVFIN", "$,"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.6": {"text": "Brav, wie Amalrich, sey solcher ein Freyer beschert!", "tokens": ["Brav", ",", "wie", "A\u00b7mal\u00b7rich", ",", "sey", "sol\u00b7cher", "ein", "Frey\u00b7er", "be\u00b7schert", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWAV", "NE", "$,", "VAFIN", "PIAT", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.7": {"text": "Welche der Frau'n mit mir theilt des Haushalts M\u00fchen; ihr sollen", "tokens": ["Wel\u00b7che", "der", "Frau'n", "mit", "mir", "theilt", "des", "Haus\u00b7halts", "M\u00fc\u00b7hen", ";", "ihr", "sol\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWS", "ART", "NN", "APPR", "PPER", "VVFIN", "ART", "NN", "NN", "$.", "PPER", "VMFIN"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.8": {"text": "T\u00f6chter, wie Thecla, so klug, fromm, wie Jucunde, gedeihn!", "tokens": ["T\u00f6ch\u00b7ter", ",", "wie", "The\u00b7cla", ",", "so", "klug", ",", "fromm", ",", "wie", "Ju\u00b7c