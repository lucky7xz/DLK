{"dta.poem.3893": {"metadata": {"author": {"name": "Spindler, Christian Gotthold", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-20581-9", "language": ["de:0.99"], "booktitle": "Spindler, Christian Gotthold: Unschuldige Jugend-Fr\u00fcchte. Leipzig, 1745."}, "poem": {"stanza.1": {"line.1": {"text": "Wohlan, ihr habt begl\u00fcckt gefreyt,", "tokens": ["Wo\u00b7hlan", ",", "ihr", "habt", "be\u00b7gl\u00fcckt", "ge\u00b7freyt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPER", "VAFIN", "VVPP", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und eurer Liebe Z\u00e4rtlichkeit", "tokens": ["Und", "eu\u00b7rer", "Lie\u00b7be", "Z\u00e4rt\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hat auch recht z\u00e4rtlich \u00fcberwunden.", "tokens": ["Hat", "auch", "recht", "z\u00e4rt\u00b7lich", "\u00fc\u00b7berw\u00b7un\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Was ich dabey noch w\u00fcnschen kan,", "tokens": ["Was", "ich", "da\u00b7bey", "noch", "w\u00fcn\u00b7schen", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PAV", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ist, fangt die Ehe gl\u00fccklich an,", "tokens": ["Ist", ",", "fangt", "die", "E\u00b7he", "gl\u00fcck\u00b7lich", "an", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "VVFIN", "ART", "NN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Euch hat weit h\u00f6hre Macht verbunden.", "tokens": ["Euch", "hat", "weit", "h\u00f6h\u00b7re", "Macht", "ver\u00b7bun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "So seyd an Gl\u00fcck und Wohlfahrt reich,", "tokens": ["So", "seyd", "an", "Gl\u00fcck", "und", "Wohl\u00b7fahrt", "reich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "NN", "KON", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Seyd fruchtbar, Sch\u00f6nste, mehret euch!", "tokens": ["Seyd", "frucht\u00b7bar", ",", "Sch\u00f6ns\u00b7te", ",", "meh\u00b7ret", "euch", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "$,", "NN", "$,", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}