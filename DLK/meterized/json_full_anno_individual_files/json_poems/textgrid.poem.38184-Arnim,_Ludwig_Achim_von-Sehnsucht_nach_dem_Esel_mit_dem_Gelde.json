{"textgrid.poem.38184": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Sehnsucht nach dem Esel mit dem Gelde", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Frisch auf ihr lieben Gesellen, ein neue Zeitung gut,", "tokens": ["Frisch", "auf", "ihr", "lie\u00b7ben", "Ge\u00b7sel\u00b7len", ",", "ein", "neu\u00b7e", "Zei\u00b7tung", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPOSAT", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Hab ich euch sagen w\u00f6llen, fa\u00dft einen guten Muth,", "tokens": ["Hab", "ich", "euch", "sa\u00b7gen", "w\u00f6l\u00b7len", ",", "fa\u00dft", "ei\u00b7nen", "gu\u00b7ten", "Muth", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPER", "VVINF", "VVINF", "$,", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+----+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Der mit dem Geld wird kommen, hab ich f\u00fcr gewi\u00df vernommen,", "tokens": ["Der", "mit", "dem", "Geld", "wird", "kom\u00b7men", ",", "hab", "ich", "f\u00fcr", "ge\u00b7wi\u00df", "ver\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VAFIN", "VVINF", "$,", "VAFIN", "PPER", "APPR", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.4": {"text": "Mit einer grosen Summen, das wird uns bringen Frommen.", "tokens": ["Mit", "ei\u00b7ner", "gro\u00b7sen", "Sum\u00b7men", ",", "das", "wird", "uns", "brin\u00b7gen", "From\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "PDS", "VAFIN", "PPER", "VVINF", "NN", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "La\u00dft uns nun fr\u00f6hlich singen, ob dieser Botschaft gut,", "tokens": ["La\u00dft", "uns", "nun", "fr\u00f6h\u00b7lich", "sin\u00b7gen", ",", "ob", "die\u00b7ser", "Bot\u00b7schaft", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ADJD", "VVINF", "$,", "KOUS", "PDAT", "NN", "ADJD", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und dem zu Lohn eins bringen, der's uns verk\u00fcnden thut,", "tokens": ["Und", "dem", "zu", "Lohn", "eins", "brin\u00b7gen", ",", "der's", "uns", "ver\u00b7k\u00fcn\u00b7den", "thut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "APPR", "NN", "PIS", "VVINF", "$,", "PRELS", "PPER", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Ich rieth zu diesen Dingen, da\u00df wir entgegen giengen,", "tokens": ["Ich", "rieth", "zu", "die\u00b7sen", "Din\u00b7gen", ",", "da\u00df", "wir", "ent\u00b7ge\u00b7gen", "gien\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PDAT", "NN", "$,", "KOUS", "PPER", "PTKVZ", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und freundlich den empfiengen, der uns das Geld thut bringen.", "tokens": ["Und", "freund\u00b7lich", "den", "emp\u00b7fi\u00b7en\u00b7gen", ",", "der", "uns", "das", "Geld", "thut", "brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "ADJA", "$,", "PRELS", "PPER", "ART", "NN", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}, "stanza.3": {"line.1": {"text": "So wolln wir frisch aufstehen, weil ihrs f\u00fcr rathsam acht,", "tokens": ["So", "wolln", "wir", "frisch", "auf\u00b7ste\u00b7hen", ",", "weil", "ihrs", "f\u00fcr", "rath\u00b7sam", "acht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "VVINF", "$,", "KOUS", "NN", "APPR", "ADJD", "CARD", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und ihm entgegen gehen, da\u00df er auf uns sey bedacht,", "tokens": ["Und", "ihm", "ent\u00b7ge\u00b7gen", "ge\u00b7hen", ",", "da\u00df", "er", "auf", "uns", "sey", "be\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PTKVZ", "VVINF", "$,", "KOUS", "PPER", "APPR", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "La\u00dft eilends uns fortlaufen, und ihm die Federn raufen,", "tokens": ["La\u00dft", "ei\u00b7lends", "uns", "fort\u00b7lau\u00b7fen", ",", "und", "ihm", "die", "Fe\u00b7dern", "rau\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "PPER", "VVPP", "$,", "KON", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+--++--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Er m\u00f6cht zur\u00fcck sonst zaufen, und sich von uns abschraufen.", "tokens": ["Er", "m\u00f6cht", "zu\u00b7r\u00fcck", "sonst", "zau\u00b7fen", ",", "und", "sich", "von", "uns", "ab\u00b7schrau\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "VVINF", "$,", "KON", "PRF", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+--+--+--", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "He lustig Compagnia! ich h\u00f6r den Esel wohl,", "tokens": ["He", "lus\u00b7tig", "Com\u00b7pag\u00b7nia", "!", "ich", "h\u00f6r", "den", "E\u00b7sel", "wohl", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "NE", "$.", "PPER", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er schreit nur I, A, I, A, die Truhen sind gewi\u00df voll,", "tokens": ["Er", "schreit", "nur", "I", ",", "A", ",", "I", ",", "A", ",", "die", "Tru\u00b7hen", "sind", "ge\u00b7wi\u00df", "voll", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "CARD", "$,", "NE", "$,", "XY", "$,", "NE", "$,", "ART", "NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Es wird uns noch gelingen, drum seyd nur guter Dingen,", "tokens": ["Es", "wird", "uns", "noch", "ge\u00b7lin\u00b7gen", ",", "drum", "seyd", "nur", "gu\u00b7ter", "Din\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVINF", "$,", "PAV", "VAFIN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Vor Freude la\u00dft uns springen, das Geld wird man uns bringen.", "tokens": ["Vor", "Freu\u00b7de", "la\u00dft", "uns", "sprin\u00b7gen", ",", "das", "Geld", "wird", "man", "uns", "brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "wo