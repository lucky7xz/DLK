{"dta.poem.2728": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Erblaste Corallen/  \n Jungf. E. R. von B. den 19. Febr. 1679.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ach h\u00f6chstbetr\u00fcbtste Frau wer kan ihr Leyd ermessen?", "tokens": ["Ach", "h\u00f6chst\u00b7be\u00b7tr\u00b7\u00fcbts\u00b7te", "Frau", "wer", "kan", "ihr", "Leyd", "er\u00b7mes\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADJA", "NN", "PWS", "VMFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und die Empsindlichkeit der Schmertzen stellen f\u00fcr?", "tokens": ["Und", "die", "Emp\u00b7sind\u00b7lich\u00b7keit", "der", "Schmert\u00b7zen", "stel\u00b7len", "f\u00fcr", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "VVFIN", "APPR", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Es sey/ da\u00df Niobe als wie ein Fels gesessen/", "tokens": ["Es", "sey", "/", "da\u00df", "Ni\u00b7o\u00b7be", "als", "wie", "ein", "Fels", "ge\u00b7ses\u00b7sen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$(", "KOUS", "NE", "KOUS", "KOKOM", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wie sie beraubet war der S\u00f6hn\u2019 und T\u00f6chter Zier.", "tokens": ["Wie", "sie", "be\u00b7rau\u00b7bet", "war", "der", "S\u00f6hn'", "und", "T\u00f6ch\u00b7ter", "Zier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "VAFIN", "ART", "NN", "KON", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So glaub ich da\u00df ihr Hertz hat bessern Fug zu klagen/", "tokens": ["So", "glaub", "ich", "da\u00df", "ihr", "Hertz", "hat", "bes\u00b7sern", "Fug", "zu", "kla\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KOUS", "PPOSAT", "NN", "VAFIN", "ADJA", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Je weit gerechter noch der Thr\u00e4nen Ursprung ist.", "tokens": ["Je", "weit", "ge\u00b7rech\u00b7ter", "noch", "der", "Thr\u00e4\u00b7nen", "Ur\u00b7sprung", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJD", "ADV", "ART", "NN", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ich seh\u2019 ein gantzes Meer des Jammers auf sie schlagen", "tokens": ["Ich", "seh'", "ein", "gant\u00b7zes", "Meer", "des", "Jam\u00b7mers", "auf", "sie", "schla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "ART", "NN", "APPR", "PPER", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und Freund und Zusprach giebt hier nicht den Seufftzern Frist.", "tokens": ["Und", "Freund", "und", "Zu\u00b7sprach", "giebt", "hier", "nicht", "den", "Seufft\u00b7zern", "Frist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VVFIN", "ADV", "PTKNEG", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Nach zweyer Kinder Tod/ der unaufh\u00f6rlich kr\u00e4ncket", "tokens": ["Nach", "zwey\u00b7er", "Kin\u00b7der", "Tod", "/", "der", "un\u00b7auf\u00b7h\u00f6r\u00b7lich", "kr\u00e4n\u00b7cket"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "NN", "$(", "ART", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und Wunden hat gemacht/ die nicht die Zeit geheilt/", "tokens": ["Und", "Wun\u00b7den", "hat", "ge\u00b7macht", "/", "die", "nicht", "die", "Zeit", "ge\u00b7heilt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "VVPP", "$(", "PRELS", "PTKNEG", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Sieht sie zum letzten noch die Tochter eingesencket/", "tokens": ["Sieht", "sie", "zum", "letz\u00b7ten", "noch", "die", "Toch\u00b7ter", "ein\u00b7ge\u00b7sen\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "ADJA", "ADV", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Mit welcher sie ihr Hertz und Seele hat getheilt.", "tokens": ["Mit", "wel\u00b7cher", "sie", "ihr", "Hertz", "und", "See\u00b7le", "hat", "ge\u00b7theilt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PPOSAT", "NN", "KON", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Kein schmertzlicher Verlust ist auf der Welt zu finden/", "tokens": ["Kein", "schmertz\u00b7li\u00b7cher", "Ver\u00b7lust", "ist", "auf", "der", "Welt", "zu", "fin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VAFIN", "APPR", "ART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Kein gr\u00f6sser Seelen Rie\u00df kan Sterblichen geschehn!", "tokens": ["Kein", "gr\u00f6s\u00b7ser", "See\u00b7len", "Rie\u00df", "kan", "Sterb\u00b7li\u00b7chen", "ge\u00b7schehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "NN", "VMFIN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Pflegt sonsten umb das Hau\u00df/ wenn eines will verschwinden/", "tokens": ["Pflegt", "sons\u00b7ten", "umb", "das", "Hau\u00df", "/", "wenn", "ei\u00b7nes", "will", "ver\u00b7schwin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "APPR", "ART", "NN", "$(", "KOUS", "PIS", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Der Nord der Traurigkeit der Seufftzer Wind zu wehn/", "tokens": ["Der", "Nord", "der", "Trau\u00b7rig\u00b7keit", "der", "Seufft\u00b7zer", "Wind", "zu", "wehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ART", "NN", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Wie soll denn alles Leyd hier nicht zusammen rauschen?", "tokens": ["Wie", "soll", "denn", "al\u00b7les", "Leyd", "hier", "nicht", "zu\u00b7sam\u00b7men", "rau\u00b7schen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "ADV", "PIAT", "NN", "ADV", "PTKNEG", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Nun sie den Angel-Stern der Hoffnung sieht vergehn.", "tokens": ["Nun", "sie", "den", "An\u00b7gel\u00b7S\u00b7tern", "der", "Hoff\u00b7nung", "sieht", "ver\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ART", "NN", "ART", "NN", "VVFIN", "VVINF", "$."], "meter": "-+-+---+-+-+", "measure": "unknown.measure.penta"}, "line.19": {"text": "Nun sie den hellen Tag mu\u00df mit der Nacht vertauschen/", "tokens": ["Nun", "sie", "den", "hel\u00b7len", "Tag", "mu\u00df", "mit", "der", "Nacht", "ver\u00b7tau\u00b7schen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ART", "ADJA", "NN", "VMFIN", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Und schaut ihr liebstes Kind itzt auf der Bahre stehn?", "tokens": ["Und", "schaut", "ihr", "liebs\u00b7tes", "Kind", "itzt", "auf", "der", "Bah\u00b7re", "stehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJA", "NN", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Die Tochter so mit Recht die Krone der Jungfrauen/", "tokens": ["Die", "Toch\u00b7ter", "so", "mit", "Recht", "die", "Kro\u00b7ne", "der", "Jung\u00b7frau\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "NN", "ART", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Die Perle keuscher Zucht/ das Bild der Fr\u00f6mmigkeit/", "tokens": ["Die", "Per\u00b7le", "keu\u00b7scher", "Zucht", "/", "das", "Bild", "der", "Fr\u00f6m\u00b7mig\u00b7keit", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$(", "ART", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Wo Tugend und Verstand sich wesendlich lie\u00df schauen/", "tokens": ["Wo", "Tu\u00b7gend", "und", "Ver\u00b7stand", "sich", "we\u00b7send\u00b7lich", "lie\u00df", "schau\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "PRF", "ADJD", "VVFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Bey der die Gratien ihn\u2019n einen Sitz bereit.", "tokens": ["Bey", "der", "die", "Gra\u00b7ti\u00b7en", "ihn'n", "ei\u00b7nen", "Sitz", "be\u00b7reit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "PPER", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Nun sie den letzten Schatz des Lebens mu\u00df vermissen/", "tokens": ["Nun", "sie", "den", "letz\u00b7ten", "Schatz", "des", "Le\u00b7bens", "mu\u00df", "ver\u00b7mis\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ART", "ADJA", "NN", "ART", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Den Trost der Einsamkeit sehn von der Seite fliehn:", "tokens": ["Den", "Trost", "der", "Ein\u00b7sam\u00b7keit", "sehn", "von", "der", "Sei\u00b7te", "fliehn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.27": {"text": "So glaub ich/ da\u00df ihr Hertz in St\u00fccke wird zerrissen/", "tokens": ["So", "glaub", "ich", "/", "da\u00df", "ihr", "Hertz", "in", "St\u00fc\u00b7cke", "wird", "zer\u00b7ris\u00b7sen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$(", "KOUS", "PPOSAT", "NN", "APPR", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Da\u00df sie das Sterbe-Kleid mu\u00df wie ihr Kind anziehn.", "tokens": ["Da\u00df", "sie", "das", "Ster\u00b7be\u00b7Kleid", "mu\u00df", "wie", "ihr", "Kind", "an\u00b7ziehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VMFIN", "KOKOM", "PPOSAT", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Allein ", "tokens": ["Al\u00b7lein"], "token_info": ["word"], "pos": ["ADV"], "meter": "-+", "measure": "iambic.single"}, "line.30": {"text": "Bey ihrem Angst-Geschrey/ was braucht man da vor Rath?", "tokens": ["Bey", "ih\u00b7rem", "Angst\u00b7Ge\u00b7schrey", "/", "was", "braucht", "man", "da", "vor", "Rath", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$(", "PWS", "VVFIN", "PIS", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Wer wei\u00df nicht/ wenn so tieff verwundte Hertzen bluten/", "tokens": ["Wer", "wei\u00df", "nicht", "/", "wenn", "so", "tieff", "ver\u00b7wund\u00b7te", "Hert\u00b7zen", "blu\u00b7ten", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PTKNEG", "$(", "KOUS", "ADV", "ADJD", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Da\u00df Redner und Poet da kein Geh\u00f6re hat?", "tokens": ["Da\u00df", "Red\u00b7ner", "und", "Po\u00b7et", "da", "kein", "Ge\u00b7h\u00f6\u00b7re", "hat", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "ADV", "PIAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Doch kan noch eintzig Trost in ihre Seele fallen/", "tokens": ["Doch", "kan", "noch", "eint\u00b7zig", "Trost", "in", "ih\u00b7re", "See\u00b7le", "fal\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "ADJD", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Und ihr bethr\u00e4nt Gesicht erblicken diese Schrifft:", "tokens": ["Und", "ihr", "be\u00b7thr\u00e4nt", "Ge\u00b7sicht", "er\u00b7bli\u00b7cken", "die\u00b7se", "Schrifft", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "NN", "VVINF", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "So hab ich unterm Bild ", "tokens": ["So", "hab", "ich", "un\u00b7term", "Bild"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.36": {"text": "Der liebsten Tochter Ruhm ein Denckmal hier gestifft.", "tokens": ["Der", "liebs\u00b7ten", "Toch\u00b7ter", "Ruhm", "ein", "Denck\u00b7mal", "hier", "ge\u00b7stifft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Ich will das Alterthum der Fabeln nicht ber\u00fchren/", "tokens": ["Ich", "will", "das", "Al\u00b7ter\u00b7thum", "der", "Fa\u00b7beln", "nicht", "be\u00b7r\u00fch\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "ART", "NN", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Wie der Medusen Haupt das voller Schlangen hieng/", "tokens": ["Wie", "der", "Me\u00b7du\u00b7sen", "Haupt", "das", "vol\u00b7ler", "Schlan\u00b7gen", "hieng", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Das Perfeus abgehaun und mit sich pflag zu f\u00fchren/", "tokens": ["Das", "Per\u00b7feus", "ab\u00b7ge\u00b7haun", "und", "mit", "sich", "pflag", "zu", "f\u00fch\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "KON", "APPR", "PRF", "VVFIN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Eh\u2019 er Andromeden zu einer Braut empfieng/", "tokens": ["Eh'", "er", "And\u00b7ro\u00b7me\u00b7den", "zu", "ei\u00b7ner", "Braut", "emp\u00b7fi\u00b7eng", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.41": {"text": "Hat an des Meeres Strand auf weiches Gra\u00df geleget/", "tokens": ["Hat", "an", "des", "Mee\u00b7res", "Strand", "auf", "wei\u00b7ches", "Gra\u00df", "ge\u00b7le\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "NN", "APPR", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Durch seiner Tropffen Blut gezeuget den Cora