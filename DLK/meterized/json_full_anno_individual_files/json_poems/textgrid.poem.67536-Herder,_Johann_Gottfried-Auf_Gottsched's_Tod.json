{"textgrid.poem.67536": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Auf Gottsched's Tod", "genre": "verse", "period": "N.A.", "pub_year": 1766, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herr Gottsched starb! Der alte wackre Mann,", "tokens": ["Herr", "Gott\u00b7sched", "starb", "!", "Der", "al\u00b7te", "wack\u00b7re", "Mann", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VVFIN", "$.", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der lebenslang viel schrieb und wenig sann", "tokens": ["Der", "le\u00b7bens\u00b7lang", "viel", "schrieb", "und", "we\u00b7nig", "sann"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ADV", "VVFIN", "KON", "PIS", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und, um nicht nachzusinnen, \u00fcbersetzte", "tokens": ["Und", ",", "um", "nicht", "nach\u00b7zu\u00b7sin\u00b7nen", ",", "\u00fc\u00b7bers\u00b7etz\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word"], "pos": ["KON", "$,", "KOUI", "PTKNEG", "VVINF", "$,", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und, statt zu \u00fcberwinden, plump zersetzte;", "tokens": ["Und", ",", "statt", "zu", "\u00fc\u00b7berw\u00b7in\u00b7den", ",", "plump", "zer\u00b7setz\u00b7te", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "$,", "KOUI", "PTKZU", "VVINF", "$,", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Der unsre Sprache, wie Augias' Stall,", "tokens": ["Der", "uns\u00b7re", "Spra\u00b7che", ",", "wie", "Au\u00b7gias'", "Stall", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "$,", "PWAV", "NE", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Rein w\u00e4sserte, ein Hercul \u00fcberall,", "tokens": ["Rein", "w\u00e4s\u00b7ser\u00b7te", ",", "ein", "Her\u00b7cul", "\u00fc\u00b7be\u00b7rall", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Mit Hand und Mund, an Schultern und an Lenden;", "tokens": ["Mit", "Hand", "und", "Mund", ",", "an", "Schul\u00b7tern", "und", "an", "Len\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Der, um die Schmach Germaniens zu enden,", "tokens": ["Der", ",", "um", "die", "Schmach", "Ger\u00b7ma\u00b7ni\u00b7ens", "zu", "en\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "KOUI", "ART", "NN", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Franz\u00f6s'schen Wind in deutsches Bleirohr zwang", "tokens": ["Fran\u00b7z\u00f6s'\u00b7schen", "Wind", "in", "deut\u00b7sches", "Blei\u00b7rohr", "zwang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Und mit dem Luftknall zwanzig Jahre lang", "tokens": ["Und", "mit", "dem", "Luft\u00b7knall", "zwan\u00b7zig", "Jah\u00b7re", "lang"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "CARD", "NN", "ADJD"], "meter": "-+-+-+-+-+", "measure": "i