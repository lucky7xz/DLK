{"textgrid.poem.37168": {"metadata": {"author": {"name": "Boie, Heinrich Christian", "birth": "N.A.", "death": "N.A."}, "title": "Hanne und Hannchen", "genre": "verse", "period": "N.A.", "pub_year": 1775, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wohl keine Frau ist ihrem Manne", "tokens": ["Wohl", "kei\u00b7ne", "Frau", "ist", "ih\u00b7rem", "Man\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PIAT", "NN", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Was du dem deinen, gute Hanne!", "tokens": ["Was", "du", "dem", "dei\u00b7nen", ",", "gu\u00b7te", "Han\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "PPER", "ART", "PPOSAT", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Hannchen, eine Hanne macht", "tokens": ["Ein", "Hann\u00b7chen", ",", "ei\u00b7ne", "Han\u00b7ne", "macht"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In dir ihn gl\u00fccklich Tag und Nacht.", "tokens": ["In", "dir", "ihn", "gl\u00fcck\u00b7lich", "Tag", "und", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PPER", "ADJD", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Will Liebe zu der Hanne stocken,", "tokens": ["Will", "Lie\u00b7be", "zu", "der", "Han\u00b7ne", "sto\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Das Hannchen wei\u00df ihn schon zu locken", "tokens": ["Das", "Hann\u00