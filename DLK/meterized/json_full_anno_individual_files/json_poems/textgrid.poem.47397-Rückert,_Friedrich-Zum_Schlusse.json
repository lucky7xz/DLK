{"textgrid.poem.47397": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Zum Schlusse", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Welt ist rauh und dumpf geworden,", "tokens": ["Die", "Welt", "ist", "rauh", "und", "dumpf", "ge\u00b7wor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "ADJD", "VAPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Stimm' entfiel ihr nach und nach,", "tokens": ["Die", "Stimm'", "ent\u00b7fiel", "ihr", "nach", "und", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "KON", "APPR", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die einst in t\u00f6nenden Akkorden", "tokens": ["Die", "einst", "in", "t\u00f6\u00b7nen\u00b7den", "Ak\u00b7kor\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zum offnen Ohr des Menschen sprach.", "tokens": ["Zum", "off\u00b7nen", "Ohr", "des", "Men\u00b7schen", "sprach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Als, aus der Welten Mitte quellend,", "tokens": ["Als", ",", "aus", "der", "Wel\u00b7ten", "Mit\u00b7te", "quel\u00b7lend", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "APPR", "ART", "NN", "NN", "VVPP", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Von Gottes Thron ein Chorgesang", "tokens": ["Von", "Got\u00b7tes", "Thron", "ein", "Chor\u00b7ge\u00b7sang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Engel, durch die R\u00e4ume schwellend,", "tokens": ["Der", "En\u00b7gel", ",", "durch", "die", "R\u00e4u\u00b7me", "schwel\u00b7lend", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bis an der Sch\u00f6pfung Grenzen drang;", "tokens": ["Bis", "an", "der", "Sch\u00f6p\u00b7fung", "Gren\u00b7zen", "drang", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Als, seine Sternenkreise schwingend,", "tokens": ["Als", ",", "sei\u00b7ne", "Ster\u00b7nen\u00b7krei\u00b7se", "schwin\u00b7gend", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Himmel sprach zur Erd' hinab,", "tokens": ["Der", "Him\u00b7mel", "sprach", "zur", "Erd'", "hin\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und sie, entgegen leise klingend,", "tokens": ["Und", "sie", ",", "ent\u00b7ge\u00b7gen", "lei\u00b7se", "klin\u00b7gend", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "VVFIN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Aus ihren Blumen Antwort gab;", "tokens": ["Aus", "ih\u00b7ren", "Blu\u00b7men", "Ant\u00b7wort", "gab", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Da, in der Ozeane Brausen,", "tokens": ["Da", ",", "in", "der", "O\u00b7zea\u00b7ne", "Brau\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPR", "ART", "NN", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Dar\u00fcber Gottes Odem fuhr,", "tokens": ["Da\u00b7r\u00fc\u00b7ber", "Got\u00b7tes", "O\u00b7dem", "fuhr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "NN", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vernahm der Mensch mit heil'gem Grausen", "tokens": ["Ver\u00b7nahm", "der", "Mensch", "mit", "heil'\u00b7gem", "Grau\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die \u00c4olsharfen der Natur.", "tokens": ["Die", "\u00c4\u00b7ols\u00b7har\u00b7fen", "der", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Die Morgen- und die Abendwinde", "tokens": ["Die", "Mor\u00b7gen", "und", "die", "Ab\u00b7end\u00b7win\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "TRUNC", "KON", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Verk\u00fcndigten den Preis des Herrn", "tokens": ["Ver\u00b7k\u00fcn\u00b7dig\u00b7ten", "den", "Preis", "des", "Herrn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und fl\u00fcsterten dazwischen linde", "tokens": ["Und", "fl\u00fcs\u00b7ter\u00b7ten", "da\u00b7zwi\u00b7schen", "lin\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PAV", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Von menschlichen Gef\u00fchlen gern.", "tokens": ["Von", "menschli\u00b7chen", "Ge\u00b7f\u00fch\u00b7len", "gern", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Von Liebe sprach das Blatt am Baume,", "tokens": ["Von", "Lie\u00b7be", "sprach", "das", "Blatt", "am", "Bau\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und lieblich war des Tieres Ruf;", "tokens": ["Und", "lieb\u00b7lich", "war", "des", "Tie\u00b7res", "Ruf", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der starre Stein, er sprach im Traume,", "tokens": ["Der", "star\u00b7re", "Stein", ",", "er", "sprach", "im", "Trau\u00b7me", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df ew'ge Lieb' auch ihn erschuf.", "tokens": ["Da\u00df", "ew'\u00b7ge", "Lieb'", "auch", "ihn", "er\u00b7schuf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "ADV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Und ungehindert wie vom Quelle", "tokens": ["Und", "un\u00b7ge\u00b7hin\u00b7dert", "wie", "vom", "Quel\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "KOKOM", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sich Wog' auf Wog' herniedergo\u00df,", "tokens": ["Sich", "Wo\u00b7g'", "auf", "Wo\u00b7g'", "her\u00b7nie\u00b7der\u00b7go\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So war des Worts kristallne Helle,", "tokens": ["So", "war", "des", "Worts", "kris\u00b7tall\u00b7ne", "Hel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die von der Menschenlippe flo\u00df.", "tokens": ["Die", "von", "der", "Men\u00b7schen\u00b7lip\u00b7pe", "flo\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Die Brust ein Spiegel ungetr\u00fcbet,", "tokens": ["Die", "Brust", "ein", "Spie\u00b7gel", "un\u00b7ge\u00b7tr\u00fc\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Gef\u00fchl ein reiner Wiederhall,", "tokens": ["Ge\u00b7f\u00fchl", "ein", "rei\u00b7ner", "Wie\u00b7der\u00b7hall", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gesang durch keine Kunst ge\u00fcbet,", "tokens": ["Ge\u00b7sang", "durch", "kei\u00b7ne", "Kunst", "ge\u00b7\u00fc\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Dichter eine Nachtigall.", "tokens": ["Der", "Dich\u00b7ter", "ei\u00b7ne", "Nach\u00b7ti\u00b7gall", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "O h\u00e4tt' in jenen goldnen Tagen,", "tokens": ["O", "h\u00e4tt'", "in", "je\u00b7nen", "gold\u00b7nen", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Als frei des Mundes Flut gerollt,", "tokens": ["Als", "frei", "des", "Mun\u00b7des", "Flut", "ge\u00b7rollt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die goldnen Saiten ", "tokens": ["Die", "gold\u00b7nen", "Sai\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Vorm Ohr der ganzen Welt gesollt.", "tokens": ["Vorm", "Ohr", "der", "gan\u00b7zen", "Welt", "ge\u00b7sollt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Wie h\u00e4tt' er von dem Ewig-Sch\u00f6nen,", "tokens": ["Wie", "h\u00e4tt'", "er", "von", "dem", "E\u00b7wig\u00b7Sch\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von Lieb', aus der die Sch\u00f6pfung quillt,", "tokens": ["Von", "Lieb'", ",", "aus", "der", "die", "Sch\u00f6p\u00b7fung", "quillt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gewollt in erdentbundnen T\u00f6nen", "tokens": ["Ge\u00b7wollt", "in", "er\u00b7dent\u00b7bund\u00b7nen", "T\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Entfalten rein ein Himmelsbild.", "tokens": ["Ent\u00b7fal\u00b7ten", "rein", "ein", "Him\u00b7mels\u00b7bild", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Nun haben der Natur Gewalten", "tokens": ["Nun", "ha\u00b7ben", "der", "Na\u00b7tur", "Ge\u00b7wal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zu wildem Kampfe sich emp\u00f6rt,", "tokens": ["Zu", "wil\u00b7dem", "Kamp\u00b7fe", "sich", "em\u00b7p\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PRF", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Harmonie der Weltgestalten", "tokens": ["Die", "Har\u00b7mo\u00b7nie", "der", "Welt\u00b7ge\u00b7stal\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ist vor des Menschen Blick gest\u00f6rt.", "tokens": ["Ist", "vor", "des", "Men\u00b7schen", "Blick", "ge\u00b7st\u00f6rt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Die ew'ge Sch\u00f6nheit hat den Schleier", "tokens": ["Die", "ew'\u00b7ge", "Sch\u00f6n\u00b7heit", "hat", "den", "Schlei\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Genommen vor ihr Angesicht,", "tokens": ["Ge\u00b7nom\u00b7men", "vor", "ihr", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und kaum vernimmt des Dichters Leier,", "tokens": ["Und", "kaum", "ver\u00b7nimmt", "des", "Dich\u00b7ters", "Lei\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Was die der Sterne droben spricht.", "tokens": ["Was", "die", "der", "Ster\u00b7ne", "dro\u00b7ben", "spricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Der Elemente seindlich Hadern", "tokens": ["Der", "E\u00b7le\u00b7men\u00b7te", "seind\u00b7lich", "Ha\u00b7dern"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Raubt seine Stille dem Gef\u00fchl,", "tokens": ["Raubt", "sei\u00b7ne", "Stil\u00b7le", "dem", "Ge\u00b7f\u00fchl", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und zuckend durch der Menschheit Adern", "tokens": ["Und", "zu\u00b7ckend", "durch", "der", "Menschheit", "A\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVPP", "APPR", "ART", "NN", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Geht leidenschaftliches Gew\u00fchl.", "tokens": ["Geht", "lei\u00b7den\u00b7schaft\u00b7li\u00b7ches", "Ge\u00b7w\u00fchl", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "Sich machen unterm Himmelsbogen", "tokens": ["Sich", "ma\u00b7chen", "un\u00b7term", "Him\u00b7mels\u00b7bo\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PRF", "VVINF", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die St\u00fcrme durch Zerst\u00f6rung Bahn,", "tokens": ["Die", "St\u00fcr\u00b7me", "durch", "Zer\u00b7st\u00f6\u00b7rung", "Bahn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und st\u00fcrmisch geht in hohlen Wogen", "tokens": ["Und", "st\u00fcr\u00b7misch", "geht", "in", "hoh\u00b7len", "Wo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Des Menschenlebens Ozean.", "tokens": ["Des", "Men\u00b7schen\u00b7le\u00b7bens", "O\u00b7ze\u00b7an", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.15": {"line.1": {"text": "Es regt sich die Natur im Grimme,", "tokens": ["Es", "regt", "sich", "die", "Na\u00b7tur", "im", "Grim\u00b7me", ","], "token_info":