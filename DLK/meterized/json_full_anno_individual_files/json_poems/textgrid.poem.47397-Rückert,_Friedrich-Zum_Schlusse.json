rd", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "VVFIN", "KON", "ADJD", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mu\u00df mit dem Wort um Ausdruck ringen", "tokens": ["Mu\u00df", "mit", "dem", "Wort", "um", "Aus\u00b7druck", "rin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "APPR", "ART", "NN", "APPR", "NN", "VVFIN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Und k\u00e4mpfen mit der Sprach' ums Lied.", "tokens": ["Und", "k\u00e4mp\u00b7fen", "mit", "der", "Sprach'", "ums", "Lied", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.38": {"line.1": {"text": "Und wenn von Nachtigall und Rosen", "tokens": ["Und", "wenn", "von", "Nach\u00b7ti\u00b7gall", "und", "Ro\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Fr\u00fchlingshauch sein Lied durchdringt,", "tokens": ["Ein", "Fr\u00fch\u00b7lings\u00b7hauch", "sein", "Lied", "durch\u00b7dringt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So seufzt er, wie das laute Tosen", "tokens": ["So", "seufzt", "er", ",", "wie", "das", "lau\u00b7te", "To\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWAV", "PDS", "VVFIN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Des Marktes spurlos es verschlingt.", "tokens": ["Des", "Mark\u00b7tes", "spur\u00b7los", "es", "ver\u00b7schlingt", "."], "token_inf