{"dta.poem.23431": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Patriotismus.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.71", "pt:0.28"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "O das R\u00fchmen, o das Preisen,", "tokens": ["O", "das", "R\u00fch\u00b7men", ",", "o", "das", "Prei\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$,", "FM", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df wir gute Deutsche sind!", "tokens": ["Da\u00df", "wir", "gu\u00b7te", "Deut\u00b7sche", "sind", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJA", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "La\u00dft uns durch die That beweisen,", "tokens": ["La\u00dft", "uns", "durch", "die", "That", "be\u00b7wei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df wir deutsche M\u00e4nner sind!", "tokens": ["Da\u00df", "wir", "deut\u00b7sche", "M\u00e4n\u00b7ner", "sind", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJA", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "La\u00dft uns auch vor K\u00f6nigsthronen", "tokens": ["La\u00dft", "uns", "auch", "vor", "K\u00f6\u00b7nigs\u00b7thro\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ADV", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ruhig sagen was wir sind,", "tokens": ["Ru\u00b7hig", "sa\u00b7gen", "was", "wir", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVINF", "PWS", "PPER", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df nicht Flinten und Kanonen", "tokens": ["Da\u00df", "nicht", "Flin\u00b7ten", "und", "Ka\u00b7no\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PTKNEG", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Unsre Herrn und Meister sind!", "tokens": ["Uns\u00b7re", "Herrn", "und", "Meis\u00b7ter", "sind", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}