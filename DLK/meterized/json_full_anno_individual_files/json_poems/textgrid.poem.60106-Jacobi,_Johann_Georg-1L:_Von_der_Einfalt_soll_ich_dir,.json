{"textgrid.poem.60106": {"metadata": {"author": {"name": "Jacobi, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "1L: Von der Einfalt soll ich dir,", "genre": "verse", "period": "N.A.", "pub_year": 1777, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von der Einfalt soll ich dir,", "tokens": ["Von", "der", "Ein\u00b7falt", "soll", "ich", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VMFIN", "PPER", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Gutes M\u00e4dchen, etwas sagen?", "tokens": ["Gu\u00b7tes", "M\u00e4d\u00b7chen", ",", "et\u00b7was", "sa\u00b7gen", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PIS", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Allzu selten t\u00f6nt von ihr", "tokens": ["All\u00b7zu", "sel\u00b7ten", "t\u00f6nt", "von", "ihr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKA", "ADJD", "VVFIN", "APPR", "PPOSAT"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Noch ein Lied in unsern Tagen!", "tokens": ["Noch", "ein", "Lied", "in", "un\u00b7sern", "Ta\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Denn, gebannt von Hof und Stadt,", "tokens": ["Denn", ",", "ge\u00b7bannt", "von", "Hof", "und", "Stadt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VVPP", "APPR", "NE", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Will sie nur im Freyen scherzen;", "tokens": ["Will", "sie", "nur", "im", "Frey\u00b7en", "scher\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Jene l\u00fcgen; Einfalt hat", "tokens": ["Je\u00b7ne", "l\u00fc\u00b7gen", ";", "Ein\u00b7falt", "hat"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PDS", "VVFIN", "$.", "NN", "VAFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Immer Eines nur im Herzen.", "tokens": ["Im\u00b7mer", "Ei\u00b7nes", "nur", "im", "Her\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Jedes Wort ist Seelenklang,", "tokens": ["Je\u00b7des", "Wort", "ist", "See\u00b7len\u00b7klang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Des Gedankens treue Stimme;", "tokens": ["Des", "Ge\u00b7dan\u00b7kens", "treu\u00b7e", "Stim\u00b7me", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ruhig, sicher ist ihr Gang,", "tokens": ["Ru\u00b7hig", ",", "si\u00b7cher", "ist", "ihr", "Gang", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJD", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und ihr Wandel ohne Kr\u00fcmme.", "tokens": ["Und", "ihr", "Wan\u00b7del", "oh\u00b7ne", "Kr\u00fcm\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Wenig thut sie nie durch Viel;", "tokens": ["We\u00b7nig", "thut", "sie", "nie", "durch", "Viel", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Aber Vieles gern durch Wenig;", "tokens": ["A\u00b7ber", "Vie\u00b7les", "gern", "durch", "We\u00b7nig", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "ADV", "APPR", "NE", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Klatschet keinem Narrenspiel,", "tokens": ["Klat\u00b7schet", "kei\u00b7nem", "Nar\u00b7ren\u00b7spiel", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "W\u00e4re gleich der Narr ein K\u00f6nig.", "tokens": ["W\u00e4\u00b7re", "gleich", "der", "Narr", "ein", "K\u00f6\u00b7nig", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Im Tyrannensaal geha\u00dft,", "tokens": ["Im", "Ty\u00b7ran\u00b7nen\u00b7saal", "ge\u00b7ha\u00dft", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "W\u00e4hlt sie, von den reinsten L\u00fcften", "tokens": ["W\u00e4hlt", "sie", ",", "von", "den", "reins\u00b7ten", "L\u00fcf\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Angef\u00e4chelt, zum Pallast", "tokens": ["An\u00b7ge\u00b7f\u00e4\u00b7chelt", ",", "zum", "Pal\u00b7last"], "token_info": ["word", "punct", "word", "word"], "pos": ["VVPP", "$,", "APPRART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Einen Busch auf armen Triften.", "tokens": ["Ei\u00b7nen", "Busch", "auf", "ar\u00b7men", "Trif\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Hohe Weisheit sucht sie nicht;", "tokens": ["Ho\u00b7he", "Weis\u00b7heit", "sucht", "sie", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ihr gen\u00fcgt, auf gr\u00fcnen Auen,", "tokens": ["Ihr", "ge\u00b7n\u00fcgt", ",", "auf", "gr\u00fc\u00b7nen", "Au\u00b7en", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "An der Wahrheit D\u00e4mmerlicht,", "tokens": ["An", "der", "Wahr\u00b7heit", "D\u00e4m\u00b7mer\u00b7licht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Um in Demuth Gott zu schauen.", "tokens": ["Um", "in", "De\u00b7muth", "Gott", "zu", "schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "APPR", "NN", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Alles zeigt ihr seine Spur,", "tokens": ["Al\u00b7les", "zeigt", "ihr", "sei\u00b7ne", "Spur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Heilig ist des Sch\u00f6pfers H\u00fclle;", "tokens": ["Hei\u00b7lig", "ist", "des", "Sch\u00f6p\u00b7fers", "H\u00fcl\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "VAFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Zu dem Vater bethet nur", "tokens": ["Zu", "dem", "Va\u00b7ter", "be\u00b7thet", "nur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Einfalt aus des Herzens F\u00fclle;", "tokens": ["Ein\u00b7falt", "aus", "des", "Her\u00b7zens", "F\u00fcl\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Singt im Dornenkranz, und legt", "tokens": ["Singt", "im", "Dor\u00b7nen\u00b7kranz", ",", "und", "legt"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "APPRART", "NN", "$,", "KON", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf ein Kreuz die matten H\u00e4nde;", "tokens": ["Auf", "ein", "Kreuz", "die", "mat\u00b7ten", "H\u00e4n\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Noch von Lieb' und Hoffnung schl\u00e4gt", "tokens": ["Noch", "von", "Lieb'", "und", "Hoff\u00b7nung", "schl\u00e4gt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "KON", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihre Brust am letzten Ende.", "tokens": ["Ih\u00b7re", "Brust", "am", "letz\u00b7ten", "En\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "So verl\u00e4\u00dft sie Flur und Hain,", "tokens": ["So", "ver\u00b7l\u00e4\u00dft", "sie", "Flur", "und", "Hain", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "KON", "NN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Blickt von ihrer stillen Wiese", "tokens": ["Blickt", "von", "ih\u00b7rer", "stil\u00b7len", "Wie\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Froh gen Himmel, schlummert ein,", "tokens": ["Froh", "gen", "Him\u00b7mel", ",", "schlum\u00b7mert", "ein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "$,", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und erwacht im Paradiese.", "tokens": ["Und", "er\u00b7wacht", "im", "Pa\u00b7ra\u00b7die\u00b7se", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Von der Einfalt soll ich dir,", "tokens": ["Von", "der", "Ein\u00b7falt", "soll", "ich", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VMFIN", "PPER", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Gutes M\u00e4dchen, etwas sagen?", "tokens": ["Gu\u00b7tes", "M\u00e4d\u00b7chen", ",", "et\u00b7was", "sa\u00b7gen", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PIS", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Allzu selten t\u00f6nt von ihr", "tokens": ["All\u00b7zu", "sel\u00b7ten", "t\u00f