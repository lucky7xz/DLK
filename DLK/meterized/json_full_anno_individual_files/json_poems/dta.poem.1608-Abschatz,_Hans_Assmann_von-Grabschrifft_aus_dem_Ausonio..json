{"dta.poem.1608": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Grabschrifft aus dem  Ausonio.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Nahmen/ Eltern/ Vaterland/", "tokens": ["Nah\u00b7men", "/", "El\u00b7tern", "/", "Va\u00b7ter\u00b7land", "/"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$(", "NN", "$(", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und mein gantzer Lebens-Stand", "tokens": ["Und", "mein", "gant\u00b7zer", "Le\u00b7bens\u00b7Stand"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mag verbleiben unbekandt/", "tokens": ["Mag", "ver\u00b7blei\u00b7ben", "un\u00b7be\u00b7kandt", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "ADJD", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ich mu\u00df ewig-schweigend seyn", "tokens": ["Ich", "mu\u00df", "e\u00b7wig\u00b7schwei\u00b7gend", "seyn"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADJD", "VAINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Asche/ Staub und d\u00fcrres Bein.", "tokens": ["A\u00b7sche", "/", "Staub", "und", "d\u00fcr\u00b7res", "Bein", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "NN", "KON", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ich bin Nichts/ wie ich Nichts war/", "tokens": ["Ich", "bin", "Nichts", "/", "wie", "ich", "Nichts", "war", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIS", "$(", "PWAV", "PPER", "PIS", "VAFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und ein Nichts/ das mich gebahr;", "tokens": ["Und", "ein", "Nichts", "/", "das", "mich", "ge\u00b7bahr", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$(", "PRELS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "La\u00df mich in der finstern Ruh/", "tokens": ["La\u00df", "mich", "in", "der", "fins\u00b7tern", "Ruh", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Decke meine Fehler zu:", "tokens": ["De\u00b7cke", "mei\u00b7ne", "Feh\u00b7ler", "zu", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Meines gleichen wirst auch du!", "tokens": ["Mei\u00b7nes", "glei\u00b7chen", "wirst", "auch", "du", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "VAFIN", "ADV", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}