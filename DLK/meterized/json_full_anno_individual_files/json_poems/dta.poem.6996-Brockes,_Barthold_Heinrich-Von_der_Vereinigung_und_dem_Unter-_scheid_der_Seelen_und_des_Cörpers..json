{"dta.poem.6996": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Vereinigung und dem Unter-  \n scheid der Seelen und des C\u00f6rpers.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wofern der gantze Mensch besteht", "tokens": ["Wo\u00b7fern", "der", "gant\u00b7ze", "Mensch", "be\u00b7steht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Blo\u00df aus Materie allein:", "tokens": ["Blo\u00df", "aus", "Ma\u00b7te\u00b7rie", "al\u00b7lein", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie mu\u00df dieselbige doch eingetheilet seyn?", "tokens": ["Wie", "mu\u00df", "die\u00b7sel\u00b7bi\u00b7ge", "doch", "ein\u00b7ge\u00b7thei\u00b7let", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PDS", "ADV", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wer von den Theilen ist dazu erh\u00f6ht,", "tokens": ["Wer", "von", "den", "Thei\u00b7len", "ist", "da\u00b7zu", "er\u00b7h\u00f6ht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "VAFIN", "PAV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Das Wohl des Gantzen einzurichten?", "tokens": ["Das", "Wohl", "des", "Gant\u00b7zen", "ein\u00b7zu\u00b7rich\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wer soll den Streit am ersten schlichten?", "tokens": ["Wer", "soll", "den", "Streit", "am", "ers\u00b7ten", "schlich\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "NN", "APPRART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Wer giebt ihm selber doch ein Vorzugs-Recht,", "tokens": ["Wer", "giebt", "ihm", "sel\u00b7ber", "doch", "ein", "Vor\u00b7zugs\u00b7Recht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und sucht die Gleichheit zu vernichten?", "tokens": ["Und", "sucht", "die", "Gleich\u00b7heit", "zu", "ver\u00b7nich\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Sind kleine runde C\u00f6rperlein,", "tokens": ["Sind", "klei\u00b7ne", "run\u00b7de", "C\u00f6r\u00b7per\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Die rollend in den Adern rennen,", "tokens": ["Die", "rol\u00b7lend", "in", "den", "A\u00b7dern", "ren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Geschickt, da\u00df sie befehlen k\u00f6nnen", "tokens": ["Ge\u00b7schickt", ",", "da\u00df", "sie", "be\u00b7feh\u00b7len", "k\u00f6n\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVPP", "$,", "KOUS", "PPER", "VVINF", "VMINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Den andern, die nicht minder seyn,", "tokens": ["Den", "an\u00b7dern", ",", "die", "nicht", "min\u00b7der", "seyn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "PRELS", "PTKNEG", "ADV", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Da\u00df sie aus Furcht, um nicht den Tod zu leiden,", "tokens": ["Da\u00df", "sie", "aus", "Furcht", ",", "um", "nicht", "den", "Tod", "zu", "lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "$,", "KOUI", "PTKNEG", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Sich lassen Arm und Bein abschneiden?", "tokens": ["Sich", "las\u00b7sen", "Arm", "und", "Bein", "ab\u00b7schnei\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVINF", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}