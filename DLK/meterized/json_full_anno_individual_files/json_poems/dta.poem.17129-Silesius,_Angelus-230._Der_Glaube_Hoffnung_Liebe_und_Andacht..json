{"dta.poem.17129": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "230.  Der Glaube/ Hoffnung/ Liebe  \n und Andacht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Der Glaube greifft nach GOtt; die Hoffnung nimbt", "tokens": ["Der", "Glau\u00b7be", "greifft", "nach", "Gott", ";", "die", "Hoff\u00b7nung", "nimbt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$.", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "jhn wahr;", "tokens": ["jhn", "wahr", ";"], "token_info": ["word", "word", "punct"], "pos": ["PPER", "PTKVZ", "$."], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Die Lieb\u2019 umbhalset Jhn: die Andacht j\u00dft Jhn gar.", "tokens": ["Die", "Lieb'", "um\u00b7bhal\u00b7set", "Jhn", ":", "die", "An\u00b7dacht", "j\u00dft", "Jhn", "gar", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$.", "ART", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}}}}}