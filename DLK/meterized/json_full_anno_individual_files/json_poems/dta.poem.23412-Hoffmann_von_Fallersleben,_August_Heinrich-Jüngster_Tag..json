{"dta.poem.23412": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "J\u00fcngster Tag.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Monde sind die Nationen,", "tokens": ["Mon\u00b7de", "sind", "die", "Na\u00b7ti\u00b7o\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und die F\u00fcrsten sind das Licht.", "tokens": ["Und", "die", "F\u00fcrs\u00b7ten", "sind", "das", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ART", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Finster wird's bei Millionen,", "tokens": ["Fins\u00b7ter", "wird's", "bei", "Mil\u00b7lion\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPR", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Wenn's an diesem Licht gebricht.", "tokens": ["Wenn's", "an", "die\u00b7sem", "Licht", "ge\u00b7bricht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "PDAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Froh la\u00dft uns der Tage genie\u00dfen,", "tokens": ["Froh", "la\u00dft", "uns", "der", "Ta\u00b7ge", "ge\u00b7nie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Der Tage des himmlischen Lichts!", "tokens": ["Der", "Ta\u00b7ge", "des", "himm\u00b7li\u00b7schen", "Lichts", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "Wer wei\u00df denn, wie bald sie verflie\u00dfen,", "tokens": ["Wer", "wei\u00df", "denn", ",", "wie", "bald", "sie", "ver\u00b7flie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "$,", "PWAV", "ADV", "PPER", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.8": {"text": "Wie bald in ein trauriges Nichts!", "tokens": ["Wie", "bald", "in", "ein", "trau\u00b7ri\u00b7ges", "Nichts", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "Denn wenn einst die F\u00fcrsten sterben,", "tokens": ["Denn", "wenn", "einst", "die", "F\u00fcrs\u00b7ten", "ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.2": {"text": "Und sie sterben doch gewi\u00df,", "tokens": ["Und", "sie", "ster\u00b7ben", "doch", "ge\u00b7wi\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach! dann m\u00fcssen wir verderben", "tokens": ["Ach", "!", "dann", "m\u00fcs\u00b7sen", "wir", "ver\u00b7der\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "ADV", "VMFIN", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "All' in Nacht und Finsterni\u00df.", "tokens": ["All'", "in", "Nacht", "und", "Fins\u00b7ter\u00b7ni\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Die Sterne sie sind ja erblichen,", "tokens": ["Die", "Ster\u00b7ne", "sie", "sind", "ja", "er\u00b7bli\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VAFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die Schimmer und Glanz uns verliehn;", "tokens": ["Die", "Schim\u00b7mer", "und", "Glanz", "uns", "ver\u00b7liehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "PPER", "VVINF", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "Die Sonne sie ist ja gewichen,", "tokens": ["Die", "Son\u00b7ne", "sie", "ist", "ja", "ge\u00b7wi\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.8": {"text": "Die gn\u00e4dig die V\u00f6lker beschien.", "tokens": ["Die", "gn\u00e4\u00b7dig", "die", "V\u00f6l\u00b7ker", "be\u00b7schien", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}