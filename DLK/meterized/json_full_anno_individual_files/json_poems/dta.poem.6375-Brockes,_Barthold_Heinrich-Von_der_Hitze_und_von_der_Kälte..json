{"dta.poem.6375": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Hitze und von der K\u00e4lte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Allein,", "tokens": ["Al\u00b7lein", ","], "token_info": ["word", "punct"], "pos": ["ADV", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "Die C\u00f6rper, welche kalt, und die durch ihre Schwere", "tokens": ["Die", "C\u00f6r\u00b7per", ",", "wel\u00b7che", "kalt", ",", "und", "die", "durch", "ih\u00b7re", "Schwe\u00b7re"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "$,", "KON", "ART", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Fast unbeweglich seyn,", "tokens": ["Fast", "un\u00b7be\u00b7weg\u00b7lich", "seyn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wofern die Flamme sie bezwinget;", "tokens": ["Wo\u00b7fern", "die", "Flam\u00b7me", "sie", "be\u00b7zwin\u00b7get", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}