{"dta.poem.5479": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die uns zur Ehre GOttes leitende  \n Creatur.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es lieget in der Menschen Seelen", "tokens": ["Es", "lie\u00b7get", "in", "der", "Men\u00b7schen", "See\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zum Dancken eine F\u00e4higkeit;", "tokens": ["Zum", "Dan\u00b7cken", "ei\u00b7ne", "F\u00e4\u00b7hig\u00b7keit", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch wird die trefliche Beschaffenheit,", "tokens": ["Doch", "wird", "die", "tref\u00b7li\u00b7che", "Be\u00b7schaf\u00b7fen\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wofern des Sch\u00f6pfers Werck\u2019 ihr fehlen,", "tokens": ["Wo\u00b7fern", "des", "Sch\u00f6p\u00b7fers", "Werck", "ihr", "feh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und nichts sie durch die Sinne r\u00fchrt;", "tokens": ["Und", "nichts", "sie", "durch", "die", "Sin\u00b7ne", "r\u00fchrt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Als w\u00e4re sie nicht da, auch nicht versp\u00fchrt.", "tokens": ["Als", "w\u00e4\u00b7re", "sie", "nicht", "da", ",", "auch", "nicht", "ver\u00b7sp\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "PTKNEG", "ADV", "$,", "ADV", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Es mu\u00df zugleich auch die Betrachtungs-Kraft,", "tokens": ["Es", "mu\u00df", "zu\u00b7gleich", "auch", "die", "Be\u00b7trach\u00b7tungs\u00b7Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "So ebenfals der Seelen Eigenschaft,", "tokens": ["So", "e\u00b7ben\u00b7fals", "der", "See\u00b7len", "Ei\u00b7gen\u00b7schaft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Sich mit den Sinnlichen verbinden;", "tokens": ["Sich", "mit", "den", "Sinn\u00b7li\u00b7chen", "ver\u00b7bin\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.10": {"text": "Wir m\u00fcssen mit Bedacht empfinden.", "tokens": ["Wir", "m\u00fcs\u00b7sen", "mit", "Be\u00b7dacht", "emp\u00b7fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Wann die\u00df geschicht; kann es nicht anders seyn,", "tokens": ["Wann", "die\u00df", "ge\u00b7schicht", ";", "kann", "es", "nicht", "an\u00b7ders", "seyn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDS", "VVPP", "$.", "VMFIN", "PPER", "PTKNEG", "ADV", "VAINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Wir werden solche Triebe f\u00fchlen,", "tokens": ["Wir", "wer\u00b7den", "sol\u00b7che", "Trie\u00b7be", "f\u00fch\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Die auf die Qvell der Lust allein,", "tokens": ["Die", "auf", "die", "Qvell", "der", "Lust", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Auf unsers Sch\u00f6pfers Ehre, zielen.", "tokens": ["Auf", "un\u00b7sers", "Sch\u00f6p\u00b7fers", "Eh\u00b7re", ",", "zie\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}