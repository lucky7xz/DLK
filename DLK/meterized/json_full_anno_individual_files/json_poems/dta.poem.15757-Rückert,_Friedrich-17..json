{"dta.poem.15757": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "17.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich will durchaus nicht thun, was wollen die und lieben,", "tokens": ["Ich", "will", "durc\u00b7haus", "nicht", "thun", ",", "was", "wol\u00b7len", "die", "und", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PTKNEG", "VVINF", "$,", "PWS", "VMFIN", "ART", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "F\u00fcr die gesungen jetzt, getanzt wird und geschrieben.", "tokens": ["F\u00fcr", "die", "ge\u00b7sun\u00b7gen", "jetzt", ",", "ge\u00b7tanzt", "wird", "und", "ge\u00b7schrie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADV", "$,", "VVPP", "VAFIN", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Nur h\u00f6ren wollen sie und sehn, ohn' aufzuthun", "tokens": ["Nur", "h\u00f6\u00b7ren", "wol\u00b7len", "sie", "und", "sehn", ",", "ohn'", "auf\u00b7zu\u00b7thun"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVINF", "VMFIN", "PPER", "KON", "VVINF", "$,", "KOUI", "VVIZU"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein innres Aug' und Ohr, im Aeu\u00dfern m\u00fc\u00dfig ruhn,", "tokens": ["Ein", "inn\u00b7res", "Aug'", "und", "Ohr", ",", "im", "A\u00b7e\u00b7u\u00b7\u00dfern", "m\u00fc\u00b7\u00dfig", "ruhn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "$,", "APPRART", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.3": {"line.1": {"text": "Genie\u00dfen, schwelgen nur, nicht denken noch sich bilden;", "tokens": ["Ge\u00b7nie\u00b7\u00dfen", ",", "schwel\u00b7gen", "nur", ",", "nicht", "den\u00b7ken", "noch", "sich", "bil\u00b7den", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ADV", "$,", "PTKNEG", "VVINF", "KON", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit ungez\u00fcgelter Einbildungskraft verwilden.", "tokens": ["Mit", "un\u00b7ge\u00b7z\u00fc\u00b7gel\u00b7ter", "Ein\u00b7bil\u00b7dungs\u00b7kraft", "ver\u00b7wil\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Dazu helf' ihnen treu Musik und B\u00fchnenkunst,", "tokens": ["Da\u00b7zu", "hel\u00b7f'", "ih\u00b7nen", "treu", "Mu\u00b7sik", "und", "B\u00fch\u00b7nen\u00b7kunst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADJD", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.2": {"text": "Du, edle Poesie, verschm\u00e4h die schn\u00f6de Brunst!", "tokens": ["Du", ",", "ed\u00b7le", "Poe\u00b7sie", ",", "ver\u00b7schm\u00e4h", "die", "schn\u00f6\u00b7de", "Brunst", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ADJA", "NN", "$,", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.5": {"line.1": {"text": "Ob von den deinen auch sich ihnen beigeselle", "tokens": ["Ob", "von", "den", "dei\u00b7nen", "auch", "sich", "ih\u00b7nen", "bei\u00b7ge\u00b7sel\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "PPOSAT", "ADV", "PRF", "PPER", "PIAT"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "M\u00e4hrchen, Roman, Romanz', Erz\u00e4hlung und Novelle;", "tokens": ["M\u00e4hr\u00b7chen", ",", "Ro\u00b7man", ",", "Ro\u00b7manz'", ",", "Er\u00b7z\u00e4h\u00b7lung", "und", "No\u00b7vel\u00b7le", ";"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}}, "stanza.6": {"line.1": {"text": "Doch, heil'ge Lyrik, du, von wo du bist, nach oben", "tokens": ["Doch", ",", "heil'\u00b7ge", "Ly\u00b7rik", ",", "du", ",", "von", "wo", "du", "bist", ",", "nach", "o\u00b7ben"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "$,", "ADJA", "NN", "$,", "PPER", "$,", "APPR", "PWAV", "PPER", "VAFIN", "$,", "APPR", "ADV"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Deut' ihnen warnend ernst, ob sie ob nicht sie's loben.", "tokens": ["Deut'", "ih\u00b7nen", "war\u00b7nend", "ernst", ",", "ob", "sie", "ob", "nicht", "sie's", "lo\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VAFIN", "ADJD", "$,", "KOUS", "PPER", "KOUS", "PTKNEG", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}