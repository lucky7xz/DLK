{"dta.poem.12090": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "12.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Zwei B\u00e4ume sah ich heut, Sinnbilder von Verj\u00fcngung,", "tokens": ["Zwei", "B\u00e4u\u00b7me", "sah", "ich", "heut", ",", "Sinn\u00b7bil\u00b7der", "von", "Ver\u00b7j\u00fcn\u00b7gung", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "PPER", "ADV", "$,", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des Abgestorbenen lebend'ge Wiederbringung.", "tokens": ["Des", "Ab\u00b7ge\u00b7stor\u00b7be\u00b7nen", "le\u00b7ben\u00b7d'\u00b7ge", "Wie\u00b7der\u00b7brin\u00b7gung", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Ein hoher Fichtenstamm, sein Haupt vom Sturm gepfl\u00fcckt,", "tokens": ["Ein", "ho\u00b7her", "Fich\u00b7ten\u00b7stamm", ",", "sein", "Haupt", "vom", "Sturm", "ge\u00b7pfl\u00fcckt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PPOSAT", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Statt einer Krone nun mit mehreren geschm\u00fcckt.", "tokens": ["Statt", "ei\u00b7ner", "Kro\u00b7ne", "nun", "mit", "meh\u00b7re\u00b7ren", "ge\u00b7schm\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADV", "APPR", "PIAT", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Denn aus der Rinde Kraft entspro\u00dften wunderhaft", "tokens": ["Denn", "aus", "der", "Rin\u00b7de", "Kraft", "ent\u00b7spro\u00df\u00b7ten", "wun\u00b7der\u00b7haft"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "F\u00fcnf neue Fichten, schlank wie Tempels\u00e4ulenschaft.", "tokens": ["F\u00fcnf", "neu\u00b7e", "Fich\u00b7ten", ",", "schlank", "wie", "Tem\u00b7pel\u00b7s\u00e4u\u00b7len\u00b7schaft", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "$,", "VVFIN", "KOKOM", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Was, wenn der Hauptstamm blieb, nur w\u00e4r' ein Ast daran,", "tokens": ["Was", ",", "wenn", "der", "Haupt\u00b7stamm", "blieb", ",", "nur", "w\u00e4r'", "ein", "Ast", "da\u00b7ran", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "$,", "KOUS", "ART", "NN", "VVFIN", "$,", "ADV", "VAFIN", "ART", "NN", "PAV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das war ein Stamm nun selbst mit Aesten angethan.", "tokens": ["Das", "war", "ein", "Stamm", "nun", "selbst", "mit", "A\u00b7es\u00b7ten", "an\u00b7ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ADV", "ADV", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.5": {"line.1": {"text": "Und alle wuchsen so umher in einem Kranz,", "tokens": ["Und", "al\u00b7le", "wuch\u00b7sen", "so", "um\u00b7her", "in", "ei\u00b7nem", "Kranz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "ADV", "PTKVZ", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bildend ein Ganzes nur, doch jeder selber ganz;", "tokens": ["Bil\u00b7dend", "ein", "Gan\u00b7zes", "nur", ",", "doch", "je\u00b7der", "sel\u00b7ber", "ganz", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADV", "$,", "ADV", "PIS", "ADV", "ADV", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}}, "stanza.6": {"line.1": {"text": "Wie, was ein Staat einst war, nun auseinander trat", "tokens": ["Wie", ",", "was", "ein", "Staat", "einst", "war", ",", "nun", "aus\u00b7ein\u00b7an\u00b7der", "trat"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "$,", "PRELS", "ART", "NN", "ADV", "VAFIN", "$,", "ADV", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu einem Staatenbund, zu einem Bundesstaat.", "tokens": ["Zu", "ei\u00b7nem", "Staa\u00b7ten\u00b7bund", ",", "zu", "ei\u00b7nem", "Bun\u00b7des\u00b7staat", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Sodann ein Ulmenbaum, vom Alter morsch gebrochen,", "tokens": ["So\u00b7dann", "ein", "Ul\u00b7men\u00b7baum", ",", "vom", "Al\u00b7ter", "morsch", "ge\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "APPRART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vermodert ist sein Mark, und m\u00fcrbe seine Knochen.", "tokens": ["Ver\u00b7mo\u00b7dert", "ist", "sein", "Mark", ",", "und", "m\u00fcr\u00b7be", "sei\u00b7ne", "Kno\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPOSAT", "NN", "$,", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Der Moder aber ward Stockerd' auf seinem Haupt;", "tokens": ["Der", "Mo\u00b7der", "a\u00b7ber", "ward", "Sto\u00b7ckerd'", "auf", "sei\u00b7nem", "Haupt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da hat sein letzter Zweig, eh ihn der Tod entlaubt,", "tokens": ["Da", "hat", "sein", "letz\u00b7ter", "Zweig", ",", "eh", "ihn", "der", "Tod", "ent\u00b7laubt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "ADJA", "NN", "$,", "KOUS", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Selbst in den Mutterschoo\u00df den Saamen so gestreut,", "tokens": ["Selbst", "in", "den", "Mut\u00b7ter\u00b7schoo\u00df", "den", "Saa\u00b7men", "so", "ge\u00b7streut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df auf sich selbst ein Baum wuchs aus sich selbst erneut.", "tokens": ["Da\u00df", "auf", "sich", "selbst", "ein", "Baum", "wuchs", "aus", "sich", "selbst", "er\u00b7neut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PRF", "ADV", "ART", "NN", "VVFIN", "APPR", "PRF", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Der abgestorben sich im Boden unten fand,", "tokens": ["Der", "ab\u00b7ge\u00b7stor\u00b7ben", "sich", "im", "Bo\u00b7den", "un\u00b7ten", "fand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "PRF", "APPRART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "War oben Boden nun, auf dem er selbst entstand.", "tokens": ["War", "o\u00b7ben", "Bo\u00b7den", "nun", ",", "auf", "dem", "er", "selbst", "ent\u00b7stand", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "NN", "ADV", "$,", "APPR", "PRELS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.11": {"line.1": {"text": "Und angesiedelt hat sich droben ein Gemisch", "tokens": ["Und", "an\u00b7ge\u00b7sie\u00b7delt", "hat", "sich", "dro\u00b7ben", "ein", "Ge\u00b7misch"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVPP", "VAFIN", "PRF", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.2": {"text": "Von Kr\u00e4utern und Gestr\u00e4uch, Verwirrung malerisch.", "tokens": ["Von", "Kr\u00e4u\u00b7tern", "und", "Ge\u00b7str\u00e4uch", ",", "Ver\u00b7wir\u00b7rung", "ma\u00b7le\u00b7risch", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}