{"textgrid.poem.53581": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Klagelied eines Einsamen", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun sch\u00fctteln wieder Mixer an den Tischen", "tokens": ["Nun", "sch\u00fct\u00b7teln", "wie\u00b7der", "Mi\u00b7xer", "an", "den", "Ti\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "NN", "APPR", "ART", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "den blank polierten Nickeltopf mit Eis.", "tokens": ["den", "blank", "po\u00b7lier\u00b7ten", "Ni\u00b7ckel\u00b7topf", "mit", "Eis", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Glast\u00fcr geht. Die Droschkenautos zischen.", "tokens": ["Die", "Glas\u00b7t\u00fcr", "geht", ".", "Die", "Droschken\u00b7au\u00b7tos", "zi\u00b7schen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "ART", "NN", "VVINF", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Man zahlt f\u00fcr alles den Valutapreis.", "tokens": ["Man", "zahlt", "f\u00fcr", "al\u00b7les", "den", "Va\u00b7lu\u00b7ta\u00b7preis", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "PIS", "ART", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Musik steigt auf. Auf pl\u00fcschbelegten Treppen", "tokens": ["Mu\u00b7sik", "steigt", "auf", ".", "Auf", "pl\u00fcschbe\u00b7leg\u00b7ten", "Trep\u00b7pen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "PTKVZ", "$.", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "l\u00e4\u00dft sich der Zigarettenmann aus Frankfurt neppen.", "tokens": ["l\u00e4\u00dft", "sich", "der", "Zi\u00b7ga\u00b7ret\u00b7ten\u00b7mann", "aus", "Frank\u00b7furt", "nep\u00b7pen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "APPR", "NE", "VVINF", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.7": {"text": "Um ein Uhr kommt die gr\u00fcne Polizei . . .", "tokens": ["Um", "ein", "Uhr", "kommt", "die", "gr\u00fc\u00b7ne", "Po\u00b7li\u00b7zei", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["KOUI", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$.", "$.", "$."], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.8": {"text": "Und ich bin nicht dabei \u2013!", "tokens": ["Und", "ich", "bin", "nicht", "da\u00b7bei", "\u2013", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPER", "VAFIN", "PTKNEG", "PAV", "$(", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Auf der Estrade steht im Reichstagssaale", "tokens": ["Auf", "der", "Est\u00b7ra\u00b7de", "steht", "im", "Reichs\u00b7tags\u00b7saa\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "APPRART", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "ein Vollbartgreis im Gehrock \u2013 und er schwitzt.", "tokens": ["ein", "Voll\u00b7bart\u00b7greis", "im", "Ge\u00b7hrock", "\u2013", "und", "er", "schwitzt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$(", "KON", "PPER", "VVFIN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ein freier Mann \u2013 jedoch das Nationale", "tokens": ["Ein", "frei\u00b7er", "Mann", "\u2013", "je\u00b7doch", "das", "Na\u00b7ti\u00b7o\u00b7na\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$(", "ADV", "ART", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "h\u00e4lt er steil in die H\u00f6h \u2013 der Speichel spritzt.", "tokens": ["h\u00e4lt", "er", "steil", "in", "die", "H\u00f6h", "\u2013", "der", "Spei\u00b7chel", "spritzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$(", "ART", "NN", "VVFIN", "$."], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Die H\u00f6rerschaft sp\u00fcrt zwischen Schlaf und Wachen:", "tokens": ["Die", "H\u00f6\u00b7rer\u00b7schaft", "sp\u00fcrt", "zwi\u00b7schen", "Schlaf", "und", "Wa\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "man mu\u00df \u2013 zur Volkswohlfahrt \u2013 Gesch\u00e4fte machen.", "tokens": ["man", "mu\u00df", "\u2013", "zur", "Volks\u00b7wohl\u00b7fahrt", "\u2013", "Ge\u00b7sch\u00e4f\u00b7te", "ma\u00b7chen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "$(", "APPRART", "NN", "$(", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und man verteilt die Posten, die noch frei . . .", "tokens": ["Und", "man", "ver\u00b7teilt", "die", "Pos\u00b7ten", ",", "die", "noch", "frei", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct", "punct"], "pos": ["KON", "PIS", "VVFIN", "ART", "NN", "$,", "PRELS", "ADV", "ADJD", "$.", "$.", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und ich bin nicht dabei \u2013!", "tokens": ["Und", "ich", "bin", "nicht", "da\u00b7bei", "\u2013", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPER", "VAFIN", "PTKNEG", "PAV", "$(", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Liane strahlt. Sie ist nur schwach bekleidet:", "tokens": ["Li\u00b7a\u00b7ne", "strahlt", ".", "Sie", "ist", "nur", "schwach", "be\u00b7klei\u00b7det", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$.", "PPER", "VAFIN", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "die Armbanduhr schm\u00fcckt glitzernd ihr Gelenk.", "tokens": ["die", "Arm\u00b7ban\u00b7duhr", "schm\u00fcckt", "glit\u00b7zernd", "ihr", "Ge\u00b7lenk", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Worum sie manche Frau so sehr beneidet,", "tokens": ["Wo\u00b7rum", "sie", "man\u00b7che", "Frau", "so", "sehr", "be\u00b7nei\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PIAT", "NN", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "beut sie den lieben G\u00e4sten als Geschenk.", "tokens": ["beut", "sie", "den", "lie\u00b7ben", "G\u00e4s\u00b7ten", "als", "Ge\u00b7schenk", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "KOUS", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.5": {"text": "Wei\u00df hebt die Haut sich ab von gr\u00fcnem Rupfen.", "tokens": ["Wei\u00df", "hebt", "die", "Haut", "sich", "ab", "von", "gr\u00fc\u00b7nem", "Rup\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "PRF", "PTKVZ", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Man sieht zwei Herrn ein Kokainchen schnupfen.", "tokens": ["Man", "sieht", "zwei", "Herrn", "ein", "Ko\u00b7ka\u00b7in\u00b7chen", "schnup\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "CARD", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Sie tanzt. Ganz leise haucht ein kleiner Schrei . . .", "tokens": ["Sie", "tanzt", ".", "Ganz", "lei\u00b7se", "haucht", "ein", "klei\u00b7ner", "Schrei", ".", ".", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PPER", "VVFIN", "$.", "ADV", "ADJD", "VVFIN", "ART", "ADJA", "NN", "$.", "$.", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und ich bin nicht dabei \u2013!", "tokens": ["Und", "ich", "bin", "nicht", "da\u00b7bei", "\u2013", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPER", "VAFIN", "PTKNEG", "PAV", "$(", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Im Kino huscht die Diva auf der Leinwand.", "tokens": ["Im", "Ki\u00b7no", "huscht", "die", "Di\u00b7va", "auf", "der", "Lein\u00b7wand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein Riesenauge, glotzt das Publikum.", "tokens": ["Ein", "Rie\u00b7sen\u00b7au\u00b7ge", ",", "glotzt", "das", "Pub\u00b7li\u00b7kum", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Kohlennot ist f\u00fcr dies Fach kein Einwand.", "tokens": ["Die", "Koh\u00b7len\u00b7not", "ist", "f\u00fcr", "dies", "Fach", "kein", "Ein\u00b7wand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "PDS", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Laut ist die Stadt und leider haltlos dumm . . .", "tokens": ["Laut", "ist", "die", "Stadt", "und", "lei\u00b7der", "halt\u00b7los", "dumm", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["APPR", "VAFIN", "ART", "NN", "KON", "ADV", "ADJD", "ADJD", "$.", "$.", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.5": {"text": "Da drau\u00dfen schwankt ein Weidenbusch im Winde.", "tokens": ["Da", "drau\u00b7\u00dfen", "schwankt", "ein", "Wei\u00b7den\u00b7busch", "im", "Win\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ein alter Herr tr\u00e4umt unter einer Linde,", "tokens": ["Ein", "al\u00b7ter", "Herr", "tr\u00e4umt", "un\u00b7ter", "ei\u00b7ner", "Lin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "NE", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "wer heut zum Skat noch einzuladen sei . . .", "tokens": ["wer", "heut", "zum", "Skat", "noch", "ein\u00b7zu\u00b7la\u00b7den", "sei", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PWS", "ADV", "APPRART", "NN", "ADV", "VVIZU", "VAFIN", "$.", "$.", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und ich bin nicht dabei \u2013!", "tokens": ["Und", "ich", "bin", "nicht", "da\u00b7bei", "\u2013", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPER", "VAFIN", "PTKNEG", "PAV", "$(", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "noch nicht dabei!", "tokens": ["noch", "nicht", "da\u00b7bei", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "PAV", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Nun sch\u00fctteln wieder Mixer an den Tischen", "tokens": ["Nun", "sch\u00fct\u00b7teln", "wie\u00b7der", "Mi\u00b7xer", "an", "den", "Ti\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "NN", "APPR", "ART", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "den blank polierten Nickeltopf mit Eis.", "tokens": ["den", "blank", "po\u00b7lier\u00b7ten", "Ni\u00b7ckel\u00b7topf", "mit", "Eis", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Glast\u00fcr geht. Die Droschkenautos zischen.", "tokens": ["Die", "Glas\u00b7t\u00fcr", "geht", ".", "Die", "Droschken\u00b7au\u00b7tos", "zi\u00b7schen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "ART", "NN", "VVINF", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Man zahlt f\u00fcr alles den Valutapreis.", "tokens": ["Man", "zahlt", "f\u0