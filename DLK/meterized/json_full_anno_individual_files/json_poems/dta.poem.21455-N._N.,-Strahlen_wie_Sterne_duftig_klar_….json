{"dta.poem.21455": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Strahlen wie Sterne duftig klar \u2026", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Strahlen wie Sterne duftigklar", "tokens": ["Strah\u00b7len", "wie", "Ster\u00b7ne", "duf\u00b7tig\u00b7klar"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KOKOM", "NN", "ADJD"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Mir deine Augen, die tiefblauen,", "tokens": ["Mir", "dei\u00b7ne", "Au\u00b7gen", ",", "die", "tief\u00b7blau\u00b7en", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Zieht mich ein Sehnen wunderbar", "tokens": ["Zieht", "mich", "ein", "Seh\u00b7nen", "wun\u00b7der\u00b7bar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ART", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nach selig-fernen Himmelsauen.", "tokens": ["Nach", "se\u00b7lig\u00b7fer\u00b7nen", "Him\u00b7mel\u00b7sau\u00b7en", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}, "stanza.2": {"line.1": {"text": "Die echte Liebe treu und wahr,", "tokens": ["Die", "ech\u00b7te", "Lie\u00b7be", "treu", "und", "wahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "W\u00e4hn\u2019 ich auf sonnigem Grund zu schauen,", "tokens": ["W\u00e4hn'", "ich", "auf", "son\u00b7ni\u00b7gem", "Grund", "zu", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "Als kennt\u2019 ich dich schon viele Jahr\u2019,", "tokens": ["Als", "kennt'", "ich", "dich", "schon", "vie\u00b7le", "Jahr'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "PRF", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "M\u00f6cht\u2019 ich die Seele dir vertrauen.", "tokens": ["M\u00f6cht'", "ich", "die", "See\u00b7le", "dir", "ver\u00b7trau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Reich\u2019 mir zum Ku\u00df die Lippen dar,", "tokens": ["Reich'", "mir", "zum", "Ku\u00df", "die", "Lip\u00b7pen", "dar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPRART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "O runzle nicht die zarten Brauen,", "tokens": ["O", "runz\u00b7le", "nicht", "die", "zar\u00b7ten", "Brau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Umduftet s\u00fc\u00df von deinem Haar,", "tokens": ["Um\u00b7duf\u00b7tet", "s\u00fc\u00df", "von", "dei\u00b7nem", "Haar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "La\u00df Frieden auf mich niederthauen.", "tokens": ["La\u00df", "Frie\u00b7den", "auf", "mich", "nie\u00b7dert\u00b7hau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ich bin so gl\u00fcck- und trostesbar \u2026", "tokens": ["Ich", "bin", "so", "gl\u00fc\u00b7ck", "und", "tros\u00b7tes\u00b7bar", "\u2026"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "TRUNC", "KON", "ADJD", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "O maiensch\u00f6nste aller Frauen", "tokens": ["O", "mai\u00b7en\u00b7sch\u00f6ns\u00b7te", "al\u00b7ler", "Frau\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sei du dem fl\u00fcgellahmen Aar", "tokens": ["Sei", "du", "dem", "fl\u00fc\u00b7gel\u00b7lah\u00b7men", "Aar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Sonne in der Nebel Grauen! \u2026", "tokens": ["Die", "Son\u00b7ne", "in", "der", "Ne\u00b7bel", "Grau\u00b7en", "!", "\u2026"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}