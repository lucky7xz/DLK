{"textgrid.poem.52902": {"metadata": {"author": {"name": "Dingelstedt, Franz von", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1847, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hinaus, hinaus aus diesen kalten Steinen,", "tokens": ["Hin\u00b7aus", ",", "hin\u00b7aus", "aus", "die\u00b7sen", "kal\u00b7ten", "Stei\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hinweg von den ver\u00f6deten Arkaden!", "tokens": ["Hin\u00b7weg", "von", "den", "ver\u00b7\u00f6\u00b7de\u00b7ten", "Ar\u00b7ka\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es lockt der Mai auf den Kastanien-Pfaden", "tokens": ["Es", "lockt", "der", "Mai", "auf", "den", "Kas\u00b7ta\u00b7ni\u00b7en\u00b7Pfa\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Zu sich zur\u00fcck die bildersatten Seinen.", "tokens": ["Zu", "sich", "zu\u00b7r\u00fcck", "die", "bil\u00b7der\u00b7sat\u00b7ten", "Sei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wie bleich, wie welk nun jene Farben scheinen,", "tokens": ["Wie", "bleich", ",", "wie", "welk", "nun", "je\u00b7ne", "Far\u00b7ben", "schei\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "$,", "PWAV", "ADJD", "ADV", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zum Himmelsblau und Gr\u00fcn der Promenaden,", "tokens": ["Zum", "Him\u00b7mels\u00b7blau", "und", "Gr\u00fcn", "der", "Pro\u00b7me\u00b7na\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie stumm zu den aufjubelnden Rouladen,", "tokens": ["Wie", "stumm", "zu", "den", "auf\u00b7ju\u00b7beln\u00b7den", "Rou\u00b7la\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-++-++-", "measure": "unknown.measure.hexa"}, "line.4": {"text": "Die aus den B\u00fcschen klingen, aus den Hainen!", "tokens": ["Die", "aus", "den", "B\u00fc\u00b7schen", "klin\u00b7gen", ",", "aus", "den", "Hai\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVINF", "$,", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "So eben h\u00f6rt' ich aus den lichten Bl\u00e4ttern,", "tokens": ["So", "e\u00b7ben", "h\u00f6rt'", "ich", "aus", "den", "lich\u00b7ten", "Bl\u00e4t\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die \u00e4ngstlich noch als k\u00f6nigliche knistern,", "tokens": ["Die", "\u00e4ngst\u00b7lich", "noch", "als", "k\u00f6\u00b7nig\u00b7li\u00b7che", "knis\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADV", "KOUS", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Vernehmlich eines Sprossers Kehle schmettern:", "tokens": ["Ver\u00b7nehm\u00b7lich", "ei\u00b7nes", "Spros\u00b7sers", "Keh\u00b7le", "schmet\u00b7tern", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Die Kunst ist fremd und tot und ohne Seele,", "tokens": ["Die", "Kunst", "ist", "fremd", "und", "tot", "und", "oh\u00b7ne", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "ADJD", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Kann sich die Freiheit ihr nicht treu verschwistern;", "tokens": ["Kann", "sich", "die", "Frei\u00b7heit", "ihr", "nicht", "treu", "ver\u00b7schwis\u00b7tern", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "ART", "NN", "PPER", "PTKNEG", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das ist, als ob dem Mai die Stimme fehle!", "tokens": ["Das", "ist", ",", "als", "ob", "dem", "Mai", "die", "Stim\u00b7me", "feh\u00b7le", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "$,", "KOKOM", "KOUS", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Hinaus, hinaus aus diesen kalten Steinen,", "tokens": ["Hin\u00b7aus", ",", "hin\u00b7aus", "aus", "die\u00b7sen", "kal\u00b7ten", "Stei\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-"