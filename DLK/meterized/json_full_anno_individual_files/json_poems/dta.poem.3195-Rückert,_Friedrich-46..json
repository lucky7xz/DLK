{"dta.poem.3195": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "46.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie oft verirrtest du, wie oft verirrst du noch,", "tokens": ["Wie", "oft", "ver\u00b7irr\u00b7test", "du", ",", "wie", "oft", "ver\u00b7irrst", "du", "noch", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "PPER", "$,", "PWAV", "ADV", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und kommst zu einem Ziel mit allem Irren doch.", "tokens": ["Und", "kommst", "zu", "ei\u00b7nem", "Ziel", "mit", "al\u00b7lem", "Ir\u00b7ren", "doch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "APPR", "PIS", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Nicht sei entschuldiget dein Irregehn, gepriesen", "tokens": ["Nicht", "sei", "ent\u00b7schul\u00b7di\u00b7get", "dein", "Ir\u00b7re\u00b7gehn", ",", "ge\u00b7prie\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["PTKNEG", "VAFIN", "VVFIN", "PPOSAT", "NN", "$,", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sei einzig Gottes Macht, die dich zurecht gewiesen.", "tokens": ["Sei", "ein\u00b7zig", "Got\u00b7tes", "Macht", ",", "die", "dich", "zu\u00b7recht", "ge\u00b7wie\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "NN", "NN", "$,", "PRELS", "PPER", "PTKVZ", "VAPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alex