{"textgrid.poem.40390": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "Ein Ackersmann", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich kann ein Ackersmann werden;", "tokens": ["Ich", "kann", "ein", "A\u00b7ckers\u00b7mann", "wer\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "VAINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "auch der mu\u00df tapfer sein.", "tokens": ["auch", "der", "mu\u00df", "tap\u00b7fer", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "VMFIN", "ADJD", "VAINF", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Mit Himmel und Erde mu\u00df er k\u00e4mpfen,", "tokens": ["Mit", "Him\u00b7mel", "und", "Er\u00b7de", "mu\u00df", "er", "k\u00e4mp\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "da\u00df seine Felder gedeihn,", "tokens": ["da\u00df", "sei\u00b7ne", "Fel\u00b7der", "ge\u00b7deihn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "ein Kriegsmann Schritt f\u00fcr Schritt.", "tokens": ["ein", "Kriegs\u00b7mann", "Schritt", "f\u00fcr", "Schritt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Um Haus, Hof, Heimat k\u00e4mpft er,", "tokens": ["Um", "Haus", ",", "Hof", ",", "Hei\u00b7mat", "k\u00e4mpft", "er", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "$,", "NN", "$,", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "potz Hagel, Blitz und Brand!", "tokens": ["potz", "Ha\u00b7gel", ",", "Blitz", "und", "Brand", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit Gleichmut ist sein Herz gepanzert,", "tokens": ["Mit", "Gleich\u00b7mut", "ist", "sein", "Herz", "ge\u00b7pan\u00b7zert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "mit Schwielen seine Hand,", "tokens": ["mit", "Schwie\u00b7len", "sei\u00b7ne", "Hand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "hart wie das Korn, das er s\u00e4t.", "tokens": ["hart", "wie", "das", "Korn", ",", "das", "er", "s\u00e4t", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.3": {"line.1": {"text": "Und will's daheim nicht fruchten,", "tokens": ["Und", "will's", "da\u00b7heim", "nicht", "fruch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "um Deutschland geht kein Zaun;", "tokens": ["um", "Deutschland", "geht", "kein", "Zaun", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "PIAT", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "noch manchen Urwald gibt's zu lichten,", "tokens": ["noch", "man\u00b7chen", "Ur\u00b7wald", "gibt's", "zu", "lich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VVFIN", "APPR", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "da kann man Blockh\u00fctten baun", "tokens": ["da", "kann", "man", "Block\u00b7h\u00fct\u00b7ten", "baun"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PIS", "NN", "VVINF"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "und neue Heimat schaffen.", "tokens": ["und", "neu\u00b7e", "Hei\u00b7mat", "schaf\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Vielleicht st\u00f6\u00dft doch das Heimweh", "tokens": ["Viel\u00b7leicht", "st\u00f6\u00dft", "doch", "das", "Heim\u00b7weh"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "langsam das Herz ihm ab?", "tokens": ["lang\u00b7sam", "das", "Herz", "ihm", "ab", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Dann aber rauschen die \u00c4hren", "tokens": ["Dann", "a\u00b7ber", "rau\u00b7schen", "die", "\u00c4h\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "weithin um sein Grab:", "tokens": ["weit\u00b7hin", "um", "sein", "Grab", ":"], "token_info": ["word", "word", "word", "word", "punct"], "po