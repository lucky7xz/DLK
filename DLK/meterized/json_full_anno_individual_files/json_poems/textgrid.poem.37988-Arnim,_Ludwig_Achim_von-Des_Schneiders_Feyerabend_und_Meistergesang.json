ord", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "NN", "PTKVZ", "$,"], "meter": "++-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Sie warf mir armen Schneider", "tokens": ["Sie", "warf", "mir", "ar\u00b7men", "Schnei\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADJA", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Ins Glas ihr Fingerlein.", "tokens": ["Ins", "Glas", "ihr", "Fin\u00b7ger\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.17": {"line.1": {"text": "Nun sitze ich in meiner Zell und sing", "tokens": ["Nun", "sit\u00b7ze", "ich", "in", "mei\u00b7ner", "Zell", "und", "sing"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "KON", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und leere meinen Becher,", "tokens": ["Und", "lee\u00b7re", "mei\u00b7nen", "Be\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da klingt der Buhlen Ring.", "tokens": ["Da", "klingt", "der", "Buh\u00b7len", "Ring", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Den Alten sing ich vor,", "tokens": ["Den", "Al\u00b7ten", "sing", "ich", "vor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sie schlafen nickend ein,", "tokens": ["Sie", "schla\u00b7fen", "ni\u00b7ckend", "ein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Mein Lieb nimmt ihren Faden,", "tokens": ["Mein", "Lieb", "nimmt", "ih\u00b7ren", "Fa\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Spinnt alte Zeit hinein,", "tokens": ["Spinnt", "al\u00b7te", "Zeit", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Und spinnt mir armen Schneider,", "tokens": ["Und", "spinnt", "mir", "ar\u00b7men", "Schnei\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJA", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Ein Brauthemd obendrein.", "tokens": ["Ein", "Brau\u00b7themd", "o\u00b7ben\u00b7drein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.18": {"line.1": {"text": "Die Alte, die zum Fenster naus nun knappet mit der Scheer,", "tokens": ["Die", "Al\u00b7te", ",", "die", "zum", "Fens\u00b7ter", "naus", "nun", "knap\u00b7pet", "mit", "der", "Scheer", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPRART", "NN", "ADV", "ADV", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Die ist der Werkstadt Zeichen,", "tokens": ["Die", "ist", "der", "Werk\u00b7stadt", "Zei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Lockt gut Gesellen her.", "tokens": ["Lockt", "gut", "Ge\u00b7sel\u00b7len", "her", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "NN", "PTKVZ", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Ich singe ihnen vor,", "tokens": ["Ich", "sin\u00b7ge", "ih\u00b7nen", "vor", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wie doch der grimme Tod", "tokens": ["Wie", "doch", "der", "grim\u00b7me", "Tod"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Nur sey ein B\u00e4renh\u00e4uter,", "tokens": ["Nur", "sey", "ein", "B\u00e4\u00b7ren\u00b7h\u00e4u\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Vor Sang und Streit, und Gott,", "tokens": ["Vor", "Sang", "und", "Streit", ",", "und", "Gott", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Das bracht mich frommen Schneider", "tokens": ["Das", "bracht", "mich", "from\u00b7men", "Schnei\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "ADJA", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Wohl wieder an das Brod.", "tokens": ["Wohl", "wie\u00b7der", "an", "das", "Brod", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}