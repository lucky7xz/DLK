{"textgrid.poem.37988": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Des Schneiders Feyerabend und Meistergesang", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und als ich sa\u00df in meiner Zell und schreib,", "tokens": ["Und", "als", "ich", "sa\u00df", "in", "mei\u00b7ner", "Zell", "und", "schreib", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Da kamen drey Beginnen", "tokens": ["Da", "ka\u00b7men", "drey", "Be\u00b7gin\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "CARD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So alte heil'ge Weib.", "tokens": ["So", "al\u00b7te", "heil'\u00b7ge", "Weib", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Sie lasen mir vor", "tokens": ["Sie", "la\u00b7sen", "mir", "vor"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Den schnellen grimmen Tod.", "tokens": ["Den", "schnel\u00b7len", "grim\u00b7men", "Tod", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Ich bin ein armer Schneider,", "tokens": ["Ich", "bin", "ein", "ar\u00b7mer", "Schnei\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und leid' es wohl durch Gott,", "tokens": ["Und", "leid'", "es", "wohl", "durch", "Gott", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Da hatt ich armer Schneider", "tokens": ["Da", "hatt", "ich", "ar\u00b7mer", "Schnei\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADJA", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "F\u00fcr sie und mich kein Brod.", "tokens": ["F\u00fcr", "sie", "und", "mich", "kein", "Brod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "KON", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die Erste spann, den Faden dreht die Zweyt,", "tokens": ["Die", "Ers\u00b7te", "spann", ",", "den", "Fa\u00b7den", "dreht", "die", "Zweyt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "$,", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Dritte hielt die Scheere", "tokens": ["Die", "Drit\u00b7te", "hielt", "die", "Schee\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Zum Schneiden schon bereit,", "tokens": ["Zum", "Schnei\u00b7den", "schon", "be\u00b7reit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Sie lasen mir vor:", "tokens": ["Sie", "la\u00b7sen", "mir", "vor", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Zum schnellen grimmen Tod", "tokens": ["Zum", "schnel\u00b7len", "grim\u00b7men", "Tod"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Bereit dich armer Schneider,", "tokens": ["Be\u00b7reit", "dich", "ar\u00b7mer", "Schnei\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJA", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Das Sterben thut dir Noth,", "tokens": ["Das", "Ster\u00b7ben", "thut", "dir", "Noth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Dieweil du armer Schneider", "tokens": ["Die\u00b7weil", "du", "ar\u00b7mer", "Schnei\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJA", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "In deinem Sack kein Brod.", "tokens": ["In", "dei\u00b7nem", "Sack", "kein", "Brod", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und als ich hungrig sa\u00df in meiner Zell und schreib,", "tokens": ["Und", "als", "ich", "hung\u00b7rig", "sa\u00df", "in", "mei\u00b7ner", "Zell", "und", "schreib", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJD", "VVFIN", "APPR", "PPOSAT", "NN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da stiegen durch die Decke", "tokens": ["Da", "stie\u00b7gen", "durch", "die", "De\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Drey junge sch\u00f6ne Weib,", "tokens": ["Drey", "jun\u00b7ge", "sch\u00f6\u00b7ne", "Weib", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Sie sangen mir vor", "tokens": ["Sie", "san\u00b7gen", "mir", "vor"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Wohl von der Ewigkeit,", "tokens": ["Wohl", "von", "der", "E\u00b7wig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.6": {"text": "Da h\u00e4tt ich armer Schneider", "tokens": ["Da", "h\u00e4tt", "ich", "ar\u00b7mer", "Schnei\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADJA", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Noch lange lange Zeit.", "tokens": ["Noch", "lan\u00b7ge", "lan\u00b7ge", "Zeit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Gebt Brod mir armen Schneider,", "tokens": ["Gebt", "Brod", "mir", "ar\u00b7men", "Schnei\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "PPER", "ADJA", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Mein Weg ist noch gar weit.", "tokens": ["Mein", "Weg", "ist", "noch", "gar", "weit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Der Erste trug ein Speer, ein Saitenspiel die Zweyt,", "tokens": ["Der", "Ers\u00b7te", "trug", "ein", "Speer", ",", "ein", "Sai\u00b7ten\u00b7spiel", "die", "Zweyt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "ART", "NN", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Dritt ein Lorbeerzweig,", "tokens": ["Die", "Dritt", "ein", "Lor\u00b7beer\u00b7zweig", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das war die Ewigkeit.", "tokens": ["Das", "war", "die", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die erste sang mir vor:", "tokens": ["Die", "ers\u00b7te", "sang", "mir", "vor", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "\u00bbder Speer in gutem Streit,", "tokens": ["\u00bb", "der", "Speer", "in", "gu\u00b7tem", "Streit", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Der tr\u00e4gt das Lorbeerzweiglein,", "tokens": ["Der", "tr\u00e4gt", "das", "Lor\u00b7beer\u00b7zwei\u00b7glein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Der tr\u00e4gt die Ewigkeit!\u00ab", "tokens": ["Der", "tr\u00e4gt", "die", "E\u00b7wig\u00b7keit", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "O h\u00e4tt ich armer Schneider", "tokens": ["O", "h\u00e4tt", "ich", "ar\u00b7mer", "Schnei\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "PPER", "ADJA", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Ein St\u00e4rkung in dem Streit.", "tokens": ["Ein", "St\u00e4r\u00b7kung", "in", "dem", "Streit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Des z\u00fcrnt die alte Katz und knappet mit der Scheer,", "tokens": ["Des", "z\u00fcrnt", "die", "al\u00b7te", "Katz", "und", "knap\u00b7pet", "mit", "der", "Scheer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVFIN", "ART", "ADJA", "NN", "KON", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da steckt ich sie zum Fenster naus,", "tokens": ["Da", "steckt", "ich", "sie", "zum", "Fens\u00b7ter", "naus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auf meinem guten Speer,", "tokens": ["Auf", "mei\u00b7nem", "gu\u00b7ten", "Speer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Da las ich ihr vor:", "tokens": ["Da", "las", "ich", "ihr", "vor", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "\u00bbdein schneller grimmer Tod,", "tokens": ["\u00bb", "dein", "schnel\u00b7ler", "grim\u00b7mer", "Tod", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Trifft nicht mich tapfern Schneider,", "tokens": ["Trifft", "nicht", "mich", "tap\u00b7fern", "Schnei\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "PPER", "ADJA", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ich fechte wohl um Gott,\u00ab", "tokens": ["Ich", "fech\u00b7te", "wohl", "um", "Gott", ",", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "NN", "$,", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Wer giebt mir m\u00fcden Schneider", "tokens": ["Wer", "giebt", "mir", "m\u00fc\u00b7den", "Schnei\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Zur St\u00e4rkung nun ein Brod.", "tokens": ["Zur", "St\u00e4r\u00b7kung", "nun", "ein", "Brod", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Da reichte mir die Dritt das Lorbeerzweigelein,", "tokens": ["Da", "reich\u00b7te", "mir", "die", "Dritt", "das", "Lor\u00b7beer\u00b7zwei\u00b7ge\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mein Haupt das war zu dicke,", "tokens": ["Mein", "Haupt", "das", "war", "zu", "di\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PDS", "VAFIN", "APPR", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Lorbeer war zu klein.", "tokens": ["Der", "Lor\u00b7beer", "war", "zu", "klein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKA", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die Zweyte sang mir vor:", "tokens": ["Die", "Zwey\u00b7te", "sang", "mir", "vor", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "\u00bbh\u00e4tst du die Harfe mein,", "tokens": ["\u00bb", "h\u00e4tst", "du", "die", "Har\u00b7fe", "mein", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "PPER", "ART", "NN", "PPOSAT", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Es m\u00fcst' der Kranz sich weiten,", "tokens": ["Es", "m\u00fcst'", "der", "Kranz", "sich", "wei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Schl\u00fcg' Gottes Finger drein!\u00ab", "tokens": ["Schl\u00fcg'", "Got\u00b7tes", "Fin\u00b7ger", "drein", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "NN", "NN", "PTKVZ", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Ach h\u00e4tt ich armer Schneider", "tokens": ["Ach", "h\u00e4tt", "ich", "ar\u00b7mer", "Schnei\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ITJ", "VAFIN", "PPER", "ADJA", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Ein Tr\u00fcnklein rheinschen Wein.", "tokens": ["Ein", "Tr\u00fcn\u00b7klein", "rhein\u00b7schen", "Wein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Da trat in meine Zell ein sch\u00f6nes Jungfr\u00e4ulein,", "tokens": ["Da", "trat", "in", "mei\u00b7ne", "Zell", "ein", "sch\u00f6\u00b7nes", "Jung\u00b7fr\u00e4u\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was trug sie auf den H\u00e4nden?", "tokens": ["Was", "trug", "sie", "auf", "den", "H\u00e4n\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein Becher Gotteswein.", "tokens": ["Ein", "Be\u00b7cher", "Got\u00b7tes\u00b7wein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Der sang ich wohl vor,", "tokens": ["Der", "sang", "ich", "wohl", "vor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Mein Harfe klang auch rein,", "tokens": ["Mein", "Har\u00b7fe", "klang", "auch", "rein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Der Lorbeer th\u00e4t sich breiten,", "tokens": ["Der", "Lor\u00b7beer", "th\u00e4t", "sich", "brei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ADJA", "