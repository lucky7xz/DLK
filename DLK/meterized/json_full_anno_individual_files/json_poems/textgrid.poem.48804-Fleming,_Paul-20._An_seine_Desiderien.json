{"textgrid.poem.48804": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "20. An seine Desiderien", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach ", "tokens": ["Ach"], "token_info": ["word"], "pos": ["ITJ"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "den ich mit dir getan, da\u00df ich so nach dir denke", "tokens": ["den", "ich", "mit", "dir", "ge\u00b7tan", ",", "da\u00df", "ich", "so", "nach", "dir", "den\u00b7ke"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "PPER", "VVPP", "$,", "KOUS", "PPER", "ADV", "APPR", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "und, weil du nicht bist da, mich sehr und herzlich kr\u00e4nke,", "tokens": ["und", ",", "weil", "du", "nicht", "bist", "da", ",", "mich", "sehr", "und", "herz\u00b7lich", "kr\u00e4n\u00b7ke", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPER", "PTKNEG", "VAFIN", "ADV", "$,", "PPER", "ADV", "KON", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "das macht der erste Tanz, da deiner Augen Glanz,", "tokens": ["das", "macht", "der", "ers\u00b7te", "Tanz", ",", "da", "dei\u00b7ner", "Au\u00b7gen", "Glanz", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "ADJA", "NN", "$,", "KOUS", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "der auch die Sonne trotzt, mich mir geraubet ganz.", "tokens": ["der", "auch", "die", "Son\u00b7ne", "trotzt", ",", "mich", "mir", "ge\u00b7rau\u00b7bet", "ganz", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "VVFIN", "$,", "PPER", "PPER", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Itzt, da ich mich vorhin selbst in die Grube senke,", "tokens": ["Itzt", ",", "da", "ich", "mich", "vor\u00b7hin", "selbst", "in", "die", "Gru\u00b7be", "sen\u00b7ke", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "PRF", "ADV", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-++-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "machst du mir noch mehr Not durch dieses dein Geschenke,", "tokens": ["machst", "du", "mir", "noch", "mehr", "Not", "durch", "die\u00b7ses", "dein", "Ge\u00b7schen\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "PIAT", "NN", "APPR", "PDAT", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "in dem dein Atem lebt, durch diesen Rosenkranz.", "tokens": ["in", "dem", "dein", "A\u00b7tem", "lebt", ",", "durch", "die\u00b7sen", "Ro\u00b7sen\u00b7kranz", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PPOSAT", "NN", "VVFIN", "$,", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Komm, mein Verlangen, komm, wie du mir denn beineben", "tokens": ["Komm", ",", "mein", "Ver\u00b7lan\u00b7gen", ",", "komm", ",", "wie", "du", "mir", "denn", "bei\u00b7ne\u00b7ben"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "PPOSAT", "NN", "$,", "VVFIN", "$,", "PWAV", "PPER", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-+-+---", "measure": "unknown.measure.penta"}, "line.2": {"text": "durch eine stille Post l\u00e4\u00dft zu vernehmen geben,", "tokens": ["durch", "ei\u00b7ne", "stil\u00b7le", "Post", "l\u00e4\u00dft", "zu", "ver\u00b7neh\u00b7men", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "PTKZU", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "komm, mein Verlangen, komm: Ich bin schon, wo du wilt,", "tokens": ["komm", ",", "mein", "Ver\u00b7lan\u00b7gen", ",", "komm", ":", "Ich", "bin", "schon", ",", "wo", "du", "wilt", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPOSAT", "NN", "$,", "VVFIN", "$.", "PPER", "VAFIN", "ADV", "$,", "PWAV", "PPER", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "wo Chloris Blumen streut, Pomona \u00d6pfel bringet,", "tokens": ["wo", "Chlo\u00b7ris", "Blu\u00b7men", "streut", ",", "Po\u00b7mo\u00b7na", "\u00d6p\u00b7fel", "brin\u00b7get", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "NN", "VVFIN", "$,", "NE", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "wo um das Lusthaus her die Schaar der V\u00f6gel singet", "tokens": ["wo", "um", "das", "Lust\u00b7haus", "her", "die", "Schaar", "der", "V\u00f6\u00b7gel", "sin\u00b7get"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "APZR", "ART", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "