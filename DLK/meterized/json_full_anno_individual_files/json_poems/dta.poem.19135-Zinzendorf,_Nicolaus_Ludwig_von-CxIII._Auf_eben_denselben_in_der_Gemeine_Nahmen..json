{"dta.poem.19135": {"metadata": {"author": {"name": "Zinzendorf, Nicolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "CxIII.  Auf eben denselben in der Gemeine  \n Nahmen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20688-6", "language": ["de:0.99"], "booktitle": "Zinzendorf, Nicolaus Ludwig von: Teutscher Gedichte Erster Theil. Herrnhuth, 1735."}, "poem": {"stanza.1": {"line.1": {"text": "Der Geist der Aeltesten kam pl\u00f6tzlich \u00fcber dich,", "tokens": ["Der", "Geist", "der", "A\u00b7el\u00b7tes\u00b7ten", "kam", "pl\u00f6tz\u00b7lich", "\u00fc\u00b7ber", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "ADJD", "APPR", "PPER", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Du Ehr-erbietiglich geliebter Hertzens-Bruder!", "tokens": ["Du", "Ehr\u00b7er\u00b7bie\u00b7tig\u00b7lich", "ge\u00b7lieb\u00b7ter", "Hert\u00b7zens\u00b7Bru\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Er leitete dich recht, und treu und m\u00e4\u00dfiglich;", "tokens": ["Er", "lei\u00b7te\u00b7te", "dich", "recht", ",", "und", "treu", "und", "m\u00e4\u00b7\u00dfig\u00b7lich", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "$,", "KON", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Drum sahen wir genau auf dich an unsrem Ruder.", "tokens": ["Drum", "sa\u00b7hen", "wir", "ge\u00b7nau", "auf", "dich", "an", "uns\u00b7rem", "Ru\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADJD", "APPR", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Herr, HErr und Ober-Haupt und W\u00e4chter unsrer Huth!", "tokens": ["Herr", ",", "Herr", "und", "O\u00b7ber\u00b7Haupt", "und", "W\u00e4ch\u00b7ter", "uns\u00b7rer", "Huth", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "KON", "NE", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Nimm mit Gewinn und Danck dein wohl-gebrauchtes Guth.", "tokens": ["Nimm", "mit", "Ge\u00b7winn", "und", "Danck", "dein", "wohl\u00b7ge\u00b7brauch\u00b7tes", "Guth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "KON", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Und du, du unser F\u00fcrst und Zeuge in der Wolcke!", "tokens": ["Und", "du", ",", "du", "un\u00b7ser", "F\u00fcrst", "und", "Zeu\u00b7ge", "in", "der", "Wol\u00b7cke", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "PPER", "PPOSAT", "NN", "KON", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Best\u00e4tigt Linner nicht, was dir so wohl bewu\u00dft", "tokens": ["Be\u00b7st\u00e4\u00b7tigt", "Lin\u00b7ner", "nicht", ",", "was", "dir", "so", "wohl", "be\u00b7wu\u00dft"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "NE", "PTKNEG", "$,", "PWS", "PPER", "ADV", "ADV", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und was dein Mund bi\u00df jetzt bezeugt von deinem Volcke,", "tokens": ["Und", "was", "dein", "Mund", "bi\u00df", "jetzt", "be\u00b7zeugt", "von", "dei\u00b7nem", "Vol\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPOSAT", "NN", "APPR", "ADV", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Womit du je und je dem Kl\u00e4ger Einhalt thust?", "tokens": ["Wo\u00b7mit", "du", "je", "und", "je", "dem", "Kl\u00e4\u00b7ger", "Ein\u00b7halt", "thust", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "KON", "ADV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ja, unsre Seele wei\u00df, da\u00df Linner frey bekennt:", "tokens": ["Ja", ",", "uns\u00b7re", "See\u00b7le", "wei\u00df", ",", "da\u00df", "Lin\u00b7ner", "frey", "be\u00b7kennt", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPOSAT", "NN", "VVFIN", "$,", "KOUS", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Mann! spricht er nach der Art von unsrer Rede Ziel,\nDein Tod, dein Tod am Creutz, dein blutiges Erwer-\nben.", "tokens": ["Mann", "!", "spricht", "er", "nach", "der", "Art", "von", "uns\u00b7rer", "Re\u00b7de", "Ziel", ",", "Dein", "Tod", ",", "dein", "Tod", "am", "Creutz", ",", "dein", "blu\u00b7ti\u00b7ges", "Er\u00b7wer", "ben", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "PPER", "APPR", "ART", "NN", "APPR", "PPOSAT", "NN", "NN", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "APPRART", "NN", "$,", "PPOSAT", "ADJA", "TRUNC", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.2": {"text": "Das war der tiefe Text im Sinn und im Gef\u00fchl", "tokens": ["Das", "war", "der", "tie\u00b7fe", "Text", "im", "Sinn", "und", "im", "Ge\u00b7f\u00fchl"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "APPRART", "NN", "KON", "APPRART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Aufs Leben unsrer Schaar. aufs Leyden und aufs Ster-\nben,", "tokens": ["Aufs", "Le\u00b7ben", "uns\u00b7rer", "Schaar", ".", "aufs", "Ley\u00b7den", "und", "aufs", "Ster", "ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "$.", "APPRART", "NN", "KON", "APPRART", "TRUNC", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Den der zur Danckbarkeit erregte Hertzens Grund", "tokens": ["Den", "der", "zur", "Dan\u00b7ck\u00b7bar\u00b7keit", "er\u00b7reg\u00b7te", "Hert\u00b7zens", "Grund"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ART", "APPRART", "NN", "VVFIN", "NN", "NN"], "meter": "+--+--+-+-+-+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Mein und der Meinigen bi\u00dfher allein verstund", "tokens": ["Mein", "und", "der", "Mei\u00b7ni\u00b7gen", "bi\u00df\u00b7her", "al\u00b7lein", "ver\u00b7stund"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "KON", "ART", "PPOSS", "ADV", "ADV", "VVFIN"], "meter": "+--+---+-+-+", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "Haupt (setzet er hinzu) und Lehrer der Gemeinen!", "tokens": ["Haupt", "(", "set\u00b7zet", "er", "hin\u00b7zu", ")", "und", "Leh\u00b7rer", "der", "Ge\u00b7mei\u00b7nen", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "VVFIN", "PPER", "PTKVZ", "$(", "KON", "NN", "ART", "NN", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Ich bitte dich vor die, so du mir anvertraut,", "tokens": ["Ich", "bit\u00b7te", "dich", "vor", "die", ",", "so", "du", "mir", "an\u00b7ver\u00b7traut", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "$,", "ADV", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "La\u00df ihr das blutge Licht recht warm, recht nahe schei-\nnen,", "tokens": ["La\u00df", "ihr", "das", "blut\u00b7ge", "Licht", "recht", "warm", ",", "recht", "na\u00b7he", "schei", "nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "ADJA", "NN", "ADV", "ADJD", "$,", "ADV", "ADJD", "TRUNC", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Bi\u00df sie dabey das Hau\u00df des Glaubens ausgebaut.", "tokens": ["Bi\u00df", "sie", "da\u00b7bey", "das", "Hau\u00df", "des", "Glau\u00b7bens", "aus\u00b7ge\u00b7baut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PAV", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Das Hau\u00df, von dem es hei\u00dft, da\u00df es dein Wohn-Ge-\nb\u00e4u", "tokens": ["Das", "Hau\u00df", ",", "von", "dem", "es", "hei\u00dft", ",", "da\u00df", "es", "dein", "Wohn\u00b7Ge", "b\u00e4u"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "APPR", "PRELS", "PPER", "VVFIN", "$,", "KOUS", "PPER", "PPOSAT", "NN", "NE"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und auch der Pfeiler Grund von deiner Wahrheit sey.", "tokens": ["Und", "auch", "der", "Pfei\u00b7ler", "Grund", "von", "dei\u00b7ner", "Wahr\u00b7heit", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "NN", "APPR", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Weil dieser Zeugen Wunsch der Lohn des Lammes ist.", "tokens": ["Weil", "die\u00b7ser", "Zeu\u00b7gen", "Wunsch", "der", "Lohn", "des", "Lam\u00b7mes", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "NN", "ART", "NN", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Seit dem es seine Stirn f\u00fcr uns hat sch\u00e4nden lassen,", "tokens": ["Seit", "dem", "es", "sei\u00b7ne", "Stirn", "f\u00fcr", "uns", "hat", "sch\u00e4n\u00b7den", "las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PPOSAT", "NN", "APPR", "PPER", "VAFIN", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So wei\u00df dein Streiter-Zelt, HErr! da\u00df du willig bist,", "tokens": ["So", "wei\u00df", "dein", "Strei\u00b7ter\u00b7Zelt", ",", "Herr", "!", "da\u00df", "du", "wil\u00b7lig", "bist", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$,", "NN", "$.", "KOUS", "PPER", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Es mit dem Siegs-Pallast in eine Schnur zu fassen.", "tokens": ["Es", "mit", "dem", "Siegs\u00b7Pal\u00b7last", "in", "ei\u00b7ne", "Schnur", "zu", "fas\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Drum, Seel\u2019ger-Aelt\u2019sten-Geist! fall vor den F\u00fcrsten hin,", "tokens": ["Drum", ",", "Seel'\u00b7ger\u00b7Aelt'\u00b7sten\u00b7Geist", "!", "fall", "vor", "den", "F\u00fcrs\u00b7ten", "hin", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "NN", "$.", "NN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und danck dem Lamm und ihm in dein und unserm Sinn.", "tokens": ["Und", "danck", "dem", "Lamm", "und", "ihm", "in", "dein", "und", "un\u00b7serm", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "KON", "PPER", "APPR", "PPOSAT", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Wir habens wohl erkannt und merckens immer besser,", "tokens": ["Wir", "ha\u00b7bens", "wohl", "er\u00b7kannt", "und", "mer\u00b7ckens", "im\u00b7mer", "bes\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVPP", "KON", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df nichts so selig ist, als unterm Creutze stehn,", "tokens": ["Da\u00df", "nichts", "so", "se\u00b7lig", "ist", ",", "als", "un\u00b7term", "Creut\u00b7ze", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "ADJD", "VAFIN", "$,", "KOUS", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das Creutz-Reich werde uns bey Schmach und Ehre gr\u00f6sser!", "tokens": ["Das", "Creutz\u00b7Reich", "wer\u00b7de", "uns", "bey", "Schmach", "und", "Eh\u00b7re", "gr\u00f6s\u00b7ser", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "APPR", "NN", "KON", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "O Mit-Knecht wolltest du uns diese Gnad erflehn!", "tokens": ["O", "Mit\u00b7Knecht", "woll\u00b7test", "du", "uns", "die\u00b7se", "Gnad", "er\u00b7flehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VMFIN", "PPER", "PRF", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Doch, Heyland! bete du. Drum Creutz-Reich gutes Muths,", "tokens": ["Doch", ",", "Hey\u00b7land", "!", "be\u00b7te", "du", ".", "Drum", "Creutz\u00b7Reich", "gu\u00b7tes", "Muths", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "NN", "$.", "VVFIN", "PPER", "$.", "PAV", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wir ", "tokens": ["Wir"], "token_info": ["word"], "pos": ["PPER"], "meter": "-", "measure": "single.down"}}}}}