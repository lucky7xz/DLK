{"textgrid.poem.62210": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "[einen Vers soll ich Dir machen]", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Einen Vers soll ich Dir machen:", "tokens": ["Ei\u00b7nen", "Vers", "soll", "ich", "Dir", "ma\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Verse, Freund, sie sind verschieden", "tokens": ["Ver\u00b7se", ",", "Freund", ",", "sie", "sind", "ver\u00b7schie\u00b7den"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "PPER", "VAFIN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie das Leben ist hienieden", "tokens": ["Wie", "das", "Le\u00b7ben", "ist", "hien\u00b7ie\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "VAFIN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Oft sehr ernst und oft zum Lachen \u2013", "tokens": ["Oft", "sehr", "ernst", "und", "oft", "zum", "La\u00b7chen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "KON", "ADV", "APPRART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Einen heitern will ich machen:", "tokens": ["Ei\u00b7nen", "hei\u00b7tern", "will", "ich", "ma\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVINF", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Heiterkeit sei Dir beschieden,", "tokens": ["Hei\u00b7ter\u00b7keit", "sei", "Dir", "be\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Allen denen, die hienieden", "tokens": ["Al\u00b7len", "de\u00b7nen", ",", "die", "hien\u00b7ie\u00b7den"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NE", "PDS", "$,", "PRELS", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Man kein X f\u00fcr U kann machen; \u2013", "tokens": ["Man", "kein", "X", "f\u00fcr", "U", "kann", "ma\u00b7chen", ";", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PIS", "PIAT", "XY", "XY", "XY", "VMFIN", "VVINF", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Den Studenten,", "tokens": ["Den", "Stu\u00b7den\u00b7ten", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+--", "measure": "dactylic.init"}, "line.10": {"text": "Die nie flennten,", "tokens": ["Die", "nie", "flenn\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.11": {"text": "Nie in falsche Schlingen rennten,", "tokens": ["Nie", "in", "fal\u00b7sche", "Schlin\u00b7gen", "renn\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Die mit eignen Ohren h\u00f6ren,", "tokens": ["Die", "mit", "eig\u00b7nen", "Oh\u00b7ren", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Nie auf eine Dummheit schw\u00f6ren,", "tokens": ["Nie", "auf", "ei\u00b7ne", "Dumm\u00b7heit", "schw\u00f6\u00b7ren",