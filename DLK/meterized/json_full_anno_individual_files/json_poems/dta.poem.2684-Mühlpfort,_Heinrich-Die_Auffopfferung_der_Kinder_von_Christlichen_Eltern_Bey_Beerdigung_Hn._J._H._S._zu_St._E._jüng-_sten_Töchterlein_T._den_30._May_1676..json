{"dta.poem.2684": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Auffopfferung der Kinder von Christlichen  \n Eltern/  \n  Bey Beerdigung Hn.   J. H. S.   zu St. E. j\u00fcng-  \n sten T\u00f6chterlein T. den 30. May 1676.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es mag das Heydenthum mit seinem Moloch huren/", "tokens": ["Es", "mag", "das", "Hey\u00b7den\u00b7thum", "mit", "sei\u00b7nem", "Mo\u00b7loch", "hu\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und in dem Aschen-Thal durchs Feur die Kinder ziehn/", "tokens": ["Und", "in", "dem", "A\u00b7schen\u00b7Thal", "durchs", "Feur", "die", "Kin\u00b7der", "ziehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "APPRART", "NN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ja es geh ferner auch auf den verfluchten Spuren/", "tokens": ["Ja", "es", "geh", "fer\u00b7ner", "auch", "auf", "den", "ver\u00b7fluch\u00b7ten", "Spu\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PPER", "VVFIN", "ADV", "ADV", "APPR", "ART", "ADJA", "NN", "$("], "meter": "+--+-----+-+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "Da\u00df Sohn und Tochter mu\u00df in seinen H\u00e4nden gl\u00fchn.", "tokens": ["Da\u00df", "Sohn", "und", "Toch\u00b7ter", "mu\u00df", "in", "sei\u00b7nen", "H\u00e4n\u00b7den", "gl\u00fchn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "VMFIN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Es hatte sieben Thor und H\u00e4user ihm bereitet;", "tokens": ["Es", "hat\u00b7te", "sie\u00b7ben", "Thor", "und", "H\u00e4u\u00b7ser", "ihm", "be\u00b7rei\u00b7tet", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "CARD", "NN", "KON", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ins erste wurde nur der Semmel-Teig gelegt/", "tokens": ["Ins", "ers\u00b7te", "wur\u00b7de", "nur", "der", "Sem\u00b7mel\u00b7Teig", "ge\u00b7legt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "VAFIN", "ADV", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Turtel-Taube drauff ins andre Thor geleitet/", "tokens": ["Die", "Tur\u00b7tel\u00b7Tau\u00b7be", "drauff", "ins", "and\u00b7re", "Thor", "ge\u00b7lei\u00b7tet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PAV", "APPRART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ins dritte denn ein Schaaf/ so nichts als Sanfftmuth hegt.", "tokens": ["Ins", "drit\u00b7te", "denn", "ein", "Schaaf", "/", "so", "nichts", "als", "Sanfft\u00b7muth", "hegt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "KON", "ART", "NN", "$(", "ADV", "PIS", "KOKOM", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Der Wider ist darzu nebst Kalb\u2019 und Ochse kommen/", "tokens": ["Der", "Wi\u00b7der", "ist", "dar\u00b7zu", "nebst", "Kalb'", "und", "O\u00b7chse", "kom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PAV", "APPR", "NN", "KON", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Damit die sechste Zahl de\u00df Opffers ward erf\u00fcllt.", "tokens": ["Da\u00b7mit", "die", "sechs\u00b7te", "Zahl", "de\u00df", "Opf\u00b7fers", "ward", "er\u00b7f\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "ADJA", "NN", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Jetzt aber mu\u00df mein Mund au\u00df Eyfersucht verstummen/", "tokens": ["Jetzt", "a\u00b7ber", "mu\u00df", "mein", "Mund", "au\u00df", "Ey\u00b7fer\u00b7sucht", "ver\u00b7stum\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VMFIN", "PPOSAT", "NN", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Weil bey dem siebenden nichts als ein Mensch nur gilt.", "tokens": ["Weil", "bey", "dem", "sie\u00b7ben\u00b7den", "nichts", "als", "ein", "Mensch", "nur", "gilt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "ADJA", "PIS", "KOKOM", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Verdammter Aberwitz/ mehr als verteuffelt rasen!", "tokens": ["Ver\u00b7damm\u00b7ter", "A\u00b7ber\u00b7witz", "/", "mehr", "als", "ver\u00b7teuf\u00b7felt", "ra\u00b7sen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$(", "PIAT", "KOKOM", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und solche Grausamkeit die kein Exempel hat.", "tokens": ["Und", "sol\u00b7che", "Grau\u00b7sam\u00b7keit", "die", "kein", "Ex\u00b7em\u00b7pel", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ART", "PIAT", "NN", "VAFIN", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Man schlug darbey die Pauck\u2019 und lie\u00df die Tromm\u2019ten blasen/", "tokens": ["Man", "schlug", "dar\u00b7bey", "die", "Pauck'", "und", "lie\u00df", "die", "Trom\u00b7m'\u00b7ten", "bla\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PAV", "ART", "NN", "KON", "VVFIN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Damit das Winseln nicht bey Eltern finde stat.", "tokens": ["Da\u00b7mit", "das", "Win\u00b7seln", "nicht", "bey", "El\u00b7tern", "fin\u00b7de", "stat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "PTKNEG", "APPR", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Und ist es m\u00f6glich nur/ da\u00df/ da wir sonst empfinden/", "tokens": ["Und", "ist", "es", "m\u00f6g\u00b7lich", "nur", "/", "da\u00df", "/", "da", "wir", "sonst", "emp\u00b7fin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADJD", "ADV", "$(", "KOUS", "$(", "KOUS", "PPER", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was unser Fleisch und Blut am wenigsten ber\u00fchrt/", "tokens": ["Was", "un\u00b7ser", "Fleisch", "und", "Blut", "am", "we\u00b7nigs\u00b7ten", "be\u00b7r\u00fchrt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "KON", "NN", "APPRART", "PIS", "VVPP", "$("], "meter": "-+-+-+-+---+", "measure": "unknown.measure.penta"}, "line.3": {"text": "Die Eltern musten so im G\u00f6tzen-Dienst verblinden/", "tokens": ["Die", "El\u00b7tern", "mus\u00b7ten", "so", "im", "G\u00f6t\u00b7zen\u00b7Dienst", "ver\u00b7blin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "APPRART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df sie hier keine Lieb noch Schuldigkeit gesp\u00fcrt.", "tokens": ["Da\u00df", "sie", "hier", "kei\u00b7ne", "Lieb", "noch", "Schul\u00b7dig\u00b7keit", "ge\u00b7sp\u00fcrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PIAT", "NN", "ADV", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Di\u00df hie\u00df ein W\u00fcrge-Thal wodurch der H\u00f6llen Schlange", "tokens": ["Di\u00df", "hie\u00df", "ein", "W\u00fcr\u00b7ge\u00b7Thal", "wo\u00b7durch", "der", "H\u00f6l\u00b7len", "Schlan\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das Opffer Jephta war zu bilden h\u00f6chst bem\u00fcht;", "tokens": ["Das", "Opf\u00b7fer", "Jeph\u00b7ta", "war", "zu", "bil\u00b7den", "h\u00f6chst", "be\u00b7m\u00fcht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VAFIN", "PTKZU", "VVINF", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Alle ine welchem Christ\u2019 wird hier nicht hertzlich bange/", "tokens": ["Al\u00b7le", "i\u00b7ne", "wel\u00b7chem", "Christ'", "wird", "hier", "nicht", "hertz\u00b7lich", "ban\u00b7ge", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "PWAT", "NN", "VAFIN", "ADV", "PTKNEG", "ADJD", "ADV", "$("], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.4": {"text": "Wenn er die Hurerey de\u00df schn\u00f6den Volcks ansiht.", "tokens": ["Wenn", "er", "die", "Hu\u00b7re\u00b7rey", "de\u00df", "schn\u00f6\u00b7den", "Volcks", "an\u00b7siht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Der Moloch der verschlang die Kinder in den Rachen/", "tokens": ["Der", "Mo\u00b7loch", "der", "ver\u00b7schlang", "die", "Kin\u00b7der", "in", "den", "Ra\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da unser Heyland sie heisst in den Himmel gehn/", "tokens": ["Da", "un\u00b7ser", "Hey\u00b7land", "sie", "heisst", "in", "den", "Him\u00b7mel", "gehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "VVFIN", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Will ihnen Thor und Th\u00fcr als Bruder selbst auffmachen/", "tokens": ["Will", "ih\u00b7nen", "Thor", "und", "Th\u00fcr", "als", "Bru\u00b7der", "selbst", "auff\u00b7ma\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "NN", "KON", "NN", "KOUS", "NN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und l\u00e4st sie neben sich an seiner Seiten stehn.", "tokens": ["Und", "l\u00e4st", "sie", "ne\u00b7ben", "sich", "an", "sei\u00b7ner", "Sei\u00b7ten", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "PRF", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Sie sollen auch das Reich der Herrligkeit besitzen/", "tokens": ["Sie", "sol\u00b7len", "auch", "das", "Reich", "der", "Herr\u00b7lig\u00b7keit", "be\u00b7sit\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ART", "NN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vor Klugen/ so die Welt in h\u00f6chster Demuth ehrt.", "tokens": ["Vor", "Klu\u00b7gen", "/", "so", "die", "Welt", "in", "h\u00f6chs\u00b7ter", "De\u00b7muth", "ehrt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$(", "ADV", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Jhr\u2019 Einfalt soll vielmehr als eitle Wei\u00dfheit n\u00fctzen/", "tokens": ["Ih\u00b7r'", "Ein\u00b7falt", "soll", "viel\u00b7mehr", "als", "eit\u00b7le", "Wei\u00df\u00b7heit", "n\u00fct\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "ADV", "KOUS", "ADJA", "NN", "VVINF", "$("], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.4": {"text": "Weil selbte meistentheils den Grund der Andacht st\u00f6rt.", "tokens": ["Weil", "selb\u00b7te", "meis\u00b7ten\u00b7theils", "den", "Grund", "der", "An\u00b7dacht", "st\u00f6rt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "ADV", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Doch kommet Eltern zu/ da\u00df ihres Blutes Trauben/", "tokens": ["Doch", "kom\u00b7met", "El\u00b7tern", "zu", "/", "da\u00df", "ih\u00b7res", "Blu\u00b7tes", "Trau\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "PTKZU", "$(", "KOUS", "PPOSAT", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Blumen des Geschlechts/ die Sprie\u00dflinge der Art/", "tokens": ["Die", "Blu\u00b7men", "des", "Ge\u00b7schlechts", "/", "die", "Sprie\u00df\u00b7lin\u00b7ge", "der", "Art", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$(", "ART", "NN", "ART", "NN", "$("], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Nach dem sie einverleibt der Kirche wahrem Glauben/", "tokens": ["Nach", "dem", "sie", "ein\u00b7ver\u00b7leibt", "der", "Kir\u00b7che", "wah\u00b7rem", "Glau\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "VVFIN", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Bedencken/ wie man sie f\u00fcr allem Fall bewahrt.", "tokens": ["Be\u00b7den\u00b7cken", "/", "wie", "man", "sie", "f\u00fcr", "al\u00b7lem", "Fall", "be\u00b7wahrt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "PWAV", "PIS", "PPER", "APPR", "PIS", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und in der Opfferung dem H\u00f6chsten eintzig giebet/", "tokens": ["Und", "in", "der", "Opf\u00b7fe\u00b7rung", "dem", "H\u00f6chs\u00b7ten", "eint\u00b7zig", "gie\u00b7bet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Zu dessen Ehr und Ruhm ihr gantzes Leben f\u00fchrt/", "tokens": ["Zu", "des\u00b7sen", "Ehr", "und", "Ruhm", "ihr", "gant\u00b7zes", "Le\u00b7ben", "f\u00fchrt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "KON", "NN", "PPOSAT", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Denn wie er alle Welt von Anbegin geliebet/", "tokens": ["Denn", "wie", "er", "al\u00b7le", "Welt", "von", "An\u00b7be\u00b7gin", "ge\u00b7lie\u00b7bet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "PIAT", "NN", "APPR", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So wird auch seine Lieb in Kindern klar gesp\u00fcrt.", "tokens": ["So", "wird", "auch", "sei\u00b7ne", "Lieb", "in", "Kin\u00b7dern", "klar", "ge\u00b7sp\u00fcrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PPOSAT", "NN", "APPR", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Sie