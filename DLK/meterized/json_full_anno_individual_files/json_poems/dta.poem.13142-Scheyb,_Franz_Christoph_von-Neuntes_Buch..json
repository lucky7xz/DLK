{"dta.poem.13142": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Neuntes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201esie lebt nach meinem Rath. Nichts \u00e4ndert meinen Geist,", "tokens": ["\u201e", "sie", "lebt", "nach", "mei\u00b7nem", "Rath", ".", "Nichts", "\u00e4n\u00b7dert", "mei\u00b7nen", "Geist", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$.", "PIS", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "\u201enichts ist, was ihn von sich und seiner Tugend rei\u00dft.", "tokens": ["\u201e", "nichts", "ist", ",", "was", "ihn", "von", "sich", "und", "sei\u00b7ner", "Tu\u00b7gend", "rei\u00dft", "."], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PIS", "VAFIN", "$,", "PRELS", "PPER", "APPR", "PRF", "KON", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "\u201eich halte Mund und Herz nach einem Thon gestimmet.", "tokens": ["\u201e", "ich", "hal\u00b7te", "Mund", "und", "Herz", "nach", "ei\u00b7nem", "Thon", "ge\u00b7stim\u00b7met", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "NN", "KON", "NN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "160\u201dIch lache, wann der Ha\u00df nach mir die Lefzen kr\u00fcmmet.", "tokens": ["\"", "Ich", "la\u00b7che", ",", "wann", "der", "Ha\u00df", "nach", "mir", "die", "Lef\u00b7zen", "kr\u00fcm\u00b7met", "."], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "$,", "PWAV", "ART", "NN", "APPR", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "\u201eder Kummer qu\u00e4lt mich nicht, ob ich geliebt, gesch\u00e4zt,", "tokens": ["\u201e", "der", "Kum\u00b7mer", "qu\u00e4lt", "mich", "nicht", ",", "ob", "ich", "ge\u00b7liebt", ",", "ge\u00b7sch\u00e4zt", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "PPER", "PTKNEG", "$,", "KOUS", "PPER", "VVPP", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "\u201eob ich verachtet sey. Nichts ist, so mich verlezt.", "tokens": ["\u201e", "ob", "ich", "ver\u00b7ach\u00b7tet", "sey", ".", "Nichts", "ist", ",", "so", "mich", "ver\u00b7lezt", "."], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "VVPP", "VAFIN", "$.", "PIS", "VAFIN", "$,", "ADV", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}