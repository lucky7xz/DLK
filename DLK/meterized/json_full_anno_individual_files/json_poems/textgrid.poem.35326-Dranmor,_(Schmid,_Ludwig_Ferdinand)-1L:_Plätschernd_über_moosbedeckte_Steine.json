{"textgrid.poem.35326": {"metadata": {"author": {"name": "Dranmor, (Schmid, Ludwig Ferdinand)", "birth": "N.A.", "death": "N.A."}, "title": "1L: Pl\u00e4tschernd \u00fcber moosbedeckte Steine", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Pl\u00e4tschernd \u00fcber moosbedeckte Steine", "tokens": ["Pl\u00e4t\u00b7schernd", "\u00fc\u00b7ber", "moos\u00b7be\u00b7deck\u00b7te", "Stei\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Kommt die Quelle, in krystallner Reine,", "tokens": ["Kommt", "die", "Quel\u00b7le", ",", "in", "krys\u00b7tall\u00b7ner", "Rei\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Einem k\u00fchlen Becken zugeh\u00fcpft;", "tokens": ["Ei\u00b7nem", "k\u00fch\u00b7len", "Be\u00b7cken", "zu\u00b7ge\u00b7h\u00fcpft", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Wo im Schilfe die Platanen fl\u00fcstern,", "tokens": ["Wo", "im", "Schil\u00b7fe", "die", "Pla\u00b7ta\u00b7nen", "fl\u00fcs\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Ist ein M\u00e4dchen, nach dem Bade l\u00fcstern,", "tokens": ["Ist", "ein", "M\u00e4d\u00b7chen", ",", "nach", "dem", "Ba\u00b7de", "l\u00fcs\u00b7tern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$,", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Ihrem leichten Morgenkleid entschl\u00fcpft,", "tokens": ["Ih\u00b7rem", "leich\u00b7ten", "Mor\u00b7gen\u00b7kleid", "ent\u00b7schl\u00fcpft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Spiegelt in der Flut die nackten Glieder,", "tokens": ["Spie\u00b7gelt", "in", "der", "Flut", "die", "nack\u00b7ten", "Glie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Schreitet langsam vorw\u00e4rts, duckt sich nieder,", "tokens": ["Schrei\u00b7tet", "lang\u00b7sam", "vor\u00b7w\u00e4rts", ",", "duckt", "sich", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PTKVZ", "$,", "VVFIN", "PRF", "PTKVZ", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Horcht und fl\u00fcchtet rasch ins hohe Rohr \u2013", "tokens": ["Horcht", "und", "fl\u00fcch\u00b7tet", "rasch", "ins", "ho\u00b7he", "Rohr", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "ADJD", "APPRART", "ADJA", "NN", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Will, Versch\u00e4mte, schon Gefahr sich zeigen?", "tokens": ["Will", ",", "Ver\u00b7sch\u00e4m\u00b7te", ",", "schon", "Ge\u00b7fahr", "sich", "zei\u00b7gen", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "$,", "VVFIN", "$,", "ADV", "NN", "PRF", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Muntre S\u00e4nger jubeln auf den Zweigen,", "tokens": ["Mun\u00b7tre", "S\u00e4n\u00b7ger", "ju\u00b7beln", "auf", "den", "Zwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Goldne K\u00e4fer summen dir ums Ohr.", "tokens": ["Gold\u00b7ne", "K\u00e4\u00b7fer", "sum\u00b7men", "dir", "ums", "Ohr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Doch des Kindes Furcht ist schon vergangen.", "tokens": ["Doch", "des", "Kin\u00b7des", "Furcht", "ist", "schon", "ver\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "VAFIN", "ADV", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Sechzehn Lenze z\u00e4hlt sie; unbefangen", "tokens": ["Sech\u00b7zehn", "Len\u00b7ze", "z\u00e4hlt", "sie", ";", "un\u00b7be\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["CARD", "NN", "VVFIN", "PPER", "$.", "ADJD"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Ist ihr Herz; kein eitler Schw\u00e4tzer pries", "tokens": ["Ist", "ihr", "Herz", ";", "kein", "eit\u00b7ler", "Schw\u00e4t\u00b7zer", "pries"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "NN", "$.", "PIAT", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Ihrer Augen Schmelz \u2013 er m\u00fc\u00dfte warten,", "tokens": ["Ih\u00b7rer", "Au\u00b7gen", "Schmelz", "\u2013", "er", "m\u00fc\u00df\u00b7te", "war\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$(", "PPER", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Wenn er's th\u00e4te, denn der Mutter Garten", "tokens": ["Wenn", "er's", "th\u00e4\u00b7te", ",", "denn", "der", "Mut\u00b7ter", "Gar\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "VVFIN", "$,", "KON", "ART", "NN", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Ist ihr K\u00f6nigreich, ihr Paradies.", "tokens": ["Ist", "ihr", "K\u00f6\u00b7nig\u00b7reich", ",", "ihr", "Pa\u00b7ra\u00b7dies", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Und sie labt sich an des Wassers Frische;", "tokens": ["Und", "sie", "labt", "sich", "an", "des", "Was\u00b7sers", "Fri\u00b7sche", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Spielend wagen sich die kleinen Fische", "tokens": ["Spie\u00b7lend", "wa\u00b7gen", "sich", "die", "klei\u00b7nen", "Fi\u00b7sche"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PRF", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Jetzt heran, und mit den H\u00e4ndchen schl\u00e4gt", "tokens": ["Jetzt", "he\u00b7ran", ",", "und", "mit", "den", "H\u00e4nd\u00b7chen", "schl\u00e4gt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "PTKVZ", "$,", "KON", "APPR", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Sie die Flut, die gleich in farb'ge Kreise", "tokens": ["Sie", "die", "Flut", ",", "die", "gleich", "in", "fa\u00b7rb'\u00b7ge", "Krei\u00b7se"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "ART", "NN", "$,", "PRELS", "ADV", "APPR", "ADJA", "NN"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Sich zerteilen mu\u00df, und leise, leise", "tokens": ["Sich", "zer\u00b7tei\u00b7len", "mu\u00df", ",", "und", "lei\u00b7se", ",", "lei\u00b7se"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PRF", "VVINF", "VMFIN", "$,", "KON", "ADJD", "$,", "ADJD"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Ihr Gemurmel ans Gestade tr\u00e4gt.", "tokens": ["Ihr", "Ge\u00b7mur\u00b7mel", "ans", "Ge\u00b7sta\u00b7de", "tr\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Schwalben sucht sie sch\u00e4kernd zu ergreifen,", "tokens": ["Schwal\u00b7ben", "sucht", "sie", "sch\u00e4\u00b7kernd", "zu", "er\u00b7grei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Die an ihrer Stirn vor\u00fcberstreifen;", "tokens": ["Die", "an", "ih\u00b7rer", "Stirn", "vor\u00b7\u00fc\u00b7bers\u00b7trei\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Auch gefangen d\u00fcrften sie entfliehn;", "tokens": ["Auch", "ge\u00b7fan\u00b7gen", "d\u00fcrf\u00b7ten", "sie", "ent\u00b7fliehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Ameischen, die schlechten Schwimmerinnen,", "tokens": ["A\u00b7mei\u00b7schen", ",", "die", "schlech\u00b7ten", "Schwim\u00b7me\u00b7rin\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "L\u00e4\u00dft sie gern den Rasensaum gewinnen", "tokens": ["L\u00e4\u00dft", "sie", "gern", "den", "Ra\u00b7sen\u00b7saum", "ge\u00b7win\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Und in Gottes Namen f\u00fcrderziehn.", "tokens": ["Und", "in", "Got\u00b7tes", "Na\u00b7men", "f\u00fcr\u00b7der\u00b7ziehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "NN", "VVINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Rosenbl\u00e4tter werden dann mit Lachen", "tokens": ["Ro\u00b7sen\u00b7bl\u00e4t\u00b7ter", "wer\u00b7den", "dann", "mit", "La\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "ADV", "APPR", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Hingestreut; sie sendet duft'ge Nachen", "tokens": ["Hin\u00b7ge\u00b7streut", ";", "sie", "sen\u00b7det", "duft'\u00b7ge", "Na\u00b7chen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$.", "PPER", "VVFIN", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Auf die hohe See und bl\u00e4st sie fort.", "tokens": ["Auf", "die", "ho\u00b7he", "See", "und", "bl\u00e4st", "sie", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "KON", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Hui, es st\u00fcrmt! Die Schiffe wehn zur K\u00fcste;", "tokens": ["Hui", ",", "es", "st\u00fcrmt", "!", "Die", "Schif\u00b7fe", "wehn", "zur", "K\u00fcs\u00b7te", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "$,", "PPER", "VVFIN", "$.", "ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Wen'ge retten sich an ihre Br\u00fcste", "tokens": ["Wen'\u00b7ge", "ret\u00b7ten", "sich", "an", "ih\u00b7re", "Br\u00fcs\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "PRF", "APPR", "PPOSAT", "NN"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Wie in einen stillen kleinen Port.", "tokens": ["Wie", "in", "ei\u00b7nen", "stil\u00b7len", "klei\u00b7nen", "Port", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "ADJA", "ADJA", "NE", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Doch genug gescherzt! Mit ernster Miene", "tokens": ["Doch", "ge\u00b7nug", "ge\u00b7scherzt", "!", "Mit", "erns\u00b7ter", "Mie\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "VVPP", "$.", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Folgt sie jetzt dem Flei\u00df der klugen Biene,", "tokens": ["Folgt", "sie", "jetzt", "dem", "Flei\u00df", "der", "klu\u00b7gen", "Bie\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Deren K\u00f6pfchen in der Sonne glimmt,", "tokens": ["De\u00b7ren", "K\u00f6pf\u00b7chen", "in", "der", "Son\u00b7ne", "glimmt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Bis das Tierchen des Geh\u00f6lzes Stille", "tokens": ["Bis", "das", "Tier\u00b7chen", "des", "Ge\u00b7h\u00f6l\u00b7zes", "Stil\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Zueilt, und das Zirpen einer Grille", "tokens": ["Zu\u00b7eilt", ",", "und", "das", "Zir\u00b7pen", "ei\u00b7ner", "Gril\u00b7le"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVPP", "$,", "KON", "ART", "NN", "ART", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Seine Morgenlieder \u00fcberstimmt.", "tokens": ["Sei\u00b7ne", "Mor\u00b7gen\u00b7lie\u00b7der", "\u00fc\u00b7bers\u00b7timmt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.8": {"line.1": {"text": "Wie, nun ist sie gar im warmen Sande", "tokens": ["Wie", ",", "nun", "ist", "sie", "gar", "im", "war\u00b7men", "San\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "$,", "ADV", "VAFIN", "PPER", "ADV", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Eingeschlummert? An des Beckens Rande", "tokens": ["Ein\u00b7ge\u00b7schlum\u00b7mert", "?", "An", "des", "Be\u00b7ckens", "Ran\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVPP", "$.", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Ruht ihr Haupt, von Locken halb bedeckt,", "tokens": ["Ruht", "ihr", "Haupt", ",", "von", "Lo\u00b7cken", "halb", "be\u00b7deckt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "APPR", "NN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Die noch immer tief ins Wasser reichen;", "tokens": ["Die", "noch", "im\u00b7mer", "tief", "ins", "Was\u00b7ser", "rei\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "ADJD", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Einem Schwane ist sie zu vergleichen,", "tokens": ["Ei\u00b7nem", "Schwa\u00b7ne", "ist", "sie", "zu", "ver\u00b7glei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Der den Kopf in sein Gefieder steckt.", "tokens": ["Der", "den", "Kopf", "in", "sein", "Ge\u00b7fie\u00b7der", "steckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.9": {"line.1": {"text": "Sie erwacht. Ein Rascheln und ein Rauschen \u2013", "tokens": ["Sie", "er\u00b7wacht", ".", "Ein", "Ra\u00b7scheln", "und", "ein", "Rau\u00b7schen", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "ART", "NN", "KON", "ART", "NN", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "War's ein Menschenfu\u00df? O banges Lauschen!", "tokens": ["Wa\u00b7r's", "ein", "Men\u00b7schen\u00b7fu\u00df", "?", "O", "ban\u00b7ges", "Lau\u00b7schen", "!"], 