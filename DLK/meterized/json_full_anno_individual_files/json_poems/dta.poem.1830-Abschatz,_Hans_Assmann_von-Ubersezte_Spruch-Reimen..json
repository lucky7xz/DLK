{"dta.poem.1830": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Ubersezte Spruch-Reimen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Was hilfft der auff sich selbst erz\u00fcrnten Sinnen Streiten/", "tokens": ["Was", "hilfft", "der", "auff", "sich", "selbst", "er\u00b7z\u00fcrn\u00b7ten", "Sin\u00b7nen", "Strei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "APPR", "PRF", "ADV", "ADJA", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ob man mit Grauen sieht den Fleck/ und l\u00e4st ihn kle-", "tokens": ["Ob", "man", "mit", "Grau\u00b7en", "sieht", "den", "Fleck", "/", "und", "l\u00e4st", "ihn", "kle"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "APPR", "NN", "VVFIN", "ART", "NN", "$(", "KON", "VVFIN", "PPER", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "ben/", "tokens": ["ben", "/"], "token_info": ["word", "punct"], "pos": ["VVPP", "$("], "meter": "-", "measure": "single.down"}, "line.4": {"text": "Die krummen Weg erkennt/ und will sie nicht begeben/", "tokens": ["Die", "krum\u00b7men", "Weg", "er\u00b7kennt", "/", "und", "will", "sie", "nicht", "be\u00b7ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$(", "KON", "VMFIN", "PPER", "PTKNEG", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Urtheilet die Gefahr/ und weicht ihr nicht bey Zeiten.", "tokens": ["Ur\u00b7thei\u00b7let", "die", "Ge\u00b7fahr", "/", "und", "weicht", "ihr", "nicht", "bey", "Zei\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$(", "KON", "VVFIN", "PPER", "PTKNEG", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}