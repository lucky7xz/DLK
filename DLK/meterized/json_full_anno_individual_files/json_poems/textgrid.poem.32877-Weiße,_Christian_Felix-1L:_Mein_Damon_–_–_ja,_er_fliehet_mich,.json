{"textgrid.poem.32877": {"metadata": {"author": {"name": "Wei\u00dfe, Christian Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mein Damon \u2013 \u2013 ja, er fliehet mich,", "genre": "verse", "period": "N.A.", "pub_year": 1765, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Damon \u2013 \u2013 ja, er fliehet mich,", "tokens": ["Mein", "Da\u00b7mon", "\u2013", "\u2013", "ja", ",", "er", "flie\u00b7het", "mich", ","], "token_info": ["word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "$(", "PTKANT", "$,", "PPER", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Entreisset meinen Fesseln sich!", "tokens": ["E\u00b7ntreis\u00b7set", "mei\u00b7nen", "Fes\u00b7seln", "sich", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PRF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was gleichet meinem Schmerz!", "tokens": ["Was", "glei\u00b7chet", "mei\u00b7nem", "Schmerz", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Er glaubt, da\u00df ich ihm grausam bin,", "tokens": ["Er", "glaubt", ",", "da\u00df", "ich", "ihm", "grau\u00b7sam", "bin", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "PPER", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und tr\u00e4gt sein Herz zu Daphnen hin:", "tokens": ["Und", "tr\u00e4gt", "sein", "Herz", "zu", "Daph\u00b7nen", "hin", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "