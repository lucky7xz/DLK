{"dta.poem.12665": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "An Celien.  \n C. H.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Lacht ihr stunden", "tokens": ["Lacht", "ihr", "stun\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPER", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Mich von neuem an,", "tokens": ["Mich", "von", "neu\u00b7em", "an", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ADJA", "PTKVZ", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Weil sich gefunden", "tokens": ["Weil", "sich", "ge\u00b7fun\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PRF", "VVPP"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Was mich vergn\u00fcgen kan,", "tokens": ["Was", "mich", "ver\u00b7gn\u00fc\u00b7gen", "kan", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Celie, der sch\u00f6nen zier", "tokens": ["Ce\u00b7lie", ",", "der", "sch\u00f6\u00b7nen", "zier"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "ART", "ADJA", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.6": {"text": "Ist wieder hier.", "tokens": ["Ist", "wie\u00b7der", "hier", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Mein vergn\u00fcgen", "tokens": ["Mein", "ver\u00b7gn\u00fc\u00b7gen"], "token_info": ["word", "word"], "pos": ["PPOSAT", "ADJA"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Keimt von neuen auf,", "tokens": ["Keimt", "von", "neu\u00b7en", "auf", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "PTKVZ", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Di\u00df mu\u00df erliegen", "tokens": ["Di\u00df", "mu\u00df", "er\u00b7lie\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["PDS", "VMFIN", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Was hindert dessen lauff,", "tokens": ["Was", "hin\u00b7dert", "des\u00b7sen", "lauff", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PDS", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Denn die lippen meiner zier", "tokens": ["Denn", "die", "lip\u00b7pen", "mei\u00b7ner", "zier"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Sind wieder hier.", "tokens": ["Sind", "wie\u00b7der", "hier", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Meine flammen", "tokens": ["Mei\u00b7ne", "flam\u00b7men"], "token_info": ["word", "word"], "pos": ["PPOSAT", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Facht die sehnsucht an,", "tokens": ["Facht", "die", "sehn\u00b7sucht", "an", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Wer will verdammen", "tokens": ["Wer", "will", "ver\u00b7dam\u00b7men"], "token_info": ["word", "word", "word"], "pos": ["PWS", "VMFIN", "VVINF"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Di\u00df, was mich heilen kan?", "tokens": ["Di\u00df", ",", "was", "mich", "hei\u00b7len", "kan", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PWS", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Weil die br\u00fcste, Sch\u00f6nstes kind!", "tokens": ["Weil", "die", "br\u00fcs\u00b7te", ",", "Sch\u00f6ns\u00b7tes", "kind", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Hier wieder sind.", "tokens": ["Hier", "wie\u00b7der", "sind", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Auf ihrsehnen!", "tokens": ["Auf", "ihr\u00b7seh\u00b7nen", "!"], "token_info": ["word", "word", "punct"], "pos": ["APPR", "PPOSAT", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Auf! und fiedert euch!", "tokens": ["Auf", "!", "und", "fie\u00b7dert", "euch", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "$.", "KON", "VVFIN", "PPER", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Weg ihr thr\u00e4nen!", "tokens": ["Weg", "ihr", "thr\u00e4\u00b7nen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Hier ist mein freuden-reich!", "tokens": ["Hier", "ist", "mein", "freu\u00b7den\u00b7reich", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Auge, lippe, brust und schoo\u00df", "tokens": ["Au\u00b7ge", ",", "lip\u00b7pe", ",", "brust", "und", "schoo\u00df"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "$,", "VVFIN", "KON", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Sind wieder lo\u00df.", "tokens": ["Sind", "wie\u00b7der", "lo\u00df", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "La\u00df uns lieben,", "tokens": ["La\u00df", "uns", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Sch\u00f6nste! recht vergn\u00fcgt,", "tokens": ["Sch\u00f6ns\u00b7te", "!", "recht", "ver\u00b7gn\u00fcgt", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$.", "ADJD", "VVPP", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Last das betr\u00fcben", "tokens": ["Last", "das", "be\u00b7tr\u00fc\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ART", "ADJA"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Von freuden seyn besiegt:", "tokens": ["Von", "freu\u00b7den", "seyn", "be\u00b7siegt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Nach dem trauren mu\u00df erfreun", "tokens": ["Nach", "dem", "trau\u00b7ren", "mu\u00df", "er\u00b7freun"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "VVINF", "VMFIN", "VVINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Das labsal seyn.", "tokens": ["Das", "lab\u00b7sal", "seyn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "VAINF", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}