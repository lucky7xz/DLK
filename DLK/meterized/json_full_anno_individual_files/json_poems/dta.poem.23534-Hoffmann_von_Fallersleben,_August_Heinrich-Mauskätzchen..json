{"dta.poem.23534": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Mausk\u00e4tzchen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1840", "urn": "urn:nbn:de:kobv:b4-200905192626", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mausk\u00e4tzchen gab ein gro\u00dfes Fest", "tokens": ["Maus\u00b7k\u00e4tz\u00b7chen", "gab", "ein", "gro\u00b7\u00dfes", "Fest"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und hatte dazu geladen", "tokens": ["Und", "hat\u00b7te", "da\u00b7zu", "ge\u00b7la\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PAV", "VVPP"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Bekannt' und Verwandte von Ost und West", "tokens": ["Be\u00b7kannt'", "und", "Ver\u00b7wand\u00b7te", "von", "Ost", "und", "West"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "APPR", "NE", "KON", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Und lauter Ihro Gnaden.", "tokens": ["Und", "lau\u00b7ter", "Ih\u00b7ro", "Gna\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Miau miau miau", "tokens": ["Mi\u00b7au", "mi\u00b7au", "mi\u00b7au"], "token_info": ["word", "word", "word"], "pos": ["NE", "NE", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Sie trieben vielerlei Possen und Scherz,", "tokens": ["Sie", "trie\u00b7ben", "vie\u00b7ler\u00b7lei", "Pos\u00b7sen", "und", "Scherz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "KON", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und f\u00fcllten sich weidlich den Ranzen,", "tokens": ["Und", "f\u00fcll\u00b7ten", "sich", "weid\u00b7lich", "den", "Ran\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADJD", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Und weil es nun eben war im M\u00e4rz,", "tokens": ["Und", "weil", "es", "nun", "e\u00b7ben", "war", "im", "M\u00e4rz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "ADV", "VAFIN", "APPRART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "So wollten die K\u00e4tzerlein tanzen.", "tokens": ["So", "woll\u00b7ten", "die", "K\u00e4t\u00b7zer\u00b7lein", "tan\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Miau miau miau", "tokens": ["Mi\u00b7au", "mi\u00b7au", "mi\u00b7au"], "token_info": ["word", "word", "word"], "pos": ["NE", "NE", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Doch alle die gn\u00e4digen K\u00e4tzerlein,", "tokens": ["Doch", "al\u00b7le", "die", "gn\u00e4\u00b7di\u00b7gen", "K\u00e4t\u00b7zer\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Die gn\u00e4digen Kater und Katzen,", "tokens": ["Die", "gn\u00e4\u00b7di\u00b7gen", "Ka\u00b7ter", "und", "Kat\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Die konnten nichts als miauen und schrein", "tokens": ["Die", "konn\u00b7ten", "nichts", "als", "mi\u00b7au\u00b7en", "und", "schrein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VMFIN", "PIS", "KOKOM", "VVINF", "KON", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Und schluchzen und pfuchzen und pfnatzen.", "tokens": ["Und", "schluch\u00b7zen", "und", "pfuch\u00b7zen", "und", "pf\u00b7nat\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "KON", "NN", "KON", "VVINF", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Miau miau miau", "tokens": ["Mi\u00b7au", "mi\u00b7au", "mi\u00b7au"], "token_info": ["word", "word", "word"], "pos": ["NE", "NE", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Mausk\u00e4tzchen schickt nach dem Pudel hin,", "tokens": ["Maus\u00b7k\u00e4tz\u00b7chen", "schickt", "nach", "dem", "Pu\u00b7del", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der konnte das Hackebrett schlagen,", "tokens": ["Der", "konn\u00b7te", "das", "Hac\u00b7ke\u00b7brett", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Der sollte so was nach ihrem Sinn", "tokens": ["Der", "soll\u00b7te", "so", "was", "nach", "ih\u00b7rem", "Sinn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VMFIN", "ADV", "PWS", "APPR", "PPOSAT", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Auf dem Hackebrett vortragen.", "tokens": ["Auf", "dem", "Hac\u00b7ke\u00b7brett", "vor\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Miau miau miau", "tokens": ["Mi\u00b7au", "mi\u00b7au", "mi\u00b7au"], "token_info": ["word", "word", "word"], "pos": ["NE", "NE", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Der Pudel war ein gescheiter Mann,", "tokens": ["Der", "Pu\u00b7del", "war", "ein", "ge\u00b7schei\u00b7ter", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Eine b\u00fcrgerliche Canaille:", "tokens": ["Ei\u00b7ne", "b\u00fcr\u00b7ger\u00b7li\u00b7che", "Ca\u00b7nail\u00b7le", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "\u201ewas geht mich Dero Gesellschaft an,", "tokens": ["\u201e", "was", "geht", "mich", "De\u00b7ro", "Ge\u00b7sell\u00b7schaft", "an", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VVFIN", "PPER", "PDS", "NN", "PTKVZ", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ew. Gnaden Katzengebalge?\u201c", "tokens": ["Ew.", "Gna\u00b7den", "Kat\u00b7zen\u00b7ge\u00b7bal\u00b7ge", "?", "\u201c"], "token_info": ["abbreviation", "word", "word", "punct", "punct"], "pos": ["NE", "NN", "NE", "$.", "$("], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "Wau wau wau wau.", "tokens": ["Wau", "wau", "wau", "wau", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ITJ", "ITJ", "ITJ", "ITJ", "$."], "meter": "+---", "measure": "dactylic.init"}}}}}