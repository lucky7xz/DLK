{"dta.poem.9380": {"metadata": {"author": {"name": "Zesen, Philipp von", "birth": "N.A.", "death": "N.A."}, "title": "XvI .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-20684-8", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sch\u00f6nste/ wil sie mit spazieren", "tokens": ["Sch\u00f6ns\u00b7te", "/", "wil", "sie", "mit", "spa\u00b7zie\u00b7ren"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$(", "VMFIN", "PPER", "APPR", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "in den garten vor das hau\u00df/", "tokens": ["in", "den", "gar\u00b7ten", "vor", "das", "hau\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ich wil bey den h\u00e4nden fuhren", "tokens": ["Ich", "wil", "bey", "den", "h\u00e4n\u00b7den", "fuh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "ART", "ADJA", "VVFIN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Sie hinnein und wider raus/", "tokens": ["Sie", "hin\u00b7nein", "und", "wi\u00b7der", "raus", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "KON", "APPR", "NE", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "da sich die Natur bem\u00fchet", "tokens": ["da", "sich", "die", "Na\u00b7tur", "be\u00b7m\u00fc\u00b7het"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "und die sch\u00f6ne Rose bl\u00fchet.", "tokens": ["und", "die", "sch\u00f6\u00b7ne", "Ro\u00b7se", "bl\u00fc\u00b7het", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}