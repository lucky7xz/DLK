{"textgrid.poem.33527": {"metadata": {"author": {"name": "Lichtenstein, Alfred", "birth": "N.A.", "death": "N.A."}, "title": "Karnevalstraum", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich mach den Karneval sobald nicht wieder mit.", "tokens": ["Ich", "mach", "den", "Kar\u00b7ne\u00b7val", "so\u00b7bald", "nicht", "wie\u00b7der", "mit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "PTKNEG", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So schlimm ist mir mein Lebtag nicht gewesen,", "tokens": ["So", "schlimm", "ist", "mir", "mein", "Leb\u00b7tag", "nicht", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PPER", "PPOSAT", "NN", "PTKNEG", "VAPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und solche Tr\u00e4ume hab ich nie getr\u00e4umt:", "tokens": ["Und", "sol\u00b7che", "Tr\u00e4u\u00b7me", "hab", "ich", "nie", "ge\u00b7tr\u00e4umt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Auf einem harten, kahlen Wege, der der Stadt,", "tokens": ["Auf", "ei\u00b7nem", "har\u00b7ten", ",", "kah\u00b7len", "We\u00b7ge", ",", "der", "der", "Stadt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$,", "PRELS", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die ihn nicht halten konnte, fast entlaufen ist", "tokens": ["Die", "ihn", "nicht", "hal\u00b7ten", "konn\u00b7te", ",", "fast", "ent\u00b7lau\u00b7fen", "ist"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "PPER", "PTKNEG", "VVINF", "VMFIN", "$,", "ADV", "VVPP", "VAFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und nun, ein Bettler, in den Himmel wandert, schreiten", "tokens": ["Und", "nun", ",", "ein", "Bett\u00b7ler", ",", "in", "den", "Him\u00b7mel", "wan\u00b7dert", ",", "schrei\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "ADV", "$,", "ART", "NN", "$,", "APPR", "ART", "NN", "VVFIN", "$,", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ein Mann, ein Weib. \u2013 Der Mann: robust, gemein,", "tokens": ["Ein", "Mann", ",", "ein", "Weib", ".", "\u2013", "Der", "Mann", ":", "ro\u00b7bust", ",", "ge\u00b7mein", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$.", "$(", "ART", "NN", "$.", "VVFIN", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ein Raubtier, das sich auf das Fressen freut.", "tokens": ["Ein", "Raub\u00b7tier", ",", "das", "sich", "auf", "das", "Fres\u00b7sen", "freut", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PRF", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Das Weib: grazi\u00f6s und schlank, halbnackt, im Domino.", "tokens": ["Das", "Weib", ":", "gra\u00b7zi\u00b7\u00f6s", "und", "schlank", ",", "halb\u00b7nackt", ",", "im", "Do\u00b7mi\u00b7no", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "ADJD", "KON", "VVFIN", "$,", "VVFIN", "$,", "APPRART", "NN", "$."], "meter": "-++-+-+-+-+-+", "measure": "unknown.measure.septa"}, "line.7": {"text": "Herzlose Blicke stechen aus verbrauchten Augen ...", "tokens": ["Herz\u00b7lo\u00b7se", "Bli\u00b7cke", "ste\u00b7chen", "aus", "ver\u00b7brauch\u00b7ten", "Au\u00b7gen", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "APPR", "ADJA", "NN", "$("], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.8": {"text": "Kein Laster, kein Verbrechen ist ihr neu \u2013", "tokens": ["Kein", "Las\u00b7ter", ",", "kein", "Ver\u00b7bre\u00b7chen", "ist", "ihr", "neu", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "VAFIN", "PPER", "ADJD", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Und jedes h\u00e4lt wie ein Paket in einem Arm", "tokens": ["Und", "je\u00b7des", "h\u00e4lt", "wie", "ein", "Pa\u00b7ket", "in", "ei\u00b7nem", "Arm"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "VVFIN", "KOKOM", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ein Bein von mir. Mein K\u00f6rper schleift am Boden.", "tokens": ["Ein", "Bein", "von", "mir", ".", "Mein", "K\u00f6r\u00b7per", "schleift", "am", "Bo\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "$.", "PPOSAT", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und immer, wenn ich st\u00f6hnend meinen Kopf", "tokens": ["Und", "im\u00b7mer", ",", "wenn", "ich", "st\u00f6h\u00b7nend", "mei\u00b7nen", "Kopf"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "$,", "KOUS", "PPER", "ADJD", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Versuche zu erheben oder mit den H\u00e4nden mich", "tokens": ["Ver\u00b7su\u00b7che", "zu", "er\u00b7he\u00b7ben", "o\u00b7der", "mit", "den", "H\u00e4n\u00b7den", "mich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PTKZU", "VVINF", "KON", "APPR", "ART", "NN", "PPER"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "Verzweifelt an die Erde klammern will,", "tokens": ["Ver\u00b7zwei\u00b7felt", "an", "die", "Er\u00b7de", "klam\u00b7mern", "will", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "F\u00fchl ich des Mannes starke Knochen fester", "tokens": ["F\u00fchl", "ich", "des", "Man\u00b7nes", "star\u00b7ke", "Kno\u00b7chen", "fes\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ART", "NN", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Um meinen Fu\u00df sich legen ... f\u00fchle, wie des Weibes", "tokens": ["Um", "mei\u00b7nen", "Fu\u00df", "sich", "le\u00b7gen", "...", "f\u00fch\u00b7le", ",", "wie", "des", "Wei\u00b7bes"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KOUI", "PPOSAT", "NN", "PRF", "VVINF", "$(", "VVFIN", "$,", "PWAV", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Grausames, k\u00fchles Fleisch sich pl\u00f6tzlich enger pre\u00dft,", "tokens": ["Grau\u00b7sa\u00b7mes", ",", "k\u00fch\u00b7les", "Fleisch", "sich", "pl\u00f6tz\u00b7lich", "en\u00b7ger", "pre\u00dft", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "PRF", "ADJD", "ADJD", "VVFIN", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.7": {"text": "Und mutlos, hoffnungslos sink ich zusammen \u2013", "tokens": ["Und", "mut\u00b7los", ",", "hoff\u00b7nungs\u00b7los", "sink", "ich", "zu\u00b7sam\u00b7men", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "ADJD", "VVFIN", "PPER", "PTKVZ", "$("], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Die beiden aber schreiten schweigend weiter,", "tokens": ["Die", "bei\u00b7den", "a\u00b7ber", "schrei\u00b7ten", "schwei\u00b7gend", "wei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ADV", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zu jeder Greueltat mit Lust bereit.", "tokens": ["Zu", "je\u00b7der", "Greu\u00b7el\u00b7tat", "mit", "Lust", "be\u00b7reit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "APPR", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Ich mach den Karneval sobald nicht wieder mit.", "tokens": ["Ich", "mach", "den", "Kar\u00b7ne\u00b7val", "so\u00b7bald", "nicht", "wie\u00b7der", "mit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "PTKNEG", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So schlimm ist mir mein Lebtag nicht gewesen,", "tokens": ["So", "schlimm", "ist", "mir", "mein", "Leb\u00b7tag", "nicht", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word",