{"dta.poem.11974": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "7.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Einst sprach ein harter Mann, von Widerw\u00e4rtigkeiten", "tokens": ["Einst", "sprach", "ein", "har\u00b7ter", "Mann", ",", "von", "Wi\u00b7der\u00b7w\u00e4r\u00b7tig\u00b7kei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Geh\u00e4rtet, diesen Spruch von zehen H\u00e4rtigkeiten:", "tokens": ["Ge\u00b7h\u00e4r\u00b7tet", ",", "die\u00b7sen", "Spruch", "von", "ze\u00b7hen", "H\u00e4r\u00b7tig\u00b7kei\u00b7ten", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PDAT", "NN", "APPR", "CARD", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Hart ist der Berg, doch wird das Eisen ihn durchschneiden;", "tokens": ["Hart", "ist", "der", "Berg", ",", "doch", "wird", "das", "Ei\u00b7sen", "ihn", "durch\u00b7schnei\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "$,", "ADV", "VAFIN", "ART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hart ist das Eisen, doch das Feuer wird's geschmeiden.", "tokens": ["Hart", "ist", "das", "Ei\u00b7sen", ",", "doch", "das", "Feu\u00b7er", "wird's", "ge\u00b7schmei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "$,", "ADV", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Hart ist das Feur, doch wird es Wasser niederschlagen;", "tokens": ["Hart", "ist", "das", "Feur", ",", "doch", "wird", "es", "Was\u00b7ser", "nie\u00b7der\u00b7schla\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "$,", "ADV", "VAFIN", "PPER", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hart ist das Wasser, doch die Wolke wird es tragen.", "tokens": ["Hart", "ist", "das", "Was\u00b7ser", ",", "doch", "die", "Wol\u00b7ke", "wird", "es", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "$,", "ADV", "ART", "NN", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Hart ist die Wolke, doch der Wind wird sie zerstreuen;", "tokens": ["Hart", "ist", "die", "Wol\u00b7ke", ",", "doch", "der", "Wind", "wird", "sie", "zer\u00b7streu\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "$,", "ADV", "ART", "NN", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hart ist der Wind, doch wird der K\u00f6rper ihn nicht scheuen.", "tokens": ["Hart", "ist", "der", "Wind", ",", "doch", "wird", "der", "K\u00f6r\u00b7per", "ihn", "nicht", "scheu\u00b7en", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "$,", "ADV", "VAFIN", "ART", "NN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Hart ist der K\u00f6rper, doch wird Kummer ihn zertr\u00fcmmern;", "tokens": ["Hart", "ist", "der", "K\u00f6r\u00b7per", ",", "doch", "wird", "Kum\u00b7mer", "ihn", "zer\u00b7tr\u00fcm\u00b7mern", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "$,", "ADV", "VAFIN", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hart ist der Kummer, doch der Wein wird ihn entk\u00fcmmern.", "tokens": ["Hart", "ist", "der", "Kum\u00b7mer", ",", "doch", "der", "Wein", "wird", "ihn", "ent\u00b7k\u00fcm\u00b7mern", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "$,", "ADV", "ART", "NN", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Hart ist der Wein, es wird ihn doch der Schlaf begraben;", "tokens": ["Hart", "ist", "der", "Wein", ",", "es", "wird", "ihn", "doch", "der", "Schlaf", "be\u00b7gra\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "$,", "PPER", "VAFIN", "PPER", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "O harter Schlaf, den wir hier auszuschlafen haben!", "tokens": ["O", "har\u00b7ter", "Schlaf", ",", "den", "wir", "hier", "aus\u00b7zu\u00b7schla\u00b7fen", "ha\u00b7ben", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "PRELS", "PPER", "ADV", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}