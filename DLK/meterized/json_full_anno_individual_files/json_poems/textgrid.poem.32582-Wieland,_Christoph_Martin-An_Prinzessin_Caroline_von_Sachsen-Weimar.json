{"textgrid.poem.32582": {"metadata": {"author": {"name": "Wieland, Christoph Martin", "birth": "N.A.", "death": "N.A."}, "title": "An Prinzessin Caroline von Sachsen-Weimar", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In des Morgens stiller Fr\u00fche,", "tokens": ["In", "des", "Mor\u00b7gens", "stil\u00b7ler", "Fr\u00fc\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "wenn aus \u00c4ther leicht gebildet", "tokens": ["wenn", "aus", "\u00c4\u00b7ther", "leicht", "ge\u00b7bil\u00b7det"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "NN", "ADJD", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "holde Tr\u00e4ume uns umflattern,", "tokens": ["hol\u00b7de", "Tr\u00e4u\u00b7me", "uns", "um\u00b7flat\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "sah ich einen sch\u00f6nen Engel", "tokens": ["sah", "ich", "ei\u00b7nen", "sch\u00f6\u00b7nen", "En\u00b7gel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "aus der Morgenr\u00f6te langsam", "tokens": ["aus", "der", "Mor\u00b7gen\u00b7r\u00f6\u00b7te", "lang\u00b7sam"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "sich zur Erde nieder senken,", "tokens": ["sich", "zur", "Er\u00b7de", "nie\u00b7der", "sen\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPRART", "NN", "PTKVZ", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "ein Gew\u00e4chs des Paradieses", "tokens": ["ein", "Ge\u00b7w\u00e4chs", "des", "Pa\u00b7ra\u00b7die\u00b7ses"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "in den Rosenarmen tragend,", "tokens": ["in", "den", "Ro\u00b7se\u00b7nar\u00b7men", "tra\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "um es in den Scho\u00df der Erde", "tokens": ["um", "es", "in", "den", "Scho\u00df", "der", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "PPER", "APPR", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "zu verpflanzen. Und der Engel,", "tokens": ["zu", "ver\u00b7pflan\u00b7zen", ".", "Und", "der", "En\u00b7gel", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$.", "KON", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "auf das Kind des Paradieses", "tokens": ["auf", "das", "Kind", "des", "Pa\u00b7ra\u00b7die\u00b7ses"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "liebevolle Blicke heftend,", "tokens": ["lie\u00b7be\u00b7vol\u00b7le", "Bli\u00b7cke", "hef\u00b7tend", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "\u00bbwachse\u00ab, sprach er, \u00bbholde Blume,", "tokens": ["\u00bb", "wach\u00b7se", "\u00ab", ",", "sprach", "er", ",", "\u00bb", "hol\u00b7de", "Blu\u00b7me", ","], "t