{"textgrid.poem.28047": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "62.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df glauben, lieben und das leiden", "tokens": ["Da\u00df", "glau\u00b7ben", ",", "lie\u00b7ben", "und", "das", "lei\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "VVINF", "$,", "ADJA", "KON", "PDS", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die lassen sich nicht gerne scheiden;", "tokens": ["Die", "las\u00b7sen", "sich", "nicht", "ger\u00b7ne", "schei\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "PTKNEG", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der diese drey begehrt zu trennen,", "tokens": ["Der", "die\u00b7se", "drey", "be\u00b7gehrt", "zu", "tren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PDAT", "CARD", "VVPP", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den darff man keinen Christen nennen.", "tokens": ["Den", "darff", "man", "kei\u00b7nen", "Chris\u00b7ten", "nen\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PIS", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Dann der, dem leiden ist verdrie\u00dflich,", "tokens": ["Dann", "der", ",", "dem", "lei\u00b7den", "ist", "ver\u00b7drie\u00df\u00b7lich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "$,", "PRELS", "VVINF", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Bey dem ist auch das glauben mi\u00dflich;", "tokens": ["Bey", "dem", "ist", "auch", "das", "glau\u00b7ben", "mi\u00df\u00b7lich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "VAFIN", "ADV", "PDS", "VVFIN", "ADJD", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.7": {"text": "Wo Glauben nicht daheime wohnet,", "tokens": ["Wo", "Glau\u00b7ben", "nicht", "da\u00b7hei\u00b7me", "woh\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "PTKNEG", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ist auch dem lieben abgelohnet.", "tokens": ["Ist", "auch", "dem", "lie\u00b7ben", "ab\u00b7ge\u00b7loh\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Drum k\u00fcmmts, da\u00df viel vom Glauben weichen,", "tokens": ["Drum", "k\u00fcmmts", ",", "da\u00df", "viel", "vom", "Glau\u00b7ben", "wei\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "$,", "KOUS", "ADV", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Damit sie gute Tag erreichen,", "tokens": ["Da\u00b7mit", "sie", "gu\u00b7te", "Tag", "er\u00b7rei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Und da\u00df sie den so m\u00f6rdlich hassen,", "tokens": ["Und", "da\u00df", "sie", "den", "so", "m\u00f6rd\u00b7lich", "has\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ART", "ADV", "ADJD", "VVINF", "$,"], "m