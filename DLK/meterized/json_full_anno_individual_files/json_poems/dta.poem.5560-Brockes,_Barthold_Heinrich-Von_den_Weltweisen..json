{"dta.poem.5560": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Weltweisen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Soll ich denn etwa denen glauben,", "tokens": ["Soll", "ich", "denn", "et\u00b7wa", "de\u00b7nen", "glau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "PDS", "VVFIN", "$,"], "meter": "+--+---+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Die, voll gelehrter Toll-und Thorheit immerzu,", "tokens": ["Die", ",", "voll", "ge\u00b7lehr\u00b7ter", "Toll\u00b7\u00b7und", "Thor\u00b7heit", "im\u00b7mer\u00b7zu", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "ADJD", "ADJA", "NN", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Durch ", "tokens": ["Durch"], "token_info": ["word"], "pos": ["APPR"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Dem Geist fast alles Wissen rauben?", "tokens": ["Dem", "Geist", "fast", "al\u00b7les", "Wis\u00b7sen", "rau\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die, da sie doch recht l\u00e4cherlich", "tokens": ["Die", ",", "da", "sie", "doch", "recht", "l\u00e4\u00b7cher\u00b7lich"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "$,", "KOUS", "PPER", "ADV", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sich \u00fcber die Gewalt der Sinne weit erh\u00f6h\u2019n,", "tokens": ["Sich", "\u00fc\u00b7ber", "die", "Ge\u00b7walt", "der", "Sin\u00b7ne", "weit", "er\u00b7h\u00f6h'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Mit ihrer spitzigen und stoltzen Tummheit sich", "tokens": ["Mit", "ih\u00b7rer", "spit\u00b7zi\u00b7gen", "und", "stolt\u00b7zen", "Tumm\u00b7heit", "sich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "KON", "ADJA", "NN", "PRF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Was einzubilden unterstehn?", "tokens": ["Was", "ein\u00b7zu\u00b7bil\u00b7den", "un\u00b7ter\u00b7stehn", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "PIS", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}