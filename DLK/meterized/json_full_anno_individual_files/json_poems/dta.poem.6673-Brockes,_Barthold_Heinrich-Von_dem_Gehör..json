{"dta.poem.6673": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Geh\u00f6r.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Durch einem Ton, der langsam, wird das Hertz", "tokens": ["Durch", "ei\u00b7nem", "Ton", ",", "der", "lang\u00b7sam", ",", "wird", "das", "Hertz"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "ADJD", "$,", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mit Ruhe, Traurigkeit, und langer Weil\u2019 erf\u00fcllet:", "tokens": ["Mit", "Ru\u00b7he", ",", "Trau\u00b7rig\u00b7keit", ",", "und", "lan\u00b7ger", "Weil'", "er\u00b7f\u00fcl\u00b7let", ":"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,", "KON", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Geschwinde T\u00f6n\u2019 hingegen w\u00fcrcken Schertz,", "tokens": ["Ge\u00b7schwin\u00b7de", "T\u00f6n'", "hin\u00b7ge\u00b7gen", "w\u00fcr\u00b7cken", "Schertz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Vergn\u00fcgen, Freude, Zorn. Nachdem die Dr\u00fccke nun", "tokens": ["Ver\u00b7gn\u00fc\u00b7gen", ",", "Freu\u00b7de", ",", "Zorn", ".", "Nach\u00b7dem", "die", "Dr\u00fc\u00b7cke", "nun"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "$.", "KOUS", "ART", "NN", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Durch unterschiedne T\u00f6n, an den Gedancken hafften,", "tokens": ["Durch", "un\u00b7ter\u00b7schied\u00b7ne", "T\u00f6n", ",", "an", "den", "Ge\u00b7dan\u00b7cken", "haff\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und unterschiedne W\u00fcrckungen ver\u00fcben,", "tokens": ["Und", "un\u00b7ter\u00b7schied\u00b7ne", "W\u00fcr\u00b7ckun\u00b7gen", "ver\u00b7\u00fc\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Nach dem sind unsre Seelen auch getrieben", "tokens": ["Nach", "dem", "sind", "uns\u00b7re", "See\u00b7len", "auch", "ge\u00b7trie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "VAFIN", "PPOSAT", "NN", "ADV", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Zu unterschiednen Leidenschafften.", "tokens": ["Zu", "un\u00b7ter\u00b7schied\u00b7nen", "Lei\u00b7den\u00b7schaff\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}