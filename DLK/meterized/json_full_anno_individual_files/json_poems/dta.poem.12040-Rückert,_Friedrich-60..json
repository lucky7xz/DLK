{"dta.poem.12040": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "60.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Buch, aus dem du viel Gedanken nehmen kannst,", "tokens": ["Ein", "Buch", ",", "aus", "dem", "du", "viel", "Ge\u00b7dan\u00b7ken", "neh\u00b7men", "kannst", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "PRELS", "PPER", "PIAT", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sei immer dankbar ihm f\u00fcr das was du gewannst.", "tokens": ["Sei", "im\u00b7mer", "dank\u00b7bar", "ihm", "f\u00fcr", "das", "was", "du", "ge\u00b7wannst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "PPER", "APPR", "ART", "PRELS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Doch was ein Ganzes ist, wird nicht so leicht zerrissen;", "tokens": ["Doch", "was", "ein", "Gan\u00b7zes", "ist", ",", "wird", "nicht", "so", "leicht", "zer\u00b7ris\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "VAFIN", "$,", "VAFIN", "PTKNEG", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es gibt ein andres Buch, dem sollst du mehr Dank wissen,", "tokens": ["Es", "gibt", "ein", "and\u00b7res", "Buch", ",", "dem", "sollst", "du", "mehr", "Dank", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,", "PDS", "VMFIN", "PPER", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Von festverschlungenen Gedankenganggeweben,", "tokens": ["Von", "fest\u00b7ver\u00b7schlun\u00b7ge\u00b7nen", "Ge\u00b7dan\u00b7ken\u00b7gang\u00b7ge\u00b7we\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das du als Ganzes nur aufnehmen kannst ins Leben.", "tokens": ["Das", "du", "als", "Gan\u00b7zes", "nur", "auf\u00b7neh\u00b7men", "kannst", "ins", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "KOUS", "NN", "ADV", "VVINF", "VMFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}