{"dta.poem.8382": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "Martin Opitzen Sch\u00e4fferey  \n von der Nimfen Hercinie.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1630", "urn": "urn:nbn:de:kobv:b4-200905197863", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Sch\u00e4fferey Von der Nimfen Hercinie. Breslau, 1630."}, "poem": {"stanza.1": {"line.1": {"text": "Du geist der du allhier bewohnst den \u00f6den plan/", "tokens": ["Du", "geist", "der", "du", "all\u00b7hier", "be\u00b7wohnst", "den", "\u00f6\u00b7den", "plan", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "PPER", "ADV", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du seist auch wer du wilt/ wann ich vollbringen kan", "tokens": ["Du", "seist", "auch", "wer", "du", "wilt", "/", "wann", "ich", "voll\u00b7brin\u00b7gen", "kan"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "PWS", "PPER", "VMFIN", "$(", "PWAV", "PPER", "VVINF", "VMFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Was mein gem\u00fcte sucht durch deine kunst vndt rhat/", "tokens": ["Was", "mein", "ge\u00b7m\u00fc\u00b7te", "sucht", "durch", "dei\u00b7ne", "kunst", "vndt", "rhat", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "ADJA", "VVFIN", "APPR", "PPOSAT", "NN", "KON", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So wil ich dir allhier an dieser gr\u00fcnen stat", "tokens": ["So", "wil", "ich", "dir", "all\u00b7hier", "an", "die\u00b7ser", "gr\u00fc\u00b7nen", "stat"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "ADV", "APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Erh\u00f6hen ein altar/ darauff zur danckbarkeit", "tokens": ["Er\u00b7h\u00f6\u00b7hen", "ein", "al\u00b7tar", "/", "dar\u00b7auff", "zur", "dan\u00b7ck\u00b7bar\u00b7keit"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ART", "NN", "$(", "PAV", "APPRART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ein opffer das du liebst soll brennen iederzeit.", "tokens": ["Ein", "opf\u00b7fer", "das", "du", "liebst", "soll", "bren\u00b7nen", "ie\u00b7der\u00b7zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "PPER", "VVFIN", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Du riesenherr/ du artzt/ du berggott/ komm herf\u00fcr;", "tokens": ["Du", "rie\u00b7sen\u00b7herr", "/", "du", "artzt", "/", "du", "berg\u00b7gott", "/", "komm", "her\u00b7f\u00fcr", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "NN", "$(", "PPER", "VVFIN", "$(", "PPER", "ADJD", "$(", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Der jene so dich ehrt erwartet deiner hier.", "tokens": ["Der", "je\u00b7ne", "so", "dich", "ehrt", "er\u00b7war\u00b7tet", "dei\u00b7ner", "hier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PDS", "ADV", "PPER", "VVFIN", "VVFIN", "PPOSAT", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}