{"dta.poem.2142": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Gott so wohl im Kleinen, als im  \n Gro\u00dfen, gro\u00df.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Gott l\u00e4\u00dft, dergestalt, im Gro\u00dfen seiner Allmacht", "tokens": ["Gott", "l\u00e4\u00dft", ",", "der\u00b7ge\u00b7stalt", ",", "im", "Gro\u00b7\u00dfen", "sei\u00b7ner", "All\u00b7macht"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "$,", "ADV", "$,", "APPRART", "NN", "PPOSAT", "NN"], "meter": "-+--+-+-+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Stralen scheinen,", "tokens": ["Stra\u00b7len", "schei\u00b7nen", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Da\u00df er kleiner nicht im Kleinen:", "tokens": ["Da\u00df", "er", "klei\u00b7ner", "nicht", "im", "Klei\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "PTKNEG", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Das, was klein, ist, nach dem Umfang und nach seinen", "tokens": ["Das", ",", "was", "klein", ",", "ist", ",", "nach", "dem", "Um\u00b7fang", "und", "nach", "sei\u00b7nen"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "$,", "PRELS", "ADJD", "$,", "VAFIN", "$,", "APPR", "ART", "NN", "KON", "APPR", "PPOSAT"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.5": {"text": "eignen Gr\u00f6\u00dfen,", "tokens": ["eig\u00b7nen", "Gr\u00f6\u00b7\u00dfen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Die so gar gering und winzig, eigentlich nicht abzumessen;", "tokens": ["Die", "so", "gar", "ge\u00b7ring", "und", "win\u00b7zig", ",", "ei\u00b7gent\u00b7lich", "nicht", "ab\u00b7zu\u00b7mes\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "ADJD", "KON", "ADJD", "$,", "ADV", "PTKNEG", "VVIZU", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.7": {"text": "Sondern, nach der weisen Allmacht dessen, dem es einerley,", "tokens": ["Son\u00b7dern", ",", "nach", "der", "wei\u00b7sen", "All\u00b7macht", "des\u00b7sen", ",", "dem", "es", "ei\u00b7ner\u00b7ley", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "APPR", "ART", "ADJA", "NN", "PDS", "$,", "PRELS", "PPER", "ADV", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.8": {"text": "Ob ein Erdenkreis erschaffen, ob ein Staub bereitet sey.", "tokens": ["Ob", "ein", "Er\u00b7den\u00b7kreis", "er\u00b7schaf\u00b7fen", ",", "ob", "ein", "Staub", "be\u00b7rei\u00b7tet", "sey", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVPP", "$,", "KOUS", "ART", "NN", "VVPP", "VAFIN", "$."], "meter": "--+-+-+-+-+-+-+", "measure": "anapaest.init"}, "line.9": {"text": "Eben der, der eine Blume bildet, formet, f\u00e4rbt und zieret,", "tokens": ["E\u00b7ben", "der", ",", "der", "ei\u00b7ne", "Blu\u00b7me", "bil\u00b7det", ",", "for\u00b7met", ",", "f\u00e4rbt", "und", "zie\u00b7ret", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "$,", "PRELS", "ART", "NN", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.10": {"text": "Hat den gro\u00dfen Sonnenk\u00f6rper zugerichtet und formiret:", "tokens": ["Hat", "den", "gro\u00b7\u00dfen", "Son\u00b7nen\u00b7k\u00f6r\u00b7per", "zu\u00b7ge\u00b7rich\u00b7tet", "und", "for\u00b7mi\u00b7ret", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "VVPP", "KON", "VVFIN", "$."], "meter": "+-+-+-+-+-+--+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.11": {"text": "Eben der, der einen Engel in dem Reich des Lichts gemacht,", "tokens": ["E\u00b7ben", "der", ",", "der", "ei\u00b7nen", "En\u00b7gel", "in", "dem", "Reich", "des", "Lichts", "ge\u00b7macht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "$,", "PRELS", "ART", "NN", "APPR", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.12": {"text": "Hat auch diesen kleinen Wurm auf der Welt hervorgebracht.", "tokens": ["Hat", "auch", "die\u00b7sen", "klei\u00b7nen", "Wurm", "auf", "der", "Welt", "her\u00b7vor\u00b7ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PDAT", "ADJA", "NN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.13": {"text": "Es kann auch die gr\u00f6\u00dfte Gr\u00f6\u00dfe Gott so wenig Ehre geben,", "tokens": ["Es", "kann", "auch", "die", "gr\u00f6\u00df\u00b7te", "Gr\u00f6\u00b7\u00dfe", "Gott", "so", "we\u00b7nig", "Eh\u00b7re", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ART", "ADJA", "NN", "NN", "ADV", "PIAT", "NN", "VVINF", "$,"], "meter": "-+--+-+-+-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.14": {"text": "Noch (da f\u00fcr ihn nichtes gro\u00df) seine wahre Gr\u00f6\u00df\u2019 erheben;", "tokens": ["Noch", "(", "da", "f\u00fcr", "ihn", "nich\u00b7tes", "gro\u00df", ")", "sei\u00b7ne", "wah\u00b7re", "Gr\u00f6\u00df'", "er\u00b7he\u00b7ben", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "ADV", "APPR", "PPER", "PIS", "ADJD", "$(", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "----+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.15": {"text": "Als, die allerkleinste Kleinheit, die er einem Stoff be-", "tokens": ["Als", ",", "die", "al\u00b7ler\u00b7kleins\u00b7te", "Klein\u00b7heit", ",", "die", "er", "ei\u00b7nem", "Stoff", "be"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "$,", "ART", "ADJA", "NN", "$,", "PRELS", "PPER", "ART", "NN", "TRUNC"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.16": {"text": "stimmt,", "tokens": ["stimmt", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.17": {"text": "Seiner allgemeinen Vorsicht, Lieb und Allmacht etwas", "tokens": ["Sei\u00b7ner", "all\u00b7ge\u00b7mei\u00b7nen", "Vor\u00b7sicht", ",", "Lieb", "und", "All\u00b7macht", "et\u00b7was"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "NN", "KON", "NN", "ADV"], "meter": "+-+-+-+-+--+-+", "measure": "trochaic.septa.relaxed"}, "line.18": {"text": "nimmt.", "tokens": ["nimmt", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+", "measure": "single.up"}, "line.19": {"text": "Ja, wir sind fast, durch die Kleinheit, zur Bewundrung", "tokens": ["Ja", ",", "wir", "sind", "fast", ",", "durch", "die", "Klein\u00b7heit", ",", "zur", "Be\u00b7wund\u00b7rung"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PTKANT", "$,", "PPER", "VAFIN", "ADV", "$,", "APPR", "ART", "NN", "$,", "APPRART", "NN"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.20": {"text": "mehr bewogen,", "tokens": ["mehr", "be\u00b7wo\u00b7gen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVPP", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.21": {"text": "Da die Wunderkunst im Kleinen unsern Augen fast ent-", "tokens": ["Da", "die", "Wun\u00b7der\u00b7kunst", "im", "Klei\u00b7nen", "un\u00b7sern", "Au\u00b7gen", "fast", "ent"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPRART", "NN", "PPOSAT", "NN", "ADV", "TRUNC"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.22": {"text": "zogen,", "tokens": ["zo\u00b7gen", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.23": {"text": "Und nur blo\u00df ein geistger Blick, was f\u00fcr Gr\u00f6\u00df im Kleinen", "tokens": ["Und", "nur", "blo\u00df", "ein", "geist\u00b7ger", "Blick", ",", "was", "f\u00fcr", "Gr\u00f6\u00df", "im", "Klei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "ART", "ADJA", "NN", "$,", "PRELS", "APPR", "NN", "APPRART", "NN"], "meter": "--+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.24": {"text": "steckt,", "tokens": ["steckt", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.25": {"text": "Mit erstaunendem Bewundern, unserm Gott zum Ruhm,", "tokens": ["Mit", "er\u00b7stau\u00b7nen\u00b7dem", "Be\u00b7wun\u00b7dern", ",", "un\u00b7serm", "Gott", "zum", "Ruhm", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "PPOSAT", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.26": {"text": "entdeckt.", "tokens": ["ent\u00b7deckt", "."], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-+", "measure": "iambic.single"}}}}}