{"dta.poem.8131": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "8.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1773", "urn": "urn:nbn:de:kobv:b4-200905199504", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Da ihr Gang Flug, und ihr Ausruf Gesang ward der Entz\u00fcckung,", "tokens": ["Da", "ihr", "Gang", "Flug", ",", "und", "ihr", "Aus\u00b7ruf", "Ge\u00b7sang", "ward", "der", "Ent\u00b7z\u00fc\u00b7ckung", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "NN", "$,", "KON", "PPOSAT", "NN", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+++-+-+-+-+-+-", "measure": "unknown.measure.octa.plus"}, "line.2": {"text": "Da vom Gefild\u2019 her sich ihr Triumphzug zum Gerichtsthron", "tokens": ["Da", "vom", "Ge\u00b7fild'", "her", "sich", "ihr", "Tri\u00b7umph\u00b7zug", "zum", "Ge\u00b7richts\u00b7thron"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "APZR", "PRF", "PPOSAT", "NN", "APPRART", "NN"], "meter": "-+-+---+-+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Emporschwang, nahm zu dem Erb\u2019 auf Er, den am Kreuz", "tokens": ["Em\u00b7por\u00b7schwang", ",", "nahm", "zu", "dem", "Erb'", "auf", "Er", ",", "den", "am", "Kreuz"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "APPR", "ART", "NN", "APPR", "PPER", "$,", "PRELS", "APPRART", "NN"], "meter": "-+-+--+-+--+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Gott sah',", "tokens": ["Gott", "sah'", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "In das Lichtreich auf, die des Altars Blutruf vom Gericht lossprach!", "tokens": ["In", "das", "Lich\u00b7treich", "auf", ",", "die", "des", "Al\u00b7tars", "Blu\u00b7truf", "vom", "Ge\u00b7richt", "los\u00b7sprach", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$,", "PRELS", "ART", "NN", "NN", "APPRART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+-+--+-+", "measure": "trochaic.octa.plus.relaxed"}}}}}