{"textgrid.poem.36210": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Bitte", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach la\u00df die teure Frau in bittern Leiden,", "tokens": ["Ach", "la\u00df", "die", "teu\u00b7re", "Frau", "in", "bit\u00b7tern", "Lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du milde K\u00f6nigin, mir nicht versinken!", "tokens": ["Du", "mil\u00b7de", "K\u00f6\u00b7ni\u00b7gin", ",", "mir", "nicht", "ver\u00b7sin\u00b7ken", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ihr Herz erfrische bald ein himmlisch Winken", "tokens": ["Ihr", "Herz", "er\u00b7fri\u00b7sche", "bald", "ein", "himm\u00b7lisch", "Win\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "ART", "ADJD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Aus jenen Augen, die mit Licht uns weiden.", "tokens": ["Aus", "je\u00b7nen", "Au\u00b7gen", ",", "die", "mit", "Licht", "uns", "wei\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "PRELS", "APPR", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Sie darf der Morgenr\u00f6te Glanz nicht meiden,", "tokens": ["Sie", "darf", "der", "Mor\u00b7gen\u00b7r\u00f6\u00b7te", "Glanz", "nicht", "mei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vor der die Sterne all' ins Dunkel sinken,", "tokens": ["Vor", "der", "die", "Ster\u00b7ne", "all'", "ins", "Dun\u00b7kel", "sin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "PIAT", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und darf aus deinem Bilde Gottheit trinken;", "tokens": ["Und", "darf", "aus", "dei\u00b7nem", "Bil\u00b7de", "Got\u00b7theit", "trin\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "APPR", "PPOSAT", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sie lebt in Lieb', und liebend wird sie scheiden.", "tokens": ["Sie", "lebt", "in", "Lieb'", ",", "und", "lie\u00b7bend", "wird", "sie", "schei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,", "KON", "ADJD", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "O Mutter! tot und arm sind jetzt die Herzen,", "tokens": ["O", "Mut\u00b7ter", "!", "tot", "und", "arm", "sind", "jetzt", "die", "Her\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "ADJD", "KON", "ADJD", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch wenn auch alle von dir abgefallen,", "tokens": ["Doch", "wenn", "auch", "al\u00b7le", "von", "dir", "ab\u00b7ge\u00b7fal\u00b7len", ","], "toke