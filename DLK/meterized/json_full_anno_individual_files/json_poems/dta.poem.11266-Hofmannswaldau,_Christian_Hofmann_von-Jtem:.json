{"dta.poem.11266": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Jtem:", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es ist der Schellenberg itzt den Frantz\u00f6schen horden", "tokens": ["Es", "ist", "der", "Schel\u00b7len\u00b7berg", "itzt", "den", "Frant\u00b7z\u00f6\u00b7schen", "hor\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Vor ihre pralerey zur trauer-schelle worden.", "tokens": ["Vor", "ih\u00b7re", "pra\u00b7le\u00b7rey", "zur", "trau\u00b7e\u00b7rschel\u00b7le", "wor\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPRART", "NN", "VAPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nun hat der Schellenberg den nahmen von den schellen,", "tokens": ["Nun", "hat", "der", "Schel\u00b7len\u00b7berg", "den", "nah\u00b7men", "von", "den", "schel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ART", "VVFIN", "APPR", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Davon dem Ludewig noch stets die ohren gellen.", "tokens": ["Da\u00b7von", "dem", "Lu\u00b7de\u00b7wig", "noch", "stets", "die", "oh\u00b7ren", "gel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NE", "ADV", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}