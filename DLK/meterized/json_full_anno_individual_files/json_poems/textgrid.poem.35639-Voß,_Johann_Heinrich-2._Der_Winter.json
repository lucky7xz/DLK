{"textgrid.poem.35639": {"metadata": {"author": {"name": "Vo\u00df, Johann Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "2. Der Winter", "genre": "verse", "period": "N.A.", "pub_year": 1771, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Reif im Haupthaar, den Bart voller Eis, taumelt der Alte Winter anitzt aus der benachteten", "tokens": ["Reif", "im", "Haupt\u00b7haar", ",", "den", "Bart", "vol\u00b7ler", "Eis", ",", "tau\u00b7melt", "der", "Al\u00b7te", "Win\u00b7ter", "a\u00b7nitzt", "aus", "der", "be\u00b7nach\u00b7te\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPRART", "NN", "$,", "ART", "NN", "ADJA", "NN", "$,", "VVFIN", "ART", "ADJA", "NN", "ADV", "APPR", "ART", "ADJA"], "meter": "+-+--+--+-+-+-+-+-+--+--", "measure": "trochaic.octa.plus.relaxed"}, "line.2": {"text": "H\u00f6hle Gr\u00f6nlands hervor, rufet, erbost umher", "tokens": ["H\u00f6h\u00b7le", "Gr\u00f6n\u00b7lands", "her\u00b7vor", ",", "ru\u00b7fet", ",", "er\u00b7bost", "um\u00b7her"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["NN", "NN", "PTKVZ", "$,", "VVFIN", "$,", "VVFIN", "PTKVZ"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Schauend, Boreas weitstreifenden Br\u00fcdern, und", "tokens": ["Schau\u00b7end", ",", "Bo\u00b7re\u00b7as", "weit\u00b7strei\u00b7fen\u00b7den", "Br\u00fc\u00b7dern", ",", "und"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ADJD", "$,", "NE", "ADJA", "NN", "$,", "KON"], "meter": "+-+---+--+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Schirrt das wilde Gespann l\u00e4rmend am Deichsel des", "tokens": ["Schirrt", "das", "wil\u00b7de", "Ge\u00b7spann", "l\u00e4r\u00b7mend", "am", "Deich\u00b7sel", "des"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN", "ADJD", "APPRART", "NN", "ART"], "meter": "+-+-+-+--+--", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Schwarzen Wagens. Und bald wird er im Donnersturm", "tokens": ["Schwar\u00b7zen", "Wa\u00b7gens", ".", "Und", "bald", "wird", "er", "im", "Don\u00b7ner\u00b7sturm"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "NN", "$.", "KON", "ADV", "VAFIN", "PPER", "APPRART", "NN"], "meter": "+-+---+--+-+", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Durch den \u00c4ther daher st\u00fcrzen, mit Flocken und", "tokens": ["Durch", "den", "\u00c4\u00b7ther", "da\u00b7her", "st\u00fcr\u00b7zen", ",", "mit", "Flo\u00b7cken", "und"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PAV", "VVINF", "$,", "APPR", "NN", "KON"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.7": {"text": "Scharfen Schlo\u00dfen bewehrt. Dann flieh, Autumnus! Dann", "tokens": ["Schar\u00b7fen", "Schlo\u00b7\u00dfen", "be\u00b7wehrt", ".", "Dann", "flieh", ",", "Au\u00b7tum\u00b7nus", "!", "Dann"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word"], "pos": ["NN", "NN", "VVPP", "$.", "ADV", "VVFIN", "$,", "NN", "$.", "ADV"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.8": {"text": "Flieh, Pomona! Und du, sanfter Ly\u00e4us, der", "tokens": ["Flieh", ",", "Po\u00b7mo\u00b7na", "!", "Und", "du", ",", "sanf\u00b7ter", "Ly\u00b7\u00e4us", ",", "der"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["NN", "$,", "NE", "$.", "KON", "PPER", "$,", "ADJD", "NE", "$,", "PRELS"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.9": {"text": "Obotritiens Flur herbere Trauben br\u00e4unt!", "tokens": ["O\u00b7bot\u00b7ri\u00b7ti\u00b7ens", "Flur", "her\u00b7be\u00b7re", "Trau\u00b7ben", "br\u00e4unt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Flieht des Rasenden Grimm, welcher den heulenden", "tokens": ["Flieht", "des", "Ra\u00b7sen\u00b7den", "Grimm", ",", "wel\u00b7cher", "den", "heu\u00b7len\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "NE", "$,", "PRELS", "ART", "ADJA"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.11": {"text": "Forst entwurzelt, den Fels spaltet, und luftige", "tokens": ["Forst", "ent\u00b7wur\u00b7zelt", ",", "den", "Fels", "spal\u00b7tet", ",", "und", "luf\u00b7ti\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVPP", "$,", "ART", "NN", "VVFIN", "$,", "KON", "ADJA"], "meter": "+-+--+---+-+", "measure": "trochaic.penta.relaxed"}, "line.12": {"text": "Berg' ersch\u00fcttert! Verkriecht, tief in der Terra Scho\u00df,", "tokens": ["Ber\u00b7g'", "er\u00b7sch\u00fct\u00b7tert", "!", "Ver\u00b7kriecht", ",", "tief", "in", "der", "Ter\u00b7ra", "Scho\u00df", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVPP", "$.", "NN", "$,", "ADJD", "APPR", "ART", "NN", "NN", "$,"], "meter": "+--+--+-+-+-+", "measure": "dactylic.di.plus"}, "line.13": {"text": "Euch, ihr Nymphen des Quells, welcher, versteinert, bald", "tokens": ["Euch", ",", "ihr", "Nym\u00b7phen", "des", "Quells", ",", "wel\u00b7cher", ",", "ver\u00b7stei\u00b7nert", ",", "bald"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["PPER", "$,", "PPOSAT", "NN", "ART", "NN", "$,", "PRELS", "$,", "VVPP", "$,", "ADV"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.14": {"text": "Zwischen Blumen nicht mehr murmelnd sich schl\u00e4ngeln wird,", "tokens": ["Zwi\u00b7schen", "Blu\u00b7men", "nicht", "mehr", "mur\u00b7melnd", "sich", "schl\u00e4n\u00b7geln", "wird", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKNEG", "ADV", "ADJD", "PRF", "VVINF", "VAFIN", "$,"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.15": {"text": "Und du, brausender Belt, decke, mit st\u00e4hlernem", "tokens": ["Und", "du", ",", "brau\u00b7sen\u00b7der", "Belt", ",", "de\u00b7cke", ",", "mit", "st\u00e4h\u00b7ler\u00b7nem"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["KON", "PPER", "$,", "ADJA", "NN", "$,", "VVFIN", "$,", "APPR", "ADJA"], "meter": "--+--+--+---", "measure": "anapaest.tri.plus"}, "line.16": {"text": "Harnisch, dich vor der Wut grauser Orkane zu!", "tokens": ["Har\u00b7nisch", ",", "dich", "vor", "der", "Wut", "grau\u00b7ser", "Or\u00b7ka\u00b7ne", "zu", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRF", "APPR", "ART", "NN", "ADJA", "NN", "PTKVZ", "$."], "meter": "+--+-++--+-+", "measure": "iambic.hexa.invert"}}, "stanza.2": {"line.1": {"text": "Aber, Knaben, itzt eilt, eilt, mit geschliffner Axt,", "tokens": ["A\u00b7ber", ",", "Kna\u00b7ben", ",", "itzt", "eilt", ",", "eilt", ",", "mit", "ge\u00b7schliff\u00b7ner", "Axt", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "NN", "$,", "ADV", "VVFIN", "$,", "VVFIN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "In die W\u00e4lder; zerst\u00fcckt wolkenber\u00fchrende", "tokens": ["In", "die", "W\u00e4l\u00b7der", ";", "zer\u00b7st\u00fcckt", "wol\u00b7ken\u00b7be\u00b7r\u00fch\u00b7ren\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "$.", "VVPP", "NN"], "meter": "--+--+-+-+--", "measure": "anapaest.di.plus"}, "line.3": {"text": "B\u00e4um', und f\u00fchrt sie auf lautknarrender Axe zu", "tokens": ["B\u00e4um'", ",", "und", "f\u00fchrt", "sie", "auf", "laut\u00b7knar\u00b7ren\u00b7der", "A\u00b7xe", "zu"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "KON", "VVFIN", "PPER", "APPR", "ADJA", "NN", "PTKZU"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Meinem Br[\u00fcckner]! Und ihr, G\u00f6tter des Feuerherds,", "tokens": ["Mei\u00b7nem", "Br", "\u00fcck\u00b7ner", "!", "Und", "ihr", ",", "G\u00f6t\u00b7ter", "des", "Feu\u00b7er\u00b7herds", ","], "token_info": ["word", "word", "punct", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "ADJA", "$(", "$.", "KON", "PPER", "$,", "NN", "ART", "NN", "$,"], "meter": "+-+--+-+--+", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Milde Laren, o sch\u00fctzt, sch\u00fctzt ihn, mit flammender", "tokens": ["Mil\u00b7de", "La\u00b7ren", ",", "o", "sch\u00fctzt", ",", "sch\u00fctzt", "ihn", ",", "mit", "flam\u00b7men\u00b7der"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "NN", "$,", "FM", "VVFIN", "$,", "VVFIN", "PPER", "$,", "APPR", "NN"], "meter": "--+--++--+--", "measure": "anapaest.di.plus"}, "line.6": {"text": "Eiche, vor der Gewalt drohender St\u00fcrme, die", "tokens": ["Ei\u00b7che", ",", "vor", "der", "Ge\u00b7walt", "dro\u00b7hen\u00b7der", "St\u00fcr\u00b7me", ",", "die"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["NN", "$,", "APPR", "ART", "NN", "ADJA", "NN", "$,", "PRELS"], "meter": "+-+--+---+-+", "measure": "trochaic.penta.relaxed"}, "line.7": {"text": "Ums beschneiete Dach br\u00fcllen: wenn er, entz\u00fcckt,", "tokens": ["Ums", "be\u00b7schnei\u00b7e\u00b7te", "Dach", "br\u00fcl\u00b7len", ":", "wenn", "er", ",", "ent\u00b7z\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KOUI", "ADJA", "NN", "VVINF", "$.", "KOUS", "PPER", "$,", "VVPP", "$,"], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.8": {"text": "Am Kamine, sein Lied, gleich dem harmonischen", "tokens": ["Am", "Ka\u00b7mi\u00b7ne", ",", "sein", "Lied", ",", "gleich", "dem", "har\u00b7mo\u00b7ni\u00b7schen"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "PPOSAT", "NN", "$,", "ADV", "ART", "ADJA"], "meter": "+-+-+++-+-+-", "measure": "unknown.measure.septa"}, "line.9": {"text": "Schwan der Sprea, beginnt, oder den tr\u00e4gen Lauf", "tokens": ["Schwan", "der", "Sprea", ",", "be\u00b7ginnt", ",", "o\u00b7der", "den", "tr\u00e4\u00b7gen", "Lauf"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "$,", "VVFIN", "$,", "KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.10": {"text": "Der verl\u00e4ngerten Nacht mit den Ges\u00e4ngen spornt,", "tokens": ["Der", "ver\u00b7l\u00e4n\u00b7ger\u00b7ten", "Nacht", "mit", "den", "Ge\u00b7s\u00e4n\u00b7gen", "spornt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.11": {"text": "Die uns Gellert gelehrt, und der helvetische", "tokens": ["Die", "uns", "Gel\u00b7lert", "ge\u00b7lehrt", ",", "und", "der", "hel\u00b7ve\u00b7ti\u00b7sche"], "token_info": ["word", "word", "word", "word", "punct", "word", "wo