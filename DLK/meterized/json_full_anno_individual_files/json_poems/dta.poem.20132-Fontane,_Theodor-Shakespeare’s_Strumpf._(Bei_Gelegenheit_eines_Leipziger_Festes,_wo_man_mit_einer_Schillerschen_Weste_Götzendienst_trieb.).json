{"dta.poem.20132": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Shakespeare\u2019s Strumpf.  \n (Bei Gelegenheit eines Leipziger Festes, wo man mit einer  \n  Schillerschen Weste  G\u00f6tzendienst trieb.)", "genre": "Lyrik", "period": "N.A.", "pub_year": "1851", "urn": "urn:nbn:de:kobv:b4-200905191321", "language": ["de:0.99"], "booktitle": "Fontane, Theodor: Gedichte. Berlin, 1851."}, "poem": {"stanza.1": {"line.1": {"text": "Hochgesprungen, lautgesungen!", "tokens": ["Hoch\u00b7ge\u00b7sprun\u00b7gen", ",", "laut\u00b7ge\u00b7sun\u00b7gen", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn verschimmelt auch und dumpf,", "tokens": ["Wenn", "ver\u00b7schim\u00b7melt", "auch", "und", "dumpf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVPP", "ADV", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sei\u2019s! wir haben ihn errungen", "tokens": ["Sei's", "!", "wir", "ha\u00b7ben", "ihn", "er\u00b7run\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$.", "PPER", "VAFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "William Shakespeare\u2019s wollnen Strumpf.", "tokens": ["Wil\u00b7li\u00b7am", "Sha\u00b7ke\u00b7spea\u00b7re's", "woll\u00b7nen", "Strumpf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADJA", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.2": {"line.1": {"text": "Sieg! wir haben jetzt die Str\u00fcmpfe,", "tokens": ["Sieg", "!", "wir", "ha\u00b7ben", "jetzt", "die", "Str\u00fcmp\u00b7fe", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Haben jetzt das heil\u2019ge Paar,", "tokens": ["Ha\u00b7ben", "jetzt", "das", "heil'\u00b7ge", "Paar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Drinnen er trotz Moor und S\u00fcmpfe", "tokens": ["Drin\u00b7nen", "er", "trotz", "Moor", "und", "S\u00fcmp\u00b7fe"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sicher vor Erk\u00e4ltung war.", "tokens": ["Si\u00b7cher", "vor", "Er\u00b7k\u00e4l\u00b7tung", "war", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Sieg! wir huld\u2019gen jetzt dem Strumpfe,", "tokens": ["Sieg", "!", "wir", "huld'\u00b7gen", "jetzt", "dem", "Strump\u00b7fe", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der der Str\u00fcmpfe Shakespear\u2019 ist,", "tokens": ["Der", "der", "Str\u00fcmp\u00b7fe", "Sha\u00b7ke\u00b7spea\u00b7r'", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "NE", "VAFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Denn er reicht uns bis zum Rumpfe,", "tokens": ["Denn", "er", "reicht", "uns", "bis", "zum", "Rump\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Weil er fast zwei Ellen mi\u00dft.", "tokens": ["Weil", "er", "fast", "zwei", "El\u00b7len", "mi\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "CARD", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Sieg! wir haben jetzt die Str\u00fcmpfe,", "tokens": ["Sieg", "!", "wir", "ha\u00b7ben", "jetzt", "die", "Str\u00fcmp\u00b7fe", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dran er putzte, wischte, rieb", "tokens": ["Dran", "er", "putz\u00b7te", ",", "wischte", ",", "rieb"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["PAV", "PPER", "VVFIN", "$,", "VVFIN", "$,", "VVFIN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Manchesmal die Federst\u00fcmpfe,", "tokens": ["Man\u00b7ches\u00b7mal", "die", "Fe\u00b7ders\u00b7t\u00fcmp\u00b7fe", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Als er seinen Hamlet schrieb.", "tokens": ["Als", "er", "sei\u00b7nen", "Ham\u00b7let", "schrieb", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Drum wer je ein Lied geleiert,", "tokens": ["Drum", "wer", "je", "ein", "Lied", "ge\u00b7lei\u00b7ert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PWS", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn er sich nicht lumpen l\u00e4\u00dft,", "tokens": ["Wenn", "er", "sich", "nicht", "lum\u00b7pen", "l\u00e4\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "PTKNEG", "VVINF", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Singt Oktaven er, und feiert", "tokens": ["Singt", "Ok\u00b7ta\u00b7ven", "er", ",", "und", "fei\u00b7ert"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "NN", "PPER", "$,", "KON", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Unser n\u00e4chstes Shakespearefest.", "tokens": ["Un\u00b7ser", "n\u00e4chs\u00b7tes", "Sha\u00b7ke\u00b7spe\u00b7a\u00b7re\u00b7fest", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-++-+", "measure": "unknown.measure.hexa"}}, "stanza.6": {"line.1": {"text": "Unsren Enkeln wird man melden:", "tokens": ["Un\u00b7sren", "En\u00b7keln", "wird", "man", "mel\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PIS", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "\u201eeuer Ahn, da\u00df ihr es wisst,", "tokens": ["\u201e", "eu\u00b7er", "Ahn", ",", "da\u00df", "ihr", "es", "wisst", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "War auch Einer von den Helden,", "tokens": ["War", "auch", "Ei\u00b7ner", "von", "den", "Hel\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PIS", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die den Shakespeare-Strumpf gek\u00fcsst.\u201c", "tokens": ["Die", "den", "Sha\u00b7ke\u00b7spea\u00b7re\u00b7Strumpf", "ge\u00b7k\u00fcsst", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ART", "NN", "VVPP", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Drum herbei was Arm\u2019 und Beine,", "tokens": ["Drum", "her\u00b7bei", "was", "Arm'", "und", "Bei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "PWS", "NN", "KON", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Unsrer harret schon Triumph,", "tokens": ["Uns\u00b7rer", "har\u00b7ret", "schon", "Tri\u00b7umph", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und dem Shakespeare-Strumpf-Vereine", "tokens": ["Und", "dem", "Sha\u00b7ke\u00b7spea\u00b7re\u00b7Strumpf\u00b7Ver\u00b7ei\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["KON", "ART", "NE"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Helfen ", "tokens": ["Hel\u00b7fen"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}}}}}