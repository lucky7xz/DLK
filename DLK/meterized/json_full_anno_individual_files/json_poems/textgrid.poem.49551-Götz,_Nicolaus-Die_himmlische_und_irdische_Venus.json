{"textgrid.poem.49551": {"metadata": {"author": {"name": "G\u00f6tz, Nicolaus", "birth": "N.A.", "death": "N.A."}, "title": "Die himmlische und irdische Venus", "genre": "verse", "period": "N.A.", "pub_year": 1751, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mich lie\u00df Apoll auf des Parna\u00dfes H\u00f6hen", "tokens": ["Mich", "lie\u00df", "A\u00b7poll", "auf", "des", "Par\u00b7na\u00b7\u00dfes", "H\u00f6\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NE", "APPR", "ART", "NN", "NN"], "meter": "-+---+-+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Die himmlische und ird'sche Venus sehen;", "tokens": ["Die", "himm\u00b7li\u00b7sche", "und", "ird'\u00b7sche", "Ve\u00b7nus", "se\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "KON", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die ein' umgab von Tugenden ein Chor:", "tokens": ["Die", "ein'", "um\u00b7gab", "von", "Tu\u00b7gen\u00b7den", "ein", "Chor", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "APPR", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ich sah bey ihr die Weisheit selber stehen;", "tokens": ["Ich", "sah", "bey", "ihr", "die", "Weis\u00b7heit", "sel\u00b7ber", "ste\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ihr Finger wie\u00df entfernt des Gl\u00fcckes Thor.", "tokens": ["Ihr", "Fin\u00b7ger", "wie\u00df", "ent\u00b7fernt", "des", "Gl\u00fc\u00b7ckes", "Thor", "."], "token_info": ["word", "word", "word", "word