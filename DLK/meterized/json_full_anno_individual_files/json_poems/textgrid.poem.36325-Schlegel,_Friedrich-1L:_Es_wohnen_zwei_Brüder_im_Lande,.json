{"textgrid.poem.36325": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es wohnen zwei Br\u00fcder im Lande,", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es wohnen zwei Br\u00fcder im Lande,", "tokens": ["Es", "woh\u00b7nen", "zwei", "Br\u00fc\u00b7der", "im", "Lan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "CARD", "NN", "APPRART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Die hausen weit und breit;", "tokens": ["Die", "hau\u00b7sen", "weit", "und", "breit", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie haben viele Verwandte,", "tokens": ["Sie", "ha\u00b7ben", "vie\u00b7le", "Ver\u00b7wand\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Zahllose in dieser Zeit.", "tokens": ["Zahl\u00b7lo\u00b7se", "in", "die\u00b7ser", "Zeit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PDAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Sie sind sich mehrenteils Feinde,", "tokens": ["Sie", "sind", "sich", "meh\u00b7ren\u00b7teils", "Fein\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PRF", "ADV", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ein jeder will haben die Welt:", "tokens": ["Ein", "je\u00b7der", "will", "ha\u00b7ben", "die", "Welt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VMFIN", "VAFIN", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Mitunter auch einmal Freunde,", "tokens": ["Mi\u00b7tun\u00b7ter", "auch", "ein\u00b7mal", "Freun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "So lange die Welt noch h\u00e4lt.", "tokens": ["So", "lan\u00b7ge", "die", "Welt", "noch", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Sie rei\u00dfen sie auf und nieder,", "tokens": ["Sie", "rei\u00b7\u00dfen", "sie", "auf", "und", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Da\u00df H\u00f6ren und Sehn ihr vergeht;", "tokens": ["Da\u00df", "H\u00f6\u00b7ren", "und", "Sehn", "ihr", "ver\u00b7geht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "VVFIN", "PPER", "VVFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Sie schleppen sie hin und wieder,", "tokens": ["Sie", "schlep\u00b7pen", "sie", "hin", "und", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "KON", "ADV", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Weil keiner den andern versteht.", "tokens": ["Weil", "kei\u00b7ner", "den", "an\u00b7dern", "ver\u00b7steht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "ADJA", "VVFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.4": {"line.1": {"text": "Der \u00c4ltste schlendert im Rechten,", "tokens": ["Der", "\u00c4lts\u00b7te", "schlen\u00b7dert", "im", "Rech\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "So wie er das Rechte versteht;", "tokens": ["So", "wie", "er", "das", "Rech\u00b7te", "ver\u00b7steht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Der J\u00fcngste schludert im Schlechten,", "tokens": ["Der", "J\u00fcngs\u00b7te", "schlu\u00b7dert", "im", "Schlech\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Was er als das Rechte verdreht.", "tokens": ["Was", "er", "als", "das", "Rech\u00b7te", "ver\u00b7dreht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "KOUS", "ART", "NN", "VVFIN", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}}, "stanza.5": {"line.1": {"text": "Sie f\u00fchren Reden unz\u00e4hlig,", "tokens": ["Sie", "f\u00fch\u00b7ren", "Re\u00b7den", "un\u00b7z\u00e4h\u00b7lig", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "ADJD", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und h\u00f6ren sich selber so gern;", "tokens": ["Und", "h\u00f6\u00b7ren", "sich", "sel\u00b7ber", "so", "gern", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADV", "ADV", "ADV", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Sie sprechen sich selber gef\u00e4llig,", "tokens": ["Sie", "spre\u00b7chen", "sich", "sel\u00b7ber", "ge\u00b7f\u00e4l\u00b7lig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "ADJD", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Doch ist in den Worten kein Kern.", "tokens": ["Doch", "ist", "in", "den", "Wor\u00b7ten", "kein", "Kern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "ART", "NN", "PIAT", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.6": {"line.1": {"text": "Das sind die feindlichen Br\u00fcder,", "tokens": ["Das", "sind", "die", "feind\u00b7li\u00b7chen", "Br\u00fc\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Der Alte hei\u00dft Schlendrian;", "tokens": ["Der", "Al\u00b7te", "hei\u00dft", "Schlen\u00b7dri\u00b7an", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NE", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und genialisch bellt wieder", "tokens": ["Und", "ge\u00b7ni\u00b7a\u00b7lisch", "bellt", "wie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "ADV"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Der Kleine Schludrian.", "tokens": ["Der", "Klei\u00b7ne", "Schlu\u00b7dri\u00b7an", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Der Alte bricht sich die Steine", "tokens": ["Der", "Al\u00b7te", "bricht", "sich", "die", "Stei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PRF", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Vom Grunde der Mauer heraus;", "tokens": ["Vom", "Grun\u00b7de", "der", "Mau\u00b7er", "he\u00b7raus", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Zu flicken und st\u00fccken das Seine,", "tokens": ["Zu", "fli\u00b7cken", "und", "st\u00fc\u00b7cken", "das", "Sei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "KON", "VVFIN", "ART", "NE", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "So Schornstein als Speisehaus.", "tokens": ["So", "Schorn\u00b7stein", "als", "Spei\u00b7se\u00b7haus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KOUS", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.8": {"line.1": {"text": "Der Junge w\u00fcrfelt in Freude", "tokens": ["Der", "Jun\u00b7ge", "w\u00fcr\u00b7felt", "in", "Freu\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die Steine mit wechselnder Hand;", "tokens": ["Die", "Stei\u00b7ne", "mit", "wech\u00b7seln\u00b7der", "Hand", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Er mauert sich sein Geb\u00e4ude", "tokens": ["Er", "mau\u00b7ert", "sich", "sein", "Ge\u00b7b\u00e4u\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "PPOSAT", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "In lustigen, fliegenden Sand.", "tokens": ["In", "lus\u00b7ti\u00b7gen", ",", "flie\u00b7gen\u00b7den", "Sand", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.9": {"line.1": {"text": "Das sind die bauenden Leute,", "tokens": ["Das", "sind", "die", "bau\u00b7en\u00b7den", "Leu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die flicken und bauen die Welt;", "tokens": ["Die", "fli\u00b7cken", "und", "bau\u00b7en", "die", "Welt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVINF", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Sie flicken und bauen f\u00fcr heute,", "tokens": ["Sie", "fli\u00b7cken", "und", "bau\u00b7en", "f\u00fcr", "heu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "KON", "VVFIN", "APPR", "ADV", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Auf morgen ist niemand gestellt.", "tokens": ["Auf", "mor\u00b7gen", "ist", "nie\u00b7mand", "ge\u00b7stellt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VAFIN", "PIS", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.10": {"line.1": {"text": "Es pfeift sein Lied so weiter", "tokens": ["Es", "pfeift", "sein", "Lied", "so", "wei\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ADV", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der muntre Schludrian;", "tokens": ["Der", "mun\u00b7tre", "Schlu\u00b7dri\u00b7an", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Voll Angst steht auf der Leiter", "tokens": ["Voll", "Angst", "steht", "auf", "der", "Lei\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der alte Schlendrian.", "tokens": ["Der", "al\u00b7te", "Schlen\u00b7dri\u00b7an", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Es hei\u00dft, wenn ich nicht irre,", "tokens": ["Es", "hei\u00dft", ",", "wenn", "ich", "nicht", "ir\u00b7re", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ihr Vater Schlechtrian;", "tokens": ["Ihr", "Va\u00b7ter", "Schlech\u00b7tri\u00b7an", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der in der Zeiten Gewirre", "tokens": ["Der", "in", "der", "Zei\u00b7ten", "Ge\u00b7wir\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Das Rechte nicht finden kann.", "tokens": ["Das", "Rech\u00b7te", "nicht", "fin\u00b7den", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.12": {"line.1": {"text": "Er kann aus dem Schlamm sich nicht winden,", "tokens": ["Er", "kann", "aus", "dem", "Schlamm", "sich", "nicht", "win\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "ART", "NN", "PRF", "PTKNEG", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Noch \u00e4ndern seinen Sinn;", "tokens": ["Noch", "\u00e4n\u00b7dern", "sei\u00b7nen", "Sinn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er kann das Ziel nicht finden,", "tokens": ["Er", "kann", "das", "Ziel", "nicht", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und tappt im Dunkeln hin.", "tokens": ["Und", "tappt", "im", "Dun\u00b7keln", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Er hat es all' vergessen,", "tokens": ["Er", "hat", "es", "all'", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PIS", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und h\u00e4lt sich die Ohren zu;", "tokens": ["Und", "h\u00e4lt", "sich", "die", "Oh\u00b7ren", "zu", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die S\u00f6hne zanken vermessen,", "tokens": ["Die", "S\u00f6h\u00b7ne", "zan\u00b7ken", "ver\u00b7mes\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und lassen ihm keine Ruh.", "tokens": ["Und", "las\u00b7sen", "ihm", "kei\u00b7ne", "Ruh", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.14": {"line.1": {"text": "Das sind die Br\u00fcder im Lande,", "tokens": ["Das", "sind", "die", "Br\u00fc\u00b7der", "im", "Lan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die schreien so weit und breit;", "tokens": ["Die", "schrei\u00b7en", "so", "weit", "und", "breit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Es l\u00e4rmen all ihre Verwandte,", "tokens": ["Es", "l\u00e4r\u00b7men", "all", "ih\u00b7re", "Ver\u00b7wand\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und machen den Geist der Zeit.", "tokens": ["Und", "ma\u00b7chen", "den", "Geist", "der", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.15": {"line.1": {"text": "Es wohnen zwei Br\u00fcder im Lande,", "tokens": ["Es", "woh\u00b7nen", "zwei", "Br\u00fc\u00b7der", "im", "Lan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "CARD", "NN", "APPRART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Die hausen weit und breit;", "tokens": ["Die", "hau\u00b7sen", "weit", "und", "breit", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "ADJD", "$."], "meter"