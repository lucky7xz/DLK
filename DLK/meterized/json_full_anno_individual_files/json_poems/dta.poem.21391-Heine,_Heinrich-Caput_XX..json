{"dta.poem.21391": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Caput XX.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-30602-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Und als ich den braven Fisch verzehrt,", "tokens": ["Und", "als", "ich", "den", "bra\u00b7ven", "Fisch", "ver\u00b7zehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Die Gans ward aufgetragen.", "tokens": ["Die", "Gans", "ward", "auf\u00b7ge\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Mutter frug wieder wohl dies, wohl das,", "tokens": ["Die", "Mut\u00b7ter", "frug", "wie\u00b7der", "wohl", "dies", ",", "wohl", "das", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADV", "PDS", "$,", "ADV", "PDS", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Mitunter verf\u00e4ngliche Fragen.", "tokens": ["Mi\u00b7tun\u00b7ter", "ver\u00b7f\u00e4ng\u00b7li\u00b7che", "Fra\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "\u201emein liebes Kind! in welchem Land", "tokens": ["\u201e", "mein", "lie\u00b7bes", "Kind", "!", "in", "wel\u00b7chem", "Land"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "PPOSAT", "ADJA", "NN", "$.", "APPR", "PWAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "L\u00e4\u00dft sich am besten leben?", "tokens": ["L\u00e4\u00dft", "sich", "am", "bes\u00b7ten", "le\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PTKA", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Hier oder in Frankreich? und welchem Volk", "tokens": ["Hier", "o\u00b7der", "in", "Fran\u00b7kreich", "?", "und", "wel\u00b7chem", "Volk"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "KON", "APPR", "NE", "$.", "KON", "PWAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wirst du den Vorzug geben?\u201c", "tokens": ["Wirst", "du", "den", "Vor\u00b7zug", "ge\u00b7ben", "?", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Die deutsche Gans, lieb M\u00fctterlein,", "tokens": ["Die", "deut\u00b7sche", "Gans", ",", "lieb", "M\u00fct\u00b7ter\u00b7lein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ist gut, jedoch die Franzosen,", "tokens": ["Ist", "gut", ",", "je\u00b7doch", "die", "Fran\u00b7zo\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$,", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sie stopfen die G\u00e4nse besser als wir,", "tokens": ["Sie", "stop\u00b7fen", "die", "G\u00e4n\u00b7se", "bes\u00b7ser", "als", "wir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD", "KOKOM", "PPER", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Auch haben sie bessere Saucen. \u2013", "tokens": ["Auch", "ha\u00b7ben", "sie", "bes\u00b7se\u00b7re", "Sau\u00b7cen", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJA", "NN", "$.", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.4": {"line.1": {"text": "Und als die Gans sich wieder empfahl,", "tokens": ["Und", "als", "die", "Gans", "sich", "wie\u00b7der", "emp\u00b7fahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "PRF", "ADV", "VVFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Da machten ihre Aufwartung", "tokens": ["Da", "mach\u00b7ten", "ih\u00b7re", "Auf\u00b7war\u00b7tung"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die Apfelsinen, sie schmeckten so s\u00fc\u00df,", "tokens": ["Die", "Ap\u00b7fel\u00b7si\u00b7nen", ",", "sie", "schmeck\u00b7ten", "so", "s\u00fc\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ganz \u00fcber alle Erwartung.", "tokens": ["Ganz", "\u00fc\u00b7ber", "al\u00b7le", "Er\u00b7war\u00b7tung", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PIAT", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}}}}