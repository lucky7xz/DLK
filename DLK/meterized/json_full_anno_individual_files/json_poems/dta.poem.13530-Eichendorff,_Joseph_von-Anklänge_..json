{"dta.poem.13530": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Ankl\u00e4nge .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn auf Bergen bl\u00fcht die Fr\u00fche,", "tokens": ["Wenn", "auf", "Ber\u00b7gen", "bl\u00fcht", "die", "Fr\u00fc\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wieder buntbewegt die Stra\u00dfen,", "tokens": ["Wie\u00b7der", "bunt\u00b7be\u00b7wegt", "die", "Stra\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Freut sich alles, wie es gl\u00fche,", "tokens": ["Freut", "sich", "al\u00b7les", ",", "wie", "es", "gl\u00fc\u00b7he", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PIS", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Himmelw\u00e4rts die Erde bl\u00fche:", "tokens": ["Him\u00b7mel\u00b7w\u00e4rts", "die", "Er\u00b7de", "bl\u00fc\u00b7he", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}