{"textgrid.poem.39255": {"metadata": {"author": {"name": "Strachwitz, Moritz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sind heute dreiunddrei\u00dfig Jahr,", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sind heute dreiunddrei\u00dfig Jahr,", "tokens": ["Sind", "heu\u00b7te", "drei\u00b7und\u00b7drei\u00b7\u00dfig", "Jahr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "CARD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Seit ich kein Segel sah,", "tokens": ["Seit", "ich", "kein", "Se\u00b7gel", "sah", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es steht der Turm unwandelbar,", "tokens": ["Es", "steht", "der", "Turm", "un\u00b7wan\u00b7del\u00b7bar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Kett' ist ewig da.", "tokens": ["Die", "Kett'", "ist", "e\u00b7wig", "da", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sie haben gemauert den Delphin", "tokens": ["Sie", "ha\u00b7ben", "ge\u00b7mau\u00b7ert", "den", "Del\u00b7phin"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "VVPP", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "In lichtlos Felsgestein", "tokens": ["In", "licht\u00b7los", "Fels\u00b7ge\u00b7stein"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJD", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und unerreichbar \u00fcber ihn", "tokens": ["Und", "un\u00b7er\u00b7reich\u00b7bar", "\u00fc\u00b7ber", "ihn"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ein winzig Fensterlein.", "tokens": ["Ein", "win\u00b7zig", "Fens\u00b7ter\u00b7lein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Nicht, da\u00df ich fern von Licht und Tag,", "tokens": ["Nicht", ",", "da\u00df", "ich", "fern", "von", "Licht", "und", "Tag", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "$,", "KOUS", "PPER", "ADJD", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Macht mir das Herz so schwer,", "tokens": ["Macht", "mir", "das", "Herz", "so", "schwer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.11": {"text": "Als da\u00df ich dich nicht zu schaun vermag,", "tokens": ["Als", "da\u00df", "ich", "dich", "nicht", "zu", "schaun", "ver\u00b7mag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "PRF", "PTKNEG", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.12": {"text": "Mein heiliges blaues Meer!", "tokens": ["Mein", "hei\u00b7li\u00b7ges", "blau\u00b7es", "Meer", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Ich h\u00f6re nicht, wie die Brandung rollt,", "tokens": ["Ich", "h\u00f6\u00b7re", "nicht", ",", "wie", "die", "Bran\u00b7dung", "rollt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und keiner M\u00f6we Geschrill.", "tokens": ["Und", "kei\u00b7ner", "M\u00f6\u00b7we", "Ge\u00b7schrill", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Und wenn die Kette nicht rasseln wollt',", "tokens": ["Und", "wenn", "die", "Ket\u00b7te", "nicht", "ras\u00b7seln", "wollt'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "PTKNEG", "VVINF", "VMFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "So w\u00e4r' es totenstill.", "tokens": ["So", "w\u00e4r'", "es", "to\u00b7tens\u00b7till", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sie bauten wohl fern vom Meer den Turm,", "tokens": ["Sie", "bau\u00b7ten", "wohl", "fern", "vom", "Meer", "den", "Turm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Wo keine Woge prallt,", "tokens": ["Wo", "kei\u00b7ne", "Wo\u00b7ge", "prallt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Kein Bootsmann pfeift, und pfeift kein Sturm,", "tokens": ["Kein", "Boots\u00b7mann", "pfeift", ",", "und", "pfeift", "kein", "Sturm", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "$,", "KON", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Kein Schu\u00df den Sturm durchschallt.", "tokens": ["Kein", "Schu\u00df", "den", "Sturm", "durch\u00b7schallt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Nicht, da\u00df man in schweigende Nacht mich warf,", "tokens": ["Nicht", ",", "da\u00df", "man", "in", "schwei\u00b7gen\u00b7de", "Nacht", "mich", "warf", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "$,", "KOUS", "PIS", "APPR", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.10": {"text": "Macht mir das Herz so schwer,", "tokens": ["Macht", "mir", "das", "Herz", "so", "schwer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.11": {"text": "Als da\u00df ich dich nicht h\u00f6ren darf,", "tokens": ["Als", "da\u00df", "ich", "dich", "nicht", "h\u00f6\u00b7ren", "darf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "PRF", "PTKNEG", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Mein tiefaufdonnerndes Meer!", "tokens": ["Mein", "tie\u00b7fauf\u00b7don\u00b7nern\u00b7des", "Meer", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Mein greises Gebein ist schwer und leer,", "tokens": ["Mein", "grei\u00b7ses", "Ge\u00b7bein", "ist", "schwer", "und", "leer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mein Leib wird nimmer heil,", "tokens": ["Mein", "Leib", "wird", "nim\u00b7mer", "heil", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Faust schwingt nimmer die Lunte mehr", "tokens": ["Die", "Faust", "schwingt", "nim\u00b7mer", "die", "Lun\u00b7te", "mehr"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "NN", "ADV"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und nimmer das Enterbeil! \u2013", "tokens": ["Und", "nim\u00b7mer", "das", "En\u00b7ter\u00b7beil", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADV", "ART", "NN", "$.", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Die gro\u00dfe Flagge auf dem Mast,", "tokens": ["Die", "gro\u00b7\u00dfe", "Flag\u00b7ge", "auf", "dem", "Mast", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Breitseit' lasset sehn,", "tokens": ["Die", "Breit\u00b7seit'", "las\u00b7set", "sehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und Jungens, wen aufs Korn ihr fa\u00dft,", "tokens": ["Und", "Jun\u00b7gens", ",", "wen", "aufs", "Korn", "ihr", "fa\u00dft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "PWS", "APPRART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der Teufel hole den! \u2013", "tokens": ["Der", "Teu\u00b7fel", "ho\u00b7le", "den", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "$.", "$("], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.9": {"text": "Nicht, da\u00df ich verwelkt in Haft und Bann,", "tokens": ["Nicht", ",", "da\u00df", "ich", "ver\u00b7welkt", "in", "Haft", "und", "Bann", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "$,", "KOUS", "PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.10": {"text": "Macht mir das Herz so schwer,", "tokens": ["Macht", "mir", "das", "Herz", "so", "schwer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.11": {"text": "Als da\u00df ich auf dir nicht fechten kann,", "tokens": ["Als", "da\u00df", "ich", "auf", "dir", "nicht", "fech\u00b7ten", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "APPR", "PPER", "PTKNEG", "VVINF", "VMFIN", "$,"], "meter": "+---+-+-+", "measure": "dactylic.init"}, "line.12": {"text": "Mein kampfersch\u00fcttertes Meer!", "tokens": ["Mein", "kamp\u00b7fer\u00b7sch\u00fct\u00b7ter\u00b7tes", "Meer", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.4": {"line.1": {"text": "Nun drauf und dran, geentert keck,", "tokens": ["Nun", "drauf", "und", "dran", ",", "ge\u00b7en\u00b7tert", "keck", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "KON", "PAV", "$,", "VVPP", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und feuert noch einmal!", "tokens": ["Und", "feu\u00b7ert", "noch", "ein\u00b7mal", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ha Schiff an Schiff und Deck an Deck,", "tokens": ["Ha", "Schiff", "an", "Schiff", "und", "Deck", "an", "Deck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPR", "NN", "KON", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und ich der Admiral! \u2013", "tokens": ["Und", "ich", "der", "Ad\u00b7mi\u00b7ral", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPER", "ART", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "O fiel' ich doch im Kugelgezisch!", "tokens": ["O", "fiel'", "ich", "doch", "im", "Ku\u00b7gel\u00b7ge\u00b7zisch", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Hier lieg' ich siech und wund,", "tokens": ["Hier", "lieg'", "ich", "siech", "und", "wund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Hinschmachtend, wie im Sand ein Fisch,", "tokens": ["Hin\u00b7schmach\u00b7tend", ",", "wie", "im", "Sand", "ein", "Fisch", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PWAV", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und sterbend, wie ein Hund.", "tokens": ["Und", "ster\u00b7bend", ",", "wie", "ein", "Hund", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "PWAV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Nicht, da\u00df ich sterbe Zoll um Zoll,", "tokens": ["Nicht", ",", "da\u00df", "ich", "ster\u00b7be", "Zoll", "um", "Zoll", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "$,", "KOUS", "PPER", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Macht mir das Herz so schwer,", "tokens": ["Macht", "mir", "das", "Herz", "so", "schwer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.11": {"text": "Als da\u00df ich auf dir nicht sterben soll,", "tokens": ["Als", "da\u00df", "ich", "auf", "dir", "nicht", "ster\u00b7ben", "soll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "APPR", "PPER", "PTKNEG", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.12": {"text": "Mein oft bezwungenes Meer!", "tokens": ["Mein", "oft", "be\u00b7zwun\u00b7ge\u00b7nes", "Meer", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADV", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.5": {"line.1": {"text": "Die Segel h\u00e4ngt das Schiff im Leid,", "tokens": ["Die", "Se\u00b7gel", "h\u00e4ngt", "das", "Schiff", "im", "Leid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein schwarzes, verwitwetes Weib,", "tokens": ["Ein", "schwar\u00b7zes", ",", "ver\u00b7wit\u00b7we\u00b7tes", "Weib", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Die Flagge deckt als Sterbekleid", "tokens": ["Die", "Flag\u00b7ge", "deckt", "als", "Ster\u00b7be\u00b7kleid"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "KOUS", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den toten Heldenleib.", "tokens": ["Den", "to\u00b7ten", "Hel\u00b7den\u00b7leib", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Er sinkt ins Meer von der Spiegelwand,", "tokens": ["Er", "sinkt", "ins", "Meer", "von", "der", "Spie\u00b7gel\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Das bebt in heiliger Scheu. \u2013", "tokens": ["Das", "bebt", "in", "hei\u00b7li\u00b7ger", "Scheu", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VVFIN", "APPR", "ADJA", "NN", "$.", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Mich aber scharren sie in den Sand", "tokens": ["Mich", "a\u00b7ber", "schar\u00b7ren", "sie", "in", "den", "Sand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-++-+", "measure": "unknown.measure.penta"}, "line.8": {"text": "Und schie\u00dfen nicht einmal dabei!", "tokens": ["Und", "schie\u00b7\u00dfen", "nicht", "ein\u00b7mal", "da\u00b7bei", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "ADV", "PAV", "$