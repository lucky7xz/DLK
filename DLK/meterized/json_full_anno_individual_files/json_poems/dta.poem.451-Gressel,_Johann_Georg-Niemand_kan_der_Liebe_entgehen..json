{"dta.poem.451": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Niemand kan der Liebe entgehen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Die Welt hegt nichts in sich/ da\u00df Amors Trieb nicht sp\u00fchret/", "tokens": ["Die", "Welt", "hegt", "nichts", "in", "sich", "/", "da\u00df", "A\u00b7mors", "Trieb", "nicht", "sp\u00fch\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIS", "APPR", "PRF", "$(", "KOUS", "NE", "NN", "PTKNEG", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Mensch-und Geister/ ja die G\u00f6tter selber r\u00fchret", "tokens": ["Der", "Men\u00b7schund", "Geis\u00b7ter", "/", "ja", "die", "G\u00f6t\u00b7ter", "sel\u00b7ber", "r\u00fch\u00b7ret"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$(", "ADV", "ART", "NN", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Purpur ist nicht frey von solcher Leydenschafft/", "tokens": ["Der", "Pur\u00b7pur", "ist", "nicht", "frey", "von", "sol\u00b7cher", "Ley\u00b7den\u00b7schafft", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKNEG", "ADJD", "APPR", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die von der Liebe kommt: es sp\u00fchret ihre Krafft", "tokens": ["Die", "von", "der", "Lie\u00b7be", "kommt", ":", "es", "sp\u00fch\u00b7ret", "ih\u00b7re", "Krafft"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "$.", "PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So wol ein hoher Geist/ als ungelehrte Bauren/", "tokens": ["So", "wol", "ein", "ho\u00b7her", "Geist", "/", "als", "un\u00b7ge\u00b7lehr\u00b7te", "Bau\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "$(", "KOUS", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Es weis sie allesamt ", "tokens": ["Es", "weis", "sie", "al\u00b7le\u00b7samt"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "PTKVZ", "PPER", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wenn jemand gleich sein Feur im Busen h\u00e4lt verdeckt/", "tokens": ["Wenn", "je\u00b7mand", "gleich", "sein", "Feur", "im", "Bu\u00b7sen", "h\u00e4lt", "ver\u00b7deckt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "PPOSAT", "NN", "APPRART", "NN", "VVFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Hat es doch/ eh man meynt/ schon Flammen ausgeheckt.", "tokens": ["Hat", "es", "doch", "/", "eh", "man", "meynt", "/", "schon", "Flam\u00b7men", "aus\u00b7ge\u00b7heckt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "$(", "KOUS", "PIS", "VVFIN", "$(", "ADV", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}