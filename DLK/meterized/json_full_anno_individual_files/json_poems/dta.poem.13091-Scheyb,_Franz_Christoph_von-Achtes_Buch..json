{"dta.poem.13091": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Achtes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201eda w\u00e4r es fest gegr\u00fcndt \u201e, sprach die Bildhauerey,", "tokens": ["\u201e", "da", "w\u00e4r", "es", "fest", "ge\u00b7gr\u00fcndt", "\u201e", ",", "sprach", "die", "Bild\u00b7hau\u00b7e\u00b7rey", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VAFIN", "PPER", "ADJD", "VVPP", "$(", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+--++-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "\u201eso fest, als ein Gem\u00e4ur in einer Schilderey.", "tokens": ["\u201e", "so", "fest", ",", "als", "ein", "Ge\u00b7m\u00e4ur", "in", "ei\u00b7ner", "Schil\u00b7de\u00b7rey", "."], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "PTKVZ", "$,", "KOUS", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "235\u201dMein, rede nichts von mir, betrachte deine Mauern,", "tokens": ["\"", "Mein", ",", "re\u00b7de", "nichts", "von", "mir", ",", "be\u00b7trach\u00b7te", "dei\u00b7ne", "Mau\u00b7ern", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "$,", "VVFIN", "PIS", "APPR", "PPER", "$,", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u201ean welchen oft die Zeit und Arbeit zu bedauern.", "tokens": ["\u201e", "an", "wel\u00b7chen", "oft", "die", "Zeit", "und", "Ar\u00b7beit", "zu", "be\u00b7dau\u00b7ern", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PWAT", "ADV", "ART", "NN", "KON", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "\u201ewarum verw\u00fchlst du oft, was du so k\u00fcnstlich baust?", "tokens": ["\u201e", "wa\u00b7rum", "ver\u00b7w\u00fchlst", "du", "oft", ",", "was", "du", "so", "k\u00fcnst\u00b7lich", "baust", "?"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "VVFIN", "PPER", "ADV", "$,", "PWS", "PPER", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "\u201ewie manch Mahl schielest du, wann du dein Werck beschaust;", "tokens": ["\u201e", "wie", "manch", "Mahl", "schie\u00b7lest", "du", ",", "wann", "du", "dein", "Werck", "be\u00b7schaust", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "PIAT", "NN", "VVFIN", "PPER", "$,", "PWAV", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "\u201ewarum? weil was zuvor auf dem Papier gepranget,", "tokens": ["\u201e", "wa\u00b7rum", "?", "weil", "was", "zu\u00b7vor", "auf", "dem", "Pa\u00b7pier", "ge\u00b7pran\u00b7get", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "$.", "KOUS", "PIS", "ADV", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "240\u201dNun seinem Grund zu schwer in Eisen-Banden hanget.", "tokens": ["\"", "Nun", "sei\u00b7nem", "Grund", "zu", "schwer", "in", "Ei\u00b7sen\u00b7Ban\u00b7den", "han\u00b7get", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "PPOSAT", "NN", "PTKA", "ADJD", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "\u201ekurz: ich verlange dich und deinen Grund-Stein nicht;", "tokens": ["\u201e", "kurz", ":", "ich", "ver\u00b7lan\u00b7ge", "dich", "und", "dei\u00b7nen", "Grun\u00b7dStein", "nicht", ";"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "$.", "PPER", "VVFIN", "PPER", "KON", "PPOSAT", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "\u201eich hab auf eigne Macht gegr\u00fcndte Zuversicht.", "tokens": ["\u201e", "ich", "hab", "auf", "eig\u00b7ne", "Macht", "ge\u00b7gr\u00fcnd\u00b7te", "Zu\u00b7ver\u00b7sicht", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "APPR", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "\u201eich will das Meisterst\u00fcck auf einen Felsen sezen:", "tokens": ["\u201e", "ich", "will", "das", "Meis\u00b7ter\u00b7st\u00fcck", "auf", "ei\u00b7nen", "Fel\u00b7sen", "se\u00b7zen", ":"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VMFIN", "ART", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "\u201eda la\u00df du nur den Neid daran die Z\u00e4hne wezen.", "tokens": ["\u201e", "da", "la\u00df", "du", "nur", "den", "Neid", "da\u00b7ran", "die", "Z\u00e4h\u00b7ne", "we\u00b7zen", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "PAV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}