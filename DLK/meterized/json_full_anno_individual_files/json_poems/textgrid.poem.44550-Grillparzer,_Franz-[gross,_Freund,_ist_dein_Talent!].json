{"textgrid.poem.44550": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[gross, Freund, ist dein Talent!]", "genre": "verse", "period": "N.A.", "pub_year": 1847, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gross, Freund, ist dein Talent! Ich sehe dich erblassen", "tokens": ["Gross", ",", "Freund", ",", "ist", "dein", "Ta\u00b7lent", "!", "Ich", "se\u00b7he", "dich", "er\u00b7blas\u00b7sen"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "NN", "$,", "VAFIN", "PPOSAT", "NN", "$.", "PPER", "VVFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und wie der Zorn dir drauf im Antlitz brennt.", "tokens": ["Und", "wie", "der", "Zorn", "dir", "drauf", "im", "Ant\u00b7litz", "brennt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "PPER", "PAV", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "\u00bbwei\u00dft du mein Lob nicht h\u00f6her aufzufassen,", "tokens": ["\u00bb", "wei\u00dft", "du", "mein", "Lob", "nicht", "h\u00f6\u00b7her", "auf\u00b7zu\u00b7fas\u00b7sen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "PPOSAT", "NN", "PTKNEG", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Als mich benennend, wie man viele nennt?\u00ab", "tokens": ["Als", "mich", "be\u00b7nen\u00b7nend", ",", "wie", "man", "vie\u00b7le", "nennt", "?", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "VVPP", "$,", "PWAV", "PIS", "PIS", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "H\u00f6r doch mein Wort nur ruhig und gelassen,", "tokens": ["H\u00f6r", "doch", "mein", "Wort", "nur", "ru\u00b7hig", "und", "ge\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PPOSAT", "NN", "ADV", "ADJD", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gro\u00df ists f\u00fcr jeden, der die Zeit erkennt;", "tokens": ["Gro\u00df", "ists", "f\u00fcr", "je\u00b7den", ",", "der", "die", "Zeit", "er\u00b7kennt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "APPR", "PIAT", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Denn das Genie, es l\u00e4uft in allen Gassen,", "tokens": ["Denn", "das", "Ge\u00b7nie", ",", "es", "l\u00e4uft", "in", "al\u00b7len", "Gas\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PPER", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Doch seltener als je ist das Talent.", "tokens": ["Doch", "sel\u00b7te\u00b7ner", "als", "je", "ist", "das", "Ta\u00b7lent", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KOKOM", "ADV", "VAFIN", "ART", "NN", "$."], "meter": "-+---+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Es hat der Geist, will es mich fast gemahnen,", "tokens": ["Es", "hat", "der", "Geist", ",", "will", "es", "mich", "fast", "ge\u00b7mah\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "VMFIN", "PPER", "PRF", "ADV", "VVPP", "$,"],