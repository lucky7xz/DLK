{"dta.poem.23765": {"metadata": {"author": {"name": "Wei\u00dfe, Christian Felix", "birth": "N.A.", "death": "N.A."}, "title": "Klagelied eines Knaben  \n auf ein junges M\u00e4dchen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1767", "urn": "urn:nbn:de:kobv:b4-20643-5", "language": ["de:0.85", "da:0.14"], "booktitle": "Wei\u00dfe, Christian Felix: Lieder f\u00fcr Kinder. Leipzig, 1767."}, "poem": {"stanza.1": {"line.1": {"text": "Die\u00df bange Klagget\u00f6ne", "tokens": ["Die\u00df", "ban\u00b7ge", "Klag\u00b7ge\u00b7t\u00f6\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["PDS", "ADV", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Gilt das Amalien?", "tokens": ["Gilt", "das", "A\u00b7ma\u00b7li\u00b7en", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+--+--", "measure": "dactylic.di.plus"}, "line.3": {"text": "Wie hab ich nicht die Sch\u00f6ne", "tokens": ["Wie", "hab", "ich", "nicht", "die", "Sch\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPER", "PTKNEG", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Vor kurzem noch gesehn?", "tokens": ["Vor", "kur\u00b7zem", "noch", "ge\u00b7sehn", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "O ja, mit ihren Schwestern", "tokens": ["O", "ja", ",", "mit", "ih\u00b7ren", "Schwes\u00b7tern"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "ITJ", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Gieng sie noch ehegestern,", "tokens": ["Gieng", "sie", "noch", "e\u00b7he\u00b7ge\u00b7stern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.7": {"text": "Zum frohen Tanz", "tokens": ["Zum", "fro\u00b7hen", "Tanz"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "In einem Blumenkranz.", "tokens": ["In", "ei\u00b7nem", "Blu\u00b7men\u00b7kranz", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wie die Orangenbl\u00fcte,", "tokens": ["Wie", "die", "O\u00b7ran\u00b7gen\u00b7bl\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So gl\u00e4nzt ihr Angesicht,", "tokens": ["So", "gl\u00e4nzt", "ihr", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und selbst die Rose gl\u00fchte", "tokens": ["Und", "selbst", "die", "Ro\u00b7se", "gl\u00fch\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Darunter sch\u00f6ner nicht:", "tokens": ["Da\u00b7run\u00b7ter", "sch\u00f6\u00b7ner", "nicht", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PAV", "ADJA", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Am Abend von dem Tage,", "tokens": ["Am", "A\u00b7bend", "von", "dem", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "War ihre letzte Klage,", "tokens": ["War", "ih\u00b7re", "letz\u00b7te", "Kla\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Da\u00df ganz und gar", "tokens": ["Da\u00df", "ganz", "und", "gar"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ADV", "KON", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Jhr Kranz entbl\u00e4ttert war.", "tokens": ["Ihr", "Kranz", "ent\u00b7bl\u00e4t\u00b7tert", "war", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wer h\u00e4tt ihr sollen sagen,", "tokens": ["Wer", "h\u00e4tt", "ihr", "sol\u00b7len", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da\u00df wir in n\u00e4chster Nacht", "tokens": ["Da\u00df", "wir", "in", "n\u00e4chs\u00b7ter", "Nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie w\u00fcrden so beklagen,", "tokens": ["Sie", "w\u00fcr\u00b7den", "so", "be\u00b7kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie sie des Kranzes Pracht.", "tokens": ["Wie", "sie", "des", "Kran\u00b7zes", "Pracht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die Blume \u2014 unter allen", "tokens": ["Die", "Blu\u00b7me", "un\u00b7ter", "al\u00b7len"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$(", "APPR", "PIAT"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Die sch\u00f6nste ist gefallen \u2014", "tokens": ["Die", "sch\u00f6ns\u00b7te", "ist", "ge\u00b7fal\u00b7len"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Sie f\u00e4llt herab,", "tokens": ["Sie", "f\u00e4llt", "her\u00b7ab", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "So fr\u00fch verwelkt, ins Grab!", "tokens": ["So", "fr\u00fch", "ver\u00b7welkt", ",", "ins", "Grab", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "$,", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wo seyd ihr s\u00fc\u00dfen Rosen,", "tokens": ["Wo", "seyd", "ihr", "s\u00fc\u00b7\u00dfen", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wo seyd ihr hingeflohn?", "tokens": ["Wo", "seyd", "ihr", "hin\u00b7ge\u00b7flohn", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Statt euch noch lieb zu kosen,", "tokens": ["Statt", "euch", "noch", "lieb", "zu", "ko\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Eilt man behend davon.", "tokens": ["Eilt", "man", "be\u00b7hend", "da\u00b7von", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADJD", "PAV", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Wie schlecht schm\u00fcckt sie die Seide", "tokens": ["Wie", "schlecht", "schm\u00fcckt", "sie", "die", "Sei\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VVFIN", "PPER", "ART", "NN"], "meter": "-++--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Von ihrem Sterbekleide,", "tokens": ["Von", "ih\u00b7rem", "Ster\u00b7be\u00b7klei\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und die\u00df ist doch", "tokens": ["Und", "die\u00df", "ist", "doch"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PDS", "VAFIN", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "An ihr das sch\u00f6nste noch.", "tokens": ["An", "ihr", "das", "sch\u00f6ns\u00b7te", "noch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PDS", "VVFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Bald wird man den Gebeinen", "tokens": ["Bald", "wird", "man", "den", "Ge\u00b7bei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PIS", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den letzten Dienst verleihn,", "tokens": ["Den", "letz\u00b7ten", "Dienst", "ver\u00b7leihn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Um sie nicht weiter weinen", "tokens": ["Um", "sie", "nicht", "wei\u00b7ter", "wei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUI", "PPER", "PTKNEG", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und sie vergessen seyn!", "tokens": ["Und", "sie", "ver\u00b7ges\u00b7sen", "seyn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVPP", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ich will ihr Blumen streuen,", "tokens": ["Ich", "will", "ihr", "Blu\u00b7men", "streu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "So oft sie sich verneuen \u2014", "tokens": ["So", "oft", "sie", "sich", "ver\u00b7neu\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "PRF", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Doch wer sagt mir,", "tokens": ["Doch", "wer", "sagt", "mir", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVFIN", "PPER", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Wie lange bin ich hier!", "tokens": ["Wie", "lan\u00b7ge", "bin", "ich", "hier", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VAFIN", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}