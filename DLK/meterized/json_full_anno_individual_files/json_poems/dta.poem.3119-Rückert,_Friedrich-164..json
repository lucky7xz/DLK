{"dta.poem.3119": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "164.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ob Gott verborgen dir erscheint in der Natur,", "tokens": ["Ob", "Gott", "ver\u00b7bor\u00b7gen", "dir", "er\u00b7scheint", "in", "der", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVPP", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ob au\u00dfer, \u00fcber ihr, ist eins im Grunde nur,", "tokens": ["Ob", "au\u00b7\u00dfer", ",", "\u00fc\u00b7ber", "ihr", ",", "ist", "eins", "im", "Grun\u00b7de", "nur", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "$,", "APPR", "PPER", "$,", "VAFIN", "PIS", "APPRART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ein Wortspiel-Formelkram, vergebens drum zu zanken,", "tokens": ["Ein", "Wort\u00b7spiel\u00b7For\u00b7mel\u00b7kram", ",", "ver\u00b7ge\u00b7bens", "drum", "zu", "zan\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "PAV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Kr\u00fcckennothbehelf gebrechlicher Gedanken.", "tokens": ["Ein", "Kr\u00fc\u00b7cken\u00b7noth\u00b7be\u00b7helf", "ge\u00b7brech\u00b7li\u00b7cher", "Ge\u00b7dan\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Gott ist, was er will seyn, wo er will seyn und wie,", "tokens": ["Gott", "ist", ",", "was", "er", "will", "seyn", ",", "wo", "er", "will", "seyn", "und", "wie", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "$,", "PWS", "PPER", "VMFIN", "VAINF", "$,", "PWAV", "PPER", "VMFIN", "VAINF", "KON", "PWAV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Anders in jedem Ding und jeder Fantasie.", "tokens": ["An\u00b7ders", "in", "je\u00b7dem", "Ding", "und", "je\u00b7der", "Fan\u00b7ta\u00b7sie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PIAT", "NN", "KON", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Anders in jedem Nu, derselb' in Ewigkeit,", "tokens": ["An\u00b7ders", "in", "je\u00b7dem", "Nu", ",", "der\u00b7selb'", "in", "E\u00b7wig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PIS", "ADV", "$,", "PRELS", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Vielheit ewig eins, die Einheit stets entzweit.", "tokens": ["Die", "Viel\u00b7heit", "e\u00b7wig", "eins", ",", "die", "Ein\u00b7heit", "stets", "ent\u00b7zweit", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PIS", "$,", "ART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Ob du Weltsch\u00f6pfer ihn, ob ihn Weltordnung nennest,", "tokens": ["Ob", "du", "Welt\u00b7sch\u00f6p\u00b7fer", "ihn", ",", "ob", "ihn", "Wel\u00b7tord\u00b7nung", "nen\u00b7nest", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "PPER", "$,", "KOUS", "PPER", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+--", "measure": "unknown.measure.hexa"}, "line.2": {"text": "In ihm ist ungetrennt, was im Begriff du trennest.", "tokens": ["In", "ihm", "ist", "un\u00b7ge\u00b7trennt", ",", "was", "im", "Be\u00b7griff", "du", "tren\u00b7nest", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "ADJD", "$,", "PRELS", "APPRART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Geordnet ist die Welt, du ordne dich ihr ein;", "tokens": ["Ge\u00b7ord\u00b7net", "ist", "die", "Welt", ",", "du", "ord\u00b7ne", "dich", "ihr", "ein", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "$,", "PPER", "VVFIN", "PPER", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das wird am G\u00f6ttlichen dein rechter Antheil seyn.", "tokens": ["Das", "wird", "am", "G\u00f6tt\u00b7li\u00b7chen", "dein", "rech\u00b7ter", "An\u00b7theil", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPRART", "NN", "PPOSAT", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}