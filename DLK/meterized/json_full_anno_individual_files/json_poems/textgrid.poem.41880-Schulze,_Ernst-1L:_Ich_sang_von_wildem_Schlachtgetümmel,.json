{"textgrid.poem.41880": {"metadata": {"author": {"name": "Schulze, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich sang von wildem Schlachtget\u00fcmmel,", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich sang von wildem Schlachtget\u00fcmmel,", "tokens": ["Ich", "sang", "von", "wil\u00b7dem", "Schlacht\u00b7ge\u00b7t\u00fcm\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von k\u00fchner Helden Sieg und Grab;", "tokens": ["Von", "k\u00fch\u00b7ner", "Hel\u00b7den", "Sieg", "und", "Grab", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da stahl ein Glanz sich, wie vom Himmel,", "tokens": ["Da", "stahl", "ein", "Glanz", "sich", ",", "wie", "vom", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PRF", "$,", "PWAV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Auf mein erz\u00fcrntes Lied herab.", "tokens": ["Auf", "mein", "er\u00b7z\u00fcrn\u00b7tes", "Lied", "her\u00b7ab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Und als ich jetzt die Augen wende,", "tokens": ["Und", "als", "ich", "jetzt", "die", "Au\u00b7gen", "wen\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Woher das helle Leuchten sey,", "tokens": ["Wo\u00b7her", "das", "hel\u00b7le", "Leuch\u00b7ten", "sey", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da geht, als ob sie Frieden sende,", "tokens": ["Da", "geht", ",", "als", "ob", "sie", "Frie\u00b7den", "sen\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "KOKOM", "KOUS", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Liebste freundlich mir vorbey.", "tokens": ["Die", "Liebs\u00b7te", "freund\u00b7lich", "mir", "vor\u00b7bey", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und sah ich sie auch nur von ferne,", "tokens": ["Und", "sah", "ich", "sie", "auch", "nur", "von", "fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "ADV", "ADV", "APPR", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und hat sie mein auch nicht gedacht,", "tokens": ["Und", "hat", "sie", "mein", "auch", "nicht", "ge\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PPOSAT", "ADV", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch waren holde Liebessterne", "tokens": ["Doch", "wa\u00b7ren", "hol\u00b7de", "Lie\u00b7bess\u00b7ter\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mir