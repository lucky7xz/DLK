{"textgrid.poem.38198": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "8.", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der s\u00fcsse Schlaf, der sonst stillt alles wohl,", "tokens": ["Der", "s\u00fcs\u00b7se", "Schlaf", ",", "der", "sonst", "stillt", "al\u00b7les", "wohl", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ADV", "VVFIN", "PIS", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Kann stillen nicht mein Herz mit Trauren voll,", "tokens": ["Kann", "stil\u00b7len", "nicht", "mein", "Herz", "mit", "Trau\u00b7ren", "voll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PTKNEG", "PPOSAT", "NN", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das schafft allein, der mich erfreuen soll.", "tokens": ["Das", "schafft", "al\u00b7lein", ",", "der", "mich", "er\u00b7freu\u00b7en", "soll", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "$,", "PRELS", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Kein Speis, kein Trank mir Lust noch Nahrung giebt,", "tokens": ["Kein", "Speis", ",", "kein", "Trank", "mir", "Lust", "noch", "Nah\u00b7rung", "giebt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "PPER", "NN", "ADV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Kein Kurzweil mehr mein traurig Herze liebt,", "tokens": ["Kein", "Kurz\u00b7weil", "mehr", "mein", "trau\u00b7rig", "Her\u00b7ze", "liebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "PPOSAT", "ADJD", "VVFIN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das schafft allein, der so mein Herz betr\u00fcbt.", "tokens": ["Das", "schafft", "al\u00b7lein", ",", "der", "so", "mein", "Herz", "be\u00b7tr\u00fcbt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "$,", "PRELS", "ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Gesellschaft ich nicht mehr besuchen mag,", "tokens": ["Ge\u00b7sell\u00b7schaft", "ich", "nicht", "mehr", "be\u00b7su\u00b7chen", "mag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PTKNEG", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ganz einig sitz in Unmuth Nacht und Tag,", "tokens": ["Ganz", "ei\u00b7nig", "sitz", "in", "Un\u00b7muth", "Nacht", "und", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "NN", "APPR", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das schafft allein, den ich im Herzen trag.", "tokens": ["Das", "schafft", "al\u00b7lein", ",", "den", "ich", "im", "Her\u00b7zen", "trag", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "$,", "PRELS", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Der s\u00fcsse Schlaf, der sonst stillt alles wohl,", "tokens": ["Der", "s\u00fcs\u00b7se", "Schlaf", ",", "der", "sonst", "stillt", "al\u00