{"dta.poem.22728": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "31. Die gl\u00fcckseelige M\u00fcsse.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1657", "urn": "urn:nbn:de:kobv:b4-200905198292", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Geistreiche Sinn- vnd Schlussrime. Wien, 1657."}, "poem": {"stanza.1": {"line.1": {"text": "Johannes an der Brust/ ", "tokens": ["Jo\u00b7han\u00b7nes", "an", "der", "Brust", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Thun alle zwey sonst nichts/ als da\u00df sie Gotts geniesse\u0303:", "tokens": ["Thun", "al\u00b7le", "zwey", "sonst", "nichts", "/", "als", "da\u00df", "sie", "Gotts", "ge\u00b7ni\u00b7ess\u1ebd", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "CARD", "ADV", "PIS", "$(", "KOKOM", "KOUS", "PPER", "NE", "NE", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.3": {"text": "Wiewol sind sie daran! k\u00f6nt\u2019 ich so m\u00fcssig sein/", "tokens": ["Wie\u00b7wol", "sind", "sie", "da\u00b7ran", "!", "k\u00f6nt'", "ich", "so", "m\u00fcs\u00b7sig", "sein", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PAV", "$.", "VMFIN", "PPER", "ADV", "ADJD", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ich regete mich nicht/ fiel auch der Himmel ein.", "tokens": ["Ich", "re\u00b7ge\u00b7te", "mich", "nicht", "/", "fiel", "auch", "der", "Him\u00b7mel", "ein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$(", "VVFIN", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}}}}}