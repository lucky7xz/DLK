{"dta.poem.5898": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Elementen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df aus der Lufft die Fluht, und aus der Fluht die Erde,", "tokens": ["Da\u00df", "aus", "der", "Lufft", "die", "Fluht", ",", "und", "aus", "der", "Fluht", "die", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "ART", "NN", "$,", "KON", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Aus Erde wieder Fluht, Lufft aus dem Wasser werde,", "tokens": ["Aus", "Er\u00b7de", "wie\u00b7der", "Fluht", ",", "Lufft", "aus", "dem", "Was\u00b7ser", "wer\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "NN", "$,", "NN", "APPR", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die, wie vorhin, sich aufwerts z\u00f6ge,", "tokens": ["Die", ",", "wie", "vor\u00b7hin", ",", "sich", "auf\u00b7werts", "z\u00f6\u00b7ge", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PWAV", "ADV", "$,", "PRF", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und in das Firmamente aufs neue wieder fl\u00f6ge.", "tokens": ["Und", "in", "das", "Fir\u00b7ma\u00b7men\u00b7te", "aufs", "neu\u00b7e", "wie\u00b7der", "fl\u00f6\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "APPRART", "ADJA", "ADV", "VVFIN", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}}}}}