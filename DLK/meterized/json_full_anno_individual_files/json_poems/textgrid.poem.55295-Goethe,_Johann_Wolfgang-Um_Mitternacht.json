{"textgrid.poem.55295": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Um Mitternacht", "genre": "verse", "period": "N.A.", "pub_year": 1818, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Um Mitternacht ging ich, nicht eben gerne,", "tokens": ["Um", "Mit\u00b7ter\u00b7nacht", "ging", "ich", ",", "nicht", "e\u00b7ben", "ger\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "VVFIN", "PPER", "$,", "PTKNEG", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Klein kleiner Knabe, jenen Kirchhof hin", "tokens": ["Klein", "klei\u00b7ner", "Kna\u00b7be", ",", "je\u00b7nen", "Kirch\u00b7hof", "hin"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "ADJA", "NN", "$,", "PDAT", "NN", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Zu Vaters Haus, des Pfarrers; Stern am Sterne,", "tokens": ["Zu", "Va\u00b7ters", "Haus", ",", "des", "Pfar\u00b7rers", ";", "Stern", "am", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "$,", "ART", "NN", "$.", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sie leuchteten doch alle gar zu sch\u00f6n;", "tokens": ["Sie", "leuch\u00b7te\u00b7ten", "doch", "al\u00b7le", "gar", "zu", "sch\u00f6n", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PIS", "ADV", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Um Mitternacht.", "tokens": ["Um", "Mit\u00b7ter\u00b7nacht", "."], "token_info": ["word", "word", "punct"], "pos": ["KOUI", "NN", "$."], "meter"