{"textgrid.poem.39839": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Auf eine Landkarte der Schweiz", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das ernste Land mit seinen Felsenstegen,", "tokens": ["Das", "erns\u00b7te", "Land", "mit", "sei\u00b7nen", "Fel\u00b7sens\u00b7te\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Abgr\u00fcnden, Bergesriesen, eis'gen Zinnen,", "tokens": ["Ab\u00b7gr\u00fcn\u00b7den", ",", "Ber\u00b7ges\u00b7rie\u00b7sen", ",", "eis'\u00b7gen", "Zin\u00b7nen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es dr\u00e4ngte meinen wanderlust'gen Sinnen", "tokens": ["Es", "dr\u00e4ng\u00b7te", "mei\u00b7nen", "wan\u00b7der\u00b7lust'\u00b7gen", "Sin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Auf diesem Blatte furchtbar sich entgegen.", "tokens": ["Auf", "die\u00b7sem", "Blat\u00b7te", "furcht\u00b7bar", "sich", "ent\u00b7ge\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ADJD", "PRF", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wer sch\u00fctzet mich auf den umdrohten Wegen,", "tokens": ["Wer", "sch\u00fct\u00b7zet", "mich", "auf", "den", "um\u00b7droh\u00b7ten", "We\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PRF", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Verbeut dem Schnee, j\u00e4h vom Gebirg zu rinnen?", "tokens": ["Ver\u00b7beut", "dem", "Schnee", ",", "j\u00e4h", "vom", "Ge\u00b7birg", "zu", "rin\u00b7nen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "ADJD", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und, wird mir hell, kann ich die H\u00f6h'n gewinnen,", "tokens": ["Und", ",", "wird", "mir", "hell", ",", "kann", "ich", "die", "H\u00f6h'n", "ge\u00b7win\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VAFIN", "PPER", "ADJD", "$,", "VMFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das Wunderland zu meinen F\u00fc\u00dfen legen?", "tokens": ["Das", "Wun\u00b7der\u00b7land", "zu", "mei\u00b7nen", "F\u00fc\u00b7\u00dfen", "le\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Da mahnt es mich, da\u00df auch mein s\u00fc\u00dfes Leben", "tokens": ["Da", "mahnt", "es", "mich", ",", "da\u00df", "auch", "mein", "s\u00fc\u00b7\u00dfes", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "$,", "KOUS", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "All diese Berg' und Th\u00e4ler j\u00fcngst durchzogen:", "tokens": ["All", "die\u00b7se", "Ber\u00b7g'", "und", "Th\u00e4\u00b7ler", "j\u00fcngst", "durch\u00b7zo\u00b