{"dta.poem.7137": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Betrachtung des Schlafs,  \n als  \n eine G\u00f6ttliche Wohlthat,  \n bey dem 1728sten Jahres-Wechsel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Man \u00fcberlege doch und dencke,", "tokens": ["Man", "\u00fc\u00b7berl\u00b7e\u00b7ge", "doch", "und", "den\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie sonderbar, wie seltsam, wie verschiedlich,", "tokens": ["Wie", "son\u00b7der\u00b7bar", ",", "wie", "selt\u00b7sam", ",", "wie", "ver\u00b7schied\u00b7lich", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "$,", "PWAV", "ADJD", "$,", "PWAV", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie unerwartet, fremd, wie sehr ver\u00e4nderlich,", "tokens": ["Wie", "un\u00b7er\u00b7war\u00b7tet", ",", "fremd", ",", "wie", "sehr", "ver\u00b7\u00e4n\u00b7der\u00b7lich", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "$,", "ADJD", "$,", "PWAV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wie n\u00e4rrisch bald, bald klug, wie eckelhafft, wie niedlich,", "tokens": ["Wie", "n\u00e4r\u00b7risch", "bald", ",", "bald", "klug", ",", "wie", "ec\u00b7kel\u00b7hafft", ",", "wie", "nied\u00b7lich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ADV", "$,", "ADV", "ADJD", "$,", "PWAV", "ADJD", "$,", "PWAV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Vergn\u00fcglich, th\u00f6richt, wild, gewaltig, gro\u00df und klein,", "tokens": ["Ver\u00b7gn\u00fcg\u00b7lich", ",", "th\u00f6\u00b7richt", ",", "wild", ",", "ge\u00b7wal\u00b7tig", ",", "gro\u00df", "und", "klein", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "VVFIN", "$,", "ADJD", "$,", "ADJD", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wie schreck-und l\u00e4cherlich, wie ungeformt und zierlich,", "tokens": ["Wie", "schreck\u00b7\u00b7und", "l\u00e4\u00b7cher\u00b7lich", ",", "wie", "un\u00b7ge\u00b7formt", "und", "zier\u00b7lich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ADJD", "$,", "PWAV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wie albern, wie gescheut, wie ernsthafft, wie po\u00dfirlich,", "tokens": ["Wie", "al\u00b7bern", ",", "wie", "ge\u00b7scheut", ",", "wie", "ernst\u00b7hafft", ",", "wie", "po\u00df\u00b7ir\u00b7lich", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "$,", "PWAV", "VVPP", "$,", "PWAV", "VVPP", "$,", "PWAV", "ADJD", "$,"], "meter": "-+-+-+-+--+--", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Die nicht zu bildende behende Tr\u00e4ume seyn.", "tokens": ["Die", "nicht", "zu", "bil\u00b7den\u00b7de", "be\u00b7hen\u00b7de", "Tr\u00e4u\u00b7me", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "PTKZU", "ADJA", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}}}}}