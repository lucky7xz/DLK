{"textgrid.poem.59472": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der zaubrische Spielmann", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "N\u00e4chtlich in dem stillen Grunde,", "tokens": ["N\u00e4cht\u00b7lich", "in", "dem", "stil\u00b7len", "Grun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn das Abendrot versank,", "tokens": ["Wenn", "das", "A\u00b7ben\u00b7drot", "ver\u00b7sank", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Um das Waldschlo\u00df in die Runde", "tokens": ["Um", "das", "Wald\u00b7schlo\u00df", "in", "die", "Run\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ging ein lieblicher Gesang.", "tokens": ["Ging", "ein", "lieb\u00b7li\u00b7cher", "Ge\u00b7sang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Fremde waren diese Weisen", "tokens": ["Frem\u00b7de", "wa\u00b7ren", "die\u00b7se", "Wei\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und der S\u00e4nger unbekannt,", "tokens": ["Und", "der", "S\u00e4n\u00b7ger", "un\u00b7be\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber, wie in Zauberkreisen,", "tokens": ["A\u00b7ber", ",", "wie", "in", "Zau\u00b7ber\u00b7krei\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWAV", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hielt er jede Brust gebannt.", "tokens": ["Hielt", "er", "je\u00b7de", "Brust", "ge\u00b7bannt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Hinter bl\u00fchnden Mandelb\u00e4umen", "tokens": ["Hin\u00b7ter", "bl\u00fchn\u00b7den", "Man\u00b7del\u00b7b\u00e4u\u00b7men"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf dem Schlo\u00df das Fr\u00e4ulein lauscht \u2013", "tokens": ["Auf", "dem", "Schlo\u00df", "das", "Fr\u00e4u\u00b7lein", "lauscht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Drunten alle Blumen tr\u00e4umen,", "tokens": ["Drun\u00b7ten", "al\u00b7le", "Blu\u00b7men", "tr\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Woll\u00fcstig der Garten rauscht.", "tokens": ["Wol\u00b7l\u00fcs\u00b7tig", "der", "Gar\u00b7ten", "rauscht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "VVFIN", "$."], "meter": "+---+-+", "measure": "dactylic.init"}}, "stanza.4": {"line.1": {"text": "Und die Wellen buhlend klingen,", "tokens": ["Und", "die", "Wel\u00b7len", "buh\u00b7lend", "klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ringend in geheimer Lust", "tokens": ["Rin\u00b7gend", "in", "ge\u00b7hei\u00b7mer", "Lust"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Kommt das wunderbare Singen", "tokens": ["Kommt", "das", "wun\u00b7der\u00b7ba\u00b7re", "Sin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "An die s\u00fc\u00df vertr\u00e4umte Brust.", "tokens": ["An", "die", "s\u00fc\u00df", "ver\u00b7tr\u00e4um\u00b7te", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "\u00bbwarum weckst du das Verlangen,", "tokens": ["\u00bb", "wa\u00b7rum", "weckst", "du", "das", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Das ich kaum zur Ruh gebracht?", "tokens": ["Das", "ich", "kaum", "zur", "Ruh", "ge\u00b7bracht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADV", "APPRART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Siehst du hoch die Lilien prangen?", "tokens": ["Siehst", "du", "hoch", "die", "Li\u00b7li\u00b7en", "pran\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "B\u00f6ser S\u00e4nger, gute Nacht!", "tokens": ["B\u00f6\u00b7ser", "S\u00e4n\u00b7ger", ",", "gu\u00b7te", "Nacht", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Sieh, die Blumen stehn voll Tr\u00e4nen,", "tokens": ["Sieh", ",", "die", "Blu\u00b7men", "stehn", "voll", "Tr\u00e4\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "VVFIN", "ADJD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Einsam die Viole wacht,", "tokens": ["Ein\u00b7sam", "die", "Vi\u00b7o\u00b7le", "wacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Als wollt sie sich schmachtend dehnen", "tokens": ["Als", "wollt", "sie", "sich", "schmach\u00b7tend", "deh\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "VMFIN", "PPER", "PRF", "ADJD", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "In die warme Sommernacht.", "tokens": ["In", "die", "war\u00b7me", "Som\u00b7mer\u00b7nacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Wohl von s\u00fc\u00dfem, rotem Munde", "tokens": ["Wohl", "von", "s\u00fc\u00b7\u00dfem", ",", "ro\u00b7tem", "Mun\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kommt so holden Sanges Macht \u2013", "tokens": ["Kommt", "so", "hol\u00b7den", "San\u00b7ges", "Macht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJA", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bleibst du ewig dort im Grunde,", "tokens": ["Bleibst", "du", "e\u00b7wig", "dort", "im", "Grun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Unerkannt in stiller Nacht?", "tokens": ["Un\u00b7er\u00b7kannt", "in", "stil\u00b7ler", "Nacht", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Ach, im Wind verfliegt mein Gr\u00fc\u00dfen!", "tokens": ["Ach", ",", "im", "Wind", "ver\u00b7fliegt", "mein", "Gr\u00fc\u00b7\u00dfen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "APPRART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Einmal, eh der Tag erwacht,", "tokens": ["Ein\u00b7mal", ",", "eh", "der", "Tag", "er\u00b7wacht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "M\u00f6cht ich deinen Mund nur k\u00fcssen,", "tokens": ["M\u00f6cht", "ich", "dei\u00b7nen", "Mund", "nur", "k\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sterbend so in s\u00fc\u00dfer Nacht!", "tokens": ["Ster\u00b7bend", "so", "in", "s\u00fc\u00b7\u00dfer", "Nacht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Nachtigall, verliebte, klage", "tokens": ["Nach\u00b7ti\u00b7gall", ",", "ver\u00b7lieb\u00b7te", ",", "kla\u00b7ge"], "token_info": ["word", "punct", "word", "punct", "word"], "pos": ["NN", "$,", "VVFIN", "$,", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nicht so schmeichelnd durch die Nacht! \u2013", "tokens": ["Nicht", "so", "schmei\u00b7chelnd", "durch", "die", "Nacht", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PTKNEG", "ADV", "ADJD", "APPR", "ART", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach! ich wei\u00df nicht, was ich sage,", "tokens": ["Ach", "!", "ich", "wei\u00df", "nicht", ",", "was", "ich", "sa\u00b7ge", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PPER", "VVFIN", "PTKNEG", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Krank bin ich und \u00fcberwacht.\u00ab", "tokens": ["Krank", "bin", "ich", "und", "\u00fc\u00b7ber\u00b7wacht", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "VAFIN", "PPER", "KON", "VVPP", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Also sprach sie, und die Lieder", "tokens": ["Al\u00b7so", "sprach", "sie", ",", "und", "die", "Lie\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KON", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lockten st\u00e4rker aus dem Tal,", "tokens": ["Lock\u00b7ten", "st\u00e4r\u00b7ker", "aus", "dem", "Tal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Rings durchs ganze Tal hallt's wider", "tokens": ["Rings", "durchs", "gan\u00b7ze", "Tal", "hallt's", "wi\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPRART", "ADJA", "NN", "VVFIN", "PTKVZ"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Von der Liebe Lust und Qual.", "tokens": ["Von", "der", "Lie\u00b7be", "Lust", "und", "Qual", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Und sie konnt nicht widerstehen,", "tokens": ["Und", "sie", "konnt", "nicht", "wi\u00b7der\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Enge ward ihr das Gemach,", "tokens": ["En\u00b7ge", "ward", "ihr", "das", "Ge\u00b7mach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aus dem Schlosse mu\u00dft sie gehen", "tokens": ["Aus", "dem", "Schlos\u00b7se", "mu\u00dft", "sie", "ge\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VMFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Diesem Zauberstrome nach.", "tokens": ["Die\u00b7sem", "Zau\u00b7ber\u00b7stro\u00b7me", "nach", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Einsam steigt sie von den Stufen", "tokens": ["Ein\u00b7sam", "steigt", "sie", "von", "den", "Stu\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ach! so schw\u00fcle weht der Wind:", "tokens": ["Ach", "!", "so", "schw\u00fc\u00b7le", "weht", "der", "Wind", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ADV", "ADJA", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Drau\u00dfen s\u00fc\u00df die Stimmen rufen", "tokens": ["Drau\u00b7\u00dfen", "s\u00fc\u00df", "die", "Stim\u00b7men", "ru\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Immerfort das sch\u00f6ne Kind.", "tokens": ["Im\u00b7mer\u00b7fort", "das", "sch\u00f6\u00b7ne", "Kind", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Alle Blumen trunken lauschen,", "tokens": ["Al\u00b7le", "Blu\u00b7men", "trun\u00b7ken", "lau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Von den Kl\u00e4ngen hold durchirrt,", "tokens": ["Von", "den", "Kl\u00e4n\u00b7gen", "hold", "durch\u00b7irrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Lieblicher die Brunnen rauschen,", "tokens": ["Lieb\u00b7li\u00b7cher", "die", "Brun\u00b7nen", "rau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und sie eilet s\u00fc\u00df verwirrt. \u2013", "tokens": ["Und", "sie", "ei\u00b7let", "s\u00fc\u00df", "ver\u00b7wirrt", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADJD", "ADJD", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Wohl am Himmel auf und nieder", "tokens": ["Wohl", "am", "Him\u00b7mel", "auf", "und", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "PTKVZ", "KON", "PTKVZ"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Trieb der Hirt die goldne Schar,", "tokens": ["Trieb", "der", "Hirt", "die", "gold\u00b7ne", "Schar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die Verliebte kehrt nicht wieder,", "tokens": ["Die", "Ver\u00b7lieb\u00b7te", "kehrt", "nicht", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Leer nun Schlo\u00df und Garten war.", "tokens": ["Leer", "nun", "Schlo\u00df", "und", "Gar\u00b7ten", "war", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "NN", "KON", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "Und der S\u00e4nger seit der Stunde", "tokens": ["Und", "der", "S\u00e4n\u00b7ger", "seit", "der", "Stun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nicht mehr weitersingen will,", "tokens": ["Nicht", "mehr", "wei\u00b7ter\u00b7sin\u00b7gen", "will", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Rings im heimlich k\u00fchlen Grunde", "tokens": ["Rings", "im", "heim\u00b7lich", "k\u00fch\u00b7len", "Grun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPRART", "ADJD", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "War's vor Liebe selig still.", "tokens": ["Wa\u00b7r's", "vor", "Lie\u00b7be", "se\u00b7lig", "still", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.16": {"line.1": {"text": "N\u00e4chtlich in dem stillen Grunde,", "tokens": ["N\u00e4cht\u00b7lich", "in", "dem", "stil\u00b7len", "Grun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn das Abendrot versank,", "tokens": ["Wenn", "das", "A\u00b7ben\u00b7drot", "ver\u00b7sank", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Um das Waldschlo\u00df in die Runde", "tokens": ["Um", "das", "Wald\u00b7schlo\u00df", "in", "die", "Run\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ging ein lieblicher Gesang.", "tokens": ["Ging", "ein", "lieb\u00b7li\u00b7cher", "Ge\u00b7sang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.17": {"line.1": {"text": "Fremde waren diese Weisen", "tokens": ["Frem\u00b7de", "wa\u00b7ren", "die\u00b7se", "Wei\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und der S\u00e4nger unbekannt,", "tokens": ["Und", "der", "S\u00e4n\u00b7ger", "un\u00b7be\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber, wie in Zauberkreisen,", "tokens": ["A\u00b7ber", ",", "wie", "in", "Zau\u00b7ber\u00b7krei\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWAV", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hielt er jede Brust gebannt.", "tokens": ["Hielt", "er", "je\u00b7de", "Brust", "ge\u00b7bannt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.18": {"line.1": {"text": "Hinter bl\u00fchnden Mandelb\u00e4umen", "tokens": ["Hin\u00b7ter", "bl\u00fchn\u00b7den", "Man\u00b7del\u00b7b\u00e4u\u00b7men"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf dem Schlo\u00df das Fr\u00e4ulein lauscht \u2013", "tokens": ["Auf", "dem", "Schlo\u00df", "das", "Fr\u00e4u\u00b7lein", "lauscht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Drunten alle Blumen tr\u00e4umen,", "tokens": ["Drun\u00b7ten", "al\u00b7le", "Blu\u00b7men", "tr\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Woll\u00fcstig der Garten rauscht.", "tokens": ["Wol\u00b7l\u00fcs\u00b7tig", "der", "Gar\u00b7ten", "rauscht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "VVFIN", "$."], "meter": "+---+-+", "measure": "dactylic.init"}}, "stanza.19": {"line.1": {"text": "Und die Wellen buhlend klingen,", "tokens": ["Und", "die", "Wel\u00b7len", "buh\u00b7lend", "klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ringend in geheimer Lust", "tokens": ["Rin\u00b7gend", "in", "ge\u00b7hei\u00b7mer", "Lust"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Kommt das wunderbare Singen", "tokens": ["Kommt", "das", "wun\u00b7der\u00b7ba\u00b7re", "Sin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "An die s\u00fc\u00df vertr\u00e4umte Brust.", "tokens": ["An", "die", "s\u00fc\u00df", "ver\u00b7tr\u00e4um\u00b7te", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.20": {"line.1": {"text": "\u00bbwarum weckst du das Verlangen,", "tokens": ["\u00bb", "wa\u00b7rum", "weckst", "du", "das", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Das ich kaum zur Ruh gebracht?", "tokens": ["Das", "ich", "kaum", "zur", "Ruh", "ge\u00b7bracht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADV", "APPRART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Siehst du hoch die Lilien prangen?", "tokens": ["Siehst", "du", "hoch", "die", "Li\u00b7li\u00b7en", "pran\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "B\u00f6ser S\u00e4nger, gute Nacht!", "tokens": ["B\u00f6\u00b7ser", "S\u00e4n\u00b7ger", ",", "gu\u00b7te", "Nacht", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.21": {"line.1": {"text": "Sieh, die Blumen stehn voll Tr\u00e4nen,", "tokens": ["Sieh", ",", "die", "Blu\u00b7men", "stehn", "voll", "Tr\u00e4\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "VVFIN", "ADJD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Einsam die Viole wacht,", "tokens": ["Ein\u00b7sam", "die", "Vi\u00b7o\u00b7le", "wacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Als wollt sie sich schmachtend dehnen", "tokens": ["Als", "wollt", "sie", "sich", "schmach\u00b7tend", "deh\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "VMFIN", "PPER", "PRF", "ADJD", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "In die warme Sommernacht.", "tokens": ["In", "die", "war\u00b7me", "Som\u00b7mer\u00b7nacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.22": {"line.1": {"text": "Wohl von s\u00fc\u00dfem, rotem Munde", "tokens": ["Wohl", "von", "s\u00fc\u00b7\u00dfem", ",", "ro\u00b7tem", "Mun\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kommt so holden Sanges Macht \u2013", "tokens": ["Kommt", "so", "hol\u00b7den", "San\u00b7ges", "Macht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJA", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bleibst du ewig dort im Grunde,", "tokens": ["Bleibst", "du", "e\u00b7wig", "dort", "im", "Grun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Unerkannt in stiller Nacht?", "tokens": ["Un\u00b7er\u00b7kannt", "in", "stil\u00b7ler", "Nacht", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.23": {"line.1": {"text": "Ach, im Wind verfliegt mein Gr\u00fc\u00dfen!", "tokens": ["Ach", ",", "im", "Wind", "ver\u00b7fliegt", "mein", "Gr\u00fc\u00b7\u00dfen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "APPRART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Einmal, eh der Tag erwacht,", "tokens": ["Ein\u00b7mal", ",", "eh", "der", "Tag", "er\u00b7wacht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "M\u00f6cht ich deinen Mund nur k\u00fcssen,", "tokens": ["M\u00f6cht", "ich", "dei\u00b7nen", "Mund", "nur", "k\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sterbend so in s\u00fc\u00dfer Nacht!", "tokens": ["Ster\u00b7bend", "so", "in", "s\u00fc\u00b7\u00dfer", "Nacht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.24": {"line.1": {"text": "Nachtigall, verliebte, klage", "tokens": ["Nach\u00b7ti\u00b7gall", ",", "ver\u00b7lieb\u00b7te", ",", "kla\u00b7ge"], "token_info": ["word", "punct", "word", "punct", "word"], "pos": ["NN", "$,", "VVFIN", "$,", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nicht so schmeichelnd durch die Nacht! \u2013", "tokens": ["Nicht", "so", "schmei\u00b7chelnd", "durch", "die", "Nacht", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PTKNEG", "ADV", "ADJD", "APPR", "ART", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach! ich wei\u00df nicht, was ich sage,", "tokens": ["Ach", "!", "ich", "wei\u00df", "nicht", ",", "was", "ich", "sa\u00b7ge", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PPER", "VVFIN", "PTKNEG", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Krank bin ich und \u00fcberwacht.\u00ab", "tokens": ["Krank", "bin", "ich", "und", "\u00fc\u00b7ber\u00b7wacht", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "VAFIN", "PPER", "KON", "VVPP", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.25": {"line.1": {"text": "Also sprach sie, und die Lieder", "tokens": ["Al\u00b7so", "sprach", "sie", ",", "und", "die", "Lie\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KON", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lockten st\u00e4rker aus dem Tal,", "tokens": ["Lock\u00b7ten", "st\u00e4r\u00b7ker", "aus", "dem", "Tal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Rings durchs ganze Tal hallt's wider", "tokens": ["Rings", "durchs", "gan\u00b7ze", "Tal", "hallt's", "wi\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPRART", "ADJA", "NN", "VVFIN", "PTKVZ"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Von der Liebe Lust und Qual.", "tokens": ["Von", "der", "Lie\u00b7be", "Lust", "und", "Qual", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.26": {"line.1": {"text": "Und sie konnt nicht widerstehen,", "tokens": ["Und", "sie", "konnt", "nicht", "wi\u00b7der\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Enge ward ihr das Gemach,", "tokens": ["En\u00b7ge", "ward", "ihr", "das", "Ge\u00b7mach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aus dem Schlosse mu\u00dft sie gehen", "tokens": ["Aus", "dem", "Schlos\u00b7se", "mu\u00dft", "sie", "ge\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VMFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Diesem Zauberstrome nach.", "tokens": ["Die\u00b7sem", "Zau\u00b7ber\u00b7stro\u00b7me", "nach", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.27": {"line.1": {"text": "Einsam steigt sie von den Stufen", "tokens": ["Ein\u00b7sam", "steigt", "sie", "von", "den", "Stu\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ach! so schw\u00fcle weht der Wind:", "tokens": ["Ach", "!", "so", "schw\u00fc\u00b7le", "weht", "der", "Wind", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ADV", "ADJA", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Drau\u00dfen s\u00fc\u00df die Stimmen rufen", "tokens": ["Drau\u00b7\u00dfen", "s\u00fc\u00df", "die", "Stim\u00b7men", "ru\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Immerfort das sch\u00f6ne Kind.", "tokens": ["Im\u00b7mer\u00b7fort", "das", "sch\u00f6\u00b7ne", "Kind", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.28": {"line.1": {"text": "Alle Blumen trunken lauschen,", "tokens": ["Al\u00b7le", "Blu\u00b7men", "trun\u00b7ken", "lau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Von den Kl\u00e4ngen hold durchirrt,", "tokens": ["Von", "den", "Kl\u00e4n\u00b7gen", "hold", "durch\u00b7irrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Lieblicher die Brunnen rauschen,", "tokens": ["Lieb\u00b7li\u00b7cher", "die", "Brun\u00b7nen", "rau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und sie eilet s\u00fc\u00df verwirrt. \u2013", "tokens": ["Und", "sie", "ei\u00b7let", "s\u00fc\u00df", "ver\u00b7wirrt", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADJD", "ADJD", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.29": {"line.1": {"text": "Wohl am Himmel auf und nieder", "tokens": ["Wohl", "am", "Him\u00b7mel", "auf", "und", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "PTKVZ", "KON", "PTKVZ"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Trieb der Hirt die goldne Schar,", "tokens": ["Trieb", "der", "Hirt", "die", "gold\u00b7ne", "Schar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die Verliebte kehrt nicht wieder,", "tokens": ["Die", "Ver\u00b7lieb\u00b7te", "kehrt", "nicht", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Leer nun Schlo\u00df und Garten war.", "tokens": ["Leer", "nun", "Schlo\u00df", "und", "Gar\u00b7ten", "war", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "NN", "KON", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.30": {"line.1": {"text": "Und der S\u00e4nger seit der Stunde", "tokens": ["Und", "der", "S\u00e4n\u00b7ger", "seit", "der", "Stun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nicht mehr weitersingen will,", "tokens": ["Nicht", "mehr", "wei\u00b7ter\u00b7sin\u00b7gen", "will", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Rings im heimlich k\u00fchlen Grunde", "tokens": ["Rings", "im", "heim\u00b7lich", "k\u00fch\u00b7len", "Grun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPRART", "ADJD", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "War's vor Liebe selig still.", "tokens": ["Wa\u00b7r's", "vor", "Lie\u00b7be", "se\u00b7lig", "still", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}