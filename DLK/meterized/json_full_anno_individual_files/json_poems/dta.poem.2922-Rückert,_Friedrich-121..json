{"dta.poem.2922": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "121.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Von keinem f\u00fchlst du mehr als einem dich beschwert,", "tokens": ["Von", "kei\u00b7nem", "f\u00fchlst", "du", "mehr", "als", "ei\u00b7nem", "dich", "be\u00b7schwert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VVFIN", "PPER", "PIAT", "KOKOM", "ART", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der an dem Leben nur des Lebens Formen ehrt.", "tokens": ["Der", "an", "dem", "Le\u00b7ben", "nur", "des", "Le\u00b7bens", "For\u00b7men", "ehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ADV", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Mit seiner F\u00f6rmlichkeit tritt er in deine Kreise,", "tokens": ["Mit", "sei\u00b7ner", "F\u00f6rm\u00b7lich\u00b7keit", "tritt", "er", "in", "dei\u00b7ne", "Krei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und nichts drin geht ihm recht, weil nicht auf seine Weise.", "tokens": ["Und", "nichts", "drin", "geht", "ihm", "recht", ",", "weil", "nicht", "auf", "sei\u00b7ne", "Wei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "ADV", "VVFIN", "PPER", "ADJD", "$,", "KOUS", "PTKNEG", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Die mangelhafte Form verdecket ihm den Sinn;", "tokens": ["Die", "man\u00b7gel\u00b7haf\u00b7te", "Form", "ver\u00b7de\u00b7cket", "ihm", "den", "Sinn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und endlich glaubst du selbst, es sei kein Takt darin.", "tokens": ["Und", "end\u00b7lich", "glaubst", "du", "selbst", ",", "es", "sei", "kein", "Takt", "da\u00b7rin", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADV", "$,", "PPER", "VAFIN", "PIAT", "NN", "PAV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Weis' ihn aus deinem Kreis und la\u00df ihn weiter wallen,", "tokens": ["Weis'", "ihn", "aus", "dei\u00b7nem", "Kreis", "und", "la\u00df", "ihn", "wei\u00b7ter", "wal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "KON", "VVIMP", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der nur an deinem Seyn dir st\u00f6rt dein Wohlgefallen.", "tokens": ["Der", "nur", "an", "dei\u00b7nem", "Seyn", "dir", "st\u00f6rt", "dein", "Wohl\u00b7ge\u00b7fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "PPOSAT", "NN", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}