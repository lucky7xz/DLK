{"dta.poem.13316": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Z w\u00f6lfftes  B uch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Hier stund sie auf und sah mit Obacht auf die Mienen,", "tokens": ["Hier", "stund", "sie", "auf", "und", "sah", "mit", "O\u00b7bacht", "auf", "die", "Mie\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "KON", "VVFIN", "APPR", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "400Die in dem Angesicht des Guten Raths erschienen.", "tokens": ["in", "dem", "An\u00b7ge\u00b7sicht", "des", "Gu\u00b7ten", "Raths", "er\u00b7schie\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Ich wei\u00df nicht, was er ihr, und was sie ihm erwies,", "tokens": ["Ich", "wei\u00df", "nicht", ",", "was", "er", "ihr", ",", "und", "was", "sie", "ihm", "er\u00b7wies", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWS", "PPER", "PPER", "$,", "KON", "PWS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Indem sie kurz darauf sich wieder nieder lie\u00df", "tokens": ["In\u00b7dem", "sie", "kurz", "da\u00b7rauf", "sich", "wie\u00b7der", "nie\u00b7der", "lie\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "PAV", "PRF", "ADV", "PTKVZ", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und sagte: \u201eNiemand wird von euch den Vorzug haben!", "tokens": ["Und", "sag\u00b7te", ":", "\u201e", "Nie\u00b7mand", "wird", "von", "euch", "den", "Vor\u00b7zug", "ha\u00b7ben", "!"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "$(", "PIS", "VAFIN", "APPR", "PPER", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "\u201enein: keine soll sich selbst mit einem Rang begaben;", "tokens": ["\u201e", "nein", ":", "kei\u00b7ne", "soll", "sich", "selbst", "mit", "ei\u00b7nem", "Rang", "be\u00b7ga\u00b7ben", ";"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKANT", "$.", "PIAT", "VMFIN", "PRF", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "405\u201dEs sey dem ganzen Krei\u00df ein jedes Werck gemein.", "tokens": ["\"", "Es", "sey", "dem", "gan\u00b7zen", "Krei\u00df", "ein", "je\u00b7des", "Werck", "ge\u00b7mein", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "ART", "ADJA", "NN", "ART", "PIAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "\u201edie\u00df soll hiemit dem Spruch und Schlu\u00df zum Grunde seyn.", "tokens": ["\u201e", "die\u00df", "soll", "hie\u00b7mit", "dem", "Spruch", "und", "Schlu\u00df", "zum", "Grun\u00b7de", "seyn", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PDS", "VMFIN", "PAV", "ART", "NN", "KON", "NN", "APPRART", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "\u201ees sey nun jenes Amt, die Kinder zu erziehen;", "tokens": ["\u201e", "es", "sey", "nun", "je\u00b7nes", "Amt", ",", "die", "Kin\u00b7der", "zu", "er\u00b7zie\u00b7hen", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "ADV", "PDAT", "NN", "$,", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "\u201esich um des Ehgemahls Vorhaben zu bem\u00fchen;", "tokens": ["\u201e", "sich", "um", "des", "Eh\u00b7ge\u00b7mahls", "Vor\u00b7ha\u00b7ben", "zu", "be\u00b7m\u00fc\u00b7hen", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PRF", "APPR", "ART", "NN", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "\u201ees sey der Ehren-Bau; das Herz; und was beliebt;", "tokens": ["\u201e", "es", "sey", "der", "Eh\u00b7ren\u00b7Bau", ";", "das", "Herz", ";", "und", "was", "be\u00b7liebt", ";"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "ART", "NN", "$.", "ART", "NN", "$.", "KON", "PWS", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "410\u201dEin Tempel, oder sonst, was euch mehr Hoheit gibt.", "tokens": ["\"", "Ein", "Tem\u00b7pel", ",", "o\u00b7der", "sonst", ",", "was", "euch", "mehr", "Ho\u00b7heit", "gibt", "."], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "$,", "KON", "ADV", "$,", "PRELS", "PPER", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "\u201eerw\u00e4hlet, was ihr wollt; ist es nur nach den Sinnen", "tokens": ["\u201e", "er\u00b7w\u00e4h\u00b7let", ",", "was", "ihr", "wollt", ";", "ist", "es", "nur", "nach", "den", "Sin\u00b7nen"], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "VVFIN", "$,", "PWS", "PPER", "VMFIN", "$.", "VAFIN", "PPER", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "\u201ewodurch ", "tokens": ["\u201e", "wo\u00b7durch"], "token_info": ["punct", "word"], "pos": ["$(", "PWAV"], "meter": "-+", "measure": "iambic.single"}, "line.15": {"text": "\u201eso sey es euch gemein. Nenn\u2019 jede, was sie will:", "tokens": ["\u201e", "so", "sey", "es", "euch", "ge\u00b7mein", ".", "Nenn'", "je\u00b7de", ",", "was", "sie", "will", ":"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VAFIN", "PPER", "PPER", "ADJD", "$.", "VVFIN", "PIAT", "$,", "PRELS", "PPER", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "\u201edas sey der Tugenden Ma\u00df, Absicht, Sorg und Ziel.", "tokens": ["\u201e", "das", "sey", "der", "Tu\u00b7gen\u00b7den", "Ma\u00df", ",", "Ab\u00b7sicht", ",", "Sorg", "und", "Ziel", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PDS", "VAFIN", "ART", "NN", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+---+-+-+", "measure": "unknown.measure.penta"}, "line.17": {"text": "415\u201dKurz: alle sollen gleich in diesem Zweifel siegen,", "tokens": ["\"", "Kurz", ":", "al\u00b7le", "sol\u00b7len", "gleich", "in", "die\u00b7sem", "Zwei\u00b7fel", "sie\u00b7gen", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "$.", "PIS", "VMFIN", "ADV", "APPR", "PDAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "\u201eund keine vor dem Rang der anderen sich schmiegen.", "tokens": ["\u201e", "und", "kei\u00b7ne", "vor", "dem", "Rang", "der", "an\u00b7de\u00b7ren", "sich", "schmie\u00b7gen", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "PIAT", "APPR", "ART", "NN", "ART", "ADJA", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}