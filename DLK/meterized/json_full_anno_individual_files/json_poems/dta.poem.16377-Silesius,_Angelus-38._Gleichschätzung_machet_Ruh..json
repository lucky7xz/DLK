{"dta.poem.16377": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "38.  Gleichsch\u00e4tzung machet Ruh.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn du die Dinge nimbst ohn allen unterscheid;", "tokens": ["Wenn", "du", "die", "Din\u00b7ge", "nimbst", "ohn", "al\u00b7len", "un\u00b7ter\u00b7scheid", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So bleibstu still und gleich in Lieb und auch in Leyd.", "tokens": ["So", "bleibs\u00b7tu", "still", "und", "gleich", "in", "Lieb", "und", "auch", "in", "Leyd", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "KON", "ADV", "APPR", "NN", "KON", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}