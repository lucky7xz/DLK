{"dta.poem.4146": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Schifferreigen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1821", "urn": "urn:nbn:de:kobv:b4-200905194209", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Des Morgens \u00fcber Meer,", "tokens": ["Des", "Mor\u00b7gens", "\u00fc\u00b7ber", "Meer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Der bringt mir Gr\u00fc\u00df' und Lieder", "tokens": ["Der", "bringt", "mir", "Gr\u00fc\u00df'", "und", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Von meinem Liebchen her.", "tokens": ["Von", "mei\u00b7nem", "Lieb\u00b7chen", "her", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wenn ich ein Vogel w\u00e4re,", "tokens": ["Wenn", "ich", "ein", "Vo\u00b7gel", "w\u00e4\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NE", "VAFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Stellt' ich das Schiffen ein,", "tokens": ["Stellt'", "ich", "das", "Schif\u00b7fen", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und wenn ich w\u00e4r' kein Schiffer,", "tokens": ["Und", "wenn", "ich", "w\u00e4r'", "kein", "Schif\u00b7fer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VAFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein Schwimmer m\u00fc\u00dft' ich sein.", "tokens": ["Ein", "Schwim\u00b7mer", "m\u00fc\u00dft'", "ich", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}