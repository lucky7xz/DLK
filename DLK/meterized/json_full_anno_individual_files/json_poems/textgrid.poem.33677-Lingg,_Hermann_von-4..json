{"textgrid.poem.33677": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dir scheint die Rose frisch und rot zu sein,", "tokens": ["Dir", "scheint", "die", "Ro\u00b7se", "frisch", "und", "rot", "zu", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD", "KON", "ADJD", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mir aber scheint in ihrer Brust der Tod zu sein,", "tokens": ["Mir", "a\u00b7ber", "scheint", "in", "ih\u00b7rer", "Brust", "der", "Tod", "zu", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "APPR", "PPOSAT", "NN", "ART", "NN", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Weil sie der Wurm in ihrem Grund umschleicht;", "tokens": ["Weil", "sie", "der", "Wurm", "in", "ih\u00b7rem", "Grund", "um\u00b7schleicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Was diesem Gift, scheint jenem Brot zu sein,", "tokens": ["Was", "die\u00b7sem", "Gift", ",", "scheint", "je\u00b7nem", "Brot", "zu", "sein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PDAT", "NN", "$,", "VVFIN", "PDAT", "NN", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Der Schmetterling, der um die Flamme streicht,", "tokens": ["Der", "Schmet\u00b7ter\u00b7ling", ",", "der", "um", "die", "Flam\u00b7me", "streicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Glaubt nicht von ihrer Glut bedroht zu sein.", "tokens": ["Glaubt", "nicht", "von", "ih\u00b7rer", "Glut", "be\u00b7droht", "zu", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "APPR", "PPOSAT", "NN", "VVPP", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Wie seltsam oft sich Gut und B\u00f6ses gleicht,", "tokens": ["Wie", "selt\u00b7sam", "oft", "sich", "Gut", "und", "B\u00f6\u00b7ses", "gleicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ADV", "PRF", "ADJD", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Es scheint auch dies ein g\u00f6ttliches Gebot zu sein.", "tokens": ["Es", "scheint", "auch", "dies", "ein", "g\u00f6tt\u00b7li\u00b7ches", "Ge\u00b7bot", "zu", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PDS", "ART", "ADJA", "NN", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Dir scheint die Rose frisch und rot zu sein,", "tokens": ["Dir", "scheint", "die", "Ro\u00b7se", "frisch", "und", "rot", "zu", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD", "KON", "ADJD", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mir aber scheint in ihrer Brust der Tod zu sein,", "tokens": ["Mir"