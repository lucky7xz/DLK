{"textgrid.poem.46202": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Ueber vorgemelten tod", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach weh! so \u00fcberschwer ist numehr mein verdru\u00df", "tokens": ["Ach", "weh", "!", "so", "\u00fc\u00b7bersc\u00b7hwer", "ist", "nu\u00b7mehr", "mein", "ver\u00b7dru\u00df"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "ADV", "$.", "ADV", "ADJD", "VAFIN", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "so gar ohn liecht und trost ist meines herzens nacht,", "tokens": ["so", "gar", "ohn", "liecht", "und", "trost", "ist", "mei\u00b7nes", "her\u00b7zens", "nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "NN", "KON", "ADJD", "VAFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "und mit so stetem lauf vergie\u00df ich meine thr\u00e4nen,", "tokens": ["und", "mit", "so", "ste\u00b7tem", "lauf", "ver\u00b7gie\u00df", "ich", "mei\u00b7ne", "thr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADV", "ADJA", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "da\u00df sinkend tiefer stets in meinem zeherflu\u00df", "tokens": ["da\u00df", "sin\u00b7kend", "tie\u00b7fer", "stets", "in", "mei\u00b7nem", "ze\u00b7her\u00b7flu\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "ADJD", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "durch meines schweren leids und finstern leidens macht", "tokens": ["durch", "mei\u00b7nes", "schwe\u00b7ren", "leids", "und", "fins\u00b7tern", "lei\u00b7dens", "macht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "ADJA", "KON", "VVFIN", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "nichts dan der tod allein kan und mu\u00df alles kr\u00f6nen.", "tokens": ["nichts", "dan", "der", "tod", "al\u00b7lein", "kan", "und", "mu\u00df", "al\u00b7les", "kr\u00f6\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "ART", "NN", "ADV", "VMFIN", "KON", "VMFIN", "PIS", "VVINF", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Demnach der schn\u00f6de tod mich leider kont entkr\u00f6nen,", "tokens": ["Dem\u00b7nach", "der", "schn\u00f6\u00b7de", "tod", "mich", "lei\u00b7der", "kont", "ent\u00b7kr\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "ADJA", "NN", "PPER", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "so lieb ich nichts dan leid und will nichts dan verdru\u00df:", "tokens": ["so", "lieb", "ich", "nichts", "dan", "leid", "und", "will", "nichts", "dan", "ver\u00b7dru\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "PIS", "ADV", "ADJD", "KON", "VMFIN", "PIS", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "ja da\u00df die ganze welt seh meines schmerzens macht,", "tokens": ["ja", "da\u00df", "die", "gan\u00b7ze", "welt", "seh", "mei\u00b7nes", "schmer\u00b7zens", "macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ART", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "so ha\u00df ich alle ruh und lauf um tag und nacht,", "tokens": ["so", "ha\u00df", "ich", "al\u00b7le", "ruh", "und", "lauf", "um", "tag", "und", "nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "NN", "KON", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "dan in den dicken wald und dan zu einem flu\u00df,", "tokens": ["dan", "in", "den", "di\u00b7cken", "wald", "und", "dan", "zu", "ei\u00b7nem", "flu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "KON", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.12": {"text": "welchen ich bald vermehr mit regenreichen thr\u00e4nen.", "tokens": ["wel\u00b7chen", "ich", "bald", "ver\u00b7mehr", "mit", "re\u00b7gen\u00b7rei\u00b7chen", "thr\u00e4\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "PPER", "ADV", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.13": {"text": "Befind ich mich dan schier erdrunken in den thr\u00e4nen,", "tokens": ["Be\u00b7find", "ich", "mich", "dan", "schier", "er\u00b7drun\u00b7ken", "in", "den", "thr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PRF", "ADV", "ADJD", "VVINF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "und da\u00df ein kurzer schlaf will meine augen kr\u00f6nen,", "tokens": ["und", "da\u00df", "ein", "kur\u00b7zer", "schlaf", "will", "mei\u00b7ne", "au\u00b7gen", "kr\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN", "VMFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "dazu dan ihr get\u00f6s verleihen wald und flu\u00df,", "tokens": ["da\u00b7zu", "dan", "ihr", "ge\u00b7t\u00f6s", "ver\u00b7lei\u00b7hen", "wald", "und", "flu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "PPOSAT", "NN", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+---+-+-+-+", "measure": "dactylic.init"}, "line.16": {"text": "so stillet er doch nicht mein elend und verdru\u00df,", "tokens": ["so", "stil\u00b7let", "er", "doch", "nicht", "mein", "e\u00b7lend", "und", "ver\u00b7dru\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PTKNEG", "PPOSAT", "ADJD", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "sondern durch den betrug der thr\u00e4nen und der nacht", "tokens": ["son\u00b7dern", "durch", "den", "be\u00b7trug", "der", "thr\u00e4\u00b7nen", "und", "der", "nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "VVFIN", "ART", "NN", "KON", "ART", "NN"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.18": {"text": "verbittert er noch mehr stracks meines schmerzens macht.", "tokens": ["ver\u00b7bit\u00b7tert", "er", "noch", "mehr", "stracks", "mei\u00b7nes", "schmer\u00b7zens", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Daher empfindlicher wird meiner tr\u00fcbsal macht,", "tokens": ["Da\u00b7her", "emp\u00b7find\u00b7li\u00b7cher", "wird", "mei\u00b7ner", "tr\u00fcb\u00b7sal", "macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADJD", "VAFIN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "die zwar bef\u00fcrdern wolt durch den sturm meiner thr\u00e4nen", "tokens": ["die", "zwar", "be\u00b7f\u00fcr\u00b7dern", "wolt", "durch", "den", "sturm", "mei\u00b7ner", "thr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "VVINF", "VMFIN", "APPR", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "mein unentfliehlichen schifbruch in finstrer nacht", "tokens": ["mein", "un\u00b7ent\u00b7flieh\u00b7li\u00b7chen", "schif\u00b7bruch", "in", "finst\u00b7rer", "nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.22": {"text": "doch kan mich mein ungl\u00fcck noch mit dem tod nicht kr\u00f6nen,", "tokens": ["doch", "kan", "mich", "mein", "un\u00b7gl\u00fcck", "noch", "mit", "dem", "tod", "nicht", "kr\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPOSAT", "NN", "ADV", "APPR", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "weil ein und andrer freind geflissen, ohn verdru\u00df", "tokens": ["weil", "ein", "und", "an\u00b7drer", "freind", "ge\u00b7flis\u00b7sen", ",", "ohn", "ver\u00b7dru\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "ART", "KON", "ADJA", "NN", "VVPP", "$,", "KOUI", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "mit aller kunst und gunst wolt dr\u00fccknen des leids flu\u00df.", "tokens": ["mit", "al\u00b7ler", "kunst", "und", "gunst", "wolt", "dr\u00fcck\u00b7nen", "des", "leids", "flu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "KON", "ADV", "VMFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Wan ich alsdan bedenk der freuden \u00fcberflu\u00df,", "tokens": ["Wan", "ich", "als\u00b7dan", "be\u00b7denk", "der", "freu\u00b7den", "\u00fc\u00b7berf\u00b7lu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "wan ich zu herzen f\u00fchr der sch\u00f6nsten sch\u00f6nheit macht,", "tokens": ["wan", "ich", "zu", "her\u00b7zen", "f\u00fchr", "der", "sch\u00f6ns\u00b7ten", "sch\u00f6n\u00b7heit", "macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PTKZU", "VVINF", "VVFIN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "den lieblichen lusts zwang, s\u00fc\u00dfsauren liebverdru\u00df", "tokens": ["den", "lieb\u00b7li\u00b7chen", "lusts", "zwang", ",", "s\u00fc\u00df\u00b7sau\u00b7ren", "lieb\u00b7ver\u00b7dru\u00df"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "und den saurs\u00fc\u00dfen glimpf der l\u00e4chelnd sch\u00f6nen thr\u00e4nen,", "tokens": ["und", "den", "saur\u00b7s\u00fc\u00b7\u00dfen", "glimpf", "der", "l\u00e4\u00b7chelnd", "sch\u00f6\u00b7nen", "thr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "wan pr\u00e4chtig Amor mich mit Myrten pflag zu kr\u00f6nen,", "tokens": ["wan", "pr\u00e4ch\u00b7tig", "A\u00b7mor", "mich", "mit", "Myr\u00b7ten", "pflag", "zu", "kr\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "NE", "PRF", "APPR", "NN", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.30": {"text": "da\u00df ich, ach da\u00df ich war gl\u00fcckselig tag und nacht:", "tokens": ["da\u00df", "ich", ",", "ach", "da\u00df", "ich", "war", "gl\u00fcck\u00b7se\u00b7lig", "tag", "und", "nacht", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "XY", "KOUS", "PPER", "VAFIN", "ADJD", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Ach, da\u00df ich (sprich ich dan) in ewiger traurnacht", "tokens": ["Ach", ",", "da\u00df", "ich", "(", "sprich", "ich", "dan", ")", "in", "e\u00b7wi\u00b7ger", "traur\u00b7nacht"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "$,", "KOUS", "PPER", "$(", "VVIMP", "PPER", "ADV", "$(", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "noch mehr auch drinken m\u00f6cht aus der vergessung flu\u00df", "tokens": ["noch", "mehr", "auch", "drin\u00b7ken", "m\u00f6cht", "aus", "der", "ver\u00b7ges\u00b7sung", "flu\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADV", "VVINF", "VMFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "und meiner lieb verlust mit mehrerm verlust kr\u00f6nen!", "tokens": ["und", "mei\u00b7ner", "lieb", "ver\u00b7lust", "mit", "meh\u00b7rerm", "ver\u00b7lust", "kr\u00f6\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJD", "VVFIN", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "ach, w\u00e4r doch numehr gleich des leids kraft der lieb macht!", "tokens": ["ach", ",", "w\u00e4r", "doch", "nu\u00b7mehr", "gleich", "des", "leids", "kraft", "der", "lieb", "macht", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["XY", "$,", "VAFIN", "ADV", "ADV", "ADV", "ART", "ADJA", "NN", "ART", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+--++", "measure": "iambic.hexa.relaxed"}, "line.35": {"text": "ach, w\u00e4r doch numehr voll das wilde meer der thr\u00e4nen,", "tokens": ["ach", ",", "w\u00e4r", "doch", "nu\u00b7mehr", "voll", "das", "wil\u00b7de", "meer", "der", "thr\u00e4\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["XY", "$,", "VAFIN", "ADV", "ADV", "ADJD", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "die unersch\u00f6pflich nu vergie\u00dfet mein verdru\u00df!", "tokens": ["die", "un\u00b7er\u00b7sch\u00f6pf\u00b7lich", "nu", "ver\u00b7gie\u00b7\u00dfet", "mein", "ver\u00b7dru\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Alsdan solt mein verdru\u00df mich bald nach dieser nacht", "tokens": ["Als\u00b7dan", "solt", "mein", "ver\u00b7dru\u00df", "mich", "bald", "nach", "die\u00b7ser", "nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPOSAT", "NN", "PPER", "ADV", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "aus der lieb \u00fcberflu\u00df widrum mit freudenthr\u00e4nen", "tokens": ["aus", "der", "lieb", "\u00fc\u00b7berf\u00b7lu\u00df", "wid\u00b7rum", "mit", "freu\u00b7dent\u00b7hr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJD", "ADV", "ADV", "APPR", "ADJA"], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.39": {"text": "durch des tods kurze macht mit Myrten ewig kr\u00f6nen.", "tokens": ["durch", "des", "tods", "kur\u00b7ze", "macht", "mit", "Myr\u00b7ten", "e\u00b7wig", "kr\u00f6\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "VVFIN", "APPR", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ach weh! so \u00fcberschwer ist numehr mein verdru\u00df", "tokens": ["Ach", "weh", "!", "so", "\u00fc\u00b7bersc\u00b7hwer", "ist", "nu\u00b7mehr", "mein", "ver\u00b7dru\u00df"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "ADV", "$.", "ADV", "ADJD", "VAFIN", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2":