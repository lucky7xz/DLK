{"dta.poem.20127": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Shakespeare  \n an  \n  einen deutschen F\u00fcrsten .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1851", "urn": "urn:nbn:de:kobv:b4-200905191321", "language": ["de:0.99"], "booktitle": "Fontane, Theodor: Gedichte. Berlin, 1851."}, "poem": {"stanza.1": {"line.1": {"text": "Du liebst die Kunst, und ziehst ihr friedlich Walten,", "tokens": ["Du", "liebst", "die", "Kunst", ",", "und", "ziehst", "ihr", "fried\u00b7lich", "Wal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "KON", "VVFIN", "PPER", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ihr Auferbaun dem L\u00e4rm der Schlachten vor;", "tokens": ["Ihr", "Au\u00b7fer\u00b7baun", "dem", "L\u00e4rm", "der", "Schlach\u00b7ten", "vor", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Sch\u00f6nheit und das Ebenmaa\u00df der Alten,", "tokens": ["Die", "Sch\u00f6n\u00b7heit", "und", "das", "E\u00b7ben\u00b7maa\u00df", "der", "Al\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie meines Geist\u2019s lebendige Gestalten \u2014", "tokens": ["Wie", "mei\u00b7nes", "Geist's", "le\u00b7ben\u00b7di\u00b7ge", "Ge\u00b7stal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Du w\u00fcrdigst sie mit oft erprobtem Ohr.", "tokens": ["Du", "w\u00fcr\u00b7digst", "sie", "mit", "oft", "er\u00b7prob\u00b7tem", "Ohr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "APPR", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Setz\u2019 ein in alte, wohlverdiente Rechte", "tokens": ["Setz'", "ein", "in", "al\u00b7te", ",", "wohl\u00b7ver\u00b7dien\u00b7te", "Rech\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "ART", "APPR", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Die Dichterf\u00fcrsten der Vergangenheit,", "tokens": ["Die", "Dich\u00b7ter\u00b7f\u00fcrs\u00b7ten", "der", "Ver\u00b7gan\u00b7gen\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Doch zwiefach schaff\u2019 dem heutigen Geschlechte", "tokens": ["Doch", "zwie\u00b7fach", "schaff'", "dem", "heu\u00b7ti\u00b7gen", "Ge\u00b7schlech\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Und seinen Dichtern, F\u00fcrst, Gerechtigkeit.", "tokens": ["Und", "sei\u00b7nen", "Dich\u00b7tern", ",", "F\u00fcrst", ",", "Ge\u00b7rech\u00b7tig\u00b7keit", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die Kunst ist frei; sie duldet keine Fessel,", "tokens": ["Die", "Kunst", "ist", "frei", ";", "sie", "dul\u00b7det", "kei\u00b7ne", "Fes\u00b7sel", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$.", "PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "All\u2019 ihr Gesetz ist Sch\u00f6nheit und Natur;", "tokens": ["All'", "ihr", "Ge\u00b7setz", "ist", "Sch\u00f6n\u00b7heit", "und", "Na\u00b7tur", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PPOSAT", "NN", "VAFIN", "NN", "KON", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Das Schwert des Zornes und des Witzes Nessel", "tokens": ["Das", "Schwert", "des", "Zor\u00b7nes", "und", "des", "Wit\u00b7zes", "Nes\u00b7sel"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "KON", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Entrei\u00df ihr nicht, es braut ihr Zauberkessel", "tokens": ["Ent\u00b7rei\u00df", "ihr", "nicht", ",", "es", "braut", "ihr", "Zau\u00b7ber\u00b7kes\u00b7sel"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKNEG", "$,", "PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.5": {"text": "Am Freiheitsfeuer Zaubertr\u00e4nke nur;", "tokens": ["Am", "Frei\u00b7heits\u00b7feu\u00b7er", "Zau\u00b7ber\u00b7tr\u00e4n\u00b7ke", "nur", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ich suchte mir und fand die Misseth\u00e4ter,", "tokens": ["Ich", "such\u00b7te", "mir", "und", "fand", "die", "Mis\u00b7set\u00b7h\u00e4\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Elisabeth, auf Deiner V\u00e4ter Thron;", "tokens": ["E\u00b7lis\u00b7a\u00b7beth", ",", "auf", "Dei\u00b7ner", "V\u00e4\u00b7ter", "Thron", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Wer aber zahlt dem t\u00fcckischen Verr\u00e4ther,", "tokens": ["Wer", "a\u00b7ber", "zahlt", "dem", "t\u00fc\u00b7cki\u00b7schen", "Ver\u00b7r\u00e4\u00b7ther", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-++--+-", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Der Kronen tr\u00e4gt, auch heut noch seinen Lohn?", "tokens": ["Der", "Kro\u00b7nen", "tr\u00e4gt", ",", "auch", "heut", "noch", "sei\u00b7nen", "Lohn", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ADV", "ADV", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wohlan denn F\u00fcrst, sei Du der Kunst Erretter,", "tokens": ["Wo\u00b7hlan", "denn", "F\u00fcrst", ",", "sei", "Du", "der", "Kunst", "Er\u00b7ret\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KON", "NN", "$,", "VAFIN", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nimm ihr das Joch, darin sie schuldlos litt,", "tokens": ["Nimm", "ihr", "das", "Joch", ",", "da\u00b7rin", "sie", "schuld\u00b7los", "litt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ART", "NN", "$,", "PAV", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Frei sei der Dichter und die Welt der Bretter,", "tokens": ["Frei", "sei", "der", "Dich\u00b7ter", "und", "die", "Welt", "der", "Bret\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "KON", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ob immer auch ein throngeborner Vetter", "tokens": ["Ob", "im\u00b7mer", "auch", "ein", "thron\u00b7ge\u00b7bor\u00b7ner", "Vet\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Als Richard Gloster auf die B\u00fchne tritt.", "tokens": ["Als", "Ric\u00b7hard", "Glos\u00b7ter", "auf", "die", "B\u00fch\u00b7ne", "tritt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Du liebst die Kunst; was Licht und Sonnenschimmer", "tokens": ["Du", "liebst", "die", "Kunst", ";", "was", "Licht", "und", "Son\u00b7nen\u00b7schim\u00b7mer"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$.", "PWS", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Der Blume sind, ist ihr die F\u00fcrstengunst,", "tokens": ["Der", "Blu\u00b7me", "sind", ",", "ist", "ihr", "die", "F\u00fcrs\u00b7ten\u00b7gunst", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$,", "VAFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Doch wie die Blume, F\u00fcrst, im Erdreich immer,", "tokens": ["Doch", "wie", "die", "Blu\u00b7me", ",", "F\u00fcrst", ",", "im", "Er\u00b7dreich", "im\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "$,", "NN", "$,", "APPRART", "NN", "ADV", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "So ", "tokens": ["So"], "token_info": ["word"], "pos": ["ADV"], "meter": "-", "measure": "single.down"}}}}}