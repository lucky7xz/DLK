{"dta.poem.1714": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Die Gicht-Accisen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Nachdem man \u00fcberall Accisen fordert ein/", "tokens": ["Nach\u00b7dem", "man", "\u00fc\u00b7be\u00b7rall", "Ac\u00b7ci\u00b7sen", "for\u00b7dert", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "NN", "VVFIN", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So will Frau Podagra auch nicht die lezte seyn.", "tokens": ["So", "will", "Frau", "Po\u00b7da\u00b7gra", "auch", "nicht", "die", "lez\u00b7te", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "NN", "NE", "ADV", "PTKNEG", "ART", "ADJA", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sie fodert den Accis von vormahls freyen Knochen/", "tokens": ["Sie", "fo\u00b7dert", "den", "Ac\u00b7cis", "von", "vor\u00b7mahls", "frey\u00b7en", "Kno\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ADV", "ADJA", "NN", "$("], "meter": "-+---+-+-+-+-", "measure": "dactylic.init"}, "line.4": {"text": "Der wird ihr durch Balbier und Aerzte zugesprochen.", "tokens": ["Der", "wird", "ihr", "durch", "Bal\u00b7bier", "und", "A\u00b7erz\u00b7te", "zu\u00b7ge\u00b7spro\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Einnehmer ist der Schmertz und Contralor Gedult/", "tokens": ["Ein\u00b7neh\u00b7mer", "ist", "der", "Schmertz", "und", "Cont\u00b7ra\u00b7lor", "Ge\u00b7dult", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "KON", "NE", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Doch zahlet einer fr\u00fch/ der andre sp\u00e4t die Schuld.", "tokens": ["Doch", "zah\u00b7let", "ei\u00b7ner", "fr\u00fch", "/", "der", "and\u00b7re", "sp\u00e4t", "die", "Schuld", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJD", "$(", "ART", "ADJA", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}