{"dta.poem.2947": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "146.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn du von Seel' und Leib dich f\u00fchlst im Gleichgewicht,", "tokens": ["Wenn", "du", "von", "Seel'", "und", "Leib", "dich", "f\u00fchlst", "im", "Gleich\u00b7ge\u00b7wicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "KON", "NN", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und um dich siehst die Welt im reinen Sonnenlicht;", "tokens": ["Und", "um", "dich", "siehst", "die", "Welt", "im", "rei\u00b7nen", "Son\u00b7nen\u00b7licht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VVFIN", "ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Dann h\u00f6rst du einen Ruf, der aus dem Innern t\u00f6nt:", "tokens": ["Dann", "h\u00f6rst", "du", "ei\u00b7nen", "Ruf", ",", "der", "aus", "dem", "In\u00b7nern", "t\u00f6nt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Zwiespalt von Natur und Geist ist ausges\u00f6hnt.", "tokens": ["Der", "Zwies\u00b7palt", "von", "Na\u00b7tur", "und", "Geist", "ist", "aus\u00b7ge\u00b7s\u00f6hnt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "KON", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Doch nur ein Augenblick! er ist nicht fest zu halten.", "tokens": ["Doch", "nur", "ein", "Au\u00b7gen\u00b7blick", "!", "er", "ist", "nicht", "fest", "zu", "hal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "$.", "PPER", "VAFIN", "PTKNEG", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "O halt ihn fest, und lern' ihn ewig zu entfalten!", "tokens": ["O", "halt", "ihn", "fest", ",", "und", "lern'", "ihn", "e\u00b7wig", "zu", "ent\u00b7fal\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PTKVZ", "$,", "KON", "VVFIN", "PPER", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Bald hat die Sonnenruh der Sch\u00f6pfung aufgeh\u00f6rt,", "tokens": ["Bald", "hat", "die", "Son\u00b7nen\u00b7ruh", "der", "Sch\u00f6p\u00b7fung", "auf\u00b7ge\u00b7h\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und in dir selber f\u00fchlst du wieder dich verst\u00f6rt.", "tokens": ["Und", "in", "dir", "sel\u00b7ber", "f\u00fchlst", "du", "wie\u00b7der", "dich", "ver\u00b7st\u00f6rt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "ADV", "VVFIN", "PPER", "ADV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Du aber halt es fest: im Himmel und auf Erden,", "tokens": ["Du", "a\u00b7ber", "halt", "es", "fest", ":", "im", "Him\u00b7mel", "und", "auf", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "PTKVZ", "$.", "APPRART", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und in dir selber soll einst ew'ger Sonnschein werden.", "tokens": ["Und", "in", "dir", "sel\u00b7ber", "soll", "einst", "ew'\u00b7ger", "Sonn\u00b7schein", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "ADV", "VMFIN", "ADV", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}