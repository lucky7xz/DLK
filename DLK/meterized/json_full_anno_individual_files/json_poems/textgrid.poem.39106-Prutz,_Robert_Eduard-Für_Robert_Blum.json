{"textgrid.poem.39106": {"metadata": {"author": {"name": "Prutz, Robert Eduard", "birth": "N.A.", "death": "N.A."}, "title": "F\u00fcr Robert Blum", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Lied! Ein Lied! Wie dumpfer Trommeln Schlag", "tokens": ["Ein", "Lied", "!", "Ein", "Lied", "!", "Wie", "dum\u00b7pfer", "Trom\u00b7meln", "Schlag"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$.", "ART", "NN", "$.", "PWAV", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "und hell und st\u00fcrmisch wie Posaunenklang!", "tokens": ["und", "hell", "und", "st\u00fcr\u00b7misch", "wie", "Po\u00b7sau\u00b7nen\u00b7klang", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KON", "ADJD", "KOKOM", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein Lied, wie man nur einmal dichten kann,", "tokens": ["Ein", "Lied", ",", "wie", "man", "nur", "ein\u00b7mal", "dich\u00b7ten", "kann", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "PIS", "ADV", "ADV", "ADJA", "VMFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "bei dem die Jungfrau Tr\u00e4nen weinen mag,", "tokens": ["bei", "dem", "die", "Jung\u00b7frau", "Tr\u00e4\u00b7nen", "wei\u00b7nen", "mag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ART", "NN", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "doch nach dem Schwerte z\u00fcrnend greift der Mann;", "tokens": ["doch", "nach", "dem", "Schwer\u00b7te", "z\u00fcr\u00b7nend", "greift", "der", "Mann", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVPP", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "ein Siegeshymnus und ein Grabgesang,", "tokens": ["ein", "Sie\u00b7ges\u00b7hym\u00b7nus", "und", "ein", "Grab\u00b7ge\u00b7sang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "ein Lied, wie ein Choral im Heiligtum:", "tokens": ["ein", "Lied", ",", "wie", "ein", "Cho\u00b7ral", "im", "Hei\u00b7lig\u00b7tum", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "Ein Feierlied f\u00fcr Robert Blum! \u2013", "tokens": ["Ein", "Fei\u00b7er\u00b7lied", "f\u00fcr", "Ro\u00b7bert", "Blum", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "APPR", "NE", "NE", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "O Robert Blum! Mit deiner breiten Brust,", "tokens": ["O", "Ro\u00b7bert", "Blum", "!", "Mit", "dei\u00b7ner", "brei\u00b7ten", "Brust", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "$.", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "dem trotz'gen Hals, dem kr\u00e4ftigen Genick,", "tokens": ["dem", "trotz'\u00b7gen", "Hals", ",", "dem", "kr\u00e4f\u00b7ti\u00b7gen", "Ge\u00b7nick", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "der starken Hand, voll Schwielen, aber rein \u2013", "tokens": ["der", "star\u00b7ken", "Hand", ",", "voll", "Schwie\u00b7len", ",", "a\u00b7ber", "rein", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADJD", "NN", "$,", "ADV", "ADJD", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mit deinem Mund, so schelmisch und voll Lust \u2013", "tokens": ["Mit", "dei\u00b7nem", "Mund", ",", "so", "schel\u00b7misch", "und", "voll", "Lust", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "ADV", "ADJD", "KON", "ADJD", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Mit deines Auges halb umflortem Schein,", "tokens": ["Mit", "dei\u00b7nes", "Au\u00b7ges", "halb", "um\u00b7flor\u00b7tem", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "und doch wie treu, wie kindlich dieser Blick \u2013", "tokens": ["und", "doch", "wie", "treu", ",", "wie", "kind\u00b7lich", "die\u00b7ser", "Blick", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "KOKOM", "ADJD", "$,", "PWAV", "ADJD", "PDAT", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "O Robert Blum, jetzt erst, jetzt wird uns klar,", "tokens": ["O", "Ro\u00b7bert", "Blum", ",", "jetzt", "erst", ",", "jetzt", "wird", "uns", "klar", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "$,", "ADV", "ADV", "$,", "ADV", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "du selber warst das Volk, f\u00fcrwahr!", "tokens": ["du", "sel\u00b7ber", "warst", "das", "Volk", ",", "f\u00fcr\u00b7wahr", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "ART", "NN", "$,", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Im Gut' und B\u00f6sen, treulich, wie es ist,", "tokens": ["Im", "Gut'", "und", "B\u00f6\u00b7sen", ",", "treu\u00b7lich", ",", "wie", "es", "ist", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "NN", "$,", "ADJD", "$,", "PWAV", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "das deutsche Volk, und wie's gezogen ward:", "tokens": ["das", "deut\u00b7sche", "Volk", ",", "und", "wie's", "ge\u00b7zo\u00b7gen", "ward", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "KON", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Zum Mi\u00dftraun rasch, noch rascher zum Vertraun,", "tokens": ["Zum", "Mi\u00df\u00b7traun", "rasch", ",", "noch", "ra\u00b7scher", "zum", "Ver\u00b7traun", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "$,", "ADV", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "zu fromm nicht blo\u00df, zu ungeschickt zur List,", "tokens": ["zu", "fromm", "nicht", "blo\u00df", ",", "zu", "un\u00b7ge\u00b7schickt", "zur", "List", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "PTKNEG", "ADV", "$,", "PTKA", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "doch stets bereit, Luftschl\u00f6sser sich zu baun;", "tokens": ["doch", "stets", "be\u00b7reit", ",", "Luft\u00b7schl\u00f6s\u00b7ser", "sich", "zu", "baun", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "$,", "NN", "PRF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "weich in Gedanken, nur in Worten hart;", "tokens": ["weich", "in", "Ge\u00b7dan\u00b7ken", ",", "nur", "in", "Wor\u00b7ten", "hart", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "$,", "ADV", "APPR", "NN", "ADJD", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.7": {"text": "wie Gold so treu, und felsenfest im Schmerz \u2013", "tokens": ["wie", "Gold", "so", "treu", ",", "und", "fel\u00b7sen\u00b7fest", "im", "Schmerz", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADV", "ADJD", "$,", "KON", "VVFIN", "APPRART", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Ein unverf\u00e4lschtes deutsches Herz! \u2013", "tokens": ["Ein", "un\u00b7ver\u00b7f\u00e4lschtes", "deut\u00b7sches", "Herz", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$.", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.4": {"line.1": {"text": "Und dieses Herz, vor dreien Tagen nun", "tokens": ["Und", "die\u00b7ses", "Herz", ",", "vor", "drei\u00b7en", "Ta\u00b7gen", "nun"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PDAT", "NN", "$,", "APPR", "CARD", "NN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "hat des Kroaten Kugel es zerw\u00fchlt,", "tokens": ["hat", "des", "Kroa\u00b7ten", "Ku\u00b7gel", "es", "zer\u00b7w\u00fchlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "gedankenlos, als w\u00e4r's ein Tier, ein Baum;", "tokens": ["ge\u00b7dan\u00b7ken\u00b7los", ",", "als", "w\u00e4r's", "ein", "Tier", ",", "ein", "Baum", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$,", "KOKOM", "VAFIN", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "bleich und verwelkt, die starken H\u00e4nde ruhn,", "tokens": ["bleich", "und", "ver\u00b7welkt", ",", "die", "star\u00b7ken", "H\u00e4n\u00b7de", "ruhn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "VVPP", "$,", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "++-+-+-+-+", "measure": "iambic.penta.spondeus"}, "line.5": {"text": "das Auge brach \u2013 o f\u00fcrchterlicher Traum!", "tokens": ["das", "Au\u00b7ge", "brach", "\u2013", "o", "f\u00fcrch\u00b7ter\u00b7li\u00b7cher", "Traum", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$(", "FM", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Still steht der Busen, der so warm gef\u00fchlt \u2013", "tokens": ["Still", "steht", "der", "Bu\u00b7sen", ",", "der", "so", "warm", "ge\u00b7f\u00fchlt", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$,", "PRELS", "ADV", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Nicht Blum allein: das ganze deutsche Land,", "tokens": ["Nicht", "Blum", "al\u00b7lein", ":", "das", "gan\u00b7ze", "deut\u00b7sche", "Land", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NE", "ADV", "$.", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "da liegt es blutend unterm Sand!", "tokens": ["da", "liegt", "es", "blu\u00b7tend", "un\u00b7term", "Sand", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Und doch, und doch \u2013 bez\u00e4hmt den wilden Schmerz:", "tokens": ["Und", "doch", ",", "und", "doch", "\u2013", "be\u00b7z\u00e4hmt", "den", "wil\u00b7den", "Schmerz", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "KON", "ADV", "$(", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Tr\u00e4nen hemmt, die auf der Wange gl\u00fchn,", "tokens": ["Die", "Tr\u00e4\u00b7nen", "hemmt", ",", "die", "auf", "der", "Wan\u00b7ge", "gl\u00fchn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PRELS", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "und haltet noch, o noch das Schwert zur\u00fcck!", "tokens": ["und", "hal\u00b7tet", "noch", ",", "o", "noch", "das", "Schwert", "zu\u00b7r\u00fcck", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "$,", "FM", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Denn diesem Sande wird ein Baum entbl\u00fchn,", "tokens": ["Denn", "die\u00b7sem", "San\u00b7de", "wird", "ein", "Baum", "ent\u00b7bl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "VAFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "darunter wohnt des deutschen Volkes Gl\u00fcck,", "tokens": ["da\u00b7run\u00b7ter", "wohnt", "des", "deut\u00b7schen", "Vol\u00b7kes", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "der Baum der Freiheit, ragend himmelw\u00e4rts:", "tokens": ["der", "Baum", "der", "Frei\u00b7heit", ",", "ra\u00b7gend", "him\u00b7mel\u00b7w\u00e4rts", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "ADJD", "ADV", "$."], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.7": {"text": "Vor diesem Blut, o glaubt es sicherlich,", "tokens": ["Vor", "die\u00b7sem", "Blut", ",", "o", "glaubt", "es", "si\u00b7cher\u00b7lich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "FM", "VVFIN", "PPER", "PRF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "entf\u00e4rbt noch mancher Purpur sich!", "tokens": ["ent\u00b7f\u00e4rbt", "noch", "man\u00b7cher", "Pur\u00b7pur", "sich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PIAT", "NN", "PRF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Du aber schlaf in deiner k\u00fchlen Gruft,", "tokens": ["Du", "a\u00b7ber", "schlaf", "in", "dei\u00b7ner", "k\u00fch\u00b7len", "Gruft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "von keinem Mal bezeichnet, eng und klein,", "tokens": ["von", "kei\u00b7nem", "Mal", "be\u00b7zeich\u00b7net", ",", "eng", "und", "klein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVPP", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "zerstampft vielleicht von des Kroaten Tritt:", "tokens": ["zer\u00b7stampft", "viel\u00b7leicht", "von", "des", "Kroa\u00b7ten", "Tritt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und doch so lang noch wehet deutsche Luft,", "tokens": ["Und", "doch", "so", "lang", "noch", "we\u00b7het", "deut\u00b7sche", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ADJD", "ADV", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "in allen Zweigen weht dein Name mit", "tokens": ["in", "al\u00b7len", "Zwei\u00b7gen", "weht", "dein", "Na\u00b7me", "mit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "PPOSAT", "NN", "APPR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "und wird unsterblich, wie die Sterne, sein!", "tokens": ["und", "wird", "uns\u00b7terb\u00b7lich", ",", "wie", "die", "Ster\u00b7ne", ",", "sein", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VAFIN", "ADJD", "$,", "PWAV", "ART", "NN", "$,", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Schlaf, schlaf, o Freund: Dein Grabesheiligtum", "tokens": ["Schlaf", ",", "schlaf", ",", "o", "Freund", ":", "Dein