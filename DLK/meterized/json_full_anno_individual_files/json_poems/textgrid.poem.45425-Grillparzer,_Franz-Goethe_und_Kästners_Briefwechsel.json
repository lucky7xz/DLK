{"textgrid.poem.45425": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Goethe und K\u00e4stners Briefwechsel", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun endlich seid ihr doch im klaren;", "tokens": ["Nun", "end\u00b7lich", "seid", "ihr", "doch", "im", "kla\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "ADV", "APPRART", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr steht auf dem Boden des wirklich Wahren,", "tokens": ["Ihr", "steht", "auf", "dem", "Bo\u00b7den", "des", "wirk\u00b7lich", "Wah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ART", "ADJD", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Es hat tats\u00e4chlich eine Lotte gegeben,", "tokens": ["Es", "hat", "tat\u00b7s\u00e4ch\u00b7lich", "ei\u00b7ne", "Lot\u00b7te", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Ihr Nachtkamisol ist gemalt nach dem Leben.", "tokens": ["Ihr", "Nacht\u00b7ka\u00b7mi\u00b7sol", "ist", "ge\u00b7malt", "nach", "dem", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.5": {"text": "Wenn wir von kleinen Rotzn\u00e4schen lasen,", "tokens": ["Wenn", "wir", "von", "klei\u00b7nen", "Rotz\u00b7n\u00e4\u00b7schen", "la\u00b7sen", ","], "token_info": 