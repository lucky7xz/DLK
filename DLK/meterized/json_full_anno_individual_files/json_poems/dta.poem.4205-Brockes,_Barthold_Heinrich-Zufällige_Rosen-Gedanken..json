{"dta.poem.4205": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Zuf\u00e4llige Rosen-Gedanken.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie j\u00fcngst von einem Rosen-Busch ich eine frische Rose", "tokens": ["Wie", "j\u00fcngst", "von", "ei\u00b7nem", "Ro\u00b7sen\u00b7Busch", "ich", "ei\u00b7ne", "fri\u00b7sche", "Ro\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "APPR", "ART", "NN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "brach,", "tokens": ["brach", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Die der gefallne Thau noch netzte,", "tokens": ["Die", "der", "ge\u00b7fall\u00b7ne", "Thau", "noch", "netz\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und sie mir den Geruch und Blick, durch ihren Duft und", "tokens": ["Und", "sie", "mir", "den", "Ge\u00b7ruch", "und", "Blick", ",", "durch", "ih\u00b7ren", "Duft", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "PPER", "ART", "NN", "KON", "NN", "$,", "APPR", "PPOSAT", "NN", "KON"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Schmuck, ergetzte;", "tokens": ["Schmuck", ",", "er\u00b7getz\u00b7te", ";"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "So dacht\u2019 ich beyden weiter nach.", "tokens": ["So", "dacht'", "ich", "bey\u00b7den", "wei\u00b7ter", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Es gaben der Geruch und ihrer Farben Schein", "tokens": ["Es", "ga\u00b7ben", "der", "Ge\u00b7ruch", "und", "ih\u00b7rer", "Far\u00b7ben", "Schein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "KON", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Mir folgende Gedanken ein:", "tokens": ["Mir", "fol\u00b7gen\u00b7de", "Ge\u00b7dan\u00b7ken", "ein", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+---+-+", "measure": "dactylic.init"}}, "stanza.2": {"line.1": {"text": "Es scheint fast auf der Welt kein C\u00f6rper zugeschickt,", "tokens": ["Es", "scheint", "fast", "auf", "der", "Welt", "kein", "C\u00f6r\u00b7per", "zu\u00b7ge\u00b7schickt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und welchem solche\u2019 Sch\u00f6nheit eigen,", "tokens": ["Und", "wel\u00b7chem", "sol\u00b7che'", "Sch\u00f6n\u00b7heit", "ei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAT", "PIAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Das sch\u00f6ne Licht so sch\u00f6n zu zeigen,", "tokens": ["Das", "sch\u00f6\u00b7ne", "Licht", "so", "sch\u00f6n", "zu", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Als wie man an der Ros\u2019 erblickt,", "tokens": ["Als", "wie", "man", "an", "der", "Ros'", "er\u00b7blickt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "PIS", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Indem in solcher Lieblichkeit,", "tokens": ["In\u00b7dem", "in", "sol\u00b7cher", "Lieb\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In solcher Vollenkommenheit,", "tokens": ["In", "sol\u00b7cher", "Vol\u00b7len\u00b7kom\u00b7men\u00b7heit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die R\u00f6hte sich darauf so s\u00fc\u00df gemenget findet,", "tokens": ["Die", "R\u00f6h\u00b7te", "sich", "da\u00b7rauf", "so", "s\u00fc\u00df", "ge\u00b7men\u00b7get", "fin\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "PAV", "ADV", "ADJD", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Da\u00df sie, fast selbst die Unaufmerksamkeit,", "tokens": ["Da\u00df", "sie", ",", "fast", "selbst", "die", "Un\u00b7auf\u00b7merk\u00b7sam\u00b7keit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "ADV", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Zum Denken und zur Lust verbindet.", "tokens": ["Zum", "Den\u00b7ken", "und", "zur", "Lust", "ver\u00b7bin\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wie ich sie nun hierauf vor meine Nase hielte,", "tokens": ["Wie", "ich", "sie", "nun", "hier\u00b7auf", "vor", "mei\u00b7ne", "Na\u00b7se", "hiel\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "ADV", "PAV", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und mich ger\u00fchrt, von ihrem Balsam, f\u00fchlte;", "tokens": ["Und", "mich", "ge\u00b7r\u00fchrt", ",", "von", "ih\u00b7rem", "Bal\u00b7sam", ",", "f\u00fchl\u00b7te", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PPER", "VVPP", "$,", "APPR", "PPOSAT", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Erwog ich, wie es doch geschehe,", "tokens": ["Er\u00b7wog", "ich", ",", "wie", "es", "doch", "ge\u00b7sche\u00b7he", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df solche Lust in uns entstehe!", "tokens": ["Da\u00df", "sol\u00b7che", "Lust", "in", "uns", "ent\u00b7ste\u00b7he", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Es kam mir vor, als ob der trockne Saft,", "tokens": ["Es", "kam", "mir", "vor", ",", "als", "ob", "der", "trock\u00b7ne", "Saft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$,", "KOKOM", "KOUS", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und die so s\u00fc\u00df in ihr vereinte Kraft,", "tokens": ["Und", "die", "so", "s\u00fc\u00df", "in", "ihr", "ver\u00b7ein\u00b7te", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADV", "ADJD", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Gerad\u2019 in unser Hirn nicht eile,", "tokens": ["Ge\u00b7rad'", "in", "un\u00b7ser", "Hirn", "nicht", "ei\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Nein, da\u00df dieselbe sich zertheile,", "tokens": ["Nein", ",", "da\u00df", "die\u00b7sel\u00b7be", "sich", "zer\u00b7thei\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "KOUS", "PDAT", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Und mit dem Athem auch sich zu der Lunge f\u00fchre,", "tokens": ["Und", "mit", "dem", "A\u00b7them", "auch", "sich", "zu", "der", "Lun\u00b7ge", "f\u00fch\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADV", "PRF", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Einfolglich uns gedoppelt r\u00fchre.", "tokens": ["Ein\u00b7folg\u00b7lich", "uns", "ge\u00b7dop\u00b7pelt", "r\u00fch\u00b7re", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Es scheint, als ob, auf diese Weise,", "tokens": ["Es", "scheint", ",", "als", "ob", ",", "auf", "die\u00b7se", "Wei\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOKOM", "KOUS", "$,", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Ros\u2019 uns Geist und C\u00f6rper speise,", "tokens": ["Die", "Ros'", "uns", "Geist", "und", "C\u00f6r\u00b7per", "spei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So im Gehirn als in der Brust,", "tokens": ["So", "im", "Ge\u00b7hirn", "als", "in", "der", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "KOKOM", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und die\u00df verdoppelt unsre Lust.", "tokens": ["Und", "die\u00df", "ver\u00b7dop\u00b7pelt", "uns\u00b7re", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ach! da\u00df es, wenn man es bed\u00e4chte,", "tokens": ["Ach", "!", "da\u00df", "es", ",", "wenn", "man", "es", "be\u00b7d\u00e4ch\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "KOUS", "PPER", "$,", "KOUS", "PIS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Auch unsern Dank verdoppeln m\u00f6gte!", "tokens": ["Auch", "un\u00b7sern", "Dank", "ver\u00b7dop\u00b7peln", "m\u00f6g\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Indem ich ferner mich nun am Geruch vergn\u00fcge,", "tokens": ["In\u00b7dem", "ich", "fer\u00b7ner", "mich", "nun", "am", "Ge\u00b7ruch", "ver\u00b7gn\u00fc\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PPER", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+++-+-+-", "measure": "unknown.measure.septa"}, "line.2": {"text": "Und \u00f6fters sie zu meiner Nase f\u00fcge;", "tokens": ["Und", "\u00f6f\u00b7ters", "sie", "zu", "mei\u00b7ner", "Na\u00b7se", "f\u00fc\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So d\u00fcnkte mir, da\u00df ich annoch entdecke,", "tokens": ["So", "d\u00fcnk\u00b7te", "mir", ",", "da\u00df", "ich", "an\u00b7noch", "ent\u00b7de\u00b7cke", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wie etwas Lehr-reichs noch in ihrem Ambra stecke.", "tokens": ["Wie", "et\u00b7was", "Lehr\u00b7reichs", "noch", "in", "ih\u00b7rem", "A\u00b7mbra", "ste\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "\u201cholde Rose, da mein Geist, durch dein D\u00fcften recht", "tokens": ["\u201c", "hol\u00b7de", "Ro\u00b7se", ",", "da", "mein", "Geist", ",", "durch", "dein", "D\u00fcf\u00b7ten", "recht"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "ADJA", "NN", "$,", "KOUS", "PPOSAT", "NN", "$,", "APPR", "PPOSAT", "NN", "ADJD"], "meter": "+-+---+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "ger\u00fchret,", "tokens": ["ge\u00b7r\u00fch\u00b7ret", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "\u201edeiner s\u00fcssen Lieblichkeit fast beseel\u2019nde Kr\u00e4fte sp\u00fchret;", "tokens": ["\u201e", "dei\u00b7ner", "s\u00fcs\u00b7sen", "Lieb\u00b7lich\u00b7keit", "fast", "be\u00b7seel'n\u00b7de", "Kr\u00e4f\u00b7te", "sp\u00fch\u00b7ret", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "ADJA", "NN", "ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "\u201ewerd\u2019 ich innerlich erfrischt", "tokens": ["\u201e", "werd'", "ich", "in\u00b7ner\u00b7lich", "er\u00b7frischt"], "token_info": ["punct", "word", "word", "word", "word"], "pos": ["$(", "VAFIN", "PPER", "ADJD", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "\u201edurch der holden S\u00fc\u00dfigkeit und des Balsams sanfte", "tokens": ["\u201e", "durch", "der", "hol\u00b7den", "S\u00fc\u00b7\u00dfig\u00b7keit", "und", "des", "Bal\u00b7sams", "sanf\u00b7te"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "APPR", "ART", "ADJA", "NN", "KON", "ART", "NN", "VVFIN"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.6": {"text": "St\u00e4rke.", "tokens": ["St\u00e4r\u00b7ke", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.7": {"text": "\u201edoch da ich in dir zugleich eine Bitterkeit bemerke,", "tokens": ["\u201e", "doch", "da", "ich", "in", "dir", "zu\u00b7gleich", "ei\u00b7ne", "Bit\u00b7ter\u00b7keit", "be\u00b7mer\u00b7ke", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "KOUS", "PPER", "APPR", "PPER", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.8": {"text": "\u201edie aus deinem Kelche d\u00fcnstet; \u00fcberleg\u2019 ich die\u00df dabey,", "tokens": ["\u201e", "die", "aus", "dei\u00b7nem", "Kel\u00b7che", "d\u00fcns\u00b7tet", ";", "\u00fc\u00b7ber\u00b7leg'", "ich", "die\u00df", "da\u00b7bey", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PRELS", "APPR", "PPOSAT", "NN", "VVFIN", "$.", "VVFIN", "PPER", "PDS", "PAV", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.9": {"text": "\u201eda\u00df, so wie fast \u00fcberall bitter sich mit s\u00fcssem mischet,", "tokens": ["\u201e", "da\u00df", ",", "so", "wie", "fast", "\u00fc\u00b7be\u00b7rall", "bit\u00b7ter", "sich", "mit", "s\u00fcs\u00b7sem", "mi\u00b7schet", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "$,", "ADV", "KOKOM", "ADV", "ADV", "ADJD", "PRF", "APPR", "ADJA", "VVFIN", "$,"], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.10": {"text": "\u201ees mit dir nicht anders sey.", "tokens": ["\u201e", "es", "mit", "dir", "nicht", "an\u00b7ders", "sey", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "APPR", "PPER", "PTKNEG", "ADV", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "\u201edennoch steckt ein Trost darinn, da, wenn man es wohl", "tokens": ["\u201e", "den\u00b7noch", "steckt", "ein", "Trost", "da\u00b7rinn", ",", "da", ",", "wenn", "man", "es", "wohl"], "token_info": ["punct", "word", "word", "word", "word", "wo