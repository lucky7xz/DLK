{"dta.poem.2083": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Cham\u00e4leon.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Auch ist der Cham\u00e4leon, mancher Ursach' halber,", "tokens": ["Auch", "ist", "der", "Cha\u00b7m\u00e4\u00b7le\u00b7on", ",", "man\u00b7cher", "Ur\u00b7sach'", "hal\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NE", "$,", "PIAT", "NN", "APPO", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "werth,", "tokens": ["werth", ","], "token_info": ["word", "punct"], "pos": ["ADJD", "$,"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Da\u00df in seinem sondern Bau man bey ihm den Sch\u00f6pfer", "tokens": ["Da\u00df", "in", "sei\u00b7nem", "son\u00b7dern", "Bau", "man", "bey", "ihm", "den", "Sch\u00f6p\u00b7fer"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PPOSAT", "KON", "NN", "PIS", "APPR", "PPER", "ART", "NN"], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.4": {"text": "ehrt.", "tokens": ["ehrt", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Wunderbar an diesem Thier ist die wandelbare Haut,", "tokens": ["Wun\u00b7der\u00b7bar", "an", "die\u00b7sem", "Thier", "ist", "die", "wan\u00b7del\u00b7ba\u00b7re", "Haut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PDAT", "NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.6": {"text": "Als worinn man alle Farben, in best\u00e4nd\u2019ger Aendrung,", "tokens": ["Als", "wo\u00b7rinn", "man", "al\u00b7le", "Far\u00b7ben", ",", "in", "be\u00b7st\u00e4n\u00b7d'\u00b7ger", "A\u00b7en\u00b7drung", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PWAV", "PIS", "PIAT", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "--+-+-+-+-+--+-+", "measure": "iambic.septa.relaxed"}, "line.7": {"text": "schaut,", "tokens": ["schaut", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.8": {"text": "Die dieselbe von den K\u00f6rpern, die ihr nahe liegen, nimmt.", "tokens": ["Die", "die\u00b7sel\u00b7be", "von", "den", "K\u00f6r\u00b7pern", ",", "die", "ihr", "na\u00b7he", "lie\u00b7gen", ",", "nimmt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PDAT", "APPR", "ART", "NN", "$,", "PRELS", "PPER", "ADJD", "VVFIN", "$,", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.9": {"text": "Seine Augen scheinen gleichfalls zur Verwunderung be-", "tokens": ["Sei\u00b7ne", "Au\u00b7gen", "schei\u00b7nen", "gleich\u00b7falls", "zur", "Ver\u00b7wun\u00b7de\u00b7rung", "be"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "APPRART", "NN", "TRUNC"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.10": {"text": "stimmt,", "tokens": ["stimmt", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.11": {"text": "Da die Aepfel sich nicht drehn, wie an allen andern Thieren,", "tokens": ["Da", "die", "A\u00b7e\u00b7pfel", "sich", "nicht", "drehn", ",", "wie", "an", "al\u00b7len", "an\u00b7dern", "Thie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PRF", "PTKNEG", "VVINF", "$,", "PWAV", "APPR", "PIAT", "ADJA", "NN", "$,"], "meter": "--+-+--+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "Wenn er sehen will, so mu\u00df er das ganze Auge r\u00fchren,", "tokens": ["Wenn", "er", "se\u00b7hen", "will", ",", "so", "mu\u00df", "er", "das", "gan\u00b7ze", "Au\u00b7ge", "r\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVINF", "VMFIN", "$,", "ADV", "VMFIN", "PPER", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.13": {"text": "Und zwar beyde nicht zugleich, sondern, wenn er eines dreht,", "tokens": ["Und", "zwar", "bey\u00b7de", "nicht", "zu\u00b7gleich", ",", "son\u00b7dern", ",", "wenn", "er", "ei\u00b7nes", "dreht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PIS", "PTKNEG", "ADV", "$,", "KON", "$,", "KOUS", "PPER", "PIS", "VVFIN", "$,"], "meter": "--+--+-+-+-+-+", "measure": "anapaest.di.plus"}, "line.14": {"text": "Wird man insgemein gewahr, da\u00df das andre stille steht.", "tokens": ["Wird", "man", "ins\u00b7ge\u00b7mein", "ge\u00b7wahr", ",", "da\u00df", "das", "and\u00b7re", "stil\u00b7le", "steht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ADV", "ADJD", "$,", "KOUS", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.15": {"text": "Aus dem nie geschlo\u00dfnen Maul schie\u00dft er eine lange Zunge", "tokens": ["Aus", "dem", "nie", "ge\u00b7schlo\u00df\u00b7nen", "Maul", "schie\u00dft", "er", "ei\u00b7ne", "lan\u00b7ge", "Zun\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADV", "ADJA", "NN", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "+-+-+-++-+-+-+-", "measure": "unknown.measure.octa.plus"}, "line.16": {"text": "Mit so gro\u00dfer Schnelligkeit, da\u00df man sie kaum sieht.", "tokens": ["Mit", "so", "gro\u00b7\u00dfer", "Schnel\u00b7lig\u00b7keit", ",", "da\u00df", "man", "sie", "kaum", "sieht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "$,", "KOUS", "PIS", "PPER", "ADV", "VVFIN", "$."], "meter": "+-+-+-+----+", "measure": "unknown.measure.penta"}, "line.17": {"text": "Die Lunge", "tokens": ["Die", "Lun\u00b7ge"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.18": {"text": "Ist an ihm besonders gro\u00df. Alle W\u00fcrmer, Fliegen,", "tokens": ["Ist", "an", "ihm", "be\u00b7son\u00b7ders", "gro\u00df", ".", "Al\u00b7le", "W\u00fcr\u00b7mer", ",", "Flie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "APPR", "PPER", "ADV", "ADJD", "$.", "PIAT", "NN", "$,", "NN", "$,"], "meter": "-+--+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.19": {"text": "M\u00fccken", "tokens": ["M\u00fc\u00b7cken"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.20": {"text": "Weis er durch die Schnelligkeit seiner Zunge zu ber\u00fccken.", "tokens": ["Weis", "er", "durch", "die", "Schnel\u00b7lig\u00b7keit", "sei\u00b7ner", "Zun\u00b7ge", "zu", "be\u00b7r\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.21": {"text": "Zwar ist nicht viel Fleisch an ihnen,", "tokens": ["Zwar", "ist", "nicht", "viel", "Fleisch", "an", "ih\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PTKNEG", "PIAT", "NN", "APPR", "PPER", "$,"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.22": {"text": "Doch soll den Cochinchinesern es zur s\u00fc\u00dfen Nahrung dienen.", "tokens": ["Doch", "soll", "den", "Coch\u00b7in\u00b7chi\u00b7ne\u00b7sern", "es", "zur", "s\u00fc\u00b7\u00dfen", "Nah\u00b7rung", "die\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "NN", "PPER", "APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+----+-+-+-", "measure": "unknown.measure.hexa"}, "line.23": {"text": "Es wird auch in Arzeneyen von verschiedenen genommen,", "tokens": ["Es", "wird", "auch", "in", "Ar\u00b7ze\u00b7ne\u00b7yen", "von", "ver\u00b7schie\u00b7de\u00b7nen", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "NN", "APPR", "ADJA", "VVPP", "$,"], "meter": "-+--+--+--+-+-+-", "measure": "amphibrach.tetra.plus"}, "line.24": {"text": "In sehr vielerley Gebrechen soll es uns zu Nutzen kommen;", "tokens": ["In", "sehr", "vie\u00b7ler\u00b7ley", "Ge\u00b7bre\u00b7chen", "soll", "es", "uns", "zu", "Nut\u00b7zen", "kom\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "PIAT", "NN", "VMFIN", "PPER", "PRF", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.25": {"text": "In den Fiebern, Podagra, auch in denen b\u00f6sen Seuchen,", "tokens": ["In", "den", "Fie\u00b7bern", ",", "Po\u00b7da\u00b7gra", ",", "auch", "in", "de\u00b7nen", "b\u00f6\u00b7sen", "Seu\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "NE", "$,", "ADV", "APPR", "PRELS", "ADJA", "NN", "$,"], "meter": "--+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.26": {"text": "Sollen sie, wie mans erfahren, \u00f6fters schnelle H\u00fclf uns", "tokens": ["Sol\u00b7len", "sie", ",", "wie", "mans", "er\u00b7fah\u00b7ren", ",", "\u00f6f\u00b7ters", "schnel\u00b7le", "H\u00fclf", "uns"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "$,", "PWAV", "PIS", "VVINF", "$,", "ADV", "ADJA", "NN", "PPER"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.27": {"text": "reichen.", "tokens": ["rei\u00b7chen", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}}}}}