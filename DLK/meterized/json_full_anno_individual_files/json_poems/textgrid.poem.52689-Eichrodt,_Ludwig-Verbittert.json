{"textgrid.poem.52689": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Verbittert", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Himmel blau und warmes Wetter!", "tokens": ["Der", "Him\u00b7mel", "blau", "und", "war\u00b7mes", "Wet\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Im Fr\u00fchling werden Menschen G\u00f6tter,", "tokens": ["Im", "Fr\u00fch\u00b7ling", "wer\u00b7den", "Men\u00b7schen", "G\u00f6t\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und tausend Wunden werden heil,", "tokens": ["Und", "tau\u00b7send", "Wun\u00b7den", "wer\u00b7den", "heil", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "CARD", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es bricht das Eis der Erdenleiden", "tokens": ["Es", "bricht", "das", "Eis", "der", "Er\u00b7den\u00b7lei\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Von der Olympier ewgen Freuden", "tokens": ["Von", "der", "O\u00b7lym\u00b7pier", "ew\u00b7gen", "Freu\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wird Sterblichen ein Lenz zu Theil.", "tokens": ["Wird", "Sterb\u00b7li\u00b7chen", "ein", "Lenz", "zu", "Theil", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "O herrlich, Sonnenschein zu trinken,", "tokens": ["O", "herr\u00b7lich", ",", "Son\u00b7nen\u00b7schein", "zu", "trin\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "$,", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "O selig, wo die Kelche winken", "tokens": ["O", "se\u00b7lig", ",", "wo", "die", "Kel\u00b7che", "win\u00b7ken"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "ADJD", "$,", "PWAV", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Mit s\u00fc\u00dfem Thau und mildem Duft!", "tokens": ["Mit", "s\u00fc\u00b7\u00dfem", "Thau", "und", "mil\u00b7dem", "Duft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Schlaffen und die Tr\u00e4gen eilen,", "tokens": ["Die", "Schlaf\u00b7fen", "und", "die", "Tr\u00e4\u00b7gen", "ei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wo sie zum lieblichsten Verweilen", "tokens": ["Wo", "sie", "zum", "lieb\u00b7lichs\u00b7ten", "Ver\u00b7wei\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+-+---+-", "measure": "unknown.measure.tri"}, "line.6": {"text": "Das Glockenspiel der W\u00e4lder ruft.", "tokens": ["Das", "Glo\u00b7cken\u00b7spiel", "der", "W\u00e4l\u00b7der", "ruft", "."], "token_info": ["word", "word", "word", "word", "word"