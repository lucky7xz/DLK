{"dta.poem.308": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Er beklaget ihre Grausamkeit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Jhr Fl\u00fcsse/ und ihr harten Steine/", "tokens": ["Ihr", "Fl\u00fcs\u00b7se", "/", "und", "ihr", "har\u00b7ten", "Stei\u00b7ne", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "KON", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Seyd Zeugen meiner Liebes-Pein/", "tokens": ["Seyd", "Zeu\u00b7gen", "mei\u00b7ner", "Lie\u00b7bes\u00b7Pein", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAIMP", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Jhr wi\u00dft wie offtmahls ich beweine", "tokens": ["Ihr", "wi\u00dft", "wie", "offt\u00b7mahls", "ich", "be\u00b7wei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KOKOM", "ADV", "PPER", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der sch\u00f6nen ", "tokens": ["Der", "sch\u00f6\u00b7nen"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Es sollen sich mit euch vermischen", "tokens": ["Es", "sol\u00b7len", "sich", "mit", "euch", "ver\u00b7mi\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PRF", "APPR", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die Th\u00e4nen/ so mein Haupt vergeu\u00dft", "tokens": ["Die", "Th\u00e4\u00b7nen", "/", "so", "mein", "Haupt", "ver\u00b7geu\u00dft"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$(", "ADV", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Es soll das d\u00fcrre Feld erfrischen", "tokens": ["Es", "soll", "das", "d\u00fcr\u00b7re", "Feld", "er\u00b7fri\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der Z\u00e4hren-Thau der von mir fleu\u00dft.", "tokens": ["Der", "Z\u00e4h\u00b7ren\u00b7\u00b7T\u00b7hau", "der", "von", "mir", "fleu\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "APPR", "PPER", "VVFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.2": {"text": "Mit ihrer Jammer-Stimme bey.", "tokens": ["Mit", "ih\u00b7rer", "Jam\u00b7mer\u00b7Stim\u00b7me", "bey", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und mehret meiner Schwermuth-Plagen", "tokens": ["Und", "meh\u00b7ret", "mei\u00b7ner", "Schwer\u00b7muth\u00b7Pla\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Durch wiederholtes Angst-Geschrey.", "tokens": ["Durch", "wie\u00b7der\u00b7hol\u00b7tes", "Angst\u00b7Ge\u00b7schrey", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Jammer zehret ab mein Leben", "tokens": ["Der", "Jam\u00b7mer", "zeh\u00b7ret", "ab", "mein", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und treibt mich zur Verzweiffelung;", "tokens": ["Und", "treibt", "mich", "zur", "Ver\u00b7zweif\u00b7fe\u00b7lung", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPRART", "NN", "$."], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.7": {"text": "Wird ", "tokens": ["Wird"], "token_info": ["word"], "pos": ["VAFIN"], "meter": "+", "measure": "single.up"}, "line.8": {"text": "Empfind\u2019 ich keine Linderung?", "tokens": ["Emp\u00b7find'", "ich", "kei\u00b7ne", "Lin\u00b7de\u00b7rung", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}}, "stanza.3": {"line.1": {"text": "Das Ungl\u00fcck schiest mit seinen Pfeilen", "tokens": ["Das", "Un\u00b7gl\u00fcck", "schiest", "mit", "sei\u00b7nen", "Pfei\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Auf mein betr\u00fcbtes Hertze zu/", "tokens": ["Auf", "mein", "be\u00b7tr\u00fcb\u00b7tes", "Hert\u00b7ze", "zu", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "VVFIN", "PTKZU", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und keiner kan die Wunden heilen", "tokens": ["Und", "kei\u00b7ner", "kan", "die", "Wun\u00b7den", "hei\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "VMFIN", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Es wei\u00df mein Geist von keiner Ruh/", "tokens": ["Es", "wei\u00df", "mein", "Geist", "von", "kei\u00b7ner", "Ruh", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "PIAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da ", "tokens": ["Da"], "token_info": ["word"], "pos": ["KOUS"], "meter": "+", "measure": "single.up"}, "line.6": {"text": "Von keiner Liebes-Gluth entbrennt/", "tokens": ["Von", "kei\u00b7ner", "Lie\u00b7bes\u00b7Gluth", "ent\u00b7brennt", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Nur lacht zu meiner Seelen-Schmertze", "tokens": ["Nur", "lacht", "zu", "mei\u00b7ner", "See\u00b7len\u00b7Schmert\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und sich mit Freuden grausahm nennt.", "tokens": ["Und", "sich", "mit", "Freu\u00b7den", "grau\u00b7sahm", "nennt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "APPR", "NN", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Soll denn mein Angst-beseltes Schreyen", "tokens": ["Soll", "denn", "mein", "Angst\u00b7be\u00b7sel\u00b7tes", "Schre\u00b7yen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Niemahls erweichen deine Brust?", "tokens": ["Nie\u00b7mahls", "er\u00b7wei\u00b7chen", "dei\u00b7ne", "Brust", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Soll sich mein Leiden stets verneuen?", "tokens": ["Soll", "sich", "mein", "Lei\u00b7den", "stets", "ver\u00b7neu\u00b7en", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "PPOSAT", "NN", "ADV", "VVINF", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Schafft dir denn meine Folter Lust?", "tokens": ["Schafft", "dir", "denn", "mei\u00b7ne", "Fol\u00b7ter", "Lust", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So la\u00df der Augen heisses Blitzen", "tokens": ["So", "la\u00df", "der", "Au\u00b7gen", "heis\u00b7ses", "Blit\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVIMP", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "So fort verzehren meinen Geist", "tokens": ["So", "fort", "ver\u00b7zeh\u00b7ren", "mei\u00b7nen", "Geist"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PTKVZ", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Denn was kan dir mein Jammer n\u00fctzen", "tokens": ["Denn", "was", "kan", "dir", "mein", "Jam\u00b7mer", "n\u00fct\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "VMFIN", "PPER", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Wenn sich dein Paradies verschleu\u00dft?", "tokens": ["Wenn", "sich", "dein", "Pa\u00b7ra\u00b7dies", "ver\u00b7schleu\u00dft", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Wo nicht so schaffe da\u00df mein Hoffen", "tokens": ["Wo", "nicht", "so", "schaf\u00b7fe", "da\u00df", "mein", "Hof\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PTKNEG", "ADV", "VVFIN", "KOUS", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den frohen Haven bald erreicht:", "tokens": ["Den", "fro\u00b7hen", "Ha\u00b7ven", "bald", "er\u00b7reicht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Steht denn dein Paradies mir offen", "tokens": ["Steht", "denn", "dein", "Pa\u00b7ra\u00b7dies", "mir", "of\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "PPER", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So mache/ da\u00df der Engel weicht", "tokens": ["So", "ma\u00b7che", "/", "da\u00df", "der", "En\u00b7gel", "weicht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$(", "KOUS", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der mit dem Schwerdte deines Zornes", "tokens": ["Der", "mit", "dem", "Schwerd\u00b7te", "dei\u00b7nes", "Zor\u00b7nes"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Den Gang zum Lebens-Baum verwehrt/", "tokens": ["Den", "Gang", "zum", "Le\u00b7bens\u00b7Baum", "ver\u00b7wehrt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So wird mein Schatz des Wollust-Hornes", "tokens": ["So", "wird", "mein", "Schatz", "des", "Wol\u00b7lust\u00b7Hor\u00b7nes"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "In dir mein ", "tokens": ["In", "dir", "mein"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPER", "PPOSAT"], "meter": "+-+", "measure": "trochaic.di"}}}}}