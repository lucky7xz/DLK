{"dta.poem.10127": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Gottes Gr\u00f6sse aus Seinen Wercken.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20086-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Auf! lasset uns dasjenige, was sch\u00f6n,", "tokens": ["Auf", "!", "las\u00b7set", "uns", "das\u00b7je\u00b7ni\u00b7ge", ",", "was", "sch\u00f6n", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "$.", "VVFIN", "PPER", "PDS", "$,", "PRELS", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Was angenehm und herrlich auf der Welt,", "tokens": ["Was", "an\u00b7ge\u00b7nehm", "und", "herr\u00b7lich", "auf", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "KON", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "In GOTTES Creatur, uns vorgestellt,", "tokens": ["In", "GoT\u00b7TES", "Crea\u00b7tur", ",", "uns", "vor\u00b7ge\u00b7stellt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "$,", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Mit aufmercksamen Augen sehn!", "tokens": ["Mit", "auf\u00b7merck\u00b7sa\u00b7men", "Au\u00b7gen", "sehn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die blaue Lufft, die gr\u00fcne See,", "tokens": ["Die", "blau\u00b7e", "Lufft", ",", "die", "gr\u00fc\u00b7ne", "See", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Bluhmen Schmuck, der Berge H\u00f6h\u2019,", "tokens": ["Der", "Bluh\u00b7men", "Schmuck", ",", "der", "Ber\u00b7ge", "H\u00f6h'", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das holde Prangen bunter Felder,", "tokens": ["Das", "hol\u00b7de", "Pran\u00b7gen", "bun\u00b7ter", "Fel\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Anmuth-mehr als Schatten-reichen W\u00e4lder,", "tokens": ["Die", "An\u00b7muth\u00b7mehr", "als", "Schat\u00b7ten\u00b7rei\u00b7chen", "W\u00e4l\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KOUS", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Das Firmament, der Mond, der Sonnen Licht und Flam\u0303en,", "tokens": ["Das", "Fir\u00b7ma\u00b7ment", ",", "der", "Mond", ",", "der", "Son\u00b7nen", "Licht", "und", "Flam\u0303en", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die zeigen offenbar, da\u00df ihrer Sch\u00f6nheit Pracht,", "tokens": ["Die", "zei\u00b7gen", "of\u00b7fen\u00b7bar", ",", "da\u00df", "ih\u00b7rer", "Sch\u00f6n\u00b7heit", "Pracht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADJD", "$,", "KOUS", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Weil nichts sich selber macht,", "tokens": ["Weil", "nichts", "sich", "sel\u00b7ber", "macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PRF", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Aus GOTT allein entstanden sind, und stammen.", "tokens": ["Aus", "GoTT", "al\u00b7lein", "ent\u00b7stan\u00b7den", "sind", ",", "und", "stam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NE", "ADV", "VVPP", "VAFIN", "$,", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wenn wir nun alles die\u00df verbinden", "tokens": ["Wenn", "wir", "nun", "al\u00b7les", "die\u00df", "ver\u00b7bin\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "PIS", "PDS", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In eine Macht, und solche Macht bedencken,", "tokens": ["In", "ei\u00b7ne", "Macht", ",", "und", "sol\u00b7che", "Macht", "be\u00b7den\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "KON", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In welcher alles die\u00df gewesen,", "tokens": ["In", "wel\u00b7cher", "al\u00b7les", "die\u00df", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PIS", "PDS", "VAPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So werden wir solch eine Gr\u00f6sse finden,", "tokens": ["So", "wer\u00b7den", "wir", "solch", "ei\u00b7ne", "Gr\u00f6s\u00b7se", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIAT", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wobey uns hier Verstand, sammt h\u00f6ren, sehn,", "tokens": ["Wo\u00b7bey", "uns", "hier", "Ver\u00b7stand", ",", "sammt", "h\u00f6\u00b7ren", ",", "sehn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "NN", "$,", "VVFIN", "VVINF", "$,", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Verschwinden und vergehn.", "tokens": ["Ver\u00b7schwin\u00b7den", "und", "ver\u00b7gehn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ja wie viel gr\u00f6sser wird annoch des Sch\u00f6pfers Gr\u00f6sse,", "tokens": ["Ja", "wie", "viel", "gr\u00f6s\u00b7ser", "wird", "an\u00b7noch", "des", "Sch\u00f6p\u00b7fers", "Gr\u00f6s\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "KOKOM", "ADV", "ADJD", "VAFIN", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn eine Zahl, die alle \u00fcbersteiget,", "tokens": ["Wenn", "ei\u00b7ne", "Zahl", ",", "die", "al\u00b7le", "\u00fc\u00b7bers\u00b7tei\u00b7get", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "PRELS", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Von solchen Welten sich an Aug\u2019 und Seele zeiget!", "tokens": ["Von", "sol\u00b7chen", "Wel\u00b7ten", "sich", "an", "Aug'", "und", "See\u00b7le", "zei\u00b7get", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PRF", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Noch mehr, was ausser dieser Gr\u00f6ss\u2019 und Macht", "tokens": ["Noch", "mehr", ",", "was", "aus\u00b7ser", "die\u00b7ser", "Gr\u00f6ss'", "und", "Macht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "$,", "PRELS", "APPR", "PDAT", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Annoch f\u00fcr andere Vollkommenheiten,", "tokens": ["An\u00b7noch", "f\u00fcr", "an\u00b7de\u00b7re", "Voll\u00b7kom\u00b7men\u00b7hei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Vortrefflichkeit- und F\u00e4higkeiten", "tokens": ["Vor\u00b7treff\u00b7lich\u00b7keit", "und", "F\u00e4\u00b7hig\u00b7kei\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["TRUNC", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In andern Welten noch hervor gebracht,", "tokens": ["In", "an\u00b7dern", "Wel\u00b7ten", "noch", "her\u00b7vor", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "PTKVZ", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Zeigt den darob erschrockenen Gedancken", "tokens": ["Zeigt", "den", "da\u00b7rob", "er\u00b7schro\u00b7cke\u00b7nen", "Ge\u00b7dan\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "PAV", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ein\u2019 Allmacht, sonder End und Schrancken.", "tokens": ["Ein'", "All\u00b7macht", ",", "son\u00b7der", "End", "und", "Schran\u00b7cken", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KON", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}