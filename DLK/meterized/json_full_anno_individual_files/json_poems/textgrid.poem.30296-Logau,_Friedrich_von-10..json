{"textgrid.poem.30296": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "10.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Jungfern, habt ihr nicht vernummen,", "tokens": ["Jung\u00b7fern", ",", "habt", "ihr", "nicht", "ver\u00b7num\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VAFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo die Bienen her sind kummen?", "tokens": ["Wo", "die", "Bie\u00b7nen", "her", "sind", "kum\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APZR", "VAFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Habt ihr doch vielleicht verstanden,", "tokens": ["Habt", "ihr", "doch", "viel\u00b7leicht", "ver\u00b7stan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Was der Venus gieng zu handen,", "tokens": ["Was", "der", "Ve\u00b7nus", "gieng", "zu", "han\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Da sie den Adonis liebte,", "tokens": ["Da", "sie", "den", "A\u00b7do\u00b7nis", "lieb\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Der sie labt und auch betr\u00fcbte?", "tokens": ["Der", "sie", "labt", "und", "auch", "be\u00b7tr\u00fcb\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "KON", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wann im Schaten k\u00fchler Myrten", "tokens": ["Wann", "im", "Scha\u00b7ten", "k\u00fch\u00b7ler", "Myr\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPRART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Sie sich kamen zu bewirthen,", "tokens": ["Sie", "sich", "ka\u00b7men", "zu", "be\u00b7wirt\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PRF", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Folgte nichts als lieblich liebeln,", "tokens": ["Folg\u00b7te", "nichts", "als", "lieb\u00b7lich", "lie\u00b7beln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "KOKOM", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Folgte nichts als t\u00fcckisch b\u00fcbeln,", "tokens": ["Folg\u00b7te", "nichts", "als", "t\u00fc\u00b7ckisch", "b\u00fc\u00b7beln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "KOKOM", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Wolten ohne s\u00fcsses k\u00fcssen", "tokens": ["Wol\u00b7ten", "oh\u00b7ne", "s\u00fcs\u00b7ses", "k\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Nimmer keine Zeit vermissen,", "tokens": ["Nim\u00b7mer", "kei\u00b7ne", "Zeit", "ver\u00b7mis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "K\u00fcsten eine lange L\u00e4nge,", "tokens": ["K\u00fcs\u00b7ten", "ei\u00b7ne", "lan\u00b7ge", "L\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "K\u00fcsten eine grosse Menge,", "tokens": ["K\u00fcs\u00b7ten", "ei\u00b7ne", "gros\u00b7se", "Men\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "K\u00fcsten immer in die Wette;", "tokens": ["K\u00fcs\u00b7ten", "im\u00b7mer", "in", "die", "Wet\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Eines war de\u00df andren Klette,", "tokens": ["Ei\u00b7nes", "war", "de\u00df", "an\u00b7dren", "Klet\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Bi\u00df es Venus so verf\u00fcgte,", "tokens": ["Bi\u00df", "es", "Ve\u00b7nus", "so", "ver\u00b7f\u00fcg\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Die di\u00df Thun so wol vergn\u00fcgte,", "tokens": ["Die", "di\u00df", "Thun", "so", "wol", "ver\u00b7gn\u00fcg\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PDS", "NN", "ADV", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Da\u00df die Geister, die sie hauchten,", "tokens": ["Da\u00df", "die", "Geis\u00b7ter", ",", "die", "sie", "hauch\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Immer blieben, nie verrauchten;", "tokens": ["Im\u00b7mer", "blie\u00b7ben", ",", "nie", "ver\u00b7rauch\u00b7ten", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Da\u00df die K\u00fcsse Fl\u00fcgel namen", "tokens": ["Da\u00df", "die", "K\u00fcs\u00b7se", "Fl\u00fc\u00b7gel", "na\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Hin und her mit Heeren kamen,", "tokens": ["Hin", "und", "her", "mit", "Hee\u00b7ren", "ka\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "V\u00f6llten alles Leer der L\u00fcffte,", "tokens": ["V\u00f6ll\u00b7ten", "al\u00b7les", "Leer", "der", "L\u00fcff\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Wiese, Thal, Berg, Wald, Feld, Kl\u00fcffte,", "tokens": ["Wie\u00b7se", ",", "Thal", ",", "Berg", ",", "Wald", ",", "Feld", ",", "Kl\u00fcff\u00b7te", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "Parten sich zum k\u00fcssen immer,", "tokens": ["Par\u00b7ten", "sich", "zum", "k\u00fcs\u00b7sen", "im\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPRART", "VVFIN", "ADV", "$,"], "meter": "+-+-+-++", "measure": "unknown.measure.penta"}, "line.26": {"text": "Hilten ohne sich sich nimmer,", "tokens": ["Hil\u00b7ten", "oh\u00b7ne", "sich", "sich", "nim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PRF", "PRF", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.27": {"text": "Sassen auff die Menschen-T\u00f6chter,", "tokens": ["Sas\u00b7sen", "auff", "die", "Men\u00b7schen\u00b7T\u00f6ch\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Machten manches Mund-Gel\u00e4chter,", "tokens": ["Mach\u00b7ten", "man\u00b7ches", "Mun\u00b7dGe\u00b7l\u00e4ch\u00b7ter", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.29": {"text": "Wann sie sie mit K\u00fcssen gr\u00fc\u00dfen,", "tokens": ["Wann", "sie", "sie", "mit", "K\u00fcs\u00b7sen", "gr\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.30": {"text": "Wann sie sie mit Gr\u00fcssen k\u00fc\u00dfen.", "tokens": ["Wann", "sie", "sie", "mit", "Gr\u00fcs\u00b7sen", "k\u00fc\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.31": {"text": "Aber Neid hat scheel gesehen,", "tokens": ["A\u00b7ber", "Neid", "hat", "scheel", "ge\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.32": {"text": "Und Verh\u00e4ngn\u00fc\u00df lie\u00df geschehen,", "tokens": ["Und", "Ver\u00b7h\u00e4ng\u00b7n\u00fc\u00df", "lie\u00df", "ge\u00b7sche\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.33": {"text": "Da\u00df ein schaumend wilder Eber", "tokens": ["Da\u00df", "ein", "schau\u00b7mend", "wil\u00b7der", "E\u00b7ber"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJD", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.34": {"text": "Ward Adonis Todtengr\u00e4ber.", "tokens": ["Ward", "A\u00b7do\u00b7nis", "Tod\u00b7ten\u00b7gr\u00e4\u00b7ber", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.35": {"text": "Venus, voller Zorn und w\u00fcten,", "tokens": ["Ve\u00b7nus", ",", "vol\u00b7ler", "Zorn", "und", "w\u00fc\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.36": {"text": "Hat gar schwerlich di\u00df erlidten.", "tokens": ["Hat", "gar", "schwer\u00b7lich", "di\u00df", "er\u00b7lid\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "PDS", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.37": {"text": "Als sie mehr nicht kunte schaffen,", "tokens": ["Als", "sie", "mehr", "nicht", "kun\u00b7te", "schaf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PTKNEG", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.38": {"text": "Gieng sie, lie\u00df zusammen raffen,", "tokens": ["Gieng", "sie", ",", "lie\u00df", "zu\u00b7sam\u00b7men", "raf\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "VVFIN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.39": {"text": "Aller dieser K\u00fcsse Summen,", "tokens": ["Al\u00b7ler", "die\u00b7ser", "K\u00fcs\u00b7se", "Sum\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "PDAT", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.40": {"text": "Wo sie waren zu bekummen,", "tokens": ["Wo", "sie", "wa\u00b7ren", "zu", "be\u00b7kum\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VAFIN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.41": {"text": "Machte drau\u00df die Honig-Leute,", "tokens": ["Mach\u00b7te", "drau\u00df", "die", "Ho\u00b7nig\u00b7Leu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PAV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.42": {"text": "Da\u00df sie geben s\u00fcsse Beute,", "tokens": ["Da\u00df", "sie", "ge\u00b7ben", "s\u00fcs\u00b7se", "Beu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVINF", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.43": {"text": "Da\u00df sie aber auch daneben", "tokens": ["Da\u00df", "sie", "a\u00b7ber", "auch", "da\u00b7ne\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADV", "PAV"], "meter": "+-+-----", "measure": "unknown.measure.di"}, "line.44": {"text": "Einen scharffen Stachel geben,", "tokens": ["Ei\u00b7nen", "scharf\u00b7fen", "Sta\u00b7chel", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.45": {"text": "So wie sie das K\u00fcssen b\u00fcssen", "tokens": ["So", "wie", "sie", "das", "K\u00fcs\u00b7sen", "b\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "PPER", "ART", "NN", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.46": {"text": "Und mit Leid verbittern m\u00fcssen.", "tokens": ["Und", "mit", "Leid", "ver\u00b7bit\u00b7tern", "m\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVINF", "VMINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.47": {"text": "Sag ich dieses einem Tauben,", "tokens": ["Sag", "ich", "die\u00b7ses", "ei\u00b7nem", "Tau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PDAT", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.48": {"text": "Und ihr Jungfern wolts nicht glauben,", "tokens": ["Und", "ihr", "Jung\u00b7fern", "wolts", "nicht", "glau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.49": {"text": "W\u00fcnsch ich euch f\u00fcr solches St\u00fccke,", "tokens": ["W\u00fcnsch", "ich", "euch", "f\u00fcr", "sol\u00b7ches", "St\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PRF", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.50": {"text": "Da\u00df euch K\u00fcssen nie erquicke;", "tokens": ["Da\u00df", "euch", "K\u00fcs\u00b7sen", "nie", "er\u00b7qui\u00b7cke", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.51": {"text": "Glaubt ihrs aber, o, so schauet,", "tokens": ["Glaubt", "ihrs", "a\u00b7ber", ",", "o", ",", "so", "schau\u00b7et", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "FM", "$,", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.52": {"text": "Da\u00df ihr nicht dem Stachel trauet!", "tokens": ["Da\u00df", "ihr", "nicht", "dem", "Sta\u00b7chel", "trau\u00b7et", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Jungfern, habt ihr nicht vernummen,", "tokens": ["Jung\u00b7fern", ",", "habt", "ihr", "nicht", "ver\u00b7num\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VAFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo die Bienen her sind kummen?", "tokens": ["Wo", "die", "Bie\u00b7nen", "her", "sind", "kum\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APZR", "VAFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Habt ihr doch vielleicht verstanden,", "tokens": ["Habt", "ihr", "doch", "viel\u00b7leicht", "ver\u00b7stan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Was der Venus gieng zu handen,", "tokens": ["Was", "der", "Ve\u00b7nus", "gieng", "zu", "han\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Da sie den Adonis liebte,", "tokens": ["Da", "sie", "den", "A\u00b7do\u00b7nis", "lieb\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Der sie labt und auch betr\u00fcbte?", "tokens": ["Der", "sie", "labt", "und", "auch", "be\u00b7tr\u00fcb\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "KON", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wann im Schaten k\u00fchler Myrten", "tokens": ["Wann", "im", "Scha\u00b7ten", "k\u00fch\u00b7ler", "Myr\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPRART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Sie sich kamen zu bewirthen,", "tokens": ["Sie", "sich", "ka\u00b7men", "zu", "be\u00b7wirt\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PRF", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Folgte nichts als lieblich liebeln,", "tokens": ["Folg\u00b7te", "nichts", "als", "lieb\u00b7lich", "lie\u00b7beln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "KOKOM", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Folgte nichts als t\u00fcckisch b\u00fcbeln,", "tokens": ["Folg\u00b7te", "nichts", "als", "t\u00fc\u00b7ckisch", "b\u00fc\u00b7beln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "KOKOM", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Wolten ohne s\u00fcsses k\u00fcssen", "tokens": ["Wol\u00b7ten", "oh\u00b7ne", "s\u00fcs\u00b7ses", "k\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Nimmer keine Zeit vermissen,", "tokens": ["Nim\u00b7mer", "kei\u00b7ne", "Zeit", "ver\u00b7mis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "K\u00fcsten eine lange L\u00e4nge,", "tokens": ["K\u00fcs\u00b7ten", "ei\u00b7ne", "lan\u00b7ge", "L\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "K\u00fcsten eine grosse Menge,", "tokens": ["K\u00fcs\u00b7ten", "ei\u00b7ne", "gros\u00b7se", "Men\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "K\u00fcsten immer in die Wette;", "tokens": ["K\u00fcs\u00b7ten", "im\u00b7mer", "in", "die", "Wet\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Eines war de\u00df andren Klette,", "tokens": ["Ei\u00b7nes", "war", "de\u00df", "an\u00b7dren", "Klet\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Bi\u00df es Venus so verf\u00fcgte,", "tokens": ["Bi\u00df", "es", "Ve\u00b7nus", "so", "ver\u00b7f\u00fcg\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Die di\u00df Thun so wol vergn\u00fcgte,", "tok