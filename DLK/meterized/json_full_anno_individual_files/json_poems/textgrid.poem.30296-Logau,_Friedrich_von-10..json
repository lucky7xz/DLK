r": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.37": {"text": "Als sie mehr nicht kunte schaffen,", "tokens": ["Als", "sie", "mehr", "nicht", "kun\u00b7te", "schaf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PTKNEG", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.38": {"text": "Gieng sie, lie\u00df zusammen raffen,", "tokens": ["Gieng", "sie", ",", "lie\u00df", "zu\u00b7sam\u00b7men", "raf\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "VVFIN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.39": {"text": "Aller dieser K\u00fcsse Summen,", "tokens": ["Al\u00b7ler", "die\u00b7ser", "K\u00fcs\u00b7se", "Sum\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "PDAT", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.40": {"text": "Wo sie waren zu bekummen,", "tokens": ["Wo", "sie", "wa\u00b7ren", "zu", "be\u00b7kum\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VAFIN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.41": {"text": "Machte drau\u00df die Honig-Leute,", "tokens": ["Mach\u00b7te", "drau\u00df", "die", "Ho\u00b7nig\u00b7Leu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PAV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.42": {"text": "Da\u00df sie geben s\u00fcsse Beute,", "tokens": ["Da\u00df", "sie", "ge\u00b7ben", "s\u00fcs\u00b7se", "Beu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVINF", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.43": {"text": "Da\u00df sie aber auch daneben", "tokens": ["Da\u00df", "sie", "a\u00b7ber", "auch", "da\u00b7ne\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADV", "PAV"], "meter": "+-+-----", "measure": "unknown.measure.di"}, "line.44": {"text": "Einen scharffen Stachel geben,", "tokens": ["Ei\u00b7nen", "scharf\u00b7fen", "Sta\u00b7chel", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.45": {"text": "So wie sie das K\u00fcssen b\u00fcssen", "tokens": ["So", "wie", "sie", "das", "K\u00fcs\u00b7sen", "b\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "PPER", "ART", "NN", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.46": {"text": "Und mit Leid verbittern m\u00fcssen.", "tokens": ["Und", "mit", "Leid", "ver\u00b7bit\u00b7tern", "m\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVINF", "VMINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.47": {"text": "Sag ich dieses einem Tauben,", "tokens": ["Sag", "ich", "die\u00b7ses", "ei\u00b7nem", "Tau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PDAT", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.48": {"text": "Und ihr Jungfern wolts nicht glauben,", "tokens": ["Und", "ihr", "Jung\u00b7fern", "wolts", "nicht", "glau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.49": {"text": "W\u00fcnsch ich euch f\u00fcr solches St\u00fccke,", "tokens": ["W\u00fcnsch", "ich", "euch", "f\u00fcr", "sol\u00b7ches", "St\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PRF", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.50": {"text": "Da\u00df euch K\u00fcssen nie erquicke;", "tokens": ["Da\u00df", "euch", "K\u00fcs\u00b7sen", "nie", "er\u00b7qui\u00b7cke", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.51": {"text": "Glaubt ihrs aber, o, so schauet,", "tokens": ["Glaubt", "ihrs", "a\u00b7ber", ",", "o", ",", "so", "schau\u00b7et", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "FM", "$,", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.52": {"text": "Da\u00df ihr nicht dem Stachel trauet!", "tokens": ["Da\u00df", "ihr", "nicht", "dem", "Sta\u00b7chel", "trau\u00b7et", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}