{"dta.poem.8633": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1832", "urn": "urn:nbn:de:kobv:b4-200905193572", "language": ["de:0.99"], "booktitle": "Lenau, Nikolaus: Gedichte. Stuttgart, 1832."}, "poem": {"stanza.1": {"line.1": {"text": "Dort heult im tiefen Waldesraum", "tokens": ["Dort", "heult", "im", "tie\u00b7fen", "Wal\u00b7des\u00b7raum"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Wolf; \u2014 wie's Kind aufweckt die Mutter,", "tokens": ["Ein", "Wolf", ";", "wie's", "Kind", "auf\u00b7weckt", "die", "Mut\u00b7ter", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "$.", "$(", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Schreit er die Nacht aus ihrem Traum,", "tokens": ["Schreit", "er", "die", "Nacht", "aus", "ih\u00b7rem", "Traum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und heischt von ihr sein blutig Futter.", "tokens": ["Und", "heischt", "von", "ihr", "sein", "blu\u00b7tig", "Fut\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "PPOSAT", "ADJD", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Nun brausen \u00fcber Schnee und Eis", "tokens": ["Nun", "brau\u00b7sen", "\u00fc\u00b7ber", "Schnee", "und", "Eis"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Winde fort mit tollem Jagen,", "tokens": ["Die", "Win\u00b7de", "fort", "mit", "tol\u00b7lem", "Ja\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Als wollten sie sich rennen hei\u00df:", "tokens": ["Als", "woll\u00b7ten", "sie", "sich", "ren\u00b7nen", "hei\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VMFIN", "PPER", "PRF", "VVFIN", "ADJD", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Wach auf, o Herz, zu wildem Klagen!", "tokens": ["Wach", "auf", ",", "o", "Herz", ",", "zu", "wil\u00b7dem", "Kla\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$,", "FM", "NN", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "La\u00df deine Todten auferstehn,", "tokens": ["La\u00df", "dei\u00b7ne", "Tod\u00b7ten", "auf\u00b7er\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und deiner Qualen dunkle Horden!", "tokens": ["Und", "dei\u00b7ner", "Qua\u00b7len", "dunk\u00b7le", "Hor\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und la\u00df sie mit den St\u00fcrmen gehn,", "tokens": ["Und", "la\u00df", "sie", "mit", "den", "St\u00fcr\u00b7men", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die frischer immer wehn vom Norden!", "tokens": ["Die", "fri\u00b7scher", "im\u00b7mer", "wehn", "vom", "Nor\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADV", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}