{"dta.poem.315": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Er liebet in der Stille.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Ich will \u00fcben", "tokens": ["Ich", "will", "\u00fc\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VMFIN", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Stilles Liben/", "tokens": ["Stil\u00b7les", "Li\u00b7ben", "/"], "token_info": ["word", "word", "punct"], "pos": ["NE", "NE", "$("], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Stilles Lieben bringet Lust.", "tokens": ["Stil\u00b7les", "Lie\u00b7ben", "brin\u00b7get", "Lust", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "VVFIN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ob es ander gleich nicht wissen/", "tokens": ["Ob", "es", "an\u00b7der", "gleich", "nicht", "wis\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "ADV", "PTKNEG", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie ich mu\u00df mein Sch\u00e4tzgen k\u00fcssen/", "tokens": ["Wie", "ich", "mu\u00df", "mein", "Sch\u00e4tz\u00b7gen", "k\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VMFIN", "PPOSAT", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ists doch mir und ihr bewust.", "tokens": ["Ists", "doch", "mir", "und", "ihr", "be\u00b7wust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "PPER", "KON", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "S\u00fcsses Leben", "tokens": ["S\u00fcs\u00b7ses", "Le\u00b7ben"], "token_info": ["word", "word"], "pos": ["NE", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Kan sie geben", "tokens": ["Kan", "sie", "ge\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["VMFIN", "PPER", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Wenn sie ihre Br\u00fcste reicht/", "tokens": ["Wenn", "sie", "ih\u00b7re", "Br\u00fcs\u00b7te", "reicht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Dieser sch\u00f6nen Wunder-Ballen", "tokens": ["Die\u00b7ser", "sch\u00f6\u00b7nen", "Wun\u00b7der\u00b7Bal\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["PDAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Bl\u00e4hen und auch niederfallen", "tokens": ["Bl\u00e4\u00b7hen", "und", "auch", "nie\u00b7der\u00b7fal\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KON", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Einem ", "tokens": ["Ei\u00b7nem"], "token_info": ["word"], "pos": ["ART"], "meter": "+-", "measure": "trochaic.single"}}, "stanza.3": {"line.1": {"text": "Jhr Umschliessen", "tokens": ["Ihr", "Um\u00b7schlies\u00b7sen"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "L\u00e4st geniessen", "tokens": ["L\u00e4st", "ge\u00b7nies\u00b7sen"], "token_info": ["word", "word"], "pos": ["NN", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Ambrosin und Nectar-Safft/", "tokens": ["A\u00b7mbro\u00b7sin", "und", "Nec\u00b7ta\u00b7r\u00b7S\u00b7afft", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$("], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Jhres Schoosses Lieblichkeiten", "tokens": ["Ih\u00b7res", "Schoos\u00b7ses", "Lieb\u00b7lich\u00b7kei\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Tausend Anmuth zu bereiten", "tokens": ["Tau\u00b7send", "An\u00b7muth", "zu", "be\u00b7rei\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["CARD", "NN", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mit vernenter Lebens-Krafft.", "tokens": ["Mit", "ver\u00b7nen\u00b7ter", "Le\u00b7bens\u00b7Krafft", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Doch die Sachen", "tokens": ["Doch", "die", "Sa\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["KON", "ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Mu\u00df ich machen", "tokens": ["Mu\u00df", "ich", "ma\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["VMFIN", "PPER", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Fein geheim und in der Still\u2019/", "tokens": ["Fein", "ge\u00b7heim", "und", "in", "der", "Still'", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "KON", "APPR", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "In der Stille darf ich k\u00fcssen", "tokens": ["In", "der", "Stil\u00b7le", "darf", "ich", "k\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VMFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und der Brust und Schoo\u00df geniessen/", "tokens": ["Und", "der", "Brust", "und", "Schoo\u00df", "ge\u00b7nies\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "KON", "NN", "VVPP", "$("], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.6": {"text": "So offt als ich kan und will.", "tokens": ["So", "offt", "als", "ich", "kan", "und", "will", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KOUS", "PPER", "VMFIN", "KON", "VMFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Wer nun Lieben", "tokens": ["Wer", "nun", "Lie\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["PWS", "ADV", "ADJA"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "So will \u00fcben", "tokens": ["So", "will", "\u00fc\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VMFIN", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Der k\u00fc\u00dft recht erw\u00fcnschte Ruh.", "tokens": ["Der", "k\u00fc\u00dft", "recht", "er\u00b7w\u00fcnschte", "Ruh", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Es wird vor die Liebes-Wunden", "tokens": ["Es", "wird", "vor", "die", "Lie\u00b7bes\u00b7Wun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Die verlangte H\u00fclff\u2019 gefunden", "tokens": ["Die", "ver\u00b7lang\u00b7te", "H\u00fclff'", "ge\u00b7fun\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Geht es nur fein stille zu.", "tokens": ["Geht", "es", "nur", "fein", "stil\u00b7le", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJD", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}