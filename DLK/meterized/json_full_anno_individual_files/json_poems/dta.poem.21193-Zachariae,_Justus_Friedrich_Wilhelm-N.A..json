{"dta.poem.21193": {"metadata": {"author": {"name": "Zachariae, Justus Friedrich Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-20676-2", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Soll der, der selbst die heilge Gegend schaut,", "tokens": ["Soll", "der", ",", "der", "selbst", "die", "heil\u00b7ge", "Ge\u00b7gend", "schaut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "$,", "PRELS", "ADV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Worin der Allmacht Sohn die Blinden sehend machte,", "tokens": ["Wo\u00b7rin", "der", "All\u00b7macht", "Sohn", "die", "Blin\u00b7den", "se\u00b7hend", "mach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Todten aus den Gr\u00e4bern brachte,", "tokens": ["Die", "Tod\u00b7ten", "aus", "den", "Gr\u00e4\u00b7bern", "brach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und endlich f\u00fcr ein s\u00fcndiges Geschlecht", "tokens": ["Und", "end\u00b7lich", "f\u00fcr", "ein", "s\u00fcn\u00b7di\u00b7ges", "Ge\u00b7schlecht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Mit tausend Martern starb;", "tokens": ["Mit", "tau\u00b7send", "Mar\u00b7tern", "starb", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Soll der denn nicht mit Recht", "tokens": ["Soll", "der", "denn", "nicht", "mit", "Recht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "ART", "ADV", "PTKNEG", "APPR", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "In heiliger Entz\u00fcckung sich verlieren?", "tokens": ["In", "hei\u00b7li\u00b7ger", "Ent\u00b7z\u00fc\u00b7ckung", "sich", "ver\u00b7lie\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und sollt ihn nicht des Ortes Anblick r\u00fchren,", "tokens": ["Und", "sollt", "ihn", "nicht", "des", "Or\u00b7tes", "An\u00b7blick", "r\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PTKNEG", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Auf welchem ehemals der gro\u00dfe S\u00fchnaltar", "tokens": ["Auf", "wel\u00b7chem", "e\u00b7he\u00b7mals", "der", "gro\u00b7\u00dfe", "S\u00fch\u00b7nal\u00b7tar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "F\u00fcr uns zum Himmel aufgerichtet war?", "tokens": ["F\u00fcr", "uns", "zum", "Him\u00b7mel", "auf\u00b7ge\u00b7rich\u00b7tet", "war", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPRART", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}