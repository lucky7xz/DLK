{"textgrid.poem.59831": {"metadata": {"author": {"name": "Bodenstedt, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "12.", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Euch mi\u00dff\u00e4llt mein Dichten, weil ich", "tokens": ["Euch", "mi\u00df\u00b7f\u00e4llt", "mein", "Dich\u00b7ten", ",", "weil", "ich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "KOUS", "PPER"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Immer nur das eine singe?", "tokens": ["Im\u00b7mer", "nur", "das", "ei\u00b7ne", "sin\u00b7ge", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nur von Rosen, Lenz und Liebe,", "tokens": ["Nur", "von", "Ro\u00b7sen", ",", "Lenz", "und", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NE", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nachtigall und Weine singe?", "tokens": ["Nach\u00b7ti\u00b7gall", "und", "Wei\u00b7ne", "sin\u00b7ge", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Was ist sch\u00f6ner: da\u00df der S\u00e4nger", "tokens": ["Was", "ist", "sch\u00f6\u00b7ner", ":", "da\u00df", "der", "S\u00e4n\u00b7ger"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "VAFIN", "ADJD", "$.", "KOUS", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Irrlicht, Nacht und Lampe preist \u2013", "tokens": ["Irr\u00b7licht", ",", "Nach