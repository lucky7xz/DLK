{"dta.poem.10954": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Kurzes  \n Gebeth-und Lob-Formular.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ach la\u00df mein Lob, voll froher Ehrfurcht, und mein Ge-\nbeth, von Zweifel rein,", "tokens": ["Ach", "la\u00df", "mein", "Lob", ",", "voll", "fro\u00b7her", "Ehr\u00b7furcht", ",", "und", "mein", "Ge", "be\u00b7th", ",", "von", "Zwei\u00b7fel", "rein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "VVIMP", "PPOSAT", "NN", "$,", "ADJD", "ADJA", "NN", "$,", "KON", "PPOSAT", "NN", "VVFIN", "$,", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-+--+--+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Unendlichs Wesen aller Wesen, aus Gnaden, dir gef\u00e4llig seyn!", "tokens": ["Un\u00b7end\u00b7lichs", "We\u00b7sen", "al\u00b7ler", "We\u00b7sen", ",", "aus", "Gna\u00b7den", ",", "dir", "ge\u00b7f\u00e4l\u00b7lig", "seyn", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PIAT", "NN", "$,", "APPR", "NN", "$,", "PPER", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.3": {"text": "Mein ", "tokens": ["Mein"], "token_info": ["word"], "pos": ["PPOSAT"], "meter": "-", "measure": "single.down"}, "line.4": {"text": "sey Preis und Ehr allein.", "tokens": ["sey", "Preis", "und", "Ehr", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mein zuversichtliches ", "tokens": ["Mein", "zu\u00b7ve