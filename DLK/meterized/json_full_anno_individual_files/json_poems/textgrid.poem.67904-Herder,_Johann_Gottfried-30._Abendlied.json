{"textgrid.poem.67904": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "30. Abendlied", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und wenn sich einst die Seele schlie\u00dft,", "tokens": ["Und", "wenn", "sich", "einst", "die", "See\u00b7le", "schlie\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PRF", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie diese Abendblume:", "tokens": ["Wie", "die\u00b7se", "A\u00b7bend\u00b7blu\u00b7me", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "PDAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wenn alles um sie D\u00e4mmrung ist", "tokens": ["Wenn", "al\u00b7les", "um", "sie", "D\u00e4mm\u00b7rung", "ist"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "APPR", "PPER", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von Lebens Licht und Ruhme:", "tokens": ["Von", "Le\u00b7bens", "Licht", "und", "Ruh\u00b7me", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und ihre letzten Blick' umher", "tokens": ["Und", "ih\u00b7re", "letz\u00b7ten", "Blick'", "um\u00b7her"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ihr kalte Schatten scheinen;", "tokens": ["Ihr", "kal\u00b7te", "Schat\u00b7ten", "schei\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "O J\u00fcngling, wirst du auch so schwer", "tokens": ["O", "J\u00fcng\u00b7ling", ",", "wirst", "du", "auch", "so", "schwer"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "$,", "VAFIN", "PPER", "ADV", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wie diese Blume weinen.", "tokens": ["Wie", "die\u00b7se", "Blu\u00b7me", "wei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "War deiner holden Jugend Saft", "tokens": ["War", "dei\u00b7ner", "hol\u00b7den", "Ju\u00b7gend", "Saft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In \u00f6de Luft verhauchet,", "tokens": ["In", "\u00f6\u00b7de", "Luft", "ver\u00b7hau\u00b7chet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Verbl\u00fcht die Bl\u00fcthe, Lebenskraft", "tokens": ["Ver\u00b7bl\u00fcht", "die", "Bl\u00fc\u00b7the", ",", "Le\u00b7bens\u00b7kraft"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["VVFIN", "ART", "NN", "$,", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auf immer misgebrauchet;", "tokens": ["Auf", "im\u00b7mer", "mis\u00b7ge\u00b7brau\u00b7chet", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und deine letzten Blick' umher", "tokens": ["Und", "dei\u00b7ne", "letz\u00b7ten", "Blick'", "um\u00b7her"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dich alle Reu-entf\u00e4rben;", "tokens": ["Dich", "al\u00b7le", "Reu\u00b7ent\u00b7f\u00e4r\u00b7ben", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "O J\u00fcngling, bleibt dir etwas mehr,", "tokens": ["O", "J\u00fcng\u00b7ling", ",", "bleibt", "dir", "et\u00b7was", "mehr", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VVFIN", "PPER", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Als Trost-verschmachtet sterben.", "tokens": ["Als", "Trost\u00b7ver\u00b7schmach\u00b7tet", "ster\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Macht Seine grosse Allmacht je", "tokens": ["Macht", "Sei\u00b7ne", "gros\u00b7se", "All\u00b7macht", "je"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Geschehnes ungeschehen?", "tokens": ["Ge\u00b7scheh\u00b7nes", "un\u00b7ge\u00b7sche\u00b7hen", "?"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und stillt sie auch das tiefe Weh,", "tokens": ["Und", "stillt", "sie", "auch", "das", "tie\u00b7fe", "Weh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sich selbst besch\u00e4mt zu sehen?", "tokens": ["Sich", "selbst", "be\u00b7sch\u00e4mt", "zu", "se\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und w\u00e4chst und w\u00e4chst nicht jeder That", "tokens": ["Und", "w\u00e4chst", "und", "w\u00e4chst", "nicht", "je\u00b7der", "That"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "PTKNEG", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Keim so tief verborgen?", "tokens": ["Der", "Keim", "so", "tief", "ver\u00b7bor\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Wer gibt, wer schafft mir neuen Rath,", "tokens": ["Wer", "gibt", ",", "wer", "schafft", "mir", "neu\u00b7en", "Rath", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "PWS", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Noch Einen Jugendmorgen?", "tokens": ["Noch", "Ei\u00b7nen", "Ju\u00b7gend\u00b7mor\u00b7gen", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und holder Schlaf, den schaffest du,", "tokens": ["Und", "hol\u00b7der", "Schlaf", ",", "den", "schaf\u00b7fest", "du", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,", "ART", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Giebst neuen Jugendmorgen.", "tokens": ["Giebst", "neu\u00b7en", "Ju\u00b7gend\u00b7mor\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Bist Labetrunk und Schattenruh,", "tokens": ["Bist", "La\u00b7be\u00b7trunk", "und", "Schat\u00b7ten\u00b7ruh", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bist Labsal aller Sorgen,", "tokens": ["Bist", "Lab\u00b7sal", "al\u00b7ler", "Sor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Bist Todesbruder! o wie sch\u00f6n", "tokens": ["Bist", "To\u00b7des\u00b7bru\u00b7der", "!", "o", "wie", "sch\u00f6n"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "NN", "$.", "FM", "KOKOM", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sich Seyn und Nichtseyn grenzen:", "tokens": ["Sich", "Seyn", "und", "Nich\u00b7tseyn", "gren\u00b7zen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PPOSAT", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Wie frisch wird meine Abendthr\u00e4n'", "tokens": ["Wie", "frisch", "wird", "mei\u00b7ne", "A\u00b7bend\u00b7thr\u00e4n'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Am fr\u00fchen Morgen gl\u00e4nzen.", "tokens": ["Am", "fr\u00fc\u00b7hen", "Mor\u00b7gen", "gl\u00e4n\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und nach dem Tod \u2013 es wird uns seyn,", "tokens": ["Und", "nach", "dem", "Tod", "\u2013", "es", "wird", "uns", "seyn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$(", "PPER", "VAFIN", "PPER", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als nach des Rausches Schlummer.", "tokens": ["Als", "nach", "des", "Rau\u00b7sches", "Schlum\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Verrauscht, verschlummert Lebenspein", "tokens": ["Ver\u00b7rauscht", ",", "ver\u00b7schlum\u00b7mert", "Le\u00b7ben\u00b7spein"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "VVFIN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und Schmerz und Reu und Kummer.", "tokens": ["Und", "Schmerz", "und", "Reu", "und", "Kum\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NE", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "O Tod, o Schlaf der dich erfand,", "tokens": ["O", "Tod", ",", "o", "Schlaf", "der", "dich", "er\u00b7fand", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "FM", "NN", "ART", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Erfand der Menschheit Segen,", "tokens": ["Er\u00b7fand", "der", "Menschheit", "Se\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Breit aus auf mich dein Schlafgewand,", "tokens": ["Breit", "aus", "auf", "mich", "dein", "Schlaf\u00b7ge\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "APPR", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Zur Ruhe mich zu legen.", "tokens": ["Zur", "Ru\u00b7he", "mich", "zu", "le\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Denn was w\u00e4r unsre Lebenszeit,", "tokens": ["Denn", "was", "w\u00e4r", "uns\u00b7re", "Le\u00b7bens\u00b7zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auch unsre Zeit der Freuden?", "tokens": ["Auch", "uns\u00b7re", "Zeit", "der", "Freu\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein Strudel von M\u00fchseligkeit,", "tokens": ["Ein", "Stru\u00b7del", "von", "M\u00fch\u00b7se\u00b7lig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Ein Wirbel s\u00fcsser Leiden,", "tokens": ["Ein", "Wir\u00b7bel", "s\u00fcs\u00b7ser", "Lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ein ew'ger Taumel! Holder Schlaf,", "tokens": ["Ein", "ew'\u00b7ger", "Tau\u00b7mel", "!", "Hol\u00b7der", "Schlaf", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Zu neuem Freudenmahle", "tokens": ["Zu", "neu\u00b7em", "Freu\u00b7den\u00b7mah\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "F\u00fcr alles, was auch heut mich traf,", "tok