{"textgrid.poem.48166": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "Tote Blumen", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In des Hofes Sterbewinkel,", "tokens": ["In", "des", "Ho\u00b7fes", "Ster\u00b7be\u00b7win\u00b7kel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "schr\u00e4g vom Dachfirst \u00fcberhangen,", "tokens": ["schr\u00e4g", "vom", "Dach\u00b7first", "\u00fc\u00b7berh\u00b7an\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "liegt auf Scherben Schutt und Kehricht", "tokens": ["liegt", "auf", "Scher\u00b7ben", "Schutt", "und", "Keh\u00b7richt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "NN", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "\u2013 tief das Haupt in Staub gepre\u00dft \u2013", "tokens": ["\u2013", "tief", "das", "Haupt", "in", "Staub", "ge\u00b7pre\u00dft", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "ART", "NN", "APPR", "NN", "VVPP", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "eine tote Sonnenblume.", "tokens": ["ei\u00b7ne", "to\u00b7te", "Son\u00b7nen\u00b7blu\u00b7me", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Aus dem Dachrohr unerm\u00fcdlich", "tokens": ["Aus", "dem", "Da\u00b7chrohr", "un\u00b7er\u00b7m\u00fcd\u00b7lich"], "token_info": ["word", "word