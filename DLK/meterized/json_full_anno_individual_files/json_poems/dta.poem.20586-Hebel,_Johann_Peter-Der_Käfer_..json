{"dta.poem.20586": {"metadata": {"author": {"name": "Hebel, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "Der K\u00e4fer .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1803", "urn": "urn:nbn:de:kobv:b4-200905192133", "language": ["de:0.99"], "booktitle": "[Hebel, Johann Peter]: Allemannische Gedichte. Karlsruhe, 1803."}, "poem": {"stanza.1": {"line.1": {"text": "Der Ch\u00e4fer fliegt der Jilge zu,               ", "tokens": ["Der", "Ch\u00e4\u00b7fer", "fliegt", "der", "Jil\u00b7ge", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "es sizt e sch\u00f6nen Engel d\u00f6rt;", "tokens": ["es", "sizt", "e", "sch\u00f6\u00b7nen", "En\u00b7gel", "d\u00f6rt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "er wirthet gwis mit Blumesaft,", "tokens": ["er", "wirt\u00b7het", "gwis", "mit", "Blu\u00b7me\u00b7saft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "und \u2019s chostet nit viel, hani gh\u00f6rt.", "tokens": ["und", "'s", "chos\u00b7tet", "nit", "viel", ",", "ha\u00b7ni", "gh\u00f6rt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PTKNEG", "ADV", "$,", "NE", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Der Engel seit: \u201eWas w\u00e4r der lieb?\u201c", "tokens": ["Der", "En\u00b7gel", "seit", ":", "\u201e", "Was", "w\u00e4r", "der", "lieb", "?", "\u201c"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "APPR", "$.", "$(", "PWS", "VAFIN", "ART", "ADJD", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u201ene Sch\u00f6pli Alte h\u00e4tti gern!\u201c", "tokens": ["\u201e", "ne", "Sch\u00f6p\u00b7li", "Al\u00b7te", "h\u00e4t\u00b7ti", "gern", "!", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "NE", "NE", "NN", "VAFIN", "ADV", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Engel seit: \u201eSel cha nit sy,", "tokens": ["Der", "En\u00b7gel", "seit", ":", "\u201e", "Sel", "cha", "nit", "sy", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "$.", "$(", "NE", "NE", "PTKNEG", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "sie hen en alle trunke fern.\u201c \u2014", "tokens": ["sie", "hen", "en", "al\u00b7le", "trun\u00b7ke", "fern", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PPER", "VVINF", "VVFIN", "PIS", "VVFIN", "PTKVZ", "$.", "$(", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "\u201ese schenk e Sch\u00f6pli Neuen i!\u201c \u2014", "tokens": ["\u201e", "se", "schenk", "e", "Sch\u00f6p\u00b7li", "Neu\u00b7en", "i", "!", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "FM", "FM", "FM", "FM", "FM", "FM", "$.", "$(", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u201edo hesch eis!\u201c het der Engel gseit.", "tokens": ["\u201e", "do", "hesch", "eis", "!", "\u201c", "het", "der", "En\u00b7gel", "gseit", "."], "token_info": ["punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADJD", "PTKVZ", "$.", "$(", "VVFIN", "ART", "NN", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Der Ch\u00e4fer trinkt, und \u2019s schmekt em wohl;", "tokens": ["Der", "Ch\u00e4\u00b7fer", "trinkt", ",", "und", "'s", "schmekt", "em", "wohl", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "KON", "PPER", "VVFIN", "PIS", "ADV", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "er frogt: \u201eWas isch mi Schuldigkeit?\u201c", "tokens": ["er", "frogt", ":", "\u201e", "Was", "isch", "mi", "Schul\u00b7dig\u00b7keit", "?", "\u201c"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "$.", "$(", "PWS", "FM", "FM", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Der Engel seit: \u201eHe, \u2019s chostet n\u00fct!", "tokens": ["Der", "En\u00b7gel", "seit", ":", "\u201e", "He", ",", "'s", "chos\u00b7tet", "n\u00fct", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "$.", "$(", "ITJ", "$,", "PPER", "VVFIN", "ADJD", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "\u201edoch richtsch mer gern e Gfallen us,", "tokens": ["\u201e", "doch", "richt\u00b7sch", "mer", "gern", "e", "Gfal\u00b7len", "us", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADJD", "ADV", "ADV", "NE", "NE", "NE", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "\u201eweisch was, se nimm das Blumemehl,", "tokens": ["\u201e", "weisch", "was", ",", "se", "nimm", "das", "Blu\u00b7me\u00b7mehl", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "PWS", "$,", "ADV", "VVIMP", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u201eund tragmers gschwind ins Nochbers Hus!\u201c", "tokens": ["\u201e", "und", "trag\u00b7mers", "gschwind", "ins", "Noch\u00b7bers", "Hus", "!", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "KON", "ADJA", "NN", "APPRART", "NN", "NE", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "\u201eer het zwor selber, was er brucht,", "tokens": ["\u201e", "er", "het", "zwor", "sel\u00b7ber", ",", "was", "er", "brucht", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "ADV", "ADV", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u201edoch freuts en, und er schickt mer au,", "tokens": ["\u201e", "doch", "freuts", "en", ",", "und", "er", "schickt", "mer", "au", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "FM", "FM", "$,", "KON", "PPER", "VVFIN", "ADV", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u201emengmol e H\u00e4mpfeli Blumemehl,", "tokens": ["\u201e", "meng\u00b7mol", "e", "H\u00e4mp\u00b7fe\u00b7li", "Blu\u00b7me\u00b7mehl", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "FM", "FM", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "\u201emengmol e Tr\u00f6pfli Morgethau.\u201c", "tokens": ["\u201e", "meng\u00b7mol", "e", "Tr\u00f6\u00b7pf\u00b7li", "Mor\u00b7ge\u00b7thau", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "FM", "FM", "FM", "FM", "$.", "$("], "meter": "+---+-+-+", "measure": "dactylic.init"}}, "stanza.6": {"line.1": {"text": "Der Ch\u00e4fer seit: \u201eJo frili, io!\u201c", "tokens": ["Der", "Ch\u00e4\u00b7fer", "seit", ":", "\u201e", "Jo", "fri\u00b7li", ",", "i\u00b7o", "!", "\u201c"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "punct", "punct"], "pos": ["ART", "NN", "APPR", "$.", "$(", "NE", "NE", "$,", "NE", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "\u201evergelts Gott, wenn de z\u2019friede bisch!\u201c", "tokens": ["\u201e", "ver\u00b7gelts", "Gott", ",", "wenn", "de", "z'\u00b7frie\u00b7de", "bisch", "!", "\u201c"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "NN", "$,", "KOUS", "NE", "VVFIN", "ADJD", "$.", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Druf treit er \u2019s Mehl ins Nochbers Hus,", "tokens": ["Druf", "treit", "er", "'s", "Mehl", "ins", "Noch\u00b7bers", "Hus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PPER", "NN", "APPRART", "NN", "NE", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "wo wieder so en Engel isch.", "tokens": ["wo", "wie\u00b7der", "so", "en", "En\u00b7gel", "isch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADV", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Er seit: \u201eJ chumm vom Nochber her,", "tokens": ["Er", "seit", ":", "\u201e", "J", "chumm", "vom", "Noch\u00b7ber", "her", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "$.", "$(", "NE", "NE", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u201egott gr\u00fce\u00df di, und er schick der do", "tokens": ["\u201e", "gott