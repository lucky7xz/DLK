{"dta.poem.18214": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "5.  \n  Nachtst\u00fcck.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1886", "urn": "urn:nbn:de:kobv:b4-200905192665", "language": ["de:0.99"], "booktitle": "Holz, Arno: Das Buch der Zeit. Lieder eines Modernen. Z\u00fcrich, 1886."}, "poem": {"stanza.1": {"line.1": {"text": "L&#228;ngst fiel von den B&#228;umen", "tokens": ["L", "&#228;", "ngst", "fiel", "von", "den", "B", "&#228;", "u\u00b7men"], "token_info": ["word", "XML_entity", "word", "word", "word", "word", "word", "XML_entity", "word"], "pos": ["NE", "$(", "ADV", "VVFIN", "APPR", "ART", "XY", "$(", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Das letzte Blatt,", "tokens": ["Das", "letz\u00b7te", "Blatt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "In Schlaf und Tr\u00e4umen", "tokens": ["In", "Schlaf", "und", "Tr\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Liegt nun die Stadt;", "tokens": ["Liegt", "nun", "die", "Stadt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Die Fenster verdunkeln", "tokens": ["Die", "Fens\u00b7ter", "ver\u00b7dun\u00b7keln"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Sich Haus an Haus", "tokens": ["Sich", "Haus", "an", "Haus"], "token_info": ["word", "word", "word", "word"], "pos": ["PRF", "NN", "APPR", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Und dr\u00fcberhin funkeln", "tokens": ["Und", "dr\u00fc\u00b7berh\u00b7in", "fun\u00b7keln"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADV", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Die Sterne sich aus;", "tokens": ["Die", "Ster\u00b7ne", "sich", "aus", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.9": {"text": "Kalt weht es vom Strom her,", "tokens": ["Kalt", "weht", "es", "vom", "Strom", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Der Eisgang kracht,", "tokens": ["Der", "Eis\u00b7gang", "kracht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "Und dr\u00fcben vom Dom her", "tokens": ["Und", "dr\u00fc\u00b7ben", "vom", "Dom", "her"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPRART", "NN", "APZR"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.12": {"text": "Dr\u00f6hnt's Mitternacht.", "tokens": ["Dr\u00f6hnt's", "Mit\u00b7ter\u00b7nacht", "."], "token_info": ["word", "word", "punct"], "pos": ["NE", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.13": {"text": "Ich aber schleppe mich zitternd nach Haus \u2014", "tokens": ["Ich", "a\u00b7ber", "schlep\u00b7pe", "mich", "zit\u00b7ternd", "nach", "Haus"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "VVPP", "APPR", "NN", "$("], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "Der Nordwind bl\u00e4st die Laternen aus!", "tokens": ["Der", "Nord\u00b7wind", "bl\u00e4st", "die", "La\u00b7ter\u00b7nen", "aus", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+----+", "measure": "unknown.measure.tri"}}, "stanza.2": {"line.1": {"text": "Was half's, da\u00df ich klagend", "tokens": ["Was", "ha\u00b7lf's", ",", "da\u00df", "ich", "kla\u00b7gend"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "VVFIN", "$,", "KOUS", "PPER", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Gassen durchlief", "tokens": ["Die", "Gas\u00b7sen", "durch\u00b7lief"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Und mitleidverzagend", "tokens": ["Und", "mit\u00b7leid\u00b7ver\u00b7za\u00b7gend"], "token_info": ["word", "word"], "pos": ["KON", "VVPP"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "\u201ehier Rosen!\u201c ausrief?", "tokens": ["\u201e", "hier", "Ro\u00b7sen", "!", "\u201c", "aus\u00b7rief", "?"], "token_info": ["punct", "word", "word", "punct", "punct", "word", "punct"], "pos": ["$(", "ADV", "NN", "$.", "$(", "VVFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "\u201ehier Rosen, o Rosen!", "tokens": ["\u201e", "hier", "Ro\u00b7sen", ",", "o", "Ro\u00b7sen", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "ADV", "NN", "$,", "FM", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Wer kauft einen Strau\u00df?\u201c", "tokens": ["Wer", "kauft", "ei\u00b7nen", "Strau\u00df", "?", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "VVFIN", "ART", "NE", "$.", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Doch die Herren Studiosen", "tokens": ["Doch", "die", "Her\u00b7ren", "Stu\u00b7di\u00b7o\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Lachten mich aus!", "tokens": ["Lach\u00b7ten", "mich", "aus", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.9": {"text": "Und keiner, keiner ....", "tokens": ["Und", "kei\u00b7ner", ",", "kei\u00b7ner", "...."], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["KON", "PIS", "$,", "PIS", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.10": {"text": "Da\u00df Gott erbarm!", "tokens": ["Da\u00df", "Gott", "er\u00b7barm", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "O unsereiner", "tokens": ["O", "un\u00b7ser\u00b7ei\u00b7ner"], "token_info": ["word", "word"], "pos": ["NE", "PPOSAT"], "meter": "-+-+-", "measure": "iambic.di"}, "line.12": {"text": "Ist gar zu arm!", "tokens": ["Ist", "gar", "zu", "arm", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PTKA", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.13": {"text": "Mir wanken die Kniee, mein Herzblut gerinnt \u2014", "tokens": ["Mir", "wan\u00b7ken", "die", "Kni\u00b7ee", ",", "mein", "Herz\u00b7blut", "ge\u00b7rinnt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+--+-+++-+", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "O Gott, mein Kind, mein armes Kind!", "tokens": ["O", "Gott", ",", "mein", "Kind", ",", "mein", "ar\u00b7mes", "Kind", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "In stockdunkler Kammer,", "tokens": ["In", "stock\u00b7dunk\u00b7ler", "Kam\u00b7mer", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Verhungert, verthiert!", "tokens": ["Ver\u00b7hun\u00b7gert", ",", "ver\u00b7thiert", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["VVPP", "$,", "VVPP", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Schon packt mich der Jammer:", "tokens": ["Schon", "packt", "mich", "der", "Jam\u00b7mer", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "\u201each Muttchen, mich friert!", "tokens": ["\u201e", "ach", "Mutt\u00b7chen", ",", "mich", "friert", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "XY", "NN", "$,", "PPER", "VVFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Ach bitte, bitte", "tokens": ["Ach", "bit\u00b7te", ",", "bit\u00b7te"], "token_info": ["word", "word", "punct", "word"], "pos": ["ITJ", "ADV", "$,", "PTKANT"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Ein St\u00fcckchen Brot!\u201c", "tokens": ["Ein", "St\u00fcck\u00b7chen", "Brot", "!", "\u201c"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "NN", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Mir ist es, als litte", "tokens": ["Mir", "ist", "es", ",", "als", "lit\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "$,", "KOUS", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Ich gleich den Tod!", "tokens": ["Ich", "gleich", "den", "Tod", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Mir ist es, als m\u00fc\u00dfte", "tokens": ["Mir", "ist", "es", ",", "als", "m\u00fc\u00df\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "$,", "KOUS", "VMFIN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Ich schreien: \u201eFluch!\u201c \u2014", "tokens": ["Ich", "schrei\u00b7en", ":", "\u201e", "Fluch", "!", "\u201c"], "token_info": ["word", "word", "punct", "punct", "word", "punct", "punct", "punct"], "pos": ["PPER", "VVFIN", "$.", "$(", "NN", "$.", "$(", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "O da\u00df ich dich k\u00fc\u00dfte", "tokens": ["O", "da\u00df", "ich", "dich", "k\u00fc\u00df\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "KOUS", "PPER", "PRF", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.12": {"text": "Durchs Leichentuch!", "tokens": ["Durchs", "Lei\u00b7chen\u00b7tuch", "!"], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.13": {"text": "Dann w\u00e4r es vorbei und sie scharrten dich ein", "tokens": ["Dann", "w\u00e4r", "es", "vor\u00b7bei", "und", "sie", "scharr\u00b7ten", "dich", "ein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PTKVZ", "KON", "PPER", "VVFIN", "PRF", "ART"], "meter": "-+-----+--+", "measure": "iambic.tri.chol"}, "line.14": {"text": "Und ich tr\u00fcg es allein, o Gott, allein ....!", "tokens": ["Und", "ich", "tr\u00fcg", "es", "al\u00b7lein", ",", "o", "Gott", ",", "al\u00b7lein", "....", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADV", "$,", "FM", "NN", "$,", "ADV", "$.", "$."], "meter": "-++--+-+-+", "measure": "iambic.penta.relaxed"}}}}}