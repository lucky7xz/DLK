{"dta.poem.5586": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Weltweisen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es wollten ehedem Empedoeles, die Ehr'", "tokens": ["Es", "woll\u00b7ten", "e\u00b7he\u00b7dem", "Em\u00b7pe\u00b7do\u00b7e\u00b7les", ",", "die", "Ehr'"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "NE", "$,", "ART", "NN"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.2": {"text": "Siciliens, und Noms Lueretius noch mehr", "tokens": ["Si\u00b7ci\u00b7li\u00b7ens", ",", "und", "Noms", "Lu\u00b7e\u00b7re\u00b7ti\u00b7us", "noch", "mehr"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "KON", "NE", "NE", "ADV", "ADV"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Die Welt-Weisheit zum Zweck von ihren Liedern wehlen,", "tokens": ["Die", "Welt\u00b7Weis\u00b7heit", "zum", "Zweck", "von", "ih\u00b7ren", "Lie\u00b7dern", "weh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-++--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und ihr gelehrter Thon bezauberte die Seelen.", "tokens": ["Und", "ihr", "ge\u00b7lehr\u00b7ter", "Thon", "be\u00b7zau\u00b7ber\u00b7te", "die", "See\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die Griechen ebenfalls, die ja so tugendhafft", "tokens": ["Die", "Grie\u00b7chen", "e\u00b7ben\u00b7falls", ",", "die", "ja", "so", "tu\u00b7gend\u00b7hafft"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "$,", "PRELS", "ADV", "ADV", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Als weise, liessen sich von gleicher Gluht entz\u00fcnden,", "tokens": ["Als", "wei\u00b7se", ",", "lies\u00b7sen", "sich", "von", "glei\u00b7cher", "Gluht", "ent\u00b7z\u00fcn\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "$,", "VVFIN", "PRF", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und, um die Hertzen recht zu r\u00fchren, zu verbinden,", "tokens": ["Und", ",", "um", "die", "Hert\u00b7zen", "recht", "zu", "r\u00fch\u00b7ren", ",", "zu", "ver\u00b7bin\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "$,", "KOUI", "ART", "NN", "ADJD", "PTKZU", "VVINF", "$,", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Gebraucht\u2019 ihr tieffer Wiz der Dicht-Kunst Reitzungs-Krafft.", "tokens": ["Ge\u00b7braucht'", "ihr", "tief\u00b7fer", "Wiz", "der", "Dicht\u00b7Kunst", "Reit\u00b7zungs\u00b7Krafft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}