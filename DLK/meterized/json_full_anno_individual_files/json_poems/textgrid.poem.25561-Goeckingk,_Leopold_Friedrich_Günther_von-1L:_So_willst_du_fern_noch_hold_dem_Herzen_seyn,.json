ie", "Wand", "in", "dei\u00b7nem", "Zim\u00b7mer", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Wo ich im Geist' vor ", "tokens": ["Wo", "ich", "im", "Geist'", "vor"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPRART", "NN", "APPR"], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "Wo ich mit Stolz dir manchen gro\u00dfen Mann", "tokens": ["Wo", "ich", "mit", "Stolz", "dir", "man\u00b7chen", "gro\u00b7\u00dfen", "Mann"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPR", "NN", "PPER", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Als einen Freund von deinem Freunde nannte;", "tokens": ["Als", "ei\u00b7nen", "Freund", "von", "dei\u00b7nem", "Freun\u00b7de", "nann\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Wo ich die Welt in ihrem Ruhm' verga\u00df,", "tokens": ["Wo", "ich", "die", "Welt", "in", "ih\u00b7rem", "Ruhm'", "ver\u00b7ga\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Ja selbst den Neid, weil ich dein Herz besa\u00df!", "tokens": ["Ja", "selbst", "den", "Neid", ",", "weil", "ich", "dein", "Herz", "be\u00b7sa\u00df", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "ART", "NN", "$,", "KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Vergi\u00df es nicht, wenn k\u00fcnftig dich zum Schach", "tokens": ["Ver\u00b7gi\u00df", "es", "nicht", ",", "wenn", "k\u00fcnf\u00b7tig", "dich", "zum", "Schach"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "PTKNEG", "$,", "KOUS", "ADJD", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Die Langeweil' anstatt der Liebe f\u00fchret,", "tokens": ["Die", "Lan\u00b7ge\u00b7weil'", "an\u00b7statt", "der", "Lie\u00b7be", "f\u00fch\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Wie du zerstreut mit einem leisen Ach!", "tokens": ["Wie", "du", "zer\u00b7streut", "mit", "ei\u00b7nem", "lei\u00b7sen", "Ach", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Statt eines Steins, oft meine Hand ber\u00fchret;", "tokens": ["Statt", "ei\u00b7nes", "Steins", ",", "oft", "mei\u00b7ne", "Hand", "be\u00b7r\u00fch\u00b7ret", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Wie dann, gestreift von deinem Kleide nur,", "tokens": ["Wie", "dann", ",", "ge\u00b7streift", "von", "dei\u00b7nem", "Klei\u00b7de", "nur", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "$,", "VVPP", "APPR", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Ich