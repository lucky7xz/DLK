{"textgrid.poem.25561": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: So willst du fern noch hold dem Herzen seyn,", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So willst du fern noch hold dem Herzen seyn,", "tokens": ["So", "willst", "du", "fern", "noch", "hold", "dem", "Her\u00b7zen", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "ADV", "ADJD", "ART", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Das ehemals dein ganzes Herz besessen?", "tokens": ["Das", "e\u00b7he\u00b7mals", "dein", "gan\u00b7zes", "Herz", "be\u00b7ses\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie viel, wie viel, wird k\u00fcnftig dich zerstreun!", "tokens": ["Wie", "viel", ",", "wie", "viel", ",", "wird", "k\u00fcnf\u00b7tig", "dich", "zer\u00b7streun", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "$,", "PWAV", "PIS", "$,", "VAFIN", "ADJD", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und o! wie leicht ist da dein Freund vergessen!", "tokens": ["Und", "o", "!", "wie", "leicht", "ist", "da", "dein", "Freund", "ver\u00b7ges\u00b7sen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "FM", "$.", "PWAV", "ADJD", "VAFIN", "ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Schwer mach' ich's dir, Geliebte! aber ach!", "tokens": ["Schwer", "mach'", "ich's", "dir", ",", "Ge\u00b7lieb\u00b7te", "!", "a\u00b7ber", "ach", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PIS", "PPER", "$,", "NN", "$.", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Sey stark aus dir! Der Lieb' ist Kunst zu schwach!", "tokens": ["Sey", "stark", "aus", "dir", "!", "Der", "Lieb'", "ist", "Kunst", "zu", "schwach", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "APPR", "PPER", "$.", "ART", "NN", "VAFIN", "NN", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Sieh nur die Wand in deinem Zimmer an,", "tokens": ["Sieh", "nur", "die", "Wand", "in", "dei\u00b7nem", "Zim\u00b7mer", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Wo ich im Geist' vor ", "tokens": ["Wo", "ich", "im", "Geist'", "vor"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPRART", "NN", "APPR"], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "Wo ich mit Stolz dir manchen gro\u00dfen Mann", "tokens": ["Wo", "ich", "mit", "Stolz", "dir", "man\u00b7chen", "gro\u00b7\u00dfen", "Mann"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPR", "NN", "PPER", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Als einen Freund von deinem Freunde nannte;", "tokens": ["Als", "ei\u00b7nen", "Freund", "von", "dei\u00b7nem", "Freun\u00b7de", "nann\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Wo ich die Welt in ihrem Ruhm' verga\u00df,", "tokens": ["Wo", "ich", "die", "Welt", "in", "ih\u00b7rem", "Ruhm'", "ver\u00b7ga\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Ja selbst den Neid, weil ich dein Herz besa\u00df!", "tokens": ["Ja", "selbst", "den", "Neid", ",", "weil", "ich", "dein", "Herz", "be\u00b7sa\u00df", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "ART", "NN", "$,", "KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Vergi\u00df es nicht, wenn k\u00fcnftig dich zum Schach", "tokens": ["Ver\u00b7gi\u00df", "es", "nicht", ",", "wenn", "k\u00fcnf\u00b7tig", "dich", "zum", "Schach"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "PTKNEG", "$,", "KOUS", "ADJD", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Die Langeweil' anstatt der Liebe f\u00fchret,", "tokens": ["Die", "Lan\u00b7ge\u00b7weil'", "an\u00b7statt", "der", "Lie\u00b7be", "f\u00fch\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Wie du zerstreut mit einem leisen Ach!", "tokens": ["Wie", "du", "zer\u00b7streut", "mit", "ei\u00b7nem", "lei\u00b7sen", "Ach", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Statt eines Steins, oft meine Hand ber\u00fchret;", "tokens": ["Statt", "ei\u00b7nes", "Steins", ",", "oft", "mei\u00b7ne", "Hand", "be\u00b7r\u00fch\u00b7ret", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Wie dann, gestreift von deinem Kleide nur,", "tokens": ["Wie", "dann", ",", "ge\u00b7streift", "von", "dei\u00b7nem", "Klei\u00b7de", "nur", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "$,", "VVPP", "APPR", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Ich wei\u00df nicht wie? mein Herz zusammen fuhr!", "tokens": ["Ich", "wei\u00df", "nicht", "wie", "?", "mein", "Herz", "zu\u00b7sam\u00b7men", "fuhr", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "PWAV", "$.", "PPOSAT", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "So oft ein Glas Burgunder vor dir spielt,", "tokens": ["So", "oft", "ein", "Glas", "Bur\u00b7gun\u00b7der", "vor", "dir", "spielt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Erinnre dich, welch Feuer aus dir spr\u00fchte,", "tokens": ["E\u00b7rinn\u00b7re", "dich", ",", "welch", "Feu\u00b7er", "aus", "dir", "spr\u00fch\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWAT", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Wenn deine Hand es schwankend kaum noch hielt,", "tokens": ["Wenn", "dei\u00b7ne", "Hand", "es", "schwan\u00b7kend", "kaum", "noch", "hielt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "ADJD", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Welch Morgenroth auf deinen Wangen gl\u00fchte,", "tokens": ["Welch", "Mor\u00b7gen\u00b7roth", "auf", "dei\u00b7nen", "Wan\u00b7gen", "gl\u00fch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Wie sch\u00fcchtern du, wie stammlend mich genannt,", "tokens": ["Wie", "sch\u00fcch\u00b7tern", "du", ",", "wie", "stamm\u00b7lend", "mich", "ge\u00b7nannt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "$,", "PWAV", "ADJD", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Als du dich sonst damit zu mir gewandt.", "tokens": ["Als", "du", "dich", "sonst", "da\u00b7mit", "zu", "mir", "ge\u00b7wandt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "PAV", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "So oft der May im Veilchenkranze lacht,", "tokens": ["So", "oft", "der", "May", "im", "Veil\u00b7chen\u00b7kran\u00b7ze", "lacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "Erinnre dich an jene B\u00e4ch' und B\u00fcsche,", "tokens": ["E\u00b7rinn\u00b7re", "dich", "an", "je\u00b7ne", "B\u00e4ch'", "und", "B\u00fc\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PDAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Wo du die Hand zum Becher oft gemacht,", "tokens": ["Wo", "du", "die", "Hand", "zum", "Be\u00b7cher", "oft", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "APPRART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "Und deinen Schoo\u00df, f\u00fcr unser Mahl, zum Tische.", "tokens": ["Und", "dei\u00b7nen", "Schoo\u00df", ",", "f\u00fcr", "un\u00b7ser", "Mahl", ",", "zum", "Ti\u00b7sche", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "APPR", "PPOSAT", "NN", "$,", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "Wie oft fiel da der Wunsch dir weinend ein?", "tokens": ["Wie", "oft", "fiel", "da", "der", "Wunsch", "dir", "wei\u00b7nend", "ein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "ADV", "ART", "NN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.30": {"text": "Ach! m\u00f6chten wir nur arme Hirten seyn!", "tokens": ["Ach", "!", "m\u00f6ch\u00b7ten", "wir", "nur", "ar\u00b7me", "Hir\u00b7ten", "seyn", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VMFIN", "PPER", "ADV", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.31": {"text": "Wann wieder Schnee in Pflocken um dich schw\u00e4rmt,", "tokens": ["Wann", "wie\u00b7der", "Schnee", "in", "Pflo\u00b7cken", "um", "dich", "schw\u00e4rmt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "NN", "APPR", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.32": {"text": "So denke noch, wie du mir oft im Schlitten", "tokens": ["So", "den\u00b7ke", "noch", ",", "wie", "du", "mir", "oft", "im", "Schlit\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "$,", "PWAV", "PPER", "PPER", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.33": {"text": "Die starre Hand an deiner Brust gew\u00e4rmt,", "tokens": ["Die", "star\u00b7re", "Hand", "an", "dei\u00b7ner", "Brust", "ge\u00b7w\u00e4rmt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.34": {"text": "Und sacht gefragt: ob meine Lippen litten?", "tokens": ["Und", "sacht", "ge\u00b7fragt", ":", "ob", "mei\u00b7ne", "Lip\u00b7pen", "lit\u00b7ten", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVPP", "$.", "KOUS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.35": {"text": "Doch wie verbarg, wann ihn der Nord bestrich,", "tokens": ["Doch", "wie", "ver\u00b7barg", ",", "wann", "ihn", "der", "Nord", "be\u00b7strich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ADJD", "$,", "PWAV", "PPER", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.36": {"text": "Mein Mund geschwind in deinem Nacken sich!", "tokens": ["Mein", "Mund", "ge\u00b7schwind", "in", "dei\u00b7nem", "Na\u00b7cken", "sich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "APPR", "PPOSAT", "NN", "PRF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.37": {"text": "Erinnre dich bei deinem N\u00e4hepult',", "tokens": ["E\u00b7rinn\u00b7re", "dich", "bei", "dei\u00b7nem", "N\u00e4\u00b7he\u00b7pult'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.38": {"text": "Wie du f\u00fcr mich noch kleine Netze stricktest,", "tokens": ["Wie", "du", "f\u00fcr", "mich", "noch", "klei\u00b7ne", "Net\u00b7ze", "strick\u00b7test", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "PPER", "ADV", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.39": {"text": "Mit gro\u00dfer Kunst, noch gr\u00f6\u00dferer Geduld,", "tokens": ["Mit", "gro\u00b7\u00dfer", "Kunst", ",", "noch", "gr\u00f6\u00b7\u00dfe\u00b7rer", "Ge\u00b7duld", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.40": {"text": "Ein Blumenbeet in meine Weste sticktest,", "tokens": ["Ein", "Blu\u00b7men\u00b7beet", "in", "mei\u00b7ne", "Wes\u00b7te", "stick\u00b7test", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.41": {"text": "Wie statt des Thau's, der Gartenblumen tr\u00e4nkt,", "tokens": ["Wie", "statt", "des", "Thau's", ",", "der", "Gar\u00b7ten\u00b7blu\u00b7men", "tr\u00e4nkt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.42": {"text": "Ich diesen oft der Thr\u00e4nen Thau geschenkt.", "tokens": ["Ich", "die\u00b7sen", "oft", "der", "Thr\u00e4\u00b7nen", "Thau", "ge\u00b7schenkt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PDAT", "ADV", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.43": {"text": "Wann du hinfort mit deinem H\u00fcndchen spielst,", "tokens": ["Wann", "du", "hin\u00b7fort", "mit", "dei\u00b7nem", "H\u00fcnd\u00b7chen", "spielst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.44": {"text": "Erinnre dich, wie du mit seidnem Tuche", "tokens": ["E\u00b7rinn\u00b7re", "dich", ",", "wie", "du", "mit", "seid\u00b7nem", "Tu\u00b7che"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.45": {"text": "Den runden Mund ihm fest verstopfet hieltst,", "tokens": ["Den", "run\u00b7den", "Mund", "ihm", "fest", "ver\u00b7stop\u00b7fet", "hieltst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "ADJD", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.46": {"text": "Wenn aufgeweckt durch n\u00e4chtliche Besuche,", "tokens": ["Wenn", "auf\u00b7ge\u00b7weckt", "durch", "n\u00e4cht\u00b7li\u00b7che", "Be\u00b7su\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.47": {"text": "