{"dta.poem.13221": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Z ehndes  B uch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Hierauf ermunterte ", "tokens": ["Hier\u00b7auf", "er\u00b7mun\u00b7ter\u00b7te"], "token_info": ["word", "word"], "pos": ["PAV", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Und gab ihr diesen Trost: \u201eIch kenne dein Verlangen;", "tokens": ["Und", "gab", "ihr", "die\u00b7sen", "Trost", ":", "\u201e", "Ich", "ken\u00b7ne", "dein", "Ver\u00b7lan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PDAT", "NN", "$.", "$(", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "\u201eso viel mein Herz vermag, so viel versprech\u2019 ich dir;", "tokens": ["\u201e", "so", "viel", "mein", "Herz", "ver\u00b7mag", ",", "so", "viel", "ver\u00b7sprech'", "ich", "dir", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADV", "PPOSAT", "NN", "VVFIN", "$,", "ADV", "ADV", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "510\u201dVerharre wo du bist, und bleibe stets bey mir.", "tokens": ["\"", "Ver\u00b7har\u00b7re", "wo", "du", "bist", ",", "und", "blei\u00b7be", "stets", "bey", "mir", "."], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "NN", "PWAV", "PPER", "VAFIN", "$,", "KON", "VVFIN", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}