{"textgrid.poem.39003": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Welche Lieblichkeit,", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Welche Lieblichkeit,", "tokens": ["Wel\u00b7che", "Lieb\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "punct"], "pos": ["PWAT", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Zier und Pracht,", "tokens": ["Zier", "und", "Pracht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Kunst und Natur!", "tokens": ["Kunst", "und", "Na\u00b7tur", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "So seh ich denn endlich,", "tokens": ["So", "seh", "ich", "denn", "end\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Was ich als Knabe schon tr\u00e4umte,", "tokens": ["Was", "ich", "als", "Kna\u00b7be", "schon", "tr\u00e4um\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "KOUS", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Als J\u00fcngling ersehnte,", "tokens": ["Als", "J\u00fcng\u00b7ling", "er\u00b7sehn\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Und nun \u2013", "tokens": ["Und", "nun", "\u2013"], "token_info": ["word", "word", "punct"], "pos": ["KON", "ADV", "$("], "meter": "-+", "measure": "iambic.si