{"textgrid.poem.64059": {"metadata": {"author": {"name": "Ahlefeld, Charlotte von", "birth": "N.A.", "death": "N.A."}, "title": "Bei \u00dcbersendung meines Bildes", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nimm hin dies Bild, das auch in weite Ferne", "tokens": ["Nimm", "hin", "dies", "Bild", ",", "das", "auch", "in", "wei\u00b7te", "Fer\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "PDS", "NN", "$,", "PRELS", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dir folgen darf, Geliebter! nimm es hin!", "tokens": ["Dir", "fol\u00b7gen", "darf", ",", "Ge\u00b7lieb\u00b7ter", "!", "nimm", "es", "hin", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "VMFIN", "$,", "NN", "$.", "VVIMP", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und glaub' es seinem L\u00e4cheln, es wird gerne", "tokens": ["Und", "glaub'", "es", "sei\u00b7nem", "L\u00e4\u00b7cheln", ",", "es", "wird", "ger\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN", "$,", "PPER", "VAFIN", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "An Deiner Brust die weite Welt durchziehn.", "tokens": ["An", "Dei\u00b7ner", "Brust", "die", "wei\u00b7te", "Welt", "durch\u00b7ziehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Betracht' es oft in stillen Augenblicken,", "tokens": ["Be\u00b7tracht'", "es", "oft", "in", "stil\u00b7len", "Au\u00b7gen\u00b7bli\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn Einsamkeit Dich schwermuthsvoll umgiebt.", "tokens": ["Wenn", "Ein\u00b7sam\u00b7keit", "Dich", "schwer\u00b7muths\u00b7voll", "um\u00b7giebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und dann gedenk' mit schmerzlichem Entz\u00fccken", "tokens": ["Und", "dann", "ge\u00b7denk'", "mit", "schmerz\u00b7li\u00b7chem", "Ent\u00b7z\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der sch\u00f6nen Zeit, in der wir uns geliebt.", "tokens": ["Der", "sch\u00f6\u00b7nen", "Zeit", ",", "in", "der", "wir", "uns", "ge\u00b7liebt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "PRELS", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Sie ist vor\u00fcber \u2013 \u2013 doch die \u00f6de Leere", "tokens": ["Sie", "ist", "vor\u00b7\u00fc\u00b7ber", "\u2013", "\u2013", "doch", "die", "\u00f6\u00b7de", "Lee\u00b7re"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "$(", "$(", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Getrennter Liebe, die im Busen mir", "tokens": ["Ge\u00b7trenn\u00b7ter", "Lie\u00b7be", ",", "die", "im", "Bu\u00b7sen", "mir"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PRELS", "APPRART", "NN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Durch Lethe's Quell nur auszuf\u00fcllen w\u00e4re,", "tokens": ["Durch", "Lethe's", "Quell", "nur", "aus\u00b7zu\u00b7f\u00fcl\u00b7len", "w\u00e4\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "ADV", "VVIZU", "VAFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Stillt meiner Z\u00fcge leiser Umriss Dir.", "tokens": ["Stillt", "mei\u00b7ner", "Z\u00fc\u00b7ge", "lei\u00b7ser", "Um\u00b7riss", "Dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADJA", "NN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "So nimm mein Bild, zum liebevollen Pfande", "tokens": ["So", "nimm", "mein", "Bild", ",", "zum", "lie\u00b7be\u00b7vol\u00b7len", "Pfan\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVIMP", "PPOSAT", "NN", "$,", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der treusten Neigung, nimm es freundlich an,", "tokens": ["Der", "treus\u00b7ten", "Nei\u00b7gung", ",", "nimm", "es", "freund\u00b7lich", "an", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "VVIMP", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und es begleite Dich in ferne Lande,", "tokens": ["Und", "es", "be\u00b7glei\u00b7te", "Dich", "in", "fer\u00b7ne", "Lan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wohin ich nur im Geist Dir folgen kann.", "tokens": ["Wo\u00b7hin", "ich", "nur", "im", "Geist", "Dir", "fol\u00b7gen", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "APPRART", "NN", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Wenn aus der F\u00fclle goldner Jugendtr\u00e4ume", "tokens": ["Wenn", "aus", "der", "F\u00fcl\u00b7le", "gold\u00b7ner", "Ju\u00b7gend\u00b7tr\u00e4u\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Wirklichkeit Dich kalt und bitter scheucht,", "tokens": ["Die", "Wirk\u00b7lich\u00b7keit", "Dich", "kalt", "und", "bit\u00b7ter", "scheucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "ADJD", "KON", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So trage Ahndung Dich in h\u00f6h're R\u00e4ume,", "tokens": ["So", "tra\u00b7ge", "Ahn\u00b7dung", "Dich", "in", "h\u00f6h'\u00b7re", "R\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und Hoffnung mache dann das Herz Dir leicht.", "tokens": ["Und", "Hoff\u00b7nung", "ma\u00b7che", "dann", "das", "Herz", "Dir", "leicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ADV", "ART", "NN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Sie zeige Dir die Zukunft, die dem Kummer", "tokens": ["Sie", "zei\u00b7ge", "Dir", "die", "Zu\u00b7kunft", ",", "die", "dem", 