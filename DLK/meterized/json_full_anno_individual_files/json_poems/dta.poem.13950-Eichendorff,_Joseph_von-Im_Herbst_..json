{"dta.poem.13950": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Im Herbst .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Der Wald wird falb, die Bl\u00e4tter fallen,", "tokens": ["Der", "Wald", "wird", "falb", ",", "die", "Bl\u00e4t\u00b7ter", "fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie \u00f6d' und still der Raum!", "tokens": ["Wie", "\u00f6d'", "und", "still", "der", "Raum", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "KON", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die B\u00e4chlein nur geh\u2019n durch die Buchenhallen", "tokens": ["Die", "B\u00e4ch\u00b7lein", "nur", "geh'n", "durch", "die", "Bu\u00b7chen\u00b7hal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VVPP", "APPR", "ART", "NN"], "meter": "-+-++--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Lindrauschend wie im Traum,", "tokens": ["Lin\u00b7drau\u00b7schend", "wie", "im", "Traum", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und Abendglocken schallen", "tokens": ["Und", "A\u00b7bend\u00b7glo\u00b7cken", "schal\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["KON", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Fern von des Waldes Saum.", "tokens": ["Fern", "von", "des", "Wal\u00b7des", "Saum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}}}}