{"dta.poem.20840": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Der  \n Sterbende  \n  Socrates.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.71", "af:0.28"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Die Seele klebet voller Flecke/", "tokens": ["Die", "See\u00b7le", "kle\u00b7bet", "vol\u00b7ler", "Fle\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So lange sie die schwartze Decke", "tokens": ["So", "lan\u00b7ge", "sie", "die", "schwart\u00b7ze", "De\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Des faulen Leibes tunckel macht/", "tokens": ["Des", "fau\u00b7len", "Lei\u00b7bes", "tun\u00b7ckel", "macht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie kan sich so nicht wol befreyen/", "tokens": ["Sie", "kan", "sich", "so", "nicht", "wol", "be\u00b7fre\u00b7yen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "ADV", "PTKNEG", "ADV", "VVINF", "$("], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Sie dienet bey der Todes Nacht/", "tokens": ["Sie", "die\u00b7net", "bey", "der", "To\u00b7des", "Nacht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Als wo die Lebens-Strahlen scheinen.", "tokens": ["Als", "wo", "die", "Le\u00b7bens\u00b7Strah\u00b7len", "schei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PWAV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Man mu\u00df sie f\u00fchren auf die Bahn/", "tokens": ["Man", "mu\u00df", "sie", "f\u00fch\u00b7ren", "auf", "die", "Bahn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPER", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Da sie was h\u00f6hers finden kan/", "tokens": ["Da", "sie", "was", "h\u00f6\u00b7hers", "fin\u00b7den", "kan", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PWS", "ADV", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Als was der Sterbligkeit beliebet;", "tokens": ["Als", "was", "der", "Ster\u00b7blig\u00b7keit", "be\u00b7lie\u00b7bet", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PWS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Weil sonsten das ber\u00fchmte Licht/", "tokens": ["Weil", "sons\u00b7ten", "das", "be\u00b7r\u00fchm\u00b7te", "Licht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "So aus der Seelen Hoheit bricht/", "tokens": ["So", "aus", "der", "See\u00b7len", "Ho\u00b7heit", "bricht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Nicht seine rechte Klarheit giebet.", "tokens": ["Nicht", "sei\u00b7ne", "rech\u00b7te", "Klar\u00b7heit", "gie\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}