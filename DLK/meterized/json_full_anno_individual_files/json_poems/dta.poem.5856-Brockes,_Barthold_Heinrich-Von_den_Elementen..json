{"dta.poem.5856": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Elementen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Kan man sich wol mit Recht verwundern, wenn man sp\u00fcrt,", "tokens": ["Kan", "man", "sich", "wol", "mit", "Recht", "ver\u00b7wun\u00b7dern", ",", "wenn", "man", "sp\u00fcrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "PRF", "ADV", "APPR", "NN", "VVINF", "$,", "KOUS", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df blinder Unverstand zuerst regiert?", "tokens": ["Da\u00df", "blin\u00b7der", "Un\u00b7ver\u00b7stand", "zu\u00b7erst", "re\u00b7giert", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Menschen, da sie nichts, als durch die Sinnen, kennten;", "tokens": ["Die", "Men\u00b7schen", ",", "da", "sie", "nichts", ",", "als", "durch", "die", "Sin\u00b7nen", ",", "kenn\u00b7ten", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "PIS", "$,", "KOUS", "APPR", "ART", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So \u00fcbergaben sie die ", "tokens": ["So", "\u00fc\u00b7berg\u00b7a\u00b7ben", "sie", "die"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Dem Stoff und auch den Elementen.", "tokens": ["Dem", "Stoff", "und", "auch", "den", "E\u00b7le\u00b7men\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ADV", "ART", "NN", "$."], "meter": "-+-+---+-", "measure": "unknown.measure.tri"}, "line.6": {"text": "In diesen Jrrthum nun verwickelt, ward sobald", "tokens": ["In", "die\u00b7sen", "Jrr\u00b7thum", "nun", "ver\u00b7wi\u00b7ckelt", ",", "ward", "so\u00b7bald"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PDAT", "NN", "ADV", "VVPP", "$,", "VAFIN", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Das, welches sonst an Sch\u00f6nheit und Gestallt", "tokens": ["Das", ",", "wel\u00b7ches", "sonst", "an", "Sch\u00f6n\u00b7heit", "und", "Ge\u00b7stallt"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "$,", "PRELS", "ADV", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Sich unterschied, auch das, was ihnen Nutzen bracht,", "tokens": ["Sich", "un\u00b7ter\u00b7schied", ",", "auch", "das", ",", "was", "ih\u00b7nen", "Nut\u00b7zen", "bracht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "$,", "ADV", "PDS", "$,", "PWS", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Zu einer GOTTHEJT gleich gemacht.", "tokens": ["Zu", "ei\u00b7ner", "GoTTHEJT", "gleich", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}