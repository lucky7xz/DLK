{"textgrid.poem.58657": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "1L: S\u00fcnde, Welt und Eigenheit halten meinen Geist verstricket,", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "S\u00fcnde, Welt und Eigenheit halten meinen Geist verstricket,", "tokens": ["S\u00fcn\u00b7de", ",", "Welt", "und", "Ei\u00b7gen\u00b7heit", "hal\u00b7ten", "mei\u00b7nen", "Geist", "ver\u00b7stri\u00b7cket", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "VVFIN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Ach, ich werd' von fremder Macht sehr geplagt und unterdr\u00fccket;", "tokens": ["Ach", ",", "ich", "werd'", "von", "frem\u00b7der", "Macht", "sehr", "ge\u00b7plagt", "und", "un\u00b7ter\u00b7dr\u00fc\u00b7cket", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VAFIN", "APPR", "ADJA", "NN", "ADV", "VVPP", "KON", "VVFIN", "$."], "meter": "-+--+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Ob ich streit' und ob ich schrei,", "tokens": ["Ob", "ich", "streit'", "und", "ob", "ich", "schrei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "KON", "KOUS", "PPER", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Doch l\u00e4\u00dft mich der Feind nicht frei,", "tokens": ["Doch", "l\u00e4\u00dft", "mich", "der", "Feind", "nicht", "frei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ART", "NN", "PTKNEG", "ADJD", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Stets ich will und doch nicht kann,", "tokens": ["Stets", "ich", "will", "und", "doch", "nicht", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VMFIN", "KON", "ADV", "PTKNEG", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ich bin ein gefangner Mann.", "tokens": ["Ich", "bin", "ein", "ge\u00b7fang\u00b7ner", "Mann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Liebe Seel', verzage nicht, w\u00e4r' dein Stand auch zehnmal b\u00f6ser;", "tokens": ["Lie\u00b7be", "Seel'", ",", "ver\u00b7za\u00b7ge", "nicht", ",", "w\u00e4r'", "dein", "Stand", "auch", "zehn\u00b7mal", "b\u00f6\u00b7ser", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "PTKNEG", "$,", "VAFIN", "PPOSAT", "NN", "ADV", "ADV", "ADJD", "$."], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.8": {"text": "Zwar der Feind ist dir zu stark, doch \u2013 wie stark ist dein Erl\u00f6ser?", "tokens": ["Zwar", "der", "Feind", "ist", "dir", "zu", "stark", ",", "doch", "\u2013", "wie", "stark", "ist", "dein", "Er\u00b7l\u00f6\u00b7ser", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "PPER", "PTKA", "ADJD", "$,", "ADV", "$(", "PWAV", "ADJD", "VAFIN", "PPOSAT", "NN", "$."], "meter": "---+--+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "La\u00df ihn f\u00fchren deine Sach',", "tokens": ["La\u00df", "ihn", "f\u00fch\u00b7ren", "dei\u00b7ne", "Sach'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Er kann, was kein Mensch vermag,", "tokens": ["Er", "kann", ",", "was", "kein", "Mensch", "ver\u00b7mag", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "PRELS", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.11": {"text": "Er zerbricht das h\u00e4rt'ste Band,", "tokens": ["Er", "zer\u00b7bricht", "das", "h\u00e4rt'\u00b7ste", "Band", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Er bringt Ruh im innern Land!", "tokens": ["Er", "bringt", "Ruh", "im", "in\u00b7nern", "Land", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "S\u00fcnde, Welt und Eigenheit halten meinen Geist verstricket,", "tokens": ["S\u00fcn\u00b7de", ",", "Welt", "und", "Ei\u00b7gen\u00b7heit", "hal\u00b7ten", "mei\u00b7nen", "Geist", "ver\u00b7stri\u00b7cket", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "VVFIN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Ach, ich werd' von fremder Macht sehr geplagt und unterdr\u00fccket;", "tokens": ["Ach", ",", "ich", "werd'", "von", "frem\u00b7der", "Macht", "sehr", "ge\u00b7plagt", "und", "un\u00b7ter\u00b7dr\u00fc\u00b7cket", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VAFIN", "APPR", "ADJA", "NN", "ADV", "VVPP", "KON", "VVFIN", 