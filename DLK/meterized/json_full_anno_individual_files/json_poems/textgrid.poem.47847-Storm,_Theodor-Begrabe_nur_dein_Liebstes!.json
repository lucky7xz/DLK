{"textgrid.poem.47847": {"metadata": {"author": {"name": "Storm, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Begrabe nur dein Liebstes!", "genre": "verse", "period": "N.A.", "pub_year": 1852, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Begrabe nur dein Liebstes! Dennoch gilt's", "tokens": ["Be\u00b7gra\u00b7be", "nur", "dein", "Liebs\u00b7tes", "!", "Den\u00b7noch", "gilt's"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "ADV", "PPOSAT", "NN", "$.", "ADV", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Nun weiterleben; \u2013 und im Drang des Tages,", "tokens": ["Nun", "wei\u00b7ter\u00b7le\u00b7ben", ";", "\u2013", "und", "im", "Drang", "des", "Ta\u00b7ges", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "$.", "$(", "KON", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dein Ich behauptend, stehst bald wieder du.", "tokens": ["Dein", "Ich", "be\u00b7haup\u00b7tend", ",", "stehst", "bald", "wie\u00b7der", "du", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "PPER", "VVPP", "$,", "VVFIN", "ADV", "ADV", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "\u2013 So j\u00fcngst im Kreis der Freunde war es, wo", "tokens": ["\u2013", "So", "j\u00fcngst", "im", "Kreis", "der", "Freun\u00b7de", "war", "es", ",", "wo"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["$(", "ADV", "ADV", "APPRART", "NN", "ART", "NN", "VAFIN", "PPER", "$,", "PWAV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Hinrei\u00dfend Wort zu lauter Rede schwoll;", "tokens": ["Hin\u00b7rei\u00b7\u00dfend", "Wort", 