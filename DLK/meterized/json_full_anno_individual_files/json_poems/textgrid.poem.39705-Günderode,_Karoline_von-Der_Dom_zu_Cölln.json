{"textgrid.poem.39705": {"metadata": {"author": {"name": "G\u00fcnderode, Karoline von", "birth": "N.A.", "death": "N.A."}, "title": "Der Dom zu C\u00f6lln", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "F\u00fcnffach w\u00f6lbt sich die Decke auf Gruppen gothischer S\u00e4ulen,", "tokens": ["F\u00fcnf\u00b7fach", "w\u00f6lbt", "sich", "die", "De\u00b7cke", "auf", "Grup\u00b7pen", "go\u00b7thi\u00b7scher", "S\u00e4u\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "ART", "NN", "APPR", "NN", "ADJA", "NN", "$,"], "meter": "+-+--+--+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "H\u00f6her hebt sich der Chor, stolzer getragen empor,", "tokens": ["H\u00f6\u00b7her", "hebt", "sich", "der", "Chor", ",", "stol\u00b7zer", "ge\u00b7tra\u00b7gen", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "ART", "NN", "$,", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Sch\u00f6n ist das Innre geziert mit Erzen und Marmor und Teppchen", "tokens": ["Sch\u00f6n", "ist", "das", "Inn\u00b7re", "ge\u00b7ziert", "mit", "Er\u00b7zen", "und", "Mar\u00b7mor", "und", "Tepp\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "ART", "NN", "VVPP", "APPR", "NN", "KON", "NN", "KON", "NN"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Und ein purpurner Tag bricht durch die farbigen Fenster. \u2013", "tokens": ["Und", "ein", "pur\u00b7pur\u00b7ner", "Tag", "bricht", "durch", "die", "far\u00b7bi\u00b7gen", "Fens\u00b7ter", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$.", "$("], "meter": "--++-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Hauchet ein Modergeruch dumpf aus der Tiefe herauf,", "tokens": ["Hau\u00b7chet", "ein", "Mo\u00b7der\u00b7ge\u00b7ruch", "dumpf", "aus", "der", "Tie\u00b7fe", "her\u00b7auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.6": {"text": "Alda schlafen die Helden der Kirche im h\u00fcllenden Sarge", "tokens": ["Al\u00b7da", "schla\u00b7fen", "die", "Hel\u00b7den", "der", "Kir\u00b7che", "im", "h\u00fcl\u00b7len\u00b7den", "Sar\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "ART", "NN", "ART", "NN", "APPRART", "ADJA", "NN"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.7": {"text": "Und ihr Bildni\u00df ruht drauf, sie falten die H\u00e4nde zum Beten,", "tokens": ["Und", "ihr", "Bild\u00b7ni\u00df", "ruht", "drauf", ",", "sie", "fal\u00b7ten", "die", "H\u00e4n\u00b7de", "zum", "Be\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PTKVZ", "$,", "PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "--+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Und ihr starrender Blick hat sich zum Himmel gewandt.", "tokens": ["Und", "ihr", "star\u00b7ren\u00b7der", "Blick", "hat", "sich", "zum", "Him\u00b7mel", "ge\u00b7wandt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VAFIN", "PRF", "APPRART", "NN", "VVPP", "$."], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.9": {"text": "Staunend seh ich sie an, mir ist, als m\u00fc\u00dften sie reden,", "tokens": ["Stau\u00b7nend", "seh", "ich", "sie", "an", ",", "mir", "ist", ",", "als", "m\u00fc\u00df\u00b7ten", "sie", "re\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PPER", "PTKVZ", "$,", "PPER", "VAFIN", "$,", "KOUS", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.10": {"text": "Aber sie starren noch fort, wie sie es Jahrhunderte thaten", "tokens": ["A\u00b7ber", "sie", "star\u00b7ren", "noch", "fort", ",", "wie", "sie", "es", "Jahr\u00b7hun\u00b7der\u00b7te", "tha\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADV", "PTKVZ", "$,", "PWAV", "PPER", "PPER", "NN", "VVFIN"], "meter": "+--+--+-+-+-+-+-", "measure": "dactylic.di.plus"}, "line.11": {"text": "Und mich schauert so tief, da\u00df also stumm sind die Todten.", "tokens": ["Und", "mich", "schau\u00b7ert", "so", "tief", ",", "da\u00df", "al\u00b7so", "stumm", "sind", "die", "Tod\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "ADJD", "$,", "KOUS", "ADV", "ADJD", "VAFIN", "ART", "NN", "$."], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.12": {"text": "Doch da hebt sich Gesang, und Orgelt\u00f6ne, sie schweben", "tokens": ["Doch", "da", "hebt", "sich", "Ge\u00b7sang", ",", "und", "Or\u00b7gel\u00b7t\u00f6\u00b7ne", ",", "sie", "schwe\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PRF", "NN", "$,", "KON", "NN", "$,", "PPER", "VVINF"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.13": {"text": "Feiernd die Dome hinauf, wo gl\u00e4nzende Heilige beten", "tokens": ["Fei\u00b7ernd", "die", "Do\u00b7me", "hin\u00b7auf", ",", "wo", "gl\u00e4n\u00b7zen\u00b7de", "Hei\u00b7li\u00b7ge", "be\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "ART", "NN", "PTKVZ", "$,", "PWAV", "ADJA", "NN", "VVINF"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.14": {"text": "Aber es wandlen die T\u00f6ne sich und in Fitt'che der Engel", "tokens": ["A\u00b7ber", "es", "wand\u00b7len", "die", "T\u00f6\u00b7ne", "sich", "und", "in", "Fitt'\u00b7che", "der", "En\u00b7gel"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "PRF", "KON", "APPR", "NN", "ART", "NN"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.15": {"text": "Und umrauschten melodisch wogend die heiligen Bilder.", "tokens": ["Und", "um\u00b7rauschten", "me\u00b7lo\u00b7disch", "wo\u00b7gend", "die", "hei\u00b7li\u00b7gen", "Bil\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "--+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.16": {"text": "Und zum Himmel verkl\u00e4ret sich alles \u2013 Musik, und Farben, und Formen,", "tokens": ["Und", "zum", "Him\u00b7mel", "ver\u00b7kl\u00e4\u00b7ret", "sich", "al\u00b7les", "\u2013", "Mu\u00b7sik", ",", "und", "Far\u00b7ben", ",", "und", "For\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVFIN", "PRF", "PIS", "$(", "NN", "$,", "KON", "NN", "$,", "KON", "NN", "$,"], "meter": "--+--+--+-+--+--+-", "measure": "anapaest.tri.plus"}, "line.17": {"text": "Aus