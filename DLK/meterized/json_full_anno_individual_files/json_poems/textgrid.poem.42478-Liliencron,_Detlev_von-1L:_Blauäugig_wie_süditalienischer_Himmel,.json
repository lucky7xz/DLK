{"textgrid.poem.42478": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Blau\u00e4ugig wie s\u00fcditalienischer Himmel,", "genre": "verse", "period": "N.A.", "pub_year": 1876, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Blau\u00e4ugig wie s\u00fcditalienischer Himmel,", "tokens": ["Blau\u00b7\u00e4u\u00b7gig", "wie", "s\u00fc\u00b7di\u00b7ta\u00b7li\u00b7e\u00b7ni\u00b7scher", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "ADJA", "NN", "$,"], "meter": "++--+-+-+--+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Schwarzhaarig wie dunkelste Mitternacht,", "tokens": ["Schwarz\u00b7haa\u00b7rig", "wie", "dun\u00b7kels\u00b7te", "Mit\u00b7ter\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Geheimnisvoll wie das Sternengewimmel,", "tokens": ["Ge\u00b7heim\u00b7nis\u00b7voll", "wie", "das", "Ster\u00b7nen\u00b7ge\u00b7wim\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Rachs\u00fcchtig wie eine verlorene Schlacht.", "tokens": ["Rach\u00b7s\u00fcch\u00b7tig", "wie", "ei\u00b7ne", "ver\u00b7lo\u00b7re\u00b7ne", "Schlacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.2": {"line.1": {"text": "Bezaubernd war dein ganzes Gebaren,", "tokens": ["Be\u00b7zau\u00b7bernd", "war", "dein", "gan\u00b7zes", "Ge\u00b7ba\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Unschuldig wie erster Fr\u00fchlingsschein.", "tokens": ["Un\u00b7schul\u00b7dig", "wie", "ers\u00b7ter", "Fr\u00fch\u00b7lings\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "ADJA", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Klein, zierlich, ein T\u00e4ubchen aus Taubenscharen,", "tokens": ["Klein", ",", "zier\u00b7lich", ",", "ein", "T\u00e4ub\u00b7chen", "aus", "Tau\u00b7ben\u00b7scha\u00b7ren", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJD", "$,", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Beruhigt dein Bild wie Elfenbein.", "tokens": ["Be\u00b7ru\u00b7higt", "dein", "Bild", "wie", "El\u00b7fen\u00b7bein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "KOKOM", "NN", "$."], "meter": "--+-+-+-+", "measure": "anapaest.init"}}, "stanza.3": {"line.1": {"text": "Schau ich hinein in deine Seele,", "tokens": ["Schau", "ich", "hin\u00b7ein", "in", "dei\u00b7ne", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sind Hochmut, Habgier und Herrschsucht drin;", "tokens": ["Sind", "Hoch\u00b7mut", ",", "Hab\u00b7gier", "und", "Herrschsucht", "drin", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "$,", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und deine un\u00fcberwindlichen Fehle", "tokens": ["Und", "dei\u00b7ne", "un\u00b7\u00fc\u00b7berw\u00b7ind\u00b7li\u00b7chen", "Feh\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "\u00dcbertrumpft dein furchtbarer M\u00f6rdersinn.", "tokens": ["\u00dc\u00b7bert\u00b7rumpft", "dein", "furcht\u00b7ba\u00b7rer", "M\u00f6r\u00b7der\u00b7sinn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "----+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Sag mir, du warst aus niedrigstem Stande,", "tokens": ["Sag", "mir", ",", "du", "warst", "aus", "nied\u00b7rigs\u00b7tem", "Stan\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "PPER", "VAFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wie wurdest du K\u00f6nigin, Fredegund?", "tokens": ["Wie", "wur\u00b7dest", "du", "K\u00f6\u00b7ni\u00b7gin", ",", "Fre\u00b7de\u00b7gund", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "\u00bbich nahm K\u00f6nig Hilprich leicht in Bande", "tokens": ["\u00bb", "ich", "nahm", "K\u00f6\u00b7nig", "Hil\u00b7prich", "leicht", "in", "Ban\u00b7de"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VVFIN", "NE", "NE", "ADJD", "APPR", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und schlo\u00df mit ihm den br\u00e4utlichen Bund.\u00ab", "tokens": ["Und", "schlo\u00df", "mit", "ihm", "den", "br\u00e4ut\u00b7li\u00b7chen", "Bund", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.5": {"line.1": {"text": "Sag mir, einst wuschest du dir die Locken,", "tokens": ["Sag", "mir", ",", "einst", "wu\u00b7schest", "du", "dir", "die", "Lo\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "ADV", "VVFIN", "PPER", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Die fielen nach vorn dir \u00fcbers Gesicht,", "tokens": ["Die", "fie\u00b7len", "nach", "vorn", "dir", "\u00fc\u00b7bers", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "ADV", "PPER", "APPRART", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Da neckte dich einer, er kam wie auf Socken,", "tokens": ["Da", "neck\u00b7te", "dich", "ei\u00b7ner", ",", "er", "kam", "wie", "auf", "So\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "$,", "PPER", "VVFIN", "KOKOM", "APPR", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Es war der K\u00f6nig, du merktest es nicht.", "tokens": ["Es", "war", "der", "K\u00f6\u00b7nig", ",", "du", "merk\u00b7test", "es", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PPER", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Und du riefst lachend durchs Haargewirre:", "tokens": ["Und", "du", "riefst", "la\u00b7chend", "durchs", "Haar\u00b7ge\u00b7wir\u00b7re", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Landrich, was willst du schon, mein Herz?", "tokens": ["Land\u00b7rich", ",", "was", "willst", "du", "schon", ",", "mein", "Herz", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWS", "VMFIN", "PPER", "ADV", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und sahst dich um, und wurdest wie irre:", "tokens": ["Und", "sahst", "dich", "um", ",", "und", "wur\u00b7dest", "wie", "ir\u00b7re", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "$,", "KON", "VAFIN", "KOKOM", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Der K\u00f6nig stand vor dir, verzerrt von Schmerz.", "tokens": ["Der", "K\u00f6\u00b7nig", "stand", "vor", "dir", ",", "ver\u00b7zerrt", "von", "Schmerz", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPER", "$,", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Der K\u00f6nig? Der war ja zur Jagd geritten;", "tokens": ["Der", "K\u00f6\u00b7nig", "?", "Der", "war", "ja", "zur", "Jagd", "ge\u00b7rit\u00b7ten", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "PDS", "VAFIN", "ADV", "APPRART", "NN", "VVPP", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Wo kam denn der noch einmal her?", "tokens": ["Wo", "kam", "denn", "der", "noch", "ein\u00b7mal", "her", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ADV", "ART", "ADV", "ADV", "PTKVZ", "$."], "meter": "-+--++-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Er ist dann finster davon geschritten,", "tokens": ["Er", "ist", "dann", "fins\u00b7ter", "da\u00b7von", "ge\u00b7schrit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "PAV", "VVPP", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und geht zur Jagd, sein Haupt h\u00e4ngt schwer.", "tokens": ["Und", "geht", "zur", "Jagd", ",", "sein", "Haupt", "h\u00e4ngt", "schwer", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "$,", "PPOSAT", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Du lie\u00dfest gleich deinen Liebsten kommen,", "tokens": ["Du", "lie\u00b7\u00dfest", "gleich", "dei\u00b7nen", "Liebs\u00b7ten", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Landrich, den Kanzler, batst du zu dir,", "tokens": ["Land\u00b7rich", ",", "den", "Kanz\u00b7ler", ",", "batst", "du", "zu", "dir", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ART", "NN", "$,", "VVFIN", "PPER", "APPR", "PPER", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Und sagtest entsetzt ihm, von Angst beklommen:", "tokens": ["Und", "sag\u00b7test", "ent\u00b7setzt", "ihm", ",", "von", "Angst", "be\u00b7klom\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "VVFIN", "PPER", "$,", "APPR", "NN", "VVINF", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "\u00bbkehrt er zur\u00fcck, spie\u00dft uns ein Stier.", "tokens": ["\u00bb", "kehrt", "er", "zu\u00b7r\u00fcck", ",", "spie\u00dft", "uns", "ein", "Stier", "."], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "PTKVZ", "$,", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.9": {"line.1": {"text": "Schnell, ich wei\u00df schon, schon ists mir geworden:", "tokens": ["Schnell", ",", "ich", "wei\u00df", "schon", ",", "schon", "ists", "mir", "ge\u00b7wor\u00b7den", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PPER", "VVFIN", "ADV", "$,", "ADV", "VAFIN", "PPER", "VAPP", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Kommt der K\u00f6nig zur\u00fcck diese Nacht,", "tokens": ["Kommt", "der", "K\u00f6\u00b7nig", "zu\u00b7r\u00fcck", "die\u00b7se", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKVZ", "PDAT", "NN", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Wir lassen ihn, wenn er vom Pferd steigt, ermorden,", "tokens": ["Wir", "las\u00b7sen", "ihn", ",", "wenn", "er", "vom", "Pferd", "steigt", ",", "er\u00b7mor\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KOUS", "PPER", "APPRART", "NN", "VVFIN", "$,", "VVINF", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Dann sind wir sicher. Uns trifft kein Verdacht.\u00ab", "tokens": ["Dann", "sind", "wir", "si\u00b7cher", ".", "Uns", "trifft", "kein", "Ver\u00b7dacht", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "$.", "PPER", "VVFIN", "PIAT", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Und so geschahs. Sag mir, Fredegunde,", "tokens": ["Und", "so", "ge\u00b7schahs", ".", "Sag", "mir", ",", "Fre\u00b7de\u00b7gun\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "$.", "NN", "PPER", "$,", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Warum traf dein Beil K\u00f6nig Sigibert?", "tokens": ["Wa\u00b7rum", "traf", "dein", "Beil", "K\u00f6\u00b7nig", "Si\u00b7gi\u00b7bert", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPOSAT", "NN", "NE", "NE", "$."], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Mit deinem l\u00e4chelnden, s\u00fc\u00dfen Munde", "tokens": ["Mit", "dei\u00b7nem", "l\u00e4\u00b7cheln\u00b7den", ",", "s\u00fc\u00b7\u00dfen", "Mun\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Hast du gleich drauf Rosen und Zymbeln begehrt.", "tokens": ["Hast", "du", "gleich", "drauf", "Ro\u00b7sen", "und", "Zym\u00b7beln", "be\u00b7gehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "PAV", "NN", "KON", "NN", "VVPP", "$."], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}}, "stanza.11": {"line.1": {"text": "Deine Tochter Rigunthe mu\u00dfte suchen", "tokens": ["Dei\u00b7ne", "Toch\u00b7ter", "Ri\u00b7gun\u00b7the", "mu\u00df\u00b7te", "su\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "NE", "VMFIN", "VVINF"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "In der ge\u00f6ffneten Truhe nach Schmuck,", "tokens": ["In", "der", "ge\u00b7\u00f6ff\u00b7ne\u00b7ten", "Tru\u00b7he", "nach", "Schmuck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Dann klapptest du ihr den Deckel beim Suchen", "tokens": ["Dann", "klapp\u00b7test", "du", "ihr", "den", "De\u00b7ckel", "beim", "Su\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ART", "NN", "APPRART", "NN"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Auf den Hals mit wuchtigem Ruck und Druck.", "tokens": ["Auf", "den", "Hals", "mit", "wuch\u00b7ti\u00b7gem", "Ruck", "und", "Druck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ADJA", "NN", "KON", "NN", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.12": {"line.1": {"text": "Du hattest den Tod K\u00f6nig Childberts erwogen,", "tokens": ["Du", "hat\u00b7test", "den", "Tod", "K\u00f6\u00b7nig", "Child\u00b7berts", "er\u00b7wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "NE", "NE", "VVPP", "$,"], "meter": "-+--++-++-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Zwei Geistliche triebst du zum Henkergericht,", "tokens": ["Zwei", "Geist\u00b7li\u00b7che", "triebst", "du", "zum", "Hen\u00b7ker\u00b7ge\u00b7richt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Und gabst ihnen Dolche, mit Gift \u00fcberzogen,", "tokens": ["Und", "gabst", "ih\u00b7nen", "Dol\u00b7che", ",", "mit", "Gift", "\u00f