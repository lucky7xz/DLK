t"], "pos": ["APPR", "NN", "KON", "PPER", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Hier ist mein demuthvolles Herz,", "tokens": ["Hier", "ist", "mein", "de\u00b7muth\u00b7vol\u00b7les", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "So sich verband, in Lieb' und Schmerz", "tokens": ["So", "sich", "ver\u00b7band", ",", "in", "Lieb'", "und", "Schmerz"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "PRF", "VVFIN", "$,", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit gleicher Andacht dir zu dienen.", "tokens": ["Mit", "glei\u00b7cher", "An\u00b7dacht", "dir", "zu", "die\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nimm, Sylvia, das Opfer hin,", "tokens": ["Nimm", ",", "Syl\u00b7via", ",", "das", "Op\u00b7fer", "hin", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "$,", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "La\u00df Augentrost in deinem Sinn,", "tokens": ["La\u00df", "Au\u00b7gen\u00b7trost", "in", "dei\u00b7nem", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Vergi\u00dfmeinnicht im Herzen gr\u00fcnen!", "tokens": ["Ver\u00b7gi\u00df\u00b7mein\u00b7nicht", "im", "Her\u00b7zen", "gr\u00fc\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART