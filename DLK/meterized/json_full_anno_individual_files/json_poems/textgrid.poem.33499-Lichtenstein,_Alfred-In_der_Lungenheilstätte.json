{"textgrid.poem.33499": {"metadata": {"author": {"name": "Lichtenstein, Alfred", "birth": "N.A.", "death": "N.A."}, "title": "In der Lungenheilst\u00e4tte", "genre": "verse", "period": "N.A.", "pub_year": 1914, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Viele kranke Leute gehen in den G\u00e4rten", "tokens": ["Vie\u00b7le", "kran\u00b7ke", "Leu\u00b7te", "ge\u00b7hen", "in", "den", "G\u00e4r\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Her und hin und liegen in den Hallen.", "tokens": ["Her", "und", "hin", "und", "lie\u00b7gen", "in", "den", "Hal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "PTKVZ", "KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Die die Kr\u00e4nksten sind, verfiebern", "tokens": ["Die", "die", "Kr\u00e4nks\u00b7ten", "sind", ",", "ver\u00b7fie\u00b7bern"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ART", "ART", "NN", "VAFIN", "$,", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Alle armen Tage in dem hei\u00dfen", "tokens": ["Al\u00b7le", "ar\u00b7men", "Ta\u00b7ge", "in", "dem", "hei\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "APPR", "ART", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Grab der Betten.", "tokens": ["Grab", "der", "Bet\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Ach, katholische Schwestern schweben", "tokens": ["Ach", ",", "ka\u00b7tho\u00b7li\u00b7sche", "Schwes\u00b7tern", "schwe\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ITJ", "$,", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "M\u00fcd umher in schwarzen Gew\u00e4ndern.", "tokens": ["M\u00fcd", "um\u00b7her", "in", "schwar\u00b7zen", "Ge\u00b7w\u00e4n\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "APPR", "ADJA", "NN", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Gestern ist einer gestorben. Heute kann einer sterben.", "tokens": ["Ge\u00b7stern", "ist", "ei\u00b7ner", "ge\u00b7stor\u00b7ben", ".", "Heu\u00b7te", "kann", "ei\u00b7ner", "ster\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "VVPP", "$.", "ADV", "VMFIN", "PIS", "VVINF", "$."], "meter": "-+-+--+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "In der Stadt feiern sie Fasching.", "tokens": ["In", "der", "Stadt", "fei\u00b7ern", "sie", "Fa\u00b7sching", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Den Unterschied", "tokens": ["Den", "Un\u00b7ter\u00b7schied"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "M\u00f6chte ich Klavier spielen k\u00f6nnen.", "tokens": ["M\u00f6ch\u00b7te", "ich", "Kla\u00b7vier", "spie\u00b7len", "k\u00f6n\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "NN", "VVINF", "VMINF", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}, "stanza.2": {"line.1": {"text": "Viele kranke Leute gehen in den G\u00e4rten", "tokens": ["Vie\u00b7le", "kran\u00b7ke", "Leu\u00b7te", "ge\u00b7hen", "in", "den",