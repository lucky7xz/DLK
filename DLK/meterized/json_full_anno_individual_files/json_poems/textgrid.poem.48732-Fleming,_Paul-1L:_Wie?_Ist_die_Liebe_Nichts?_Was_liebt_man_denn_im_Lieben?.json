{"textgrid.poem.48732": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie? Ist die Liebe Nichts? Was liebt man denn im Lieben?", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie? Ist die Liebe Nichts? Was liebt man denn im Lieben?", "tokens": ["Wie", "?", "Ist", "die", "Lie\u00b7be", "Nichts", "?", "Was", "liebt", "man", "denn", "im", "Lie\u00b7ben", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "VAFIN", "ART", "NN", "PIS", "$.", "PWS", "VVFIN", "PIS", "ADV", "APPRART", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was aber? Alles? Nein. Wer ist vergn\u00fcgt mit ihr?", "tokens": ["Was", "a\u00b7ber", "?", "Al\u00b7les", "?", "Nein", ".", "Wer", "ist", "ver\u00b7gn\u00fcgt", "mit", "ihr", "?"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "$.", "PIS", "$.", "PTKANT", "$.", "PWS", "VAFIN", "VVPP", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nicht Wasser; sie ergl\u00fcht die Herzen f\u00fcr und f\u00fcr.", "tokens": ["Nicht", "Was\u00b7ser", ";", "sie", "er\u00b7gl\u00fcht", "die", "Her\u00b7zen", "f\u00fcr", "und", "f\u00fcr", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "$.", "PPER", "VVFIN", "ART", "NN", "APPR", "KON", "APPR", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Auch Feuer nicht. Warumb? Was ist f\u00fcr Flammen blieben?", "tokens": ["Auch", "Feu\u00b7er", "nicht", ".", "Wa\u00b7rumb", "?", "Was", "ist", "f\u00fcr", "Fl