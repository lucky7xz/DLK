{"textgrid.poem.56496": {"metadata": {"author": {"name": "Heym, Georg", "birth": "N.A.", "death": "N.A."}, "title": "1L: Deine Wimpern, die langen,", "genre": "verse", "period": "N.A.", "pub_year": 1899, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Deine Wimpern, die langen,", "tokens": ["Dei\u00b7ne", "Wim\u00b7pern", ",", "die", "lan\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ART", "ADJA", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Deiner Augen dunkele Wasser,", "tokens": ["Dei\u00b7ner", "Au\u00b7gen", "dun\u00b7ke\u00b7le", "Was\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "La\u00df mich tauchen darein,", "tokens": ["La\u00df", "mich", "tau\u00b7chen", "da\u00b7rein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVFIN", "PAV", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "La\u00df mich zur Tiefe gehn.", "tokens": ["La\u00df", "mich", "zur", "Tie\u00b7fe", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Steigt der Bergmann zum Schacht", "tokens": ["Steigt", "der", "Berg\u00b7mann", "zum", "Schacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "APPRART", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Und schwankt seine tr\u00fcbe Lampe", "tokens": ["Und", "schwankt", "sei\u00b7ne", "tr\u00fc\u00b7be", "Lam\u00b7pe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "\u00dcber der Erze Tor,", "tokens": ["\u00dc\u00b7ber", "der", "Er\u00b7ze", "Tor", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NE", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Hoch an der Schattenwand,", "tokens": ["Hoch", "an", "der", "Schat\u00b7ten\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.3": {"line.1": {"text": "Sieh, ich steige hinab,", "tokens": ["Sieh", ",", "ich", "stei\u00b7ge", "hin\u00b7ab", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.2": {"text": "In deinem Scho\u00df zu vergessen,", "tokens": ["In", "dei\u00b7nem", "Scho\u00df", "zu", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Fern, was von oben dr\u00f6hnt,", "tokens": ["Fern", ",", "was", "von", "o\u00b7ben", "dr\u00f6hnt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "APPR", "ADV", "VVFIN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Helle und Qual und Tag.", "tokens": ["Hel\u00b7le", "und", "Qual", "und", "Tag", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "KON", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.4": {"line.1": {"text": "An den Feldern verw\u00e4chst,", "tokens": ["An", "den", "Fel\u00b7dern", "ver\u00b7w\u00e4chst", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Wo der Wind steht, trunken vom Korn,", "tokens": ["Wo", "der", "Wind", "steht", ",", "trun\u00b7ken", "vom", "Korn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$,", "ADJD", "APPRART", "NN", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Hoher Dorn, hoch und krank", "tokens": ["Ho\u00b7her", "Dorn", ",", "hoch", "und", "krank"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "ADJD", "KON", "ADJD"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Gegen das Himmelsblau.", "tokens": ["Ge\u00b7gen", "das", "Him\u00b7mels\u00b7blau", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Gib mir die Hand,", "tokens": ["Gib", "mir", "die", "Hand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Wir wollen einander verwachsen,", "tokens": ["Wir", "wol\u00b7len", "ein\u00b7an\u00b7der", "ver\u00b7wach\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Einem Wind Beute,", "tokens": ["Ei\u00b7nem", "Wind", "Beu\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "+-++-", "measure": "unknown.measure.tri"}, "line.4": {"text": "Einsamer V\u00f6gel Flug,", "tokens": ["Ein\u00b7sa\u00b7mer", "V\u00f6\u00b7gel", "Flug", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.6": {"line.1": {"text": "H\u00f6ren im Sommer", "tokens": ["H\u00f6\u00b7ren", "im", "Som\u00b7mer"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "APPRART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Die Orgel der matten Gewitter,", "tokens": ["Die", "Or\u00b7gel", "der", "mat\u00b7ten", "Ge\u00b7wit\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Baden in Herbsteslicht,", "tokens": ["Ba\u00b7den", "in", "Herbs\u00b7tes\u00b7licht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Am Ufer des blauen Tags.", "tokens": ["Am", "U\u00b7fer", "des", "blau\u00b7en", "Tags", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "Manchmal wollen wir stehn", "tokens": ["Manch\u00b7mal", "wol\u00b7len", "wir", "stehn"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "VVINF"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Am Rand des dunkelen Brunnens,", "tokens": ["Am", "Rand", "des", "dun\u00b7ke\u00b7len", "Brun\u00b7nens", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Tief in die Stille zu sehn,", "tokens": ["Tief", "in", "die", "Stil\u00b7le", "zu", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.4": {"text": "Unsere Liebe zu suchen.", "tokens": ["Un\u00b7se\u00b7re", "Lie\u00b7be", "zu", "su\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.8": {"line.1": {"text": "Oder wir treten hinaus", "tokens": ["O\u00b7der", "wir", "tre\u00b7ten", "hin\u00b7aus"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APZR"], "meter": "---+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Vom Schatten der goldenen W\u00e4lder,", "tokens": ["Vom", "Schat\u00b7ten", "der", "gol\u00b7de\u00b7nen", "W\u00e4l\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Gro\u00df in ein Abendrot,", "tokens": ["Gro\u00df", "in", "ein", "A\u00b7ben\u00b7drot", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Das dir ber\u00fchrt sanft die Stirn.", "tokens": ["Das", "dir", "be\u00b7r\u00fchrt", "sanft", "die", "Stirn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "VVFIN", "ADJD", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.9": {"line.1": {"text": "G\u00f6ttliche Trauer,", "tokens": ["G\u00f6tt\u00b7li\u00b7che", "Trau\u00b7er", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Schweige der ewigen Liebe.", "tokens": ["Schwei\u00b7ge", "der", "e\u00b7wi\u00b7gen", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Hebe den Krug herauf,", "tokens": ["He\u00b7be", "den", "Krug", "her\u00b7auf", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "PTKVZ", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Trinke den Schlaf.", "tokens": ["Trin\u00b7ke", "den", "Schlaf", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.10": {"line.1": {"text": "Einmal am Ende zu stehen,", "tokens": ["Ein\u00b7mal", "am", "En\u00b7de", "zu", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Wo Meer in gelblichen Flecken", "tokens": ["Wo", "Meer", "in", "gelb\u00b7li\u00b7chen", "Fle\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Leise schwimmt schon herein", "tokens": ["Lei\u00b7se", "schwimmt", "schon", "her\u00b7ein"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ADV", "PTKVZ"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Zu der September Bucht.", "tokens": ["Zu", "der", "Sep\u00b7tem\u00b7ber", "Bucht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}}, "stanza.11": {"line.1": {"text": "Oben zu ruhn", "tokens": ["O\u00b7ben", "zu", "ruhn"], "token_info": ["word", "word", "word"], "pos": ["ADV", "PTKZU", "VVINF"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Im Hause der durstigen Blumen,", "tokens": ["Im", "Hau\u00b7se", "der", "durs\u00b7ti\u00b7gen", "Blu\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri