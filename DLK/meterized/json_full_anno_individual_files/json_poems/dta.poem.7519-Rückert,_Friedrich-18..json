{"dta.poem.7519": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "18.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich streue Perlen aus, und Niemand achtet drauf;", "tokens": ["Ich", "streu\u00b7e", "Per\u00b7len", "aus", ",", "und", "Nie\u00b7mand", "ach\u00b7tet", "drauf", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "PTKVZ", "$,", "KON", "PIS", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bald streu' ich keine mehr, dann lest ihr diese auf.", "tokens": ["Bald", "streu'", "ich", "kei\u00b7ne", "mehr", ",", "dann", "lest", "ihr", "die\u00b7se", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "PIS", "$,", "ADV", "VVFIN", "PPER", "PDS", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wenn du erkennen kannst, wie vielfach ist das Eins,", "tokens": ["Wenn", "du", "er\u00b7ken\u00b7nen", "kannst", ",", "wie", "viel\u00b7fach", "ist", "das", "Eins", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVINF", "VMFIN", "$,", "PWAV", "ADV", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "F\u00e4llt mit der Vielheit ein die ganze Welt des Scheins.", "tokens": ["F\u00e4llt", "mit", "der", "Viel\u00b7heit", "ein", "die", "gan\u00b7ze", "Welt", "des", "Scheins", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ART", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Das Eins ist zweierlei, das Eine und das Zweite,", "tokens": ["Das", "Eins", "ist", "zwei\u00b7er\u00b7lei", ",", "das", "Ei\u00b7ne", "und", "das", "Zwei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PIS", "$,", "PRELS", "ART", "KON", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Zwei sind Eines mit sich selbst im Widerstreite.", "tokens": ["Die", "Zwei", "sind", "Ei\u00b7nes", "mit", "sich", "selbst", "im", "Wi\u00b7der\u00b7strei\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "CARD", "VAFIN", "PIS", "APPR", "PRF", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Das eine Eins ist hier, das andre Eins ist dort,", "tokens": ["Das", "ei\u00b7ne", "Eins", "ist", "hier", ",", "das", "and\u00b7re", "Eins", "ist", "dort", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "VAFIN", "ADV", "$,", "ART", "ADJA", "NN", "VAFIN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die tauschen unter sich den Namen und den Ort.", "tokens": ["Die", "tau\u00b7schen", "un\u00b7ter", "sich", "den", "Na\u00b7men", "und", "den", "Ort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "APPR", "PRF", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Blick' in den Spiegel und verdoppelt siehst du dich;", "tokens": ["Blick'", "in", "den", "Spie\u00b7gel", "und", "ver\u00b7dop\u00b7pelt", "siehst", "du", "dich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "KON", "VVPP", "VVFIN", "PPER", "PRF", "$."], "meter": "++-+-+-+-+-+", "measure": "unknown.measure.septa"}, "line.2": {"text": "Blick weg, und auf in Eins l\u00f6st sich das Doppel-Ich.", "tokens": ["Blick", "weg", ",", "und", "auf", "in", "Eins", "l\u00f6st", "sich", "das", "Dop\u00b7pel\u00b7Ich", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "KON", "APPR", "APPR", "NN", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Im Spiegel ist dein Bild, du selber aber bist", "tokens": ["Im", "Spie\u00b7gel", "ist", "dein", "Bild", ",", "du", "sel\u00b7ber", "a\u00b7ber", "bist"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VAFIN", "PPOSAT", "NN", "$,", "PPER", "ADV", "ADV", "VAFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nur dessen Spiegelbild, der Aller Urbild ist.", "tokens": ["Nur", "des\u00b7sen", "Spie\u00b7gel\u00b7bild", ",", "der", "Al\u00b7ler", "Ur\u00b7bild", "ist", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "ART", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Wenn in den Spiegel er mit Liebesblicken schaut,", "tokens": ["Wenn", "in", "den", "Spie\u00b7gel", "er", "mit", "Lie\u00b7bes\u00b7bli\u00b7cken", "schaut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Entsteht ein Weltbild, das, blickt er hinweg, zerthaut.", "tokens": ["Ent\u00b7steht", "ein", "Welt\u00b7bild", ",", "das", ",", "blickt", "er", "hin\u00b7weg", ",", "zer\u00b7thaut", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "PDS", "$,", "VVFIN", "PPER", "PTKVZ", "$,", "VVPP", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.8": {"line.1": {"text": "Drum preist die Liebe, die ihm stets den Spiegel h\u00e4lt,", "tokens": ["Drum", "preist", "die", "Lie\u00b7be", ",", "die", "ihm", "stets", "den", "Spie\u00b7gel", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "NN", "$,", "PRELS", "PPER", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df ihm, dem Einen, sich als zwei zu schaun gef\u00e4llt!", "tokens": ["Da\u00df", "ihm", ",", "dem", "Ei\u00b7nen", ",", "sich", "als", "zwei", "zu", "schaun", "ge\u00b7f\u00e4llt", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "PRELS", "ART", "$,", "PRF", "KOKOM", "CARD", "PTKZU", "VVINF", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Das Eins ist zweierlei, hier Einheit unentzweit,", "tokens": ["Das", "Eins", "ist", "zwei\u00b7er\u00b7lei", ",", "hier", "Ein\u00b7heit", "un\u00b7ent\u00b7zweit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PIS", "$,", "ADV", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": " dort in der Zweiheit hergestellte Einigkeit.", "tokens": ["dort", "in", "der", "Zwei\u00b7heit", "her\u00b7ge\u00b7stell\u00b7te", "Ei\u00b7nig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Eins ist der Punkt, der Kreis das andre, und das dritte", "tokens": ["Eins", "ist", "der", "Punkt", ",", "der", "Kreis", "das", "and\u00b7re", ",", "und", "das", "drit\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VAFIN", "ART", "NN", "$,", "ART", "NN", "ART", "ADJA", "$,", "KON", "ART", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ist zwischen Kreis und Punkt die vieltheilbare Mitte.", "tokens": ["Ist", "zwi\u00b7schen", "Kreis", "und", "Punkt", "die", "vielt\u00b7heil\u00b7ba\u00b7re", "Mit\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NN", "KON", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.11": {"line.1": {"text": "Was ist der Kreis? Ein Punkt, der um sich selber kreist,", "tokens": ["Was", "ist", "der", "Kreis", "?", "Ein", "Punkt", ",", "der", "um", "sich", "sel\u00b7ber", "kreist", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "$.", "ART", "NN", "$,", "PRELS", "APPR", "PRF", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und seinen Umfang w\u00f6lbt, wie seinen Leib der Geist.", "tokens": ["Und", "sei\u00b7nen", "Um\u00b7fang", "w\u00f6lbt", ",", "wie", "sei\u00b7nen", "Leib", "der", "Geist", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "$,", "PWAV", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.12": {"line.1": {"text": "Zieh einen weitsten Kreis, und r\u00fcck' ihn weit ins Ferne,", "tokens": ["Zieh", "ei\u00b7nen", "weits\u00b7ten", "Kreis", ",", "und", "r\u00fcck", "ihn", "weit", "ins", "Fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,", "KON", "VVFIN", "PPER", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sogleich erscheint er dir als Punkt, gleich jedem Sterne.", "tokens": ["Sog\u00b7leich", "er\u00b7scheint", "er", "dir", "als", "Punkt", ",", "gleich", "je\u00b7dem", "Ster\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "KOUS", "NN", "$,", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.13": {"line.1": {"text": "Setz' einen kleinsten Punkt, ob unsichtbar er w\u00e4re,", "tokens": ["Setz'", "ei\u00b7nen", "kleins\u00b7ten", "Punkt", ",", "ob", "un\u00b7sicht\u00b7bar", "er", "w\u00e4\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,", "KOUS", "ADJD", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Brauch' ein Vergr\u00f6\u00dfrungsglas, und er erw\u00e4chst zur Sf\u00e4re.", "tokens": ["Brauch'", "ein", "Ver\u00b7gr\u00f6\u00df\u00b7rungs\u00b7glas", ",", "und", "er", "er\u00b7w\u00e4chst", "zur", "Sf\u00e4\u00b7re", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "KON", "PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.14": {"line.1": {"text": "Ins Wasser wirf den Stein, und sieh wie sich erweitern", "tokens": ["Ins", "Was\u00b7ser", "wirf", "den", "Stein", ",", "und", "sieh", "wie", "sich", "er\u00b7wei\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$,", "KON", "VVFIN", "KOKOM", "PRF", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Aus Kreisen Kreise, um im weitesten zu scheitern.", "tokens": ["Aus", "Krei\u00b7sen", "Krei\u00b7se", ",", "um", "im", "wei\u00b7tes\u00b7ten", "zu", "schei\u00b7tern", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "$,", "KOUI", "APPRART", "ADJA", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.15": {"line.1": {"text": "Eins ob der Kreis zerflo\u00df, Eins ob er nie entstand,", "tokens": ["Eins", "ob", "der", "Kreis", "zer\u00b7flo\u00df", ",", "Eins", "ob", "er", "nie", "ent\u00b7stand", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOUS", "ART", "NN", "VVFIN", "$,", "NN", "KOUS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Denn Eins ist Alles, wenn der Schein der Zweiheit schwand.", "tokens": ["Denn", "Eins", "ist", "Al\u00b7les", ",", "wenn", "der", "Schein", "der", "Zwei\u00b7heit", "schwand", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "PIS", "$,", "KOUS", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.16": {"line.1": {"text": "Die Zwei ist Zweifel, Zwist, ist Zwietracht, Zwiespalt, Zwitter;", "tokens": ["Die", "Zwei", "ist", "Zwei\u00b7fel", ",", "Zwist", ",", "ist", "Zwiet\u00b7racht", ",", "Zwies\u00b7palt", ",", "Zwit\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "CARD", "VAFIN", "NN", "$,", "NN", "$,", "VAFIN", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Zwei ist Zwillingsfrucht am Zweige s\u00fc\u00df und bitter.", "tokens": ["Die", "Zwei", "ist", "Zwil\u00b7lings\u00b7frucht", "am", "Zwei\u00b7ge", "s\u00fc\u00df", "und", "bit\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "CARD", "VAFIN", "NN", "APPRART", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.17": {"line.1": {"text": "Wenn Zwietracht Eintracht wird, und Einfalt das Zwiefalte,", "tokens": ["Wenn", "Zwiet\u00b7racht", "Ein\u00b7tracht", "wird", ",", "und", "Ein\u00b7falt", "das", "Zwie\u00b7fal\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "NN", "VAFIN", "$,", "KON", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+--+--", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Dann wird der Schaden heil am alten Weltzwiespalte.", "tokens": ["Dann", "wird", "der", "Scha\u00b7den", "heil", "am", "al\u00b7ten", "Welt\u00b7zwies\u00b7pal\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADJD", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}