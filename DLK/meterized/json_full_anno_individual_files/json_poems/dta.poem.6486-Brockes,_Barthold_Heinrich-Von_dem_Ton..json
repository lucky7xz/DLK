{"dta.poem.6486": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Ton.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Des Werckzeugs Bau, so Ursach, da\u00df in Ohren", "tokens": ["Des", "Wer\u00b7ck\u00b7zeugs", "Bau", ",", "so", "Ur\u00b7sach", ",", "da\u00df", "in", "Oh\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$,", "ADV", "NN", "$,", "KOUS", "APPR", "NN"], "meter": "--+-+-+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Bey einem jeglichen, durch das Bewegen,", "tokens": ["Bey", "ei\u00b7nem", "jeg\u00b7li\u00b7chen", ",", "durch", "das", "Be\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PIAT", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein andre Art von T\u00f6nen wird gebohren;", "tokens": ["Ein", "and\u00b7re", "Art", "von", "T\u00f6\u00b7nen", "wird", "ge\u00b7boh\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Kan auch in der Musie Ver\u00e4nderung erregen.", "tokens": ["Kan", "auch", "in", "der", "Mu\u00b7sie", "Ver\u00b7\u00e4n\u00b7de\u00b7rung", "er\u00b7re\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "APPR", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die T\u00f6ne, wodurch wir ger\u00fchret seyn,", "tokens": ["Die", "T\u00f6\u00b7ne", ",", "wo\u00b7durch", "wir", "ge\u00b7r\u00fch\u00b7ret", "seyn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "PPER", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Die stimmen, auf verborgne Weise,", "tokens": ["Die", "stim\u00b7men", ",", "auf", "ver\u00b7borg\u00b7ne", "Wei\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Mit unserm Puls-Schlag \u00fcberein.", "tokens": ["Mit", "un\u00b7serm", "Puls\u00b7Schlag", "\u00fc\u00b7be\u00b7re\u00b7in", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Wenn V\u00f6lcker in der Ordnung stehen,", "tokens": ["Wenn", "V\u00f6l\u00b7cker", "in", "der", "Ord\u00b7nung", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Um auf gegebenes Signal:", "tokens": ["Um", "auf", "ge\u00b7ge\u00b7be\u00b7nes", "Sig\u00b7nal", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.10": {"text": "Zur Schlacht zu gehen,", "tokens": ["Zur", "Schlacht", "zu", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.11": {"text": "So w\u00fcrckt bey ihnen \u00fcberall", "tokens": ["So", "w\u00fcrckt", "bey", "ih\u00b7nen", "\u00fc\u00b7be\u00b7rall"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Der Kriegrischen Trompeten-Schall", "tokens": ["Der", "Krie\u00b7gri\u00b7schen", "Trom\u00b7pe\u00b7ten\u00b7Schall"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.13": {"text": "Ein neu und ungewohnt Bewegen.", "tokens": ["Ein", "neu", "und", "un\u00b7ge\u00b7wohnt", "Be\u00b7we\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "KON", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Es f\u00f6rdern und erregen", "tokens": ["Es", "f\u00f6r\u00b7dern", "und", "er\u00b7re\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVINF", "KON", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "Die St\u00f6sse, die sodann in L\u00fcfften wallen,", "tokens": ["Die", "St\u00f6s\u00b7se", ",", "die", "so\u00b7dann", "in", "L\u00fcff\u00b7ten", "wal\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "In Adern des Gebl\u00fctes Lauff.", "tokens": ["In", "A\u00b7dern", "des", "Ge\u00b7bl\u00fc\u00b7tes", "Lauff", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Das Lermen, Schreyen und das Schallen", "tokens": ["Das", "Ler\u00b7men", ",", "Schre\u00b7yen", "und", "das", "Schal\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "NN", "KON", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.18": {"text": "Erregt und weckt den Geist in kalten Nerven auf.", "tokens": ["Er\u00b7regt", "und", "weckt", "den", "Geist", "in", "kal\u00b7ten", "Ner\u00b7ven", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}