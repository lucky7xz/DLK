{"dta.poem.17634": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "256.  Zur\u00fckke sehn ist wieder Verlohren  \n werden.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn du au\u00df Sodom gehst/ und dem Gericht ent-", "tokens": ["Wenn", "du", "au\u00df", "So\u00b7dom", "gehst", "/", "und", "dem", "Ge\u00b7richt", "ent"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "NE", "VVFIN", "$(", "KON", "ART", "NN", "TRUNC"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "fliehest/", "tokens": ["flie\u00b7hest", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "So steht dein Heil darauf da\u00df du nicht rukwerts siehest.", "tokens": ["So", "steht", "dein", "Heil", "da\u00b7rauf", "da\u00df", "du", "nicht", "ruk\u00b7werts", "sie\u00b7hest", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "PAV", "KOUS", "PPER", "PTKNEG", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}}}}}