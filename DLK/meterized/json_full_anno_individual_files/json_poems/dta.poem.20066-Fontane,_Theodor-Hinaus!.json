{"dta.poem.20066": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Hinaus!", "genre": "Lyrik", "period": "N.A.", "pub_year": "1851", "urn": "urn:nbn:de:kobv:b4-200905191321", "language": ["de:0.99"], "booktitle": "Fontane, Theodor: Gedichte. Berlin, 1851."}, "poem": {"stanza.1": {"line.1": {"text": "Ich bin es satt auf Polstern mich zu dehnen,", "tokens": ["Ich", "bin", "es", "satt", "auf", "Pols\u00b7tern", "mich", "zu", "deh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "APPR", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es ekelt mich dies weibergleiche Thun,", "tokens": ["Es", "e\u00b7kelt", "mich", "dies", "wei\u00b7berg\u00b7lei\u00b7che", "Thun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PDS", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich m\u00f6cht im Kampf anspannen alle Sehnen,", "tokens": ["Ich", "m\u00f6cht", "im", "Kampf", "an\u00b7span\u00b7nen", "al\u00b7le", "Seh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPRART", "NN", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mich m\u00fcd und matt an die Lafette lehnen,", "tokens": ["Mich", "m\u00fcd", "und", "matt", "an", "die", "La\u00b7fet\u00b7te", "leh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "KON", "ADJD", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und k\u00e4m der Schlaf auf blo\u00dfer Erde ruhn.", "tokens": ["Und", "k\u00e4m", "der", "Schlaf", "auf", "blo\u00b7\u00dfer", "Er\u00b7de", "ruhn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ich m\u00f6cht hinaus! umbr\u00fcllt von Sturm und", "tokens": ["Ich", "m\u00f6cht", "hin\u00b7aus", "!", "um\u00b7br\u00fcllt", "von", "Sturm", "und"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PTKVZ", "$.", "VVFIN", "APPR", "NN", "KON"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wettern", "tokens": ["Wet\u00b7tern"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "M\u00f6cht ich zu Schiff auf hohem Meere sein;", "tokens": ["M\u00f6cht", "ich", "zu", "Schiff", "auf", "ho\u00b7hem", "Mee\u00b7re", "sein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "NN", "APPR", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Vom Blitz umflammt m\u00f6cht ich den Mast er-", "tokens": ["Vom", "Blitz", "um\u00b7flammt", "m\u00f6cht", "ich", "den", "Mast", "er"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "VMFIN", "PPER", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "klettern,", "tokens": ["klet\u00b7tern", ","], "token_info": ["word", "punct"], "pos": ["VVINF", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.6": {"text": "Und wenn die Wellen unser Schiff zerschmettern,", "tokens": ["Und", "wenn", "die", "Wel\u00b7len", "un\u00b7ser", "Schiff", "zer\u00b7schmet\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Ein k\u00fchner Schwimmer um das Leben frein.", "tokens": ["Ein", "k\u00fch\u00b7ner", "Schwim\u00b7mer", "um", "das", "Le\u00b7ben", "frein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ich m\u00f6cht hinaus! mag schleudern mich die Reise", "tokens": ["Ich", "m\u00f6cht", "hin\u00b7aus", "!", "mag", "schleu\u00b7dern", "mich", "die", "Rei\u00b7se"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PTKVZ", "$.", "VMFIN", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wohin sie will, mir gilt es gleich f\u00fcrwahr;", "tokens": ["Wo\u00b7hin", "sie", "will", ",", "mir", "gilt", "es", "gleich", "f\u00fcr\u00b7wahr", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VMFIN", "$,", "PPER", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Heraus nur endlich aus dem alten Gleise,", "tokens": ["He\u00b7raus", "nur", "end\u00b7lich", "aus", "dem", "al\u00b7ten", "Glei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das Leben steigt mit der Gefahr im Preise, \u2014", "tokens": ["Das", "Le\u00b7ben", "steigt", "mit", "der", "Ge\u00b7fahr", "im", "Prei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "APPRART", "NN", "$,", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Auf denn, hinaus! zu Thaten und Gefahr.", "tokens": ["Auf", "denn", ",", "hin\u00b7aus", "!", "zu", "Tha\u00b7ten", "und", "Ge\u00b7fahr", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "$,", "PTKVZ", "$.", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}