{"dta.poem.10661": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "Epigramma.  \n  An eine vngestaltete Jungfraw.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1624", "urn": "urn:nbn:de:kobv:b4-200905197859", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Teutsche P\u00f6emata und: Aristarchvs Wieder die verachtung Teutscher Sprach. Stra\u00dfburg, 1624."}, "poem": {"stanza.1": {"line.1": {"text": "Die Spiegel sein gantz falsch: dann wann sie richtig wehren/", "tokens": ["Die", "Spie\u00b7gel", "sein", "gantz", "falsch", ":", "dann", "wann", "sie", "rich\u00b7tig", "weh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "ADV", "ADJD", "$.", "ADV", "PWAV", "PPER", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du w\u00fcrdest dir zusehn in keinen nicht begehren.", "tokens": ["Du", "w\u00fcr\u00b7dest", "dir", "zu\u00b7sehn", "in", "kei\u00b7nen", "nicht", "be\u00b7geh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVFIN", "APPR", "PIAT", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}