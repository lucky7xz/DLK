{"textgrid.poem.47390": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Scheidungsbr\u00fccke", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zwischen Zeit und Ewigkeit", "tokens": ["Zwi\u00b7schen", "Zeit", "und", "E\u00b7wig\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Steht die Scheidungsbr\u00fccke,", "tokens": ["Steht", "die", "Schei\u00b7dungs\u00b7br\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "F\u00fcllend mit dem Schreckensglanz", "tokens": ["F\u00fcl\u00b7lend", "mit", "dem", "Schre\u00b7ckens\u00b7glanz"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Die furchtbare L\u00fccke.", "tokens": ["Die", "furcht\u00b7ba\u00b7re", "L\u00fc\u00b7cke", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Wei\u00dft du wohl, wie scharf und fein", "tokens": ["Wei\u00dft", "du", "wohl", ",", "wie", "scharf", "und", "fein"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "$,", "PWAV", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist der Br\u00fccke Bogen?", "tokens": ["Ist", "der", "Br\u00fc\u00b7cke", "Bo\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Wie ein Schwert ist sie gez\u00fcckt,", "tokens": ["Wie", "ein", "Schwert", "ist", "sie", "ge\u00b7z\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie ein Haar gezogen.", "tokens": ["Wie", "ein", "Haar", "ge\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Soll ein Fu\u00df des Menschen gehn", "tokens": ["Soll", "ein", "Fu\u00df", "des", "Men\u00b7schen", "gehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "ART", "NN", "ART", "NN", "VVINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf der schmalen Br\u00fccke,", "tokens": ["Auf", "der", "schma\u00b7len", "Br\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Wo nicht aufzufu\u00dfen hat", "tokens": ["Wo", "nicht", "auf\u00b7zu\u00b7fu\u00b7\u00dfen", "hat"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PTKNEG", "VVPP", "VAFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Raum ein Fu\u00df der M\u00fccke!", "tokens": ["Raum", "ein", "Fu\u00df", "der", "M\u00fc\u00b7cke", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Wer nicht fest dar\u00fcber hin", "tokens": ["Wer", "nicht", "fest", "da\u00b7r\u00fc\u00b7ber", "hin"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "PTKNEG", "ADJD", "PAV", "PTKVZ"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Sich zu schreiten trauet,", "tokens": ["Sich", "zu", "schrei\u00b7ten", "trau\u00b7et", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Hoffe nicht, da\u00df dr\u00fcben ihm", "tokens": ["Hof\u00b7fe", "nicht", ",", "da\u00df", "dr\u00fc\u00b7ben", "ihm"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PTKNEG", "$,", "KOUS", "ADV", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Edens Wonne tauet.", "tokens": ["E\u00b7dens", "Won\u00b7ne", "tau\u00b7et", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Wenn der Frevler angelangt,", "tokens": ["Wenn", "der", "Frev\u00b7ler", "an\u00b7ge\u00b7langt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Steht die Br\u00fcck' und funkelt,", "tokens": ["Steht", "die", "Br\u00fcck", "und", "fun\u00b7kelt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "KON", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Da\u00df sich die Besinnung ganz", "tokens": ["Da\u00df", "sich", "die", "Be\u00b7sin\u00b7nung", "ganz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "ART", "NN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Schwindelnd ihm verdunkelt.", "tokens": ["Schwin\u00b7delnd", "ihm", "ver\u00b7dun\u00b7kelt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Ihn verwirrend, tritt heran", "tokens": ["Ihn", "ver\u00b7wir\u00b7rend", ",", "tritt", "he\u00b7ran"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPER", "VVPP", "$,", "VVFIN", "PTKVZ"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit des Todes Schrecken", "tokens": ["Mit", "des", "To\u00b7des", "Schre\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Das Ged\u00e4chtnis seiner Schuld,", "tokens": ["Das", "Ge\u00b7d\u00e4cht\u00b7nis", "sei\u00b7ner", "Schuld", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Grau'n ihm zu erwecken.", "tokens": ["Grau'n", "ihm", "zu", "er\u00b7we\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "Drunten g\u00e4hnt der Abgrund auf,", "tokens": ["Drun\u00b7ten", "g\u00e4hnt", "der", "Ab\u00b7grund", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und der Seele Beben", "tokens": ["Und", "der", "See\u00b7le", "Be\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Treibet ihn, dem eignen Sturz", "tokens": ["Trei\u00b7bet", "ihn", ",", "dem", "eig\u00b7nen", "Sturz"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Selber zuzustreben.", "tokens": ["Sel\u00b7ber", "zu\u00b7zus\u00b7tre\u00b7ben", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVIZU", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.8": {"line.1": {"text": "Doch, wo ein Gerechter geht,", "tokens": ["Doch", ",", "wo", "ein", "Ge\u00b7rech\u00b7ter", "geht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Schwebt um ihn Vertrauen,", "tokens": ["Schwebt", "um", "ihn", "Ver\u00b7trau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Das den Abgrund ihm entr\u00fcckt", "tokens": ["Das", "den", "Ab\u00b7grund", "ihm", "ent\u00b7r\u00fcckt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "ART", "NN", "PPER", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und ihm l\u00e4\u00dft nicht grauen.", "tokens": ["Und", "ihm", "l\u00e4\u00dft", "nicht", "grau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.9": {"line.1": {"text": "Hoffnung hebet seinen Blick,", "tokens": ["Hoff\u00b7nung", "he\u00b7bet", "sei\u00b7nen", "Blick", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Liebe gibt ihm Schwinge,", "tokens": ["Lie\u00b7be", "gibt", "ihm", "Schwin\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Glaube l\u00e4chelt, da\u00df sein Geist", "tokens": ["Glau\u00b7be", "l\u00e4\u00b7chelt", ",", "da\u00df", "sein", "Geist"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "$,", "KOUS", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Selig vorw\u00e4rts dringe.", "tokens": ["Se\u00b7lig", "vor\u00b7w\u00e4rts", "drin\u00b7ge", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.10": {"line.1": {"text": "Seiner guten Werke Duft", "tokens": ["Sei\u00b7ner", "gu\u00b7ten", "Wer\u00b7ke", "Duft"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wird zu Goldwolkr\u00e4ndern,", "tokens": ["Wird", "zu", "Gold\u00b7wol\u00b7kr\u00e4n\u00b7dern", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Da\u00df sich ihm die Br\u00fccke rings", "tokens": ["Da\u00df", "sich", "ihm", "die", "Br\u00fc\u00b7cke", "rings"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "PPER", "ART", "NN", "ADV"], "meter": "----+-+", "measure": "unknown.measure.di"}, "line.4": {"text": "Schm\u00fccke mit Gel\u00e4ndern.", "tokens": ["Schm\u00fc\u00b7cke", "mit", "Ge\u00b7l\u00e4n\u00b7dern", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.11": {"line.1": {"text": "Auf der Br\u00fccke geht er hin,", "tokens": ["Auf", "der", "Br\u00fc\u00b7cke", "geht", "er", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Unter seinem Fu\u00dfe", "tokens": ["Un\u00b7ter", "sei\u00b7nem", "Fu\u00b7\u00dfe"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "lin