{"textgrid.poem.48413": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "5. Auf eines Kindes Ableben", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wo ist der G\u00e4rten Pracht, der Blumen K\u00f6nigin,", "tokens": ["Wo", "ist", "der", "G\u00e4r\u00b7ten", "Pracht", ",", "der", "Blu\u00b7men", "K\u00f6\u00b7ni\u00b7gin", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "NN", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "der Augen liebe Lust, die Anemone hin,", "tokens": ["der", "Au\u00b7gen", "lie\u00b7be", "Lust", ",", "die", "A\u00b7ne\u00b7mo\u00b7ne", "hin", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "$,", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "die so nur gestern noch in ihrem Purpur-Munde", "tokens": ["die", "so", "nur", "ge\u00b7stern", "noch", "in", "ih\u00b7rem", "Pur\u00b7pur\u00b7Mun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ADV", "ADV", "ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "und keuschem Angesicht' allhier zugegen stunde?", "tokens": ["und", "keu\u00b7schem", "An\u00b7ge\u00b7sicht'", "all\u00b7hier", "zu\u00b7ge\u00b7gen", "stun\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ADV", "ADJD", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wo ist denn heut' ihr Schmuck, ihr wollustvolles H\u00e4upt", 