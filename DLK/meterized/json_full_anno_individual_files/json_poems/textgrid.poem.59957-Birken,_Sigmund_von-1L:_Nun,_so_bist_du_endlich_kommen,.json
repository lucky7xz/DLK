uns viel Fr\u00fcchte zeig'!", "tokens": ["Da\u00df", "er", "uns", "viel", "Fr\u00fcch\u00b7te", "zeig'", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "PIAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Unsre Schuld mit Gnad' anschaue,", "tokens": ["Uns\u00b7re", "Schuld", "mit", "Gnad'", "an\u00b7schau\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Da\u00df die Axt in deiner Hand", "tokens": ["Da\u00df", "die", "Axt", "in", "dei\u00b7ner", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ihn im Zorn uns nicht abhaue,", "tokens": ["Ihn", "im", "Zorn", "uns", "nicht", "ab\u00b7hau\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "PPER", "PTKNEG", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und der Streit ersteh' im Land!", "tokens": ["Und", "der", "Streit", "er\u00b7steh'", "im", "Land", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}}}}