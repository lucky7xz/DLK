{"dta.poem.19358": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "91.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was hei\u00dft dich, wie dich selbst, jedweden Menschen achten?", "tokens": ["Was", "hei\u00dft", "dich", ",", "wie", "dich", "selbst", ",", "jed\u00b7we\u00b7den", "Men\u00b7schen", "ach\u00b7ten", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "PWAV", "PPER", "ADV", "$,", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das Menschenangesicht! Du darfst es nur betrachten.", "tokens": ["Das", "Men\u00b7schen\u00b7an\u00b7ge\u00b7sicht", "!", "Du", "darfst", "es", "nur", "be\u00b7trach\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "PPER", "VVFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Du siehst dein eigen Bild, und hast dich selbst entehrt,", "tokens": ["Du", "siehst", "dein", "ei\u00b7gen", "Bild", ",", "und", "hast", "dich", "selbst", "ent\u00b7ehrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJD", "NN", "$,", "KON", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn du die Achtung, die es fordert, ihm verwehrt.", "tokens": ["Wenn", "du", "die", "Ach\u00b7tung", ",", "die", "es", "for\u00b7dert", ",", "ihm", "ver\u00b7wehrt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Aus jedem Angesicht blickt menschliche Vernumft,", "tokens": ["Aus", "je\u00b7dem", "An\u00b7ge\u00b7sicht", "blickt", "menschli\u00b7che", "Ver\u00b7numft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Das Gotteslicht, wieauch getr\u00fcbt, ged\u00e4mpft, verdumft.", "tokens": ["Das", "Got\u00b7tes\u00b7licht", ",", "wie\u00b7auch", "ge\u00b7tr\u00fcbt", ",", "ge\u00b7d\u00e4mpft", ",", "ver\u00b7dumft", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "VVPP", "$,", "VVPP", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Wenn du es nicht erkennst, so liegt die Schuld an dir;", "tokens": ["Wenn", "du", "es", "nicht", "er\u00b7kennst", ",", "so", "liegt", "die", "Schuld", "an", "dir", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PTKNEG", "VVFIN", "$,", "ADV", "VVFIN", "ART", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du siehst das Thier nur, weil du selbst nur siehst als Thier.", "tokens": ["Du", "siehst", "das", "Thier", "nur", ",", "weil", "du", "selbst", "nur", "siehst", "als", "Thier", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "$,", "KOUS", "PPER", "ADV", "ADV", "ADV", "KOUS", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Des Thieres Seyn ist Kampf, des Menschen Geist ist Frieden;", "tokens": ["Des", "Thie\u00b7res", "Seyn", "ist", "Kampf", ",", "des", "Men\u00b7schen", "Geist", "ist", "Frie\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "VAFIN", "NN", "$,", "ART", "NN", "NN", "VAFIN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sind wir erst Menschen ganz, so ist der Kampf geschieden.", "tokens": ["Sind", "wir", "erst", "Men\u00b7schen", "ganz", ",", "so", "ist", "der", "Kampf", "ge\u00b7schie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "NN", "ADV", "$,", "ADV", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}