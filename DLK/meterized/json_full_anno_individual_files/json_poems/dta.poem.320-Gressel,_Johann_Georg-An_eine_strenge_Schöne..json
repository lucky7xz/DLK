{"dta.poem.320": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "An eine strenge Sch\u00f6ne.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Allzu strenge Grausahmkeit", "tokens": ["All\u00b7zu", "stren\u00b7ge", "Grau\u00b7sahm\u00b7keit"], "token_info": ["word", "word", "word"], "pos": ["PTKA", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Mu\u00df die krancke Seele leiden;", "tokens": ["Mu\u00df", "die", "kran\u00b7cke", "See\u00b7le", "lei\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sie vergeht in ihrer Pein/", "tokens": ["Sie", "ver\u00b7geht", "in", "ih\u00b7rer", "Pein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wasser kan den Fels durchbohren/", "tokens": ["Was\u00b7ser", "kan", "den", "Fels", "durch\u00b7boh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "ART", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Aber ach! ich bin verlohren", "tokens": ["A\u00b7ber", "ach", "!", "ich", "bin", "ver\u00b7loh\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "$.", "PPER", "VAFIN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Deine Brust ist mehr als Stein.", "tokens": ["Dei\u00b7ne", "Brust", "ist", "mehr", "als", "Stein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "KOUS", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Allzu strenge Grausahmkeit", "tokens": ["All\u00b7zu", "stren\u00b7ge", "Grau\u00b7sahm\u00b7keit"], "token_info": ["word", "word", "word"], "pos": ["PTKA", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Mu\u00df die matte Seele leiden.", "tokens": ["Mu\u00df", "die", "mat\u00b7te", "See\u00b7le", "lei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Du bist zwar sch\u00f6n", "tokens": ["Du", "bist", "zwar", "sch\u00f6n"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADJD"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Und lieblich anzusehn;", "tokens": ["Und", "lieb\u00b7lich", "an\u00b7zu\u00b7sehn", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVIZU", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Deiner Augen Wunder-Pracht", "tokens": ["Dei\u00b7ner", "Au\u00b7gen", "Wun\u00b7der\u00b7Pracht"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Hat meinen Geist verliebt gemacht;", "tokens": ["Hat", "mei\u00b7nen", "Geist", "ver\u00b7liebt", "ge\u00b7macht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "VVPP", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Auf den Wangen bl\u00fchen Rosen/", "tokens": ["Auf", "den", "Wan\u00b7gen", "bl\u00fc\u00b7hen", "Ro\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Die Wollust da wie Thau zerflie\u00dft.", "tokens": ["Die", "Wol\u00b7lust", "da", "wie", "Thau", "zer\u00b7flie\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "KOKOM", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Silber weisse Perlen kr\u00f6hnen", "tokens": ["Sil\u00b7ber", "weis\u00b7se", "Per\u00b7len", "kr\u00f6h\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Das zarte Kinn.", "tokens": ["Das", "zar\u00b7te", "Kinn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Die Anmuth k\u00fc\u00dft", "tokens": ["Die", "An\u00b7muth", "k\u00fc\u00dft"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Die glatten Wangen/", "tokens": ["Die", "glat\u00b7ten", "Wan\u00b7gen", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.11": {"text": "So mit den reinsten Liljen prangen/", "tokens": ["So", "mit", "den", "reins\u00b7ten", "Lil\u00b7jen", "pran\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Und Milch und Schnee verh\u00f6hnen.", "tokens": ["Und", "Milch", "und", "Schnee", "ver\u00b7h\u00f6h\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Der Busen f\u00e4hrt dahin", "tokens": ["Der", "Bu\u00b7sen", "f\u00e4hrt", "da\u00b7hin"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PAV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.14": {"text": "Als wie ein weisses Meer/", "tokens": ["Als", "wie", "ein", "weis\u00b7ses", "Meer", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.15": {"text": "Die Hertzen w\u00fcnschen sehr", "tokens": ["Die", "Hert\u00b7zen", "w\u00fcn\u00b7schen", "sehr"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.16": {"text": "Denselben liebzukosen.", "tokens": ["Den\u00b7sel\u00b7ben", "lieb\u00b7zu\u00b7ko\u00b7sen", "."], "token_info": ["word", "word", "punct"], "pos": ["PDS", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.17": {"text": "So bist du sch\u00f6n", "tokens": ["So", "bist", "du", "sch\u00f6n"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADJD"], "meter": "-+-+", "measure": "iambic.di"}, "line.18": {"text": "Und lieblich anzusehn/", "tokens": ["Und", "lieb\u00b7lich", "an\u00b7zu\u00b7sehn", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVIZU", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.19": {"text": "Doch deine Grausamkeit/", "tokens": ["Doch", "dei\u00b7ne", "Grau\u00b7sam\u00b7keit", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.20": {"text": "Der du dich gantz geweiht", "tokens": ["Der", "du", "dich", "gantz", "ge\u00b7weiht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "PRF", "ADV", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.21": {"text": "Macht mehr ein Tyger-Thier", "tokens": ["Macht", "mehr", "ein", "Ty\u00b7ger\u00b7Thier"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADV", "ART", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.22": {"text": "Als wie ein Wunder-Bild aus dir.", "tokens": ["Als", "wie", "ein", "Wun\u00b7der\u00b7Bild", "aus", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "ART", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Sch\u00f6ne Augen zwingt die Blicke", "tokens": ["Sch\u00f6\u00b7ne", "Au\u00b7gen", "zwingt", "die", "Bli\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df sie nicht so strenge seyn/", "tokens": ["Da\u00df", "sie", "nicht", "so", "stren\u00b7ge", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADV", "VVFIN", "VAINF", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Kehrt den/ Blitz in eine Sonne", "tokens": ["Kehrt", "den", "/", "Blitz", "in", "ei\u00b7ne", "Son\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "ART", "$(", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "H\u00fclt die dunckeln Strahlen ein.", "tokens": ["H\u00fclt", "die", "dun\u00b7ckeln", "Strah\u00b7len", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "So erblick\u2019 ich meine Wonne", "tokens": ["So", "er\u00b7blick'", "ich", "mei\u00b7ne", "Won\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mit erw\u00fcnschetem Gel\u00fccke.", "tokens": ["Mit", "er\u00b7w\u00fcn\u00b7sche\u00b7tem", "Ge\u00b7l\u00fc\u00b7cke", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "+-++--+-", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "Sch\u00f6ne Augen zwingt die Blicke", "tokens": ["Sch\u00f6\u00b7ne", "Au\u00b7gen", "zwingt", "die", "Bli\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Da\u00df sie nicht so strenge seyn.", "tokens": ["Da\u00df", "sie", "nicht", "so", "stren\u00b7ge", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADV", "VVFIN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Was n\u00fctzt dir meine Quaal?", "tokens": ["Was", "n\u00fctzt", "dir", "mei\u00b7ne", "Qua\u00b7al", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was bringet dir mein Leiden?", "tokens": ["Was", "brin\u00b7get", "dir", "mein", "Lei\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Nichts! spricht dein Mund", "tokens": ["Nichts", "!", "spricht", "dein", "Mund"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PIS", "$.", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Und gibt dadurch sein Unrecht kund.", "tokens": ["Und", "gibt", "da\u00b7durch", "sein", "Un\u00b7recht", "kund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PAV", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sey nicht mehr Stein und Stahl/", "tokens": ["Sey", "nicht", "mehr", "Stein", "und", "Stahl", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "PIAT", "NN", "KON", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Liebe den/ der dich verehret", "tokens": ["Lie\u00b7be", "den", "/", "der", "dich", "ver\u00b7eh\u00b7ret"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ART", "$(", "PRELS", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und so reine Flammen nehret", "tokens": ["Und", "so", "rei\u00b7ne", "Flam\u00b7men", "neh\u00b7ret"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Da\u00df er sich dadurch verzehret.", "tokens": ["Da\u00df", "er", "sich", "da\u00b7durch", "ver\u00b7zeh\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "PAV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Du must doch endlich lieben!", "tokens": ["Du", "must", "doch", "end\u00b7lich", "lie\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Darum so fange an", "tokens": ["Da\u00b7rum", "so", "fan\u00b7ge", "an"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "ADV", "VVFIN", "PTKVZ"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Den Wechsel auszu\u00fcben.", "tokens": ["Den", "Wech\u00b7sel", "aus\u00b7zu\u00b7\u00fc\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVIZU", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Wenn Zeit und Jahre", "tokens": ["Wenn", "Zeit", "und", "Jah\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "NN", "KON", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.13": {"text": "Den Leib zur Bahre", "tokens": ["Den", "Leib", "zur", "Bah\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.14": {"text": "Und ins Grab bescheiden/", "tokens": ["Und", "ins", "Grab", "be\u00b7schei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVINF", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.15": {"text": "Alsdenn so ists mit aller Lieb und Gunst gethan.", "tokens": ["Als\u00b7denn", "so", "ists", "mit", "al\u00b7ler", "Lieb", "und", "Gunst", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "APPR", "PIAT", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Oder meynest du vielleicht", "tokens": ["O\u00b7der", "mey\u00b7nest", "du", "viel\u00b7leicht"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.17": {"text": "Nicht zu r\u00e4chen trachten?", "tokens": ["Nicht", "zu", "r\u00e4\u00b7chen", "trach\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PTKZU", "VVINF", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.18": {"text": "Wenn dem also? so bist du \u00fcbel dran.", "tokens": ["Wenn", "dem", "al\u00b7so", "?", "so", "bist", "du", "\u00fc\u00b7bel", "dran", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADV", "$.", "ADV", "VAFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Stoltze Sinnen", "tokens": ["Stolt\u00b7ze", "Sin\u00b7nen"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "M\u00fcssen gleich dem Wachs zerrinnen/", "tokens": ["M\u00fcs\u00b7sen", "gleich", "dem", "Wachs", "zer\u00b7rin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "So der Sonnen nahe steht;", "tokens": ["So", "der", "Son\u00b7nen", "na\u00b7he", "steht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Es kan ", "tokens": ["Es", "kan"], "token_info": ["word", "word"], "pos": ["PPER", "VMFIN"], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "Alsbald ein kaltes Hertz entz\u00fcnden/", "tokens": ["Als\u00b7bald", "ein", "kal\u00b7tes", "Hertz", "ent\u00b7z\u00fcn\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wann wird das Zunder glim\u0303end finden/", "tokens": ["Wann", "wird", "das", "Zun\u00b7der", "glim\u0303end", "fin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "ADJD", "VVINF", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Wann er durch die Seele geht.", "tokens": ["Wann", "er", "durch", "die", "See\u00b7le", "geht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}