{"dta.poem.21901": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "4. Man mu\u00df gantz G\u00f6ttlich seyn.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1657", "urn": "urn:nbn:de:kobv:b4-200905198292", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Geistreiche Sinn- vnd Schlussrime. Wien, 1657."}, "poem": {"stanza.1": {"line.1": {"text": "Herr es gen\u00fcgt mir nicht/ da\u00df ich dir Englisch diene", "tokens": ["Herr", "es", "ge\u00b7n\u00fcgt", "mir", "nicht", "/", "da\u00df", "ich", "dir", "Eng\u00b7lisch", "die\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "VVFIN", "PPER", "PTKNEG", "$(", "KOUS", "PPER", "PPER", "NN", "PDAT"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Und in Vollkommenheit der G\u00f6tter f\u00fcr dir Gr\u00fcne:", "tokens": ["Und", "in", "Voll\u00b7kom\u00b7men\u00b7heit", "der", "G\u00f6t\u00b7ter", "f\u00fcr", "dir", "Gr\u00fc\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "ART", "NN", "APPR", "PPER", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Es ist mir vil zuschlecht/ und meinem Geist zu klein:", "tokens": ["Es", "ist", "mir", "vil", "zu\u00b7schlecht", "/", "und", "mei\u00b7nem", "Geist", "zu", "klein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVPP", "$(", "KON", "PPOSAT", "NN", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wer Dir recht dienen wil mu\u00df mehr als G\u00f6ttlich seyn.", "tokens": ["Wer", "Dir", "recht", "die\u00b7nen", "wil", "mu\u00df", "mehr", "als", "G\u00f6tt\u00b7lich", "seyn", "."], "token_info": ["word