{"textgrid.poem.35239": {"metadata": {"author": {"name": "Harsd\u00f6rffer, Georg Philipp", "birth": "N.A.", "death": "N.A."}, "title": "1L: In den gr\u00fcnlich falben Matten,", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In den gr\u00fcnlich falben Matten,", "tokens": ["In", "den", "gr\u00fcn\u00b7lich", "fal\u00b7ben", "Mat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Unter einer Linde Schatten,", "tokens": ["Un\u00b7ter", "ei\u00b7ner", "Lin\u00b7de", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Hat ein m\u00fcder Wandersmann", "tokens": ["Hat", "ein", "m\u00fc\u00b7der", "Wan\u00b7ders\u00b7mann"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Seine Laute hingeleget,", "tokens": ["Sei\u00b7ne", "Lau\u00b7te", "hin\u00b7ge\u00b7le\u00b7get", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Weil er, von dem Schlaf erreget,", "tokens": ["Weil", "er", ",", "von", "dem", "Schlaf", "er\u00b7re\u00b7get", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mund und Augen zugethan.", "tokens": ["Mund", "und", "Au\u00b7gen", "zu\u00b7ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Auf den nah geleg'nen Auen", "tokens": ["Auf", "den", "nah", "ge\u00b7leg'\u00b7nen", "Au\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJD", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Weidet in dem k\u00fchlen Thauen", "tokens": ["Wei\u00b7det", "in", "dem", "k\u00fch\u00b7len", "Thau\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ein darob erstaunter Knab'.", "tokens": ["Ein", "da\u00b7rob", "er\u00b7staun\u00b7ter", "Knab'", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PAV", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Als er nun nichts mehr vernommen,", "tokens": ["Als", "er", "nun", "nichts", "mehr", "ver\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PIS", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ist er n\u00e4her beigekommen,", "tokens": ["Ist", "er", "n\u00e4\u00b7her", "bei\u00b7ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Hinterlassend seinen Stab.", "tokens": ["Hin\u00b7ter\u00b7las\u00b7send", "sei\u00b7nen", "Stab", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Er kniet bei der Laute nieder,", "tokens": ["Er", "kniet", "bei", "der", "Lau\u00b7te", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die zuvor so holde Lieder", "tokens": ["Die", "zu\u00b7vor", "so", "hol\u00b7de", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ADV", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und den wunderreinen Klang,", "tokens": ["Und", "den", "wun\u00b7der\u00b7rei\u00b7nen", "Klang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Diesen Knaben zu beth\u00f6ren,", "tokens": ["Die\u00b7sen", "Kna\u00b7ben", "zu", "be\u00b7th\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "In den L\u00fcften lassen h\u00f6ren", "tokens": ["In", "den", "L\u00fcf\u00b7ten", "las\u00b7sen", "h\u00f6\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVINF", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Durch den strengen", "tokens": ["Durch", "den", "stren\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "ADJA"], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.4": {"line.1": {"text": "Er wollt' dem Geh\u00f6r nicht trauen,", "tokens": ["Er", "wollt'", "dem", "Ge\u00b7h\u00f6r", "nicht", "trau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und mit seinen Augen schauen,", "tokens": ["Und", "mit", "sei\u00b7nen", "Au\u00b7gen", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie des stummen Holzes Stern", "tokens": ["Wie", "des", "stum\u00b7men", "Hol\u00b7zes", "Stern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "K\u00f6nnte sonder Sinn und Leben", "tokens": ["K\u00f6nn\u00b7te", "son\u00b7der", "Sinn", "und", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PIAT", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "So beliebte Stimme geben,", "tokens": ["So", "be\u00b7lieb\u00b7te", "Stim\u00b7me", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wollt' er selber schauen gern.", "tokens": ["Wollt'", "er", "sel\u00b7ber", "schau\u00b7en", "gern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "VVFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Als er nun nicht m\u00f6gen sehen,", "tokens": ["Als", "er", "nun", "nicht", "m\u00f6\u00b7gen", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PTKNEG", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie der Klang pflegt zu geschehen,", "tokens": ["Wie", "der", "Klang", "pflegt", "zu", "ge\u00b7sche\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "R\u00fchret er die Saiten an.", "tokens": ["R\u00fch\u00b7ret", "er", "die", "Sai\u00b7ten", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Bald die Laute murmelnd klagte,", "tokens": ["Bald", "die", "Lau\u00b7te", "mur\u00b7melnd", "klag\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVPP", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und dem Wandersmann ansagte,", "tokens": ["Und", "dem", "Wan\u00b7ders\u00b7mann", "an\u00b7sag\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.6": {"text": "Was der Hirtenknab' gethan.", "tokens": ["Was", "der", "Hir\u00b7ten\u00b7knab'", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Sind nicht in des Holzes Kr\u00fcmmen", "tokens": ["Sind", "nicht", "in", "des", "Hol\u00b7zes", "Kr\u00fcm\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PTKNEG", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Aller V\u00f6gel zarte Stimmen,", "tokens": ["Al\u00b7ler", "V\u00f6\u00b7gel", "zar\u00b7te", "Stim\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die doch niemand hier beschaut?", "tokens": ["Die", "doch", "nie\u00b7mand", "hier", "be\u00b7schaut", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PIS", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ich hab' mit dem Ohr vernommen,", "tokens": ["Ich", "hab'", "mit", "dem", "Ohr", "ver\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Da\u00df aus diesem Holze kommen", "tokens": ["Da\u00df", "aus", "die\u00b7sem", "Hol\u00b7ze", "kom\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PDAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ein gar wunders\u00fc\u00dfer Laut.", "tokens": ["Ein", "gar", "wun\u00b7der\u00b7s\u00fc\u00b7\u00dfer", "Laut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Wie kann aus des Bauches Kl\u00fcften", "tokens": ["Wie", "kann", "aus", "des", "Bau\u00b7ches", "Kl\u00fcf\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Etwas t\u00f6nen in den L\u00fcften,", "tokens": ["Et\u00b7was", "t\u00f6\u00b7nen", "in", "den", "L\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sag' mir, lieber Wandersmann.", "tokens": ["Sag'", "mir", ",", "lie\u00b7ber", "Wan\u00b7ders\u00b7mann", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "ADV", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sag' mir, wie kann doch geschehen,", "tokens": ["Sag'", "mir", ",", "wie", "kann", "doch", "ge\u00b7sche\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "PWAV", "VMFIN", "ADV", "VVPP", "$,"], "meter": "+-++--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Da\u00df ich hier nicht kann ersehen,", "tokens": ["Da\u00df", "ich", "hier", "nicht", "kann", "er\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PTKNEG", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Was ich hab' geh\u00f6ret an?", "tokens": ["Was", "ich", "hab'", "ge\u00b7h\u00f6\u00b7ret", "an", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VAFIN", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Knab', du mu\u00dft den Ohren trauen;", "tokens": ["Knab'", ",", "du", "mu\u00dft", "den", "Oh\u00b7ren", "trau\u00b7en", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was du h\u00f6rst, ist nicht zu schauen,", "tokens": ["Was", "du", "h\u00f6rst", ",", "ist", "nicht", "zu", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "VAFIN", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dich vergn\u00fcge das Geh\u00f6r.", "tokens": ["Dich", "ver\u00b7gn\u00fc\u00b7ge", "das", "Ge\u00b7h\u00f6r", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Man mu\u00df seinen Sinn betauben,", "tokens": ["Man", "mu\u00df", "sei\u00b7nen", "Sinn", "be\u00b7tau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Gottes Wort in Einfalt glauben.", "tokens": ["Got\u00b7tes", "Wort", "in", "Ein\u00b7falt", "glau\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Selig ist, der glaubt der Lehr'!", "tokens": ["Se\u00b7lig", "ist", ",", "der", "glaubt", "der", "Lehr'", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "$,", "PRELS", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "In den gr\u00fcnlich falben Matten,", "tokens": ["In", "den", "gr\u00fcn\u00b7lich", "fal\u00b7ben", "Mat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Unter einer Linde Schatten,", "tokens": ["Un\u00b7ter", "ei\u00b7ner", "Lin\u00b7de", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Hat ein m\u00fcder Wandersmann", "tokens": ["Hat", "ein", "m\u00fc\u00b7der", "Wan\u00b7ders\u00b7mann"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Seine Laute hingeleget,", "tokens": ["Sei\u00b7ne", "Lau\u00b7te", "hin\u00b7ge\u00b7le\u00b7get", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Weil er, von dem Schlaf erreget,", "tokens": ["Weil", "er", ",", "von", "dem", "Schla