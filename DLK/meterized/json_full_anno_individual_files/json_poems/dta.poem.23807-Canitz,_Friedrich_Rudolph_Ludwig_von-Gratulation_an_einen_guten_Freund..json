{"dta.poem.23807": {"metadata": {"author": {"name": "Canitz, Friedrich Rudolph Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "Gratulation  an einen guten Freund.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1700", "urn": "urn:nbn:de:kobv:b4-200905197532", "language": ["de:0.99"], "booktitle": "[Canitz, Friedrich Rudolph Ludwig von]: Neben-Stunden Unterschiedener Gedichte. [Hrsg. v. Joachim Lange]. Berlin, 1700."}, "poem": {"stanza.1": {"line.1": {"text": "Verg\u00f6nne mir mein Freund/ da\u00df ich dir etwas stiffte/", "tokens": ["Ver\u00b7g\u00f6n\u00b7ne", "mir", "mein", "Freund", "/", "da\u00df", "ich", "dir", "et\u00b7was", "stiff\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$(", "KOUS", "PPER", "PPER", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das l\u00e4nger dauren sol/ als Ertz und Marmelstein.", "tokens": ["Das", "l\u00e4n\u00b7ger", "dau\u00b7ren", "sol", "/", "als", "Ertz", "und", "Mar\u00b7mel\u00b7stein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "VVINF", "VMFIN", "$(", "KOUS", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mich freut dein Wohlergehn/ drum fahr ich durch die", "tokens": ["Mich", "freut", "dein", "Woh\u00b7ler\u00b7gehn", "/", "drum", "fahr", "ich", "durch", "die"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$(", "PAV", "VVFIN", "PPER", "APPR", "ART"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Die zwischen dir und mir nunmehr befestigt seyn.", "tokens": ["Die", "zwi\u00b7schen", "dir", "und", "mir", "nun\u00b7mehr", "be\u00b7fes\u00b7tigt", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "KON", "PPER", "ADV", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Du wirst des F\u00fcrsten Raht im allerh\u00f6chsten Orden/", "tokens": ["Du", "wirst", "des", "F\u00fcrs\u00b7ten", "Raht", "im", "al\u00b7ler\u00b7h\u00f6chs\u00b7ten", "Or\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "NN", "APPRART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da dieser Name sich bey mir im Schatten weist/", "tokens": ["Da", "die\u00b7ser", "Na\u00b7me", "sich", "bey", "mir", "im", "Schat\u00b7ten", "weist", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "PRF", "APPR", "PPER", "APPRART", "NN", "VVFIN", "$("], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Und bist im rechtem Ernst zur Excellentz geworden/", "tokens": ["Und", "bist", "im", "rech\u00b7tem", "Ernst", "zur", "Ex\u00b7cel\u00b7lentz", "ge\u00b7wor\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPRART", "ADJA", "NN", "APPRART", "NN", "VAPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Da mich mein Bauer kaum gestrenger Juncker hei\u00dft.", "tokens": ["Da", "mich", "mein", "Bau\u00b7er", "kaum", "ge\u00b7stren\u00b7ger", "Jun\u00b7cker", "hei\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Getrost! ein gleicher Blick wird auch auf diese Zeilen", "tokens": ["Ge\u00b7trost", "!", "ein", "glei\u00b7cher", "Blick", "wird", "auch", "auf", "die\u00b7se", "Zei\u00b7len"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "ART", "ADJA", "NN", "VAFIN", "ADV", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und meine Niedrigkeit von deinem Gipffel gehn/", "tokens": ["Und", "mei\u00b7ne", "Nied\u00b7rig\u00b7keit", "von", "dei\u00b7nem", "Gipf\u00b7fel", "gehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Als du dich nicht gesch\u00e4mt/ den Brieffen zu ertheilen/", "tokens": ["Als", "du", "dich", "nicht", "ge\u00b7sch\u00e4mt", "/", "den", "Brief\u00b7fen", "zu", "er\u00b7thei\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "PTKNEG", "VVPP", "$(", "ART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Die dir von Wort zu Wort noch im Ged\u00e4chtni\u00df stehn.", "tokens": ["Die", "dir", "von", "Wort", "zu", "Wort", "noch", "im", "Ge\u00b7d\u00e4cht\u00b7ni\u00df", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "APPR", "NN", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Du hast dich nimmer nicht/ noch andre so vergessen/", "tokens": ["Du", "hast", "dich", "nim\u00b7mer", "nicht", "/", "noch", "and\u00b7re", "so", "ver\u00b7ges\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "PTKNEG", "$(", "ADV", "PIS", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Da\u00df man Ver\u00e4nderung an dir bef\u00fcrchten kan/", "tokens": ["Da\u00df", "man", "Ver\u00b7\u00e4n\u00b7de\u00b7rung", "an", "dir", "be\u00b7f\u00fcrch\u00b7ten", "kan", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "NN", "APPR", "PPER", "VVINF", "VMFIN", "$("], "meter": "++-+-+-+-+-+", "measure": "unknown.measure.septa"}, "line.15": {"text": "Noch nach der Aemter Maa\u00df die Freundschafft abge-", "tokens": ["Noch", "nach", "der", "A\u00b7em\u00b7ter", "Maa\u00df", "die", "Freund\u00b7schafft", "ab\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "ART", "NN", "TRUNC"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.16": {"text": "Du sahst die Redlichkeit und nicht den Purpur an.", "tokens": ["Du", "sahst", "die", "Red\u00b7lich\u00b7keit", "und", "nicht", "den", "Pur\u00b7pur", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "KON", "PTKNEG", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "So ist ein jeder froh/ da\u00df Fridrich dich erhoben/", "tokens": ["So", "ist", "ein", "je\u00b7der", "froh", "/", "da\u00df", "Frid\u00b7rich", "dich", "er\u00b7ho\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "PIS", "ADJD", "$(", "KOUS", "NE", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Da\u00df endlich dich das Gl\u00fcck erwischet bey der Hand/", "tokens": ["Da\u00df", "end\u00b7lich", "dich", "das", "Gl\u00fcck", "er\u00b7wi\u00b7schet", "bey", "der", "Hand", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPER", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Und gleichsam mit Gewalt auf einen Ort geschoben/", "tokens": ["Und", "gleich\u00b7sam", "mit", "Ge\u00b7walt", "auf", "ei\u00b7nen", "Ort", "ge\u00b7scho\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "NN", "APPR", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Den dir Verdienst und Wunsch hat lange zuerkant.", "tokens": ["Den", "dir", "Ver\u00b7dienst", "und", "Wunsch", "hat", "lan\u00b7ge", "zu\u00b7er\u00b7kant", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "NN", "KON", "NN", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Denn mit der Mutter-Milch hast du den Trieb gesogen/", "tokens": ["Denn", "mit", "der", "Mut\u00b7ter\u00b7Milch", "hast", "du", "den", "Trieb", "ge\u00b7so\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VAFIN", "PPER", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Den deines Bruders Zucht vollkommener gemacht/", "tokens": ["Den", "dei\u00b7nes", "Bru\u00b7ders", "Zucht", "voll\u00b7kom\u00b7me\u00b7ner", "ge\u00b7macht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "NN", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+---+", "measure": "unknown.measure.penta"}, "line.23": {"text": "Des Bruders/ dessen Lob Europa durch geflogen/", "tokens": ["Des", "Bru\u00b7ders", "/", "des\u00b7sen", "Lob", "Eu\u00b7ro\u00b7pa", "durch", "ge\u00b7flo\u00b7gen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PRELAT", "NN", "NE", "APPR", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Der euren Sieben-Stern zum Vorschein hat gebracht.", "tokens": ["Der", "eu\u00b7ren", "Sie\u00b7ben\u00b7S\u00b7tern", "zum", "Vor\u00b7schein", "hat", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "APPRART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.25": {"text": "Wie r\u00fchmlich du die Zeit auf Schulen angeleget/", "tokens": ["Wie", "r\u00fchm\u00b7lich", "du", "die", "Zeit", "auf", "Schu\u00b7len", "an\u00b7ge\u00b7le\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "ART", "NN", "APPR", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Das gab uns zuverstehn das tief-gelehrte Blat/", "tokens": ["Das", "gab", "uns", "zu\u00b7ver\u00b7stehn", "das", "tie\u00b7fge\u00b7lehr\u00b7te", "Blat", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Dadurch Arminius ward in der Grufft beweget/", "tokens": ["Da\u00b7durch", "Ar\u00b7mi\u00b7nius", "ward", "in", "der", "Grufft", "be\u00b7we\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "NE", "VAFIN", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.28": {"text": "So bald der munter - - auf den Catheder trat;", "tokens": ["So", "bald", "der", "mun\u00b7ter", "auf", "den", "Cat\u00b7he\u00b7der", "trat", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJD", "$(", "$(", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Hernach nahmst du den Weg nach weit entlegnen Orten/", "tokens": ["Her\u00b7nach", "nahmst", "du", "den", "Weg", "nach", "weit", "ent\u00b7leg\u00b7nen", "Or\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "APPR", "ADJD", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Und ludest da dein Schiff mit solchem Zeuge voll/", "tokens": ["Und", "lu\u00b7dest", "da", "dein", "Schiff", "mit", "sol\u00b7chem", "Zeu\u00b7ge", "voll", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PPOSAT", "NN", "APPR", "PIAT", "NN", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Das dir den Grund gelegt zu einer Ehren-Pforten/", "tokens": ["Das", "dir", "den", "Grund", "ge\u00b7legt", "zu", "ei\u00b7ner", "Eh\u00b7ren\u00b7P\u00b7for\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ART", "NN", "VVPP", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "An der die sp\u00e4te Welt dein Denckmahl lesen soll.", "tokens": ["An", "der", "die", "sp\u00e4\u00b7te", "Welt", "dein", "Denck\u00b7mahl", "le\u00b7sen", "soll", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "ADJA", "NN", "PPOSAT", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Die Seyne mit der Theems zusamt der Norden Cronen/", "tokens": ["Die", "Sey\u00b7ne", "mit", "der", "Theems", "zu\u00b7samt", "der", "Nor\u00b7den", "Cro\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "APPR", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Die sahen so entz\u00fcckt dich edlen M\u00e4rcker an/", "tokens": ["Die", "sa\u00b7hen", "so", "ent\u00b7z\u00fcckt", "dich", "ed\u00b7len", "M\u00e4r\u00b7cker", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "VVFIN", "PPER", "ADJA", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Als der so erst gesehn/ da\u00df Moskau die Melonen", "tokens": ["Als", "der", "so", "erst", "ge\u00b7sehn", "/", "da\u00df", "Mos\u00b7kau", "die", "Me\u00b7lo\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADV", "ADV", "VVPP", "$(", "KOUS", "NE", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "So gut und besser noch als Welschland zeugen kan.", "tokens": ["So", "gut", "und", "bes\u00b7ser", "noch", "als", "Wel\u00b7schland", "zeu\u00b7gen", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "ADV", "KOUS", "NN", "VVFIN", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Bald wurdest du entdeckt von Fridrich Wilhelms Blick\u0113/", "tokens": ["Bald", "wur\u00b7dest", "du", "ent\u00b7deckt", "von", "Frid\u00b7rich", "Wil\u00b7helms", "Blick\u0113", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVPP", "APPR", "NE", "NE", "NE", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Du h\u00f6rtest sein Gehei\u00df/ das eine Pr\u00fcfung war/", "tokens": ["Du", "h\u00f6r\u00b7test", "sein", "Ge\u00b7hei\u00df", "/", "das", "ei\u00b7ne", "Pr\u00fc\u00b7fung", "war", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$(", "PDS", "ART", "NN", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Wie du zu seinem Dienst dich k\u00fcnfftig w\u00fcrdest schicken/", "tokens": ["Wie", "du", "zu", "sei\u00b7nem", "Dienst", "dich", "k\u00fcnff\u00b7tig", "w\u00fcr\u00b7dest", "schi\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "PPOSAT", "NN", "PPER", "ADJD", "VAFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Und legtst ein Meister-St\u00fcck an statt der Probe dar.", "tokens": ["Und", "legtst", "ein", "Meis\u00b7ter\u00b7St\u00fcck", "an", "statt", "der", "Pro\u00b7be", "dar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Sarmatien z\u00fcrnt noch/ weil jenen Haupt-Rebellen", "tokens": ["Sar\u00b7ma\u00b7ti\u00b7en", "z\u00fcrnt", "noch", "/", "weil", "je\u00b7nen", "Haup\u00b7tRe\u00b7bel\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "