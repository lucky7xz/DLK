rd", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das richt', O Gott, Dir zu.", "tokens": ["Das", "richt'", ",", "O", "Gott", ",", "Dir", "zu", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "NE", "NN", "$,", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df, was es nur gedencket,", "tokens": ["Da\u00df", ",", "was", "es", "nur", "ge\u00b7den\u00b7cket", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "PWS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sey nichts als einig Du.", "tokens": ["Sey", "nichts", "als", "ei\u00b7nig", "Du", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "KOKOM", "ADJD", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Entzeuch es dieser Welt,", "tokens": ["Ent\u00b7zeuch", "es", "die\u00b7ser", "Welt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PDAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Da\u00df es aus diesen Thr\u00e4nen", "tokens": ["Da\u00df", "es", "aus", "die\u00b7sen", "Thr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PDAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "In deiner Freuden Feld", "tokens": ["In", "dei\u00b7ner", "Freu\u00b7den", "Feld"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Sich m\u00f6g' ohn abla\u00df sehnen.", "tokens": ["Sich", "m\u00f6g'", "ohn", "ab\u00b7la\u00df", "seh\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VMFIN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Vnd da ich heut ver\u00fcbet", "tokens": ["Vnd", "da", "ich", "heut", "ver\u00b7\u00fc\u00b7bet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ADV", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was wieder Dein Gebot", "tokens": ["Was", "wie\u00b7der", "Dein", "Ge\u00b7bot"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vnd deinen Geist betr\u00fcbet,", "tokens": ["Vnd", "dei\u00b7nen", "Geist", "be\u00b7tr\u00fc\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das sey vertilgt und tod", "tokens": ["Das", "sey", "ver\u00b7tilgt", "und", "tod"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "VVPP", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Durch Christi theures Blut,", "tokens": ["Durch", "Chris\u00b7ti", "theu\u00b7res", "Blut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Das mildiglich geflossen,", "tokens": ["Das", "mil\u00b7dig\u00b7lich", "ge\u00b7flos\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Als Er es mir zu gut", "tokens": ["Als", "Er", "es", "mir", "zu", "gut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "PPER", "PTKA", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Aus Liebe hat vergossen.", "tokens": ["Aus", "Lie\u00b7be", "hat", "ver\u00b7gos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Vnd weil ich itzt soll schlaffen,", "tokens": ["Vnd", "weil", "ich", "itzt", "soll", "schlaf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So la\u00df mich sicher seyn", "tokens": ["So", "la\u00df", "mich", "si\u00b7cher", "seyn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVIMP", "PPER", "ADJD", "VAINF"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Durch deiner Auffsicht Waffen,", "tokens": ["Durch", "dei\u00b7ner", "Auff\u00b7sicht", "Waf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Schleu\u00df deiner Hut mich ein,", "tokens": ["Schleu\u00df", "dei\u00b7ner", "Hut", "mich", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Des Teuffels Mord und List,", "tokens": ["Des", "Teuf\u00b7fels", "Mord", "und", "List", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Der b\u00f6sen Menschen T\u00fccke", "tokens": ["Der", "b\u00f6\u00b7sen", "Men\u00b7schen", "T\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Vnd was sonst sch\u00e4dlich ist,", "tokens": ["Vnd", "was", "sonst", "sch\u00e4d\u00b7lich", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Treib, Herr, von mir zur\u00fccke.", "tokens": ["Treib", ",", "Herr", ",", "von", "mir", "zu\u00b7r\u00fc\u00b7cke", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "La\u00df mich kein b\u00f6ses Ende", "tokens": ["La\u00df", "mich", "kein", "b\u00f6\u00b7ses", "En\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Betretten allermeist,", "tokens": ["Be\u00b7tret\u00b7ten", "al\u00b7ler\u00b7meist", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Denn ich in deine H\u00e4nde", "tokens": ["Denn", "ich", "in", "dei\u00b7ne", "H\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Befehle meinen Geist.", "tokens": ["Be\u00b7feh\u00b7le", "mei\u00b7nen", "Geist", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ich bin zu allerzeit", "tokens": ["Ich", "bin", "zu", "al\u00b7ler\u00b7zeit"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Dein Eigenthum und Erbe,", "tokens": ["Dein", "Ei\u00b7gen\u00b7thum", "und", "Er\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Es sey Lieb' oder Leid,", "tokens": ["Es", "sey", "Lieb'", "o\u00b7der", "Leid", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "KON", "NN", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.8": {"text": "Ich leb', Herr, oder sterbe.", "tokens": ["Ich", "leb'", ",", "Herr", ",", "o\u00b7der", "ster\u00b7be", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "NN", "$,", "KON", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}