{"dta.poem.2793": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Verlangen nach dem Tode.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich freue mich der letzten Stunde/", "tokens": ["Ich", "freu\u00b7e", "mich", "der", "letz\u00b7ten", "Stun\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die sonst dem Menschen Schmertzen macht.", "tokens": ["Die", "sonst", "dem", "Men\u00b7schen", "Schmert\u00b7zen", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Geht Erd und Himmel selbst zu Grunde/", "tokens": ["Geht", "Erd", "und", "Him\u00b7mel", "selbst", "zu", "Grun\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "ADV", "APPR", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Was bin denn ich/ ein Wurm/ bedacht/", "tokens": ["Was", "bin", "denn", "ich", "/", "ein", "Wurm", "/", "be\u00b7dacht", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "VAFIN", "KON", "PPER", "$(", "ART", "NN", "$(", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mein l\u00e4imen Hau\u00df/ die irdnen W\u00e4nde", "tokens": ["Mein", "l\u00e4i\u00b7men", "Hau\u00df", "/", "die", "ird\u00b7nen", "W\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$(", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Zu sch\u00e4tzen ewig und ohn Ende.", "tokens": ["Zu", "sch\u00e4t\u00b7zen", "e\u00b7wig", "und", "ohn", "En\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ADJD", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Die Hand so mich zu erst erbauet/", "tokens": ["Die", "Hand", "so", "mich", "zu", "erst", "er\u00b7bau\u00b7et", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PPER", "PTKA", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die rei\u00dft mich/ ihr Gesch\u00f6pff auch ein.", "tokens": ["Die", "rei\u00dft", "mich", "/", "ihr", "Ge\u00b7sch\u00f6pff", "auch", "ein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "$(", "PPOSAT", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Wem vor des Todes Pfeilen grauet/", "tokens": ["Wem", "vor", "des", "To\u00b7des", "Pfei\u00b7len", "grau\u00b7et", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Der kan kein Uberwinder seyn.", "tokens": ["Der", "kan", "kein", "U\u00b7ber\u00b7win\u00b7der", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PIAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "La\u00df Fleisch und Blut das Leben lieben/", "tokens": ["La\u00df", "Fleisch", "und", "Blut", "das", "Le\u00b7ben", "lie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "KON", "NN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Die Seele mu\u00df sich anders \u00fcben.", "tokens": ["Die", "See\u00b7le", "mu\u00df", "sich", "an\u00b7ders", "\u00fc\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PRF", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Ich seh/ da\u00df jeden Tag was stirbet;", "tokens": ["Ich", "seh", "/", "da\u00df", "je\u00b7den", "Tag", "was", "stir\u00b7bet", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "KOUS", "PIAT", "NN", "PWS", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Ja/ da\u00df der Leib ein Siechhau\u00df heist;", "tokens": ["Ja", "/", "da\u00df", "der", "Leib", "ein", "Siech\u00b7hau\u00df", "heist", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$(", "KOUS", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Wenn da bald Hand bald Fu\u00df verdirbet/", "tokens": ["Wenn", "da", "bald", "Hand", "bald", "Fu\u00df", "ver\u00b7dir\u00b7bet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADV", "NN", "ADV", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Und das veralte Kleid zerschlei\u00dft:", "tokens": ["Und", "das", "ver\u00b7al\u00b7te", "Kleid", "zer\u00b7schlei\u00dft", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "So sucht der Geist sich frey zu machen/", "tokens": ["So", "sucht", "der", "Geist", "sich", "frey", "zu", "ma\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PRF", "ADJD", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Und wird der m\u00fcrben Fessel lachen.", "tokens": ["Und", "wird", "der", "m\u00fcr\u00b7ben", "Fes\u00b7sel", "la\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Das Eisen rost\u2019t/ die Steine brechen/", "tokens": ["Das", "Ei\u00b7sen", "rost't", "/", "die", "Stei\u00b7ne", "bre\u00b7chen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$(", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Porphyr und Marmor sind nicht vest.", "tokens": ["Por\u00b7phyr", "und", "Mar\u00b7mor", "sind", "nicht", "vest", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "VAFIN", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Wie kan der schn\u00f6de Mensch doch sprechen/", "tokens": ["Wie", "kan", "der", "schn\u00f6\u00b7de", "Mensch", "doch", "spre\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "ART", "ADJA", "NN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Da\u00df sich was Ewigs sp\u00fcren l\u00e4st/", "tokens": ["Da\u00df", "sich", "was", "E\u00b7wigs", "sp\u00fc\u00b7ren", "l\u00e4st", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "PWS", "ADJA", "VVINF", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "In des verdorrten Leibes Beinen/", "tokens": ["In", "des", "ver\u00b7dorr\u00b7ten", "Lei\u00b7bes", "Bei\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Den noch belebten Leichen-Steinen?", "tokens": ["Den", "noch", "be\u00b7leb\u00b7ten", "Lei\u00b7chen\u00b7Stei\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Drumb komm/ O Tod/ denn meine Seele", "tokens": ["Drumb", "komm", "/", "O", "Tod", "/", "denn", "mei\u00b7ne", "See\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PAV", "VVFIN", "$(", "NE", "NN", "$(", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Er schrickt ob deiner Ankunfft nicht.", "tokens": ["Er", "schrickt", "ob", "dei\u00b7ner", "An\u00b7kunfft", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOUS", "PPOSAT", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Zeuch sie aus dieses C\u00f6rpers H\u00f6le", "tokens": ["Zeuch", "sie", "aus", "die\u00b7ses", "C\u00f6r\u00b7pers", "H\u00f6\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "APPR", "PDAT", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Zu jenem unumbschriebnen Licht.", "tokens": ["Zu", "je\u00b7nem", "u\u00b7numbsc\u00b7hrieb\u00b7nen", "Licht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Komm s\u00fcsser Gast/ mein hei\u00df Verlangen", "tokens": ["Komm", "s\u00fcs\u00b7ser", "Gast", "/", "mein", "hei\u00df", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ADJA", "NN", "$(", "PPOSAT", "ADJD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "Ist schon bestellt dich zu empfangen.", "tokens": ["Ist", "schon", "be\u00b7stellt", "dich", "zu", "emp\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "VVFIN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.31": {"text": "Ich bilde mir nicht d\u00fcrre Knochen/", "tokens": ["Ich", "bil\u00b7de", "mir", "nicht", "d\u00fcr\u00b7re", "Kno\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.32": {"text": "Und wie man dich sonst mahlet ein.", "tokens": ["Und", "wie", "man", "dich", "sonst", "mah\u00b7let", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PIS", "PRF", "ADV", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Wenn meine Augen sind gebrochen/", "tokens": ["Wenn", "mei\u00b7ne", "Au\u00b7gen", "sind", "ge\u00b7bro\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.34": {"text": "So wirds ein sanffter Schlaf nur seyn.", "tokens": ["So", "wirds", "ein", "sanff\u00b7ter", "Schlaf", "nur", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "ADV", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Ein Thor der mag dich he\u00dflich nennen/", "tokens": ["Ein", "Thor", "der", "mag", "dich", "he\u00df\u00b7lich", "nen\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "VMFIN", "PRF", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.36": {"text": "Ich aber mu\u00df dich sch\u00f6n bekennen.", "tokens": ["Ich", "a\u00b7ber", "mu\u00df", "dich", "sch\u00f6n", "be\u00b7ken\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VMFIN", "PRF", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.37": {"text": "Das Grab das auch die Alten fliehen/", "tokens": ["Das", "Grab", "das", "auch", "die", "Al\u00b7ten", "flie\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADV", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.38": {"text": "Nenn ich des Himmels Vorgemach.", "tokens": ["Nenn", "ich", "des", "Him\u00b7mels", "Vor\u00b7ge\u00b7mach", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "Der Kittel den man an-mu\u00df ziehen/", "tokens": ["Der", "Kit\u00b7tel", "den", "man", "an\u00b7mu\u00df", "zie\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "PIS", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.40": {"text": "Scheint heller als der Sternen Dach.", "tokens": ["Scheint", "hel\u00b7ler", "als", "der", "Ster\u00b7nen", "Dach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "KOKOM", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.41": {"text": "Der Schlaf ist kurtz/ die Nacht ist enge/", "tokens": ["Der", "Schlaf", "ist", "kurtz", "/", "die", "Nacht", "ist", "en\u00b7ge", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$(", "ART", "NN", "VAFIN", "ADJA", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.42": {"text": "Zu jenes grossen Tages L\u00e4nge.", "tokens": ["Zu", "je\u00b7nes", "gros\u00b7sen", "Ta\u00b7ges", "L\u00e4n\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}