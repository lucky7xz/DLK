{"dta.poem.19316": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "49.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Woher ich kam, wohin ich gehe, wei\u00df ich nicht;", "tokens": ["Wo\u00b7her", "ich", "kam", ",", "wo\u00b7hin", "ich", "ge\u00b7he", ",", "wei\u00df", "ich", "nicht", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "PWAV", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch dis: von Gott zu Gott! ist meine Zuversicht.", "tokens": ["Doch", "dis", ":", "von", "Gott", "zu", "Gott", "!", "ist", "mei\u00b7ne", "Zu\u00b7ver\u00b7sicht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "$.", "APPR", "NN", "APPR", "NN", "$.", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Warum ich jetzo bin, und andre sonst gewesen;", "tokens": ["Wa\u00b7rum", "ich", "jet\u00b7zo", "bin", ",", "und", "and\u00b7re", "sonst", "ge\u00b7we\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VAFIN", "$,", "KON", "PIS", "ADV", "VAPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Warum mir dieser Platz, kein andrer, ist erlesen?", "tokens": ["Wa\u00b7rum", "mir", "die\u00b7ser", "Platz", ",", "kein", "an\u00b7drer", ",", "ist", "er\u00b7le\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PDAT", "NN", "$,", "PIAT", "ADJA", "$,", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ich bl\u00fche wie die Blum', und wachse wie der Baum,", "tokens": ["Ich", "bl\u00fc\u00b7he", "wie", "die", "Blum'", ",", "und", "wach\u00b7se", "wie", "der", "Baum", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "ART", "NN", "$,", "KON", "VVFIN", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In meiner Jahreszeit, in meinem Gartenraum.", "tokens": ["In", "mei\u00b7ner", "Jah\u00b7res\u00b7zeit", ",", "in", "mei\u00b7nem", "Gar\u00b7ten\u00b7raum", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Im gro\u00dfen Garten ist kein abgelegnes Beet,", "tokens": ["Im", "gro\u00b7\u00dfen", "Gar\u00b7ten", "ist", "kein", "ab\u00b7ge\u00b7leg\u00b7nes", "Beet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VAFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das nicht zu seiner Zeit von Lenzluft ist durchweht.", "tokens": ["Das", "nicht", "zu", "sei\u00b7ner", "Zeit", "von", "Lenz\u00b7luft", "ist", "durch\u00b7weht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PTKNEG", "APPR", "PPOSAT", "NN", "APPR", "NN", "VAFIN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Kein abgelegnes Beet, das nicht erbl\u00fcht in Wonne", "tokens": ["Kein", "ab\u00b7ge\u00b7leg\u00b7nes", "Beet", ",", "das", "nicht", "er\u00b7bl\u00fcht", "in", "Won\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "$,", "PRELS", "PTKNEG", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "An seines G\u00e4rtners Blick, sein Blick ist Mond und Sonne.", "tokens": ["An", "sei\u00b7nes", "G\u00e4rt\u00b7ners", "Blick", ",", "sein", "Blick", "ist", "Mond", "und", "Son\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,", "PPOSAT", "NN", "VAFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Ich f\u00fchle Sommerlust, und f\u00fchle Winterschauer,", "tokens": ["Ich", "f\u00fch\u00b7le", "Som\u00b7mer\u00b7lust", ",", "und", "f\u00fch\u00b7le", "Win\u00b7ter\u00b7schau\u00b7er", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,", "KON", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und einen Schauder, da\u00df ich bin von kurzer Dauer,", "tokens": ["Und", "ei\u00b7nen", "Schau\u00b7der", ",", "da\u00df", "ich", "bin", "von", "kur\u00b7zer", "Dau\u00b7er", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "KOUS", "PPER", "VAFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Doch eine Ahnung, da\u00df ich ewig bin von Stamme,", "tokens": ["Doch", "ei\u00b7ne", "Ah\u00b7nung", ",", "da\u00df", "ich", "e\u00b7wig", "bin", "von", "Stam\u00b7me", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "KOUS", "PPER", "ADJD", "VAFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und da\u00df nicht sich verzehrt, die mich verzehrt, die Flamme.", "tokens": ["Und", "da\u00df", "nicht", "sich", "ver\u00b7zehrt", ",", "die", "mich", "ver\u00b7zehrt", ",", "die", "Flam\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "KOUS", "PTKNEG", "PRF", "VVPP", "$,", "PRELS", "PPER", "VVPP", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Es ist ein niedrer Trieb in mir und h\u00f6hres Streben,", "tokens": ["Es", "ist", "ein", "nie\u00b7drer", "Trieb", "in", "mir", "und", "h\u00f6h\u00b7res", "Stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "APPR", "PPER", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dem soll ich folgen und mich jenem nicht ergeben.", "tokens": ["Dem", "soll", "ich", "fol\u00b7gen", "und", "mich", "je\u00b7nem", "nicht", "er\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "VVINF", "KON", "PPER", "PDS", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Zur reinsten Bl\u00fcte will ich meine Lust entfalten,", "tokens": ["Zur", "reins\u00b7ten", "Bl\u00fc\u00b7te", "will", "ich", "mei\u00b7ne", "Lust", "ent\u00b7fal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VMFIN", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und meine Schmerzen selbst zu Wonnen umgestalten.", "tokens": ["Und", "mei\u00b7ne", "Schmer\u00b7zen", "selbst", "zu", "Won\u00b7nen", "um\u00b7ge\u00b7stal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Ich steh' in Gottes Hand, und ruh' in Gottes Schoo\u00df;", "tokens": ["Ich", "steh'", "in", "Got\u00b7tes", "Hand", ",", "und", "ruh'", "in", "Got\u00b7tes", "Schoo\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "NN", "$,", "KON", "VVFIN", "APPR", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vor ihm f\u00fchl' ich mich klein, in ihm f\u00fchl' ich mich gro\u00df.", "tokens": ["Vor", "ihm", "f\u00fchl'", "ich", "mich", "klein", ",", "in", "ihm", "f\u00fchl'", "ich", "mich", "gro\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PPER", "PRF", "ADJD", "$,", "APPR", "PPER", "VVFIN", "PPER", "PRF", "ADJD", "$."], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}}}}}