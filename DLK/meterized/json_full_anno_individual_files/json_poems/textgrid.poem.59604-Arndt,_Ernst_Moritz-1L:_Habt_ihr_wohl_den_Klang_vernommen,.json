{"textgrid.poem.59604": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "1L: Habt ihr wohl den Klang vernommen,", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Habt ihr wohl den Klang vernommen,", "tokens": ["Habt", "ihr", "wohl", "den", "Klang", "ver\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der durch alle L\u00e4nder klingt,", "tokens": ["Der", "durch", "al\u00b7le", "L\u00e4n\u00b7der", "klingt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie der Ruhm den Flug genommen", "tokens": ["Wie", "der", "Ruhm", "den", "Flug", "ge\u00b7nom\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die goldnen Fl\u00fcgel schwingt?", "tokens": ["Und", "die", "gold\u00b7nen", "Fl\u00fc\u00b7gel", "schwingt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie der Schande Zentnerschwere", "tokens": ["Wie", "der", "Schan\u00b7de", "Zent\u00b7ner\u00b7schwe\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sich von Deutschlands Nacken l\u00f6st,", "tokens": ["Sich", "von", "Deutschlands", "Na\u00b7cken", "l\u00f6st", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "NE", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und die alte deutsche Ehre", "tokens": ["Und", "die", "al\u00b7te", "deut\u00b7sche", "Eh\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Hell in Kriegsposaunen st\u00f6\u00dft?", "tokens": ["Hell", "in", "Kriegs\u00b7po\u00b7sau\u00b7nen", "st\u00f6\u00dft", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "H\u00f6rtet ihr die hohen Namen?", "tokens": ["H\u00f6r\u00b7tet", "ihr", "die", "ho\u00b7hen", "Na\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Preu\u00dfen! Preu\u00dfen! klingt der Klang;", "tokens": ["Preu\u00b7\u00dfen", "!", "Preu\u00b7\u00dfen", "!", "klingt", "der", "Klang", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$.", "NE", "$.", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Tausend Stimmen rufen Amen,", "tokens": ["Tau\u00b7send", "Stim\u00b7men", "ru\u00b7fen", "A\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Tausend Herzen gl\u00fchen Dank,", "tokens": ["Tau\u00b7send", "Her\u00b7zen", "gl\u00fc\u00b7hen", "Dank", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Millionen Knie sinken", "tokens": ["Mil\u00b7lion\u00b7en", "Knie", "sin\u00b7ken"], "token_info": ["word", "word", "word"], "pos": ["NN", "NN", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Freudig betend vor dem Herrn,", "tokens": ["Freu\u00b7dig", "be\u00b7tend", "vor", "dem", "Herrn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Millionen Augen blinken", "tokens": ["Mil\u00b7lion\u00b7en", "Au\u00b7gen", "blin\u00b7ken"], "token_info": ["word", "word", "word"], "pos": ["NN", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Selig auf zum Tagesstern.", "tokens": ["Se\u00b7lig", "auf", "zum", "Ta\u00b7gess\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "APPRART", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.3": {"line.1": {"text": "Nehmt den Stolz, ihr frommen M\u00e4nner! \u2013", "tokens": ["Nehmt", "den", "Stolz", ",", "ihr", "from\u00b7men", "M\u00e4n\u00b7ner", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "PPOSAT", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dieser Preis wird nimmer Wahn \u2013", "tokens": ["Die\u00b7ser", "Preis", "wird", "nim\u00b7mer", "Wahn", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VAFIN", "ADV", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nehmt den Stolz, ihr ersten Renner", "tokens": ["Nehmt", "den", "Stolz", ",", "ihr", "ers\u00b7ten", "Ren\u00b7ner"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$,", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf der deutschen Ehrenbahn,", "tokens": ["Auf", "der", "deut\u00b7schen", "Eh\u00b7ren\u00b7bahn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Die der s\u00fc\u00dfe Reiz der Tugend", "tokens": ["Die", "der", "s\u00fc\u00b7\u00dfe", "Reiz", "der", "Tu\u00b7gend"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ART", "ADJA", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Lockte froh zu Sieg und Tod,", "tokens": ["Lock\u00b7te", "froh", "zu", "Sieg", "und", "Tod", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wie der Br\u00e4ute Rosentugend", "tokens": ["Wie", "der", "Br\u00e4u\u00b7te", "Ro\u00b7sen\u00b7tu\u00b7gend"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Lockt der Wonnefackel Rot.", "tokens": ["Lockt", "der", "Won\u00b7ne\u00b7fa\u00b7ckel", "Rot", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Tapfre Preu\u00dfen! Tapfre Preu\u00dfen!", "tokens": ["Tapf\u00b7re", "Preu\u00b7\u00dfen", "!", "Tapf\u00b7re", "Preu\u00b7\u00dfen", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$.", "NE", "NE", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ihr, die Gl\u00fcck und Sieg vers\u00f6hnt,", "tokens": ["Ihr", ",", "die", "Gl\u00fcck", "und", "Sieg", "ver\u00b7s\u00f6hnt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ART", "NN", "KON", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Deutschlands Retter sollt ihr hei\u00dfen,", "tokens": ["Deutschlands", "Ret\u00b7ter", "sollt", "ihr", "hei\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wo nur deutsche Sprache t\u00f6nt;", "tokens": ["Wo", "nur", "deut\u00b7sche", "Spra\u00b7che", "t\u00f6nt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "In der Enkel fernsten Tagen,", "tokens": ["In", "der", "En\u00b7kel", "ferns\u00b7ten", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Durch der S\u00e4keln Nacht hinaus", "tokens": ["Durch", "der", "S\u00e4\u00b7keln", "Nacht", "hin\u00b7aus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN", "APZR"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Soll noch jeder Deutsche sagen:", "tokens": ["Soll", "noch", "je\u00b7der", "Deut\u00b7sche", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PIAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Diese fochten's mutig aus.", "tokens": ["Die\u00b7se", "foch\u00b7ten's", "mu\u00b7tig", "aus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Jetzt die edlen Heldenschatten,", "tokens": ["Jetzt", "die", "ed\u00b7len", "Hel\u00b7den\u00b7schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die der dunkle Tod umfing,", "tokens": ["Die", "der", "dunk\u00b7le", "Tod", "um\u00b7fing", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wollen wir mit Glanz bestatten", "tokens": ["Wol\u00b7len", "wir", "mit", "Glanz", "be\u00b7stat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In des Schlachtfelds blut'gem Ring;", "tokens": ["In", "des", "Schlacht\u00b7felds", "blut'\u00b7gem", "Ring", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Auf! Und t\u00fcrmt den Berg von Steinen!", "tokens": ["Auf", "!", "Und", "t\u00fcrmt", "den", "Berg", "von", "Stei\u00b7nen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$.", "KON", "VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "T\u00fcrmt ein deutsches Heldenmal!", "tokens": ["T\u00fcrmt", "ein", "deut\u00b7sches", "Hel\u00b7den\u00b7mal", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Sonne, komm mit hellsten Scheinen,", "tokens": ["Son\u00b7ne", ",", "komm", "mit", "hells\u00b7ten", "Schei\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Leuchte drauf den sch\u00f6nsten Strahl!", "tokens": ["Leuch\u00b7te", "drauf", "den", "sch\u00f6ns\u00b7ten", "Strahl", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PAV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Auf! Und pflanzet gr\u00fcner Eichen", "tokens": ["Auf", "!", "Und", "pflan\u00b7zet", "gr\u00fc\u00b7ner", "Ei\u00b7chen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "$.", "KON", "VVFIN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ernste Haine ringsumher!", "tokens": ["Erns\u00b7te", "Hai\u00b7ne", "rings\u00b7um\u00b7her", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Betet, da\u00df in deutschen Reichen", "tokens": ["Be\u00b7tet", ",", "da\u00df", "in", "deut\u00b7schen", "Rei\u00b7chen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "KOUS", "APPR", "ADJA", "NN"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.4": {"text": "Buben freveln nimmermehr!", "tokens": ["Bu\u00b7ben", "fre\u00b7veln", "nim\u00b7mer\u00b7mehr", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Betet, fluchet, da\u00df die Sklaven,", "tokens": ["Be\u00b7tet", ",", "flu\u00b7chet", ",", "da\u00df", "die", "Skla\u00b7ven", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$,", "KOUS", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Die den heil'gen Toten nahn,", "tokens": ["Die", "den", "heil'\u00b7gen", "To\u00b7ten", "nahn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "ADJA", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Pl\u00f6tzlich alle Himmelsstrafen", "tokens": ["Pl\u00f6tz\u00b7lich", "al\u00b7le", "Him\u00b7mels\u00b7stra\u00b7fen"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Auf das schuld'ge Haupt empfahn!", "tokens": ["Auf", "das", "schuld'\u00b7ge", "Haupt", "em\u00b7pfahn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Und ihr, die von listen H\u00f6hen,", "tokens": ["Und", "ihr", ",", "die", "von", "lis\u00b7ten", "H\u00f6\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "PRELS", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Von dem heitern Element,", "tokens": ["Von", "dem", "hei\u00b7tern", "E\u00b7le\u00b7ment", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo die Geister wandeln gehen,", "tokens": ["Wo", "die", "Geis\u00b7ter", "wan\u00b7deln", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Alles schauet, alles kennt,", "tokens": ["Al\u00b7les", "schau\u00b7et", ",", "al\u00b7les", "kennt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Helden aus den grauen Zeiten,", "tokens": ["Hel\u00b7den", "aus", "den", "grau\u00b7en", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Die ihr l\u00e4ngst geschieden seid,", "tokens": ["Die", "ihr", "l\u00e4ngst", "ge\u00b7schie\u00b7den", "seid", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVPP", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "H\u00f6rt die Siegesglocken l\u00e4uten,", "tokens": ["H\u00f6rt", "die", "Sie\u00b7ges\u00b7glo\u00b7cken", "l\u00e4u\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Freut euch deutscher Herrlichkeit!", "tokens": ["Freut", "euch", "deut\u00b7scher", "Herr\u00b7lich\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Ihr auch, die auf diesen Auen", "tokens": ["Ihr", "auch", ",", "die", "auf", "die\u00b7sen", "Au\u00b7en"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "$,", "PRELS", "APPR", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "J\u00fcngrer Schlachten Staub erregt", "tokens": ["J\u00fcng\u00b7rer", "Schlach\u00b7ten", "Staub", "er\u00b7regt"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "NN", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und mit Schrecken, Tod und Grauen", "tokens": ["Und", "mit", "Schre\u00b7cken", ",", "Tod", "und", "Grau\u00b7en"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "$,", "NN", "KON"