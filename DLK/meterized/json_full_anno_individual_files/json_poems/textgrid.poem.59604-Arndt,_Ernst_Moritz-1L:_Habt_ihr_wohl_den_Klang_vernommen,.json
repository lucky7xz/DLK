"meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Reihen gegen Reih' bewegt,", "tokens": ["Rei\u00b7hen", "ge\u00b7gen", "Reih'", "be\u00b7wegt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Gustav, gro\u00dfer Schwedenk\u00f6nig,", "tokens": ["Gus\u00b7tav", ",", "gro\u00b7\u00dfer", "Schwe\u00b7den\u00b7k\u00f6\u00b7nig", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Zweiter Friedrich, Wallenstein,", "tokens": ["Zwei\u00b7ter", "Fried\u00b7rich", ",", "Wal\u00b7len\u00b7stein", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NN", "NE", "$,", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Lernt, wie eure Schlachten wenig", "tokens": ["Lernt", ",", "wie", "eu\u00b7re", "Schlach\u00b7ten", "we\u00b7nig"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "PWAV", "PPOSAT", "NN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Sind vor diesem Ehrenschein.", "tokens": ["Sind", "vor", "die\u00b7sem", "Eh\u00b7ren\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PDAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.19": {"line.1": {"text": "Denn das Lied mu\u00df schwarz sich kleiden,", "tokens": ["Denn", "das", "Lied", "mu\u00df", "schwarz", "sich", "klei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VMFIN", "ADJD", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Welches euch besingen will,", "tokens": ["Wel\u00b7ches", "euch", "be\u00b7sin\u00b7gen", "will", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der helle Klang der Freuden", "tokens": ["Und", "der", "hel\u00b7le", "Klang", "der", "Freu\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wird bei euren Taten still,", "tokens": ["Wird", "bei", "eu\u00b7ren", "Ta\u00b7ten", "still", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und Germanien mag wohl klagen", "tokens": ["Und", "Ger\u00b7ma\u00b7ni\u00b7en", "mag", "wohl", "kla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VMFIN", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Um den schweren Ha\u00df und Neid,", "tokens": ["Um", "den", "schwe\u00b7ren", "Ha\u00df", "und", "Neid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wodurch in vergangnen Tagen", "tokens": ["Wo\u00b7durch", "in", "ver\u00b7gang\u00b7nen", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ihr so gro\u00df geworden seid.", "tokens": ["Ihr", "so", "gro\u00df", "ge\u00b7wor\u00b7den", "seid", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJD", "VAPP", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.20": {"line.1": {"text": "Aber selig, wer in diesen", "tokens": ["A\u00b7ber", "se\u00b7lig", ",", "wer", "in", "die\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADJD", "$,", "PWS", "APPR", "PDAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hehren G\u00f6tterschlachten f\u00e4llt!", "tokens": ["Heh\u00b7ren", "G\u00f6t\u00b7ter\u00b7schlach\u00b7ten", "f\u00e4llt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Der wird ewig froh gepriesen", "tokens": ["Der", "wird", "e\u00b7wig", "froh", "ge\u00b7prie\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ADJD", "ADJD", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Als ein Heiland, als ein Held;", "tokens": ["Als", "ein", "Hei\u00b7land", ",", "als", "ein", "Held", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "KOUS", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Auf der Freiheit Siegesst\u00e4tten", "tokens": ["Auf", "der", "Frei\u00b7heit", "Sie\u00b7ges\u00b7st\u00e4t\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Bl\u00fcht die Ehre ewig gr\u00fcn,", "tokens": ["Bl\u00fcht", "die", "Eh\u00b7re", "e\u00b7wig", "gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Heil'ge kommen da zu beten,", "tokens": ["Heil'\u00b7ge", "kom\u00b7men", "da", "zu", "be\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Engel kommen da zu knien.", "tokens": ["En\u00b7gel", "kom\u00b7men", "da", "zu", "kni\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "PTKZU", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}