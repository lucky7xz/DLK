{"textgrid.poem.57335": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nun von ihr denn so gar gellt der zerplauderte", "genre": "verse", "period": "N.A.", "pub_year": 1783, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun von ihr denn so gar gellt der zerplauderte", "tokens": ["Nun", "von", "ihr", "denn", "so", "gar", "gellt", "der", "zer\u00b7plau\u00b7der\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPER", "ADV", "ADV", "ADV", "VVPP", "ART", "ADJA"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Mund des entscheidenden Manns!", "tokens": ["Mund", "des", "ent\u00b7schei\u00b7den\u00b7den", "Manns", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Keiner schweigt ihn: und doch sieht er den Schatten nicht", "tokens": ["Kei\u00b7ner", "schweigt", "ihn", ":", "und", "doch", "sieht", "er", "den", "Schat\u00b7ten", "nicht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PPER", "$.", "KON", "ADV", "VVFIN", "PPER", "ART", "NN", "PTKNEG"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Von der Unsterblichen, hat", "tokens": ["Von", "der", "U\u00b7nsterb\u00b7li\u00b7chen", ",", "hat"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["APPR", "ART", "NN", "$,", "VAFIN"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.5": {"text": "Selbst nicht Tr\u00e4ume von ihm, diese verirrtesten", "tokens": ["Selbst", "nicht", "Tr\u00e4u\u00b7me", "von", "ihm", ",", "die\u00b7se", "ver\u00b7irr\u00b7tes\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "PTKNEG", "NN", "APPR", "PPER", "$,", "PDS", "VVFIN"], "meter": "+-+-+-+--+--", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Aller Gedanken, die sind.", "tokens": ["Al\u00b7ler", "Ge\u00b7dan\u00b7ken", ",", "die", "sind", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PRELS", "VAFIN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.7": {"text": "Fl\u00f6h der bet\u00e4ubende doch endlich zum Sessel, wo", "tokens": ["Fl\u00f6h", "der", "be\u00b7t\u00e4u\u00b7ben\u00b7de", "doch", "end\u00b7lich", "zum", "Ses\u00b7sel", ",", "wo"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["NN", "ART", "ADJA", "ADV", "ADV", "APPRART", "NN", "$,", "PWAV"], "meter": "+--+---+--+-+", "measure": "dactylic.di.plus"}, "line.8": {"text": "Geist gelehrt wird, auf ihn", "tokens": ["Geist", "ge\u00b7lehrt", "wird", ",", "auf", "ihn"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VVPP", "VAFIN", "$,", "APPR", "PPER"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.9": {"text": "Lehrlinge harren, dann stumm seiner Beredtsamkeit", "tokens": ["Lehr\u00b7lin\u00b7ge", "har\u00b7ren", ",", "dann", "stumm", "sei\u00b7ner", "Be\u00b7redt\u00b7sam\u00b7keit"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVINF", "$,", "ADV", "ADJD", "PPOSAT", "NN"], "meter": "-+-+--++--+-+", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Horchen, und durstiges Ohrs.", "tokens": ["Hor\u00b7chen", ",", "und", "durs\u00b7ti\u00b7ges", "Ohrs", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KON", "ADJA", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.11": {"text": "O wie gl\u00fcheten wir, sie, die sich jetzt entw\u00f6lkt,", "tokens": ["O", "wie", "gl\u00fc\u00b7he\u00b7ten", "wir", ",", "sie", ",", "die", "sich", "jetzt", "ent\u00b7w\u00f6lkt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "VVFIN", "PPER", "$,", "PPER", "$,", "PRELS", "PRF", "ADV", "VVPP", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.12": {"text": "Jene Zinne zu sehn!", "tokens": ["Je\u00b7ne", "Zin\u00b7ne", "zu", "sehn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.13": {"text": "Denn dort ist es, o dort, wo sich der Tempel w\u00f6lbt,", "tokens": ["Denn", "dort", "ist", "es", ",", "o", "dort", ",", "wo", "sich", "der", "Tem\u00b7pel", "w\u00f6lbt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "$,", "FM", "ADV", "$,", "PWAV", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Sich die G\u00f6ttin uns zeigt.", "tokens": ["Sich", "die", "G\u00f6t\u00b7tin", "uns", "zeigt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.15": {"text": "Eilt, er keuchet uns nach, auf! den gewundnen Pfad,", "tokens": ["Eilt", ",", "er", "keuc\u00b7het", "uns", "nach", ",", "auf", "!", "den", "ge\u00b7wund\u00b7nen", "Pfad", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPER", "VVFIN", "PPER", "PTKVZ", "$,", "PTKVZ", "$.", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.16": {"text": "Welcher steiler empor", "tokens": ["Wel\u00b7cher", "stei\u00b7ler", "em\u00b7por"], "token_info": ["word", "word", "word"], "pos": ["PWAT", "ADJA", "PTKVZ"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.17": {"text": "Mit dem Felsen sich hebt, dass des be\u00e4ugenden", "tokens": ["Mit", "dem", "Fel\u00b7sen", "sich", "hebt", ",", "dass", "des", "be\u00b7\u00e4u\u00b7gen\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PRF", "VVFIN", "$,", "KOUS", "ART", "ADJA"], "meter": "--+--+-+-+--", "measure": "anapaest.di.plus"}, "line.18": {"text": "Blicke wir endlich entfliehn!", "tokens": ["Bli\u00b7cke", "wir", "end\u00b7lich", "ent\u00b7fliehn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "VVINF", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.19": {"text": "Sehet, der lebende Quell, so zur Betrachtung st\u00e4rkt;", "tokens": ["Se\u00b7het", ",", "der", "le\u00b7ben\u00b7de", "Quell", ",", "so", "zur", "Be\u00b7trach\u00b7tung", "st\u00e4rkt", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ART", "ADJA", "NN", "$,", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "+--+--+-+-+-+", "measure": "dactylic.di.plus"}, "line.20": {"text": "Dran der Schweigenden Blatt.", "tokens": ["Dran", "der", "Schwei\u00b7gen\u00b7den", "Blatt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "ADJA", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.21": {"text": "Schweigen freuet, entflamt, reitzet der Schwierigkeit", "tokens": ["Schwei\u00b7gen", "freu\u00b7et", ",", "ent\u00b7flamt", ",", "reit\u00b7zet", "der", "Schwie\u00b7rig\u00b7keit"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "$,", "VVPP", "$,", "VVFIN", "ART", "NN"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.22": {"text": "K\u00fchn entgegen zu gehn.", "tokens": ["K\u00fchn", "ent\u00b7ge\u00b7gen", "zu", "gehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "PTKZU", "VVINF", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.23": {"text": "Unten dorrte diess Laub, s\u00e4nke; hier oben gr\u00fcnts,", "tokens": ["Un\u00b7ten", "dorr\u00b7te", "diess", "Laub", ",", "s\u00e4n\u00b7ke", ";", "hier", "o\u00b7ben", "gr\u00fcnts", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PDAT", "NN", "$,", "VVFIN", "$.", "ADV", "ADV", "VVFIN", "$,"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.24": {"text": "Festigt den stolzen Entschluss!", "tokens": ["Fes\u00b7tigt", "den", "stol\u00b7zen", "Ent\u00b7schluss", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.25": {"text": "Unten ist Sage nur noch, fabelt es um: man nimt", "tokens": ["Un\u00b7ten", "ist", "Sa\u00b7ge", "nur", "noch", ",", "fa\u00b7belt", "es", "um", ":", "man", "nimt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VAFIN", "NN", "ADV", "ADV", "$,", "VVFIN", "PPER", "PTKVZ", "$.", "PIS", "VVFIN"], "meter": "+--+--+-+-+-+", "measure": "dactylic.di.plus"}, "line.26": {"text": "Dort kein Blatt vor den Mund.", "tokens": ["Dort", "kein", "Blatt", "vor", "den", "Mund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.27": {"text": "Auf! schon t\u00f6net ihr Schritt, naht die Vortreflichkeit", "tokens": ["Auf", "!", "schon", "t\u00f6\u00b7net", "ihr", "Schritt", ",", "naht", "die", "Vor\u00b7tre\u00b7flich\u00b7keit"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "$.", "ADV", "VVFIN", "PPOSAT", "NN", "$,", "VVFIN", "ART", "NN"], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.28": {"text": "In der Halle! Musik", "tokens": ["In", "der", "Hal\u00b7le", "!", "Mu\u00b7sik"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["APPR", "ART", "NN", "$.", "NN"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.29": {"text": "Ist der kommenden Gang, jede der Wendungen", "tokens": ["Ist", "der", "kom\u00b7men\u00b7den", "Gang", ",", "je\u00b7de", "der", "Wen\u00b7dun\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$,", "PIAT", "ART", "NN"], "meter": "+-+--++-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.30": {"text": "Welche sie schwebt, Harmonie!", "tokens": ["Wel\u00b7che", "sie", "schwebt", ",", "Har\u00b7mo\u00b7nie", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.31": {"text": "Jene Blum' in dem Kranz bracht' ihr M\u00e4onides;", "tokens": ["Je\u00b7ne", "Blum'", "in", "dem", "Kranz", "bracht'", "ihr", "M\u00e4o\u00b7ni\u00b7des", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "APPR", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+--+--++-", "measure": "trochaic.penta.relaxed"}, "line.32": {"text": "Und sie nahm sie von ihm:", "tokens": ["Und", "sie", "nahm", "sie", "von", "ihm", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "APPR", "PPER", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.33": {"text": "Jene Leibniz; (gewelkt lag es um sie herum)", "tokens": ["Je\u00b7ne", "Leib\u00b7niz", ";(", "ge\u00b7welkt", "lag", "es", "um", "sie", "he\u00b7rum", ")"], "token_info": ["word", "word", "emoticon", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$(", "VVPP", "VVFIN", "PPER", "APPR", "PPER", "PTKVZ", "$("], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.34": {"text": "Und sie nahm sie von ihm.", "tokens": ["Und", "sie", "nahm", "sie", "von", "ihm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "APPR", "PPER", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.35": {"text": "Freude! nun wendet sie sich gegen uns, steht, und g\u00f6nnt", "tokens": ["Freu\u00b7de", "!", "nun", "wen\u00b7det", "sie", "sich", "ge\u00b7gen", "uns", ",", "steht", ",", "und", "g\u00f6nnt"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["NN", "$.", "ADV", "VVFIN", "PPER", "PRF", "APPR", "PPER", "$,", "VVFIN", "$,", "KON", "VVFIN"], "meter": "+--+-+-+--+-+", "measure": "iambic.hexa.invert"}, "line.36": {"text": "Sich der liebenden Blick,", "tokens": ["Sich", "der", "lie\u00b7ben\u00b7den", "Blick", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.37": {"text": "Sich der Betrachtung! Auch ruhn ihre begeisterten", "tokens": ["Sich", "der", "Be\u00b7trach\u00b7tung", "!", "Auch", "ruhn", "ih\u00b7re", "be\u00b7geis\u00b7ter\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PRF", "ART", "NN", "$.", "ADV", "VVFIN", "PPOSAT", "NN"], "meter": "+--+-+-+--+--", "measure": "iambic.penta.invert"}, "line.38": {"text": "Ideale vom Tanz.", "tokens": ["I\u00b7dea\u00b7le", "vom", "Tanz", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.39": {"text": "\u00bbunser Auge war licht, sah 