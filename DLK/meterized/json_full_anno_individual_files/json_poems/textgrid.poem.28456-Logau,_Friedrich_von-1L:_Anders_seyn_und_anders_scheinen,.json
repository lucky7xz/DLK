{"textgrid.poem.28456": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Anders seyn und anders scheinen,", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Anders seyn und anders scheinen,", "tokens": ["An\u00b7ders", "seyn", "und", "an\u00b7ders", "schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAINF", "KON", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Anders reden, anders meinen,", "tokens": ["An\u00b7ders", "re\u00b7den", ",", "an\u00b7ders", "mei\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVINF", "$,", "ADV", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Alles loben, alles tragen,", "tokens": ["Al\u00b7les", "lo\u00b7ben", ",", "al\u00b7les", "tra\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "VVINF", "$,", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Allen heucheln, stets behagen,", "tokens": ["Al\u00b7len", "heu\u00b7cheln", ",", "stets", "be\u00b7ha\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "VVINF", "$,", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Allem Winde Segel geben,", "tokens": ["Al\u00b7lem", "Win\u00b7de", "Se\u00b7gel", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "B\u00f6s- und Guten dienstbar leben;", "tokens": ["B\u00f6s", "und", "Gu\u00b7ten", "dienst\u00b7bar", "le\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["TRUNC", "KON", "NN", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Alles Thun und alles Tichten", "tokens": ["Al\u00b7les", "Thun", "und", "al\u00b7les", "Tich\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "KON", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Blo\u00df auf eignen Nutzen richten:", "tokens": ["Blo\u00df", "auf", "eig\u00b7nen", "Nut\u00b7zen", "rich\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Wer sich dessen wil befleissen,", "tokens": ["Wer", "sich", "des\u00b7sen", "wil", "be\u00b7fleis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "PDS", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Kan politisch heuer heissen.", "tokens": ["Kan", "po\u00b7li\u00b7tisch", "heu\u00b7er", "heis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "ADV", "VVINF", "$."], "meter": "+---+-+-", "measure": "dactylic.init"}}, "stanza.2": {"line.1": {"text": "Anders seyn und anders scheinen,", "tokens": ["An\u00b7ders", "seyn", "und", "an\u00b7ders", "schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAINF", "KON", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Anders reden, anders meinen,", "tokens": ["An\u00b7ders", "re\u00b7den", ",", "an\u00b7ders", "mei\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVINF", "$,", "ADV", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Alles loben, alles tragen,", "