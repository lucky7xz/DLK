{"dta.poem.18361": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "7.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1886", "urn": "urn:nbn:de:kobv:b4-200905192665", "language": ["de:0.99"], "booktitle": "Holz, Arno: Das Buch der Zeit. Lieder eines Modernen. Z\u00fcrich, 1886."}, "poem": {"stanza.1": {"line.1": {"text": "Der Regen tropft, der Nebel spinnt ...", "tokens": ["Der", "Re\u00b7gen", "tropft", ",", "der", "Ne\u00b7bel", "spinnt", "..."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "O, da\u00df die M\u00e4rchen immer l\u00fcgen,", "tokens": ["O", ",", "da\u00df", "die", "M\u00e4r\u00b7chen", "im\u00b7mer", "l\u00fc\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die M\u00e4rchen, die die Jugend sinnt!", "tokens": ["Die", "M\u00e4r\u00b7chen", ",", "die", "die", "Ju\u00b7gend", "sinnt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie lieblich hat sich einst getrunken", "tokens": ["Wie", "lieb\u00b7lich", "hat", "sich", "einst", "ge\u00b7trun\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VAFIN", "PRF", "ADV", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der Hoffnung goldner Feuerwein!", "tokens": ["Der", "Hoff\u00b7nung", "gold\u00b7ner", "Feu\u00b7er\u00b7wein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und jetzt? Erbarmungslos versunken", "tokens": ["Und", "jetzt", "?", "Er\u00b7bar\u00b7mungs\u00b7los", "ver\u00b7sun\u00b7ken"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["KON", "ADV", "$.", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "In dieses Elend der Spelunken \u2014", "tokens": ["In", "die\u00b7ses", "E\u00b7lend", "der", "Spe\u00b7lun\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "O Sonnenschein! O Sonnenschein!", "tokens": ["O", "Son\u00b7nen\u00b7schein", "!", "O", "Son\u00b7nen\u00b7schein", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "NE", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Nur einmal, einmal noch im Traume", "tokens": ["Nur", "ein\u00b7mal", ",", "ein\u00b7mal", "noch", "im", "Trau\u00b7me"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "$,", "ADV", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "La\u00dft mich hinaus, o Gott, hinaus!", "tokens": ["La\u00dft", "mich", "hin\u00b7aus", ",", "o", "Gott", ",", "hin\u00b7aus", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKVZ", "$,", "FM", "NN", "$,", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Denn s\u00fc\u00df rauscht's nachts im Lindenbaume", "tokens": ["Denn", "s\u00fc\u00df", "rauscht's", "nachts", "im", "Lin\u00b7den\u00b7bau\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Vor meines Vaters F\u00f6rsterhaus.", "tokens": ["Vor", "mei\u00b7nes", "Va\u00b7ters", "F\u00f6rs\u00b7ter\u00b7haus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Mond lugt golden um den Giebel,", "tokens": ["Der", "Mond", "lugt", "gol\u00b7den", "um", "den", "Gie\u00b7bel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der Vater tr\u00e4umt von Mars la Tour,", "tokens": ["Der", "Va\u00b7ter", "tr\u00e4umt", "von", "Mars", "la", "Tour", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "NE", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Lieb M\u00fctterchen studirt die Bibel,", "tokens": ["Lieb", "M\u00fct\u00b7ter\u00b7chen", "stu\u00b7dirt", "die", "Bi\u00b7bel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.8": {"text": "Ihr Nestling colorirt die Fibel", "tokens": ["Ihr", "Nest\u00b7ling", "co\u00b7lo\u00b7rirt", "die", "Fi\u00b7bel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Und leise, leise tickt die Uhr!", "tokens": ["Und", "lei\u00b7se", ",", "lei\u00b7se", "tickt", "die", "Uhr", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "O goldne Lenznacht der Jasminen,", "tokens": ["O", "gold\u00b7ne", "Lenz\u00b7nacht", "der", "Jas\u00b7mi\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "O, w\u00e4r ich niemals dir entr\u00fcckt!", "tokens": ["O", ",", "w\u00e4r", "ich", "nie\u00b7mals", "dir", "ent\u00b7r\u00fcckt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VAFIN", "PPER", "ADV", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das ewge R\u00e4dern der Maschinen", "tokens": ["Das", "ew\u00b7ge", "R\u00e4\u00b7dern", "der", "Ma\u00b7schi\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hat mir das Hirn zerpfl\u00fcckt, zerst\u00fcckt!", "tokens": ["Hat", "mir", "das", "Hirn", "zer\u00b7pfl\u00fcckt", ",", "zer\u00b7st\u00fcckt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "VVPP", "$,", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Einst schlich ich aus dem Haus der V\u00e4ter", "tokens": ["Einst", "schlich", "ich", "aus", "dem", "Haus", "der", "V\u00e4\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "PPER", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Nachts in die Welt mich wie ein Dieb", "tokens": ["Nachts", "in", "die", "Welt", "mich", "wie", "ein", "Dieb"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "PPER", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und heut \u2014 drei kurze J\u00e4hrchen sp\u00e4ter! \u2014", "tokens": ["Und", "heut", "drei", "kur\u00b7ze", "J\u00e4hr\u00b7chen", "sp\u00e4\u00b7ter", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADV", "$(", "CARD", "ADJA", "NN", "ADJD", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Wie ein geschlagner Misseth\u00e4ter,", "tokens": ["Wie", "ein", "ge\u00b7schlag\u00b7ner", "Mis\u00b7set\u00b7h\u00e4\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Schluchz ich: Vergieb, o Gott, vergieb!", "tokens": ["Schluchz", "ich", ":", "Ver\u00b7gieb", ",", "o", "Gott", ",", "ver\u00b7gieb", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "PPER", "$.", "NN", "$,", "FM", "NN", "$,", "VVIMP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wozu dein armes Hirn zerw\u00fchlen?", "tokens": ["Wo\u00b7zu", "dein", "ar\u00b7mes", "Hirn", "zer\u00b7w\u00fch\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du gr\u00fcbelst und die Weltlust lacht!", "tokens": ["Du", "gr\u00fc\u00b7belst", "und", "die", "Welt\u00b7lust", "lacht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Denn von Gedanken, von Gef\u00fchlen,", "tokens": ["Denn", "von", "Ge\u00b7dan\u00b7ken", ",", "von", "Ge\u00b7f\u00fch\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "$,", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hat noch kein Mensch sich satt gemacht!", "tokens": ["Hat", "noch", "kein", "Mensch", "sich", "satt", "ge\u00b7macht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PIAT", "NN", "PRF", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ja, Recht hat, o du s\u00fc\u00dfe Mutter,", "tokens": ["Ja", ",", "Recht", "hat", ",", "o", "du", "s\u00fc\u00b7\u00dfe", "Mut\u00b7ter", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "NN", "VAFIN", "$,", "FM", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Dein Spruch, vor dem's mir stets gegraust:", "tokens": ["Dein", "Spruch", ",", "vor", "dem's", "mir", "stets", "ge\u00b7graust", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "APPR", "PRELS", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Was soll uns Shakespeare, Kant und Luther?", "tokens": ["Was", "soll", "uns", "Sha\u00b7ke\u00b7spe\u00b7a\u00b7re", ",", "Kant", "und", "Lu\u00b7ther", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "NE", "$,", "NE", "KON", "NE", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Dem Elend d\u00fcnkt ein St\u00fcckchen Butter", "tokens": ["Dem", "E\u00b7lend", "d\u00fcnkt", "ein", "St\u00fcck\u00b7chen", "But\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Erhabner als der ganze Faust!", "tokens": ["Er\u00b7hab\u00b7ner", "als", "der", "gan\u00b7ze", "Faust", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "KOKOM", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}