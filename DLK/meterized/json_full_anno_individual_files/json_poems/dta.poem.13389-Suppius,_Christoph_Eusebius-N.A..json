{"dta.poem.13389": {"metadata": {"author": {"name": "Suppius, Christoph Eusebius", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1749", "urn": "urn:nbn:de:kobv:b4-20594-7", "language": ["de:0.99"], "booktitle": "Suppius, Christoph Eusebius: Oden und Lieder. Gotha, 1749."}, "poem": {"stanza.1": {"line.1": {"text": "Mein", "tokens": ["Mein"], "token_info": ["word"], "pos": ["PPOSAT"], "meter": "-", "measure": "single.down"}, "line.2": {"text": "Ein Trieb der Freude reizet mich,\nund die oft wiederholte Sage,", "tokens": ["Ein", "Trieb", "der", "Freu\u00b7de", "rei\u00b7zet", "mich", ",", "und", "die", "oft", "wie\u00b7der\u00b7hol\u00b7te", "Sa\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "PPER", "$,", "KON", "ART", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.3": {"text": "da\u00df", "tokens": ["da\u00df"], "token_info": ["word"], "pos": ["KOUS"], "meter": "-", "measure": "single.down"}, "line.4": {"text": "Ein langer Fr\u00fchling wird", "tokens": ["Ein", "lan\u00b7ger", "Fr\u00fch\u00b7ling", "wird"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "zeigt", "tokens": ["zeigt"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "+", "measure": "single.up"}, "line.6": {"text": "ein tiefgebeugter Unterthan,", "tokens": ["ein", "tief\u00b7ge\u00b7beug\u00b7ter", "Un\u00b7ter\u00b7than", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "der", "tokens": ["der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}}}}}