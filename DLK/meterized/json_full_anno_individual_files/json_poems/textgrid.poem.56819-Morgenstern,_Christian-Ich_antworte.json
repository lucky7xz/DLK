{"textgrid.poem.56819": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Ich antworte", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach wenn ich gewinnen k\u00f6nnte", "tokens": ["Ach", "wenn", "ich", "ge\u00b7win\u00b7nen", "k\u00f6nn\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ITJ", "KOUS", "PPER", "VVINF", "VMFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "kindesweich noch wie vor Jahren!", "tokens": ["kin\u00b7des\u00b7weich", "noch", "wie", "vor", "Jah\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "KOKOM", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Allzufr\u00fch schon, Mutter, g\u00f6nnte", "tokens": ["All\u00b7zu\u00b7fr\u00fch", "schon", ",", "Mut\u00b7ter", ",", "g\u00f6nn\u00b7te"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["ADJD", "ADV", "$,", "NN", "$,", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "mir mein Stern, allein zu fahren.", "tokens": ["mir", "mein", "Stern", ",", "al\u00b7lein", "zu", "fah\u00b7ren", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "$,", "ADV", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Kannst mir Lieb' und Heimat geben?", "tokens": ["Kannst", "mir", "Lieb'", "und", "Hei\u00b7mat", "ge\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "NN", "KON", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "F\u00fcr mich Tote neu mir schenken?", "tokens": ["F\u00fcr", "mich", "To\u00b