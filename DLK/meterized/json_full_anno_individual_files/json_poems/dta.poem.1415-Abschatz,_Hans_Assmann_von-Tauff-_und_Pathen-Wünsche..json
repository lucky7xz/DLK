{"dta.poem.1415": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Tauff- und Pathen-W\u00fcnsche.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Wohl/ Pathe/ dir/ denn dich befreyt das neue Jahr", "tokens": ["Wohl", "/", "Pa\u00b7the", "/", "dir", "/", "denn", "dich", "be\u00b7freyt", "das", "neu\u00b7e", "Jahr"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$(", "NE", "$(", "PPER", "$(", "KON", "PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der alten Adams-Schuld/ die angeerbet war/", "tokens": ["Der", "al\u00b7ten", "A\u00b7dams\u00b7Schuld", "/", "die", "an\u00b7ge\u00b7er\u00b7bet", "war", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "VVPP", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Du trittst den neuen Bund mit GOtt im Tauff-Bad ein:", "tokens": ["Du", "trittst", "den", "neu\u00b7en", "Bund", "mit", "Gott", "im", "Tauff\u00b7Bad", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "APPR", "NN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die G\u00fctte GOttes mu\u00df auch t\u00e4glich neu dir seyn/", "tokens": ["Die", "G\u00fct\u00b7te", "Got\u00b7tes", "mu\u00df", "auch", "t\u00e4g\u00b7lich", "neu", "dir", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VMFIN", "ADV", "ADJD", "ADJD", "PPER", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Mit neuem Segen dir/ den Deinen beyzustehn/", "tokens": ["Mit", "neu\u00b7em", "Se\u00b7gen", "dir", "/", "den", "Dei\u00b7nen", "bey\u00b7zu\u00b7stehn", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "$(", "ART", "PPOSAT", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Bi\u00df du ins neue Land der Ewigkeit wirst gehn.", "tokens": ["Bi\u00df", "du", "ins", "neu\u00b7e", "Land", "der", "E\u00b7wig\u00b7keit", "wirst", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "ADJA", "NN", "ART", "NN", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}