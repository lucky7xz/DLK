{"dta.poem.13160": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Neuntes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Thalia fuhr nun fort: \u201eVerzeiht mir das Beginnen,", "tokens": ["Tha\u00b7lia", "fuhr", "nun", "fort", ":", "\u201e", "Ver\u00b7zeiht", "mir", "das", "Be\u00b7gin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "PTKVZ", "$.", "$(", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "\u201edas nur erdichte Werck, Kunstreiche Meisterinnen", "tokens": ["\u201e", "das", "nur", "er\u00b7dich\u00b7te", "Werck", ",", "Kuns\u00b7trei\u00b7che", "Meis\u00b7te\u00b7rin\u00b7nen"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["$(", "PDS", "ADV", "ADJA", "NN", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "\u201eund h\u00f6rt mich ferner an! ich schreite zu der Sach:", "tokens": ["\u201e", "und", "h\u00f6rt", "mich", "fer\u00b7ner", "an", "!", "ich", "schrei\u00b7te", "zu", "der", "Sach", ":"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "VVFIN", "PPER", "ADV", "PTKVZ", "$.", "PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u201eich gehe, wie man h\u00f6rt, der Dicht-Kunst Lehre nach.", "tokens": ["\u201e", "ich", "ge\u00b7he", ",", "wie", "man", "h\u00f6rt", ",", "der", "Dicht\u00b7Kunst", "Leh\u00b7re", "nach", "."], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "$,", "PWAV", "PIS", "VVFIN", "$,", "ART", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "\u201egar wohl: es ist ein Herz, was ich beschreiben wollte;", "tokens": ["\u201e", "gar", "wohl", ":", "es", "ist", "ein", "Herz", ",", "was", "ich", "be\u00b7schrei\u00b7ben", "woll\u00b7te", ";"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADV", "$.", "PPER", "VAFIN", "ART", "NN", "$,", "PWS", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "260\u201dIch hoffte, da\u00df der Bau den Rath vergn\u00fcgen sollte.", "tokens": ["\"", "Ich", "hoff\u00b7te", ",", "da\u00df", "der", "Bau", "den", "Rath", "ver\u00b7gn\u00fc\u00b7gen", "soll\u00b7te", "."], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "$,", "KOUS", "ART", "NN", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "\u201edarinnen sehet ihr den Geist, den Muth, den Sinn,", "tokens": ["\u201e", "da\u00b7rin\u00b7nen", "se\u00b7het", "ihr", "den", "Geist", ",", "den", "Muth", ",", "den", "Sinn", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "\u201edie St\u00e4rcke, das Gem\u00fcth, die Macht der K\u00f6niginn.", "tokens": ["\u201e", "die", "St\u00e4r\u00b7cke", ",", "das", "Ge\u00b7m\u00fcth", ",", "die", "Macht", "der", "K\u00f6\u00b7ni\u00b7ginn", "."], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "\u201eso hoch, so lang, so weit als ich ihn aufgef\u00fchret,", "tokens": ["\u201e", "so", "hoch", ",", "so", "lang", ",", "so", "weit", "als", "ich", "ihn", "auf\u00b7ge\u00b7f\u00fch\u00b7ret", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,", "ADV", "ADJD", "KOKOM", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "\u201eist jener Raum der Welt, der dieser Frau geb\u00fchret.", "tokens": ["\u201e", "ist", "je\u00b7ner", "Raum", "der", "Welt", ",", "der", "die\u00b7ser", "Frau", "ge\u00b7b\u00fch\u00b7ret", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "PDAT", "NN", "ART", "NN", "$,", "PRELS", "PDAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "265\u201dSo viel er Glanz begreifft, so viel ist auch in ihr;", "tokens": ["\"", "So", "viel", "er", "Glanz", "be\u00b7greifft", ",", "so", "viel", "ist", "auch", "in", "ihr", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADV", "PPER", "NN", "VVPP", "$,", "ADV", "ADV", "VAFIN", "ADV", "APPR", "PPOSAT", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "\u201eder Strahlen Majest\u00e4t ist ihre Pracht und Zier.", "tokens": ["\u201e", "der", "Strah\u00b7len", "Ma\u00b7jes\u00b7t\u00e4t", "ist", "ih\u00b7re", "Pracht", "und", "Zier", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "NN", "VAFIN", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "\u201efragt ihr was dieses Paar der Sonnen soll bedeuten?", "tokens": ["\u201e", "fragt", "ihr", "was", "die\u00b7ses", "Paar", "der", "Son\u00b7nen", "soll", "be\u00b7deu\u00b7ten", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "PIS", "PDAT", "NN", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "\u201edie seynd des ganzen Wercks vornehmste Trefflichkeiten:", "tokens": ["\u201e", "die", "seynd", "des", "gan\u00b7zen", "Wercks", "vor\u00b7nehms\u00b7te", "Treff\u00b7lich\u00b7kei\u00b7ten", ":"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PDS", "VAFIN", "ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "\u201ewer kennet diese nicht? ist jemand in dem Saal?", "tokens": ["\u201e", "wer", "ken\u00b7net", "die\u00b7se", "nicht", "?", "ist", "je\u00b7mand", "in", "dem", "Saal", "?"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VVFIN", "PDS", "PTKNEG", "$.", "VAFIN", "PIS", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "270\u201dEs ist Theresia mit ihrem Ehgemahl.", "tokens": ["\"", "Es", "ist", "The\u00b7re\u00b7sia", "mit", "ih\u00b7rem", "Eh\u00b7ge\u00b7mahl", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "NE", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.17": {"text": "\u201ehat nicht der gute Rath erkl\u00e4ret und erwogen:", "tokens": ["\u201e", "hat", "nicht", "der", "gu\u00b7te", "Rath", "er\u00b7kl\u00e4\u00b7ret", "und", "er\u00b7wo\u00b7gen", ":"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "PTKNEG", "ART", "ADJA", "NN", "VVFIN", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "\u201ewie beyder Herzen sich in Eins zusammen zogen?", "tokens": ["\u201e", "wie", "bey\u00b7der", "Her\u00b7zen", "sich", "in", "Eins", "zu\u00b7sam\u00b7men", "zo\u00b7gen", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "PIAT", "NN", "PRF", "APPR", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}