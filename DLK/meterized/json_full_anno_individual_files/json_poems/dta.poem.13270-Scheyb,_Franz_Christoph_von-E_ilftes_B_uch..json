{"dta.poem.13270": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "E ilftes  B uch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201eob also gleich der Ruff des Volcks sich sollte mehren,", "tokens": ["\u201e", "ob", "al\u00b7so", "gleich", "der", "Ruff", "des", "Volcks", "sich", "soll\u00b7te", "meh\u00b7ren", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "ADV", "ADV", "ART", "NN", "ART", "NN", "PRF", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "\u201eso find ich nicht was er zuk\u00fcnftiges kann lehren.", "tokens": ["\u201e", "so", "find", "ich", "nicht", "was", "er", "zu\u00b7k\u00fcnf\u00b7ti\u00b7ges", "kann", "leh\u00b7ren", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "PTKNEG", "PWS", "PPER", "ADJA", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "\u201eder P\u00f6bel w\u00fcnscht, verschm\u00e4ht, liebt, ha\u00dft, flieht und begehrt,", "tokens": ["\u201e", "der", "P\u00f6\u00b7bel", "w\u00fcnscht", ",", "ver\u00b7schm\u00e4ht", ",", "liebt", ",", "ha\u00dft", ",", "flieht", "und", "be\u00b7gehrt", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "$,", "VVPP", "$,", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u201eund alles achtet er vor richtig und bew\u00e4hrt.", "tokens": ["\u201e", "und", "al\u00b7les", "ach\u00b7tet", "er", "vor", "rich\u00b7tig", "und", "be\u00b7w\u00e4hrt", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "PIS", "VVFIN", "PPER", "APPR", "ADJD", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "\u201edas Vorurtheil, der Stolz bewohnen sein Gem\u00fcthe;", "tokens": ["\u201e", "das", "Vor\u00b7urt\u00b7heil", ",", "der", "Stolz", "be\u00b7woh\u00b7nen", "sein", "Ge\u00b7m\u00fc\u00b7the", ";"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "$,", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "460\u201dEr folgt auch einem Traum, wann er ihm etwas riethe.", "tokens": ["\"", "Er", "folgt", "auch", "ei\u00b7nem", "Traum", ",", "wann", "er", "ihm", "et\u00b7was", "rie\u00b7the", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "ADV", "ART", "NN", "$,", "PWAV", "PPER", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "\u201ewas seiner Sinnen Kraft und Einfalt nicht erkennt,", "tokens": ["\u201e", "was", "sei\u00b7ner", "Sin\u00b7nen", "Kraft", "und", "Ein\u00b7falt", "nicht", "er\u00b7kennt", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "PPOSAT", "NN", "NN", "KON", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "\u201ewird Ungl\u00fcck oder Gl\u00fcck von seinem Wahn genennt.", "tokens": ["\u201e", "wird", "Un\u00b7gl\u00fcck", "o\u00b7der", "Gl\u00fcck", "von", "sei\u00b7nem", "Wahn", "ge\u00b7nennt", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "NN", "KON", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "\u201ejhm ist die Wissenschaft, der klugen Macht und St\u00e4rcke", "tokens": ["\u201e", "jhm", "ist", "die", "Wis\u00b7sen\u00b7schaft", ",", "der", "klu\u00b7gen", "Macht", "und", "St\u00e4r\u00b7cke"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VAFIN", "ART", "NN", "$,", "ART", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "\u201eein Vorwand des Betrugs, ein Schein der Wunderwercke.", "tokens": ["\u201e", "ein", "Vor\u00b7wand", "des", "Be\u00b7trugs", ",", "ein", "Schein", "der", "Wun\u00b7der\u00b7wer\u00b7cke", "."], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "ART", "NN", "$,", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "465\u201dEr sieht, was k\u00fcnftig ist, nicht was vor Augen steht;", "tokens": ["\"", "Er", "sieht", ",", "was", "k\u00fcnf\u00b7tig", "ist", ",", "nicht", "was", "vor", "Au\u00b7gen", "steht", ";"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "$,", "PRELS", "ADJD", "VAFIN", "$,", "PTKNEG", "PWS", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "\u201eer fragt nicht wie es hier, nein: wie es dorten geht.", "tokens": ["\u201e", "er", "fragt", "nicht", "wie", "es", "hier", ",", "nein", ":", "wie", "es", "dor\u00b7ten", "geht", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PTKNEG", "PWAV", "PPER", "ADV", "$,", "PTKANT", "$.", "PWAV", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "\u201ewas meine M\u00fche, Flei\u00df, Sorg\u2019, Arbeit, Kunst verm\u00f6gen,", "tokens": ["\u201e", "was", "mei\u00b7ne", "M\u00fc\u00b7he", ",", "Flei\u00df", ",", "Sor\u00b7g'", ",", "Ar\u00b7beit", ",", "Kunst", "ver\u00b7m\u00f6\u00b7gen", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PWS", "PPOSAT", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "VVFIN", "$,"], "meter": "+--+--+-+-+-+-", "measure": "elegiambus"}, "line.14": {"text": "\u201edem pflegt er den Begriff der Falschheit beyzulegen.", "tokens": ["\u201e", "dem", "pflegt", "er", "den", "Be\u00b7griff", "der", "Falschheit", "bey\u00b7zu\u00b7le\u00b7gen", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PDS", "VVFIN", "PPER", "ART", "NN", "ART", "NN", "VVIZU", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.15": {"text": "\u201eer tadelt mein Bem\u00fchn. Was er am Abend schilt,", "tokens": ["\u201e", "er", "ta\u00b7delt", "mein", "Be\u00b7m\u00fchn", ".", "Was", "er", "am", "A\u00b7bend", "schilt", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPOSAT", "NN", "$.", "PWS", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "470\u201dIst, was am Morgen mehr, als was er lobte, gilt.", "tokens": ["\"", "Ist", ",", "was", "am", "Mor\u00b7gen", "mehr", ",", "als", "was", "er", "lob\u00b7te", ",", "gilt", "."], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "VAFIN", "$,", "PRELS", "APPRART", "NN", "ADV", "$,", "KOUS", "PRELS", "PPER", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "\u201eich richte, was ich will, so treff\u2019 ich keine Sache,", "tokens": ["\u201e", "ich", "rich\u00b7te", ",", "was", "ich", "will", ",", "so", "treff'", "ich", "kei\u00b7ne", "Sa\u00b7che", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "$,", "PWS", "PPER", "VMFIN", "$,", "ADV", "VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "\u201eaus der sein Aberwiz nicht etwas anders mache.", "tokens": ["\u201e", "aus", "der", "sein", "A\u00b7ber\u00b7wiz", "nicht", "et\u00b7was", "an\u00b7ders", "ma\u00b7che", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "ART", "PPOSAT", "NN", "PTKNEG", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "\u201enichts ist, worauf er nicht des Tadels M\u00e4rckmahl schl\u00e4gt,", "tokens": ["\u201e", "nichts", "ist", ",", "wo\u00b7rauf", "er", "nicht", "des", "Ta\u00b7dels", "M\u00e4rck\u00b7mahl", "schl\u00e4gt", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PIS", "VAFIN", "$,", "PWAV", "PPER", "PTKNEG", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "\u201eer, der doch selber es auf seiner Stirne tr\u00e4gt.", "tokens": ["\u201e", "er", ",", "der", "doch", "sel\u00b7ber", "es", "auf", "sei\u00b7ner", "Stir\u00b7ne", "tr\u00e4gt", "."], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "$,", "PRELS", "ADV", "ADV", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "475\u201dEr l\u00e4\u00dft der Gallen-Sucht die Oberhand erlangen:", "tokens": ["\"", "Er", "l\u00e4\u00dft", "der", "Gal\u00b7len\u00b7Sucht", "die", "O\u00b7ber\u00b7hand", "er\u00b7lan\u00b7gen", ":"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "\u201ewas er besieht, das mu\u00df in gelber Farbe prangen.", "tokens": ["\u201e", "was", "er", "be\u00b7sieht", ",", "das", "mu\u00df", "in", "gel\u00b7ber", "Far\u00b7be", "pran\u00b7gen", "."], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "PPER", "VVFIN", "$,", "PDS", "VMFIN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "\u201edann, weil sein Eigensinn nur seinem Auge traut,", "tokens": ["\u201e", "dann", ",", "weil", "sein", "Ei\u00b7gen\u00b7sinn", "nur", "sei\u00b7nem", "Au\u00b7ge", "traut", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "$,", "KOUS", "PPOSAT", "NN", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "\u201eso ists, da\u00df er das Blau oft wie das Gelbe schaut.", "tokens": ["\u201e", "so", "ists", ",", "da\u00df", "er", "das", "Blau", "oft", "wie", "das", "Gel\u00b7be", "schaut", "."], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VAFIN", "$,", "KOUS", "PPER", "ART", "NN", "ADV", "KOKOM", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "\u201ewie viel Mahl hab ich nicht den sch\u00e4rffsten Streich verhindert?", "tokens": ["\u201e", "wie", "viel", "Mahl", "hab", "ich", "nicht", "den", "sch\u00e4rffs\u00b7ten", "Streich", "ver\u00b7hin\u00b7dert", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "PIAT", "NN", "VAFIN", "PPER", "PTKNEG", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "480\u201dDa rieff er: alles Wohl der L\u00e4nder sey gemindert.", "tokens": ["\"", "Da", "rieff", "er", ":", "al\u00b7les", "Wohl", "der", "L\u00e4n\u00b7der", "sey", "ge\u00b7min\u00b7dert", "."], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "$.", "PIS", "ADV", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "\u201eer kennt nicht, was er sieht; wie w\u00e4r ihm dann bewu\u00dft", "tokens": ["\u201e", "er", "kennt", "nicht", ",", "was", "er", "sieht", ";", "wie", "w\u00e4r", "ihm", "dann", "be\u00b7wu\u00dft"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VVFIN", "PTKNEG", "$,", "PWS", "PPER", "VVFIN", "$.", "PWAV", "VAFIN", "PPER", "ADV", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "\u201ewas die Geheimnisse der Weisheit leiten mu\u00dft?", "tokens": ["\u201e", "was", "die", "Ge\u00b7heim\u00b7nis\u00b7se", "der", "Weis\u00b7heit", "lei\u00b7ten", "mu\u00dft", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "ART", "NN", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "\u201ebald steigt er in die Luft; bald kriecht er wie die Schlangen", "tokens": ["\u201e", "bald", "steigt", "er", "in", "die", "Luft", ";", "bald", "kriecht", "er", "wie", "die", "Schlan\u00b7gen"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$.", "ADV", "VVFIN", "PPER", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "\u201edie hundert-k\u00f6pficht seynd, sich im Gestr\u00e4uche fangen.", "tokens": ["\u201e", "die", "hun\u00b7der\u00b7tk\u00f6p\u00b7ficht", "seynd", ",", "sich", "im", "Ge\u00b7str\u00e4u\u00b7che", "fan\u00b7gen", "."], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VAFIN", "$,", "PRF", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "485\u201dNicht Schl\u00fcsse der Vernunft seynd seines Raths Beweis;", "tokens": ["\"", "Nicht", "Schl\u00fcs\u00b7se", "der", "Ver\u00b7nunft", "seynd", "sei\u00b7nes", "Raths", "Be\u00b7weis", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKNEG", "NN", "ART", "NN", "VAFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "\u201ejm Wohlgefallen nur beruht der Wahrheit Preis.", "tokens": ["\u201e", "jm", "Wohl\u00b7ge\u00b7fal\u00b7len", "nur", "be\u00b7ruht", "der", "Wahr\u00b7heit", "Preis", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "NN", "ADV", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "\u201eer weis die Triebe nicht, die Nuz und Schaden bringen;", "tokens": ["\u201e", "er", "weis", "die", "Trie\u00b7be", "nicht", ",", "die", "Nuz", "und", "Scha\u00b7den", "brin\u00b7gen", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "PTKVZ", "ART", "NN", "PTKNEG", "$,", "ART", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "\u201eer glaubt, man k\u00f6nne sie mit Troz und Hochmuth zwingen.", "tokens": ["\u201e", "er", "glaubt", ",", "man", "k\u00f6n\u00b7ne", "sie", "mit", "Troz", "und", "Hoch\u00b7muth", "zwin\u00b7gen", "."], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "$,", "PIS", "VMFIN", "PPER", "APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "\u201eer scheut das Ungl\u00fcck nicht, in dem er oftmahls wanckt;", "tokens": ["\u201e", "er", "scheut", "das", "Un\u00b7gl\u00fcck", "nicht", ",", "in", "dem", "er", "oft\u00b7mahls", "wanckt", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "ART", "NN", "PTKNEG", "$,", "APPR", "PRELS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "490\u201dOft wird von ihm das Heil um Unheil abgedanckt.", "tokens": ["\"", "Oft", "wird", "von", "ihm", "das", "Heil", "um", "Un\u00b7heil", "ab\u00b7ge\u00b7danckt", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VAFIN", "APPR", "PPER", "ART", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "\u201ewann sich sein Wohlergehn soll mit dem Staat verbinden,", "tokens": ["\u201e", "wann", "sich", "sein", "Woh\u00b7ler\u00b7gehn", "soll", "mit", "dem", "Staat", "ver\u00b7bin\u00b7den", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "PRF", "PPOSAT", "NN", "VMFIN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "\u201eso l\u00e4\u00dft er sein Gem\u00fcth durch Mi\u00dftraun \u00fcberwinden.", "tokens": ["\u201e", "so", "l\u00e4\u00dft", "er", "sein", "Ge\u00b7m\u00fcth", "durch", "Mi\u00df\u00b7traun", "\u00fc\u00b7berw\u00b7in\u00b7den", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "PPOSAT", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "\u201esein wanckelbares Herz entscheidt nicht, was es will;", "tokens": ["\u201e", "sein", "wan\u00b7ckel\u00b7ba\u00b7res", "Herz", "ent\u00b7scheidt", "nicht", ",", "was", "es", "will", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "ADJA", "NN", "VVFIN", "PTKNEG", "$,", "PWS", "PPER", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "\u201ees seufzt im gr\u00f6\u00dften Gl\u00fcck, im Ungl\u00fcck schweigt es still.", "tokens": ["\u201e", "es", "seufzt", "im", "gr\u00f6\u00df\u00b7ten", "Gl\u00fcck", ",", "im", "Un\u00b7gl\u00fcck", "schweigt", "es", "still", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "APPRART", "ADJA", "NN", "$,", "APPRART", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "495\u201dVon ihm wird weder Gut noch Uebel abgemessen,", "tokens": ["\"", "Von", "ihm", "wird", "we\u00b7der", "Gut", "noch", "Ue\u00b7bel", "ab\u00b7ge\u00b7mes\u00b7sen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PPER", "VAFIN", "KON", "ADJD", "ADV", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "\u201eein unverhoffter Fall bringt beydes in Vergessen.", "tokens": ["\u201e", "ein", "un\u00b7ver\u00b7hoff\u00b7ter", "Fall", "bringt", "bey\u00b7des", "in", "Ver\u00b7ges\u00b7sen", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADJA", "NN", "VVFIN", "PIS", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "\u201ejhm scheinet ein Gespr\u00e4ch der Weisheit nicht so sch\u00f6n,", "tokens": ["\u201e", "jhm", "schei\u00b7net", "ein", "Ge\u00b7spr\u00e4ch", "der", "Weis\u00b7heit", "nicht", "so", "sch\u00f6n", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "ART", "NN", "ART", "NN", "PTKNEG", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "\u201eals eines Froschs Geschw\u00e4z, als einer Leyr Geth\u00f6n.", "tokens": ["\u201e", "als", "ei\u00b7nes", "Froschs", "Ge\u00b7schw\u00e4z", ",", "als", "ei\u00b7ner", "Leyr", "Ge\u00b7th\u00f6n", "."], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "ART", "ADJA", "NN", "$,", "KOUS", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "\u201esein bl\u00f6des