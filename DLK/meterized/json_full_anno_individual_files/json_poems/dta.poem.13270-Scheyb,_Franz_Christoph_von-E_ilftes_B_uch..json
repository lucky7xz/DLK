+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.49": {"text": "\u201eund will die Sonne nicht; warum? er strebt nach Regen,", "tokens": ["\u201e", "und", "will", "die", "Son\u00b7ne", "nicht", ";", "wa\u00b7rum", "?", "er", "strebt", "nach", "Re\u00b7gen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "VMFIN", "ART", "NN", "PTKNEG", "$.", "PWAV", "$.", "PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.50": {"text": "\u201ebey dem ist ihm sodann am Sonnen-Schein gelegen.", "tokens": ["\u201e", "bey", "dem", "ist", "ihm", "so\u00b7dann", "am", "Son\u00b7nen\u00b7Schein", "ge\u00b7le\u00b7gen", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "ART", "VAFIN", "PPER", "ADV", "APPRART", "NN", "VVPP", "$."], "meter": "---+-+-+-+-+-", "measure": "unknown.measure.penta"}, "line.51": {"text": "505\u201dSteht Ordnung und Gesaz zu seinem Wohl bereit,", "tokens": ["\"", "Steht", "Ord\u00b7nung", "und", "Ge\u00b7saz", "zu", "sei\u00b7nem", "Wohl", "be\u00b7reit", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "NN", "KON", "NN", "APPR", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.52": {"text": "\u201eso sagt er, beydes sey Zwang und Bottm\u00e4ssigkeit.", "tokens": ["\u201e", "so", "sagt", "er", ",", "bey\u00b7des", "sey", "Zwang", "und", "Bott\u00b7m\u00e4s\u00b7sig\u00b7keit", "."], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "$,", "PIS", "VAFIN", "NN", "KON", "NN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.53": {"text": "\u201eer schont der F\u00fcrsten nicht, der allerbesten F\u00fcrsten,", "tokens": ["\u201e", "er", "schont", "der", "F\u00fcrs\u00b7ten", "nicht", ",", "der", "al\u00b7ler\u00b7bes\u00b7ten", "F\u00fcrs\u00b7ten", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "ART", "NN", "PTKNEG", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.54": {"text": "\u201eer glaubt ihr Herrschen sey nach seinen G\u00fctern d\u00fcrsten.", "tokens": ["\u201e", "er", "glaubt", "ihr", "Herr\u00b7schen", "sey", "nach", "sei\u00b7nen", "G\u00fc\u00b7tern", "d\u00fcrs\u00b7ten", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPOSAT", "NN", "VAFIN", "APPR", "PPOSAT", "NN", "VMFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.55": {"text": "\u201eer argwohnt, da\u00df der Thron, worauf man sie erh\u00f6ht,", "tokens": ["\u201e", "er", "arg\u00b7wohnt", ",", "da\u00df", "der", "Thron", ",", "wo\u00b7rauf", "man", "sie", "er\u00b7h\u00f6ht", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "$,", "KOUS", "ART", "NN", "$,", "PWAV", "PIS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.56": {"text": "510\u201dNur auf des Unterthans beschwerten Schultern steht.", "tokens": ["\"", "Nur", "auf", "des", "Un\u00b7ter\u00b7thans", "be\u00b7schwer\u00b7ten", "Schul\u00b7tern", "steht", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "APPR", "ART", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}