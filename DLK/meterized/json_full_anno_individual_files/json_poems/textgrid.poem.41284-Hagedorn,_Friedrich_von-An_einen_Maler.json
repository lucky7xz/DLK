{"textgrid.poem.41284": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "An einen Maler", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Willst du den Stolz f\u00fcr alle kenntlich malen,", "tokens": ["Willst", "du", "den", "Stolz", "f\u00fcr", "al\u00b7le", "kennt\u00b7lich", "ma\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "APPR", "PIS", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So la\u00df den Muth ihm aus den Augen strahlen!", "tokens": ["So", "la\u00df", "den", "Muth", "ihm", "aus", "den", "Au\u00b7gen", "strah\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "ART", "NN", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sein Blick sei Hohn: ein Trotz, der herrisch droht,", "tokens": ["Sein", "Blick", "sei", "Hohn", ":", "ein", "Trotz", ",", "der", "her\u00b7risch", "droht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "NN", "$.", "ART", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Kr\u00fcmm' ihm den Mund, f\u00e4rb' ihm die Wangen roth:", "tokens": ["Kr\u00fcmm'", "ihm", "den", "Mund", ",", "f\u00e4rb'", "ihm", "die", "Wan\u00b7gen", "roth", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "VVFIN", "PPER", "ART", "NN", "ADJD", "$."], "meter": "++-+-+-+-+", "measure": "iambic.penta.spondeus"}, "lin