{"dta.poem.6663": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Geh\u00f6r.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es ist nicht in der Lufft der Ton, wie man vermeint,", "tokens": ["Es", "ist", "nicht", "in", "der", "Lufft", "der", "Ton", ",", "wie", "man", "ver\u00b7meint", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "APPR", "ART", "NN", "ART", "NN", "$,", "PWAV", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und C\u00f6rper, welche klingen,", "tokens": ["Und", "C\u00f6r\u00b7per", ",", "wel\u00b7che", "klin\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "NE", "$,", "PRELS", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die haben nicht den Klang in ihnen, wie es scheint.", "tokens": ["Die", "ha\u00b7ben", "nicht", "den", "Klang", "in", "ih\u00b7nen", ",", "wie", "es", "scheint", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PTKNEG", "ART", "NN", "APPR", "PPER", "$,", "PWAV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Es mag ein laut Geheul\u2019 uns in die Ohren dringen", "tokens": ["Es", "mag", "ein", "laut", "Ge\u00b7heul'", "uns", "in", "die", "Oh\u00b7ren", "drin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "APPR", "NN", "PPER", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Vom Walde, der gepeitscht durch wilder Winde Wuth;", "tokens": ["Vom", "Wal\u00b7de", ",", "der", "ge\u00b7peitscht", "durch", "wil\u00b7der", "Win\u00b7de", "Wuth", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "VVPP", "APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Man h\u00f6re das Gebr\u00fcll der aufgebrachten Fluth.", "tokens": ["Man", "h\u00f6\u00b7re", "das", "Ge\u00b7br\u00fcll", "der", "auf\u00b7ge\u00b7brach\u00b7ten", "Fluth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der Ton ist nicht vermischt mit dem Bewegen:", "tokens": ["Der", "Ton", "ist", "nicht", "ver\u00b7mischt", "mit", "dem", "Be\u00b7we\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKNEG", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Er wird in uns gezeugt dadurch allein,", "tokens": ["Er", "wird", "in", "uns", "ge\u00b7zeugt", "da\u00b7durch", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "VVPP", "PAV", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Da\u00df sich der Ohren Nerven regen.", "tokens": ["Da\u00df", "sich", "der", "Oh\u00b7ren", "Ner\u00b7ven", "re\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "NN", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}