{"dta.poem.10692": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "An die Asterien.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1624", "urn": "urn:nbn:de:kobv:b4-200905197859", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Teutsche P\u00f6emata und: Aristarchvs Wieder die verachtung Teutscher Sprach. Stra\u00dfburg, 1624."}, "poem": {"stanza.1": {"line.1": {"text": "Zweymal ist jetzund gleich der sch\u00f6ne Fr\u00fcling kommen/", "tokens": ["Zwey\u00b7mal", "ist", "je\u00b7tzund", "gleich", "der", "sch\u00f6\u00b7ne", "Fr\u00fc\u00b7ling", "kom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ADV", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vnd zweymal hat der Frost de\u00df Winters abgenommen", "tokens": ["Vnd", "zwey\u00b7mal", "hat", "der", "Frost", "de\u00df", "Win\u00b7ters", "ab\u00b7ge\u00b7nom\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "ART", "NN", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der B\u00e4wme gr\u00fcnes Kleyd/ als Venus zu mir kam/", "tokens": ["Der", "B\u00e4w\u00b7me", "gr\u00fc\u00b7nes", "Kleyd", "/", "als", "Ve\u00b7nus", "zu", "mir", "kam", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$(", "KOUS", "NN", "APPR", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Vnd mich/ Asterie/ von Ph", "tokens": ["Vnd", "mich", "/", "As\u00b7te\u00b7rie", "/", "von", "Ph"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["KON", "PPER", "$(", "NN", "$(", "APPR", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Vnd dirzugab: vorhin entbrandten meine Sinnen", "tokens": ["Vnd", "dir\u00b7zu\u00b7gab", ":", "vor\u00b7hin", "ent\u00b7brand\u00b7ten", "mei\u00b7ne", "Sin\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$.", "ADV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Vor Durst der Ewigkeyt/ als Ich den Neun G\u00f6ttinnen", "tokens": ["Vor", "Durst", "der", "E\u00b7wig\u00b7keyt", "/", "als", "Ich", "den", "Neun", "G\u00f6t\u00b7tin\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "ART", "NE", "$(", "KOUS", "PPER", "ART", "CARD", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "In jhren Armen lag/ jetzt bin ich/ meine Zier/", "tokens": ["In", "jhren", "Ar\u00b7men", "lag", "/", "jetzt", "bin", "ich", "/", "mei\u00b7ne", "Zier", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$(", "ADV", "VAFIN", "PPER", "$(", "PPOSAT", "NN", "$("], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.8": {"text": "So weit von jhnen ab/ so nah ich bin bey dir/", "tokens": ["So", "weit", "von", "jh\u00b7nen", "ab", "/", "so", "nah", "ich", "bin", "bey", "dir", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "PPER", "PTKVZ", "$(", "ADV", "ADJD", "PPER", "VAFIN", "APPR", "PPER", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wie offt hab ich bi\u00dfher gehoffet frey zu werden/", "tokens": ["Wie", "offt", "hab", "ich", "bi\u00df\u00b7her", "ge\u00b7hof\u00b7fet", "frey", "zu", "wer\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VAFIN", "PPER", "ADV", "VVFIN", "ADJD", "PTKZU", "VAINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Wie offtmals hetten mich gef\u00fchret von der Erden", "tokens": ["Wie", "offt\u00b7mals", "het\u00b7ten", "mich", "ge\u00b7f\u00fch\u00b7ret", "von", "der", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "VAFIN", "PPER", "VVPP", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Die Fl\u00fcgel der Vernunfft/ wann nicht das weite Meer", "tokens": ["Die", "Fl\u00fc\u00b7gel", "der", "Ver\u00b7nunfft", "/", "wann", "nicht", "das", "wei\u00b7te", "Meer"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "$(", "PWAV", "PTKNEG", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Der grossen Freundligkeit in dir gewesen wer?", "tokens": ["Der", "gros\u00b7sen", "Freund\u00b7lig\u00b7keit", "in", "dir", "ge\u00b7we\u00b7sen", "wer", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPER", "VAPP", "PWS", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Jedoch wird dich vnd mich Thalia nicht verschweigen/", "tokens": ["Je\u00b7doch", "wird", "dich", "vnd", "mich", "Tha\u00b7lia", "nicht", "ver\u00b7schwei\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "KON", "PPER", "NE", "PTKNEG", "VVINF", "$("], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.14": {"text": "Mein Augentrost/ ich geh\u2019 ich geh\u2019 jetz zu ersteigen", "tokens": ["Mein", "Au\u00b7gen\u00b7trost", "/", "ich", "geh'", "ich", "geh'", "jetz", "zu", "er\u00b7stei\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$(", "PPER", "VVFIN", "PPER", "VVFIN", "ADV", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Der Ehren hohes Schloss: ob gleich der schn\u00f6de Neidt", "tokens": ["Der", "Eh\u00b7ren", "ho\u00b7hes", "Schloss", ":", "ob", "gleich", "der", "schn\u00f6\u00b7de", "Neidt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "$.", "KOUS", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Verwachen wird den Weg/ den Weg der Ewigkeit.", "tokens": ["Ver\u00b7wa\u00b7chen", "wird", "den", "Weg", "/", "den", "Weg", "der", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "$(", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Der schnellen Jahren flucht/ so alles sonst kan t\u00f6dten/", "tokens": ["Der", "schnel\u00b7len", "Jah\u00b7ren", "flucht", "/", "so", "al\u00b7les", "sonst", "kan", "t\u00f6d\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$(", "ADV", "PIS", "ADV", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Hat nicht Gewalt in vns. Die trefflichen Poeten", "tokens": ["Hat", "nicht", "Ge\u00b7walt", "in", "vns", ".", "Die", "treff\u00b7li\u00b7chen", "Po\u00b7et\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PTKNEG", "NN", "APPR", "PPER", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Sein viel mehr/ als man meint: jhr hoher Sinn vnd Geist", "tokens": ["Sein", "viel", "mehr", "/", "als", "man", "meint", ":", "jhr", "ho\u00b7her", "Sinn", "vnd", "Geist"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADV", "ADV", "$(", "KOUS", "PIS", "VVFIN", "$.", "PPOSAT", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Ist von de\u00df Himmels Sitz in sie herab gereist.", "tokens": ["Ist", "von", "de\u00df", "Him\u00b7mels", "Sitz", "in", "sie", "her\u00b7ab", "ge\u00b7reist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "NN", "APPR", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Ein frey Gewissen auch ist gar nicht angebunden", "tokens": ["Ein", "frey", "Ge\u00b7wis\u00b7sen", "auch", "ist", "gar", "nicht", "an\u00b7ge\u00b7bun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "NN", "ADV", "VAFIN", "ADV", "PTKNEG", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "An den Rumor de\u00df Volcks/ so ehnlich ist den Hunden/", "tokens": ["An", "den", "Ru\u00b7mor", "de\u00df", "Volcks", "/", "so", "ehn\u00b7lich", "ist", "den", "Hun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$(", "ADV", "ADJD", "VAFIN", "ART", "NN", "$("], "meter": "-++--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.23": {"text": "Sie bellen in die Lufft/ wo sie nicht k\u00f6nnen gehn/", "tokens": ["Sie", "bel\u00b7len", "in", "die", "Lufft", "/", "wo", "sie", "nicht", "k\u00f6n\u00b7nen", "gehn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$(", "PWAV", "PPER", "PTKNEG", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Vnd bleiben doch alhier weit von dem Himmel stehn.", "tokens": ["Vnd", "blei\u00b7ben", "doch", "al\u00b7hier", "weit", "von", "dem", "Him\u00b7mel", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "ADJD", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.25": {"text": "So bald vns Atropos den Faden abgeschnitten/", "tokens": ["So", "bald", "vns", "A\u00b7tro\u00b7pos", "den", "Fa\u00b7den", "ab\u00b7ge\u00b7schnit\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "NE", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "So balde haben wir auch vnser Recht erlitten:", "tokens": ["So", "bal\u00b7de", "ha\u00b7ben", "wir", "auch", "vn\u00b7ser", "Recht", "er\u00b7lit\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Wann vnser Seel vnd Geist de\u00df Leibes sind befreit/", "tokens": ["Wann", "vn\u00b7ser", "Seel", "vnd", "Geist", "de\u00df", "Lei\u00b7bes", "sind", "be\u00b7freit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "KON", "NN", "ART", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Vnd lassen diese Welt/ so la\u00dft vns auch der Neidt/", "tokens": ["Vnd", "las\u00b7sen", "die\u00b7se", "Welt", "/", "so", "la\u00dft", "vns", "auch", "der", "Neidt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PDAT", "NN", "$(", "ADV", "VVIMP", "PPER", "ADV", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "So ward auch Hercules/ der Kern der Helden/ jnne/", "tokens": ["So", "ward", "auch", "Her\u00b7cu\u00b7les", "/", "der", "Kern", "der", "Hel\u00b7den", "/", "jn\u00b7ne", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "NN", "$(", "ART", "NN", "ART", "NN", "$(", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Da\u00df niemand vor dem Todt/ die Mi\u00dfgunst z\u00e4umen k\u00f6nne/", "tokens": ["Da\u00df", "nie\u00b7mand", "vor", "dem", "Todt", "/", "die", "Mi\u00df\u00b7gunst", "z\u00e4u\u00b7men", "k\u00f6n\u00b7ne", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "ART", "NN", "$(", "ART", "NN", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Di\u00df ist der alte Lauff. Ich/ den du hier sichst stehn/", "tokens": ["Di\u00df", "ist", "der", "al\u00b7te", "Lauff", ".", "Ich", "/", "den", "du", "hier", "sichst", "stehn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$.", "PPER", "$(", "ART", "PPER", "ADV", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Vnd auch dein Lob mit mir/ soll nimmer vndergehn/", "tokens": ["Vnd", "auch", "dein", "Lob", "mit", "mir", "/", "soll", "nim\u00b7mer", "vn\u00b7der\u00b7gehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPOSAT", "NN", "APPR", "PPER", "$(", "VMFIN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Es sey das mir hinfort f\u00fcr andern wird belieben", "tokens": ["Es", "sey", "das", "mir", "hin\u00b7fort", "f\u00fcr", "an\u00b7dern", "wird", "be\u00b7lie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "PPER", "ADV", "APPR", "PIS", "VAFIN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Was Aristoteles/ was Xenophon geschrieben/", "tokens": ["Was", "A\u00b7ris\u00b7to\u00b7te\u00b7les", "/", "was", "Xe\u00b7no\u00b7phon", "ge\u00b7schrie\u00b7ben", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "NE", "$(", "PWS", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Was Plato reich von List/ was Seneca gesagt/", "tokens": ["Was", "Pla\u00b7to", "reich", "von", "List", "/", "was", "Se\u00b7ne\u00b7ca", "ge\u00b7sagt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "NE", "ADJD", "APPR", "NE", "$(", "PWS", "NE", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Was Cato; oder auch/ es sey das mir behagt/", "tokens": ["Was", "Ca\u00b7to", ";", "o\u00b7der", "auch", "/", "es", "sey", "das", "mir", "be\u00b7hagt", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NE", "$.", "KON", "ADV", "$(", "PPER", "VAFIN", "ART", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Ohn einigen Termin die B\u00fccher aller Alten/", "tokens": ["Ohn", "ei\u00b7ni\u00b7gen", "Ter\u00b7min", "die", "B\u00fc\u00b7cher", "al\u00b7ler", "Al\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ART", "NN", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "So durch de\u00df Himmels gunst bi\u00dfher sind vorbehalten/", "tokens": ["So", "durch", "de\u00df", "Him\u00b7mels", "gunst", "bi\u00df\u00b7her", "sind", "vor\u00b7be\u00b7hal\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ADV", "ADV", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Zu schliessen in mein Hertz; gleich wie ein mutig Pferdt/", "tokens": ["Zu", "schlies\u00b7sen", "in", "mein", "Hertz", ";", "gleich", "wie", "ein", "mu\u00b7tig", "Pferdt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "APPR", "PPOSAT", "NN", "$.", "ADV", "KOKOM", "ART", "ADJD", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Das sich an keinen Zaum/ an keine Schrancken kehrt.", "tokens": ["Das", "sich", "an", "kei\u00b7nen", "Zaum", "/", "an", "kei\u00b7ne", "Schran\u00b7cken", "kehrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRF", "APPR", "PIAT", "NN", "$(", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Vnd kan nit stille stehn/ begierig fort zulauffen/", "tokens": ["Vnd", "kan", 