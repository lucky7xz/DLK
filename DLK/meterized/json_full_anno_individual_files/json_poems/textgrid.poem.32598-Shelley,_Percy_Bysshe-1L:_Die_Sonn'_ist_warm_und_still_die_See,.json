{"textgrid.poem.32598": {"metadata": {"author": {"name": "Shelley, Percy Bysshe", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Sonn' ist warm und still die See,", "genre": "verse", "period": "N.A.", "pub_year": 1818, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Sonn' ist warm und still die See,", "tokens": ["Die", "Sonn'", "ist", "warm", "und", "still", "die", "See", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit L\u00e4cheln blickt der Himmel drein,", "tokens": ["Mit", "L\u00e4\u00b7cheln", "blickt", "der", "Him\u00b7mel", "drein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Inseln Blau, der Berge Schnee", "tokens": ["Der", "In\u00b7seln", "Blau", ",", "der", "Ber\u00b7ge", "Schnee"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Umkr\u00e4nzt der goldne Abendschein.", "tokens": ["Um\u00b7kr\u00e4nzt", "der", "gold\u00b7ne", "A\u00b7bend\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Hauch des Aethers, klar und rein,", "tokens": ["Der", "Hauch", "des", "A\u00b7e\u00b7thers", ",", "klar", "und", "rein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Umspielt sein tr\u00e4umend Rosenkind;", "tokens": ["Um\u00b7spielt", "sein", "tr\u00e4u\u00b7mend", "Ro\u00b7sen\u00b7kind", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "In wunderbaren Melodein", "tokens": ["In", "wun\u00b7der\u00b7ba\u00b7ren", "Me\u00b7lo\u00b7dein"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Erklingen Vogel, Meer und Wind \u2013", "tokens": ["Er\u00b7klin\u00b7gen", "Vo\u00b7gel", ",", "Meer", "und", "Wind", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Der L\u00e4rm der Stadt sogar ist hier ged\u00e4mpft und lind.", "tokens": ["Der", "L\u00e4rm", "der", "Stadt", "so\u00b7gar", "ist", "hier", "ge\u00b7d\u00e4mpft", "und", "lind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADV", "VAFIN", "ADV", "VVPP", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "In nie betretner Tiefe schau'", "tokens": ["In", "nie", "be\u00b7tret\u00b7ner", "Tie\u00b7fe", "schau'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich Moos und Flechten ausgespannt;", "tokens": ["Ich", "Moos", "und", "Flech\u00b7ten", "aus\u00b7ge\u00b7spannt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie Sternenfluth der Wellen Blau", "tokens": ["Wie", "Ster\u00b7nen\u00b7fluth", "der", "Wel\u00b7len", "Blau"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hinpl\u00e4tschert leis zum Uferrand.", "tokens": ["Hin\u00b7pl\u00e4t\u00b7schert", "leis", "zum", "Uf\u00b7er\u00b7rand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich sitz' allein am Meeressand;", "tokens": ["Ich", "sitz'", "al\u00b7lein", "am", "Mee\u00b7res\u00b7sand", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Fluth entblitzt wie leuchtend Erz", "tokens": ["Der", "Fluth", "ent\u00b7blitzt", "wie", "leuch\u00b7tend", "Erz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVPP", "KOKOM", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ein Funkeln, und im Abendbrand", "tokens": ["Ein", "Fun\u00b7keln", ",", "und", "im", "A\u00b7bend\u00b7brand"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "KON", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Entsteigt ein Klingen uferw\u00e4rts \u2013", "tokens": ["Ent\u00b7steigt", "ein", "Klin\u00b7gen", "u\u00b7fer\u00b7w\u00e4rts", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Wie s\u00fc\u00df, erbebte nur wie meins ein einzig Herz!", "tokens": ["Wie", "s\u00fc\u00df", ",", "er\u00b7beb\u00b7te", "nur", "wie", "meins", "ein", "ein\u00b7zig", "Herz", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "$,", "VVFIN", "ADV", "KOKOM", "PIS", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Weh mir! ich hab' nicht Gl\u00fcck noch Ruh',", "tokens": ["Weh", "mir", "!", "ich", "hab'", "nicht", "Gl\u00fcck", "noch", "Ruh'", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$.", "PPER", "VAFIN", "PTKNEG", "NN", "ADV", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Noch Frieden in des Herzens Nacht,", "tokens": ["Noch", "Frie\u00b7den", "in", "des", "Her\u00b7zens", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Noch fiel mir jener Reichthum zu,", "tokens": ["Noch", "fiel", "mir", "je\u00b7ner", "Reicht\u00b7hum", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PDAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Weisheit bringen und Bedacht,", "tokens": ["Den", "Weis\u00b7heit", "brin\u00b7gen", "und", "Be\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Gekr\u00f6nt mit innrer Glorie Pracht.", "tokens": ["Ge\u00b7kr\u00f6nt", "mit", "inn\u00b7rer", "Glo\u00b7rie", "Pracht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nicht Ruhm noch Macht, nicht Lieb' und Heil \u2013", "tokens": ["Nicht", "Ruhm", "noch", "Macht", ",", "nicht", "Lieb'", "und", "Heil", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "ADV", "NN", "$,", "PTKNEG", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ach, Andern hat das All' gelacht;", "tokens": ["Ach", ",", "An\u00b7dern", "hat", "das", "All'", "ge\u00b7lacht", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADJA", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Sie sprachen jedem Tag: \u00bbVerweil!\u00ab \u2013", "tokens": ["Sie", "spra\u00b7chen", "je\u00b7dem", "Tag", ":", "\u00bb", "Ver\u00b7weil", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "punct", "punct", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$.", "$(", "NN", "$.", "$(", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Mir ward des Lebens Kelch in anderm Maa\u00df zu Theil.", "tokens": ["Mir", "ward", "des", "Le\u00b7bens", "Kelch", "in", "an\u00b7derm", "Maa\u00df", "zu", "Theil", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "NN", "APPR", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Doch hier ist selbst Verzweiflung lind,", "tokens": ["Doch", "hier", "ist", "selbst", "Ver\u00b7zwei\u00b7flung", "lind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "ADV", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie Abendrauschen, Meer und Flu\u00df;", "tokens": ["Wie", "A\u00b7ben\u00b7drau\u00b7schen", ",", "Meer", "und", "Flu\u00df", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Fortweinen wie ein m\u00fcdes Kind", "tokens": ["Fort\u00b7wei\u00b7nen", "wie", "ein", "m\u00fc\u00b7des", "Kind"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KOKOM", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "M\u00f6cht' ich dies Leben voll Verdru\u00df,", "tokens": ["M\u00f6cht'", "ich", "dies", "Le\u00b7ben", "voll", "Ver\u00b7dru\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PDS", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das ich ertrug und tragen mu\u00df,", "tokens": ["Das", "ich", "er\u00b7trug", "und", "tra\u00b7gen", "mu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "VVFIN", "KON", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Bis mir der Tod den Schlummer bringt,", "tokens": ["Bis", "mir", "der", "Tod", "den", "Schlum\u00b7mer", "bringt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und in der L\u00fcfte warmem Gu\u00df", "tokens": ["Und", "in", "der", "L\u00fcf\u00b7te", "war\u00b7mem", "Gu\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mein Geist ins weite All verklingt,", "tokens": ["Mein", "Geist", "ins", "wei\u00b7te", "All", "ver\u00b7klingt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und meinem Ohr das Meer sein letztes Murmeln singt.", "tokens": ["Und", "mei\u00b7nem", "Ohr", "das", "Meer", "sein", "letz\u00b7tes", "Mur\u00b7meln", "singt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ART", "NN", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Wohl h\u00f6r' ich z\u00fcrnen, ich sei kalt,", "tokens": ["Wohl", "h\u00f6r'", "ich", "z\u00fcr\u00b7nen", ",", "ich", "sei", "kalt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVINF", "$,", "PPER", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df ich gest\u00f6rt in dunklem Sinn", "tokens": ["Da\u00df", "ich", "ge\u00b7st\u00f6rt", "in", "dunk\u00b7lem", "Sinn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVPP", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit einem Herzen, tr\u00fcb und alt,", "tokens": ["Mit", "ei\u00b7nem", "Her\u00b7zen", ",", "tr\u00fcb", "und", "alt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auch dieser Stunde Hochgewinn.", "tokens": ["Auch", "die\u00b7ser", "Stun\u00b7de", "Hoch\u00b7ge\u00b7winn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PDAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Z\u00fcrnt immer! denn von Menschen bin", "tokens": ["Z\u00fcrnt", "im\u00b7mer", "!", "denn", "von", "Men\u00b7schen", "bin"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "$.", "ADV", "APPR", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ich nicht geliebt und doch beklagt,", "tokens": ["Ich", "nicht", "ge\u00b7liebt", "und", "doch", "be\u00b7klagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "VVPP", "KON", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ungleich dem Tag, der, wenn dahin", "tokens": ["Un\u00b7gleich", "dem", "Tag", ",", "der", ",", "wenn", "da\u00b7hin"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ADJD", "ART", "NN", "$,", "PRELS", "$,", "KOUS", "PAV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Sein Glanz, der pr\u00e4chtig uns getagt,", "tokens": ["Sein", "Glanz", ",", "der", "pr\u00e4ch\u00b7tig", "uns", "ge\u00b7tagt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "ADJD", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Voll Licht und Freude ganz noch im Ged\u00e4chtni\u00df ragt.", "tokens": ["Voll", "Licht", "und", "Freu\u00b7de", "ganz", "noch", "im", "Ge\u00b7d\u00e4cht\u00b7ni\u00df", "ragt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "KON", "NN", "ADV", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Die Sonn' ist warm und still die See,", "tokens": ["Die", "Sonn'", "ist", "warm", "und", "still", "die", "See", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit L\u00e4che