{"textgrid.poem.57284": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "1L: Bekr\u00e4nzt mein Haar, o Blumen des Hains,", "genre": "verse", "period": "N.A.", "pub_year": 1766, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bekr\u00e4nzt mein Haar, o Blumen des Hains,", "tokens": ["Be\u00b7kr\u00e4nzt", "mein", "Haar", ",", "o", "Blu\u00b7men", "des", "Hains", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "FM", "NN", "ART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Die am Schattenbach des bardischen Quells", "tokens": ["Die", "am", "Schat\u00b7ten\u00b7bach", "des", "bar\u00b7di\u00b7schen", "Quells"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPRART", "NN", "ART", "ADJA", "NN"], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Nossa's Hand sorgsam erzog, Braga mir", "tokens": ["Nos\u00b7sa's", "Hand", "sorg\u00b7sam", "er\u00b7zog", ",", "Bra\u00b7ga", "mir"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "NN", "ADJD", "VVFIN", "$,", "NE", "PPER"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "Brachte, bekr\u00e4nzt, Blumen, mein Haar!", "tokens": ["Brach\u00b7te", ",", "be\u00b7kr\u00e4nzt", ",", "Blu\u00b7men", ",", "mein", "Haar", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVPP", "$,", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "+---+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Es wendet nach dem Strome des Quells", "tokens": ["Es", "wen\u00b7det", "nach", "dem", "Stro\u00b7me", "des", "Quells"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Sich der Lautenklang des wehenden Bachs.", "tokens": ["Sich", "der", "Lau\u00b7ten\u00b7klang", "des", "we\u00b7hen\u00b7den", "Bachs", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Tief, und still str\u00f6met der Strom; tonbeseelt", "tokens": ["Tief", ",", "und", "still", "str\u00f6\u00b7met", "der", "Strom", ";", "ton\u00b7be\u00b7seelt"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADJD", "$,", "KON", "ADJD", "VVFIN", "ART", "NN", "$.", "VVFIN"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "Rauschet der Bach neben ihm fort.", "tokens": ["Rau\u00b7schet", "der", "Bach", "ne\u00b7ben", "ihm", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.3": {"line.1": {"text": "Inhalt, den volle Seel', im Erguss", "tokens": ["In\u00b7halt", ",", "den", "vol\u00b7le", "Seel'", ",", "im", "Er\u00b7guss"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "$,", "ART", "ADJA", "NN", "$,", "APPRART", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Der Erfindung, und der innersten Kraft,", "tokens": ["Der", "Er\u00b7fin\u00b7dung", ",", "und", "der", "in\u00b7ners\u00b7ten", "Kraft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KON", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Sich entwirft, str\u00f6met; allein lebend muss,", "tokens": ["Sich", "ent\u00b7wirft", ",", "str\u00f6\u00b7met", ";", "al\u00b7lein", "le\u00b7bend", "muss", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "$,", "VVFIN", "$.", "ADV", "ADJD", "VMFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Will es ihm nahn, t\u00f6nen das Wort.", "tokens": ["Will", "es", "ihm", "nahn", ",", "t\u00f6\u00b7nen", "das", "Wort", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "ADJA", "$,", "VVFIN", "ART", "NN", "$."], "meter": "+--++--+", "measure": "iambic.tetra.chol"}}, "stanza.4": {"line.1": {"text": "Wohllaut gef\u00e4llt, Bewegung noch mehr;", "tokens": ["Wohl\u00b7laut", "ge\u00b7f\u00e4llt", ",", "Be\u00b7we\u00b7gung", "noch", "mehr", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "$,", "NN", "ADV", "ADV", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Zur Gespielin kohr das Herz sie sich aus.", "tokens": ["Zur", "Ge\u00b7spie\u00b7lin", "kohr", "das", "Herz", "sie", "sich", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "PPER", "PRF", "PTKVZ", "$."], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Diesem s\u00e4umt, eilet sie nach; Bildern folgt,", "tokens": ["Die\u00b7sem", "s\u00e4umt", ",", "ei\u00b7let", "sie", "nach", ";", "Bil\u00b7dern", "folgt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "VVFIN", "PPER", "PTKVZ", "$.", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Leiseres Tritts, ferne sie nur.", "tokens": ["Lei\u00b7se\u00b7res", "Tritts", ",", "fer\u00b7ne", "sie", "nur", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "PPER", "ADV", "$."], "meter": "+--++--+", "measure": "iambic.tetra.chol"}}, "stanza.5": {"line.1": {"text": "So s\u00e4umet, und so eilt sie nicht nur:", "tokens": ["So", "s\u00e4u\u00b7met", ",", "und", "so", "eilt", "sie", "nicht", "nur", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "KON", "ADV", "VVFIN", "PPER", "PTKNEG", "ADV", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Auch empfindungsvolle Wendung beseelt", "tokens": ["Auch", "emp\u00b7fin\u00b7dungs\u00b7vol\u00b7le", "Wen\u00b7dung", "be\u00b7seelt"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Ihr den Tanz, Tragung, die spricht, ihr den Tanz,", "tokens": ["Ihr", "den", "Tanz", ",", "Tra\u00b7gung", ",", "die", "spricht", ",", "ihr", "den", "Tanz", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "$,", "NN", "$,", "PRELS", "VVFIN", "$,", "PPER", "ART", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "All ihr Gelenk schwebt in Verhalt.", "tokens": ["All", "ihr", "Ge\u00b7lenk", "schwebt", "in", "Ver\u00b7halt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.6": {"line.1": {"text": "Mir gab Siona Sulamith schon", "tokens": ["Mir", "gab", "Si\u00b7o\u00b7na", "Su\u00b7la\u00b7mith", "schon"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NE", "NE", "ADV"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "An der Palmenh\u00f6h den r\u00f6thlichen Kranz", "tokens": ["An", "der", "Pal\u00b7men\u00b7h\u00f6h", "den", "r\u00f6th\u00b7li\u00b7chen", "Kranz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN"], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Sarons. Ihr weiht' ich zuerst jenen Reihn,", "tokens": ["Sa\u00b7rons", ".", "Ihr", "weiht'", "ich", "zu\u00b7erst", "je\u00b7nen", "Reihn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PPER", "VVFIN", "PPER", "ADV", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Welcher im Chor hallt des Triumphs,", "tokens": ["Wel\u00b7cher", "im", "Chor", "hallt", "des", "Tri\u00b7umphs", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "APPRART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.7": {"line.1": {"text": "Nun rufet seinen Reihen durch mich", "tokens": ["Nun", "ru\u00b7fet", "sei\u00b7nen", "Rei\u00b7hen", "durch", "mich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "APPR", "PPER"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "In der Eiche Schatten Braga zur\u00fcck.", "tokens": ["In", "der", "Ei\u00b7che", "Schat\u00b7ten", "Bra\u00b7ga", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NE", "PTKVZ", "$."], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "H\u00fcllte nicht daurende Nacht Lieder ein,", "tokens": ["H\u00fcll\u00b7te", "nicht", "dau\u00b7ren\u00b7de", "Nacht", "Lie\u00b7der", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ADJA", "NN", "NN", "PTKVZ", "$,"], "meter": "+--+--++-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Lyrischen Flug, welchem die H\u00f6hn", "tokens": ["Ly\u00b7ri\u00b7schen", "Flug", ",", "wel\u00b7chem", "die", "H\u00f6hn"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PRELS", "ART", "NN"], "meter": "+--+---+", "measure": "dactylic.di.plus"}}, "stanza.8": {"line.1": {"text": "Des Lorberh\u00fcgels horchten; o schlief'", "tokens": ["Des", "Lor\u00b7ber\u00b7h\u00fc\u00b7gels", "horch\u00b7ten", ";", "o", "schlie\u00b7f'"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "FM", "FM"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "In der Tr\u00fcmmer Graun Alz\u00e4us nicht selbst:", "tokens": ["In", "der", "Tr\u00fcm\u00b7mer", "Graun", "A\u00b7lz\u00e4us", "nicht", "selbst", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NE", "NE", "PTKNEG", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "R\u00fchmt' ich mich k\u00fchneres Schwungs, t\u00f6ne, stolz", "tokens": ["R\u00fchmt'", "ich", "mich", "k\u00fch\u00b7ne\u00b7res", "Schwungs", ",", "t\u00f6\u00b7ne", ",", "stolz"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["VVFIN", "PPER", "PRF", "ADJA", "NN", "$,", "ADJA", "$,", "ADJD"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "R\u00fchmt' ichs, uns mehr Wendung f\u00fcrs Herz,", "tokens": ["R\u00fchmt'", "ichs", ",", "uns", "mehr", "Wen\u00b7dung", "f\u00fcrs", "Herz", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$,", "PPER", "PIAT", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.9": {"line.1": {"text": "Als Tempe's Hirt vom Felsen vernahm!", "tokens": ["Als", "Tem\u00b7pe's", "Hirt", "vom", "Fel\u00b7sen", "ver\u00b7nahm", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "NE", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Und der K\u00e4mpfer Schaar in Elis Gefild!", "tokens": ["Und", "der", "K\u00e4mp\u00b7fer", "Schaar", "in", "E\u00b7lis", "Ge\u00b7fild", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "APPR", "NE", "NN", "$."], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Als mit Tanz Sparta zur Schlacht eilend! Zeus", "tokens": ["Als", "mit", "Tanz", "Spar\u00b7ta", "zur", "Schlacht", "ei\u00b7lend", "!", "Zeus"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["KOUS", "APPR", "NN", "NE", "APPRART", "NN", "VVPP", "$.", "NE"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Aus des Altars hohem Gew\u00f6lk!", "tokens": ["Aus", "des", "Al\u00b7tars", "ho\u00b7hem", "Ge\u00b7w\u00f6lk", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}}, "stanza.10": {"line.1": {"text": "Der grosse S\u00e4nger Ossian folgt", "tokens": ["Der", "gros\u00b7se", "S\u00e4n\u00b7ger", "Os\u00b7si\u00b7an", "folgt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NE", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Musik des vollen Baches nicht stets.", "tokens": ["Der", "Mu\u00b7sik", "des", "vol\u00b7len", "Ba\u00b7ches", "nicht", "stets", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "PTKNEG", "ADV", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Taub ihm, z\u00e4hlt Galliens Lied Laute nur!", "tokens": ["Taub", "ihm", ",", "z\u00e4hlt", "Gal\u00b7li\u00b7ens", "Lied", "Lau\u00b7te", "nur", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "VVFIN", "NE", "NN", "VVFIN", "ADV", "$."], "meter": "+-++-+-+-+", "measure": "zehnsilber"}, "line.4": {"text": "Zwischen der Zahl, schwankt und dem Maass,", "tokens": ["Zwi\u00b7schen", "der", "Zahl", ",", "schwankt", "und", "dem", "Maass", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "VVFIN", "KON", "ART", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.11": {"line.1": {"text": "Der Britte; selbst Hesperien schl\u00e4ft!", "tokens": ["Der", "Brit\u00b7te", ";", "selbst", "Hes\u00b7pe\u00b7ri\u00b7en", "schl\u00e4ft", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "ADV", "NE", "VVFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "O sie wecke nie die Sait' und das Horn", "tokens": ["O", "sie", "we\u00b7cke", "nie", "die", "Sait'", "und", "das", "Horn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "PPER", "VVFIN", "ADV", "ART", "NN", "KON", "ART", "NN"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Braga's auf! Fl\u00f6gen sie einst deinen Flug,", "tokens": ["Bra\u00b7ga's", "auf", "!", "Fl\u00f6\u00b7gen", "sie", "einst", "dei\u00b7nen", "Flug", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$.", "NN", "PPER", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Schwan des Glasoor; neidet' ich sie!", "tokens": ["Schwan", "des", "Gla\u00b7so\u00b7or", ";", "nei\u00b7det'", "ich", "sie", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$.", "VVFIN", "PPER", "PPER", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.12": {"line.1": {"text": "Nachahmer, wie Nachahmer nicht sind,", "tokens": ["Nac\u00b7hah\u00b7mer", ",", "wie", "Nac\u00b7hah\u00b7mer", "nicht", "sind", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "NN", "PTKNEG", "VAFIN", "$,"], "meter": "-+-+++-+-", "measure": "unknown.measure.penta"}, "line.2": {"text": "Du erwecktest selbst, o Flakkus, sie nicht!", "tokens": ["Du", "er\u00b7weck\u00b7test", "selbst", ",", "o", "Flak\u00b7kus", ",", "sie", "nicht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "FM", "NN", "$,", "PPER", "PTKNEG", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Graue Zeit w\u00e4hret' ihr Schlaf! O, er w\u00e4hrt", "tokens": ["Grau\u00b7e", "Zeit", "w\u00e4h\u00b7ret'", "ihr", "Schlaf", "!", "O", ",", "er", "w\u00e4hrt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$.", "NE", "$,", "PPER", "VVFIN"], "meter": "+-+-+-++-+", "measure": "unknown.measure.hexa"}, "line.4": {"text": "Immer, und ich neide sie nie!", "tokens": ["Im\u00b7mer", ",", "und", "ich", "nei\u00b7de", "sie", "nie", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KON", "PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.13": {"line.1": {"text": "Schon lange mass der Dichter des Rheins", "tokens": ["Schon", "lan\u00b7ge", "mass", "der", "Dich\u00b7ter", "des", "Rheins"], "token_info": ["word", "word", "word", "word"