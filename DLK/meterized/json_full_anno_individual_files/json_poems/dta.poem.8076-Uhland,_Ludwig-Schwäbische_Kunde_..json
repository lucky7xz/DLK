{"dta.poem.8076": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Schw\u00e4bische Kunde .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Als Kaiser Rothbart lobesam", "tokens": ["Als", "Kai\u00b7ser", "Roth\u00b7bart", "lo\u00b7be\u00b7sam"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "NN", "NE", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zum heil\u2019gen Land gezogen kam,", "tokens": ["Zum", "heil'\u00b7gen", "Land", "ge\u00b7zo\u00b7gen", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da mu\u00dft\u2019 er mit dem frommen Heer", "tokens": ["Da", "mu\u00dft'", "er", "mit", "dem", "from\u00b7men", "Heer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Durch ein Gebirge, w\u00fcst und leer.", "tokens": ["Durch", "ein", "Ge\u00b7bir\u00b7ge", ",", "w\u00fcst", "und", "leer", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "VVFIN", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Daselbst erhub sich gro\u00dfe Noth,", "tokens": ["Da\u00b7selbst", "er\u00b7hub", "sich", "gro\u00b7\u00dfe", "Noth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Viel Steine gab\u2019s und wenig Brot,", "tokens": ["Viel", "Stei\u00b7ne", "gab's", "und", "we\u00b7nig", "Brot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "KON", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und mancher deutsche Reitersmann", "tokens": ["Und", "man\u00b7cher", "deut\u00b7sche", "Rei\u00b7ters\u00b7mann"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Hat dort den Trunk sich abgethan.", "tokens": ["Hat", "dort", "den", "Trunk", "sich", "ab\u00b7ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "PRF", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Den Pferden war\u2019s so schwach im Magen,", "tokens": ["Den", "Pfer\u00b7den", "wa\u00b7r's", "so", "schwach", "im", "Ma\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "APPRART", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Fast mu\u00dft\u2019 der Reiter die M\u00e4hre tragen.", "tokens": ["Fast", "mu\u00dft'", "der", "Rei\u00b7ter", "die", "M\u00e4h\u00b7re", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Nun war ein Herr aus Schwabenland,", "tokens": ["Nun", "war", "ein", "Herr", "aus", "Schwa\u00b7ben\u00b7land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Von hohem Wuchs und starker Hand,", "tokens": ["Von", "ho\u00b7hem", "Wuchs", "und", "star\u00b7ker", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "De\u00df R\u00f6\u00dflein war so krank und schwach,", "tokens": ["De\u00df", "R\u00f6\u00df\u00b7lein", "war", "so", "krank", "und", "schwach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Er zog es nur am Zaume nach,", "tokens": ["Er", "zog", "es", "nur", "am", "Zau\u00b7me", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Er h\u00e4tt\u2019 es nimmer aufgegeben", "tokens": ["Er", "h\u00e4tt'", "es", "nim\u00b7mer", "auf\u00b7ge\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Und kostet\u2019s ihn das eigne Leben.", "tokens": ["Und", "kos\u00b7tet's", "ihn", "das", "eig\u00b7ne", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "So blieb er bald ein gutes St\u00fcck", "tokens": ["So", "blieb", "er", "bald", "ein", "gu\u00b7tes", "St\u00fcck"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Hinter dem Heereszug zur\u00fcck,", "tokens": ["Hin\u00b7ter", "dem", "Hee\u00b7res\u00b7zug", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.19": {"text": "Da sprengten pl\u00f6tzlich in die Queer", "tokens": ["Da", "spreng\u00b7ten", "pl\u00f6tz\u00b7lich", "in", "die", "Que\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "F\u00fcnfzig t\u00fcrkische Reiter daher,", "tokens": ["F\u00fcnf\u00b7zig", "t\u00fcr\u00b7ki\u00b7sche", "Rei\u00b7ter", "da\u00b7her", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "PAV", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.21": {"text": "Die huben an, auf ihn zu schie\u00dfen,", "tokens": ["Die", "hu\u00b7ben", "an", ",", "auf", "ihn", "zu", "schie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PTKVZ", "$,", "APPR", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Nach ihm zu werfen mit den Spie\u00dfen.", "tokens": ["Nach", "ihm", "zu", "wer\u00b7fen", "mit", "den", "Spie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PTKZU", "VVINF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Der wackre Schwabe forcht\u2019 sich nit,", "tokens": ["Der", "wack\u00b7re", "Schwa\u00b7be", "forcht'", "sich", "nit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PRF", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Ging seines Weges Schritt vor Schritt,", "tokens": ["Ging", "sei\u00b7nes", "We\u00b7ges", "Schritt", "vor", "Schritt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Lie\u00df sich den Schild mit Pfeilen spicken", "tokens": ["Lie\u00df", "sich", "den", "Schild", "mit", "Pfei\u00b7len", "spi\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "ART", "NN", "APPR", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Und th\u00e4t nur sp\u00f6ttlich um sich blicken,", "tokens": ["Und", "th\u00e4t", "nur", "sp\u00f6tt\u00b7lich", "um", "sich", "bli\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADJD", "APPR", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Bis Einer, dem die Zeit zu lang,", "tokens": ["Bis", "Ei\u00b7ner", ",", "dem", "die", "Zeit", "zu", "lang", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "$,", "PRELS", "ART", "NN", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Auf ihn den krummen S\u00e4bel schwang.", "tokens": ["Auf", "ihn", "den", "krum\u00b7men", "S\u00e4\u00b7bel", "schwang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Da wallt dem Deutschen auch sein Blut,", "tokens": ["Da", "wallt", "dem", "Deut\u00b7schen", "auch", "sein", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Er trifft des T\u00fcrken Pferd so gut,", "tokens": ["Er", "trifft", "des", "T\u00fcr\u00b7ken", "Pferd", "so", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Er haut ihm ab mit Einem Streich", "tokens": ["Er", "haut", "ihm", "ab", "mit", "Ei\u00b7nem", "Streich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Die beiden Vorderf\u00fc\u00df\u2019 zugleich.", "tokens": ["Die", "bei\u00b7den", "Vor\u00b7der\u00b7f\u00fc\u00df'", "zu\u00b7gleich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Als er das Thier zu Fall gebracht,", "tokens": ["Als", "er", "das", "Thier", "zu", "Fall", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Da fa\u00dft er erst sein Schwerdt mit Macht,", "tokens": ["Da", "fa\u00dft", "er", "erst", "sein", "Schwerdt", "mit", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Er schwingt es auf des Reiters Kopf,", "tokens": ["Er", "schwingt", "es", "auf", "des", "Rei\u00b7ters", "Kopf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "Haut durch bis auf den Sattelknopf,", "tokens": ["Haut", "durch", "bis", "auf", "den", "Sat\u00b7tel\u00b7knopf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.37": {"text": "Haut auch den Sattel noch zu St\u00fccken", "tokens": ["Haut", "auch", "den", "Sat\u00b7tel", "noch", "zu", "St\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "ART", "NN", "ADV", "APPR", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.38": {"text": "Und tief noch in des Pferdes R\u00fccken;", "tokens": ["Und", "tief", "noch", "in", "des", "Pfer\u00b7des", "R\u00fc\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.39": {"text": "Zur Rechten sieht man, wie zur Linken,", "tokens": ["Zur", "Rech\u00b7ten", "sieht", "man", ",", "wie", "zur", "Lin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PIS", "$,", "PWAV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.40": {"text": "Einen halben T\u00fcrken heruntersinken.", "tokens": ["Ei\u00b7nen", "hal\u00b7ben", "T\u00fcr\u00b7ken", "her\u00b7un\u00b7ter\u00b7sin\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.41": {"text": "Da packt die Andern kalter Graus,", "tokens": ["Da", "packt", "die", "An\u00b7dern", "kal\u00b7ter", "Graus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.42": {"text": "Sie fliehen in alle Welt hinaus,", "tokens": ["Sie", "flie\u00b7hen", "in", "al\u00b7le", "Welt", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN", "APZR", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.43": {"text": "Und Jedem ist\u2019s, als w\u00fcrd\u2019 ihm mitten", "tokens": ["Und", "Je\u00b7dem", "ist's", ",", "als", "w\u00fcrd'", "ihm", "mit\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PIS", "VAFIN", "$,", "KOKOM", "VAFIN", "PPER", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.44": {"text": "Durch Kopf und Leib hindurchgeschnitten.", "tokens": ["Durch", "Kopf", "und", "Leib", "hin\u00b7durch\u00b7ge\u00b7schnit\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.45": {"text": "Drauf kam des Wegs \u2019ne Christenschaar,", "tokens": ["Drauf", "kam", "des", "Wegs", "'ne", "Chris\u00b7ten\u00b7schaar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.46": {"text": "Die auch zur\u00fcck geblieben war,", "tokens": ["Die", "auch", "zu\u00b7r\u00fcck", "ge\u00b7blie\u00b7ben", "war", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.47": {"text": "Die sahen nun mit gutem Bedacht", "tokens": ["Die", "sa\u00b7hen", "nun", "mit", "gu\u00b7tem", "Be\u00b7dacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.48": {"text": "Was Arbeit unser Held gemacht.", "tokens": ["Was", "Ar\u00b7beit", "un\u00b7ser", "Held", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.49": {"text": "Von denen hat\u2019s der Kaiser vernommen,", "tokens": ["Von", "de\u00b7nen", "hat's", "der", "Kai\u00b7ser", "ver\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDS", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.50": {"text": "Der lie\u00df den Schwaben vor sich kommen,", "tokens": ["Der", "lie\u00df", "den", "Schwa\u00b7ben", "vor", "sich", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "APPR", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.51": {"text": "Er sprach: \u201eSagt an, mein Ritter werth!", "tokens": ["Er", "sprach", ":", "\u201e", "Sagt", "an", ",", "mein", "Rit\u00b7ter", "werth", "!"], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "$(", "VVFIN", "PTKVZ", "$,", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.52": {"text": "Wer hat Euch solche Streich\u2019 gelehrt?\u201c", "tokens": ["Wer", "hat", "Euch", "sol\u00b7che", "Streich'", "ge\u00b7lehrt", "?", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "VAFIN", "PPER", "PIAT", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.53": {"text": "Der Held bedacht\u2019 sich nicht zu lang:", "tokens": ["Der", "Held", "be\u00b7dacht'", "sich", "nicht", "zu", "lang", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN"