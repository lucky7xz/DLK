{"textgrid.poem.33748": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "28.", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von allen Masken hatt' ich mir die letzte,", "tokens": ["Von", "al\u00b7len", "Mas\u00b7ken", "hatt'", "ich", "mir", "die", "letz\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VAFIN", "PPER", "PPER", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das letzte mir erw\u00e4hlt von allen Losen,", "tokens": ["Das", "letz\u00b7te", "mir", "er\u00b7w\u00e4hlt", "von", "al\u00b7len", "Lo\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "PPER", "VVPP", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das schwerste: fern zu sein fortan vom Tosen", "tokens": ["Das", "schwers\u00b7te", ":", "fern", "zu", "sein", "for\u00b7tan", "vom", "To\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "$.", "ADJD", "PTKZU", "VAINF", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Des Weltl\u00e4rms, der mich doch so oft verletzte.", "tokens": ["Des", "Welt\u00b7l\u00e4rms", ",", "der", "mich", "doch", "so", "oft", "ver\u00b7letz\u00b7te", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADV", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Dem Becher, dessen Feuerquell mich letzte,", "tokens": ["Dem", "Be\u00b7cher", ",", "des\u00b7sen", "Feu\u00b7er\u00