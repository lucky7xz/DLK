{"dta.poem.5986": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Bau der Welt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Kan bald, gleich unsrer Erden,", "tokens": ["Kan", "bald", ",", "gleich", "uns\u00b7rer", "Er\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "$,", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein undurchsichtiger, ein dunckler C\u00f6rper werden;", "tokens": ["Ein", "un\u00b7durch\u00b7sich\u00b7ti\u00b7ger", ",", "ein", "dunck\u00b7ler", "C\u00f6r\u00b7per", "wer\u00b7den", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ART", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Bis da\u00df dies Wunder-Licht, in seiner vor\u2019gen Krafft,", "tokens": ["Bis", "da\u00df", "dies", "Wun\u00b7der\u00b7Licht", ",", "in", "sei\u00b7ner", "vor'\u00b7gen", "Krafft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PDS", "NN", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Flecken von sich ab und wieder fortgeschafft.", "tokens": ["Die", "Fle\u00b7cken", "von", "sich", "ab", "und", "wie\u00b7der", "fort\u00b7ge\u00b7schafft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PRF", "PTKVZ", "KON", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}