{"textgrid.poem.48885": {"metadata": {"author": {"name": "Kleist, Ewald Christian von", "birth": "N.A.", "death": "N.A."}, "title": "An Herrn Rittmeister Adler", "genre": "verse", "period": "N.A.", "pub_year": 1737, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die St\u00fcrme w\u00fcten nicht mehr, man sieht die Zacken der Tannen", "tokens": ["Die", "St\u00fcr\u00b7me", "w\u00fc\u00b7ten", "nicht", "mehr", ",", "man", "sieht", "die", "Za\u00b7cken", "der", "Tan\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "ADV", "$,", "PIS", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+--+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Nicht mehr durch gl\u00e4sernen Reif; man sieht im eislosen Bach", "tokens": ["Nicht", "mehr", "durch", "gl\u00e4\u00b7ser\u00b7nen", "Reif", ";", "man", "sieht", "im", "eis\u00b7lo\u00b7sen", "Bach"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "APPR", "ADJA", "NN", "$.", "PIS", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Am Grunde Muscheln und Gras und junge wankende Blumen,", "tokens": ["Am", "Grun\u00b7de", "Mu\u00b7scheln", "und", "Gras", "und", "jun\u00b7ge", "wan\u00b7ken\u00b7de", "Blu\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NN", "KON", "NN", "KON", "ADJA", "ADJA", "NN", "$,"], "meter": "-+----+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Ein dunkles schwebendes Laub erf\u00fcllt den Buchwald mit Nacht.", "tokens": ["Ein", "dunk\u00b7les", "schwe\u00b7ben\u00b7des", "Laub", "er\u00b7f\u00fcllt", "den", "Buch\u00b7wald", "mit", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Hier reizt der Nachtigall Lied durch tausend laufende T\u00f6ne,", "tokens": ["Hier", "reizt", "der", "Nach\u00b7ti\u00b7gall", "Lied", "durch", "tau\u00b7send", "lau\u00b7fen\u00b7de", "T\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "APPR", "CARD", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Der West im Rosengeb\u00fcsch bl\u00e4st s\u00fcsse D\u00fcfte zur Flur,", "tokens": ["Der", "West", "im", "Ro\u00b7sen\u00b7ge\u00b7b\u00fcsch", "bl\u00e4st", "s\u00fcs\u00b7se", "D\u00fcf\u00b7te", "zur", "Flur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Dort stralt im gl\u00e4nzenden Strom das Bildni\u00df bl\u00fchender Hecken,", "tokens": ["Dort", "stralt", "im", "gl\u00e4n\u00b7zen\u00b7den", "Strom", "das", "Bild\u00b7ni\u00df", "bl\u00fc\u00b7hen\u00b7der", "He\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "ADJA", "NN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Und flieht nebst Ufer und Rohr des Fischers gleitenden Kahn.", "tokens": ["Und", "flieht", "nebst", "U\u00b7fer", "und", "Rohr", "des", "Fi\u00b7schers", "glei\u00b7ten\u00b7den", "Kahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "KON", "NN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Freund! flieh der Waffen Ger\u00e4usch, jezt ist die Zeit des Vergn\u00fcgens,", "tokens": ["Freund", "!", "flieh", "der", "Waf\u00b7fen", "Ge\u00b7r\u00e4usch", ",", "jezt", "ist", "die", "Zeit", "des", "Ver\u00b7gn\u00fc\u00b7gens", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "ART", "NN", "NN", "$,", "ADV", "VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.10": {"text": "F\u00fchl jezt in W\u00e4ldern die Lust, die Held und H\u00f6fling nicht kennt.", "tokens": ["F\u00fchl", "jezt", "in", "W\u00e4l\u00b7dern", "die", "Lust", ",", "die", "Held", "und", "H\u00f6f\u00b7ling", "nicht", "kennt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "NN", "ART", "NN", "$,", "ART", "NN", "KON", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "Was hilfts mit freudigem Blick, vom Dunst der Ehre betrunken,", "tokens": ["Was", "hilfts", "mit", "freu\u00b7di\u00b7gem", "Blick", ",", "vom", "Dunst", "der", "Eh\u00b7re", "be\u00b7trun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPR", "ADJA", "NN", "$,", "APPRART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+--+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "Mit Ordensketten beschwert, gekr\u00f6nte Henker zu scheun?", "tokens": ["Mit", "Or\u00b7dens\u00b7ket\u00b7ten", "be\u00b7schwert", ",", "ge\u00b7kr\u00f6n\u00b7te", "Hen\u00b7ker", "zu", "scheun", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.13": {"text": "Was hilfts wenn k\u00fcnftig dein Grab verg\u00fcldete Waffen besch\u00fctzen,", "tokens": ["Was", "hilfts", "wenn", "k\u00fcnf\u00b7tig", "dein", "Grab", "ver\u00b7g\u00fcl\u00b7de\u00b7te", "Waf\u00b7fen", "be\u00b7sch\u00fct\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "KOUS", "ADJD", "PPOSAT", "NN", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+--+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "Wenn man aus Marmor dein Bild im schreckenden Panzer erh\u00f6ht!", "tokens": ["Wenn", "man", "aus", "Mar\u00b7mor", "dein", "Bild", "im", "schre\u00b7cken\u00b7den", "Pan\u00b7zer", "er\u00b7h\u00f6ht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "NN", "PPOSAT", "NN", "APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+--+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.15": {"text": "Achill und Hanniball mu\u00df die Nacht des Todes durchschlafen,", "tokens": ["A\u00b7chill", "und", "Han\u00b7ni\u00b7ball", "mu\u00df", "die", "Nacht", "des", "To\u00b7des", "durch\u00b7schla\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "VMFIN", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.16": {"text": "Die nach der Schickung Gesetz mich einst in Finsterni\u00df h\u00fcllt.", "tokens": ["Die", "nach", "der", "Schi\u00b7ckung", "Ge\u00b7setz", "mich", "einst", "in", "Fins\u00b7ter\u00b7ni\u00df", "h\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "NN", "PPER", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.17": {"text": "Im Tode werd ich ihm gleich, im Leben bin ich begl\u00fcckter.", "tokens": ["Im", "To\u00b7de", "werd", "ich", "ihm", "gleich", ",", "im", "Le\u00b7ben", "bin", "ich", "be\u00b7gl\u00fcck\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "PPER", "ADV", "$,", "APPRART", "NN", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+--+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.18": {"text": "Er sah nur Auen voll Blut, schlief nur vom Himmel bedeckt,", "tokens": ["Er", "sah", "nur", "Au\u00b7en", "voll", "Blut", ",", "schlief", "nur", "vom", "Him\u00b7mel", "be\u00b7deckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "NN", "ADJD", "NN", "$,", "VVFIN", "ADV", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.19": {"text": "Und h\u00f6rt ein ewig Geschwirr, von Schilden, Spiessen und Pfeilen,", "tokens": ["Und", "h\u00f6rt", "ein", "e\u00b7wig", "Ge\u00b7schwirr", ",", "von", "Schil\u00b7den", ",", "Spies\u00b7sen", "und", "Pfei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJD", "NN", "$,", "APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+--+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.20": {"text": "Ihn flohn Vergn\u00fcgen und Scherz, und Cypris freundlicher Sohn.", "tokens": ["Ihn", "flohn", "Ver\u00b7gn\u00fc\u00b7gen", "und", "Scherz", ",", "und", "Cyp\u00b7ris", "freund\u00b7li\u00b7cher", "Sohn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "$,", "KON", "NE", "ADJA", "NN", "$."], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.21": {"text": "Ich seh auf blumigter Flur das Winken schattigter Erlen,", "tokens": ["Ich", "seh", "auf", "blu\u00b7mig\u00b7ter", "Flur", "das", "Win\u00b7ken", "schat\u00b7tig\u00b7ter", "Er\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.22": {"text": "Den Schmuck des lachenden Thals, die weissen Birken voll Laub,", "tokens": ["Den", "Schmuck", "des", "la\u00b7chen\u00b7den", "Thals", ",", "die", "weis\u00b7sen", "Bir\u00b7ken", "voll", "Laub", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "ADJD", "NN", "$,"], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.23": {"text": "Den drinnen irrenden Bach. Ich schlaf in Lauben von Rosen,", "tokens": ["Den", "drin\u00b7nen", "ir\u00b7ren\u00b7den", "Bach", ".", "Ich", "schlaf", "in", "Lau\u00b7ben", "von", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$.", "PPER", "VVFIN", "APPR", "NN", "APPR", "NE", "$,"], "meter": "-+-+--+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.24": {"text": "Und h\u00f6re Chloens Gesang, ob dem die Nachtigall schweigt,", "tokens": ["Und", "h\u00f6\u00b7re", "Chloens", "Ge\u00b7sang", ",", "ob", "dem", "die", "Nach\u00b7ti\u00b7gall", "schweigt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NE", "NN", "$,", "KOUS", "ART", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.25": {"text": "Und lauscht, und aufmerksam horcht. Rings um mich flattert die Freude.", "tokens": ["Und", "lauscht", ",", "und", "auf\u00b7merk\u00b7sam", "horcht", ".", "Rings", "um", "mich", "flat\u00b7tert", "die", "Freu\u00b7de", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KON", "ADJD", "VVFIN", "$.", "NN", "APPR", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+---+--+--+-", "measure": "iambic.penta.relaxed"}, "line.26": {"text": "Die kleine Fillis im Hain verbirgt sich wenn sie mich merkt,", "tokens": ["Die", "klei\u00b7ne", "Fil\u00b7lis", "im", "Hain", "ver\u00b7birgt", "sich", "wenn", "sie", "mich", "merkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "APPRART", "NN", "VVFIN", "PRF", "KOUS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+--+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.27": {"text": "Ich such und finde sie nicht; bis sie im dicken Gestr\u00e4uche,", "tokens": ["Ich", "such", "und", "fin\u00b7de", "sie", "nicht", ";", "bis", "sie", "im", "di\u00b7cken", "Ge\u00b7str\u00e4u\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PPER", "PTKNEG", "$.", "KOUS", "PPER", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+---+--+--+-", "measure": "iambic.penta.relaxed"}, "line.28": {"text": "Wo Ph\u00f6bus selbst sie nicht sieht, ein schalkhaft L\u00e4cheln verr\u00e4th.", "tokens": ["Wo", "Ph\u00f6\u00b7bus", "selbst", "sie", "nicht", "sieht", ",", "ein", "schalk\u00b7haft", "L\u00e4\u00b7cheln", "ver\u00b7r\u00e4\u00b7th", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "ADV", "PPER", "PTKNEG", "VVFIN", "$,", "ART", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+--+-+-+--+-", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Die St\u00fcrme w\u00fcten nicht mehr, man sieht die Zacken der Tannen", "tokens": ["Die", "St\u00fcr\u00b7me", "w\u00fc\u00b7ten", "nicht", "mehr", ",", "man", "sieht", "die", "Za\u00b7cken", "der", "Tan\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "ADV", "$,", "PIS", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+--+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Nicht mehr durch gl\u00e4sernen Reif; man sieht im eislosen Bach", "tokens": ["Nicht", "mehr", "durch", "gl\u00e4\u00b7ser\u00b7nen", "Reif", ";", "man", "sieht", "im", "eis\u00b7lo\u00b7sen", "Bach"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "APPR", "ADJA", "NN", "$.", "PIS", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Am Grunde Muscheln und Gras und junge wankende Blumen,", "tokens": ["Am", "Grun\u00b7de", "Mu\u00b7scheln", "und", "Gras", "und", "jun\u00b7ge", "wan\u00b7ken\u00b7de", "Blu\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NN", "KON", "NN", "KON", "ADJA", "ADJA", "NN", "$,"], "meter": "-+----+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Ein dunkles schwebendes Laub erf\u00fcllt den Buchwald mit Nacht.", "tokens": ["Ein", "dunk\u00b7les", "schwe\u00b7ben\u00b7des", "Laub", "er\u00b7f\u00fcllt", "den", "Buch\u00b7wald", "mit", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"],