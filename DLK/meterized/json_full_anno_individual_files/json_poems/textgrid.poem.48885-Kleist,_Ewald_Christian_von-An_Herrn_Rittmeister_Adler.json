 "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Freund! flieh der Waffen Ger\u00e4usch, jezt ist die Zeit des Vergn\u00fcgens,", "tokens": ["Freund", "!", "flieh", "der", "Waf\u00b7fen", "Ge\u00b7r\u00e4usch", ",", "jezt", "ist", "die", "Zeit", "des", "Ver\u00b7gn\u00fc\u00b7gens", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "ART", "NN", "NN", "$,", "ADV", "VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.10": {"text": "F\u00fchl jezt in W\u00e4ldern die Lust, die Held und H\u00f6fling nicht kennt.", "tokens": ["F\u00fchl", "jezt", "in", "W\u00e4l\u00b7dern", "die", "Lust", ",", "die", "Held", "und", "H\u00f6f\u00b7ling", "nicht", "kennt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "NN", "ART", "NN", "$,", "ART", "NN", "KON", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "Was hilfts mit freudigem Blick, vom Dunst der Ehre betrunken,", "tokens": ["Was", "hilfts", "mit", "freu\u00b7di\u00b7gem", "Blick", ",", "vom", "Dunst", "der", "Eh\u00b7re", "be\u00b7trun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPR", "ADJA", "NN", "$,", "APPRART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+--+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "Mit Ordensketten beschwert, gekr\u00f6nte Henker zu scheun?", "tokens": ["Mit", "Or\u00b7dens\u00b7ket\u00b7ten", "be\u00b7schwert", ",", "ge\u00b7kr\u00f6n\u00b7te", "Hen\u00b7ker", "zu", "scheun", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", 