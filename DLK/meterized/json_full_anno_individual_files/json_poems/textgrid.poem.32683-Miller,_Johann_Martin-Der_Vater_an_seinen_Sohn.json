{"textgrid.poem.32683": {"metadata": {"author": {"name": "Miller, Johann Martin", "birth": "N.A.", "death": "N.A."}, "title": "Der Vater an seinen Sohn", "genre": "verse", "period": "N.A.", "pub_year": 1772, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Sohn, du wendest dein Gesicht?", "tokens": ["O", "Sohn", ",", "du", "wen\u00b7dest", "dein", "Ge\u00b7sicht", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und h\u00f6rest deinen Vater nicht,", "tokens": ["Und", "h\u00f6\u00b7rest", "dei\u00b7nen", "Va\u00b7ter", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und spottest seiner Z\u00e4hren?", "tokens": ["Und", "spot\u00b7test", "sei\u00b7ner", "Z\u00e4h\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nicht deinen Gott, nicht die Natur,", "tokens": ["Nicht", "dei\u00b7nen", "Gott", ",", "nicht", "die", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PPOSAT", "NN", "$,", "PTKNEG", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Des Lasters Stimme willst du nur,", "tokens": ["Des", "Las\u00b7ters", "Stim\u00b7me", "willst", "du", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VMFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Betrogner J\u00fcngling, h\u00f6ren?", "tokens": ["Be\u00b7trog\u00b7ner", "J\u00fcng\u00b7ling", ",", "h\u00f6\u00b7ren", "?"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Sieh einmal noch zur\u00fcck, eh du", "tokens": ["Sieh", "ein\u00b7mal", "noch", "zu\u00b7r\u00fcck", ",", "eh", "du"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "ADV", "ADV", "PTKVZ", "$,", "KOUS", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der rettungslosen Tiefe zu", "tokens": ["Der", "ret\u00b7tungs\u00b7lo\u00b7sen", "Tie\u00b7fe", "zu"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PTKZU"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Im trunknen Taumel rennest!", "tokens": ["Im", "trunk\u00b7nen", "Tau\u00b7mel", "ren\u00b7nest", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und dann vergeblich H\u00fclfe suchst,", "tokens": ["Und", "dann", "ver\u00b7geb\u00b7lich", "H\u00fcl\u00b7fe", "suchst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und dir und deinem Sch\u00f6pfer fluchst,", "tokens": ["Und", "dir", "und", "dei\u00b7nem", "Sch\u00f6p\u00b7fer", "fluchst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "KON", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und W\u00fcterich ihn nennest!", "tokens": ["Und", "W\u00fc\u00b7te\u00b7rich", "ihn", "nen\u00b7nest", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Der feilen Buhlerin im Arm,", "tokens": ["Der", "fei\u00b7len", "Buh\u00b7le\u00b7rin", "im", "Arm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Umflattert dich der Tr\u00e4ume Schwarm,", "tokens": ["Um\u00b7flat\u00b7tert", "dich", "der", "Tr\u00e4u\u00b7me", "Schwarm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die mit der Nacht verschwinden,", "tokens": ["Die", "mit", "der", "Nacht", "ver\u00b7schwin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und deinen hei\u00dfen Busen bald,", "tokens": ["Und", "dei\u00b7nen", "hei\u00b7\u00dfen", "Bu\u00b7sen", "bald", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "In schlangenf\u00f6rmiger Gestalt,", "tokens": ["In", "schlan\u00b7gen\u00b7f\u00f6r\u00b7mi\u00b7ger", "Ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit tiefem Schmerz umwinden.", "tokens": ["Mit", "tie\u00b7fem", "Schmerz", "um\u00b7win\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Trink immer deinen Taumelwein", "tokens": ["Trink", "im\u00b7mer", "dei\u00b7nen", "Tau\u00b7mel\u00b7wein"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit gierigstarken Z\u00fcgen ein!", "tokens": ["Mit", "gie\u00b7rigs\u00b7tar\u00b7ken", "Z\u00fc\u00b7gen", "ein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bald wird der Rausch entweichen;", "tokens": ["Bald", "wird", "der", "Rausch", "ent\u00b7wei\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und abgemattet, freudenleer,", "tokens": ["Und", "ab\u00b7ge\u00b7mat\u00b7tet", ",", "freu\u00b7den\u00b7leer", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["KON", "VVPP", "$,", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wirst du im Schwelgersaal' umher", "tokens": ["Wirst", "du", "im", "Schwel\u00b7ger\u00b7saal'", "um\u00b7her"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "APPRART", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Beim Reihentanze schleichen.", "tokens": ["Beim", "Rei\u00b7hen\u00b7tan\u00b7ze", "schlei\u00b7chen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ich sterbe, Sohn! Der Gram beschlie\u00dft", "tokens": ["Ich", "ster\u00b7be", ",", "Sohn", "!", "Der", "Gram", "be\u00b7schlie\u00dft"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "NN", "$.", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Leben, das dir l\u00e4stig ist;", "tokens": ["Ein", "Le\u00b7ben", ",", "das", "dir", "l\u00e4s\u00b7tig", "ist", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bald wird das Grab mich decken.", "tokens": ["Bald", "wird", "das", "Grab", "mich", "de\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Komm dann, mit deiner Buhlerin,", "tokens": ["Komm", "dann", ",", "mit", "dei\u00b7ner", "Buh\u00b7le\u00b7rin", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Zur Asche deines Vaters hin,", "tokens": ["Zur", "A\u00b7sche", "dei\u00b7nes", "Va\u00b7ters", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ganz deinen Sieg zu schmecken!", "tokens": ["Ganz", "dei\u00b7nen", "Sieg", "zu", "schme\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Dann pfl\u00fccke, Sohn, auf meinem Grab", "tokens": ["Dann", "pfl\u00fc\u00b7cke", ",", "Sohn", ",", "auf", "mei\u00b7nem", "Grab"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "NN", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dir Blumen zu den Kr\u00e4nzen ab,", "tokens": ["Dir", "Blu\u00b7men", "zu", "den", "Kr\u00e4n\u00b7zen", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie um ihr Haar zu winden!", "tokens": ["Sie", "um", "ihr", "Haar", "zu", "win\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und tanze frech auf meiner Gruft,", "tokens": ["Und", "tan\u00b7ze", "frech", "auf", "mei\u00b7ner", "Gruft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und, wann noch das Gewissen ruft,", "tokens": ["Und", ",", "wann", "noch", "das", "Ge\u00b7wis\u00b7sen", "ruft", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWAV", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "T\u00f6t' es mit neuen S\u00fcnden!", "tokens": ["T\u00f6t'", "es", "mit", "neu\u00b7en", "S\u00fcn\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.7": {"line.1": {"text": "Zuviel, o Gott! Ach, h\u00f6re, Sohn,", "tokens": ["Zu\u00b7viel", ",", "o", "Gott", "!", "Ach", ",", "h\u00f6\u00b7re", ",", "Sohn", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "FM", "NN", "$.", "ITJ", "$,", "VVFIN", "$,", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Noch einmal die Religion,", "tokens": ["Noch", "ein\u00b7mal", "die", "Re\u00b7li\u00b7gi\u00b7on", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sie will sich dein erbarmen.", "tokens": ["Sie", "will", "sich", "dein", "er\u00b7bar\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "PPOSAT", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nicht fremden Lobes darf sie, Sohn;", "tokens": ["Nicht", "frem\u00b7den", "Lo\u00b7bes", "darf", "sie", ",", "Sohn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PTKNEG", "ADJA", "NN", "VMFIN", "PPER", "$,", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ohn' Eigennutz und Eigenlohn", "tokens": ["Ohn'", "Ei\u00b7gen\u00b7nutz", "und", "Ei\u00b7gen\u00b7lohn"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Will sie sich dein erbarmen.", "tokens": ["Will", "sie", "sich", "dein", "er\u00b7bar\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "PPOSAT", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Du h\u00f6rest, ach, du h\u00f6rest nicht?", "tokens": ["Du", "h\u00f6\u00b7rest", ",", "ach", ",", "du", "h\u00f6\u00b7rest", "nicht", "?"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ITJ", "$,", "PPER", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wohlan, so h\u00f6re Gott dich nicht!", "tokens": ["Wo\u00b7hlan", ",", "so", "h\u00f6\u00b7re", "Gott", "dich", "nicht", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "VVFIN", "NN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wann nun der Tod dich schrecket,", "tokens": ["Wann", "nun", "der", "Tod", "dich", "schre\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und dein Gewissen f\u00fcrchterlich", "tokens": ["Und", "dein", "Ge\u00b7wis\u00b7sen", "f\u00fcrch\u00b7ter\u00b7lich"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Zu H\u00f6llenpein und Qualen, dich", "tokens": ["Zu", "H\u00f6l\u00b7len\u00b7pein", "und", "Qua\u00b7len", ",", "dich"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "NN", "KON", "NN", "$,", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Aus tiefem Schlummer wecket.", "tokens": ["Aus", "tie\u00b7fem", "Schlum\u00b7mer", "we\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Doch, Sohn! mir bricht das Herz. Dein Gott", "tokens": ["Doch", ",", "Sohn", "!", "mir", "bricht", "das", "Herz", ".", "Dein", "Gott"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "$,", "NN", "$.", "PPER", "VVFIN", "ART", "NN", "$.", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Erbarme sich in deiner Not,", "tokens": ["Er\u00b7bar\u00b7me", "sich", "in", "dei\u00b7ner", "Not", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und horch' auf deine Klagen!", "tokens": ["Und", "horch'", "auf", "dei\u00b7ne", "Kla\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "O m\u00f6cht' er, durch mein Leid erfleht,", "tokens": ["O", "m\u00f6cht'", "er", ",", "durch", "mein", "Leid", "er\u00b7fleht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPER", "$,", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Zu meinem jammernden Gebet", "tokens": ["Zu", "mei\u00b7nem", "jam\u00b7mern\u00b7den", "Ge\u00b7bet"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Ein gn\u00e4dig Amen! sagen!", "tokens": ["Ein", "gn\u00e4\u00b7dig", "A\u00b7men", "!", "sa\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "O Sohn, du wendest dein Gesicht?", "tokens": ["O", "Sohn", ",", "du", "wen\u00b7dest", "dein", "Ge\u00b7sicht", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und h\u00f6rest deinen Vater nicht,", "tokens": ["Und", "h\u00f6\u00b7rest", "dei\u00b7nen", "Va\u00b7ter", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und spottest seiner Z\u00e4hren?", "tokens": ["Und", "spot\u00b7test", "sei\u00b7ner", "Z\u00e4h\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$."], "met