{"textgrid.poem.59226": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der Wegelagerer", "genre": "verse", "period": "N.A.", "pub_year": 1839, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es ist ein Land, wo die Philister thronen,", "tokens": ["Es", "ist", "ein", "Land", ",", "wo", "die", "Phi\u00b7lis\u00b7ter", "thro\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PWAV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Kr\u00e4mer fahren und das Gr\u00fcn verstauben,", "tokens": ["Die", "Kr\u00e4\u00b7mer", "fah\u00b7ren", "und", "das", "Gr\u00fcn", "ver\u00b7stau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "KON", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Liebe selber altklug feilscht mit Hauben \u2013", "tokens": ["Die", "Lie\u00b7be", "sel\u00b7ber", "alt\u00b7klug", "feilscht", "mit", "Hau\u00b7ben", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "VVFIN", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Herr Gott, wie lang willst du die Brut verschonen!", "tokens": ["Herr", "Gott", ",", "wie", "lang", "willst", "du", "die", "Brut", "ver\u00b7scho\u00b7nen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "PWAV", "ADJD", "VMFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Es ist ein Wald, der rauscht mit gr\u00fcnen Kronen,", "tokens": ["Es", "ist", "ein", "Wald", ",", "der", "rauscht", "mit", "gr\u00fc\u00b7nen", "Kro\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PRELS", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wo frei die Adler horsten, und die Tauben", "tokens": ["Wo", "frei", "die", "Ad\u00b7ler", "hors\u00b7ten", ",", "und", "die", "Tau\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ADJD", "ART", "NN", "VVFIN", "$,", "KON", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Unschuldig girren in den k\u00fchlen Lauben,", "tokens": ["Un\u00b7schul\u00b7dig", "gir\u00b7ren", "in", "den", "k\u00fch\u00b7len", "Lau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die noch kein Fu\u00df betrat \u2013 dort will ich wohnen!", "tokens": ["Die", "noch", "kein", "Fu\u00df", "be\u00b7trat", "\u2013", "dort", "will", "ich", "woh\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PIAT", "NN", "VVFIN", "$(", "ADV", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Dort will ich n\u00e4chtlich auf die Kr\u00e4mer lauern", "tokens": ["Dort", "will", "ich", "n\u00e4cht\u00b7lich", "auf", "die", "Kr\u00e4\u00b7mer", "lau\u00b7ern"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und k\u00fchn zerhaun der armen Sch\u00f6nheit Bande,", "tokens": ["Und", "k\u00fchn", "zer\u00b7haun", "der", "ar\u00b7men", "Sch\u00f6n\u00b7heit", "Ban\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die sie als niedre Magd zu Markte f\u00fchren.", "tokens": ["Die", "sie", "als", "nied\u00b7re", "Magd", "zu", "Mark\u00b7te", "f\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "KOUS", "ADJA", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Hoch soll sie stehn auf gr\u00fcnen Felsenmauern,", "tokens": ["Hoch", "soll", "sie", "stehn", "auf", "gr\u00fc\u00b7nen", "Fel\u00b7sen\u00b7mau\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VMFIN", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Da\u00df mahnend \u00fcber alle stillen Lande", "tokens": ["Da\u00df", "mah\u00b7nend", "\u00fc\u00b7ber", "al\u00b7le", "stil\u00b7len", "Lan\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "APPR", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die L\u00fcfte nachts ihr Zauberlied verf\u00fchren.", "tokens": ["Die", "L\u00fcf\u00b7te", "nachts", "ihr", "Zau\u00b7berl\u00b7ied", "ver\u00b7f\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Es ist ein Land, wo die Philister thronen,", "tokens": ["Es", "ist", "ein", "Land", ",", "wo", "die", "Phi\u00b