{"dta.poem.20632": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "Jupiter und sein Adler.  \n An  \n den Verfasser des Gesanges  Ptolom\u00e4us  und  \n  Berenice .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Freund! des Olympus G\u00f6tter leerten                 ", "tokens": ["Freund", "!", "des", "O\u00b7lym\u00b7pus", "G\u00f6t\u00b7ter", "leer\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "ART", "NE", "NN", "VVINF"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "J\u00fcngst ihre Nektarschaalen nicht,", "tokens": ["J\u00fcngst", "ih\u00b7re", "Nek\u00b7tarsc\u00b7haa\u00b7len", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie lie\u00dfen sich herab und h\u00f6rten,", "tokens": ["Sie", "lie\u00b7\u00dfen", "sich", "her\u00b7ab", "und", "h\u00f6r\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Auf Wolken lauschend, Dein Gedicht.", "tokens": ["Auf", "Wol\u00b7ken", "lau\u00b7schend", ",", "Dein", "Ge\u00b7dicht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die Wonnel\u00e4chelnde Cythere", "tokens": ["Die", "Won\u00b7ne\u00b7l\u00e4\u00b7cheln\u00b7de", "Cy\u00b7the\u00b7re"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NE"], "meter": "-+-+--+--", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Trat zur Olympia, und sprach:", "tokens": ["Trat", "zur", "O\u00b7lym\u00b7pia", ",", "und", "sprach", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "$,", "KON", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Welch Sterblicher singt mir zur Ehre", "tokens": ["Welch", "Sterb\u00b7li\u00b7cher", "singt", "mir", "zur", "Eh\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "So lieblich dem Apollo nach?", "tokens": ["So", "lieb\u00b7lich", "dem", "A\u00b7pol\u00b7lo", "nach", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NE", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wie einst Homer ihm nachgesungen,", "tokens": ["Wie", "einst", "Ho\u00b7mer", "ihm", "nach\u00b7ge\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "NE", "PPER", "VVIZU", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Als er des Donnergottes Trieb", "tokens": ["Als", "er", "des", "Don\u00b7ner\u00b7got\u00b7tes", "Trieb"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und neue s\u00fc\u00dfe Schmeichelungen", "tokens": ["Und", "neu\u00b7e", "s\u00fc\u00b7\u00dfe", "Schmei\u00b7che\u00b7lun\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und meines G\u00fcrtels Macht beschrieb?", "tokens": ["Und", "mei\u00b7nes", "G\u00fcr\u00b7tels", "Macht", "be\u00b7schrieb", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "So sagte Venus, und die gro\u00dfe", "tokens": ["So", "sag\u00b7te", "Ve\u00b7nus", ",", "und", "die", "gro\u00b7\u00dfe"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NE", "$,", "KON", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Verfolgerinn des Herkuls schwieg,", "tokens": ["Ver\u00b7fol\u00b7ge\u00b7rinn", "des", "Her\u00b7kuls", "schwieg", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als von des Wolkenlenkers Schoo\u00dfe", "tokens": ["Als", "von", "des", "Wol\u00b7ken\u00b7len\u00b7kers", "Schoo\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Selbst Ganimed zur Erde stieg;", "tokens": ["Selbst", "Ga\u00b7ni\u00b7med", "zur", "Er\u00b7de", "stieg", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Und selbst der Adler von dem Sitze", "tokens": ["Und", "selbst", "der", "Ad\u00b7ler", "von", "dem", "Sit\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Des Gottes sich erhub, und k\u00fchn", "tokens": ["Des", "Got\u00b7tes", "sich", "er\u00b7hub", ",", "und", "k\u00fchn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "PRF", "VVFIN", "$,", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zur\u00fcckwarf die getragnen Blitze,", "tokens": ["Zu\u00b7r\u00fcck\u00b7warf", "die", "ge\u00b7trag\u00b7nen", "Blit\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und auf dem Haine vor Berlin", "tokens": ["Und", "auf", "dem", "Hai\u00b7ne", "vor", "Ber\u00b7lin"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "APPR", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Sein ewig gl\u00e4nzendes Gefieder,", "tokens": ["Sein", "e\u00b7wig", "gl\u00e4n\u00b7zen\u00b7des", "Ge\u00b7fie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Indem er horchte, niedersank,", "tokens": ["In\u00b7dem", "er", "horch\u00b7te", ",", "nie\u00b7der\u00b7sank", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und Jupiter ihm rief: Komm wieder!", "tokens": ["Und", "Ju\u00b7pi\u00b7ter", "ihm", "rief", ":", "Komm", "wie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "NN", "PPER", "VVFIN", "$.", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und bringe mir zum G\u00f6ttertrank", "tokens": ["Und", "brin\u00b7ge", "mir", "zum", "G\u00f6t\u00b7ter\u00b7trank"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Das goldne Trinkgef\u00e4\u00df, und bringe", "tokens": ["Das", "gold\u00b7ne", "Trink\u00b7ge\u00b7f\u00e4\u00df", ",", "und", "brin\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "KON", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Des Dichters Lied darin gelegt,", "tokens": ["Des", "Dich\u00b7ters", "Lied", "da\u00b7rin", "ge\u00b7legt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PAV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df der Latona Sohn es singe,", "tokens": ["Da\u00df", "der", "La\u00b7to\u00b7na", "Sohn", "es", "sin\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NE", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn er vor uns die Cyther schl\u00e4gt;", "tokens": ["Wenn", "er", "vor", "uns", "die", "Cy\u00b7ther", "schl\u00e4gt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Und trag\u2019 in deiner rechten Klaue", "tokens": ["Und", "trag'", "in", "dei\u00b7ner", "rech\u00b7ten", "Klau\u00b7e"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Berenice Locke, die", "tokens": ["Der", "Be\u00b7re\u00b7ni\u00b7ce", "Lo\u00b7cke", ",", "die"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "NN", "$,", "PRELS"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich dir auf immer anvertraue;", "tokens": ["Ich", "dir", "auf", "im\u00b7mer", "an\u00b7ver\u00b7trau\u00b7e", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPER", "APPR", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Statt meiner Blitze f\u00fchre sie!", "tokens": ["Statt", "mei\u00b7ner", "Blit\u00b7ze", "f\u00fch\u00b7re", "sie", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}