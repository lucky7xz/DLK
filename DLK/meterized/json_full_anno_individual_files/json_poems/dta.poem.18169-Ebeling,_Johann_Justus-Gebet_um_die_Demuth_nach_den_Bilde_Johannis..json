{"dta.poem.18169": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Gebet um die Demuth nach den  \n Bilde Johannis.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198805", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mein GOtt! gib mir stets Kraft in De-", "tokens": ["Mein", "Gott", "!", "gib", "mir", "stets", "Kraft", "in", "De"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$.", "VVIMP", "PPER", "ADV", "NN", "APPR", "TRUNC"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "muth so zu leben,", "tokens": ["muth", "so", "zu", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Wie mir des Heilands Lehr kan Un-", "tokens": ["Wie", "mir", "des", "Hei\u00b7lands", "Lehr", "kan", "Un"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ART", "ADJA", "NN", "VMFIN", "TRUNC"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "terweisung geben;", "tokens": ["ter\u00b7wei\u00b7sung", "ge\u00b7ben", ";"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Da\u00df ich im Niedrigsein die wahre H\u00f6he suche,", "tokens": ["Da\u00df", "ich", "im", "Nied\u00b7rig\u00b7sein", "die", "wah\u00b7re", "H\u00f6\u00b7he", "su\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und den erhabnen Schwulst des Hochmuts Geistes", "tokens": ["Und", "den", "er\u00b7hab\u00b7nen", "Schwulst", "des", "Hoch\u00b7muts", "Geis\u00b7tes"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "fluche.", "tokens": ["flu\u00b7che", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.8": {"text": "Johannes war sehr gros; er war ein scheinend Licht", "tokens": ["Jo\u00b7han\u00b7nes", "war", "sehr", "gros", ";", "er", "war", "ein", "schei\u00b7nend", "Licht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "ADV", "ADJD", "$.", "PPER", "VAFIN", "ART", "ADJD", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ein glimmend d\u00fcstrer Tacht, gleicht einer Fakkel nicht;", "tokens": ["Ein", "glim\u00b7mend", "d\u00fcst\u00b7rer", "Tacht", ",", "gleicht", "ei\u00b7ner", "Fak\u00b7kel", "nicht", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$,", "VVFIN", "ART", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "So wenig gleich ich ihn: doch werd ich ihm noch", "tokens": ["So", "we\u00b7nig", "gleich", "ich", "ihn", ":", "doch", "werd", "ich", "ihm", "noch"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADV", "PPER", "PPER", "$.", "ADV", "VAFIN", "PPER", "PPER", "ADV"], "meter": "-+----+-+-+", "measure": "dactylic.init"}, "line.11": {"text": "gleich", "tokens": ["gleich"], "token_info": ["word"], "pos": ["ADV"], "meter": "-", "measure": "single.down"}, "line.12": {"text": "Wenn ich dem\u00fcthig bin in Christi Himmelreich.", "tokens": ["Wenn", "ich", "de\u00b7m\u00fct\u00b7hig", "bin", "in", "Chris\u00b7ti", "Him\u00b7mel\u00b7reich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VAFIN", "APPR", "NE", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}