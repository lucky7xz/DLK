{"dta.poem.1216": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Ich bringe wieder her und \u00fcber mein Verhoffen", "tokens": ["Ich", "brin\u00b7ge", "wie\u00b7der", "her", "und", "\u00fc\u00b7ber", "mein", "Ver\u00b7hof\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PTKVZ", "KON", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In di\u00df betr\u00fcbte Land der siechen Glieder Last/", "tokens": ["In", "di\u00df", "be\u00b7tr\u00fcb\u00b7te", "Land", "der", "sie\u00b7chen", "Glie\u00b7der", "Last", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDS", "VVFIN", "NN", "ART", "ADJA", "NN", "NE", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Den Tod/ den ich gesucht/ hab ich nicht angetroffen/", "tokens": ["Den", "Tod", "/", "den", "ich", "ge\u00b7sucht", "/", "hab", "ich", "nicht", "an\u00b7ge\u00b7trof\u00b7fen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PRELS", "PPER", "VVPP", "$(", "VAFIN", "PPER", "PTKNEG", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ich habe mir umsonst zum Sterben Mutt gefast;", "tokens": ["Ich", "ha\u00b7be", "mir", "um\u00b7sonst", "zum", "Ster\u00b7ben", "Mutt", "ge\u00b7fast", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "APPRART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Weil ich/ mein s\u00fcsser Tod/ von dir entfernt gewesen/", "tokens": ["Weil", "ich", "/", "mein", "s\u00fcs\u00b7ser", "Tod", "/", "von", "dir", "ent\u00b7fernt", "ge\u00b7we\u00b7sen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$(", "PPOSAT", "ADJA", "NN", "$(", "APPR", "PPER", "VVPP", "VAPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So hab ich nicht gek\u00f6nnt noch sterben noch genesen.", "tokens": ["So", "hab", "ich", "nicht", "ge\u00b7k\u00f6nnt", "noch", "ster\u00b7ben", "noch", "ge\u00b7ne\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PTKNEG", "VVPP", "ADV", "VVFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Das macht dein edles Bild/ in meine Brust gepr\u00e4get/", "tokens": ["Das", "macht", "dein", "ed\u00b7les", "Bild", "/", "in", "mei\u00b7ne", "Brust", "ge\u00b7pr\u00e4\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "ADJA", "NN", "$(", "APPR", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Das ich in deine Hand zu lieffern schuldig bin.", "tokens": ["Das", "ich", "in", "dei\u00b7ne", "Hand", "zu", "lief\u00b7fern", "schul\u00b7dig", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Schau deinen Knecht/ der sich zu deinen F\u00fcssen leget:", "tokens": ["Schau", "dei\u00b7nen", "Knecht", "/", "der", "sich", "zu", "dei\u00b7nen", "F\u00fcs\u00b7sen", "le\u00b7get", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$(", "PRELS", "PRF", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Nimm diesen edlen Schatz samt meinem Hertzen hin.", "tokens": ["Nimm", "die\u00b7sen", "ed\u00b7len", "Schatz", "samt", "mei\u00b7nem", "Hert\u00b7zen", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PDAT", "ADJA", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ich sterbe wohl vergn\u00fcgt/ ich sterbe gnung beklaget/", "tokens": ["Ich", "ster\u00b7be", "wohl", "ver\u00b7gn\u00fcgt", "/", "ich", "ster\u00b7be", "gnung", "be\u00b7kla\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "VVPP", "$(", "PPER", "VVFIN", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Wenn nur dein Mund/ Ade du treue Seele/ saget.", "tokens": ["Wenn", "nur", "dein", "Mund", "/", "A\u00b7de", "du", "treu\u00b7e", "See\u00b7le", "/", "sa\u00b7get", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "$(", "NN", "PPER", "ADJA", "NN", "$(", "VVFIN", "$."], "meter": "-+-++--+-+-+-", "measure": "iambic.hexa.relaxed"}}}}}