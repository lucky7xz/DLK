{"textgrid.poem.40132": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "1. K\u00e4fers Ende", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich habe den Traum der Rose belauscht,", "tokens": ["Ich", "ha\u00b7be", "den", "Traum", "der", "Ro\u00b7se", "be\u00b7lauscht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "der still, vom k\u00fchlen Duft umspr\u00fcht,", "tokens": ["der", "still", ",", "vom", "k\u00fch\u00b7len", "Duft", "um\u00b7spr\u00fcht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "$,", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "aus ihrer Blumenseele gl\u00fcht;", "tokens": ["aus", "ih\u00b7rer", "Blu\u00b7men\u00b7see\u00b7le", "gl\u00fcht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "ich hab' ihn mit allen Sinnen belauscht,", "tokens": ["ich", "hab'", "ihn", "mit", "al\u00b7len", "Sin\u00b7nen", "be\u00b7lauscht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "APPR", "PIAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.5": {"text": "bis ich berauscht.", "tokens": ["bis", "ich", "be\u00b7rauscht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Von Sonnenstrahl die Rose tr\u00e4umt,", "tokens": ["Von", "Son\u00b7nen\u00b7strahl", "die", "Ro\u00b7se", "tr\u00e4umt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "der sich tags ihr flammend ins Innerste w\u00fchlt,", "tokens": ["der", "sich", "tags", "ihr", "flam\u00b7mend", "ins", "In\u00b7ners\u00b7te", "w\u00fchlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ADV", "PPER", "VVPP", "APPRART", "NN", "VVFIN", "$,"], "meter": "--+-+-+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "der im Mondlicht nachts sie weich umsp\u00fclt,", "tokens": ["der", "im", "Mond\u00b7licht", "nachts", "sie", "weich", "um\u00b7sp\u00fclt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "APPR", "PPER", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "der mit schaffender Macht das All durchsch\u00e4umt;", "tokens": ["der", "mit", "schaf\u00b7fen\u00b7der", "Macht", "das", "All", "durch\u00b7sch\u00e4umt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "ART", "NN", "VVPP", "$."], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "von ", "tokens": ["von"], "token_info": ["word"], "pos": ["APPR"], "meter": "-", "measure": "single.down"}}, "stanza.3": {"line.1": {"text": "der still zum stillen Glutkelch klimmt,", "tokens": ["der", "still", "zum", "stil\u00b7len", "Glut\u00b7kelch", "klimmt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "in dem die Sehnsucht zehrt und glimmt", "tokens": ["in", "dem", "die", "Sehn\u00b7sucht", "zehrt