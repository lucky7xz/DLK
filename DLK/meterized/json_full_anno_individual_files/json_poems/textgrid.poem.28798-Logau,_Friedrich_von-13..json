{"textgrid.poem.28798": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "13.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich bin zwar auch ein Theil und denen beygestellet,", "tokens": ["Ich", "bin", "zwar", "auch", "ein", "Theil", "und", "de\u00b7nen", "bey\u00b7ge\u00b7stel\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ART", "NN", "KON", "PDS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die ihres Geistes Hoch zusammen hat gesellet", "tokens": ["Die", "ih\u00b7res", "Geis\u00b7tes", "Hoch", "zu\u00b7sam\u00b7men", "hat", "ge\u00b7sel\u00b7let"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPOSAT", "NN", "ADJD", "VVPP", "VAFIN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Zu treffen einen Bund, zu w\u00fcrcken tapffre Frucht,", "tokens": ["Zu", "tref\u00b7fen", "ei\u00b7nen", "Bund", ",", "zu", "w\u00fcr\u00b7cken", "tapf\u00b7fre", "Frucht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ART", "NN", "$,", "PTKZU", "VVINF", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df deutsches Hertz und Mund von neuem auffgesucht", "tokens": ["Da\u00df", "deut\u00b7sches", "Hertz", "und", "Mund", "von", "neu\u00b7em", "auff\u00b7ge\u00b7sucht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJA", "NN", "KON", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und seiner W\u00fcrd und Zier sey wieder \u00fcbergeben,", "tokens": ["Und", "sei\u00b7ner", "W\u00fcrd", "und", "Zier", "sey", "wie\u00b7der", "\u00fc\u00b7ber\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "KON", "NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und d\u00fcrffe ferner nicht ein armer Sclave leben", "tokens": ["Und", "d\u00fcrf\u00b7fe", "fer\u00b7ner", "nicht", "ein", "ar\u00b7mer", "Scla\u00b7ve", "le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "ADV", "PTKNEG", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der fremden Pralerey. Das Miltzkraut soll ich seyn;", "tokens": ["Der", "frem\u00b7den", "Pra\u00b7le\u00b7rey", ".", "Das", "Miltz\u00b7kraut", "soll", "ich", "seyn", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "ART", "NN", "VMFIN", "PPER", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Verkleinern soll ich stets, soll helffen treiben ein", "tokens": ["Ver\u00b7klei\u00b7nern", "soll", "ich", "stets", ",", "soll", "helf\u00b7fen", "trei\u00b7ben", "ein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VMFIN", "PPER", "ADV", "$,", "VMFIN", "VVINF", "VVINF", "ART"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Den auffgeschwollnen Miltz, die Art der stoltzen Sinnen,", "tokens": ["Den", "auff\u00b7ge\u00b7schwoll\u00b7nen", "Miltz", ",", "die", "Art", "der", "stolt\u00b7zen", "Sin\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Die sich in ihnen selbst beherbergen nicht k\u00fcnnen,", "tokens": ["Die", "sich", "in", "ih\u00b7nen", "selbst", "be\u00b7her\u00b7ber\u00b7gen", "nicht", "k\u00fcn\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "PPER", "ADV", "VVFIN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und denen viel zu eng ihr deutsches Vaterland.", "tokens": ["Und", "de\u00b7nen", "viel", "zu", "eng", "ihr", "deut\u00b7sches", "Va\u00b7ter\u00b7land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "ADV", "PTKA", "ADJD", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Sie lassen eignen Werth und wehlen fremden Tand,", "tokens": ["Sie", "las\u00b7sen", "eig\u00b7nen", "Werth", "und", "weh\u00b7len", "frem\u00b7den", "Tand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "KON", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Erkiesen Glas f\u00fcr Gold und wollen nichts beginnen,", "tokens": ["Er\u00b7kie\u00b7sen", "Glas", "f\u00fcr", "Gold", "und", "wol\u00b7len", "nichts", "be\u00b7gin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "NN", "KON", "VMFIN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Was diesem ist gem\u00e4\u00df, was etwa k\u00fcmmt von hinnen.", "tokens": ["Was", "die\u00b7sem", "ist", "ge\u00b7m\u00e4\u00df", ",", "was", "et\u00b7wa", "k\u00fcmmt", "von", "hin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PDS", "VAFIN", "ADJD", "$,", "PRELS", "ADV", "VVFIN", "APPR", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "So wie in Kleidern sie nunmehr sind Deutschen nicht,", "tokens": ["So", "wie", "in", "Klei\u00b7dern", "sie", "nun\u00b7mehr", "sind", "Deut\u00b7schen", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "APPR", "NN", "PPER", "ADV", "VAFIN", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "So soll auch nicht mehr deutsch seyn, was die Zunge spricht.", "tokens": ["So", "soll", "auch", "nicht", "mehr", "deutsch", "seyn", ",", "was", "die", "Zun\u00b7ge", "spricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ADV", "PTKNEG", "ADV", "ADJD", "VAINF", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Wie mu\u00df das Hertze seyn? Ich wil zwar nicht erm\u00fcden,", "tokens": ["Wie", "mu\u00df", "das", "Hert\u00b7ze", "seyn", "?", "Ich", "wil", "zwar", "nicht", "er\u00b7m\u00fc\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PDS", "VVFIN", "VAINF", "$.", "PPER", "VMFIN", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Da\u00df stets an ihren Orth sey meine Pflicht beschieden;", "tokens": ["Da\u00df", "stets", "an", "ih\u00b7ren", "Orth", "sey", "mei\u00b7ne", "Pflicht", "be\u00b7schie\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPR", "PPOSAT", "NN", "VAFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Da\u00df ich, Verkleinernder, verkleinre nicht den Stand,", "tokens": ["Da\u00df", "ich", ",", "Ver\u00b7klei\u00b7nern\u00b7der", ",", "ver\u00b7klein\u00b7re", "nicht", "den", "Stand", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "NN", "$,", "VVFIN", "PTKNEG", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Den mein Kraut unverhofft in diesem Garten fand,", "tokens": ["Den", "mein", "Kraut", "un\u00b7ver\u00b7hofft", "in", "die\u00b7sem", "Gar\u00b7ten", "fand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADJD", "APPR", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Wo so viel Cedern stehn und reiche Palmen prangen.", "tokens": ["Wo", "so", "viel", "Ce\u00b7dern", "stehn", "und", "rei\u00b7che", "Pal\u00b7men", "pran\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PIAT", "NN", "VVINF", "KON", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "So aber, was ich soll und wil, nicht zu erlangen,", "tokens": ["So", "a\u00b7ber", ",", "was", "ich", "soll", "und", "wil", ",", "nicht", "zu", "er\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "PWS", "PPER", "VMFIN", "KON", "VMFIN", "$,", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"t