{"dta.poem.18187": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Das  \n entz\u00fckkende Vergn\u00fcgen  \n aus dem Anschauen des Himmels bei  \n der Nacht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198805", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Farben Mannigfalt zeugt von", "tokens": ["Der", "Far\u00b7ben", "Man\u00b7nig\u00b7falt", "zeugt", "von"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVFIN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "des H\u00f6chsten G\u00fcte,", "tokens": ["des", "H\u00f6chs\u00b7ten", "G\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Erg\u00f6zzet unser Aug, erquikket", "tokens": ["Er\u00b7g\u00f6z\u00b7zet", "un\u00b7ser", "Aug", ",", "er\u00b7quik\u00b7ket"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "das Gem\u00fcthe,", "tokens": ["das", "Ge\u00b7m\u00fc\u00b7the", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Wie die Erfahrung lehrt, da uns", "tokens": ["Wie", "die", "Er\u00b7fah\u00b7rung", "lehrt", ",", "da", "uns"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$,", "KOUS", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "in dieser Welt,", "tokens": ["in", "die\u00b7ser", "Welt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Am Himmel, auf der Erd, viel", "tokens": ["Am", "Him\u00b7mel", ",", "auf", "der", "Erd", ",", "viel"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["APPRART", "NN", "$,", "APPR", "ART", "NN", "$,", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Sch\u00f6nes dargestellt.", "tokens": ["Sch\u00f6\u00b7nes", "dar\u00b7ge\u00b7stellt", "."], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.9": {"text": "Wer kann ohn s\u00fcsse Lust, in frohen Fr\u00fchlings-Tagen,", "tokens": ["Wer", "kann", "ohn", "s\u00fcs\u00b7se", "Lust", ",", "in", "fro\u00b7hen", "Fr\u00fch\u00b7lings\u00b7Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "APPR", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Der Felder Schmuk beschaun? Ohn inniges Behagen", "tokens": ["Der", "Fel\u00b7der", "Schmuk", "be\u00b7schaun", "?", "Ohn", "in\u00b7ni\u00b7ges", "Be\u00b7ha\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVINF", "$.", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ohn einen innren Reiz in einen Garten sehn,", "tokens": ["Ohn", "ei\u00b7nen", "inn\u00b7ren", "Reiz", "in", "ei\u00b7nen", "Gar\u00b7ten", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Worin auf gr\u00fcnen Grund geschm\u00fckte Blumen stehn?", "tokens": ["Wo\u00b7rin", "auf", "gr\u00fc\u00b7nen", "Grund", "ge\u00b7schm\u00fck\u00b7te", "Blu\u00b7men", "stehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ADJA", "NN", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Die roth, gr\u00fcn, gelb, blau wei\u00df so durch einander", "tokens": ["Die", "roth", ",", "gr\u00fcn", ",", "gelb", ",", "blau", "wei\u00df", "so", "durch", "ein\u00b7an\u00b7der"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "$,", "ADJD", "$,", "ADJD", "$,", "ADJD", "VVFIN", "ADV", "APPR", "PRF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "legen", "tokens": ["le\u00b7gen"], "token_info": ["word"], "pos": ["VVINF"], "meter": "+-", "measure": "trochaic.single"}, "line.15": {"text": "Da\u00df wir an ihrer Pracht, die Sch\u00f6nheit selbst er-", "tokens": ["Da\u00df", "wir", "an", "ih\u00b7rer", "Pracht", ",", "die", "Sch\u00f6n\u00b7heit", "selbst", "er"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "$,", "ART", "NN", "ADV", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "wegen;", "tokens": ["we\u00b7gen", ";"], "token_info": ["word", "punct"], "pos": ["APPR", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.17": {"text": "Die so viel Mischungen, die so viel Herrlichkeit,", "tokens": ["Die", "so", "viel", "Misc\u00b7hun\u00b7gen", ",", "die", "so", "viel", "Herr\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PIAT", "NN", "$,", "PRELS", "ADV", "PIAT", "NN", "$,"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.18": {"text": "Die unbeschreiblich ist, in unsre Augen streut.", "tokens": ["Die", "un\u00b7be\u00b7schreib\u00b7lich", "ist", ",", "in", "uns\u00b7re", "Au\u00b7gen", "streut", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VAFIN", "$,", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Wenn wir dies alles sehn; so werden wir ger\u00fchret", "tokens": ["Wenn", "wir", "dies", "al\u00b7les", "sehn", ";", "so", "wer\u00b7den", "wir", "ge\u00b7r\u00fch\u00b7ret"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PDS", "PIS", "VVINF", "$.", "ADV", "VAFIN", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Von Freudigkeit entz\u00fckt, zum Sch\u00f6pfer hinge-", "tokens": ["Von", "Freu\u00b7dig\u00b7keit", "ent\u00b7z\u00fckt", ",", "zum", "Sch\u00f6p\u00b7fer", "hin\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "VVPP", "$,", "APPRART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "f\u00fchret.", "tokens": ["f\u00fch\u00b7ret", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.22": {"text": "Sein Glanz der spiegelt sich, in jeder Kreatur,", "tokens": ["Sein", "Glanz", "der", "spie\u00b7gelt", "sich", ",", "in", "je\u00b7der", "Kre\u00b7a\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "VVFIN", "PRF", "$,", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Und seine Herrligkeit im Reiche der Natur,", "tokens": ["Und", "sei\u00b7ne", "Herr\u00b7lig\u00b7keit", "im", "Rei\u00b7che", "der", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPRART", "NE", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Mein ", "tokens": ["Mein"], "token_info": ["word"], "pos": ["PPOSAT"], "meter": "-", "measure": "single.down"}, "line.25": {"text": "mels Auen,", "tokens": ["mels", "Au\u00b7en", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.26": {"text": "Da du selbst Sonne bist, vor Glanz und Sch\u00f6nheit", "tokens": ["Da", "du", "selbst", "Son\u00b7ne", "bist", ",", "vor", "Glanz", "und", "Sch\u00f6n\u00b7heit"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "NN", "VAFIN", "$,", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "schauen,", "tokens": ["schau\u00b7en", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.28": {"text": "So dacht ich, als mein Herz bei einer stillen Nacht,", "tokens": ["So", "dacht", "ich", ",", "als", "mein", "Herz", "bei", "ei\u00b7ner", "stil\u00b7len", "Nacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "PPOSAT", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Sich von der Tags-Arbeit und Sorgen frei gemacht,", "tokens": ["Sich", "von", "der", "Tags\u00b7A\u00b7rbeit", "und", "Sor\u00b7gen", "frei", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "KON", "NN", "ADJD", "VVPP", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.30": {"text": "Und dies ermunterte mich zur vergn\u00fcgten Freude,", "tokens": ["Und", "dies", "er\u00b7mun\u00b7ter\u00b7te", "mich", "zur", "ver\u00b7gn\u00fcg\u00b7ten", "Freu\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "PRF", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.31": {"text": "Ich fand auch bei der Nacht die sch\u00f6nste Augen-Weide.", "tokens": ["Ich", "fand", "auch", "bei", "der", "Nacht", "die", "sch\u00f6ns\u00b7te", "Au\u00b7gen\u00b7Wei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Der Himmel ward recht hell; ich sah denselben an,", "tokens": ["Der", "Him\u00b7mel", "ward", "recht", "hell", ";", "ich", "sah", "den\u00b7sel\u00b7ben", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "$.", "PPER", "VVFIN", "PDS", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Das schlummernd Augenpaar, ward wieder aufge-", "tokens": ["Das", "schlum\u00b7mernd", "Au\u00b7gen\u00b7paar", ",", "ward", "wie\u00b7der", "auf\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "ADJD", "NN", "$,", "VAFIN", "ADV", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.34": {"text": "than,", "tokens": ["than", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.35": {"text": "So bald sie merketen, da\u00df die Saphirnen Bogen", "tokens": ["So", "bald", "sie", "mer\u00b7ke\u00b7ten", ",", "da\u00df", "die", "Sa\u00b7phir\u00b7nen", "Bo\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PPER", "VVFIN", "$,", "KOUS", "ART", "ADJA", "NN"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.36": {"text": "Des klaren Fiemaments, mit sch\u00f6nsten Schmuk", "tokens": ["Des", "kla\u00b7ren", "Fie\u00b7ma\u00b7ments", ",", "mit", "sch\u00f6ns\u00b7ten", "Schmuk"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.37": {"text": "bezogen.", "tokens": ["be\u00b7zo\u00b7gen", "."], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.38": {"text": "Mein ", "tokens": ["Mein"], "token_info": ["word"], "pos": ["PPOSAT"], "meter": "-", "measure": "single.down"}, "line.39": {"text": "daran gewahr,", "tokens": ["da\u00b7ran", "ge\u00b7wahr", ","], "token_info": ["word", "word", "punct"], "pos": ["PAV", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.40": {"text": "Es stellte sich zuerst ein hell Gew\u00f6lke dar,", "tokens": ["Es", "stell\u00b7te", "sich", "zu\u00b7erst", "ein", "hell", "Ge\u00b7w\u00f6l\u00b7ke", "dar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "ART", "ADJD", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Wo blau mit Golde strahlt, und an der andern", "tokens": ["Wo", "blau", "mit", "Gol\u00b7de", "strahlt", ",", "und", "an", "der", "an\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "APPR", "NN", "VVFIN", "$,", "KON", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.42": {"text": "Seiten,", "tokens": ["Sei\u00b7ten", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.43": {"text": "Wie\u00df sich ein Silberschein, aus grauen Dunkel-", "tokens": ["Wie\u00df", "sich", "ein", "Sil\u00b7ber\u00b7schein", ",", "aus", "grau\u00b7en", "Dun\u00b7kel"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PRF", "ART", "NN", "$,", "APPR", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.44": {"text": "heiten.", "tokens": ["hei\u00b7ten", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.45": {"text": "Die Wolke flog hinweg, mein ganz verschlungner", "tokens": ["Die", "Wol\u00b7ke", "flog", "hin\u00b7weg", ",", "mein", "ganz", "ver\u00b7schlung\u00b7ner"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,", "PPOSAT", "ADV", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.46": {"text": "Sin,", "tokens": ["Sin", ","], "token_info": ["word", "punct"], "pos": ["NE", "$,"], "meter": "+", "measure": "single.up"}, "line.47": {"text": "Fand immer neue Pracht, wo ich nur sahe hin,", "tokens": ["Fand", "im\u00b7mer", "neu\u00b7e", "Pracht", ",", "wo", "ich", "nur", "sa\u00b7he", "hin", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJA", "NN", "$,", "PWAV", "PPER", "ADV", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Ein sch\u00f6nes Himmelblau, mit Silber untermenget,", "tokens": ["Ein", "sch\u00f6\u00b7nes", "Him\u00b7mel\u00b7blau", ",", "mit", "Sil\u00b7ber", "un\u00b7ter\u00b7men\u00b7get", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.49": {"text": "Ein weisser Attlas Schein, mit sanften Gr\u00fcn ge-", "tokens": ["Ein", "weis\u00b7ser", "Att\u00b7las", "Schein", ",", "mit", "sanf\u00b7ten", "Gr\u00fcn", "ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN", "$,", "APPR", "ADJA", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.50": {"text": "sprenget;", "tokens": ["spren\u00b7get", ";"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.51": {"text": "Ein untermischtes Gelb, war in der Wolken Kleid,", "tokens": ["Ein", "un\u00b7ter\u00b7mischtes", "Gelb", ",", "war", "in", "der", "Wol\u00b7ken", "Kleid", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "VAFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.52": {"text": "Recht wunderbar versezt, und lieblich ausgestreut.", "tokens": ["Recht", "wun\u00b7der\u00b7bar", "ver\u00b7sezt", ",", "und", "lieb\u00b7lich", "aus\u00b7ge\u00b7streut", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VVPP", "$,", "KON", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.53": {"text": "Bald deuchte mir, da\u00df ich nichts als Drap d\u2019or er-", "tokens": ["Bald", "deuch\u00b7te", "mir", ",", "da\u00df", "ich", "nichts", "als", "Drap", "d'\u00b7or", "er"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "PIS", "KOKOM", "NE", "NE", "TRUNC"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.54": {"text": "blikket,", "tokens": ["blik\u00b7ket", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.55": {"text": "Bald ward mir dessen Glanz, aus dem Gesicht ent-", "toke