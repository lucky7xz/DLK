{"textgrid.poem.60413": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "1L: \u203aschon weht es k\u00fchler auf Erden;", "genre": "verse", "period": "N.A.", "pub_year": 1839, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u203aschon weht es k\u00fchler auf Erden;", "tokens": ["\u203a", "schon", "weht", "es", "k\u00fch\u00b7ler", "auf", "Er\u00b7den", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "ADJD", "APPR", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Es m\u00f6chte Abend werden,", "tokens": ["Es", "m\u00f6ch\u00b7te", "A\u00b7bend", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "NN", "VAINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Es m\u00f6chte werden Nacht,", "tokens": ["Es", "m\u00f6ch\u00b7te", "wer\u00b7den", "Nacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VAFIN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Bevor durchrungen die Schlacht,", "tokens": ["Be\u00b7vor", "durch\u00b7run\u00b7gen", "die", "Schlacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Der Menschheit altes Gefecht", "tokens": ["Der", "Menschheit", "al\u00b7tes", "Ge\u00b7fecht"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Um Freiheit, Licht und Recht.", "tokens": ["Um", "Frei\u00b7heit", ",", "Licht", "und", "Recht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ich reiche beiden Heeren", "tokens": ["Ich", "rei\u00b7che", "bei\u00b7den", "Hee\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Beschleunigend Waffen und Wehren,", "tokens": ["Be\u00b7schleu\u00b7ni\u00b7gend", "Waf\u00b7fen", "und", "Weh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "NN", "KON", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.9": {"text": "Es soll ihr Letztes wagen", "tokens": ["Es", "soll", "ihr", "Letz\u00b7tes", "wa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "PIS", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Die H\u00f6ll und werden erschlagen;", "tokens": ["Die", "H\u00f6ll", "und", "wer\u00b7den", "er\u00b7schla\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "VAFIN", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.11": {"text": "Da\u00df noch ein St\u00fcndlein Frieden", "tokens": ["Da\u00df", "noch", "ein", "St\u00fcnd\u00b7lein", "Frie\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Der Menschheit sei beschieden.\u2039", "tokens": ["Der", "Menschheit", "sei", "be\u00b7schie\u00b7den", ".", "\u2039"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$.", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "So dachte der Genius, der die Menschheit f\u00fchrt,", "tokens": ["So", "dach\u00b7te", "der", "Ge\u00b7nius", ",", "der", "die", "Menschheit", "f\u00fchrt", ","], "token_info": ["