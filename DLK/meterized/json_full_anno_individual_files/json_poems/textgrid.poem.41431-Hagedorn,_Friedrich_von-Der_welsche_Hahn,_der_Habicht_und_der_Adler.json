{"textgrid.poem.41431": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Der welsche Hahn, der Habicht und der Adler", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Man diene, wem man kann, doch nicht um reich zu werden.", "tokens": ["Man", "die\u00b7ne", ",", "wem", "man", "kann", ",", "doch", "nicht", "um", "reich", "zu", "wer\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "PWS", "PIS", "VMFIN", "$,", "ADV", "PTKNEG", "APPR", "ADJD", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Denn nichts ist k\u00e4rglicher, als die Erkenntlichkeit.", "tokens": ["Denn", "nichts", "ist", "k\u00e4rg\u00b7li\u00b7cher", ",", "als", "die", "Er\u00b7kennt\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "ADJD", "$,", "KOUS", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Es ging ein welscher Hahn, in stolzer Sicherheit,", "tokens": ["Es", "ging", "ein", "wel\u00b7scher", "Hahn", ",", "in", "stol\u00b7zer", "Si\u00b7cher\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Aus seinem Hof ins Feld, und musterte die Heerden.", "tokens": ["Aus", "sei\u00b7nem", "Hof", "ins", "Feld", ",", "und", "mus\u00b7ter\u00b7te", "die", "Heer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPRART", "NN", "$,", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ein Habicht, welchem nur der Adler schrecklich war,", "tokens": ["Ein", "Ha\u00b7bicht", ",", "wel\u00b7chem", "nur", "der", "Ad\u00b7ler", "schreck\u00b7lich", "war", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "ART", "NN", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "An F\u00e4ngen stark, schlau wie ein Hasengeier,", "tokens": ["An", "F\u00e4n\u00b7gen", "stark", ",", "schlau", "wie", "ein", "Ha\u00b7sen\u00b7gei\u00b7er", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "$,", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "Scho\u00df auf den Hahn herab, und, durch ein Abenteuer,", "tokens": ["Scho\u00df", "auf", "den", "Hahn", "her\u00b7ab", ",", "und", ",", "durch", "ein", "A\u00b7bent\u00b7eu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ADV", "$,", "KON", "$,", "APPR", "ART", "NN", "$,"], "meter": "++-+-+-+-+-+-", "measure": "unknown.measure.septa"}, "line.8": {"text": "Entri\u00df ein Adler ihn der pl\u00f6tzlichen Gefahr.", "tokens": ["Ent\u00b7ri\u00df", "ein", "Ad\u00b7ler", "ihn", "der", "pl\u00f6tz\u00b7li\u00b7chen", "Ge\u00b7fahr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Damit ich, sprach der Hahn, nicht dankvergessen scheine,", "tokens": ["Da\u00b7mit", "ich", ",", "sprach", "der", "Hahn", ",", "nicht", "dank\u00b7ver\u00b7ges\u00b7sen", "schei\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "VVFIN", "ART", "NN", "$,", "PTKNEG", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Sing' ich dein Lob: ich singe meisterlich.", "tokens": ["Sing'", "ich", "dein", "Lob", ":", "ich", "sin\u00b7ge", "meis\u00b7ter\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "$.", "PPER", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Auch hab' ich ein Geschenk f\u00fcr dich.", "tokens": ["Auch", "hab'", "ich", "ein", "Ge\u00b7schenk", "f\u00fcr", "dich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Ich gebe gern. Was? Meiner Federn eine.", "tokens": ["Ich", "ge\u00b7be", "gern", ".", "Was", "?", "Mei\u00b7ner", "Fe\u00b7dern", "ei\u00b7ne", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$.", "PWS", "$.", "PPOSAT", "NN", "ART", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Es drohte Spanien Alphonsens Thron den Fall,", "tokens": ["Es", "droh\u00b7te", "Spa\u00b7ni\u00b7en", "Al\u00b7phon\u00b7sens", "Thron", "den", "Fall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "NE", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch Englands zweiter Carl besch\u00fctzte Portugal.", "tokens": ["Doch", "En\u00b7glands", "zwei\u00b7ter", "Carl", "be\u00b7sch\u00fctz\u00b7te", "Por\u00b7tu\u00b7gal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "ADJA", "NE", "VVFIN", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "F\u00fcr den zu schwachen K\u00f6nig stritten", "tokens": ["F\u00fcr", "den", "zu", "schwa\u00b7chen", "K\u00f6\u00b7nig", "strit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die unerschrocknen freien Britten,", "tokens": ["Die", "un\u00b7er\u00b7schrock\u00b7nen", "frei\u00b7en", "Brit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und siegten, so wie sonst, auch bei Amexial.", "tokens": ["Und", "sieg\u00b7ten", ",", "so", "wie", "sonst", ",", "auch", "bei", "A\u00b7me\u00b7xi\u00b7al", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ADV", "KOKOM", "ADV", "$,", "ADV", "APPR", "NE", "$."], "meter": "-+---+--+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Alphonsus lobt den Heldenmuth der Schaaren,", "tokens": ["Al\u00b7phon\u00b7sus", "lobt", "den", "Hel\u00b7den\u00b7muth", "der", "Schaa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Durch deren Arm sein Reich bestund;", "tokens": ["Durch", "de\u00b7ren", "Arm", "sein", "Reich", "be\u00b7stund", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Doch macht er seinen Dank auch durch Geschenke kund.", "tokens": ["Doch", "macht", "er", "sei\u00b7nen", "Dank", "auch", "durch", "Ge\u00b7schen\u00b7ke", "kund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN", "ADV", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Die k\u00f6niglichen Gaben waren,", "tokens": ["Die", "k\u00f6\u00b7nig\u00b7li\u00b7chen", "Ga\u00b7ben", "wa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "F\u00fcr jede Compagnie, an Schnupftabak, drei Pfund.", "tokens": ["F\u00fcr", "je\u00b7de", "Com\u00b7pag\u00b7nie", ",", "an", "Schnupf\u00b7ta\u00b7bak", ",", "drei", "Pfund", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "APPR", "NN", "$,", "CARD", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Man diene, wem man kann, doch nicht um reich zu werden.", "tokens": ["Man", "die\u00b7ne", ",", "wem", "man", "kann", ",", "doch", "nicht", "um", "reich", "zu", "wer\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "PWS", "PIS", "VMFIN", "$,", "ADV", "PTKNEG", "APPR", "ADJD", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Denn nichts ist k\u00e4rglicher, als die Erkenntlichkeit.", "tokens": ["Denn", "nichts", "ist", "k\u00e4rg\u00b7li\u00b7cher", ",", "als", "die", "Er\u00b7kennt\u00b7lich\u00b7keit", "."], "token_info": ["wor