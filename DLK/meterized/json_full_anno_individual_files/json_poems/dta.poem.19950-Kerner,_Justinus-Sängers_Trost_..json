{"dta.poem.19950": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "S\u00e4ngers Trost .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905193111", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Weint auch einst kein Liebchen", "tokens": ["Weint", "auch", "einst", "kein", "Lieb\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ADV", "PIAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Thr\u00e4nen auf mein Grab,", "tokens": ["Thr\u00e4\u00b7nen", "auf", "mein", "Grab", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Tr\u00e4ufeln doch die Blumen", "tokens": ["Tr\u00e4u\u00b7feln", "doch", "die", "Blu\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADV", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Milden Thau hinab.", "tokens": ["Mil\u00b7den", "Thau", "hin\u00b7ab", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Weilt an ihm kein Wandrer", "tokens": ["Weilt", "an", "ihm", "kein", "Wand\u00b7rer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PPER", "PIAT", "NN"], "meter": "+---+-", "measure": "dactylic.init"}, "line.2": {"text": "Im Vor\u00fcberzieh'n,", "tokens": ["Im", "Vor\u00b7\u00fcber\u00b7zieh'n", ","], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Blickt auf seiner Reise", "tokens": ["Blickt", "auf", "sei\u00b7ner", "Rei\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Doch der Mond auf ihn.", "tokens": ["Doch", "der", "Mond", "auf", "ihn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "PPER", "$."], "meter": "--+-+", "measure": "anapaest.init"}}, "stanza.3": {"line.1": {"text": "Denkt auf diesen Fluren", "tokens": ["Denkt", "auf", "die\u00b7sen", "Flu\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PDAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Bald kein Erdner mein,", "tokens": ["Bald", "kein", "Erd\u00b7ner", "mein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "PPOSAT", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Denkt doch mein die Aue", "tokens": ["Denkt", "doch", "mein", "die", "Au\u00b7e"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "PPOSAT", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Und der stille Hain.", "tokens": ["Und", "der", "stil\u00b7le", "Hain", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Blumen, Hain und Aue,", "tokens": ["Blu\u00b7men", ",", "Hain", "und", "Au\u00b7e", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Stern und Mondenlicht,", "tokens": ["Stern", "und", "Mon\u00b7den\u00b7licht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Die ich sang, vergessen", "tokens": ["Die", "ich", "sang", ",", "ver\u00b7ges\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ART", "PPER", "VVFIN", "$,", "VVPP"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Ihres S\u00e4ngers nicht.", "tokens": ["Ih\u00b7res", "S\u00e4n\u00b7gers", "nicht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKNEG", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}