{"textgrid.poem.24025": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Andacht zu meiner Welt", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In einem Haus, versteckt in Linden,", "tokens": ["In", "ei\u00b7nem", "Haus", ",", "ver\u00b7steckt", "in", "Lin\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "VVFIN", "APPR", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Durch die das Licht gr\u00fcngolden f\u00e4llt,", "tokens": ["Durch", "die", "das", "Licht", "gr\u00fcn\u00b7gol\u00b7den", "f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ART", "NN", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Will ich vergehen und verschwinden", "tokens": ["Will", "ich", "ver\u00b7ge\u00b7hen", "und", "ver\u00b7schwin\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "VVINF", "KON", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Vor dem Gel\u00e4rme dieser Welt.", "tokens": ["Vor", "dem", "Ge\u00b7l\u00e4r\u00b7me", "die\u00b7ser", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PDAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Aus dieser Welt der Wutgrimassen,", "tokens": ["Aus", "die\u00b7ser", "Welt", "der", "Wut\u00b7gri\u00b7mas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In der der Zaum die Bl\u00f6den h\u00e4lt,", "tokens": ["In", "der", "der", "Zaum", "die", "Bl\u00f6\u00b7den", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Im Lieben dumpf und flau im Hassen,", "tokens": ["Im", "Lie\u00b7ben", "dumpf", "und", "flau", "im", "Has\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "ADJD", "KON", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Aus dieser lauten, leeren Welt.", "tokens": ["Aus", "die\u00b7ser", "lau\u00b7ten", ",", "lee\u00b7ren", "Welt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ich will mir eine andre bauen,", "tokens": ["Ich", "will", "mir", "ei\u00b7ne", "and\u00b7re", "bau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ART", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von innerlichem Licht erhellt,", "tokens": ["Von", "in\u00b7ner\u00b7li\u00b7chem", "Licht", "er\u00b7hellt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und will in Andacht still beschauen", "tokens": ["Und", "will", "in", "An\u00b7dacht", "still", "be\u00b7schau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "APPR", "NN", "ADJD", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die von mir selber ist, die Welt.", "tokens": ["Die", "von", "mir", "sel\u00b7ber", "ist", ",", "die"