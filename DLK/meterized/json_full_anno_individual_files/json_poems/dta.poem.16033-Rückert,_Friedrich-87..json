{"dta.poem.16033": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "87.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Warum das gro\u00dfe Ich der Menschheit sich gespalten", "tokens": ["Wa\u00b7rum", "das", "gro\u00b7\u00dfe", "Ich", "der", "Menschheit", "sich", "ge\u00b7spal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "PPER", "ART", "NN", "PRF", "VVPP"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "In viele kleine, die uns auseinander halten?", "tokens": ["In", "vie\u00b7le", "klei\u00b7ne", ",", "die", "uns", "aus\u00b7ein\u00b7an\u00b7der", "hal\u00b7ten", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "$,", "PRELS", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Da\u00df auseinander sie uns halten, statt zusammen,", "tokens": ["Da\u00df", "aus\u00b7ein\u00b7an\u00b7der", "sie", "uns", "hal\u00b7ten", ",", "statt", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPER", "PRF", "VVINF", "$,", "KOUI", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ist Schuld der Einzelnen, die aus dem Einen stammen;", "tokens": ["Ist", "Schuld", "der", "Ein\u00b7zel\u00b7nen", ",", "die", "aus", "dem", "Ei\u00b7nen", "stam\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ART", "NN", "$,", "PRELS", "APPR", "ART", "ART", "ADJA", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.3": {"line.1": {"text": "Da\u00df sie in Einzelheit die Einheit nicht beh\u00fcten,", "tokens": ["Da\u00df", "sie", "in", "Ein\u00b7zel\u00b7heit", "die", "Ein\u00b7heit", "nicht", "be\u00b7h\u00fc\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie einen Bl\u00fctenbaum ausmachen alle Bl\u00fcten:", "tokens": ["Wie", "ei\u00b7nen", "Bl\u00fc\u00b7ten\u00b7baum", "aus\u00b7ma\u00b7chen", "al\u00b7le", "Bl\u00fc\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "So sollten, ohne da\u00df sie ineinander schwammen,", "tokens": ["So", "soll\u00b7ten", ",", "oh\u00b7ne", "da\u00df", "sie", "in\u00b7ein\u00b7an\u00b7der", "schwam\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "$,", "KOUI", "KOUS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Eine Glut beseelt, auch ineinander stammen;", "tokens": ["Die", "Ei\u00b7ne", "Glut", "be\u00b7seelt", ",", "auch", "in\u00b7ein\u00b7an\u00b7der", "stam\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVPP", "$,", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Ein Baum der Weltvernunft, verzweigt in seine Ranken,", "tokens": ["Ein", "Baum", "der", "Welt\u00b7ver\u00b7nunft", ",", "ver\u00b7zweigt", "in", "sei\u00b7ne", "Ran\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sich denkend Eines Geists eintr\u00e4chtige Gedanken;", "tokens": ["Sich", "den\u00b7kend", "Ei\u00b7nes", "Geists", "ein\u00b7tr\u00e4ch\u00b7ti\u00b7ge", "Ge\u00b7dan\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Wo jeder g\u00f6ttliche Gedanke w\u00e4r' ein Glanz", "tokens": ["Wo", "je\u00b7der", "g\u00f6tt\u00b7li\u00b7che", "Ge\u00b7dan\u00b7ke", "w\u00e4r'", "ein", "Glanz"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PIAT", "ADJA", "NN", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "F\u00fcr sich, doch erst ein Licht zusammen alle ganz.", "tokens": ["F\u00fcr", "sich", ",", "doch", "erst", "ein", "Licht", "zu\u00b7sam\u00b7men", "al\u00b7le", "ganz", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "$,", "ADV", "ADV", "ART", "NN", "ADV", "PIS", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Ann\u00e4herung dazu ist jedes Geistes Macht,", "tokens": ["An\u00b7n\u00e4\u00b7he\u00b7rung", "da\u00b7zu", "ist", "je\u00b7des", "Geis\u00b7tes", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PAV", "VAFIN", "PIAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der alles denket nach, was andre vorgedacht,", "tokens": ["Der", "al\u00b7les", "den\u00b7ket", "nach", ",", "was", "and\u00b7re", "vor\u00b7ge\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "PTKVZ", "$,", "PRELS", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Der selber denket vor, was nach ihm fort sich denkt,", "tokens": ["Der", "sel\u00b7ber", "den\u00b7ket", "vor", ",", "was", "nach", "ihm", "fort", "sich", "denkt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "PTKVZ", "$,", "PRELS", "APPR", "PPER", "PTKVZ", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In jede Denkform sich, und jed' in sich versenkt.", "tokens": ["In", "je\u00b7de", "Denk\u00b7form", "sich", ",", "und", "jed'", "in", "sich", "ver\u00b7senkt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PRF", "$,", "KON", "PIS", "APPR", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Vorahnend l\u00f6st sein Geist der Geister Widerspruch,", "tokens": ["Vor\u00b7ah\u00b7nend", "l\u00f6st", "sein", "Geist", "der", "Geis\u00b7ter", "Wi\u00b7der\u00b7spruch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPOSAT", "NN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie Fr\u00fchling Wald und Feld in Einen Wohlgeruch.", "tokens": ["Wie", "Fr\u00fch\u00b7ling", "Wald", "und", "Feld", "in", "Ei\u00b7nen", "Wohl\u00b7ge\u00b7ruch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "NN", "KON", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}