{"dta.poem.21047": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "An Dieselbe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1798", "urn": "urn:nbn:de:kobv:b4-200905193300", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Rosa, denkst du an mich? Innig gedenk' ich dein.               ", "tokens": ["Ro\u00b7sa", ",", "denkst", "du", "an", "mich", "?", "In\u00b7nig", "ge\u00b7denk'", "ich", "dein", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PPER", "APPR", "PPER", "$.", "NN", "VVFIN", "PPER", "PPOSAT", "$."], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Durch den gr\u00fcnlichen Wald schimmert das Abend-", "tokens": ["Durch", "den", "gr\u00fcn\u00b7li\u00b7chen", "Wald", "schim\u00b7mert", "das", "A\u00b7ben\u00b7d"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "ART", "TRUNC"], "meter": "+-+--++--+--", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "roth,", "tokens": ["roth", ","], "token_info": ["word", "punct"], "pos": ["ADJD", "$,"], "meter": "-", "measure": "single.down"}, "line.4": {"text": "Und die Wipfel der Tannen", "tokens": ["Und", "die", "Wip\u00b7fel", "der", "Tan\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ART", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "Regt das S\u00e4useln des Ewigen.", "tokens": ["Regt", "das", "S\u00e4u\u00b7seln", "des", "E\u00b7wi\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "ADJA", "$."], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Rosa, w\u00e4rest du hier, s\u00e4h' ich ins Abendroth", "tokens": ["Ro\u00b7sa", ",", "w\u00e4\u00b7rest", "du", "hier", ",", "s\u00e4h'", "ich", "ins", "A\u00b7ben\u00b7droth"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "VAFIN", "PPER", "ADV", "$,", "VVFIN", "PPER", "APPRART", "NN"], "meter": "+--+-+++-+-+", "measure": "iambic.septa.invert"}, "line.2": {"text": "Deine Wangen getaucht, s\u00e4h' ich vom Abendhauch", "tokens": ["Dei\u00b7ne", "Wan\u00b7gen", "ge\u00b7taucht", ",", "s\u00e4h'", "ich", "vom", "A\u00b7bend\u00b7hauch"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVPP", "$,", "VVFIN", "PPER", "APPRART", "NN"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Deine Locken geringelt \u2014", "tokens": ["Dei\u00b7ne", "Lo\u00b7cken", "ge\u00b7rin\u00b7gelt"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$("], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Edle Seele, mir w\u00e4re wohl!", "tokens": ["Ed\u00b7le", "See\u00b7le", ",", "mir", "w\u00e4\u00b7re", "wohl", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPER", "VAFIN", "ADV", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.3": {"line.1": {"text": "Lieber lehn' ich an dir, als an der Einsamkeit", "tokens": ["Lie\u00b7ber", "lehn'", "ich", "an", "dir", ",", "als", "an", "der", "Ein\u00b7sam\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "PPER", "$,", "KOUS", "APPR", "ART", "NN"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Trautem Busen. Mir klingt s\u00fcsser der Fl\u00f6tenton", "tokens": ["Trau\u00b7tem", "Bu\u00b7sen", ".", "Mir", "klingt", "s\u00fcs\u00b7ser", "der", "Fl\u00f6\u00b7ten\u00b7ton"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "PPER", "VVFIN", "ADJD", "ART", "NN"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Deiner klagenden Stimme,", "tokens": ["Dei\u00b7ner", "kla\u00b7gen\u00b7den", "Stim\u00b7me", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Als das S\u00e4useln im Tannenhayn.", "tokens": ["Als", "das", "S\u00e4u\u00b7seln", "im", "Tan\u00b7nen\u00b7hayn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.4": {"line.1": {"text": "Oft umfingest du mich, meine Holdselige,", "tokens": ["Oft", "um\u00b7fin\u00b7gest", "du", "mich", ",", "mei\u00b7ne", "Hold\u00b7se\u00b7li\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Mit vertraulichem Arm, wenn ich an deiner Brust", "tokens": ["Mit", "ver\u00b7trau\u00b7li\u00b7chem", "Arm", ",", "wenn", "ich", "an", "dei\u00b7ner", "Brust"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "KOUS", "PPER", "APPR", "PPOSAT", "NN"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Melancholischen Frieden,", "tokens": ["Me\u00b7lan\u00b7cho\u00b7li\u00b7schen", "Frie\u00b7den", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+---+-", "measure": "dactylic.init"}, "line.4": {"text": "Schw\u00e4rmens m\u00fcde, mich rettete.", "tokens": ["Schw\u00e4r\u00b7mens", "m\u00fc\u00b7de", ",", "mich", "ret\u00b7te\u00b7te", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "PPER", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.5": {"line.1": {"text": "Jedes leisere Weh, jedes verschwiegne Ach,", "tokens": ["Je\u00b7des", "lei\u00b7se\u00b7re", "Weh", ",", "je\u00b7des", "ver\u00b7schwieg\u00b7ne", "Ach", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,", "PIAT", "ADJA", "NN", "$,"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.2": {"text": "Das den Busen mir presst, haucht' ich dir \u00f6fter aus,", "tokens": ["Das", "den", "Bu\u00b7sen", "mir", "presst", ",", "haucht'", "ich", "dir", "\u00f6f\u00b7ter", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "PPER", "ADV", "PTKVZ", "$,"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Sch\u00f6pfte freyeren Odem,", "tokens": ["Sch\u00f6pf\u00b7te", "frey\u00b7e\u00b7ren", "O\u00b7dem", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Klomm' heroischer felsenan.", "tokens": ["Klomm'", "he\u00b7ro\u00b7i\u00b7scher", "fel\u00b7se\u00b7nan", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJD", "PTKVZ", "$."], "meter": "+----+-+", "measure": "dactylic.init"}}, "stanza.6": {"line.1": {"text": "Nie soll darum ein Freund meiner holdseligen", "tokens": ["Nie", "soll", "da\u00b7rum", "ein", "Freund", "mei\u00b7ner", "hold\u00b7se\u00b7li\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PAV", "ART", "NN", "PPOSAT", "NN"], "meter": "+--+-+--+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Rosa mangeln, und nie Milderung ihrem Gram!", "tokens": ["Ro\u00b7sa", "man\u00b7geln", ",", "und", "nie", "Mil\u00b7de\u00b7rung", "ih\u00b7rem", "Gram", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVINF", "$,", "KON", "ADV", "NN", "PPOSAT", "NN", "$."], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.3": {"text": "Nie sey trostlos ihr Leiden,", "tokens": ["Nie", "sey", "trost\u00b7los", "ihr", "Lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADJD", "PPOSAT", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Ihre Urne nie blumenleer!", "tokens": ["Ih\u00b7re", "Ur\u00b7ne", "nie", "blu\u00b7men\u00b7leer", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "ADJD", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}}}}