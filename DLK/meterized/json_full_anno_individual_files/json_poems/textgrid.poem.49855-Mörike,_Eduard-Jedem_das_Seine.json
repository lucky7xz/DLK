{"textgrid.poem.49855": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Jedem das Seine", "genre": "verse", "period": "N.A.", "pub_year": 1861, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aninka tanzte", "tokens": ["A\u00b7nin\u00b7ka", "tanz\u00b7te"], "token_info": ["word", "word"], "pos": ["NE", "VVFIN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Vor uns im Grase", "tokens": ["Vor", "uns", "im", "Gra\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPER", "APPRART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Die raschen Weisen.", "tokens": ["Die", "ra\u00b7schen", "Wei\u00b7sen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Wie sch\u00f6n war sie!", "tokens": ["Wie", "sch\u00f6n", "war", "sie", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Mit den gesenkten,", "tokens": ["Mit", "den", "ge\u00b7senk\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Bescheidnen Augen", "tokens": ["Be\u00b7scheid\u00b7nen", "Au\u00b7gen"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Das stille M\u00e4dchen \u2013", "tokens": ["Das", "stil\u00b7le", "M\u00e4d\u00b7chen", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Mich macht' es toll!", "tokens": ["Mich", "macht'", "es", "toll", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Da sprang ein Kn\u00f6pfchen", "tokens": ["Da", "sprang", "ein", "Kn\u00f6pf\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Ihr von der Jacke,", "tokens": ["Ihr", "von", "der", "Ja\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Ein goldnes Kn\u00f6pfchen,", "tokens": ["Ein", "gold\u00b7nes", "Kn\u00f6pf\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Ich fing es auf \u2013", "tokens": ["Ich", "fing", "es", "auf", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "$("], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Und dachte Wunder", "tokens": ["Und", "dach\u00b7te", "Wun\u00b7der"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVFIN", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Was mir's bedeute,", "tokens": ["Was", "mir's", "be\u00b7deu\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "NE", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Doch h\u00e4misch l\u00e4chelt'", "tokens": ["Doch", "h\u00e4\u00b7misch", "l\u00e4\u00b7chelt'"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Jeg\u00f3r dazu,", "tokens": ["Jeg\u00f3r", "da\u00b7zu", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "PAV", "$,"], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.5": {"line.