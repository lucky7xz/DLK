{"textgrid.poem.43162": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ausgetobt die wilden St\u00fcrme,", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ausgetobt die wilden St\u00fcrme,", "tokens": ["Aus\u00b7ge\u00b7tobt", "die", "wil\u00b7den", "St\u00fcr\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Heiter, friedlich gl\u00e4nzt das Meer,", "tokens": ["Hei\u00b7ter", ",", "fried\u00b7lich", "gl\u00e4nzt", "das", "Meer", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nichts erinnert an die K\u00e4mpfe,", "tokens": ["Nichts", "e\u00b7rin\u00b7nert", "an", "die", "K\u00e4mp\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Todesseufzer bang und schwer. \u2013", "tokens": ["To\u00b7des\u00b7seuf\u00b7zer", "bang", "und", "schwer", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "ADJD", "KON", "ADJD", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Eine Kapsel, fest verschlossen", "tokens": ["Ei\u00b7ne", "Kap\u00b7sel", ",", "fest", "ver\u00b7schlos\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "ADJD", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schaukelt auf dem wei\u00dfen Schaum", "tokens": ["Schau\u00b7kelt", "auf", "dem", "wei\u00b7\u00dfen", "Schaum"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der Fischer, sorglos singend", "tokens": ["Und", "der", "Fi\u00b7scher", ",", "sorg\u00b7los", "sin\u00b7gend"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ART", "NN", "$,", "ADJD", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wirft sie in des Schiffleins Raum.", "tokens": ["Wirft", "sie", "in", "des", "Schif\u00b7fleins", "Raum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ist die Kapsel erst zerbrochen,", "tokens": ["Ist", "die", "Kap\u00b7sel", "erst", "zer\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Liest er von dem gelben Blatt \u2013", "tokens": ["Liest", "er", "von", "dem", "gel\u00b7ben", "Blatt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie viel Sch\u00e4tze, Gl\u00fcck und Leben", "tokens": ["Wie", "viel", "Sch\u00e4t\u00b7ze", ",", "Gl\u00fcck", "und", "Le\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "PIAT", "NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "J\u00fcngst das Meer verschlungen hat,", "tokens": ["J\u00fcngst", "das", "Meer", "ver\u00b7schlun\u00b7gen", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVPP", "VAFIN", "$,"], "me