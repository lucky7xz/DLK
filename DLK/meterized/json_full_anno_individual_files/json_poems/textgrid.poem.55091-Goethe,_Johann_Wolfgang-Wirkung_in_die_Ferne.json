{"textgrid.poem.55091": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Wirkung in die Ferne", "genre": "verse", "period": "N.A.", "pub_year": 1808, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die K\u00f6nigin steht im hohen Saal,", "tokens": ["Die", "K\u00f6\u00b7ni\u00b7gin", "steht", "im", "ho\u00b7hen", "Saal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da brennen der Kerzen so viele;", "tokens": ["Da", "bren\u00b7nen", "der", "Ker\u00b7zen", "so", "vie\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "PIS", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Sie spricht zum Pagen: \u00bbDu l\u00e4ufst einmal", "tokens": ["Sie", "spricht", "zum", "Pa\u00b7gen", ":", "\u00bb", "Du", "l\u00e4ufst", "ein\u00b7mal"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$.", "$(", "PPER", "VVFIN", "ADV"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und holst mir den Beutel zum Spiele.", "tokens": ["Und", "holst", "mir", "den", "Beu\u00b7tel", "zum", "Spie\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Er liegt zur Hand", "tokens": ["Er", "liegt", "zur", "Hand"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Auf meines Tisches Rand.\u00ab", "tokens": ["Auf", "mei\u00b7nes", "Ti\u00b7sches", "Rand", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Der Knabe, der eilt so behende,", "tokens": ["Der", "Kna\u00b7be", ",", "der", "eilt", "so", "be\u00b7hen\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "ADV", "ADJA", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.8": {"text": "War bald an Schlosses Ende.", "tokens": ["War", "bald", "an", "Schlos\u00b7ses", "En\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und neben der K\u00f6nigin schl\u00fcrft zur Stund", "tokens": ["Und", "ne\u00b7ben", "der", "K\u00f6\u00b7ni\u00b7gin", "schl\u00fcrft", "zur", "Stund"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "APPRART", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Sorbet die sch\u00f6nste der Frauen.", "tokens": ["Sor\u00b7bet", "die", "sch\u00f6ns\u00b7te", "der", "Frau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "ART", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Da brach ihr die Tasse so hart an dem Mund,", "tokens": ["Da", "brach", "ihr", "die", "Tas\u00b7se", "so", "hart", "an", "dem", "Mund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "ADV", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Es war ein Greuel zu schauen.", "tokens": ["Es", "war", "ein", "Greu\u00b7el", "zu", "schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Verlegenheit! Scham!", "tokens": ["Ver\u00b7le\u00b7gen\u00b7heit", "!", "Scham", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$.", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Ums Prachtkleid ist's getan!", "tokens": ["Ums", "Pracht\u00b7kleid", "ist's", "ge\u00b7tan", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Sie eilt und fliegt so behende", "tokens": ["Sie", "eilt", "und", "fliegt", "so", "be\u00b7hen\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ADV", "ADJA"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Entgegen des Schlosses Ende.", "tokens": ["Ent\u00b7ge\u00b7gen", "des", "Schlos\u00b7ses", "En\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Der Knabe zur\u00fcck zu laufen kam", "tokens": ["Der", "Kna\u00b7be", "zu\u00b7r\u00fcck", "zu", "lau\u00b7fen", "kam"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PTKVZ", "PTKZU", "VVINF", "VVFIN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Entgegen der Sch\u00f6nen in Schmerzen,", "tokens": ["Ent\u00b7ge\u00b7gen", "der", "Sch\u00f6\u00b7nen", "in", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Es wu\u00dft es niemand, doch beide zusamm',", "tokens": ["Es", "wu\u00dft", "es", "nie\u00b7mand", ",", "doch", "bei\u00b7de", "zu\u00b7samm'", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIS", "$,", "ADV", "PIS", "PTKVZ", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sie hegten einander im Herzen;", "tokens": ["Sie", "heg\u00b7ten", "ein\u00b7an\u00b7der", "im", "Her\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPRART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Und o des Gl\u00fccks,", "tokens": ["Und", "o", "des", "Gl\u00fccks", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "FM", "ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Des g\u00fcnst'gen Geschicks!", "tokens": ["Des", "g\u00fcnst'\u00b7gen", "Ge\u00b7schicks", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Sie warfen mit Brust sich zu Br\u00fcsten", "tokens": ["Sie", "war\u00b7fen", "mit", "Brust", "sich", "zu", "Br\u00fcs\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "PRF", "APPR", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.8": {"text": "Und herzten und k\u00fc\u00dften nach L\u00fcsten.", "tokens": ["Und", "herz\u00b7ten", "und", "k\u00fc\u00df\u00b7ten", "nach", "L\u00fcs\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "APPR", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.4": {"line.1": {"text": "Doch endlich beide sich rei\u00dfen los;", "tokens": ["Doch", "end\u00b7lich", "bei\u00b7de", "sich", "rei\u00b7\u00dfen", "los", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PIS", "PRF", "VVFIN", "PTKVZ", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sie eilt in ihre Gem\u00e4cher;", "tokens": ["Sie", "eilt", "in", "ih\u00b7re", "Ge\u00b7m\u00e4\u00b7cher", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Der Page dr\u00e4ngt sich zur K\u00f6nigin gro\u00df", "tokens": ["Der", "Pa\u00b7ge", "dr\u00e4ngt", "sich", "zur", "K\u00f6\u00b7ni\u00b7gin", "gro\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPRART", "NN", "ADJD"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Durch alle die Degen und F\u00e4cher.", "tokens": ["Durch", "al\u00b7le", "die", "De\u00b7gen", "und", "F\u00e4\u00b7cher", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "ART", "NN", "KON", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Die F\u00fcrstin entdeckt", "tokens": ["Die", "F\u00fcrs\u00b7tin", "ent\u00b7deckt"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVPP"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Das Westchen befleckt:", "tokens": ["Das", "West\u00b7chen", "be\u00b7fleckt", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "F\u00fcr sie war nichts unerreichbar,", "tokens": ["F\u00fcr", "sie", "war", "nichts", "un\u00b7er\u00b7reich\u00b7bar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "PIS", "ADJD", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Der K\u00f6nigin von Saba vergleichbar.", "tokens": ["Der", "K\u00f6\u00b7ni\u00b7gin", "von", "Sa\u00b7ba", "ver\u00b7gleich\u00b7bar", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "ADJD", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Und sie die Hofmeisterin rufen l\u00e4\u00dft:", "tokens": ["Und", "sie", "die", "Hof\u00b7meis\u00b7te\u00b7rin", "ru\u00b7fen", "l\u00e4\u00dft", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "NN", "VVINF", "VVFIN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "\u00bbwir kamen doch neulich zu Streite,", "tokens": ["\u00bb", "wir", "ka\u00b7men", "doch", "neu\u00b7lich", "zu", "Strei\u00b7te", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "ADV", "ADV", "APPR", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Und Ihr behauptetet steif und fest,", "tokens": ["Und", "Ihr", "be\u00b7haup\u00b7te\u00b7tet", "steif", "und", "fest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADJD", "KON", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Nicht reiche der Geist in die Weite;", "tokens": ["Nicht", "rei\u00b7che", "der", "Geist", "in", "die", "Wei\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Die Gegenwart nur,", "tokens": ["Die", "Ge\u00b7gen\u00b7wart", "nur", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Die lasse wohl Spur;", "tokens": ["Die", "las\u00b7se", "wohl", "Spur", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Doch niemand wirk in die Ferne,", "tokens": ["Doch", "nie\u00b7mand", "wirk", "in", "die", "Fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Sogar nicht die himmlischen Sterne.", "tokens": ["So\u00b7gar", "nicht", "die", "himm\u00b7li\u00b7schen", "Ster\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.6": {"line.1": {"text": "Nun seht! Soeben ward mir zur Seit", "tokens": ["Nun", "seht", "!", "Soe\u00b7ben", "ward", "mir", "zur", "Seit"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$.", "NN", "VAFIN", "PPER", "APPRART", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Der geistige S\u00fc\u00dftrank versch\u00fcttet,", "tokens": ["Der", "geis\u00b7ti\u00b7ge", "S\u00fc\u00df\u00b7trank", "ver\u00b7sch\u00fct\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Und gleich darauf hat er dort hinten so weit", "tokens": ["Und", "gleich", "da\u00b7rauf", "hat", "er", "dort", "hin\u00b7ten", "so", "weit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "PAV", "VAFIN", "PPER", "ADV", "ADV", "ADV", "ADJD"], "meter": "-+-+---+--+", "measu