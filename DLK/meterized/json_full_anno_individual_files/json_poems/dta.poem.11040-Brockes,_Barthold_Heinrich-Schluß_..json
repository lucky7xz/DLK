{"dta.poem.11040": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Schlu\u00df .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Und hiemit will ich denn zugleich mein Neujahrslied vor die\u00df-\nmal enden:", "tokens": ["Und", "hie\u00b7mit", "will", "ich", "denn", "zu\u00b7gleich", "mein", "Neu\u00b7jahrs\u00b7lied", "vor", "die\u00df", "mal", "en\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "VMFIN", "PPER", "ADV", "ADV", "PPOSAT", "NN", "APPR", "TRUNC", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.2": {"text": "Jedoch vorher noch die\u00df Gebeth zum Herrn der Jahr und Zei-", "tokens": ["Je\u00b7doch", "vor\u00b7her", "noch", "die\u00df", "Ge\u00b7beth", "zum", "Herrn", "der", "Jahr", "und", "Zei"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADV", "PDS", "NN", "APPRART", "NN", "ART", "NN", "KON", "TRUNC"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "O Gott! durch dessen Gnad und Huld ich einige von meinen", "tokens": ["O", "Gott", "!", "durch", "des\u00b7sen", "Gnad", "und", "Huld", "ich", "ei\u00b7ni\u00b7ge", "von", "mei\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "$.", "APPR", "PRELAT", "NN", "KON", "NN", "PPER", "PIS", "APPR", "PPOSAT"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.4": {"text": "Jm abgewichnen Jahre zwar geschickt gewesen, zu verrichten;", "tokens": ["Jm", "ab\u00b7ge\u00b7wich\u00b7nen", "Jah\u00b7re", "zwar", "ge\u00b7schickt", "ge\u00b7we\u00b7sen", ",", "zu", "ver\u00b7rich\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ADV", "VVPP", "VAPP", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.5": {"text": "Wann aber doch noch viel gefehlt: So ruf ich dich in De-", "tokens": ["Wann", "a\u00b7ber", "doch", "noch", "viel", "ge\u00b7fehlt", ":", "So", "ruf", "ich", "dich", "in", "De"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ADV", "ADV", "ADV", "VVPP", "$.", "ADV", "NN", "PPER", "PRF", "APPR", "TRUNC"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.6": {"text": "Gieb doch im k\u00fcnftgen Jahre mir, den Meinigen und jedermann,", "tokens": ["Gieb", "doch", "im", "k\u00fcnft\u00b7gen", "Jah\u00b7re", "mir", ",", "den", "Mei\u00b7ni\u00b7gen", "und", "je\u00b7der\u00b7mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "APPRART", "ADJA", "NN", "PPER", "$,", "ART", "NN", "KON", "PIS", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.7": {"text": "Da\u00df wir, ger\u00fchrt durch deine Wunder, mit allen Kr\u00e4ften uns", "tokens": ["Da\u00df", "wir", ",", "ge\u00b7r\u00fchrt", "durch", "dei\u00b7ne", "Wun\u00b7der", ",", "mit", "al\u00b7len", "Kr\u00e4f\u00b7ten", "uns"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "$,", "VVPP", "APPR", "PPOSAT", "NN", "$,", "APPR", "PIAT", "NN", "PPER"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.8": {"text": "Dich zu erkennen, zu ", "tokens": ["Dich", "zu", "er\u00b7ken\u00b7nen", ",", "zu"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["PPER", "PTKZU", "VVINF", "$,", "APPR"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.9": {"text": "Jm Denken, welches deiner w\u00fcrdig; im Gl\u00fcck, durch frohe", "tokens": ["Jm", "Den\u00b7ken", ",", "wel\u00b7ches", "dei\u00b7ner", "w\u00fcr\u00b7dig", ";", "im", "Gl\u00fcck", ",", "durch", "fro\u00b7he"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["APPRART", "NN", "$,", "PRELS", "PPOSAT", "ADJD", "$.", "APPRART", "NN", "$,", "APPR", "ADJA"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.11": {"text": "lassenheit,", "tokens": ["las\u00b7sen\u00b7heit", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.12": {"text": "Dein Wollen zu verehren suchen; und unsre ganze Lebenszeit", "tokens": ["Dein", "Wol\u00b7len", "zu", "ver\u00b7eh\u00b7ren", "su\u00b7chen", ";", "und", "uns\u00b7re", "gan\u00b7ze", "Le\u00b7bens\u00b7zeit"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "VVINF", "$.", "KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.13": {"text": "Sich in bewundernden Vertrauen dem Sch\u00f6pfer", "tokens": ["Sich", "in", "be\u00b7wun\u00b7dern\u00b7den", "Ver\u00b7trau\u00b7en", "dem", "Sch\u00f6p\u00b7fer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PRF", "APPR", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "kindlich \u00fcbergeben,\nUnd ewig Gutes von ihm hoffen. Die\u00df ist allein ein\nwahres Leben.", "tokens": ["kind\u00b7lich", "\u00fc\u00b7ber\u00b7ge\u00b7ben", ",", "Und", "e\u00b7wig", "Gu\u00b7tes", "von", "ihm", "hof\u00b7fen", ".", "Die\u00df", "ist", "al\u00b7lein", "ein", "wah\u00b7res", "Le\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,", "KON", "ADJD", "NN", "APPR", "PPER", "VVINF", "$.", "PDS", "VAFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+--+-+-+-+--+-+-+-+-", "measure": "trochaic.octa.plus.relaxed"}}}}}