{"textgrid.poem.57664": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Osterfeuer", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die goldenen Osterfeuer gl\u00fchen,", "tokens": ["Die", "gol\u00b7de\u00b7nen", "Os\u00b7ter\u00b7feu\u00b7er", "gl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der Fr\u00fchling kam in die Welt hinein,", "tokens": ["Der", "Fr\u00fch\u00b7ling", "kam", "in", "die", "Welt", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ich sehe deine Wangen gl\u00fchen", "tokens": ["Ich", "se\u00b7he", "dei\u00b7ne", "Wan\u00b7gen", "gl\u00fc\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In unserer Feuer rotem Schein.", "tokens": ["In", "un\u00b7se\u00b7rer", "Feu\u00b7er", "ro\u00b7tem", "Schein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Zwei Feuer nebeneinander flammen,", "tokens": ["Zwei", "Feu\u00b7er", "ne\u00b7ben\u00b7ein\u00b7an\u00b7der", "flam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "APPR", "VVINF", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wir haben l\u00e4chelnd die Glut entfacht,", "tokens": ["Wir", "ha