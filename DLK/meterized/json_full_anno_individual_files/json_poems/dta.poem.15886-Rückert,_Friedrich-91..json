{"dta.poem.15886": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "91.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es ist nicht wahr, da\u00df man ein Gl\u00fcck, das man nicht kennt,", "tokens": ["Es", "ist", "nicht", "wahr", ",", "da\u00df", "man", "ein", "Gl\u00fcck", ",", "das", "man", "nicht", "kennt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADJD", "$,", "KOUS", "PIS", "ART", "NN", "$,", "PRELS", "PIS", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nicht misset, und dich das, was du nicht wei\u00dft, nicht brennt.", "tokens": ["Nicht", "mis\u00b7set", ",", "und", "dich", "das", ",", "was", "du", "nicht", "wei\u00dft", ",", "nicht", "brennt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "$,", "KON", "PPER", "PDS", "$,", "PWS", "PPER", "PTKNEG", "VVFIN", "$,", "PTKNEG", "VVFIN", "$."], "meter": "-+---+-+-+-+", "measure": "dactylic.init"}}, "stanza.2": {"line.1": {"text": "Berufen f\u00fchlet sich zum Gl\u00fccke jedermann,", "tokens": ["Be\u00b7ru\u00b7fen", "f\u00fch\u00b7let", "sich", "zum", "Gl\u00fc\u00b7cke", "je\u00b7der\u00b7mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "APPRART", "NN", "PIS", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dem, wers auch nie gewann, doch nie entsagen kann.", "tokens": ["Dem", ",", "wers", "auch", "nie", "ge\u00b7wann", ",", "doch", "nie", "ent\u00b7sa\u00b7gen", "kann", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "VAFIN", "ADV", "ADV", "VVFIN", "$,", "ADV", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Der ahnt, wers nicht geschmeckt, doch wie es schmecken m\u00fc\u00dfe,", "tokens": ["Der", "ahnt", ",", "wers", "nicht", "ge\u00b7schmeckt", ",", "doch", "wie", "es", "schme\u00b7cken", "m\u00fc\u00b7\u00dfe", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "PWS", "PTKNEG", "VVPP", "$,", "KON", "PWAV", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und bitter seinen Mund macht die entbehrte S\u00fc\u00dfe.", "tokens": ["Und", "bit\u00b7ter", "sei\u00b7nen", "Mund", "macht", "die", "ent\u00b7behr\u00b7te", "S\u00fc\u00b7\u00dfe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PPOSAT", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}