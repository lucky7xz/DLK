{"dta.poem.9140": {"metadata": {"author": {"name": "Weise, Christian", "birth": "N.A.", "death": "N.A."}, "title": "V.  \n Der getreue haus-knecht.", "genre": "Lyrik; Drama; Prosa", "period": "N.A.", "pub_year": "1701", "urn": "urn:nbn:de:kobv:b4-25043-0", "language": ["de:0.99"], "booktitle": "Weise, Christian: \u00dcberfl\u00fc\u00dfige Gedancken Der gr\u00fcnenden jugend. Leipzig, 1701."}, "poem": {"stanza.1": {"line.1": {"text": "Ich armer haus-knecht habe nun", "tokens": ["Ich", "ar\u00b7mer", "haus\u00b7knecht", "ha\u00b7be", "nun"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADJA", "NN", "VAFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein \u00e4mptgen angenommen/", "tokens": ["Mein", "\u00e4mpt\u00b7gen", "an\u00b7ge\u00b7nom\u00b7men", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "VVPP", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Nun mu\u00df ich helffen wirthlich thun", "tokens": ["Nun", "mu\u00df", "ich", "helf\u00b7fen", "wirth\u00b7lich", "thun"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "VVINF", "ADJD", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn frembde g\u00e4ste kommen/", "tokens": ["Wenn", "fremb\u00b7de", "g\u00e4s\u00b7te", "kom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sonst werd ich wieder weggejagt", "tokens": ["Sonst", "werd", "ich", "wie\u00b7der", "weg\u00b7ge\u00b7jagt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Von meiner lieben jungen magd.", "tokens": ["Von", "mei\u00b7ner", "lie\u00b7ben", "jun\u00b7gen", "magd", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "2. Ich geh und lasse mich durchaus", "tokens": ["Ich", "geh", "und", "las\u00b7se", "mich", "durc\u00b7haus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die m\u00fche nicht beschweren/", "tokens": ["Die", "m\u00fc\u00b7he", "nicht", "be\u00b7schwe\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und solt ich funffzehenmahl das hau\u00df", "tokens": ["Und", "solt", "ich", "funff\u00b7ze\u00b7hen\u00b7mahl", "das", "hau\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "In einem tage kehren:", "tokens": ["In", "ei\u00b7nem", "ta\u00b7ge", "keh\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Denn wenn mich ja die arbeit plagt/", "tokens": ["Denn", "wenn", "mich", "ja", "die", "ar\u00b7beit", "plagt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So denck ich an die junge magd.", "tokens": ["So", "denck", "ich", "an", "die", "jun\u00b7ge", "magd", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "3. Die k\u00f6chin mag so offt sie will", "tokens": ["Die", "k\u00f6\u00b7chin", "mag", "so", "offt", "sie", "will"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "VMFIN", "ADV", "ADV", "PPER", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mich etwas anders heissen/", "tokens": ["Mich", "et\u00b7was", "an\u00b7ders", "heis\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die frau die mag den besen-stiel", "tokens": ["Die", "frau", "die", "mag", "den", "be\u00b7sen\u00b7stiel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "VMFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mir umb die ohren schmeissen:", "tokens": ["Mir", "umb", "die", "oh\u00b7ren", "schmeis\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ich bleibe dennoch unverzagt", "tokens": ["Ich", "blei\u00b7be", "den\u00b7noch", "un\u00b7ver\u00b7zagt"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Bey meiner lieben jungen magd.", "tokens": ["Bey", "mei\u00b7ner", "lie\u00b7ben", "jun\u00b7gen", "magd", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "4. Wenn sie mir an der seiten steht", "tokens": ["Wenn", "sie", "mir", "an", "der", "sei\u00b7ten", "steht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So hab ich schon gewonnen/", "tokens": ["So", "hab", "ich", "schon", "ge\u00b7won\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "VVPP", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und alle traurigkeit vergeht", "tokens": ["Und", "al\u00b7le", "trau\u00b7rig\u00b7keit", "ver\u00b7geht"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie butter an der sonnen/", "tokens": ["Wie", "but\u00b7ter", "an", "der", "son\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "APPR", "ART", "ADJA", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ich bin ihr knecht/ der ihr behagt/", "tokens": ["Ich", "bin", "ihr", "knecht", "/", "der", "ihr", "be\u00b7hagt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVFIN", "$(", "PRELS", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sie meine liebe junge magd.", "tokens": ["Sie", "mei\u00b7ne", "lie\u00b7be", "jun\u00b7ge", "magd", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "5. Wir stecken fast den gantzen tag", "tokens": ["Wir", "ste\u00b7cken", "fast", "den", "gant\u00b7zen", "tag"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Beysammen in der k\u00fcchen/", "tokens": ["Bey\u00b7sam\u00b7men", "in", "der", "k\u00fc\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und wann ich possen treiben mag/", "tokens": ["Und", "wann", "ich", "pos\u00b7sen", "trei\u00b7ben", "mag", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "VVINF", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So ist sie auch verglichen/", "tokens": ["So", "ist", "sie", "auch", "ver\u00b7gli\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "VVPP", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Doch ist kein mensch/ der mich verklagt", "tokens": ["Doch", "ist", "kein", "mensch", "/", "der", "mich", "ver\u00b7klagt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VAFIN", "PIAT", "NN", "$(", "PRELS", "PPER", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit meiner lieben jungen magd.", "tokens": ["Mit", "mei\u00b7ner", "lie\u00b7ben", "jun\u00b7gen", "magd", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "6. Ich geh in keller/ hole bier/", "tokens": ["Ich", "geh", "in", "kel\u00b7ler", "/", "ho\u00b7le", "bier", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NE", "$(", "VVFIN", "ADV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und wann ich nunter schreite/", "tokens": ["Und", "wann", "ich", "nun\u00b7ter", "schrei\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADV", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So kommt die magd und giebet mir", "tokens": ["So", "kommt", "die", "magd", "und", "gie\u00b7bet", "mir"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "KON", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Fein sauber das geleite/", "tokens": ["Fein", "sau\u00b7ber", "das", "ge\u00b7lei\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Doch niemand in dem hause fragt/", "tokens": ["Doch", "nie\u00b7mand", "in", "dem", "hau\u00b7se", "fragt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Was machst du mit der jungen magd.", "tokens": ["Was", "machst", "du", "mit", "der", "jun\u00b7gen", "magd", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "7. Der Boden und der ober-saal", "tokens": ["Der", "Bo\u00b7den", "und", "der", "o\u00b7ber\u00b7saal"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die stehn uns allzeit offen/", "tokens": ["Die", "stehn", "uns", "all\u00b7zeit", "of\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "ADJD", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da haben sie uns hundertmal", "tokens": ["Da", "ha\u00b7ben", "sie", "uns", "hun\u00b7dert\u00b7mal"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Beysammen angetroffen/", "tokens": ["Bey\u00b7sam\u00b7men", "an\u00b7ge\u00b7trof\u00b7fen", "/"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Doch hat der herr noch nie gesagt/", "tokens": ["Doch", "hat", "der", "herr", "noch", "nie", "ge\u00b7sagt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "ADV", "ADV", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Geh/ pack dich von der jungen magd.", "tokens": ["Geh", "/", "pack", "dich", "von", "der", "jun\u00b7gen", "magd", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "VVIMP", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "8. Ich wolt auch wol kein hau\u00df-knecht seyn/", "tokens": ["Ich", "wolt", "auch", "wol", "kein", "hau\u00df\u00b7knecht", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "PIAT", "NN", "VAINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wann ich nicht eben w\u00fcste/", "tokens": ["Wann", "ich", "nicht", "e\u00b7ben", "w\u00fcs\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PTKNEG", "ADV", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df ich das liebe m\u00e4gdelein", "tokens": ["Da\u00df", "ich", "das", "lie\u00b7be", "m\u00e4g\u00b7del\u00b7ein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sonst gar entrathen m\u00fcste/", "tokens": ["Sonst", "gar", "ent\u00b7ra\u00b7then", "m\u00fcs\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "So bleib ich gleichwol ungeplagt", "tokens": ["So", "bleib", "ich", "gleich\u00b7wol", "un\u00b7ge\u00b7plagt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Bey meiner lieben jungen magd.", "tokens": ["Bey", "mei\u00b7ner", "lie\u00b7ben", "jun\u00b7gen", "magd", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "9. Die sch\u00f6nen jungfern m\u00f6gen nun", "tokens": ["Die", "sch\u00f6\u00b7nen", "jung\u00b7fern", "m\u00f6\u00b7gen", "nun"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "VMFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Aus meinem hertzen weichen/", "tokens": ["Aus", "mei\u00b7nem", "hert\u00b7zen", "wei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dann wann ich ja will freundlich thun/", "tokens": ["Dann", "wann", "ich", "ja", "will", "freund\u00b7lich", "thun", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWAV", "PPER", "ADV", "VMFIN", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Geh ich zu meines gleichen.", "tokens": ["Geh", "ich", "zu", "mei\u00b7nes", "glei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "APPR", "PPOSAT", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ich armer hau\u00df-knecht habs gewagt/", "tokens": ["Ich", "ar\u00b7mer", "hau\u00df\u00b7knecht", "habs", "ge\u00b7wagt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "NE", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und l\u00f6ffle mit der jungen magd.", "tokens": ["Und", "l\u00f6ff\u00b7le", "mit", "der", "jun\u00b7gen", "magd", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}