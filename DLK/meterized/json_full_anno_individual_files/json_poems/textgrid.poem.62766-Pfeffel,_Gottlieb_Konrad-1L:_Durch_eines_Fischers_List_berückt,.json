{"textgrid.poem.62766": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Durch eines Fischers List ber\u00fcckt,", "genre": "verse", "period": "N.A.", "pub_year": 1758, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Durch eines Fischers List ber\u00fcckt,", "tokens": ["Durch", "ei\u00b7nes", "Fi\u00b7schers", "List", "be\u00b7r\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ward in sein Garn ein junger Hecht verstrickt.", "tokens": ["Ward", "in", "sein", "Garn", "ein", "jun\u00b7ger", "Hecht", "ver\u00b7strickt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPOSAT", "NN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das Spr\u00fcchwort sagt: Die Noth bricht Eisen.", "tokens": ["Das", "Spr\u00fcch\u00b7wort", "sagt", ":", "Die", "Noth", "bricht", "Ei\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "ART", "NN", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Kriegsgefangne nagt so lang,", "tokens": ["Der", "Kriegs\u00b7ge\u00b7fang\u00b7ne", "nagt", "so", "lang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Bis da\u00df es ihm zuletzt gelang", "tokens": ["Bis", "da\u00df", "es", "ihm", "zu\u00b7letzt", "ge\u00b7lang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "KOUS", "PPER", "PPER", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sich aus den Banden loszurei\u00dfen.", "tokens": ["Sich", "aus", "den", "Ban\u00b7den", "los\u00b7zu\u00b7rei\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Itzt, sprach er bey sich selbst: Ey, ey,", "tokens": ["Itzt", ",", "sprach", "er", "bey", "sich", "selbst", ":", "Ey", ",", "ey", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "$,", "VVFIN", "PPER", "APPR", "PRF", "ADV", "$.", "NN", "$,", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ich dacht es nicht, bey meiner Ehre,", "tokens": ["Ich", "dacht", "es", "nicht", ",", "bey", "mei\u00b7ner", "Eh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Da\u00df hier ein Netz verborgen w\u00e4re.", "tokens": ["Da\u00df", "hier", "ein", "Netz", "ver\u00b7bor\u00b7gen", "w\u00e4\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Je nun, ich bin ja wieder frey,", "tokens": ["Je", "nun", ",", "ich", "bin", "ja", "wie\u00b7der", "frey", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "PPER", "VAFIN", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Kein Henker soll zum zweytenmal mich kriegen.", "tokens": ["Kein", "Hen\u00b7ker", "soll", "zum", "zwey\u00b7ten\u00b7mal", "mich", "krie\u00b7gen", "."], "token_info