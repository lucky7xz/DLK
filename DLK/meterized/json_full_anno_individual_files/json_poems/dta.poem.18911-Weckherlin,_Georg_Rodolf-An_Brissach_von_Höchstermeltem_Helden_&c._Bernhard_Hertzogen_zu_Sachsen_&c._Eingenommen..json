{"dta.poem.18911": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "An Brissach von H\u00f6chstermeltem Helden  \n  &c.   Bernhard Hertzogen zu  \n Sachsen   &c.  \n Eingenommen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-200905198111", "language": ["de:0.99"], "booktitle": "Weckherlin, Georg Rodolf: Gaistliche und Weltliche Gedichte. Amsterdam, 1641."}, "poem": {"stanza.1": {"line.1": {"text": "Ja/ Brissach/ dein verlust ist dein gewin vnd prey\u00df;", "tokens": ["Ja", "/", "Bris\u00b7sach", "/", "dein", "ver\u00b7lust", "ist", "dein", "ge\u00b7win", "vnd", "prey\u00df", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$(", "NE", "$(", "PPOSAT", "NN", "VAFIN", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du hast/ in dem du dich verloren/ dich gefunden;", "tokens": ["Du", "hast", "/", "in", "dem", "du", "dich", "ver\u00b7lo\u00b7ren", "/", "dich", "ge\u00b7fun\u00b7den", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$(", "APPR", "PRELS", "PPER", "PRF", "VVPP", "$(", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Du hast/ von disem schwert erobert/ vberwunden;", "tokens": ["Du", "hast", "/", "von", "di\u00b7sem", "schwert", "er\u00b7o\u00b7bert", "/", "vber\u00b7wun\u00b7den", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "$(", "APPR", "PDS", "VVFIN", "VVPP", "$(", "VVPP", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Vnd vneinnehmlich nu wirst du auff dise wei\u00df.", "tokens": ["Vnd", "vnein\u00b7nehm\u00b7lich", "nu", "wirst", "du", "auff", "di\u00b7se", "wei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "VAFIN", "PPER", "APPR", "PDS", "VVFIN", "$."], "meter": "-+--++-+--+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Dan diser F\u00fcrst/ Held/ Mars (dein Siger) ist so wei\u00df/", "tokens": ["Dan", "di\u00b7ser", "F\u00fcrst", "/", "Held", "/", "Mars", "(", "dein", "Si\u00b7ger", ")", "ist", "so", "wei\u00df", "/"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PDAT", "NN", "$(", "NN", "$(", "NE", "$(", "PPOSAT", "NN", "$(", "VAFIN", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So g\u00fctig/ m\u00e4chtig/ gro\u00df/ da\u00df dein verdru\u00df ver-", "tokens": ["So", "g\u00fc\u00b7tig", "/", "m\u00e4ch\u00b7tig", "/", "gro\u00df", "/", "da\u00df", "dein", "ver\u00b7dru\u00df", "ver"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "$(", "ADJD", "$(", "ADJD", "$(", "KOUS", "PPOSAT", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "schwunden/", "tokens": ["schwun\u00b7den", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Al\u00dfbald dich seine faust zu seinem dienst verbun-", "tokens": ["Al\u00df\u00b7bald", "dich", "sei\u00b7ne", "faust", "zu", "sei\u00b7nem", "dienst", "ver\u00b7bun"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "APPR", "PPOSAT", "ADV", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "den/", "tokens": ["den", "/"], "token_info": ["word", "punct"], "pos": ["ART", "$("], "meter": "-", "measure": "single.down"}, "line.6": {"text": "Darumb mit frewd vnd danck gehorsam dich er-", "tokens": ["Da\u00b7rumb", "mit", "frewd", "vnd", "danck", "ge\u00b7hor\u00b7sam", "dich", "er"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "APPR", "NN", "KON", "ADV", "ADJD", "PPER", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "wei\u00df.", "tokens": ["wei\u00df", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+", "measure": "single.up"}}, "stanza.3": {"line.1": {"text": "Sih doch/ bedenck vnd merck/ wie herrlich Er dich", "tokens": ["Sih", "doch", "/", "be\u00b7denck", "vnd", "merck", "/", "wie", "herr\u00b7lich", "Er", "dich"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "ADV", "$(", "ADJD", "KON", "NN", "$(", "PWAV", "ADJD", "PPER", "PRF"], "meter": "+--+-+-+--+", "measure": "iambic.penta.invert"}, "line.2": {"text": "zieret/", "tokens": ["zie\u00b7ret", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Vn\u0303 du mit nichten Jhn; wie durch jhn Got in dir/", "tokens": ["V\u00f1", "du", "mit", "nich\u00b7ten", "Jhn", ";", "wie", "durch", "jhn", "Got", "in", "dir", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PIS", "PPER", "$.", "KOKOM", "APPR", "PPER", "NN", "APPR", "PPER", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Als durch Got vber dich Er (sigreich) triumfieret!", "tokens": ["Als", "durch", "Got", "vber", "dich", "Er", "(", "sig\u00b7reich", ")", "tri\u00b7um\u00b7fie\u00b7ret", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KOUS", "APPR", "NN", "APPR", "PPER", "PPER", "$(", "ADJD", "$(", "VVFIN", "$."], "meter": "--+--+--+-+-", "measure": "anapaest.tri.plus"}}, "stanza.4": {"line.1": {"text": "So lern nu seine lehr (vnd gib jhm danck darf\u00fcr)", "tokens": ["So", "lern", "nu", "sei\u00b7ne", "lehr", "(", "vnd", "gib", "jhm", "danck", "dar\u00b7f\u00fcr", ")"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPOSAT", "NN", "$(", "KON", "VVIMP", "PPER", "ADV", "PAV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil Got den F\u00fcrsten selbs/ wie der F\u00fcrst dich/", "tokens": ["Weil", "Got", "den", "F\u00fcrs\u00b7ten", "selbs", "/", "wie", "der", "F\u00fcrst", "dich", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "NN", "ADV", "$(", "KOKOM", "ART", "NN", "PPER", "$("], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "regieret/", "tokens": ["re\u00b7gie\u00b7ret", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Da\u00df jhm allzeit der Sig/ vnd Got die ehr/ gebihr.", "tokens": ["Da\u00df", "jhm", "all\u00b7zeit", "der", "Sig", "/", "vnd", "Got", "die", "ehr", "/", "ge\u00b7bihr", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "$(", "KON", "NN", "ART", "NN", "$(", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}