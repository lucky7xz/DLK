{"dta.poem.21674": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Mir sind deine schwarzen Augen,", "tokens": ["Mir", "sind", "dei\u00b7ne", "schwar\u00b7zen", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie Fahnen auf dem Zelt", "tokens": ["Wie", "Fah\u00b7nen", "auf", "dem", "Zelt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Von unserm Herrn zu Bagdad,", "tokens": ["Von", "un\u00b7serm", "Herrn", "zu", "Bag\u00b7dad", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wo der Halbmond Wache h\u00e4lt.", "tokens": ["Wo", "der", "Halb\u00b7mond", "Wa\u00b7che", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ro\u00dfschweife ihn umflattern \u2014", "tokens": ["Ro\u00df\u00b7schwei\u00b7fe", "ihn", "um\u00b7flat\u00b7tern"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVINF", "$("], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Wie Wolken des Mondes Pracht", "tokens": ["Wie", "Wol\u00b7ken", "des", "Mon\u00b7des", "Pracht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "ART", "NN", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Umwallen auf dem Zelte", "tokens": ["Um\u00b7wal\u00b7len", "auf", "dem", "Zel\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der allbedeckenden Nacht.", "tokens": ["Der", "all\u00b7be\u00b7de\u00b7cken\u00b7den", "Nacht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Ich bin ein Kalif des Geistes,", "tokens": ["Ich", "bin", "ein", "Ka\u00b7lif", "des", "Geis\u00b7tes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ein Mehrer des Reichs f\u00fcrwahr.", "tokens": ["Ein", "Meh\u00b7rer", "des", "Reichs", "f\u00fcr\u00b7wahr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Doch ein K\u00f6nig ohne Land nur", "tokens": ["Doch", "ein", "K\u00f6\u00b7nig", "oh\u00b7ne", "Land", "nur"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "NN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Im gro\u00dfen Weltbazar.", "tokens": ["Im", "gro\u00b7\u00dfen", "Welt\u00b7ba\u00b7zar", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Einen Schatz nur hat der Kurde                   ", "tokens": ["Ei\u00b7nen", "Schatz", "nur", "hat", "der", "Kur\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VAFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "An Harmonia\u2019s Bucht:", "tokens": ["An", "Har\u00b7mo\u00b7nia's", "Bucht", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Die stolzen Feuerrosse", "tokens": ["Die", "stol\u00b7zen", "Feu\u00b7er\u00b7ros\u00b7se"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Von Kochlani\u2019s alter Zucht \u2014", "tokens": ["Von", "Koch\u00b7la\u00b7ni's", "al\u00b7ter", "Zucht"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADJA", "NN", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "So habe ich nur den Simurg,", "tokens": ["So", "ha\u00b7be", "ich", "nur", "den", "Si\u00b7murg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ART", "NE", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Den alten Fabelgreif;", "tokens": ["Den", "al\u00b7ten", "Fa\u00b7bel\u00b7greif", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der tr\u00e4gt mich zum siebenten Himmel", "tokens": ["Der", "tr\u00e4gt", "mich", "zum", "sie\u00b7ben\u00b7ten", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Auf seinem flammenden Schweif.", "tokens": ["Auf", "sei\u00b7nem", "flam\u00b7men\u00b7den", "Schweif", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.6": {"line.1": {"text": "Den Isthakar-Schatz kann ich heben,", "tokens": ["Den", "Ist\u00b7ha\u00b7ka\u00b7rSchatz", "kann", "ich", "he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den Gott meinem Innern verlieh,", "tokens": ["Den", "Gott", "mei\u00b7nem", "In\u00b7nern", "ver\u00b7lieh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Und mit Salomos Siegel beschw\u00f6ren", "tokens": ["Und", "mit", "Sa\u00b7lo\u00b7mos", "Sie\u00b7gel", "be\u00b7schw\u00f6\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NE", "NN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Die Geister der Phantasie.", "tokens": ["Die", "Geis\u00b7ter", "der", "Phan\u00b7ta\u00b7sie", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "O k\u00f6nnte ich mir beschw\u00f6ren \u2014", "tokens": ["O", "k\u00f6nn\u00b7te", "ich", "mir", "be\u00b7schw\u00f6\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPER", "PPER", "VVINF", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Nicht die F\u00fcrstin von Saba, nein,", "tokens": ["Nicht", "die", "F\u00fcrs\u00b7tin", "von", "Sa\u00b7ba", ",", "nein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "APPR", "NE", "$,", "PTKANT", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Nur Dich. So folge ich immer", "tokens": ["Nur", "Dich", ".", "So", "fol\u00b7ge", "ich", "im\u00b7mer"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "$.", "ADV", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Deiner schwarzen Augen Schein.", "tokens": ["Dei\u00b7ner", "schwar\u00b7zen", "Au\u00b7gen", "Schein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}