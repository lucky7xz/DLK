{"textgrid.poem.60019": {"metadata": {"author": {"name": "Jacobi, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sch\u00fcchtern fliehn die Jungen Hirten?", "genre": "verse", "period": "N.A.", "pub_year": 1777, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sch\u00fcchtern fliehn die Jungen Hirten?", "tokens": ["Sch\u00fcch\u00b7tern", "fliehn", "die", "Jun\u00b7gen", "Hir\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wen verbergen diese Myrthen,", "tokens": ["Wen", "ver\u00b7ber\u00b7gen", "die\u00b7se", "Myr\u00b7then", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "In geheimni\u00dfvoller Nacht,", "tokens": ["In", "ge\u00b7heim\u00b7ni\u00df\u00b7vol\u00b7ler", "Nacht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Unter ihren leisen Bl\u00e4ttern?", "tokens": ["Un\u00b7ter", "ih\u00b7ren", "lei\u00b7sen", "Bl\u00e4t\u00b7tern", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "O von tausend Liebesg\u00f6ttern", "tokens": ["O", "von", "tau\u00b7send", "Lie\u00b7bes\u00b7g\u00f6t\u00b7tern"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "APPR", "CARD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wird der ganze Hain bewacht!", "tokens": ["Wird", "der", "gan\u00b7ze", "Hain", "be\u00b7wacht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "T\u00e4ubchen lassen sich hernieder,", "tokens": ["T\u00e4ub\u00b7chen", "las\u00b7sen", "sich", "her\u00b7nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Huldg\u00f6ttinnen singen Lieder:", "tokens": ["Huld\u00b7g\u00f6t\u00b7tin\u00b7nen", "sin\u00b7gen", "Lie\u00b7der", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ist es Venus? will sie hier", "tokens": ["Ist", "es", "Ve\u00b7nus", "?", "will", "sie", "hier"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PPER", "NN", "$.", "VMFIN", "PPER", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "In dem Silberteiche baden?", "tokens": ["In", "dem", "Sil\u00b7ber\u00b7tei\u00b7che", "ba\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ihr gef\u00e4lligen Dryaden,", "tokens": ["Ihr", "ge\u00b7f\u00e4l\u00b7li\u00b7gen", "Dry\u00b7a\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Einen Blick gew\u00e4hret mir.", "tokens": ["Ei\u00b7nen", "Blick", "ge\u00b7w\u00e4h\u00b7ret", "mir", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wollt ihr unter euern Zweigen", "tokens": ["Wollt", "ihr", "un\u00b7ter", "eu\u00b7ern", "Zwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mich besch\u00fctzen, mir sie zeigen?", "tokens": ["Mich", "be\u00b7sch\u00fct\u00b7zen", ",", "mir", "sie", "zei\u00b7gen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "$,", "PPER", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ewig dank ich euch mein Gl\u00fcck,", "tokens": ["E\u00b7wig", "dank", "ich", "euch", "mein", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ewig soll mein Lied euch ehren;", "tokens": ["E\u00b7wig", "soll", "mein", "Lied", "euch", "eh\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPOSAT", "NN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Zeigt, ach! zeiget mir Cytheren:", "tokens": ["Zeigt", ",", "ach", "!", "zei\u00b7get", "mir", "Cy\u00b7the\u00b7ren", ":"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ITJ", "$.", "VVFIN", "PPER", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "O ihr Nymphen, einen Blick!", "tokens": ["O", "ihr", "Nym\u00b7phen", ",", "ei\u00b7nen", "Blick", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "$,", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Die Geb\u00fcsche, die sie decken,", "tokens": ["Die", "Ge\u00b7b\u00fc\u00b7sche", ",", "die", "sie", "de\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "H\u00f6ren mich. O s\u00fc\u00dfes Schrecken,", "tokens": ["H\u00f6\u00b7ren", "mich", ".", "O", "s\u00fc\u00b7\u00dfes", "Schre\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$.", "NE", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Eine G\u00f6ttin unverh\u00fcllt?", "tokens": ["Ei\u00b7ne", "G\u00f6t\u00b7tin", "un\u00b7ver\u00b7h\u00fcllt", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wag' ich es nach der zu blicken,", "tokens": ["Wag'", "ich", "es", "nach", "der", "zu", "bli\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPER", "APPR", "ART", "PTKZU", "VVINF", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.5": {"text": "Die mit Liebe, mit Entz\u00fccken", "tokens": ["Die", "mit", "Lie\u00b7be", ",", "mit", "Ent\u00b7z\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "APPR", "NN", "$,", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Eine ganze Welt erf\u00fcllt?", "tokens": ["Ei\u00b7ne", "gan\u00b7ze", "Welt", "er\u00b7f\u00fcllt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Darf ein Sterblicher? Es gl\u00fchet", "tokens": ["Darf", "ein", "Sterb\u00b7li\u00b7cher", "?", "Es", "gl\u00fc\u00b7het"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VMFIN", "ART", "NN", "$.", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mars, wenn er die Reize siehet,", "tokens": ["Mars", ",", "wenn", "er", "die", "Rei\u00b7ze", "sie\u00b7het", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn ihr Busen sich emp\u00f6rt,", "tokens": ["Wenn", "ihr", "Bu\u00b7sen", "sich", "em\u00b7p\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PRF", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und er nicht den L\u00e4rm des Krieges,", "tokens": ["Und", "er", "nicht", "den", "L\u00e4rm", "des", "Krie\u00b7ges", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PTKNEG", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Nicht den wilden Ruf des Sieges,", "tokens": ["Nicht", "den", "wil\u00b7den", "Ruf", "des", "Sie\u00b7ges", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Nur ein z\u00e4rtlich Seufzen h\u00f6rt.", "tokens": ["Nur", "ein", "z\u00e4rt\u00b7lich", "Seuf\u00b7zen", "h\u00f6rt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJD", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "O ihr Myrthen! o umschlie\u00dfet", "tokens": ["O", "ihr", "Myr\u00b7then", "!", "o", "um\u00b7schlie\u00b7\u00dfet"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NE", "PPOSAT", "NN", "$.", "FM", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sie vor mir. Der G\u00fcrtel flie\u00dfet", "tokens": ["Sie", "vor", "mir", ".", "Der", "G\u00fcr\u00b7tel", "flie\u00b7\u00dfet"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "APPR", "PPER", "$.", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nun auf heil'gen Rasen hin.", "tokens": ["Nun", "auf", "heil'\u00b7gen", "Ra\u00b7sen", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Nieder steigt sie schon zur Quelle!", "tokens": ["Nie\u00b7der", "steigt", "sie", "schon", "zur", "Quel\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Schon ber\u00fchrt der Fu\u00df die Welle,", "tokens": ["Schon", "be\u00b7r\u00fchrt", "der", "Fu\u00df", "die", "Wel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Dem in W\u00fcsten Rosen bl\u00fchn.", "tokens": ["Dem", "in", "W\u00fcs\u00b7ten", "Ro\u00b7sen", "bl\u00fchn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Nie wird euch ein Sturm entehren,", "tokens": ["Nie", "wird", "euch", "ein", "Sturm", "en\u00b7teh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ihr Geb\u00fcsche, wo Cytheren", "tokens": ["Ihr", "Ge\u00b7b\u00fc\u00b7sche", ",", "wo", "Cy\u00b7the\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PWAV", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Der verliebte Fr\u00fchling fand.", "tokens": ["Der", "ver\u00b7lieb\u00b7te", "Fr\u00fch\u00b7ling", "fand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "K\u00f6mmt ein M\u00e4dchen sich zu k\u00fchlen,", "tokens": ["K\u00f6mmt", "ein", "M\u00e4d\u00b7chen", "sich", "zu", "k\u00fch\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PRF", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "An den Teich, so wird es f\u00fchlen,", "tokens": ["An", "den", "Teich", ",", "so", "wird", "es", "f\u00fch\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ADV", "VAFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Was kein M\u00e4dchen noch empfand.", "tokens": ["Was", "kein", "M\u00e4d\u00b7chen", "noch", "emp\u00b7fand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIAT", "NN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Sch\u00fcchtern fliehn die Jungen Hirten?", "tokens": ["Sch\u00fcch\u00b7tern", "fliehn", "die", "Jun\u00b7gen", "Hir\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wen verbergen diese Myrthen,", "tokens": ["Wen", "ver\u00b7ber\u00b7gen", "die\u00b7se", "Myr\u00b7then", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measur