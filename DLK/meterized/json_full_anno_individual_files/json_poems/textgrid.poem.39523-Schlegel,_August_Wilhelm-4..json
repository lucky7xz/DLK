{"textgrid.poem.39523": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Choliambe scheint ein Vers f\u00fcr Kunstrichter,", "tokens": ["Der", "Cho\u00b7li\u00b7am\u00b7be", "scheint", "ein", "Vers", "f\u00fcr", "Kuns\u00b7trich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die immerfort voll Naseweisheit mitsprechen,", "tokens": ["Die", "im\u00b7mer\u00b7fort", "voll", "Na\u00b7se\u00b7weis\u00b7heit", "mit\u00b7spre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "NN", "VVINF", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und eins nur wi\u00dfen sollten, da\u00df sie nichts wi\u00dfen.", "tokens": ["Und", "eins", "nur", "wi\u00b7\u00dfen", "soll\u00b7ten", ",", "da\u00df", "sie", "nichts", "wi\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "ADV", "VVINF", "VMFIN", "$,", "KOUS", "PPER", "PIS", "VVINF", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Wo die Kritik hinkt, mu\u00df ja auch der Vers lahm sein.", "tokens": ["Wo", "die", "Kri\u00b7tik", "hinkt", ",", "mu\u00df", "ja", "auch", "der", "Vers", "lahm", "sein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$,", "VMFIN", "ADV", "ADV", "ART", "NN", "VVFIN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wer sein Gem\u00fcth labt am Gesang der Nachteulen,", "tokens": ["Wer", "sein", "Ge\u00b7m\u00fcth", "labt", "am", "Ge\u00b7sang", "der", "Nach\u00b7teu\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Und wenn die Nachtigall beginnt, das Ohr zustopft,", "tokens": ["Und", "wenn", "die", "Nach\u00b7ti\u00b7gall", "be\u00b7ginnt", ",", "das", "Ohr", "zu\u00b7stopft", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVFIN", "$,", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Dem sollte man's mit scharfer Dissonanz abhaun.", "tokens": ["Dem", "soll\u00b7te", "man's", "mit", "schar\u00b7fer", "Dis\u00b7so\u00b7nanz", "ab\u00b7haun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PIS", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Der Choliambe scheint ein Vers f\u00fcr Kunstrichter,", "tokens": ["Der", "Cho\u00b7li\u00b7am\u00b7be", "scheint", "ein", "Vers", "f\u00fcr", "Kuns\u00b7trich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die immerfort voll Naseweisheit mitsprechen,", "tokens": ["Die", "im\u00b7mer\u00b7fort", "voll", "Na\u00b7se\u00b7weis\u00b7heit", "mit\u00b7spre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "NN", "VVINF", "$,"], "meter": "-+-+-+-+