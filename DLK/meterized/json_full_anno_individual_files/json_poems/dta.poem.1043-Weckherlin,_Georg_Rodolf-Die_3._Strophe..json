{"dta.poem.1043": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Die 3. Strophe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1618", "urn": "urn:nbn:de:kobv:b4-20628-9", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dvrch dise wort war bald entgr\u00fcndet", "tokens": ["Dvrch", "di\u00b7se", "wort", "war", "bald", "ent\u00b7gr\u00fcn\u00b7det"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "VAFIN", "ADV", "VVFIN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Des Rheins angst/ so allein erdicht/", "tokens": ["Des", "Rheins", "angst", "/", "so", "al\u00b7lein", "er\u00b7dicht", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVPP", "$(", "ADV", "ADV", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das Er mit klarem angesicht", "tokens": ["Das", "Er", "mit", "kla\u00b7rem", "an\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sich bald bey der gesellschafft findet/", "tokens": ["Sich", "bald", "bey", "der", "ge\u00b7sell\u00b7schafft", "fin\u00b7det", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "In einem Tyrischen talar", "tokens": ["In", "ei\u00b7nem", "Ty\u00b7ri\u00b7schen", "ta\u00b7lar"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Von gold vnd kleinoten vmbhangen/", "tokens": ["Von", "gold", "vnd", "klei\u00b7no\u00b7ten", "vmb\u00b7han\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADJA", "NN", "$("], "meter": "-+-+---+-", "measure": "unknown.measure.tri"}, "line.7": {"text": "Seine Landsf\u00fcrstin zuempfangen", "tokens": ["Sei\u00b7ne", "Lands\u00b7f\u00fcrs\u00b7tin", "zu\u00b7em\u00b7pfan\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "VVINF"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Mit seiner gr\u00fcn-blawlechtenschar/", "tokens": ["Mit", "sei\u00b7ner", "gr\u00fcn\u00b7blaw\u00b7lech\u00b7ten\u00b7schar", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Darau\u00df etliche sich zufrayen", "tokens": ["Dar\u00b7au\u00df", "et\u00b7li\u00b7che", "sich", "zuf\u00b7ra\u00b7yen"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "PIAT", "PRF", "VVINF"], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.10": {"text": "Singen vnd dantzen einen rayen;", "tokens": ["Sin\u00b7gen", "vnd", "dant\u00b7zen", "ei\u00b7nen", "ra\u00b7yen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "ART", "VVINF", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.11": {"text": "Andre mit frischen bl\u00fcmelein", "tokens": ["And\u00b7re", "mit", "fri\u00b7schen", "bl\u00fc\u00b7me\u00b7lein"], "token_info": ["word", "word", "word", "word"], "pos": ["PIS", "APPR", "ADJA", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.12": {"text": "Myrten/ gilgen vnd r\u00f6selein", "tokens": ["Myr\u00b7ten", "/", "gil\u00b7gen", "vnd", "r\u00f6\u00b7se\u00b7lein"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$(", "VVFIN", "KON", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.13": {"text": "Volversetzte str\u00e4u\u00df vnd kr\u00e4ntz binden/", "tokens": ["Vol\u00b7ver\u00b7setz\u00b7te", "str\u00e4u\u00df", "vnd", "kr\u00e4ntz", "bin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "VVINF", "$("], "meter": "+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.14": {"text": "Vnd brennen zu mahl von begihr", "tokens": ["Vnd", "bren\u00b7nen", "zu", "mahl", "von", "be\u00b7gihr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVINF", "PTKZU", "ADV", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Sich bey disem einzug zufinden", "tokens": ["Sich", "bey", "di\u00b7sem", "ein\u00b7zug", "zu\u00b7fin\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PRF", "APPR", "PDAT", "NN", "VVINF"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.16": {"text": "Vnd zu frolocken nach gebihr.", "tokens": ["Vnd", "zu", "fro\u00b7lo\u00b7cken", "nach", "ge\u00b7bihr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKZU", "VVINF", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}