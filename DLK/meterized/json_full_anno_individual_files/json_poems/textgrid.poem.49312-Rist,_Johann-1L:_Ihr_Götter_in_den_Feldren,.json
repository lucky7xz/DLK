{"textgrid.poem.49312": {"metadata": {"author": {"name": "Rist, Johann", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ihr G\u00f6tter in den Feldren,", "genre": "verse", "period": "N.A.", "pub_year": 1637, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr G\u00f6tter in den Feldren,", "tokens": ["Ihr", "G\u00f6t\u00b7ter", "in", "den", "Feld\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sylvanus und du Pan,", "tokens": ["Syl\u00b7va\u00b7nus", "und", "du", "Pan", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "PPER", "NE", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Ihr Nymphen in den W\u00e4ldren,", "tokens": ["Ihr", "Nym\u00b7phen", "in", "den", "W\u00e4ld\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "H\u00f6rt doch mein Klagen an!", "tokens": ["H\u00f6rt", "doch", "mein", "Kla\u00b7gen", "an", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ich armer Held mu\u00df sterben", "tokens": ["Ich", "ar\u00b7mer", "Held", "mu\u00df", "ster\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADJA", "NN", "VMFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und das wol tausendmal,", "tokens": ["Und", "das", "wol", "tau\u00b7send\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "ADV", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Kan doch nicht gar verderben,", "tokens": ["Kan", "doch", "nicht", "gar", "ver\u00b7der\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PTKNEG", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Verbleib' in steter Qual.", "tokens": ["Ver\u00b7bleib'", "in", "ste\u00b7ter", "Qual", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Die Thier' im finstren Kl\u00fcften", "tokens": ["Die", "Thier'", "im", "finst\u00b7ren", "Kl\u00fcf\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Erdulden nicht so viel,", "tokens": ["E\u00b7rdul\u00b7den", "nicht", "so", "viel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ADV", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Die V\u00f6glein in den L\u00fcften", "tokens": ["Die", "V\u00f6\u00b7glein", "in", "den", "L\u00fcf\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Die haben noch ihr Ziel.", "tokens": ["Die", "ha\u00b7ben", "noch", "ihr", "Ziel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Der Hirsch wird oft gejaget,", "tokens": ["Der", "Hirsch", "wird", "oft", "ge\u00b7ja\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Komt auch noch oft davon;", "tokens": ["Komt", "auch", "noch", "oft", "da\u00b7von", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "ADV", "PAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.15": {"text": "Ich lebe gar verzaget,", "tokens": ["Ich", "le\u00b7be", "gar", "ver\u00b7za\u00b7get", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.16": {"text": "Amor zu Spott und Hohn.", "tokens": ["A\u00b7mor", "zu", "Spott", "und", "Hohn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "KON", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.17": {"text": "Wird schon der Hirsch getroffen,", "tokens": ["Wird", "schon", "der", "Hirsch", "ge\u00b7trof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.18": {"text": "Lebt er doch gleichwol noch;", "tokens": ["Lebt", "er", "doch", "gleich\u00b7wol", "noch", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.19": {"text": "Ich leb' ohn' alles Hoffen,", "tokens": ["Ich", "leb'", "ohn'", "al\u00b7les", "Hof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.20": {"text": "Ich sterb' und lebe doch.", "tokens": ["Ich", "sterb'", "und", "le\u00b7be", "doch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.21": {"text": "Wird schon der Hirsch gestellet,", "tokens": ["Wird", "schon", "der", "Hirsch", "ge\u00b7stel\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.22": {"text": "F\u00e4ngt sichs doch nicht allzeit;", "tokens": ["F\u00e4ngt", "sichs", "doch", "nicht", "all\u00b7zeit", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "PTKNEG", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.23": {"text": "Ich werde stets gef\u00e4llet,", "tokens": ["Ich", "wer\u00b7de", "stets", "ge\u00b7f\u00e4l\u00b7let", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.24": {"text": "Wann Amor ist bereit.", "tokens": ["Wann", "A\u00b7mor", "ist", "be\u00b7reit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "VAFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.25": {"text": "Das Wild wird wol geschossen,", "tokens": ["Das", "Wild", "wird", "wol", "ge\u00b7schos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.26": {"text": "Doch nur ein einzigs Mal;", "tokens": ["Doch", "nur", "ein", "ein\u00b7zigs", "Mal", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.27": {"text": "Cupido unverdrossen", "tokens": ["Cu\u00b7pi\u00b7do", "un\u00b7ver\u00b7dros\u00b7sen"], "token_info": ["word", "word"], "pos": ["FM.la", "FM.la"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.28": {"text": "Scheu\u00dft aus mich ohne Zahl.", "tokens": ["Scheu\u00dft", "aus", "mich", "oh\u00b7ne", "Zahl", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PRF", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.29": {"text": "Wird schon der Hirsch verwundet,", "tokens": ["Wird", "schon", "der", "Hirsch", "ver\u00b7wun\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.30": {"text": "F\u00fchlt er doch wenig Schmerz;", "tokens": ["F\u00fchlt", "er", "doch", "we\u00b7nig", "Schmerz", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.31": {"text": "Mir ist so gar entz\u00fcndet", "tokens": ["Mir", "ist", "so", "gar", "ent\u00b7z\u00fcn\u00b7det"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.32": {"text": "Mit hei\u00dfer Glut mein Herz.", "tokens": ["Mit", "hei\u00b7\u00dfer", "Glut", "mein", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.33": {"text": "Dem Hirschlein wird genommen", "tokens": ["Dem", "Hirsc\u00b7hlein", "wird", "ge\u00b7nom\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.34": {"text": "Sein Leben weg in Eil';", "tokens": ["Sein", "Le\u00b7ben", "weg", "in", "Eil'", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.35": {"text": "Mir wil der Tod nicht kommen", "tokens": ["Mir", "wil", "der", "Tod", "nicht", "kom\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "NN", "PTKNEG", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.36": {"text": "Mit seinem J\u00e4gerpfeil.", "tokens": ["Mit", "sei\u00b7nem", "J\u00e4\u00b7ger\u00b7pfeil", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.37": {"text": "O, wol den schnellen Thieren,", "tokens": ["O", ",", "wol", "den", "schnel\u00b7len", "Thie\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.38": {"text": "Die in den W\u00e4ldern sein,", "tokens": ["Die", "in", "den", "W\u00e4l\u00b7dern", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.39": {"text": "Sie d\u00f6rfen gar nicht f\u00fchren", "tokens": ["Sie", "d\u00f6r\u00b7fen", "gar", "nicht", "f\u00fch\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PTKNEG", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.40": {"text": "Schmerz, Klagen, Leid und Pein.", "tokens": ["Schmerz", ",", "Kla\u00b7gen", ",", "Leid", "und", "Pein", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.41": {"text": "Sie leben nur in Freuden,", "tokens": ["Sie", "le\u00b7ben", "nur", "in", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.42": {"text": "Die Freiheit ist ihr Gut.", "tokens": ["Die", "Frei\u00b7heit", "ist", "ihr", "Gut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.43": {"text": "Wol dem, der so ohn' Leiden", "tokens": ["Wol", "dem", ",", "der", "so", "ohn'", "Lei\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ART", "$,", "PRELS", "ADV", "APPR", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.44": {"text": "Stets f\u00fchrt ein frischen Mut!", "tokens": ["Stets", "f\u00fchrt", "ein", "fri\u00b7schen", "Mut", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.45": {"text": "Ei, wil denn meine Schmerzen", "tokens": ["Ei", ",", "wil", "denn", "mei\u00b7ne", "Schmer\u00b7zen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "VMFIN", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.46": {"text": "Mein Lieb nicht lindern bald,", "tokens": ["Mein", "Lieb", "nicht", "lin\u00b7dern", "bald", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKNEG", "ADV", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.47": {"text": "So w\u00fcnsch' ich mir von Herzen,", "tokens": ["So", "w\u00fcn\u00b7sch'", "ich", "mir", "von", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.48": {"text": "Zu sein ein Hirsch im Wald.", "tokens": ["Zu", "sein", "ein", "Hirsch", "im", "Wald", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ihr G\u00f6tter in den Feldren,", "tokens": ["Ihr", "G\u00f6t\u00b7ter", "in", "den", "Feld\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punc