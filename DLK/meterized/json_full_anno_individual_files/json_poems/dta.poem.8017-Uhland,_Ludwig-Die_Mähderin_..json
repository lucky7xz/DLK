{"dta.poem.8017": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Die M\u00e4hderin .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "\u201eguten Morgen, Marie! so fr\u00fche schon r\u00fcstig und rege?", "tokens": ["\u201e", "gu\u00b7ten", "Mor\u00b7gen", ",", "Ma\u00b7rie", "!", "so", "fr\u00fc\u00b7he", "schon", "r\u00fcs\u00b7tig", "und", "re\u00b7ge", "?"], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJA", "NN", "$,", "NE", "$.", "ADV", "ADJA", "ADV", "ADJD", "KON", "ADJA", "$."], "meter": "+-+--+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Dich, treuste der M\u00e4gde, dich machet die Liebe nicht tr\u00e4ge.", "tokens": ["Dich", ",", "treus\u00b7te", "der", "M\u00e4g\u00b7de", ",", "dich", "ma\u00b7chet", "die", "Lie\u00b7be", "nicht", "tr\u00e4\u00b7ge", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+--+--+--+--+-", "measure": "amphibrach.penta.plus"}, "line.3": {"text": "Ja! m\u00e4hst du die Wiese mir ab von jetzt in drei Tagen,", "tokens": ["Ja", "!", "m\u00e4hst", "du", "die", "Wie\u00b7se", "mir", "ab", "von", "jetzt", "in", "drei", "Ta\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "VVFIN", "PPER", "ART", "NN", "PPER", "PTKVZ", "APPR", "ADV", "APPR", "CARD", "NN", "$,"], "meter": "-+--+--+-+--+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Nicht d\u00fcrft\u2019 ich den Sohn dir, den einzigen, l\u00e4nger versagen.\u201c", "tokens": ["Nicht", "d\u00fcrft'", "ich", "den", "Sohn", "dir", ",", "den", "ein\u00b7zi\u00b7gen", ",", "l\u00e4n\u00b7ger", "ver\u00b7sa\u00b7gen", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["PTKNEG", "VMFIN", "PPER", "ART", "NN", "PPER", "$,", "ART", "ADJA", "$,", "ADJD", "VVINF", "$.", "$("], "meter": "-+--+--+--+--+-", "measure": "amphibrach.penta.plus"}}, "stanza.2": {"line.1": {"text": "Der P\u00e4chter, der stattlich beg\u00fcterte, hat es gesprochen,", "tokens": ["Der", "P\u00e4ch\u00b7ter", ",", "der", "statt\u00b7lich", "be\u00b7g\u00fc\u00b7ter\u00b7te", ",", "hat", "es", "ge\u00b7spro\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$,", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+--+--+--+--+-", "measure": "amphibrach.penta.plus"}, "line.2": {"text": "Marie, wie f\u00fchlt sie den liebenden Busen sich pochen!", "tokens": ["Ma\u00b7rie", ",", "wie", "f\u00fchlt", "sie", "den", "lie\u00b7ben\u00b7den", "Bu\u00b7sen", "sich", "po\u00b7chen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "VVFIN", "PPER", "ART", "ADJA", "NN", "PRF", "VVINF", "$."], "meter": "-+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ein neues, ein kr\u00e4ftiges Leben durchdringt ihr die Glieder,", "tokens": ["Ein", "neu\u00b7es", ",", "ein", "kr\u00e4f\u00b7ti\u00b7ges", "Le\u00b7ben", "durch\u00b7dringt", "ihr", "die", "Glie\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ART", "ADJA", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+--+--+--+--+-", "measure": "amphibrach.penta.plus"}, "line.4": {"text": "Wie schwingt sie die Sense, wie streckt sie die Mahden danieder!", "tokens": ["Wie", "schwingt", "sie", "die", "Sen\u00b7se", ",", "wie", "streckt", "sie", "die", "Mah\u00b7den", "da\u00b7nie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ART", "NN", "$,", "PWAV", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+--+--+--+-", "measure": "amphibrach.penta.plus"}}, "stanza.3": {"line.1": {"text": "Der Mittag gl\u00fchet, die M\u00e4hder des Feldes ermatten,", "tokens": ["Der", "Mit\u00b7tag", "gl\u00fc\u00b7het", ",", "die", "M\u00e4h\u00b7der", "des", "Fel\u00b7des", "er\u00b7mat\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Sie suchen zur Labe den Quell und zum Schlummer den Schatten,", "tokens": ["Sie", "su\u00b7chen", "zur", "La\u00b7be", "den", "Quell", "und", "zum", "Schlum\u00b7mer", "den", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ART", "NN", "KON", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+--+--+--+--+-", "measure": "amphibrach.penta.plus"}, "line.3": {"text": "Noch schaffen im hei\u00dfen Gefilde die summenden Bienen,", "tokens": ["Noch", "schaf\u00b7fen", "im", "hei\u00b7\u00dfen", "Ge\u00b7fil\u00b7de", "die", "sum\u00b7men\u00b7den", "Bie\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+--+-", "measure": "amphibrach.penta.plus"}, "line.4": {"text": "Marie, sie ruht nicht, sie schafft in die Wette mit ihnen.", "tokens": ["Ma\u00b7rie", ",", "sie", "ruht", "nicht", ",", "sie", "schafft", "in", "die", "Wet\u00b7te", "mit", "ih\u00b7nen", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VVFIN", "PTKNEG", "$,", "PPER", "VVFIN", "APPR", "ART", "NN", "APPR", "PPER", "$."], "meter": "+--+--+--+--+-", "measure": "dactylic.penta"}}, "stanza.4": {"line.1": {"text": "Die Sonne versinkt, es ert\u00f6net das Abendgel\u00e4ute,", "tokens": ["Die", "Son\u00b7ne", "ver\u00b7sinkt", ",", "es", "er\u00b7t\u00f6\u00b7net", "das", "A\u00b7bend\u00b7ge\u00b7l\u00e4u\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+--+--+-", "measure": "amphibrach.penta.plus"}, "line.2": {"text": "Wohl rufen die Nachbarn: \u201eMarie, genug ist\u2019s f\u00fcr heute!\u201c", "tokens": ["Wohl", "ru\u00b7fen", "die", "Nach\u00b7barn", ":", "\u201e", "Ma\u00b7rie", ",", "ge\u00b7nug", "ist's", "f\u00fcr", "heu\u00b7te", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "$(", "NE", "$,", "ADV", "VAFIN", "APPR", "ADV", "$.", "$("], "meter": "-+--+----+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Wohl ziehen die M\u00e4hder, der Hirt und die Herde von hinnen,", "tokens": ["Wohl", "zie\u00b7hen", "die", "M\u00e4h\u00b7der", ",", "der", "Hirt", "und", "die", "Her\u00b7de", "von", "hin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ART", "NN", "KON", "ART", "NN", "APPR", "ADV", "$,"], "meter": "-+--+--+--+--+-", "measure": "amphibrach.penta.plus"}, "line.4": {"text": "Marie, sie dengelt die Sense zu neuem Beginnen.", "tokens": ["Ma\u00b7rie", ",", "sie", "den\u00b7gelt", "die", "Sen\u00b7se", "zu", "neu\u00b7em", "Be\u00b7gin\u00b7nen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+--+--+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.5": {"line.1": {"text": "Schon sinket der Thau, schon ergl\u00e4nzen der Mond und die Sterne,", "tokens": ["Schon", "sin\u00b7ket", "der", "Thau", ",", "schon", "er\u00b7gl\u00e4n\u00b7zen", "der", "Mond", "und", "die", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ADV", "VVFIN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+--+--+--+--+-", "measure": "amphibrach.penta.plus"}, "line.2": {"text": "Es duften die Mahden, die Nachtigall schl\u00e4gt aus der Ferne,", "tokens": ["Es", "duf\u00b7ten", "die", "Mah\u00b7den", ",", "die", "Nach\u00b7ti\u00b7gall", "schl\u00e4gt", "aus", "der", "Fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+--+--+-", "measure": "amphibrach.penta.plus"}, "line.3": {"text": "Marie verlangt nicht zu rasten, verlangt nicht zu lauschen,", "tokens": ["Ma\u00b7rie", "ver\u00b7langt", "nicht", "zu", "ras\u00b7ten", ",", "ver\u00b7langt", "nicht", "zu", "lau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKNEG", "PTKZU", "VVINF", "$,", "VVFIN", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "+--+--+--+--+-", "measure": "dactylic.penta"}, "line.4": {"text": "Stets l\u00e4\u00dft sie die Sense, die kr\u00e4ftig geschwungene, rauschen.", "tokens": ["Stets", "l\u00e4\u00dft", "sie", "die", "Sen\u00b7se", ",", "die", "kr\u00e4f\u00b7tig", "ge\u00b7schwun\u00b7ge\u00b7ne", ",", "rau\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,", "PRELS", "ADJD", "ADJA", "$,", "VVFIN", "$."], "meter": "-+--+--+--+--+-", "measure": "amphibrach.penta.plus"}}, "stanza.6": {"line.1": {"text": "So f\u00fcrder von Abend zu Morgen, von Morgen zu Abend,", "tokens": ["So", "f\u00fcr\u00b7der", "von", "A\u00b7bend", "zu", "Mor\u00b7gen", ",", "von", "Mor\u00b7gen", "zu", "A\u00b7bend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "NN", "APPR", "NN", "$,", "APPR", "NN", "APPR", "NN", "$,"], "meter": "-+--+--+--+--+-", "measure": "amphibrach.penta.plus"}, "line.2": {"text": "Mit Liebe sich n\u00e4hrend, mit seliger Hoffnung sich labend;", "tokens": ["Mit", "Lie\u00b7be", "sich", "n\u00e4h\u00b7rend", ",", "mit", "se\u00b7li\u00b7ger", "Hoff\u00b7nung", "sich", "la\u00b7bend", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PRF", "ADJD", "$,", "APPR", "ADJA", "NN", "PRF", "ADJD", "$."], "meter": "-+--+--+--+--+-", "measure": "amphibrach.penta.plus"}, "line.3": {"text": "Zum drittenmal hebt sich die Sonne, da ist es geschehen,", "tokens": ["Zum", "drit\u00b7ten\u00b7mal", "hebt", "sich", "die", "Son\u00b7ne", ",", "da", "ist", "es", "ge\u00b7sche\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADV", "VVFIN", "PRF", "ART", "NN", "$,", "ADV", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+--+--+--+--+-", "measure": "amphibrach.penta.plus"}, "line.4": {"text": "Dort seht ihr Marien, die wonniglich weinende, stehen.", "tokens": ["Dort", "seht", "ihr", "Ma\u00b7ri\u00b7en", ",", "die", "won\u00b7nig\u00b7lich", "wei\u00b7nen\u00b7de", ",", "ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$,", "PRELS", "ADJD", "ADJA", "$,", "VVFIN", "$."], "meter": "-+-+-+-+--+--+-", "measure": "iambic.hexa.relaxed"}}, "stanza.7": {"line.1": {"text": "\u201eguten Morgen, Marie! was seh\u2019 ich! o flei\u00dfige H\u00e4nde!", "tokens": ["\u201e", "gu\u00b7ten", "Mor\u00b7gen", ",", "Ma\u00b7rie", "!", "was", "seh'", "ich", "!", "o", "flei\u00b7\u00dfi\u00b7ge", "H\u00e4n\u00b7de", "!"], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADJA", "NN", "$,", "NE", "$.", "PWS", "VVFIN", "PPER", "$.", "FM", "ADJA", "NN", "$."], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Gem\u00e4ht ist die Wiese! das lohn\u2019 ich mit reichlicher Spende;", "tokens": ["Ge\u00b7m\u00e4ht", "ist", "die", "Wie\u00b7se", "!", "das", "lohn'", "ich", "mit", "reich\u00b7li\u00b7cher", "Spen\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "$.", "PDS", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+--+--+-", "measure": "amphibrach.penta.plus"}, "line.3": {"text": "Allein mit der Heurath \u2014 du nahmest im Ernste mein Scherzen,", "tokens": ["Al\u00b7lein", "mit", "der", "Heu\u00b7rath", "du", "nah\u00b7mest", "im", "Erns\u00b7te", "mein", "Scher\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$(", "PPER", "VVFIN", "APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+--+--+-+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Leichtgl\u00e4ubig, man sieht es, und th\u00f6richt sind liebende Herzen.\u201c", "tokens": ["Leicht\u00b7gl\u00e4u\u00b7big", ",", "man", "sieht", "es", ",", "und", "th\u00f6\u00b7richt", "sind", "lie\u00b7ben\u00b7de", "Her\u00b7zen", ".", "\u201c"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "$,", "PIS", "VVFIN", "PPER", "$,", "KON", "ADJD", "VAFIN", "ADJA", "NN", "$.", "$("], "meter": "-+--+--+--+--+-", "measure": "amphibrach.penta.plus"}}, "stanza.8": {"line.1": {"text": "Er spricht es und gehet des Wegs, doch der armen Marie", "tokens": ["Er", "spricht", "es", "und", "ge\u00b7het", "des", "Wegs", ",", "doch", "der", "ar\u00b7men", "Ma\u00b7rie"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "KON", "VVFIN", "ART", "NN", "$,", "ADV", "ART", "ADJA", "NE"], "meter": "-+--+--+--+--+", "measure": "amphibrach.penta.plus"}, "line.2": {"text": "Erstarret das Herz, ihr brechen die bebenden Kniee.", "tokens": ["Er\u00b7star\u00b7ret", "das", "Herz", ",", "ihr", "bre\u00b7chen", "die", "be\u00b7ben\u00b7den", "Kni\u00b7ee", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Die Sprache verloren, Gef\u00fchl und Besinnung geschwunden,", "tokens": ["Die", "Spra\u00b7che", "ver\u00b7lo\u00b7ren", ",", "Ge\u00b7f\u00fchl", "und", "Be\u00b7sin\u00b7nung", "ge\u00b7schwun\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+--+--+--+--+-", "measure": "amphibrach.penta.plus"}, "line.4": {"text": "So wird sie, die M\u00e4hderin, dort in den Mahden gefunden.", "tokens": ["So", "wird", "sie", ",", "die", "M\u00e4h\u00b7de\u00b7rin", ",", "dort", "in", "den", "Mah\u00b7den", "ge\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "ART", "NN", "$,", "ADV", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+--+--+--+--+-", "measure": "amphibrach.penta.plus"}}, "stanza.9": {"line.1": {"text": "So lebt sie noch Jahre, so stummer, erstorbener Weise,", "tokens": ["So", "lebt", "sie", "noch", "Jah\u00b7re", ",", "so", "stum\u00b7mer", ",", "ers\u00b7tor\u00b7be\u00b7ner", "Wei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "NN", "$,", "ADV", "ADJD", "$,", "ADJA", "NN", "$,"], "meter": "-+--+--+-+-+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Und Honig, ein Tropfen, das ist ihr die einzige Speise.", "tokens": ["Und", "Ho\u00b7nig", ",", "ein", "Trop\u00b7fen", ",", "das", "ist", "ihr", "die", "ein\u00b7zi\u00b7ge", "Spei\u00b7se", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "ART", "NN", "$,", "PDS", "VAFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-+-+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "O haltet ein Grab ihr bereit auf der bl\u00fchendsten Wiese!", "tokens": ["O", "hal\u00b7tet", "ein", "Grab", "ihr", "be\u00b7reit", "auf", "der", "bl\u00fc\u00b7hends\u00b7ten", "Wie\u00b7se", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "PPER", "ADJD", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--++-+-", "measure"