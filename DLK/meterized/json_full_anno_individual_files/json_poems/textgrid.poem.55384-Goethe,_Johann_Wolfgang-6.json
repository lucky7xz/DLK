{"textgrid.poem.55384": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "6", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie saugt mit Gier verr\u00e4trisches Getr\u00e4nke", "tokens": ["Sie", "saugt", "mit", "Gier", "ver\u00b7r\u00e4t\u00b7ri\u00b7sches", "Ge\u00b7tr\u00e4n\u00b7ke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Unabgesetzt, vom ersten Zug verf\u00fchrt;", "tokens": ["Un\u00b7ab\u00b7ge\u00b7setzt", ",", "vom", "ers\u00b7ten", "Zug", "ver\u00b7f\u00fchrt", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sie f\u00fchlt sich wohl, und l\u00e4ngst sind die Gelenke", "tokens": ["Sie", "f\u00fchlt", "sich", "wohl", ",", "und", "l\u00e4ngst", "sind", "die", "Ge\u00b7len\u00b7ke"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "$,", "KON", "ADV", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der zarten Beinchen schon paralysiert,", "tokens": ["Der", "zar\u00b7ten", "Be\u00b7in\u00b7chen", "schon", "pa\u00b7ra\u00b7ly\u00b7siert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVPP", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Nicht mehr gewandt, die Fl\u00fcgelchen zu putzen,", "tokens": ["Nicht", "mehr", "ge\u00b7wandt", ",", "die", "Fl\u00fc\u00b7gel\u00b7chen", "zu", "put\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "VVPP", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Nicht mehr geschickt, das K\u00f6pfchen aufzustutzen,", "tokens": ["Nicht", "mehr", "ge\u00b7schickt", ",", "das", "K\u00f6pf\u00b7chen", "auf\u00b7zu\u00b7stut\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "VVPP", "$,", "ART", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Das Leben so sich im Genu\u00df verliert.", "tokens": ["Das", "Le\u00b7ben", "so", "sich", "im", "Ge\u00b7nu\u00df", "ver\u00b7liert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PRF", "APPRART", "NN", "VVFIN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "Zum Stehen kaum wird noch das F\u00fc\u00dfchen taugen;", "tokens": ["Zum", "Ste\u00b7hen", "kaum", "wird", "noch", "das", "F\u00fc\u00df\u00b7chen", "tau\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VAFIN", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "So schl\u00fcrft sie fort, und mitten unterm Saugen", "tokens": ["So", "schl\u00fcrft", "sie", "fort", ",", "und", "mit\u00b7ten", "un\u00b7term", "Sau\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,", "KON", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Umnebelt ihr der Tod die tausend Augen.", "tokens": ["Um\u00b7ne\u00b7belt", "ihr", "der", "Tod", "die", "tau\u00b7send", "Au\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punc