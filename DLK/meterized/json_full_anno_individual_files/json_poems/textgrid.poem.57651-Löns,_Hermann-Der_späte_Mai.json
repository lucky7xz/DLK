{"textgrid.poem.57651": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Der sp\u00e4te Mai", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die roten Bl\u00e4tter rauschen,", "tokens": ["Die", "ro\u00b7ten", "Bl\u00e4t\u00b7ter", "rau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Sommer ist lange vorbei,", "tokens": ["Der", "Som\u00b7mer", "ist", "lan\u00b7ge", "vor\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "PTKVZ", "$,"], "meter": "-+--+---", "measure": "iambic.di.relaxed"}, "line.3": {"text": "Es leuchten unsere Augen,", "tokens": ["Es", "leuch\u00b7ten", "un\u00b7se\u00b7re", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Es bl\u00fcht in uns der Mai.", "tokens": ["Es", "bl\u00fcht", "in", "uns", "der", "Mai", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wir k\u00f6nnen die Liebe nicht bergen,", "tokens": ["Wir", "k\u00f6n\u00b7nen", "die", "Lie\u00b7be", "nicht", "ber\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wir sind uns viel zu gut,", "tokens": ["Wir", "sind", "uns", "viel", "zu", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punc