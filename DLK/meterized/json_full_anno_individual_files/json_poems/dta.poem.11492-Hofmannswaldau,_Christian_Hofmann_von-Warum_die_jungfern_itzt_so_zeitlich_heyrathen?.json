{"dta.poem.11492": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Warum die jungfern itzt so zeitlich  \n heyrathen?", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der jungfer-krantz ist schwer: und bricht das alter ein,", "tokens": ["Der", "jung\u00b7fer\u00b7krantz", "ist", "schwer", ":", "und", "bricht", "das", "al\u00b7ter", "ein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$.", "KON", "VVFIN", "ART", "ADJA", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So mu\u00df das arme ding verlegne waare seyn;", "tokens": ["So", "mu\u00df", "das", "ar\u00b7me", "ding", "ver\u00b7leg\u00b7ne", "waa\u00b7re", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "ADJA", "NN", "ADJA", "ADJA", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Welch kluger wolte nun die damen itzt verdencken,", "tokens": ["Welch", "klu\u00b7ger", "wol\u00b7te", "nun", "die", "da\u00b7men", "itzt", "ver\u00b7den\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "VMFIN", "ADV", "ART", "ADJA", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df sie den jungfer-krantz bald an den nagel hencken?", "tokens": ["Da\u00df", "sie", "den", "jung\u00b7fer\u00b7krantz", "bald", "an", "den", "na\u00b7gel", "hen\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}