{"dta.poem.11317": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Grabschrifft eines gehangenen diebes.  \n A. C. T.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die parcen zohen mich viel andern menschen f\u00fcr,", "tokens": ["Die", "par\u00b7cen", "zo\u00b7hen", "mich", "viel", "an\u00b7dern", "men\u00b7schen", "f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PIAT", "ADJA", "NN", "APPR", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie spannen einen strick statt eines fadens mir:", "tokens": ["Sie", "span\u00b7nen", "ei\u00b7nen", "strick", "statt", "ei\u00b7nes", "fa\u00b7dens", "mir", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Zur ehre hat man mich am hals damit gezieret;", "tokens": ["Zur", "eh\u00b7re", "hat", "man", "mich", "am", "hals", "da\u00b7mit", "ge\u00b7zie\u00b7ret", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "VVFIN", "VAFIN", "PIS", "PRF", "PTKA", "ADJD", "PAV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "H\u00e4tt\u2019 ich zur danckbarkeit doch ihre scheer entf\u00fchret!", "tokens": ["H\u00e4tt'", "ich", "zur", "dan\u00b7ck\u00b7bar\u00b7keit", "doch", "ih\u00b7re", "scheer", "ent\u00b7f\u00fch\u00b7ret", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "ADV", "PPOSAT", "ADJD", "VVFIN", "$."], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}}}}}