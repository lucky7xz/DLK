{"dta.poem.13775": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "An die Meisten.  \n 1810.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Wie so lustig, ihr Poeten,", "tokens": ["Wie", "so", "lus\u00b7tig", ",", "ihr", "Po\u00b7et\u00b7en", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADJD", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "An den blumenreichen Hagen", "tokens": ["An", "den", "blu\u00b7men\u00b7rei\u00b7chen", "Ha\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "In dem Abendgold zu fl\u00f6ten,", "tokens": ["In", "dem", "A\u00b7bend\u00b7gold", "zu", "fl\u00f6\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Quellen, Nymphen nachzujagen!", "tokens": ["Quel\u00b7len", ",", "Nym\u00b7phen", "nach\u00b7zu\u00b7ja\u00b7gen", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wenn erst muth'ge Sch\u00fcsse fallen,", "tokens": ["Wenn", "erst", "muth'\u00b7ge", "Sch\u00fcs\u00b7se", "fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Von den sch\u00f6nen Wiederhallen", "tokens": ["Von", "den", "sch\u00f6\u00b7nen", "Wie\u00b7der\u00b7hal\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "La\u00dft ihr zart Sonnette schallen.", "tokens": ["La\u00dft", "ihr", "zart", "Son\u00b7net\u00b7te", "schal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJD", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}