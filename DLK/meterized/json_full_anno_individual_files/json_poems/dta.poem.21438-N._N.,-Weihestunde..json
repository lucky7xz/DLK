{"dta.poem.21438": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Weihestunde.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "O Weihestunde!", "tokens": ["O", "Wei\u00b7hes\u00b7tun\u00b7de", "!"], "token_info": ["word", "word", "punct"], "pos": ["NE", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "O k\u00f6stliche Stunde!", "tokens": ["O", "k\u00f6st\u00b7li\u00b7che", "Stun\u00b7de", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Sanft k\u00fc\u00dft die Nacht,", "tokens": ["Sanft", "k\u00fc\u00dft", "die", "Nacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Die vielholde Tr\u00f6sterin,", "tokens": ["Die", "viel\u00b7hol\u00b7de", "Tr\u00f6s\u00b7te\u00b7rin", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Die tagm\u00fcde Erde.", "tokens": ["Die", "tag\u00b7m\u00fc\u00b7de", "Er\u00b7de", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Und m\u00e4hlich verhallen", "tokens": ["Und", "m\u00e4h\u00b7lich", "ver\u00b7hal\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJD", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Im ewigen Schweigen", "tokens": ["Im", "e\u00b7wi\u00b7gen", "Schwei\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Die Stimmen des Lebens \u2026", "tokens": ["Die", "Stim\u00b7men", "des", "Le\u00b7bens", "\u2026"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Immer lichter umwebt", "tokens": ["Im\u00b7mer", "lich\u00b7ter", "um\u00b7webt"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.10": {"text": "Die erstorbenen Auen,", "tokens": ["Die", "ers\u00b7tor\u00b7be\u00b7nen", "Au\u00b7en", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Des Mondes fluthender", "tokens": ["Des", "Mon\u00b7des", "flut\u00b7hen\u00b7der"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Silberschleier.", "tokens": ["Sil\u00b7ber\u00b7schlei\u00b7er", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.13": {"text": "Mild gr\u00fc\u00dfen hernieder", "tokens": ["Mild", "gr\u00fc\u00b7\u00dfen", "her\u00b7nie\u00b7der"], "token_info": ["word", "word", "word"], "pos": ["NN", "VVFIN", "PTKVZ"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.14": {"text": "Die ewigen Sterne \u2014", "tokens": ["Die", "e\u00b7wi\u00b7gen", "Ster\u00b7ne"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.15": {"text": "Lautlos wogt", "tokens": ["Laut\u00b7los", "wogt"], "token_info": ["word", "word"], "pos": ["ADJD", "VVFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.16": {"text": "Der wortlose Zauber", "tokens": ["Der", "wort\u00b7lo\u00b7se", "Zau\u00b7ber"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.17": {"text": "Unendlicher Ruhe.", "tokens": ["Un\u00b7end\u00b7li\u00b7cher", "Ru\u00b7he", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.18": {"text": "Nur manchmal", "tokens": ["Nur", "manch\u00b7mal"], "token_info": ["word", "word"], "pos": ["ADV", "ADV"], "meter": "-+-", "measure": "amphibrach.single"}, "line.19": {"text": "Fl\u00fcstert\u2019s und raunt\u2019s", "tokens": ["Fl\u00fcs\u00b7tert's", "und", "raunt's"], "token_info": ["word", "word", "word"], "pos": ["NE", "KON", "NE"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.20": {"text": "Im \u00fcppigen Laube;", "tokens": ["Im", "\u00fcp\u00b7pi\u00b7gen", "Lau\u00b7be", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.21": {"text": "Wie in Geisterumarmung", "tokens": ["Wie", "in", "Geis\u00b7te\u00b7rum\u00b7ar\u00b7mung"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "APPR", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.22": {"text": "Erschauern j\u00e4hlings", "tokens": ["Er\u00b7schau\u00b7ern", "j\u00e4h\u00b7lings"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.23": {"text": "Die B\u00e4ume und Str\u00e4ucher,", "tokens": ["Die", "B\u00e4u\u00b7me", "und", "Str\u00e4u\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.24": {"text": "Als wollten sie k\u00fcnden", "tokens": ["Als", "woll\u00b7ten", "sie", "k\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "VMFIN", "PPER", "VVFIN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.25": {"text": "Die ewigen R\u00e4thsel,", "tokens": ["Die", "e\u00b7wi\u00b7gen", "R\u00e4th\u00b7sel", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.26": {"text": "Die da walten von Urbeginn", "tokens": ["Die", "da", "wal\u00b7ten", "von", "Ur\u00b7be\u00b7ginn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "VVFIN", "APPR", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.27": {"text": "In H\u00f6hen und Tiefen \u2026", "tokens": ["In", "H\u00f6\u00b7hen", "und", "Tie\u00b7fen", "\u2026"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.28": {"text": "Wie Erl\u00f6sung umspinnt", "tokens": ["Wie", "Er\u00b7l\u00f6\u00b7sung", "um\u00b7spinnt"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "NN", "VVFIN"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.29": {"text": "Die quald\u00fcst\u2019ren Sinne", "tokens": ["Die", "qual\u00b7d\u00fcst'\u00b7ren", "Sin\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.30": {"text": "S\u00fc\u00dfes M\u00e4rchenvergessen.", "tokens": ["S\u00fc\u00b7\u00dfes", "M\u00e4r\u00b7chen\u00b7ver\u00b7ges\u00b7sen", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.31": {"text": "Eingewiegt von der Sph\u00e4ren", "tokens": ["Ein\u00b7ge\u00b7wiegt", "von", "der", "Sph\u00e4\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "APPR", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.32": {"text": "Leisrauschenden Hymnen,", "tokens": ["Leis\u00b7rau\u00b7schen\u00b7den", "Hym\u00b7nen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.33": {"text": "Umspielt vom Traumodem", "tokens": ["Um\u00b7spielt", "vom", "Trau\u00b7mo\u00b7dem"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "APPRART", "NN"], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.34": {"text": "Der wonnesam schlummernden", "tokens": ["Der", "won\u00b7ne\u00b7sam", "schlum\u00b7mern\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJD", "VVINF"], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.35": {"text": "Allmutter Natur", "tokens": ["A\u00b7llmut\u00b7ter", "Na\u00b7tur"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.36": {"text": "Trink\u2019 auch ich", "tokens": ["Trink'", "auch", "ich"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ADV", "PPER"], "meter": "+-+", "measure": "trochaic.di"}, "line.37": {"text": "Unaussprechlicher Inbrunst voll", "tokens": ["Un\u00b7aus\u00b7sprech\u00b7li\u00b7cher", "In\u00b7brunst", "voll"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "ADJD"], "meter": "+-+--++-", "measure": "trochaic.tetra.relaxed"}, "line.38": {"text": "Gottseligen Frieden,", "tokens": ["Gott\u00b7se\u00b7li\u00b7gen", "Frie\u00b7den", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$,"], "meter": "+---+-", "measure": "dactylic.init"}, "line.39": {"text": "Gl\u00fcck ohne Ende \u2026", "tokens": ["Gl\u00fcck", "oh\u00b7ne", "En\u00b7de", "\u2026"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.40": {"text": "In der Mainacht Duftthau.", "tokens": ["In", "der", "Mai\u00b7nacht", "Duftt\u00b7hau", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "--++-+", "measure": "anapaest.init"}, "line.41": {"text": "Im ewigen Hauche", "tokens": ["Im", "e\u00b7wi\u00b7gen", "Hau\u00b7che"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.42": {"text": "Ersterben des Leibes", "tokens": ["E\u00b7rster\u00b7ben", "des", "Lei\u00b7bes"], "token_info": ["word", "word", "word"], "pos": ["NN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.43": {"text": "Fiebernde Pulse.", "tokens": ["Fie\u00b7bern\u00b7de", "Pul\u00b7se", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.44": {"text": "Mit Sternen und Welten", "tokens": ["Mit", "Ster\u00b7nen", "und", "Wel\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.45": {"text": "Wall\u2019 ich entgegen", "tokens": ["Wall'", "ich", "ent\u00b7ge\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["NE", "PPER", "PTKVZ"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.46": {"text": "Dem d\u00e4mmernden Morgen.", "tokens": ["Dem", "d\u00e4m\u00b7mern\u00b7den", "Mor\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}}}}