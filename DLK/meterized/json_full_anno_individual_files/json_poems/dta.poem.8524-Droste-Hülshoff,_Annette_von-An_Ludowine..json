{"dta.poem.8524": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "An Ludowine.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1860", "urn": "urn:nbn:de:kobv:b4-200905191007", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was ist mehr denn Schmuck und Kleid?", "tokens": ["Was", "ist", "mehr", "denn", "Schmuck", "und", "Kleid", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "ADV", "NN", "KON", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "\u201eein g\u2019sunder Leib, so\u2019s in Freuden treit.\u201c", "tokens": ["\u201e", "ein", "g'\u00b7sun\u00b7der", "Leib", ",", "so's", "in", "Freu\u00b7den", "treit", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "ADJA", "NN", "$,", "PIS", "APPR", "NN", "NN", "$.", "$("], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Was ist mehr denn Gold so werth?", "tokens": ["Was", "ist", "mehr", "denn", "Gold", "so", "werth", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "KON", "NN", "ADV", "ADJD", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "\u201eein frei Gem\u00fcth, so des mit entbehrt.\u201c", "tokens": ["\u201e", "ein", "frei", "Ge\u00b7m\u00fcth", ",", "so", "des", "mit", "ent\u00b7behrt", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "ADJD", "NN", "$,", "ADV", "ART", "APPR", "VVPP", "$.", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Was ist mehr denn Kron\u2019 und Grund?", "tokens": ["Was", "ist", "mehr", "denn", "Kron'", "und", "Grund", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "ADV", "NN", "KON", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "\u201eein klug Gem\u00fcth, so des brauchen kunnt.\u201c", "tokens": ["\u201e", "ein", "klug", "Ge\u00b7m\u00fcth", ",", "so", "des", "brau\u00b7chen", "kunnt", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "ADJD", "NN", "$,", "ADV", "ART", "VVFIN", "PTKVZ", "$.", "$("], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.7": {"text": "Was ist mehr, denn gl\u00fcckselig sein?", "tokens": ["Was", "ist", "mehr", ",", "denn", "gl\u00fcck\u00b7se\u00b7lig", "sein", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "$,", "KON", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "\u201eein fein Gem\u00fcth, so des werth allein.\u201c", "tokens": ["\u201e", "ein", "fein", "Ge\u00b7m\u00fcth", ",", "so", "des", "werth", "al\u00b7lein", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "ADJD", "NN", "$,", "ADV", "ART", "ADJD", "ADV", "$.", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}}}}