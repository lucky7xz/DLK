{"textgrid.poem.40158": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich seh' in deine Augen wieder,", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich seh' in deine Augen wieder,", "tokens": ["Ich", "seh'", "in", "dei\u00b7ne", "Au\u00b7gen", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "so friedevoll, so still und tief;", "tokens": ["so", "frie\u00b7de\u00b7voll", ",", "so", "still", "und", "tief", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "da ", "tokens": ["da"], "token_info": ["word"], "pos": ["KOUS"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "die schrill in mir der Zweifel rief.", "tokens": ["die", "schrill", "in", "mir", "der", "Zwei\u00b7fel", "rief", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Du darfst den tr\u00fcben Wahnsinn ", "tokens": ["Du", "darfst", "den", "tr\u00fc\u00b7ben", "Wahn\u00b7sinn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "der gr\u00e4\u00dflich lacht in mir und schreit,", "tokens": ["der", "gr\u00e4\u00df\u00b7lich", "lacht", "in", "mir", "und", "schreit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART",