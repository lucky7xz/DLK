{"dta.poem.9286": {"metadata": {"author": {"name": "Weise, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Das vierdte Gespr\u00e4ch.", "genre": "Lyrik; Drama; Prosa", "period": "N.A.", "pub_year": "1701", "urn": "urn:nbn:de:kobv:b4-25043-0", "language": ["de:0.71", "nl:0.28"], "booktitle": "Weise, Christian: \u00dcberfl\u00fc\u00dfige Gedancken Der gr\u00fcnenden jugend. Leipzig, 1701."}, "poem": {"stanza.1": {"line.1": {"text": "Was machstu doch du seele meiner seele!", "tokens": ["Was", "machs\u00b7tu", "doch", "du", "see\u00b7le", "mei\u00b7ner", "see\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ach weistu nicht wie heftig ich mich qu\u00e4le", "tokens": ["Ach", "weis\u00b7tu", "nicht", "wie", "hef\u00b7tig", "ich", "mich", "qu\u00e4\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "VVFIN", "PTKNEG", "KOKOM", "ADJD", "PPER", "PRF", "NE"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nachdem ich dir den letzten abschieds-gru\u00df", "tokens": ["Nach\u00b7dem", "ich", "dir", "den", "letz\u00b7ten", "ab\u00b7schieds\u00b7gru\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Durch dieses lied betr\u00fcbt ertheilen mu\u00df.", "tokens": ["Durch", "die\u00b7ses", "lied", "be\u00b7tr\u00fcbt", "er\u00b7thei\u00b7len", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVPP", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "2. Ich bin zu schwach den himmel anzuhalten/", "tokens": ["Ich", "bin", "zu", "schwach", "den", "him\u00b7mel", "an\u00b7zu\u00b7hal\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKA", "ADJD", "ART", "NN", "VVIZU", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der mir das hertz im leibe wil zerspalten.", "tokens": ["Der", "mir", "das", "hertz", "im", "lei\u00b7be", "wil", "zer\u00b7spal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "APPRART", "VVFIN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Denn weil ich doch darzu versehen bin/", "tokens": ["Denn", "weil", "ich", "doch", "dar\u00b7zu", "ver\u00b7se\u00b7hen", "bin", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "PAV", "VVPP", "VAFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "So schick ich mich und zieh ins elend hin.", "tokens": ["So", "schick", "ich", "mich", "und", "zieh", "ins", "e\u00b7lend", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "KON", "VVFIN", "APPRART", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "3. Wiewol dein geist der soll mich stets begleiten\u0303/", "tokens": ["Wie\u00b7wol", "dein", "geist", "der", "soll", "mich", "stets", "be\u00b7glei\u00b7te\u00f1", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ART", "VMFIN", "PPER", "ADV", "ADJD", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und \u00fcber mich die liebes-fl\u00fcgel breiten/", "tokens": ["Und", "\u00fc\u00b7ber", "mich", "die", "lie\u00b7bes\u00b7fl\u00fc\u00b7gel", "brei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "ART", "NN", "ADJA", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Bi\u00df ich einmahl die sauer-s\u00fcsse bahn", "tokens": ["Bi\u00df", "ich", "ein\u00b7mahl", "die", "sau\u00b7e\u00b7rs\u00fcs\u00b7se", "bahn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Zu deiner zier zur\u00fccke wandern kan.", "tokens": ["Zu", "dei\u00b7ner", "zier", "zu\u00b7r\u00fc\u00b7cke", "wan\u00b7dern", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "VVFIN", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "4. Und ob ich schon von dir entfernet bleibe/", "tokens": ["Und", "ob", "ich", "schon", "von", "dir", "ent\u00b7fer\u00b7net", "blei\u00b7be", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "APPR", "PPER", "VVFIN", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So bin ich nur abwesend mit dem leibe/", "tokens": ["So", "bin", "ich", "nur", "ab\u00b7we\u00b7send", "mit", "dem", "lei\u00b7be", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADJD", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die seele setzt sich fest in deine brust/", "tokens": ["Die", "see\u00b7le", "setzt", "sich", "fest", "in", "dei\u00b7ne", "brust", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ADJD", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und nimmt den zoll von meiner s\u00fcssen lust.", "tokens": ["Und", "nimmt", "den", "zoll", "von", "mei\u00b7ner", "s\u00fcs\u00b7sen", "lust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJD", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "5. Nun liebstes kind was wilstu dich betr\u00fcben/", "tokens": ["Nun", "liebs\u00b7tes", "kind", "was", "wils\u00b7tu", "dich", "be\u00b7tr\u00fc\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "PWS", "VMFIN", "PRF", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich kan dich dort und allenthalben lieben/", "tokens": ["Ich", "kan", "dich", "dort", "und", "al\u00b7len\u00b7thal\u00b7ben", "lie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "KON", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Drum weine nicht/ und tr\u00f6ste deinen sinn/", "tokens": ["Drum", "wei\u00b7ne", "nicht", "/", "und", "tr\u00f6s\u00b7te", "dei\u00b7nen", "sinn", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PTKNEG", "$(", "KON", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Es ist genung da\u00df ich verlohren bin.", "tokens": ["Es", "ist", "ge\u00b7nung", "da\u00df", "ich", "ver\u00b7loh\u00b7ren", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "KOUS", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "6. Und sol ich dir ein festes merckmahl lassen/", "tokens": ["Und", "sol", "ich", "dir", "ein", "fes\u00b7tes", "merck\u00b7mahl", "las\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PPER", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So will ich dort auch alle Nymfen hassen/", "tokens": ["So", "will", "ich", "dort", "auch", "al\u00b7le", "Nym\u00b7fen", "has\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ADV", "PIAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da soll man sehn/ da\u00df meine liebes-treu/", "tokens": ["Da", "soll", "man", "sehn", "/", "da\u00df", "mei\u00b7ne", "lie\u00b7bes\u00b7treu", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "VVINF", "$(", "KOUS", "PPOSAT", "ADJD", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Nur dir mein kind/ sonst niemand offen sey.", "tokens": ["Nur", "dir", "mein", "kind", "/", "sonst", "nie\u00b7mand", "of\u00b7fen", "sey", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "PPOSAT", "NN", "$(", "ADV", "PIS", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "7. Zu guter nacht ich werde wieder kommen.", "tokens": ["Zu", "gu\u00b7ter", "nacht", "ich", "wer\u00b7de", "wie\u00b7der", "kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "VAFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch werd ich dort von schmertzen eingenommen/", "tokens": ["Doch", "werd", "ich", "dort", "von", "schmert\u00b7zen", "ein\u00b7ge\u00b7nom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "APPR", "ADJA", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df ich das grab daselbsten f\u00fcllen mu\u00df/", "tokens": ["Da\u00df", "ich", "das", "grab", "da\u00b7selbs\u00b7ten", "f\u00fcl\u00b7len", "mu\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVINF", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "So gib mir itzt den allerletzten ku\u00df.", "tokens": ["So", "gib", "mir", "itzt", "den", "al\u00b7ler\u00b7letz\u00b7ten", "ku\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}