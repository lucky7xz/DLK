{"dta.poem.15033": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Nachrichten von dem Ursprung und  \n Wachsthum der Critik bey den  \n Deutschen.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1741", "urn": "urn:nbn:de:kobv:b4-200905198526", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein halb mit Pikelschertz vermengtes Operettchen,", "tokens": ["Ein", "halb", "mit", "Pi\u00b7kel\u00b7schertz", "ver\u00b7meng\u00b7tes", "O\u00b7pe\u00b7rett\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein stinkender Roman von rasenden Chrysettchen,", "tokens": ["Ein", "stin\u00b7ken\u00b7der", "Ro\u00b7man", "von", "ra\u00b7sen\u00b7den", "Chry\u00b7sett\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Ein geiles Myrthenlied, und ein nach dem Adon", "tokens": ["Ein", "gei\u00b7les", "Myr\u00b7then\u00b7lied", ",", "und", "ein", "nach", "dem", "A\u00b7don"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "KON", "ART", "APPR", "ART", "NN"], "meter": "-+-+-+-+--++", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Des \u00fcppigen Marin erbauter Venusthron,", "tokens": ["Des", "\u00fcp\u00b7pi\u00b7gen", "Ma\u00b7rin", "er\u00b7bau\u00b7ter", "Ve\u00b7nus\u00b7thron", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der der Geliebten Schoos bis auf den Grund entdecket,", "tokens": ["Der", "der", "Ge\u00b7lieb\u00b7ten", "Schoos", "bis", "auf", "den", "Grund", "ent\u00b7de\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "NN", "APPR", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und B\u00fcsch und Br\u00fcnnen draus, und Vogelnester hecket", "tokens": ["Und", "B\u00fcsch", "und", "Br\u00fcn\u00b7nen", "draus", ",", "und", "Vo\u00b7gel\u00b7nes\u00b7ter", "he\u00b7cket"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN", "PAV", "$,", "KON", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "\u2012 \u2012 \u2012 \u2012 \u2012 \u2012 \u2012 \u2012", "tokens": ["\u2012", "\u2012", "\u2012", "\u2012", "\u2012", "\u2012", "\u2012", "\u2012"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "$(", "$(", "$(", "$(", "$(", "$(", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ein rohes Trauerspiel, in dem die Regeln fehlen,", "tokens": ["Ein", "ro\u00b7hes", "Trau\u00b7er\u00b7spiel", ",", "in", "dem", "die", "Re\u00b7geln", "feh\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Und so viel Schnizer fast, als Sylben sind, zu zehlen;", "tokens": ["Und", "so", "viel", "Sch\u00b7ni\u00b7zer", "fast", ",", "als", "Syl\u00b7ben", "sind", ",", "zu", "zeh\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "PIAT", "NN", "ADV", "$,", "KOUS", "NN", "VAFIN", "$,", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.10": {"text": "Ein Brief, den Adam schon der Eva zugesandt,", "tokens": ["Ein", "Brief", ",", "den", "A\u00b7dam", "schon", "der", "E\u00b7va", "zu\u00b7ge\u00b7sandt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Da beyde dazumahl doch keine Schrift gekandt;", "tokens": ["Da", "bey\u00b7de", "da\u00b7zu\u00b7mahl", "doch", "kei\u00b7ne", "Schrift", "ge\u00b7kandt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "ADV", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Ein kreissendes Sonnet, das mit dem Tode ringet,", "tokens": ["Ein", "kreis\u00b7sen\u00b7des", "Son\u00b7net", ",", "das", "mit", "dem", "To\u00b7de", "rin\u00b7get", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.13": {"text": "Und der Gedanken Rad, so wie die Reimen zwinget,", "tokens": ["Und", "der", "Ge\u00b7dan\u00b7ken", "Rad", ",", "so", "wie", "die", "Rei\u00b7men", "zwin\u00b7get", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "$,", "ADV", "KOKOM", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und ein nach P\u00f6belart gepriesner Buhlerblick", "tokens": ["Und", "ein", "nach", "P\u00f6\u00b7be\u00b7lart", "ge\u00b7pri\u00b7es\u00b7ner", "Buh\u00b7ler\u00b7blick"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "APPR", "NE", "ADJA", "NN"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.15": {"text": "Ist oft bey dieser Zeit das gr\u00f6ste Meisterst\u00fcck.", "tokens": ["Ist", "oft", "bey", "die\u00b7ser", "Zeit", "das", "gr\u00f6s\u00b7te", "Meis\u00b7ter\u00b7st\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "PDAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "So lang ich meinen Vers nach gleicher Art gewogen,", "tokens": ["So", "lang", "ich", "mei\u00b7nen", "Vers", "nach", "glei\u00b7cher", "Art", "ge\u00b7wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Dem Bilde der Natur die Schminke vorgezogen,", "tokens": ["Dem", "Bil\u00b7de", "der", "Na\u00b7tur", "die", "Schmin\u00b7ke", "vor\u00b7ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Der Reime d\u00fcrren Leib mit Purpur ausgeschm\u00fcckt,", "tokens": ["Der", "Rei\u00b7me", "d\u00fcr\u00b7ren", "Leib", "mit", "Pur\u00b7pur", "aus\u00b7ge\u00b7schm\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Und abgeborgte Kraft den W\u00f6rtern angeflickt;", "tokens": ["Und", "ab\u00b7ge\u00b7borg\u00b7te", "Kraft", "den", "W\u00f6r\u00b7tern", "an\u00b7ge\u00b7flickt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "So war ich auch ein Mann von hohen Dichtergaben;", "tokens": ["So", "war", "ich", "auch", "ein", "Mann", "von", "ho\u00b7hen", "Dich\u00b7ter\u00b7ga\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Allein sobald ich nur der Spure nachgegraben,", "tokens": ["Al\u00b7lein", "so\u00b7bald", "ich", "nur", "der", "Spu\u00b7re", "nach\u00b7ge\u00b7gra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "ADV", "ART", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Auf der man zur Vernunft besch\u00e4mt zurucke kehrt,", "tokens": ["Auf", "der", "man", "zur", "Ver\u00b7nunft", "be\u00b7sch\u00e4mt", "zu\u00b7ru\u00b7cke", "kehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PIS", "APPRART", "NN", "ADJD", "VVFIN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Und endlich nach und nach nur den Parna\u00df erreicht,", "tokens": ["Und", "end\u00b7lich", "nach", "und", "nach", "nur", "den", "Par\u00b7na\u00df", "er\u00b7reicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "KON", "APPR", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.24": {"text": "So ist es aus mit mir.", "tokens": ["So", "ist", "es", "aus", "mit", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}