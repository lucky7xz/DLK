{"dta.poem.23399": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Wiegenlied.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Schlafe, schlafe, schlafe, schlafe!", "tokens": ["Schla\u00b7fe", ",", "schla\u00b7fe", ",", "schla\u00b7fe", ",", "schla\u00b7fe", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wozu willst du wach noch sein?", "tokens": ["Wo\u00b7zu", "willst", "du", "wach", "noch", "sein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "VVFIN", "ADV", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn die Welt ist voller Schafe,", "tokens": ["Denn", "die", "Welt", "ist", "vol\u00b7ler", "Scha\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "B\u00f6cke, Sch\u00f6ps' und L\u00e4mmelein.", "tokens": ["B\u00f6\u00b7cke", ",", "Sch\u00f6ps'", "und", "L\u00e4m\u00b7mel\u00b7ein", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Schlafe, schlafe! bleib doch l\u00e4nger", "tokens": ["Schla\u00b7fe", ",", "schla\u00b7fe", "!", "bleib", "doch", "l\u00e4n\u00b7ger"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "$.", "VVFIN", "ADV", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Noch in deiner Ruh' und Rast!", "tokens": ["Noch", "in", "dei\u00b7ner", "Ruh'", "und", "Rast", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schafe sind die besten S\u00e4nger", "tokens": ["Scha\u00b7fe", "sind", "die", "bes\u00b7ten", "S\u00e4n\u00b7ger"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In der H\u00fctt' und im Palast.", "tokens": ["In", "der", "H\u00fctt'", "und", "im", "Pa\u00b7last", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wenn die frommen Schafe singen", "tokens": ["Wenn", "die", "from\u00b7men", "Scha\u00b7fe", "sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ihre s\u00fc\u00dfen Melodein,", "tokens": ["Ih\u00b7re", "s\u00fc\u00b7\u00dfen", "Me\u00b7lo\u00b7dein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "O so h\u00fcpfen, tanzen, springen", "tokens": ["O", "so", "h\u00fcp\u00b7fen", ",", "tan\u00b7zen", ",", "sprin\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["NE", "ADV", "VVINF", "$,", "VVFIN", "$,", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Alle lieben L\u00e4mmelein.", "tokens": ["Al\u00b7le", "lie\u00b7ben", "L\u00e4m\u00b7mel\u00b7ein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Schlafe! denn du kannst nicht werden", "tokens": ["Schla\u00b7fe", "!", "denn", "du", "kannst", "nicht", "wer\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "KON", "PPER", "VMFIN", "PTKNEG", "VAINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "So ein gutes frommes Vieh;", "tokens": ["So", "ein", "gu\u00b7tes", "from\u00b7mes", "Vieh", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schlafe! denn es gilt auf Erden", "tokens": ["Schla\u00b7fe", "!", "denn", "es", "gilt", "auf", "Er\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "KON", "PPER", "VVFIN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nur die L\u00e4mmelpoesie.", "tokens": ["Nur", "die", "L\u00e4m\u00b7mel\u00b7po\u00b7e\u00b7sie", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}