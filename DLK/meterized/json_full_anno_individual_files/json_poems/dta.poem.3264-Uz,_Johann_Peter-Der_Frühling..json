ON", "NE", "VVFIN", "PPER", "$.", "PPER", "VVFIN", "APPRART", "NN", "TRUNC"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "r\u00fcstet,", "tokens": ["r\u00fcs\u00b7tet", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Und hat die blutige Sehne gespannt.", "tokens": ["Und", "hat", "die", "blu\u00b7ti\u00b7ge", "Seh\u00b7ne", "ge\u00b7spannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wie will ich seine Gewalt, bey frohem Weine, besingen,", "tokens": ["Wie", "will", "ich", "sei\u00b7ne", "Ge\u00b7walt", ",", "bey", "fro\u00b7hem", "Wei\u00b7ne", ",", "be\u00b7sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "PPOSAT", "NN", "$,", "APPR", "ADJA", "NN", "$,", "VVPP", "$,"], "meter": "-+-+--+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Wann du einst seine Triumphe gemehrt!", "tokens": ["Wann", "du", "einst", "sei\u00b7ne", "Tri\u00b7um\u00b7phe", "ge\u00b7mehrt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}