{"dta.poem.3264": {"metadata": {"author": {"name": "Uz, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "Der Fr\u00fchling.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1755", "urn": "urn:nbn:de:kobv:b4-20614-6", "language": ["de:0.99"], "booktitle": "Uz, Johann Peter: Lyrische und andere Gedichte. 2. Aufl. Ansbach, 1755."}, "poem": {"stanza.1": {"line.1": {"text": "Ich will, vom Weine berauscht, die Lust der Erde", "tokens": ["Ich", "will", ",", "vom", "Wei\u00b7ne", "be\u00b7rauscht", ",", "die", "Lust", "der", "Er\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "$,", "APPRART", "NN", "VVPP", "$,", "ART", "NN", "ART", "NN"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "besingen,", "tokens": ["be\u00b7sin\u00b7gen", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Jhr Sch\u00f6nen! eure gef\u00e4hrliche Lust,", "tokens": ["Ihr", "Sch\u00f6\u00b7nen", "!", "eu\u00b7re", "ge\u00b7f\u00e4hr\u00b7li\u00b7che", "Lust", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Den Fr\u00fchling, welcher anitzt, durch Florens H\u00e4nde be-", "tokens": ["Den", "Fr\u00fch\u00b7ling", ",", "wel\u00b7cher", "a\u00b7nitzt", ",", "durch", "Flo\u00b7rens", "H\u00e4n\u00b7de", "be"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "$,", "APPR", "NN", "NN", "TRUNC"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "kr\u00e4nzet,", "tokens": ["kr\u00e4n\u00b7zet", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.6": {"text": "Siegprangend unsre Gefilde beherrscht.", "tokens": ["Sieg\u00b7pran\u00b7gend", "uns\u00b7re", "Ge\u00b7fil\u00b7de", "be\u00b7herrscht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Fangt an! ich gl\u00fche bereits; fangt an, holdselige Saiten!", "tokens": ["Fangt", "an", "!", "ich", "gl\u00fc\u00b7he", "be\u00b7reits", ";", "fangt", "an", ",", "hold\u00b7se\u00b7li\u00b7ge", "Sai\u00b7ten", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$.", "PPER", "VVFIN", "ADV", "$.", "VVFIN", "PTKVZ", "$,", "ADJA", "NN", "$."], "meter": "+--+--+--+---+-", "measure": "dactylic.tetra.plus"}, "line.2": {"text": "Entz\u00fcckt der Eccho begieriges Ohr!", "tokens": ["Ent\u00b7z\u00fcckt", "der", "Ec\u00b7cho", "be\u00b7gie\u00b7ri\u00b7ges", "Ohr", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "T\u00f6nt sanft durchs ruhige Thal! da lauschen furchtsame", "tokens": ["T\u00f6nt", "sanft", "durchs", "ru\u00b7hi\u00b7ge", "Thal", "!", "da", "lau\u00b7schen", "furcht\u00b7sa\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "APPRART", "ADJA", "NN", "$.", "ADV", "ADJA", "NN"], "meter": "-+-+--+-+-+--", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Nymphen,", "tokens": ["Nym\u00b7phen", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Nur halb durch junge Gestr\u00e4uche bedeckt.", "tokens": ["Nur", "halb", "durch", "jun\u00b7ge", "Ge\u00b7str\u00e4u\u00b7che", "be\u00b7deckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Wer kommt vom H\u00fcgel herab, voll unaussprechlicher", "tokens": ["Wer", "kommt", "vom", "H\u00fc\u00b7gel", "her\u00b7ab", ",", "voll", "un\u00b7aus\u00b7sprech\u00b7li\u00b7cher"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWS", "VVFIN", "APPRART", "NN", "ADV", "$,", "ADJD", "ADJA"], "meter": "-+-+--+-+-+--", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Anmuth,", "tokens": ["An\u00b7muth", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Dem Glanz die fr\u00f6hlige Stirne bestrahlt,", "tokens": ["Dem", "Glanz", "die", "fr\u00f6h\u00b7li\u00b7ge", "Stir\u00b7ne", "be\u00b7strahlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Den Philomele begr\u00fc\u00dft? Jhm d\u00fcften fr\u00fche Violen;", "tokens": ["Den", "Phi\u00b7lo\u00b7me\u00b7le", "be\u00b7gr\u00fc\u00dft", "?", "Jhm", "d\u00fcf\u00b7ten", "fr\u00fc\u00b7he", "Vi\u00b7o\u00b7len", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$.", "PPER", "VVFIN", "ADJA", "NN", "$."], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.5": {"text": "Jhm gr\u00fcnt der Erde beschattete Schoos.", "tokens": ["Jhm", "gr\u00fcnt", "der", "Er\u00b7de", "be\u00b7schat\u00b7te\u00b7te", "Schoos", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVFIN", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Wunsch meiner Muse, du kommst! O Fr\u00fchling,", "tokens": ["Wunsch", "mei\u00b7ner", "Mu\u00b7se", ",", "du", "kommst", "!", "O", "Fr\u00fch\u00b7ling", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "$.", "NE", "NE", "$,"], "meter": "+---+-+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Wonne Dionens,", "tokens": ["Won\u00b7ne", "Di\u00b7o\u00b7nens", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "NE", "$,"], "meter": "+---+", "measure": "dactylic.init"}, "line.3": {"text": "Du kommst, vom feurigen Amor umarmt!", "tokens": ["Du", "kommst", ",", "vom", "feu\u00b7ri\u00b7gen", "A\u00b7mor", "um\u00b7armt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "APPRART", "ADJA", "NE", "VVPP", "$."], "meter": "-+-+---+-+", "measure": "zehnsilber"}, "line.4": {"text": "Und Amors muthige Faust schwingt siegbegierige Pfeile:", "tokens": ["Und", "A\u00b7mors", "mut\u00b7hi\u00b7ge", "Faust", "schwingt", "sieg\u00b7be\u00b7gie\u00b7ri\u00b7ge", "Pfei\u00b7le", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "ADJA", "NN", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+--+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Die stolzen Sterblichen huldigen ihm.", "tokens": ["Die", "stol\u00b7zen", "Sterb\u00b7li\u00b7chen", "hul\u00b7di\u00b7gen", "ihm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+--++-+", "measure": "iambic.penta.relaxed"}}, "stanza.5": {"line.1": {"text": "Ein Schwarm der Freuden ereilt vor dir muthwillige", "tokens": ["Ein", "Schwarm", "der", "Freu\u00b7den", "er\u00b7eilt", "vor", "dir", "mut\u00b7hwil\u00b7li\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "VVPP", "APPR", "PPER", "ADJA"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Weste,", "tokens": ["Wes\u00b7te", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "In T\u00e4nzen, welche die Fl\u00f6te belebt:", "tokens": ["In", "T\u00e4n\u00b7zen", ",", "wel\u00b7che", "die", "Fl\u00f6\u00b7te", "be\u00b7lebt", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "ART", "NN", "VVPP", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Vor dir scherzt Hebe dahin: es lachen lauere L\u00fcfte", "tokens": ["Vor", "dir", "scherzt", "He\u00b7be", "da\u00b7hin", ":", "es", "la\u00b7chen", "lau\u00b7e\u00b7re", "L\u00fcf\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VVFIN", "NE", "PAV", "$.", "PPER", "VVINF", "ADJA", "NN"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.5": {"text": "Dich, Kind der Sonne! gef\u00e4lliger an.", "tokens": ["Dich", ",", "Kind", "der", "Son\u00b7ne", "!", "ge\u00b7f\u00e4l\u00b7li\u00b7ger", "an", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "ART", "NN", "$.", "ADJD", "PTKVZ", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Durchzeuch nicht l\u00e4nger, o Nord! verheerend unsre Gefilde!", "tokens": ["Durch\u00b7zeuch", "nicht", "l\u00e4n\u00b7ger", ",", "o", "Nord", "!", "ver\u00b7hee\u00b7rend", "uns\u00b7re", "Ge\u00b7fil\u00b7de", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ADJD", "$,", "FM", "NE", "$.", "ADJD", "PPOSAT", "NN", "$."], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Entfleuch nach ewigem Eise zur\u00fcck:", "tokens": ["Ent\u00b7fleuch", "nach", "e\u00b7wi\u00b7gem", "Ei\u00b7se", "zu\u00b7r\u00fcck", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "---+--+--+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Weil nun der sch\u00f6nere Lenz, den Zephyrs Fittige k\u00fchlen,", "tokens": ["Weil", "nun", "der", "sch\u00f6\u00b7ne\u00b7re", "Lenz", ",", "den", "Ze\u00b7phyrs", "Fit\u00b7ti\u00b7ge", "k\u00fch\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "$,", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+--+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Siegprangend unsre Gefilde beherrscht!", "tokens": ["Sieg\u00b7pran\u00b7gend", "uns\u00b7re", "Ge\u00b7fil\u00b7de", "be\u00b7herrscht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "Sie bl\u00fchn, vom Thaue beperlt, und Anmuth lachet", "tokens": ["Sie", "bl\u00fchn", ",", "vom", "Thau\u00b7e", "be\u00b7perlt", ",", "und", "An\u00b7muth", "la\u00b7chet"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "APPRART", "NN", "VVFIN", "$,", "KON", "NN", "VVFIN"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "in allen;", "tokens": ["in", "al\u00b7len", ";"], "token_info": ["word", "word", "punct"], "pos": ["APPR", "PIAT", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Es lacht die ganze smaragdene Flur,", "tokens": ["Es", "lacht", "die", "gan\u00b7ze", "sma\u00b7rag\u00b7de\u00b7ne", "Flur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "In deren Arme so oft, bey frischer B\u00e4che Geschw\u00e4tze,", "tokens": ["In", "de\u00b7ren", "Ar\u00b7me", "so", "oft", ",", "bey", "fri\u00b7scher", "B\u00e4\u00b7che", "Ge\u00b7schw\u00e4t\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "ADV", "ADV", "$,", "APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+--+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Der Schlaf mein williges Auge beschleicht.", "tokens": ["Der", "Schlaf", "mein", "wil\u00b7li\u00b7ges", "Au\u00b7ge", "be\u00b7schleicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.8": {"line.1": {"text": "Berg, Thal und Aue bes\u00e4t der Bluhmen pr\u00e4chtige", "tokens": ["Berg", ",", "Thal", "und", "Au\u00b7e", "be\u00b7s\u00e4t", "der", "Bluh\u00b7men", "pr\u00e4ch\u00b7ti\u00b7ge"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "NN", "KON", "NN", "VVFIN", "ART", "NN", "ADJA"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Menge:", "tokens": ["Men\u00b7ge", ":"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Voll Stolz auf ihre beliebte Gestalt,", "tokens": ["Voll", "Stolz", "auf", "ih\u00b7re", "be\u00b7lieb\u00b7te", "Ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "B\u00fcckt sich doch iede daselbst vor dir, du Bluhme Ly\u00e4ens,", "tokens": ["B\u00fcckt", "sich", "doch", "ie\u00b7de", "da\u00b7selbst", "vor", "dir", ",", "du", "Bluh\u00b7me", "Ly\u00b7\u00e4\u00b7ens", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "PIS", "PAV", "APPR", "PPER", "$,", "PPER", "NN", "NE", "$,"], "meter": "+--+--+-+-+--++", "measure": "dactylic.di.plus"}, "line.5": {"text": "Die s\u00fcssem Scherze geheiliget ist!", "tokens": ["Die", "s\u00fcs\u00b7sem", "Scher\u00b7ze", "ge\u00b7hei\u00b7li\u00b7get", "ist", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.9": {"line.1": {"text": "Schm\u00fcck itzt mein finsteres Haar! Wenn du mich, Rose!", "tokens": ["Schm\u00fcck", "itzt", "mein", "fins\u00b7te\u00b7res", "Haar", "!", "Wenn", "du", "mich", ",", "Ro\u00b7se", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "ADV", "PPOSAT", "ADJA", "NN", "$.", "KOUS", "PPER", "PRF", "$,", "NE", "$."], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "bekr\u00e4nzest,", "tokens": ["be\u00b7kr\u00e4n\u00b7zest", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Und Bacchus meine Ges\u00e4nge beseelt:", "tokens": ["Und", "Bac\u00b7chus", "mei\u00b7ne", "Ge\u00b7s\u00e4n\u00b7ge", "be\u00b7seelt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Flieht schnell mein trauriger Ernst; da klingt die Laute", "tokens": ["Flieht", "schnell", "mein", "trau\u00b7ri\u00b7ger", "Ernst", ";", "da", "klingt", "die", "Lau\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "PPOSAT", "ADJA", "NN", "$.", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "bezaubernd", "tokens": ["be\u00b7zau\u00b7bernd"], "token_info": ["word"], "pos": ["VVPP"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "In meiner Muse gesch\u00e4ftigen Hand.", "tokens": ["In", "mei\u00b7ner", "Mu\u00b7se", "ge\u00b7sch\u00e4f\u00b7ti\u00b7gen", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.10": {"line.1": {"text": "Sie selbst auch werde bekr\u00e4nzt, die nicht mehr schl\u00e4f-", "tokens": ["Sie", "selbst", "auch", "wer\u00b7de", "be\u00b7kr\u00e4nzt", ",", "die", "nicht", "mehr", "schl\u00e4f"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "ADV", "VAFIN", "VVPP", "$,", "PRELS", "PTKNEG", "ADV", "TRUNC"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "rige Laute:", "tokens": ["ri\u00b7ge", "Lau\u00b7te", ":"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "--+-", "measure": "anapaest.init"}, "line.3": {"text": "Denn itzt (willkommen o liebliche Zeit!)", "tokens": ["Denn", "itzt", "(", "will\u00b7kom\u00b7men", "o", "lieb\u00b7li\u00b7che", "Zeit", "!", ")"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADV", "$(", "FM", "FM", "ADJA", "NN", "$.", "$("], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Erwacht der frohe Gesang, und ied\u2019 entschlafene Cyther", "tokens": ["Er\u00b7wacht", "der", "fro\u00b7he", "Ge\u00b7sang", ",", "und", "ied'", "ent\u00b7schla\u00b7fe\u00b7ne", "Cy\u00b7ther"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN", "$,", "KON", "PIAT", "ADJA", "NN"], "meter": "-+-+--+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Ist auf erhabnere T\u00f6ne bedacht:", "tokens": ["Ist", "auf", "er\u00b7hab\u00b7ne\u00b7re", "T\u00f6\u00b7ne", "be\u00b7dacht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.11": {"line.1": {"text": "Und auch die ganze Natur f\u00fchlt sich aufs neue begeistert,", "tokens": ["Und", "auch", "die", "gan\u00b7ze", "Na\u00b7tur", "f\u00fchlt", "sich", "aufs", "neu\u00b7e", "be\u00b7geis\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "VVFIN", "PRF", "APPRART", "ADJA", "VVPP", "$,"], "meter": "-+-+---+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Da sich die Sonne der Erde genaht;", "tokens": ["Da", "sich", "die", "Son\u00b7ne", "der", "Er\u00b7de", "ge\u00b7naht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und iedes frostige Thal, so Wald, als gr\u00fcne Geb\u00fcrge", "tokens": ["Und", "ie\u00b7des", "fros\u00b7ti\u00b7ge", "Thal", ",", "so", "Wald", ",", "als", "gr\u00fc\u00b7ne", "Ge\u00b7b\u00fcr\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PIAT", "ADJA", "NN", "$,", "ADV", "NN", "$,", "KOUS", "ADJA", "NN"], "meter": "-+-+--+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Sind reg, und alle Gefilde belebt.", "tokens": ["Sind", "reg", ",", "und", "al\u00b7le", "Ge\u00b7fil\u00b7de", "be\u00b7lebt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$,", "KON", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.12": {"line.1": {"text": "Drum ist die Stille geflohn, auch aus dem heiligen", "tokens": ["Drum", "ist", "die", "Stil\u00b7le", "ge\u00b7flohn", ",", "auch", "aus", "dem", "hei\u00b7li\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PAV", "VAFIN", "ART", "NN", "VVPP", "$,", "ADV", "APPR", "ART", "ADJA"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Hayne;", "tokens": ["Hay\u00b7ne", ";"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Der L\u00e4rm regieret im heiligen Hayn:", "tokens": ["Der", "L\u00e4rm", "re\u00b7gie\u00b7ret", "im", "hei\u00b7li\u00b7gen", "Hayn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Bald rauscht ein fr\u00f6hliger Hirsch, der sich im Flusse ge-", "tokens": ["Bald", "rauscht", "ein", "fr\u00f6h\u00b7li\u00b7ger", "Hirsch", ",", "der", "sich", "im", "Flus\u00b7se", "ge"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,", "PRELS", "PRF", "APPRART", "NN", "TRUNC"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "badet,", "tokens": ["ba\u00b7det", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.6": {"text": "Durch frischbethaute Geb\u00fcsche zur\u00fcck:", "tokens": ["Durch", "frischbe\u00b7thau\u00b7te", "Ge\u00