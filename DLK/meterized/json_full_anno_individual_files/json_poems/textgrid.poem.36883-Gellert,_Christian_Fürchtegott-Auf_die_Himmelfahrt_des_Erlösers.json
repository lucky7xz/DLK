{"textgrid.poem.36883": {"metadata": {"author": {"name": "Gellert, Christian F\u00fcrchtegott", "birth": "N.A.", "death": "N.A."}, "title": "Auf die Himmelfahrt des Erl\u00f6sers", "genre": "verse", "period": "N.A.", "pub_year": 1742, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Jauchzt, ihr Erl\u00f6sten, dem Herrn! Er hat sein Werk vollendet;", "tokens": ["Jauchzt", ",", "ihr", "Er\u00b7l\u00f6s\u00b7ten", ",", "dem", "Herrn", "!", "Er", "hat", "sein", "Werk", "voll\u00b7en\u00b7det", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPOSAT", "NN", "$,", "ART", "NN", "$.", "PPER", "VAFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Des m\u00fcsse sich der Erdkreis freun!", "tokens": ["Des", "m\u00fcs\u00b7se", "sich", "der", "Erd\u00b7kreis", "freun", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VMFIN", "PRF", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er f\u00e4hrt verkl\u00e4ret hinauf zu dem, der ihn gesendet,", "tokens": ["Er", "f\u00e4hrt", "ver\u00b7kl\u00e4\u00b7ret", "hin\u00b7auf", "zu", "dem", ",", "der", "ihn", "ge\u00b7sen\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VVFIN", "ADV", "APPR", "ART", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und nimmt die Himmel wieder ein.", "tokens": ["Und", "nimmt", "die", "Him\u00b7mel", "wie\u00b7der", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der Herr, nachdem er das Heil und unverg\u00e4nglich Leben", "tokens": ["Der", "Herr", ",", "nach\u00b7dem", "er", "das", "Heil", "und", "un\u00b7ver\u00b7g\u00e4ng\u00b7lich", "Le\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "ART", "NN", "KON", "ADJD", "NN"], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Auf Erden an das Licht gebracht,", "tokens": ["Auf", "Er\u00b7den", "an", "das", "Licht", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den Weg zu Gott uns gelehrt, sich selbst f\u00fcr uns gegeben,", "tokens": ["Den", "Weg", "zu", "Gott", "uns", "ge\u00b7lehrt", ",", "sich", "selbst", "f\u00fcr", "uns", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "PPER", "VVPP", "$,", "PRF", "ADV", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "F\u00e4hrt auf zur Rechten seiner Macht.", "tokens": ["F\u00e4hrt", "auf", "zur", "Rech\u00b7ten", "sei\u00b7ner", "Macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "APPRART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Sein, sein ist alle Gewalt im Himmel und auf Erden,", "tokens": ["Sein", ",", "sein", "ist", "al\u00b7le", "Ge\u00b7walt", "im", "Him\u00b7mel", "und", "auf", "Er\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "$,", "VAINF", "VAFIN", "PIAT", "NN", "APPRART", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und uns hat er das Heil verdient.", "tokens": ["Und", "uns", "hat", "er", "das", "Heil", "ver\u00b7dient", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-++--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Wer sein Wort gl\u00e4ubet und h\u00e4lt, soll nicht verloren werden;", "tokens": ["Wer", "sein", "Wort", "gl\u00e4u\u00b7bet", "und", "h\u00e4lt", ",", "soll", "nicht", "ver\u00b7lo\u00b7ren", "wer\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "VVFIN", "KON", "VVFIN", "$,", "VMFIN", "PTKNEG", "VVPP", "VAINF", "$."], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Er hat die Welt mit Gott vers\u00fchnt.", "tokens": ["Er", "hat", "die", "Welt", "mit", "Gott", "ver\u00b7s\u00fchnt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Hoch \u00fcber alle Vernunft besiegt er ihr Verderben,", "tokens": ["Hoch", "\u00fc\u00b7ber", "al\u00b7le", "Ver\u00b7nunft", "be\u00b7siegt", "er", "ihr", "Ver\u00b7der\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PIAT", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und seine Lieb erm\u00fcdet nie.", "tokens": ["Und", "sei\u00b7ne", "Lieb", "er\u00b7m\u00fc\u00b7det", "nie", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein unverg\u00e4ngliches Gl\u00fcck den Menschen zu erwerben,", "tokens": ["Ein", "un\u00b7ver\u00b7g\u00e4ng\u00b7li\u00b7ches", "Gl\u00fcck", "den", "Men\u00b7schen", "zu", "er\u00b7wer\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "So heiligt er sich selbst f\u00fcr sie.", "tokens": ["So", "hei\u00b7ligt", "er", "sich", "selbst", "f\u00fcr", "sie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Jauchzt, ihr Gerechten, dem Herrn, und preist seinen Namen!", "tokens": ["Jauchzt", ",", "ihr", "Ge\u00b7rech\u00b7ten", ",", "dem", "Herrn", ",", "und", "preist", "sei\u00b7nen", "Na\u00b7men", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPOSAT", "NN", "$,", "ART", "NN", "$,", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+--+--+-+--+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Ihm danken, das ist unsre Pflicht.", "tokens": ["Ihm", "dan\u00b7ken", ",", "das", "ist", "uns\u00b7re", "Pflicht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "$,", "PDS", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Wir sind gl\u00fcckselig in ihm. Sein Wort ist Ja und Amen;", "tokens": ["Wir", "sind", "gl\u00fcck\u00b7se\u00b7lig", "in", "ihm", ".", "Sein", "Wort", "ist", "Ja", "und", "A\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "APPR", "PPER", "$.", "PPOSAT", "NN", "VAFIN", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.4": {"text": "Und Gott ist unsre Zuversicht.", "tokens": ["Und", "Gott", "ist", "uns\u00b7re", "Zu\u00b7ver\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Preist, ihr Erl\u00f6sten, den Herrn, und r\u00fchmet all, ihr Frommen!", "tokens": ["Preist", ",", "ihr", "Er\u00b7l\u00f6s\u00b7ten", ",", "den", "Herrn", ",", "und", "r\u00fch\u00b7met", "all", ",", "ihr", "From\u00b7men", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "$,", "ART", "NN", "$,", "KON", "VVFIN", "PIAT", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Er f\u00e4hrt gen Himmel, als ein Held,", "tokens": ["Er", "f\u00e4hrt", "gen", "Him\u00b7mel", ",", "als", "ein", "Held", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,", "KOUS", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In Wolken f\u00e4hrt er hinauf; so wird er wiederkommen,", "tokens": ["In", "Wol\u00b7ken", "f\u00e4hrt", "er", "hin\u00b7auf", ";", "so", "wird", "er", "wie\u00b7der\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "PTKVZ", "$.", "ADV", "VAFIN", "PPER", "VVINF", "$,"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Ein Herr und Richter aller Welt.", "tokens": ["Ein", "Herr", "und", "Rich\u00b7ter", "al\u00b7ler", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Dies ist des Gl\u00e4ubigen Trost, verkl\u00e4rt ihn einst zu schauen,", "tokens": ["Dies", "ist", "des", "Gl\u00e4u\u00b7bi\u00b7gen", "Trost", ",", "ver\u00b7kl\u00e4rt", "ihn", "einst", "zu", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "NN", "$,", "VVFIN", "PPER", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+----+-+-+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Und seiner Liebe sich zu freun.", "tokens": ["Und", "sei\u00b7ner", "Lie\u00b7be", "sich", "zu", "freun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PRF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dies ist des Gl\u00e4ubigen Pflicht, ihm ewig zu vertrauen,", "tokens": ["Dies", "ist", "des", "Gl\u00e4u\u00b7bi\u00b7gen", "Pflicht", ",", "ihm", "e\u00b7wig", "zu", "ver\u00b7trau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "NN", "$,", "PPER", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und sich durch Tugend ihm zu weihn.", "tokens": ["Und", "sich", "durch", "Tu\u00b7gend", "ihm", "zu", "weihn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "APPR", "NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Wer des Erl\u00f6sers sich sch\u00e4mt, des wird auch er sich sch\u00e4men;", "tokens": ["Wer", "des", "Er\u00b7l\u00f6\u00b7sers", "sich", "sch\u00e4mt", ",", "des", "wird", "auch", "er", "sich", "sch\u00e4\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "PRF", "ADJD", "$,", "PDS", "VAFIN", "ADV", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Den wieder ehren, der ihn ehrt.", "tokens": ["Den", "wie\u00b7der", "eh\u00b7ren", ",", "der", "ihn", "ehrt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVINF", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "La\u00df uns das Leben von dir und Gnad um Gnade nehmen,", "tokens": ["La\u00df", "uns", "das", "Le\u00b7ben", "von", "dir", "und", "Gnad", "um", "Gna\u00b7de", "neh\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "APPR", "PPER", "KON", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "---+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Herr, dessen Herrschaft ewig w\u00e4hrt!", "tokens": ["Herr", ",", "des\u00b7sen", "Herr\u00b7schaft", "e\u00b7wig", "w\u00e4hrt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELAT", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Ich bin ein irrendes Schaf, du weisest mich zu rechte,", "tokens": ["Ich", "bin", "ein", "ir\u00b7ren\u00b7des", "Schaf", ",", "du", "wei\u00b7sest", "mich", "zu", "rech\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,", "PPER", "VVFIN", "PPER", "APPR", "ADJA", "$,"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und leitest mich nach deinem Rat;", "tokens": ["Und", "lei\u00b7test", "mich", "nach", "dei\u00b7nem", "Rat", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Machst mich vom Knechte der Welt zu einem deiner Knechte,", "tokens": ["Machst", "mich", "vom", "Knech\u00b7te", "der", "Welt", "zu", "ei\u00b7nem", "dei\u00b7ner", "Knech\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "ART", "NN", "APPR", "ART", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und tilgest meine Missetat.", "tokens": ["Und", "til\u00b7gest", "mei\u00b7ne", "Mis\u00b7se\u00b7tat", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Was ist die Hoheit der Welt? Sie r\u00fchrt den Christen wenig.", "tokens": ["Was", "ist", "die", "Ho\u00b7heit", "der", "Welt", "?", "Sie", "r\u00fchrt", "den", "Chris\u00b7ten", "we\u00b7nig", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "ART", "NN", "$.", "PPER", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Du kleidest ihn mit Ruhm und Pracht.", "tokens": ["Du", "klei\u00b7dest", "ihn", "mit", "Ruhm", "und", "Pracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was ist die Hoheit der Welt? Zum Priester und zum K\u00f6nig", "tokens": ["Was", "ist", "die", "Ho\u00b7heit", "der", "Welt", "?", "Zum", "Pries\u00b7ter", "und", "zum", "K\u00f6\u00b7nig"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "ART", "NN", "ART", "NN", "$.", "APPRART", "NN", "KON", "APPRART", "NN"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Bin ich durch dich vor Gott gemacht.", "tokens": ["Bin", "ich", "durch", "dich", "vor", "Gott", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "PRF", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Dank sei dem Heiland der Welt! Er hat sein Werk vollf\u00fchret.", "tokens": ["Dank", "sei", "dem", "Hei\u00b7land", "der", "Welt", "!", "Er", "hat", "sein", "Werk", "voll\u00b7f\u00fch\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VAFIN", "ART", "NN", "ART", "NN", "$.", "PPER", "VAFIN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Frohlock ihm, Volk der Christenheit!", "tokens": ["Froh\u00b7lock", "ihm", ",", "Volk", "der", "Chris\u00b7ten\u00b7heit", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "$,", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er sitzt zur Rechten des Herrn. Er lebet und regieret", "tokens": ["Er", "sitzt", "zur", "Rech\u00b7ten", "des", "Herrn", ".", "Er", "le\u00b7bet", "und", "re\u00b7gie\u00b7ret"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ART", "NN", "$.", "PPER", "VVFIN", "KON", "VVFIN"], "meter": "-+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Von Ewigkeit zu Ewigkeit.", "tokens": ["Von", "E\u00b7wig\u00b7keit", "zu", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Jauchzt, ihr Erl\u00f6sten, dem Herrn! Er hat sein Werk vollendet;", "tokens": ["Jauchzt", ",", "ihr", "Er\u00b7l\u00f6s\u00b7ten", ",", "dem", "Herrn", "!", "Er", "hat", "sein", "Werk", "voll\u00b7en\u00b7det", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPOSAT", "NN", "$,", "ART", "NN", "$.", "PPER", "VAFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Des m\u00fcsse sich der Erdkreis freun!", "tokens": ["Des", "m\u00fcs\u00b7se", "sich", "der", "Erd\u00b7kreis", "freun", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VMFIN", "PRF", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er f\u00e4hrt verkl\u00e4ret hinauf zu dem, der ihn gesendet,", "tokens": ["Er", "f\u00e4hrt", "ver\u00b7kl\u00e4\u00b7ret", "hin\u00b7auf", "zu", "dem", ",", "der", "ihn", "ge\u00b7sen\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VVFIN", "ADV", "APPR", "ART", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und nimmt die Himmel wieder ein.", "tokens": ["Und", "nimmt", "die", "Him\u00b7mel", "wie\u00b7der", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Der Herr, nachdem er das Heil und unverg\u00e4nglich Leben", "tokens": ["Der", "Herr", ",", "nach\u00b7dem", "er", "das", "Heil", "und", "un\u00b7ver\u00b7g\u00e4ng\u00b7lich", "Le\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "ART", "NN", "KON", "ADJD", "NN"], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Auf Erden an das Licht gebracht,", "tokens": ["Auf", "Er\u00b7den", "an", "das", "Licht", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den Weg zu Gott uns gelehrt, sich selbst f\u00fcr uns gegeben,", "tokens": ["Den", "Weg", "zu", "Gott", "uns", "ge\u00b7lehrt", ",", "sich", "selbst", "f\u00fcr", "uns", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "PPER", "VVPP", "$,", "PRF", "ADV", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "F\u00e4hrt auf zur Rechten seiner Macht.", "tokens": ["F\u00e4hrt", "auf", "zur", "Rech\u00b7ten", "sei\u00b7ner", "Macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "APPRART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "Sein, sein ist alle Gewalt im Himmel und auf Erden,", "tokens": ["Sein", ",", "sein", "ist", "al\u00b7le", "Ge\u00b7walt", "im", "Him\u00b7mel", "und", "auf", "Er\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "$,", "VAINF", "VAFIN", "PIAT", "NN", "APPRART", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und uns hat er das Heil verdient.", "tokens": ["Und", "uns", "hat", "er", "das", "Heil", "ver\u00b7dient", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-++--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Wer sein Wort gl\u00e4ubet und h\u00e4lt, soll nicht verloren werden;", "tokens": ["Wer", "sein", "Wort", "gl\u00e4u\u00b7bet", "und", "h\u00e4lt", ",", "soll", "nicht", "ver\u00b7lo\u00b7ren", "wer\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "VVFIN", "KON", "VVFIN", "$,", "VMFIN", "PTKNEG", "VVPP", "VAINF", "$."], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Er hat die Welt mit Gott vers\u00fchnt.", "tokens": ["Er", "hat", "die", "Welt", "mit", "Gott", "ver\u00b7s\u00fchnt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.15": {"line.1": {"text": "Hoch \u00fcber alle Vernunft besiegt er ihr Verderben,", "tokens": ["Hoch", "\u00fc\u00b7ber", "al\u00b7le", "Ver\u00b7nunft", "be\u00b7siegt", "er", "ihr", "Ver\u00b7der\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PIAT", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und seine Lieb erm\u00fcdet nie.", "tokens": ["Und", "sei\u00b7ne", "Lieb", "er\u00b7m\u00fc\u00b7det", "nie", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein unverg\u00e4ngliches Gl\u00fcck den Menschen zu erwerben,", "tokens": ["Ein", "un\u00b7ver\u00b7g\u00e4ng\u00b7li\u00b7ches", "Gl\u00fcck", "den", "Men\u00b7schen", "zu", "er\u00b7wer\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "So heiligt er sich selbst f\u00fcr sie.", "tokens": ["So", "hei\u00b7ligt", "er", "sich", "selbst", "f\u00fcr", "sie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.16": {"line.1": {"text": "Jauchzt, ihr Gerechten, dem Herrn, und preist seinen Namen!", "tokens": ["Jauchzt", ",", "ihr", "Ge\u00b7rech\u00b7ten", ",", "dem", "Herrn", ",", "und", "preist", "sei\u00b7nen", "Na\u00b7men", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPOSAT", "NN", "$,", "ART", "NN", "$,", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+--+--+-+--+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Ihm danken, das ist unsre Pflicht.", "tokens": ["Ihm", "dan\u00b7ken", ",", "das", "ist", "uns\u00b7re", "Pflicht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "$,", "PDS", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Wir sind gl\u00fcckselig in ihm. Sein Wort ist Ja und Amen;", "tokens": ["Wir", "sind", "gl\u00fcck\u00b7se\u00b7lig", "in", "ihm", ".", "Sein", "Wort", "ist", "Ja", "und", "A\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "APPR", "PPER", "$.", "PPOSAT", "NN", "VAFIN", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.4": {"text": "Und Gott ist unsre Zuversicht.", "tokens": ["Und", "Gott", "ist", "uns\u00b7re", "Zu\u00b7ver\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.17": {"line.1": {"text": "Preist, ihr Erl\u00f6sten, den Herrn, und r\u00fchmet all, ihr Frommen!", "tokens": ["Preist", ",", "ihr", "Er\u00b7l\u00f6s\u00b7ten", ",", "den", "Herrn", ",", "und", "r\u00fch\u00b7met", "all", ",", "ihr", "From\u00b7men", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "$,", "ART", "NN", "$,", "KON", "VVFIN", "PIAT", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Er f\u00e4hrt gen Himmel, als ein Held,", "tokens": ["Er", "f\u00e4hrt", "gen", "Him\u00b7mel", ",", "als", "ein", "Held", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,", "KOUS", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In Wolken f\u00e4hrt er hinauf; so wird er wiederkommen,", "tokens": ["In", "Wol\u00b7ken", "f\u00e4hrt", "er", "hin\u00b7auf", ";", "so", "wird", "er", "wie\u00b7der\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "PTKVZ", "$.", "ADV", "VAFIN", "PPER", "VVINF", "$,"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Ein Herr und Richter aller Welt.", "tokens": ["Ein", "Herr", "und", "Rich\u00b7ter", "al\u00b7ler", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.18": {"line.1": {"text": "Dies ist des Gl\u00e4ubigen Trost, verkl\u00e4rt ihn einst zu schauen,", "tokens": ["Dies", "ist", "des", "Gl\u00e4u\u00b7bi\u00b7gen", "Trost", ",", "ver\u00b7kl\u00e4rt", "ihn", "einst", "zu", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "NN", "$,", "VVFIN", "PPER", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+----+-+-+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Und seiner Liebe sich zu freun.", "tokens": ["Und", "sei\u00b7ner", "Lie\u00b7be", "sich", "zu", "freun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PRF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dies ist des Gl\u00e4ubigen Pflicht, ihm ewig zu vertrauen,", "tokens": ["Dies", "ist", "des", "Gl\u00e4u\u00b7bi\u00b7gen", "Pflicht", ",", "ihm", "e\u00b7wig", "zu", "ver\u00b7trau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "NN", "$,", "PPER", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und sich durch Tugend ihm zu weihn.", "tokens": ["Und", "sich", "durch", "Tu\u00b7gend", "ihm", "zu", "weihn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "APPR", "NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.19": {"line.1": {"text": "Wer des Erl\u00f6sers sich sch\u00e4mt, des wird auch er sich sch\u00e4men;", "tokens": ["Wer", "des", "Er\u00b7l\u00f6\u00b7sers", "sich", "sch\u00e4mt", ",", "des", "wird", "auch", "er", "sich", "sch\u00e4\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "PRF", "ADJD", "$,", "PDS", "VAFIN", "ADV", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Den wieder ehren, der ihn ehrt.", "tokens": ["Den", "wie\u00b7der", "eh\u00b7ren", ",", "der", "ihn", "ehrt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVINF", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "La\u00df uns das Leben von dir und Gnad um Gnade nehmen,", "tokens": ["La\u00df", "uns", "das", "Le\u00b7ben", "von", "dir", "und", "Gnad", "um", "Gna\u00b7de", "neh\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "APPR", "PPER", "KON", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "---+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Herr, dessen Herrschaft ewig w\u00e4hrt!", "tokens": ["Herr", ",", "des\u00b7sen", "Herr\u00b7schaft", "e\u00b7wig", "w\u00e4hrt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELAT", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.20": {"line.1": {"text": "Ich bin ein irrendes Schaf, du weisest mich zu rechte,", "tokens": ["Ich", "bin", "ein", "ir\u00b7ren\u00b7des", "Schaf", ",", "du", "wei\u00b7sest", "mich", "zu", "rech\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,", "PPER", "VVFIN", "PPER", "APPR", "ADJA", "$,"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und leitest mich nach deinem Rat;", "tokens": ["Und", "lei\u00b7test", "mich", "nach", "dei\u00b7nem", "Rat", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Machst mich vom Knechte der Welt zu einem deiner Knechte,", "tokens": ["Machst", "mich", "vom", "Knech\u00b7te", "der", "Welt", "zu", "ei\u00b7nem", "dei\u00b7ner", "Knech\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "ART", "NN", "APPR", "ART", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und tilgest meine Missetat.", "tokens": ["Und", "til\u00b7gest", "mei\u00b7ne", "Mis\u00b7se\u00b7tat", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.21": {"line.1": {"text": "Was ist die Hoheit der Welt? Sie r\u00fchrt den Christen wenig.", "tokens": ["Was", "ist", "die", "Ho\u00b7heit", "der", "Welt", "?", "Sie", "r\u00fchrt", "den", "Chris\u00b7ten", "we\u00b7nig", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "ART", "NN", "$.", "PPER", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Du kleidest ihn mit Ruhm und Pracht.", "tokens": ["Du", "klei\u00b7dest", "ihn", "mit", "Ruhm", "und", "Pracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was ist die Hoheit der Welt? Zum Priester und zum K\u00f6nig", "tokens": ["Was", "ist", "die", "Ho\u00b7heit", "der", "Welt", "?", "Zum", "Pries\u00b7ter", "und", "zum", "K\u00f6\u00b7nig"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "ART", "NN", "ART", "NN", "$.", "APPRART", "NN", "KON", "APPRART", "NN"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Bin ich durch dich vor Gott gemacht.", "tokens": ["Bin", "ich", "durch", "dich", "vor", "Gott", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "PRF", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.22": {"line.1": {"text": "Dank sei dem Heiland der Welt! Er hat sein Werk vollf\u00fchret.", "tokens": ["Dank", "sei", "dem", "Hei\u00b7land", "der", "Welt", "!", "Er", "hat", "sein", "Werk", "voll\u00b7f\u00fch\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VAFIN", "ART", "NN", "ART", "NN", "$.", "PPER", "VAFIN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Frohlock ihm, Volk der Christenheit!", "tokens": ["Froh\u00b7lock", "ihm", ",", "Volk", "der", "Chris\u00b7ten\u00b7heit", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "$,", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er sitzt zur Rechten des Herrn. Er lebet und regieret", "tokens": ["Er", "sitzt", "zur", "Rech\u00b7ten", "des", "Herrn", ".", "Er", "le\u00b7bet", "und", "re\u00b7gie\u00b7ret"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ART", "NN", "$.", "PPER", "VVFIN", "KON", "VVFIN"], "meter": "-+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Von Ewigkeit zu Ewigkeit.", "tokens": ["Von", "E\u00b7wig\u00b7keit", "zu", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}