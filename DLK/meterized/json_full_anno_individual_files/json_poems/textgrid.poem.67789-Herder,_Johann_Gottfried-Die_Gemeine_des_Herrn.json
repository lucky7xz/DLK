11": {"line.1": {"text": "Erd' und Himmel ist verbunden", "tokens": ["Erd'", "und", "Him\u00b7mel", "ist", "ver\u00b7bun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "VAFIN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Durch der Ueberwinder Zahl.", "tokens": ["Durch", "der", "Ue\u00b7ber\u00b7win\u00b7der", "Zahl", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie die droben \u00fcberwunden,", "tokens": ["Wie", "die", "dro\u00b7ben", "\u00fc\u00b7berw\u00b7un\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "H\u00f6rt man ihren Freudenschall", "tokens": ["H\u00f6rt", "man", "ih\u00b7ren", "Freu\u00b7den\u00b7schall"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Widerklingen hier auf Erden;", "tokens": ["Wi\u00b7der\u00b7klin\u00b7gen", "hier", "auf", "Er\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wie sie droben lobend stehn,", "tokens": ["Wie", "sie", "dro\u00b7ben", "lo\u00b7bend", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Werden wir auch lobend gehn,", "tokens": ["Wer\u00b7den", "wir", "auch", "lo\u00b7bend", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "VVINF", "$,"], "meter": "----+-+", "measure": "unknown.measure.di"}, "line.8": {"text": "Lobend dort gekr\u00f6net werden!", "tokens": ["Lo\u00b7bend", "dort", "ge\u00b7kr\u00f6\u00b7net", "wer\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "VVPP", "VAINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Ihr und unser Bruder ist,", "tokens": ["Ihr", "und", "un\u00b7ser", "Bru\u00b7der", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KON", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Aller Bruder Jesus Christ.", "tokens": ["Al\u00b7ler", "Bru\u00b7der", "Je\u00b7sus", "Christ", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NE", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Und der ", "tokens": ["Und", "der"], "token_info": ["word", "word"], "pos": ["KON", "ART"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Geht durch Stimmen und Gesch\u00e4fte,", "tokens": ["Geht", "durch", "Stim\u00b7men", "und", "Ge\u00b7sch\u00e4f\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die er alle klingen hei\u00dft:", "tokens": ["Die", "er", "al\u00b7le", "klin\u00b7gen", "hei\u00dft", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PIS", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ueberall im Heiligthum;", "tokens": ["Ue\u00b7be\u00b7rall", "im", "Hei\u00b7lig\u00b7thum", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Hoch und nieden, um und um", "tokens": ["Hoch", "und", "nie\u00b7den", ",", "um", "und", "um"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "KON", "ADJA", "$,", "KOUI", "KON", "KOUI"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ist es stille, und sie singen,", "tokens": ["Ist", "es", "stil\u00b7le", ",", "und", "sie", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVFIN", "$,", "KON", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wie Jedwedes Tiefe wird", "tokens": ["Wie", "Jed\u00b7we\u00b7des", "Tie\u00b7fe", "wird"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PIAT", "NN", "VAFIN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Von der Geisteskraft ger\u00fchrt.", "tokens": ["Von", "der", "Geis\u00b7tes\u00b7kraft", "ge\u00b7r\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Alles ist ", "tokens": ["Al\u00b7les", "ist"], "token_info": ["word", "word"], "pos": ["PIS", "VAFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Wenn sie Gottes Werke loben,", "tokens": ["Wenn", "sie", "Got\u00b7tes", "Wer\u00b7ke", "lo\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie ", "tokens": ["Wie"], "token_info": ["word"], "pos": ["PWAV"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Jesus' Wort in allen Stimmen,", "tokens": ["Je\u00b7sus'", "Wort", "in", "al\u00b7len", "Stim\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Aller Lichter Jesus' Licht!", "tokens": ["Al\u00b7ler", "Lich\u00b7ter", "Je\u00b7sus'", "Licht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wie in seinem Angesicht", "tokens": ["Wie", "in", "sei\u00b7nem", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Alle Strahlen lieblich stimmen,", "tokens": ["Al\u00b7le", "Strah\u00b7len", "lieb\u00b7lich", "stim\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und in seinem Gotteswerk", "tokens": ["Und", "in", "sei\u00b7nem", "Got\u00b7tes\u00b7werk"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Aller Glieder ", "tokens": ["Al\u00b7ler", "Glie\u00b7der"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.14": {"line.1": {"text": "Aller Glieder Chor anstimmet", "tokens": ["Al\u00b7ler", "Glie\u00b7der", "Chor", "an\u00b7stim\u00b7met"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und lobsinget unserm Gott;", "tokens": ["Und", "lob\u00b7sin\u00b7get", "un\u00b7serm", "Gott", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Jeder singt in seiner Stimme", "tokens": ["Je\u00b7der", "singt", "in", "sei\u00b7ner", "Stim\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dank dem Herren Zebaoth!", "tokens": ["Dank", "dem", "Her\u00b7ren", "Ze\u00b7bao\u00b7th", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Nehmt die Harfen, Harfenspieler,", "tokens": ["Nehmt", "die", "Har\u00b7fen", ",", "Har\u00b7fen\u00b7spie\u00b7ler", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Lobet ihn in ", "tokens": ["Lo\u00b7bet", "ihn", "in"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Himmel, Erde, preiset! preist!", "tokens": ["Him\u00b7mel", ",", "Er\u00b7de", ",", "prei\u00b7set", "!", "preist", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "VVFIN", "$.", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Ihr, die Meister, Ihr, die Sch\u00fcler,", "tokens": ["Ihr", ",", "die", "Meis\u00b7ter", ",", "Ihr", ",", "die", "Sch\u00fc\u00b7ler", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "ART", "NN", "$,", "PPER", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "All' in ", "tokens": ["All'", "in"], "token_info": ["word", "word"], "pos": ["PIS", "APPR"], "meter": "-+", "measure": "iambic.single"}, "line.10": {"text": "Gott in ", "tokens": ["Gott", "in"], "token_info": ["word", "word"], "pos": ["NN", "APPR"], "meter": "+-", "measure": "trochaic.single"}}}}}