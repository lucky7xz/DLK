{"textgrid.poem.67789": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Die Gemeine des Herrn", "genre": "verse", "period": "N.A.", "pub_year": 1769, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "H\u00f6rt! es singen Harfenspieler", "tokens": ["H\u00f6rt", "!", "es", "sin\u00b7gen", "Har\u00b7fen\u00b7spie\u00b7ler"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVIMP", "$.", "PPER", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Droben hoch im Heiligthum.", "tokens": ["Dro\u00b7ben", "hoch", "im", "Hei\u00b7lig\u00b7thum", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "H\u00f6rt und lernt! des Himmels Sch\u00fcler,", "tokens": ["H\u00f6rt", "und", "lernt", "!", "des", "Him\u00b7mels", "Sch\u00fc\u00b7ler", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "KON", "VVFIN", "$.", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Alle singen Gottes Ruhm,", "tokens": ["Al\u00b7le", "sin\u00b7gen", "Got\u00b7tes", "Ruhm", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Jeder ganz nach seiner Weise,", "tokens": ["Je\u00b7der", "ganz", "nach", "sei\u00b7ner", "Wei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Tief von Herzen und allein,", "tokens": ["Tief", "von", "Her\u00b7zen", "und", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "KON", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und doch stimmen Alle ein", "tokens": ["Und", "doch", "stim\u00b7men", "Al\u00b7le", "ein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PIS", "ART"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Lieblich, einig, Gott zum Preise!", "tokens": ["Lieb\u00b7lich", ",", "ei\u00b7nig", ",", "Gott", "zum", "Prei\u00b7se", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "$,", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "H\u00f6rt! so wird der Geist gesandt", "tokens": ["H\u00f6rt", "!", "so", "wird", "der", "Geist", "ge\u00b7sandt"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "$.", "ADV", "VAFIN", "ART", "NN", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Siebenfach in alle Land'.", "tokens": ["Sie\u00b7ben\u00b7fach", "in", "al\u00b7le", "Land'", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie sie droben lieblich spielen,", "tokens": ["Wie", "sie", "dro\u00b7ben", "lieb\u00b7lich", "spie\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "So ert\u00f6net \u00fcberall,", "tokens": ["So", "er\u00b7t\u00f6\u00b7net", "\u00fc\u00b7be\u00b7rall", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo hier Seelen himmlisch f\u00fchlen,", "tokens": ["Wo", "hier", "See\u00b7len", "himm\u00b7lisch", "f\u00fch\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "NN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zarter, leiser Widerhall.", "tokens": ["Zar\u00b7ter", ",", "lei\u00b7ser", "Wi\u00b7der\u00b7hall", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Tief im Herzen wird's so stille!", "tokens": ["Tief", "im", "Her\u00b7zen", "wird's", "so", "stil\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "VAFIN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ist's so heilig, lieblich, rein!", "tokens": ["Ist's", "so", "hei\u00b7lig", ",", "lieb\u00b7lich", ",", "rein", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "ADV", "ADJD", "$,", "ADJD", "$,", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Da ert\u00f6nt denn engelfein", "tokens": ["Da", "er\u00b7t\u00f6nt", "denn", "en\u00b7gel\u00b7fein"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "PTKVZ"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Harfenklang in sanfter Stille,", "tokens": ["Har\u00b7fen\u00b7klang", "in", "sanf\u00b7ter", "Stil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Und der Harfen Himmelston", "tokens": ["Und", "der", "Har\u00b7fen", "Him\u00b7mel\u00b7ston"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "NN"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.10": {"text": "T\u00f6net auf zu Gottes Thron.", "tokens": ["T\u00f6\u00b7net", "auf", "zu", "Got\u00b7tes", "Thron", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "APPR", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "All' ein neues Lied sie singen,", "tokens": ["All'", "ein", "neu\u00b7es", "Lied", "sie", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ART", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Das kein Andrer lernen kann,", "tokens": ["Das", "kein", "A\u00b7ndrer", "ler\u00b7nen", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Als die auf zum Lamme dringen", "tokens": ["Als", "die", "auf", "zum", "Lam\u00b7me", "drin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "APPR", "APPRART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf der Ueberwinder Bahn!", "tokens": ["Auf", "der", "Ue\u00b7ber\u00b7win\u00b7der", "Bahn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Rein und heilig und Jungfrauen,", "tokens": ["Rein", "und", "hei\u00b7lig", "und", "Jung\u00b7frau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Alle Christus' Ebenbild,", "tokens": ["Al\u00b7le", "Chris\u00b7tus'", "E\u00b7ben\u00b7bild", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Alle seines Sinns erf\u00fcllt,", "tokens": ["Al\u00b7le", "sei\u00b7nes", "Sinns", "er\u00b7f\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Er in Allen anzuschauen!", "tokens": ["Er", "in", "Al\u00b7len", "an\u00b7zu\u00b7schau\u00b7en", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NE", "VVIZU", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Von der S\u00fcnde rein und gut", "tokens": ["Von", "der", "S\u00fcn\u00b7de", "rein", "und", "gut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Sie gewaschen durch sein Blut.", "tokens": ["Sie", "ge\u00b7wa\u00b7schen", "durch", "sein", "Blut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Erd' und Himmel ist verbunden", "tokens": ["Erd'", "und", "Him\u00b7mel", "ist", "ver\u00b7bun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "VAFIN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Durch der Ueberwinder Zahl.", "tokens": ["Durch", "der", "Ue\u00b7ber\u00b7win\u00b7der", "Zahl", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie die droben \u00fcberwunden,", "tokens": ["Wie", "die", "dro\u00b7ben", "\u00fc\u00b7berw\u00b7un\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "H\u00f6rt man ihren Freudenschall", "tokens": ["H\u00f6rt", "man", "ih\u00b7ren", "Freu\u00b7den\u00b7schall"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Widerklingen hier auf Erden;", "tokens": ["Wi\u00b7der\u00b7klin\u00b7gen", "hier", "auf", "Er\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wie sie droben lobend stehn,", "tokens": ["Wie", "sie", "dro\u00b7ben", "lo\u00b7bend", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Werden wir auch lobend gehn,", "tokens": ["Wer\u00b7den", "wir", "auch", "lo\u00b7bend", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "VVINF", "$,"], "meter": "----+-+", "measure": "unknown.measure.di"}, "line.8": {"text": "Lobend dort gekr\u00f6net werden!", "tokens": ["Lo\u00b7bend", "dort", "ge\u00b7kr\u00f6\u00b7net", "wer\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "VVPP", "VAINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Ihr und unser Bruder ist,", "tokens": ["Ihr", "und", "un\u00b7ser", "Bru\u00b7der", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KON", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Aller Bruder Jesus Christ.", "tokens": ["Al\u00b7ler", "Bru\u00b7der", "Je\u00b7sus", "Christ", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NE", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Und der ", "tokens": ["Und", "der"], "token_info": ["word", "word"], "pos": ["KON", "ART"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Geht durch Stimmen und Gesch\u00e4fte,", "tokens": ["Geht", "durch", "Stim\u00b7men", "und", "Ge\u00b7sch\u00e4f\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die er alle klingen hei\u00dft:", "tokens": ["Die", "er", "al\u00b7le", "klin\u00b7gen", "hei\u00dft", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PIS", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ueberall im Heiligthum;", "tokens": ["Ue\u00b7be\u00b7rall", "im", "Hei\u00b7lig\u00b7thum", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Hoch und nieden, um und um", "tokens": ["Hoch", "und", "nie\u00b7den", ",", "um", "und", "um"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "KON", "ADJA", "$,", "KOUI", "KON", "KOUI"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ist es stille, und sie singen,", "tokens": ["Ist", "es", "stil\u00b7le", ",", "und", "sie", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVFIN", "$,", "KON", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wie Jedwedes Tiefe wird", "tokens": ["Wie", "Jed\u00b7we\u00b7des", "Tie\u00b7fe", "wird"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PIAT", "NN", "VAFIN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Von der Geisteskraft ger\u00fchrt.", "tokens": ["Von", "der", "Geis\u00b7tes\u00b7kraft", "ge\u00b7r\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Alles ist ", "tokens": ["Al\u00b7les", "ist"], "token_info": ["word", "word"], "pos": ["PIS", "VAFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Wenn sie Gottes Werke loben,", "tokens": ["Wenn", "sie", "Got\u00b7tes", "Wer\u00b7ke", "lo\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie ", "tokens": ["Wie"], "token_info": ["word"], "pos": ["PWAV"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Jesus' Wort in allen Stimmen,", "tokens": ["Je\u00b7sus'", "Wort", "in", "al\u00b7len", "Stim\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Aller Lichter Jesus' Licht!", "tokens": ["Al\u00b7ler", "Lich\u00b7ter", "Je\u00b7sus'", "Licht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wie in seinem Angesicht", "tokens": ["Wie", "in", "sei\u00b7nem", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Alle Strahlen lieblich stimmen,", "tokens": ["Al\u00b7le", "Strah\u00b7len", "lieb\u00b7lich", "stim\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und in seinem Gotteswerk", "tokens": ["Und", "in", "sei\u00b7nem", "Got\u00b7tes\u00b7werk"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Aller Glieder ", "tokens": ["Al\u00b7ler", "Glie\u00b7der"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.7": {"line.1": {"text": "Aller Glieder Chor anstimmet", "tokens": ["Al\u00b7ler", "Glie\u00b7der", "Chor", "an\u00b7stim\u00b7met"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und lobsinget unserm Gott;", "tokens": ["Und", "lob\u00b7sin\u00b7get", "un\u00b7serm", "Gott", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Jeder singt in seiner Stimme", "tokens": ["Je\u00b7der", "singt", "in", "sei\u00b7ner", "Stim\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dank dem Herren Zebaoth!", "tokens": ["Dank", "dem", "Her\u00b7ren", "Ze\u00b7bao\u00b7th", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Nehmt die Harfen, Harfenspieler,", "tokens": ["Nehmt", "die", "Har\u00b7fen", ",", "Har\u00b7fen\u00b7spie\u00b7ler", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Lobet ihn in ", "tokens": ["Lo\u00b7bet", "ihn", "in"], "token_info": ["wo