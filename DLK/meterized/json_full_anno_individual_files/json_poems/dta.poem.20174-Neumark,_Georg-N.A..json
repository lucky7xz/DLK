{"dta.poem.20174": {"metadata": {"author": {"name": "Neumark, Georg", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1666", "urn": "urn:nbn:de:kobv:b4-20424-1", "language": ["de:0.99"], "booktitle": "Neumark, Georg: Poetisch-Historischer Lustgarten. Frankfurt (Main), 1666."}, "poem": {"stanza.1": {"line.1": {"text": "Kein Wetter ist so gro\u00df/ kein Donner kan so knallen/", "tokens": ["Kein", "Wet\u00b7ter", "ist", "so", "gro\u00df", "/", "kein", "Don\u00b7ner", "kan", "so", "knal\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ADV", "ADJD", "$(", "PIAT", "NN", "VMFIN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Darauf nicht wiederumb ein klarer ", "tokens": ["Da\u00b7rauf", "nicht", "wie\u00b7de\u00b7rumb", "ein", "kla\u00b7rer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "PTKNEG", "ADV", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Erfolgen solte bald; so geht es in gemein/", "tokens": ["Er\u00b7fol\u00b7gen", "sol\u00b7te", "bald", ";", "so", "geht", "es", "in", "ge\u00b7mein", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "ADV", "$.", "ADV", "VVFIN", "PPER", "APPR", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mit allen Menschen zu: Es kan nicht auf sie fallen/", "tokens": ["Mit", "al\u00b7len", "Men\u00b7schen", "zu", ":", "Es", "kan", "nicht", "auf", "sie", "fal\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PTKVZ", "$.", "PPER", "VMFIN", "PTKNEG", "APPR", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Des Leides grosse Last/ so sich nicht mit der Zeit/", "tokens": ["Des", "Lei\u00b7des", "gros\u00b7se", "Last", "/", "so", "sich", "nicht", "mit", "der", "Zeit", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$(", "ADV", "PRF", "PTKNEG", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Erleichtert dermahleins durch eine Fr\u00f6ligkeit.", "tokens": ["Er\u00b7leich\u00b7tert", "der\u00b7mah\u00b7leins", "durch", "ei\u00b7ne", "Fr\u00f6\u00b7lig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}}}}}