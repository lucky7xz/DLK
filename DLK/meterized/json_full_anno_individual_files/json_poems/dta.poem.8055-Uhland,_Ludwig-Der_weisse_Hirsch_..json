{"dta.poem.8055": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Der weisse Hirsch .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Und als ich den Hirsch an der Erde sah,", "tokens": ["Und", "als", "ich", "den", "Hirsch", "an", "der", "Er\u00b7de", "sah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Da stie\u00df ich lustig in\u2019s Horn, trara!", "tokens": ["Da", "stie\u00df", "ich", "lus\u00b7tig", "in's", "Horn", ",", "tra\u00b7ra", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPRART", "NN", "$,", "ITJ", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "So lagen sie da und sprachen, die drei,", "tokens": ["So", "la\u00b7gen", "sie", "da", "und", "spra\u00b7chen", ",", "die", "drei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "KON", "VVFIN", "$,", "PRELS", "CARD", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da rannte der weisse Hirsch vorbei.", "tokens": ["Da", "rann\u00b7te", "der", "weis\u00b7se", "Hirsch", "vor\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Und eh\u2019 die drei J\u00e4ger ihn recht gesehn,", "tokens": ["Und", "eh'", "die", "drei", "J\u00e4\u00b7ger", "ihn", "recht", "ge\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "CARD", "NN", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "So war er davon \u00fcber Tiefen und H\u00f6hn.", "tokens": ["So", "war", "er", "da\u00b7von", "\u00fc\u00b7ber", "Tie\u00b7fen", "und", "H\u00f6hn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PAV", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}}}}