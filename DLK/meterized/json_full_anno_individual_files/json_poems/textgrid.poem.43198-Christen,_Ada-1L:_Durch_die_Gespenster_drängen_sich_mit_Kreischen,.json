{"textgrid.poem.43198": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "1L: Durch die Gespenster dr\u00e4ngen sich mit Kreischen,", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Durch die Gespenster dr\u00e4ngen sich mit Kreischen,", "tokens": ["Durch", "die", "Ge\u00b7spens\u00b7ter", "dr\u00e4n\u00b7gen", "sich", "mit", "Krei\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PRF", "APPR", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Mit heftigen und m\u00e4kelnden Geberden", "tokens": ["Mit", "hef\u00b7ti\u00b7gen", "und", "m\u00e4\u00b7keln\u00b7den", "Ge\u00b7ber\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Verkommne M\u00e4nner, die in schmutzbeschwerten,", "tokens": ["Ver\u00b7komm\u00b7ne", "M\u00e4n\u00b7ner", ",", "die", "in", "schmutz\u00b7be\u00b7schwer\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PRELS", "APPR", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zerlumpten Kleidern frech Almosen heischen.", "tokens": ["Zer\u00b7lump\u00b7ten", "Klei\u00b7dern", "frech", "Al\u00b7mo\u00b7sen", "hei\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und braune Weiber mit verwelkten Z\u00fcgen,", "tokens": ["Und", "brau\u00b7ne", "Wei\u00b7ber", "mit", "ver\u00b7welk\u00b7ten", "Z\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die freundlich lachen und bescheiden nicken,", "tokens": ["Die", "freund\u00b7lich", "la\u00b7chen", "und", "be\u00b7schei\u00b7den", "ni\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVINF", "KON", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sie bieten Blumen mit beredten Blicken \u2013", "tokens": ["Sie", "bie\u00b7ten", "Blu\u00b7men", "mit", "be\u00b7red\u00b7ten", "Bli\u00b7cken", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Es kann ihr Wesen, nicht ihr Auge l\u00fcgen.", "tokens": ["Es", "kann", "ihr", "We\u00b7sen", ",", "nicht", "ihr", "Au\u00b7ge", "l\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "$,", "PTKNEG", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Laut zanken Menschen hier aus fernen Zonen,", "tokens": ["Laut", "zan\u00b7ken", "Men\u00b7schen", "hier", "aus", "fer\u00b7nen", "Zo\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die zahmen Tauben f\u00fcttern ihre Kleinen,", "tokens": ["Die", "zah\u00b7men", "Tau\u00b7ben", "f\u00fct\u00b7tern", "ih\u00b7re", "Klei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+