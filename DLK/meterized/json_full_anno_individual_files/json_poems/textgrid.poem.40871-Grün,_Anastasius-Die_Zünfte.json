{"textgrid.poem.40871": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "Die Z\u00fcnfte", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zunftmeister Br\u00fcgge's sa\u00dfen bei Karten, Wein und Lied,", "tokens": ["Zunft\u00b7meis\u00b7ter", "Br\u00fcg\u00b7ge's", "sa\u00b7\u00dfen", "bei", "Kar\u00b7ten", ",", "Wein", "und", "Lied", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VVFIN", "APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Da sa\u00df der braune Schiffer, der F\u00e4rber und ru\u00dfige Schmied,", "tokens": ["Da", "sa\u00df", "der", "brau\u00b7ne", "Schif\u00b7fer", ",", "der", "F\u00e4r\u00b7ber", "und", "ru\u00b7\u00dfi\u00b7ge", "Schmied", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,", "ART", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+--+--+--+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Der Genterschuster Kopp'noll gesellt als Gast sich zu,", "tokens": ["Der", "Gen\u00b7ter\u00b7schus\u00b7ter", "Kopp'\u00b7noll", "ge\u00b7sellt", "als", "Gast", "sich", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVPP", "KOKOM", "NN", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Er sprach im Rath am laut'sten und machte schlechte Schuh'.", "tokens": ["Er", "sprach", "im", "Rath", "am", "laut'\u00b7sten", "und", "mach\u00b7te", "schlech\u00b7te", "Schuh'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "APPRART", "ADJA", "KON", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Der Schuster rief: \u00bbIhr Herren, wi\u00dft ihr das Neu'ste nicht?", "tokens": ["Der", "Schus\u00b7ter", "rief", ":", "\u00bb", "Ihr", "Her\u00b7ren", ",", "wi\u00dft", "ihr", "das", "Neu'\u00b7ste", "nicht", "?"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "$(", "PPOSAT", "NN", "$,", "VVFIN", "PPER", "ART", "NN", "PTKNEG", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Zu Lichtme\u00df kommt der K\u00f6nig; Gott geb', es werde Licht!\u00ab", "tokens": ["Zu", "Licht\u00b7me\u00df", "kommt", "der", "K\u00f6\u00b7nig", ";", "Gott", "geb'", ",", "es", "wer\u00b7de", "Licht", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "$.", "NN", "VVFIN", "$,", "PPER", "VAFIN", "NN", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Inde\u00df der F\u00e4rber heimlich guckt in die Karten dem Schmied,", "tokens": ["In\u00b7de\u00df", "der", "F\u00e4r\u00b7ber", "heim\u00b7lich", "guckt", "in", "die", "Kar\u00b7ten", "dem", "Schmied", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJD", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und murmelt dabei halb leise das alte sch\u00f6ne Lied:", "tokens": ["Und", "mur\u00b7melt", "da\u00b7bei", "halb", "lei\u00b7se", "das", "al\u00b7te", "sch\u00f6\u00b7ne", "Lied", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PAV", "ADJD", "ADJD", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "\u00bbein K\u00f6niglein gab's einmal \u2013 wollt' sagen ein Murmelthier \u2013", "tokens": ["\u00bb", "ein", "K\u00f6\u00b7nig\u00b7lein", "gab's", "ein\u00b7mal", "\u2013", "wollt'", "sa\u00b7gen", "ein", "Mur\u00b7melt\u00b7hier", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "ADV", "$(", "VMFIN", "VVINF", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Gesch\u00e4ftlos war es keinmal, schlief Nachts und Tages schier!", "tokens": ["Ge\u00b7sch\u00e4ft\u00b7los", "war", "es", "kein\u00b7mal", ",", "schlief", "Nachts", "und", "Ta\u00b7ges", "schier", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "ADV", "$,", "VVFIN", "ADV", "KON", "NN", "ADJD", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Des Nachts, weil's Mod' im Leben zu schlafen bei der Nacht,", "tokens": ["Des", "Nachts", ",", "weil's", "Mod'", "im", "Le\u00b7ben", "zu", "schla\u00b7fen", "bei", "der", "Nacht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "$,", "ADJA", "NN", "APPRART", "NN", "PTKZU", "VVINF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und Tages, weil die\u00df Schlafen es m\u00fcd' und matt' gemacht.\u00ab", "tokens": ["Und", "Ta\u00b7ges", ",", "weil", "die\u00df", "Schla\u00b7fen", "es", "m\u00fcd'", "und", "matt'", "ge\u00b7macht", ".", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "NN", "$,", "KOUS", "PDS", "NN", "PPER", "ADJD", "KON", "ADJD", "VVPP", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "Es sprach der Schmied inzwischen: \u00bbDen Max, den hass' ich nicht,", "tokens": ["Es", "sprach", "der", "Schmied", "in\u00b7zwi\u00b7schen", ":", "\u00bb", "Den", "Max", ",", "den", "hass'", "ich", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVINF", "$.", "$(", "ART", "NE", "$,", "ART", "NN", "PPER", "PTKNEG", "$,"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Ein wackrer Kerl ist's immer; \u2013 doch ach, sein Hofgez\u00fccht!", "tokens": ["Ein", "wack\u00b7rer", "Kerl", "ist's", "im\u00b7mer", ";", "\u2013", "doch", "ach", ",", "sein", "Hof\u00b7ge\u00b7z\u00fccht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "$.", "$(", "ADV", "ADV", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Mit derben Eisenhufen beschlagen sind die Herrn,", "tokens": ["Mit", "der\u00b7ben", "Ei\u00b7sen\u00b7hu\u00b7fen", "be\u00b7schla\u00b7gen", "sind", "die", "Herrn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVPP", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und auf des Volkes Leichdorn spazieren und tanzen sie gern!\u00ab", "tokens": ["Und", "auf", "des", "Vol\u00b7kes", "Leich\u00b7dorn", "spa\u00b7zie\u00b7ren", "und", "tan\u00b7zen", "sie", "gern", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "VVINF", "KON", "VVFIN", "PPER", "ADV", "$.", "$("], "meter": "-+-+-+--+--+--+", "measure": "iambic.hexa.relaxed"}}, "stanza.5": {"line.1": {"text": "Der Schuster, pfiffig schmunzelnd, klopft' auf die Achseln dem Mann:", "tokens": ["Der", "Schus\u00b7ter", ",", "pfif\u00b7fig", "schmun\u00b7zelnd", ",", "klopft'", "auf", "die", "Ach\u00b7seln", "dem", "Mann", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "VVPP", "$,", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "\u00bbein P\u00e4rlein neue Stiefel! gern m\u00e4\u00df' ich's ihnen an!\u00ab", "tokens": ["\u00bb", "ein", "P\u00e4r\u00b7lein", "neu\u00b7e", "Stie\u00b7fel", "!", "gern", "m\u00e4\u00df'", "ich's", "ih\u00b7nen", "an", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "NN", "ADJA", "NN", "$.", "ADV", "VVFIN", "PIS", "PPER", "PTKVZ", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Jetzt schlug auf den Tisch der F\u00e4rber und br\u00fcllte jubelnd drein:", "tokens": ["Jetzt", "schlug", "auf", "den", "Tisch", "der", "F\u00e4r\u00b7ber", "und", "br\u00fcll\u00b7te", "ju\u00b7belnd", "drein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "KON", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-+--+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "\u00bbha, Vivat Eichelk\u00f6nig! der Schellenbub' ist mein!\u00ab", "tokens": ["\u00bb", "ha", ",", "Vi\u00b7vat", "Ei\u00b7chel\u00b7k\u00f6\u00b7nig", "!", "der", "Schel\u00b7len\u00b7bub'", "ist", "mein", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ITJ", "$,", "NN", "NN", "$.", "ART", "NN", "VAFIN", "PPOSAT", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.6": {"line.1": {"text": "Da schmi\u00df im Zorn der Schiffer die Karten an die Wand:", "tokens": ["Da", "schmi\u00df", "im", "Zorn", "der", "Schif\u00b7fer", "die", "Kar\u00b7ten", "an", "die", "Wand", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "ART", "NN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "\u00bbein gotterb\u00e4rmlich Leben ist's doch bei euch zu Land'!", "tokens": ["\u00bb", "ein", "got\u00b7ter\u00b7b\u00e4rm\u00b7lich", "Le\u00b7ben", "ist's", "doch", "bei", "euch", "zu", "Land'", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADJD", "NN", "VAFIN", "ADV", "APPR", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.3": {"text": "Verdammt! sticht doch den Buben der K\u00f6nig immer wieder!\u00ab", "tokens": ["Ver\u00b7dammt", "!", "sticht", "doch", "den", "Bu\u00b7ben", "der", "K\u00f6\u00b7nig", "im\u00b7mer", "wie\u00b7der", "!", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVPP", "$.", "VVFIN", "ADV", "ART", "NN", "ART", "NN", "ADV", "ADV", "$.", "$("], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Aufsprangen All', es tobten die polternden St\u00fchle nieder.", "tokens": ["Auf\u00b7spran\u00b7gen", "All'", ",", "es", "tob\u00b7ten", "die", "pol\u00b7tern\u00b7den", "St\u00fch\u00b7le", "nie\u00b7der", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "$,", "PPER", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+--+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.7": {"line.1": {"text": "Es rief der Schmied: \u00bbSo 'n Zepter ist doch ein elend Ding!", "tokens": ["Es", "rief", "der", "Schmied", ":", "\u00bb", "So", "'n", "Zep\u00b7ter", "ist", "doch", "ein", "e\u00b7lend", "Ding", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$.", "$(", "ADV", "ART", "NN", "VAFIN", "ADV", "ART", "ADJD", "NN", "$."], "meter": "-+-+--+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Zum Meisterprobst\u00fcck w\u00e4r' mir solch Machwerk zu gering!\u00ab", "tokens": ["Zum", "Meis\u00b7ter\u00b7prob\u00b7st\u00fcck", "w\u00e4r'", "mir", "solch", "Mach\u00b7werk", "zu", "ge\u00b7ring", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "PIAT", "NN", "PTKA", "ADJD", "$.", "$("], "meter": "-+-+----+-+-+", "measure": "unknown.measure.penta"}, "line.3": {"text": "Der F\u00e4rber sprach: \u00bbMir modert manch rother Fetzen zu Haus;", "tokens": ["Der", "F\u00e4r\u00b7ber", "sprach", ":", "\u00bb", "Mir", "mo\u00b7dert", "manch", "ro\u00b7ther", "Fet\u00b7zen", "zu", "Haus", ";"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "$(", "PPER", "VVFIN", "PIAT", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+-+-+--+-+--+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Hing' er auf des Schusters Dreifu\u00df, n\u00e4hm' gut als Thron er sich aus.\u00ab", "tokens": ["Hin\u00b7g'", "er", "auf", "des", "Schus\u00b7ters", "Drei\u00b7fu\u00df", ",", "n\u00e4hm'", "gut", "als", "Thron", "er", "sich", "aus", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "PPER", "APPR", "ART", "NN", "NN", "$,", "VVFIN", "ADJD", "KOKOM", "NN", "PPER", "PRF", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+-+--+--+", "measure": "iambic.septa.relaxed"}}, "stanza.8": {"line.1": {"text": "Sein Haupt bed\u00e4chtig sch\u00fcttelnd, mit hochgewichtigem Sinn", "tokens": ["Sein", "Haupt", "be\u00b7d\u00e4ch\u00b7tig", "sch\u00fct\u00b7telnd", ",", "mit", "hoch\u00b7ge\u00b7wich\u00b7ti\u00b7gem", "Sinn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJD", "VVPP", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+--+-+--+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Stand Koppenoll, der Schuster, und murmelt vor sich hin:", "tokens": ["Stand", "Kop\u00b7pe\u00b7noll", ",", "der", "Schus\u00b7ter", ",", "und", "mur\u00b7melt", "vor", "sich", "hin", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "ART", "NN", "$,", "KON", "VVFIN", "APPR", "PRF", "PTKVZ", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "\u00bbrespublica stie\u00df j\u00fcngsthin sich in den Schuh ein Loch,", "tokens": ["\u00bb", "res\u00b7pub\u00b7li\u00b7ca", "stie\u00df", "j\u00fcng\u00b7sthin", "sich", "in", "den", "Schuh", "ein", "Loch", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "NE", "VVFIN", "ADV", "PRF", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Doch Meister Kopp'noll denket, es sei zu flicken noch.", "tokens": ["Doch", "Meis\u00b7ter", "Kopp'\u00b7noll", "den\u00b7ket", ",", "es", "sei", "zu", "fli\u00b7cken", "noch", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "$,", "PPER", "VAFIN", "PTKZU", "VVINF", "ADV", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.9": {"line.1": {"text": "Ihr Herrn! wer gibt das Zepter den K\u00f6nigen in die Hand?", "tokens": ["Ihr", "Herrn", "!", "wer", "gibt", "das", "Zep\u00b7ter", "den", "K\u00f6\u00b7ni\u00b7gen", "in", "die", "Hand", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "PWS", "VVFIN", "ART", "NN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Der oben im Himmel herrschet, denn er schuf auch ihr Land!", "tokens": ["Der", "o\u00b7ben", "im", "Him\u00b7mel", "herr\u00b7schet", ",", "denn", "er", "schuf", "auch", "ihr", "Land", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPRART", "NN", "VVFIN", "$,", "KON", "PPER", "VVFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+--+-+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Das Niederland doch schufen wir selbst durch Menschenkunst,", "tokens": ["Das", "Nie\u00b7der\u00b7land", "doch", "schu\u00b7fen", "wir", "selbst", "durch", "Men\u00b7schen\u00b7kunst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "PPER", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Drum w\u00e4hlen wir auch den Herrscher aus eigner Macht und Gunst.\u00ab", "tokens": ["Drum", "w\u00e4h\u00b7len", "wir", "auch", "den", "Herr\u00b7scher", "aus", "eig\u00b7n