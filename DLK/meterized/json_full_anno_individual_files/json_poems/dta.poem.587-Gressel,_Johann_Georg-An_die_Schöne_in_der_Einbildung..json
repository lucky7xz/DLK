{"dta.poem.587": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "An die Sch\u00f6ne in der Einbildung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Auch diesem Fall ich bey/ dein Hals ist Marmor-Stein", "tokens": ["Auch", "die\u00b7sem", "Fall", "ich", "bey", "/", "dein", "Hals", "ist", "Mar\u00b7mor\u00b7Stein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "PDAT", "NN", "PPER", "APPR", "$(", "PPOSAT", "NN", "VAFIN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der mit dem Eben-Holtz noch vor dem Richter steht/", "tokens": ["Der", "mit", "dem", "E\u00b7ben\u00b7Holtz", "noch", "vor", "dem", "Rich\u00b7ter", "steht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ADV", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Brust/ die ist so weis/ als die Citronen seyn/", "tokens": ["Die", "Brust", "/", "die", "ist", "so", "weis", "/", "als", "die", "Cit\u00b7ro\u00b7nen", "seyn", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PDS", "VAFIN", "ADV", "PTKVZ", "$(", "KOUS", "ART", "NN", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Damit die Tr\u00f6del-Frau von Th\u00fcr zu Th\u00fcren geht.", "tokens": ["Da\u00b7mit", "die", "Tr\u00f6\u00b7del\u00b7Frau", "von", "Th\u00fcr", "zu", "Th\u00fc\u00b7ren", "geht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "APPR", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Dann gehts/ schaut die sch\u00f6nen Br\u00fcste", "tokens": ["Dann", "gehts", "/", "schaut", "die", "sch\u00f6\u00b7nen", "Br\u00fcs\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$(", "VVFIN", "ART", "ADJA", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.6": {"text": "Und die L\u00fcste", "tokens": ["Und", "die", "L\u00fcs\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["KON", "ART", "NN"], "meter": "--+-", "measure": "anapaest.init"}, "line.7": {"text": "So deren Kosten zeugt/ die Buhlers nie geschmecket/", "tokens": ["So", "de\u00b7ren", "Kos\u00b7ten", "zeugt", "/", "die", "Buh\u00b7lers", "nie", "ge\u00b7schme\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PRELAT", "NN", "VVFIN", "$(", "ART", "NN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Weil sie von mir verstecket", "tokens": ["Weil", "sie", "von", "mir", "ver\u00b7ste\u00b7cket"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Als solche Sch\u00e4tze sind/", "tokens": ["Als", "sol\u00b7che", "Sch\u00e4t\u00b7ze", "sind", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "VAFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Die man nicht jeden g\u00fcnnt.", "tokens": ["Die", "man", "nicht", "je\u00b7den", "g\u00fcnnt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "PTKNEG", "PIAT", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}