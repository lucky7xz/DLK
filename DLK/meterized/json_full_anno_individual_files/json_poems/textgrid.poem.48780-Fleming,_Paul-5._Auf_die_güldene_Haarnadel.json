{"textgrid.poem.48780": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "5. Auf die g\u00fcldene Haarnadel", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df du ihr g\u00fcldnes Haar noch g\u00fcldner denkst zu machen,", "tokens": ["Da\u00df", "du", "ihr", "g\u00fcld\u00b7nes", "Haar", "noch", "g\u00fcld\u00b7ner", "denkst", "zu", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "ADJA", "NN", "ADV", "ADJD", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "du zwar auch reines Gold, eracht' ich Nichts zu sein,", "tokens": ["du", "zwar", "auch", "rei\u00b7nes", "Gold", ",", "er\u00b7acht'", "ich", "Nichts", "zu", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "ADJA", "NN", "$,", "VVFIN", "PPER", "PIS", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "du, bleiche, borgst von ihr selbst deinen schwachen Schein.", "tokens": ["du", ",", "blei\u00b7che", ",", "borgst", "von", "ihr", "selbst", "dei\u00b7nen", "schwa\u00b7chen", "Schein", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "VVFIN", "$,", "VVFIN", "APPR", "PPER", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Was unterf\u00e4ngst du dich so einer hohen Sachen?", "tokens": ["Was", "un\u00b7ter\u00b7f\u00e4ngst", "du", "dich", "so", "ei\u00b7ner", "ho\u00b7hen", "Sa\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PRF", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Verwegne, mache nicht, da\u00df man dich aus mu\u00df lachen,", "tokens": ["Ver\u00b7weg\u00b7ne", ",", "ma\u00b7che", "nicht", ",", "da\u00df", "man", "dich", "aus", "mu\u00df", "la\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "PTKNEG", "$,", "KOUS", "PIS", "PRF", "APZR", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "leg deinen Hochmut hin und bilde dir nicht ein!", "tokens": ["leg", "dei\u00b7nen", "Hoch\u00b7mut", "hin", "und", "bil\u00b7de", "dir", "nicht", "ein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "PTKVZ", "KON", "VVFIN", "PPER", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "auch Titan h\u00e4lt sein Gold f\u00fcr ihrem nicht f\u00fcr rein'.", "tokens": ["auch", "Ti\u00b7tan", "h\u00e4lt", "sein", "Gold", "f\u00fcr", "ih\u00b7rem", "nicht", "f\u00fcr", "rein'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "PTKNEG", "APPR", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Er sch\u00e4mt sich aufzustehn, wenn er sie schon sieht wachen.", "tokens": ["Er", "sch\u00e4mt", "sich", "auf\u00b7zu\u00b7stehn", ",", "wenn", "er", "sie", "schon", "sieht", "wa\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "VVINF", "$,", "KOUS", 