{"textgrid.poem.62533": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dem kalten Tode war nicht Macht gegeben,", "tokens": ["Dem", "kal\u00b7ten", "To\u00b7de", "war", "nicht", "Macht", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PTKNEG", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zu nahen sich der Lieblichen und Frommen;", "tokens": ["Zu", "na\u00b7hen", "sich", "der", "Lieb\u00b7li\u00b7chen", "und", "From\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "PRF", "ART", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Im tiefsten Inn'ren war ihr fr\u00fch entglommen", "tokens": ["Im", "tiefs\u00b7ten", "Inn'\u00b7ren", "war", "ihr", "fr\u00fch", "ent\u00b7glom\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "VAFIN", "PPER", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein strahlendes und wunderbares Leben.", "tokens": ["Ein", "strah\u00b7len\u00b7des", "und", "wun\u00b7der\u00b7ba\u00b7res", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Als ihre letzte Stunde nun gekommen,", "tokens": ["Als", "ih\u00b7re", "letz\u00b7te", "Stun\u00b7de", "nun", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "ADV", "