{"dta.poem.42": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Gedanken  \n bei dem Anblik eines mit Bohnen be-  \n saamten Akkers.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198797", "language": ["de:0.71", "fr:0.28"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Hier quillt des Seegens F\u00fcllehorn", "tokens": ["Hier", "quillt", "des", "See\u00b7gens", "F\u00fcl\u00b7le\u00b7horn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der reichen G\u00fcte Wunderborn", "tokens": ["Der", "rei\u00b7chen", "G\u00fc\u00b7te", "Wun\u00b7der\u00b7born"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und duftet durch ein sanftes Bla-", "tokens": ["Und", "duf\u00b7tet", "durch", "ein", "sanf\u00b7tes", "Bla"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "TRUNC"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Stets Balsam in die hohle Nasen.", "tokens": ["Stets", "Bal\u00b7sam", "in", "die", "hoh\u00b7le", "Na\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die Lunge wird vergn\u00fcgt ge-", "tokens": ["Die", "Lun\u00b7ge", "wird", "ver\u00b7gn\u00fcgt", "ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "VVPP", "TRUNC"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Das Auge wird vergn\u00fcgt erquikt;", "tokens": ["Das", "Au\u00b7ge", "wird", "ver\u00b7gn\u00fcgt", "er\u00b7quikt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wenn ich der Bohnen weise Bl\u00fcte,", "tokens": ["Wenn", "ich", "der", "Boh\u00b7nen", "wei\u00b7se", "Bl\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Als holde Spiegel deiner G\u00fcte,", "tokens": ["Als", "hol\u00b7de", "Spie\u00b7gel", "dei\u00b7ner", "G\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "O! Sch\u00f6pfer! auf dem Akker seh,", "tokens": ["O", "!", "Sch\u00f6p\u00b7fer", "!", "auf", "dem", "Ak\u00b7ker", "seh", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "NN", "$.", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Da ich aus Lust spatzieren geh.", "tokens": ["Da", "ich", "aus", "Lust", "spat\u00b7zie\u00b7ren", "geh."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["KOUS", "PPER", "APPR", "NN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Das Rokkenfeld prangt schon mit Aehren,", "tokens": ["Das", "Rok\u00b7ken\u00b7feld", "prangt", "schon", "mit", "A\u00b7eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Die uns mit g\u00fcldnen Zungen lehren,", "tokens": ["Die", "uns", "mit", "g\u00fcld\u00b7nen", "Zun\u00b7gen", "leh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Da\u00df du es seist der uns ern\u00e4hrt;", "tokens": ["Da\u00df", "du", "es", "seist", "der", "uns", "er\u00b7n\u00e4hrt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VAFIN", "ART", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Dein Seegen wird dadurch vermehrt", "tokens": ["Dein", "See\u00b7gen", "wird", "da\u00b7durch", "ver\u00b7mehrt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "PAV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Da\u00df du dem Vieh auch Futter giebest,", "tokens": ["Da\u00df", "du", "dem", "Vieh", "auch", "Fut\u00b7ter", "gie\u00b7best", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Und zeigst wie du das alles liebest,", "tokens": ["Und", "zeigst", "wie", "du", "das", "al\u00b7les", "lie\u00b7best", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KOKOM", "PPER", "ART", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Was deine Wunderhand gemacht,", "tokens": ["Was", "dei\u00b7ne", "Wun\u00b7der\u00b7hand", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Als du die Welt herf\u00fcr gebracht.", "tokens": ["Als", "du", "die", "Welt", "her\u00b7f\u00fcr", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Dies Bohnenfeld das jetzo bl\u00fchet,", "tokens": ["Dies", "Boh\u00b7nen\u00b7feld", "das", "jet\u00b7zo", "bl\u00fc\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "ART", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Und Wachsthum aus der Erde ziehet", "tokens": ["Und", "Wach\u00b7sthum", "aus", "der", "Er\u00b7de", "zie\u00b7het"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Ist warlich die Betrachtung werth,", "tokens": ["Ist", "war\u00b7lich", "die", "Be\u00b7trach\u00b7tung", "werth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Die uns HErr! deine Weisheit lehrt.", "tokens": ["Die", "uns", "Herr", "!", "dei\u00b7ne", "Weis\u00b7heit", "lehrt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "NN", "$.", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Vierekkicht sind des Stengels R\u00f6hren,", "tokens": ["Vie\u00b7rek\u00b7kicht", "sind", "des", "Sten\u00b7gels", "R\u00f6h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Die ihre Spizzen aufwerts kehren,", "tokens": ["Die", "ih\u00b7re", "Spiz\u00b7zen", "auf\u00b7werts", "keh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Die hie und da mit Laub besezt;", "tokens": ["Die", "hie", "und", "da", "mit", "Laub", "be\u00b7sezt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "KON", "ADV", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Die Bl\u00fcthen dran man sich erg\u00f6zt,", "tokens": ["Die", "Bl\u00fc\u00b7then", "dran", "man", "sich", "er\u00b7g\u00f6zt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PAV", "PIS", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Die sind recht wunderbahr geleget,", "tokens": ["Die", "sind", "recht", "wun\u00b7der\u00b7bahr", "ge\u00b7le\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Wenn man die Wikkelung erweget.", "tokens": ["Wenn", "man", "die", "Wik\u00b7ke\u00b7lung", "er\u00b7we\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "Des Stengels aufgeschlungnes Haupt,", "tokens": ["Des", "Sten\u00b7gels", "auf\u00b7ge\u00b7schlung\u00b7nes", "Haupt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Das ist mit gr\u00fcner Zier belaubt,", "tokens": ["Das", "ist", "mit", "gr\u00fc\u00b7ner", "Zier", "be\u00b7laubt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Mit einer gr\u00fcnen Kron bekr\u00e4nzet,", "tokens": ["Mit", "ei\u00b7ner", "gr\u00fc\u00b7nen", "Kron", "be\u00b7kr\u00e4n\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.32": {"text": "Die lieblich in die Augen gl\u00e4nzet.", "tokens": ["Die", "lieb\u00b7lich", "in", "die", "Au\u00b7gen", "gl\u00e4n\u00b7zet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.33": {"text": "Nnd wenn des Wachsthums Treibekraft.", "tokens": ["Nnd", "wenn", "des", "Wach\u00b7sthums", "Trei\u00b7be\u00b7kraft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "ART", "NN", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.34": {"text": "Aus Bl\u00fcten erst die Schoten schaft;", "tokens": ["Aus", "Bl\u00fc\u00b7ten", "erst", "die", "Scho\u00b7ten", "schaft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "So sieht man HErr! dein weises F\u00fcgen,", "tokens": ["So", "sieht", "man", "Herr", "!", "dein", "wei\u00b7ses", "F\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "NN", "$.", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.36": {"text": "Durch die Betrachtung mit Vergn\u00fcgen.", "tokens": ["Durch", "die", "Be\u00b7trach\u00b7tung", "mit", "Ver\u00b7gn\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.37": {"text": "Der Bohnen wollgeorndte Zahl,", "tokens": ["Der", "Boh\u00b7nen", "woll\u00b7ge\u00b7ornd\u00b7te", "Zahl", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.38": {"text": "W\u00e4chst da in einen Futteral", "tokens": ["W\u00e4chst", "da", "in", "ei\u00b7nen", "Fut\u00b7te\u00b7ral"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "Da jede in dem Fache stekket,", "tokens": ["Da", "je\u00b7de", "in", "dem", "Fa\u00b7che", "stek\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.40": {"text": "Recht wol verwahrt, recht sanft bedekket.", "tokens": ["Recht", "wol", "ver\u00b7wahrt", ",", "recht", "sanft", "be\u00b7dek\u00b7ket", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVPP", "$,", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.41": {"text": "Ich dachte warum ist die Frucht,", "tokens": ["Ich", "dach\u00b7te", "wa\u00b7rum", "ist", "die", "Frucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PWAV", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.42": {"text": "Die nur das Vieh zur Nahrung sucht:", "tokens": ["Die", "nur", "das", "Vieh", "zur", "Nah\u00b7rung", "sucht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.43": {"text": "So fest bewahrt und eingeschlossen", "tokens": ["So", "fest", "be\u00b7wahrt", "und", "ein\u00b7ge\u00b7schlos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVPP", "KON", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.44": {"text": "Und in den Schooten ausgesprossen?", "tokens": ["Und", "in", "den", "Schoo\u00b7ten", "aus\u00b7ge\u00b7spros\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.45": {"text": "Es fiel mir in den Denken ein,", "tokens": ["Es", "fiel", "mir", "in", "den", "Den\u00b7ken", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.46": {"text": "Da\u00df dieses m\u00fcsse n\u00f6tig seyn.", "tokens": ["Da\u00df", "die\u00b7ses", "m\u00fcs\u00b7se", "n\u00f6\u00b7tig", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "VMFIN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.47": {"text": "Und da\u00df des weisen Sch\u00f6pfers Sorgen,", "tokens": ["Und", "da\u00df", "des", "wei\u00b7sen", "Sch\u00f6p\u00b7fers", "Sor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.48": {"text": "Aus weisen Grund sie so verborgen.", "tokens": ["Aus", "wei\u00b7sen", "Grund", "sie", "so", "ver\u00b7bor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.49": {"text": "Die Bohnenfrucht die keimet leicht,", "tokens": ["Die", "Boh\u00b7nen\u00b7frucht", "die", "kei\u00b7met", "leicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.50": {"text": "Wenn man sie nur in Wasser weicht", "tokens": ["Wenn", "man", "sie", "nur", "in", "Was\u00b7ser", "weicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "PPER", "ADV", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.51": {"text": "Und wenn sie nicht in Capseln l\u00e4gen;", "tokens": ["Und", "wenn", "sie", "nicht", "in", "Cap\u00b7seln", "l\u00e4\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PTKNEG", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.52": {"text": "So w\u00fcrde N\u00e4sse, Thau und Regen", "tokens": ["So", "w\u00fcr\u00b7de", "N\u00e4s\u00b7se", ",", "Thau", "und", "Re\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VAFIN", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.53": {"text": "Gar h\u00e4uffig in die Bohnen ziehu,", "tokens": ["Gar", "h\u00e4uf\u00b7fig", "in", "die", "Boh\u00b7nen", "zie\u00b7hu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.54": {"text": "Und daraus frische Keimen bl\u00fchn;", "tokens": ["Und", "da\u00b7raus", "fri\u00b7sche", "Kei\u00b7men", "bl\u00fchn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.55": {"text": "Die wenn sie in der F\u00e4ulung sterben", "tokens": ["Die", "wenn", "sie", "in", "der", "F\u00e4u\u00b7lung", "ster\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "KOUS", "PPER", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.56": {"text": "Mit sich die ganze Frucht verderben:", "tokens": ["Mit", "sich", "die", "gan\u00b7ze", "Frucht", "ver\u00b7der\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.57": {"text": "So aber da sie wol verwahrt,", "tokens": ["So", "a\u00b7ber", "da", "sie", "wol", "ver\u00b7wahrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KOUS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.58": {"text": "Sind sie zur Winterskost verspart,", "tokens": ["Sind", "sie", "zur", "Win\u00b7ters\u00b7kost", "ver\u00b7spart", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.59": {"text": "Dadurch das Mastvieh zu erhalten.", "tokens": ["Da\u00b7durch", "das", "Mast\u00b7vieh", "zu", "er\u00b7hal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.60": {"text": "O! Sch\u00f6pfer alle die Anstalten,", "tokens": ["O", "!", "Sch\u00f6p\u00b7fer", "al\u00b7le", "die", "An\u00b7stal\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "NN", "PIS", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.61": {"text": "Die wir beschaun in der Natur", "tokens": ["Die", "wir", "be\u00b7schaun", "in", "der", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.62": {"text": "Die geben uns so manche Spur,", "tokens": ["Die", "ge\u00b7ben", "uns", "so", "man\u00b7che", "Spur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.63": {"text": "Darin wir sichtbahr sehen k\u00f6nnen,", "tokens": ["Da\u00b7rin", "wir", "sicht\u00b7bahr", "se\u00b7hen", "k\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "PRF", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.64": {"text": "Du seist ein weiser ", "tokens": ["Du", "seist", "ein", "wei\u00b7ser"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA"], "meter": "-+-+-", "measure": "iambic.di"}, "line.65": {"text": "La\u00df mich dies nie mit Lust ansehn,", "tokens": ["La\u00df", "mich", "dies", "nie", "mit", "Lust", "an\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PDS", "ADV", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.66": {"text": "Ohn deinen Nahmen zu erh\u00f6hn;", "tokens": ["Ohn", "dei\u00b7nen", "Nah\u00b7men", "zu", "er\u00b7h\u00f6hn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.67": {"text": "Und seh ich deine Wunderg\u00fcte;", "tokens": ["Und", "seh", "ich", "dei\u00b7ne", "Wun\u00b7der\u00b7g\u00fc\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.68": {"text": "So gieb ein dankbares Gem\u00fcthe,", "tokens": ["So", "gieb", "ein", "dank\u00b7ba\u00b7res", "Ge\u00b7m\u00fc\u00b7the", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.69": {"text": "Das dich an einem jeden Ort", "tokens": ["Das", "dich", "an", "ei\u00b7nem", "je\u00b7den", "Ort"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "PRF", "APPR", "ART", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.70": {"text": "Wo es dieselbe sieht, so fort,", "tokens": ["Wo", "es", "die\u00b7sel\u00b7be", "sieht", ",", "so", "fort", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PDAT", "VVFIN", "$,", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.71": {"text": "Mit wahrer Herzens Andacht preise,", "tokens": ["Mit", "wah\u00b7rer", "Her\u00b7zens", "An\u00b7dacht", "prei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.72": {"text": "Das dir die Menschenpflicht beweise!", "tokens": ["Das", "dir", "die", "Men\u00b7schen\u00b7pflicht", "be\u00b7wei\u00b7se", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}