{"textgrid.poem.25254": {"metadata": {"author": {"name": "Wille, Bruno", "birth": "N.A.", "death": "N.A."}, "title": "Seek\u00f6nigs Krone", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Des Eilands felsige Reckenbrust", "tokens": ["Des", "Ei\u00b7lands", "fel\u00b7si\u00b7ge", "Re\u00b7cken\u00b7brust"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Umtobte der Sturm mit r\u00fcttelnden Schl\u00e4gen.", "tokens": ["Um\u00b7tob\u00b7te", "der", "Sturm", "mit", "r\u00fct\u00b7teln\u00b7den", "Schl\u00e4\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Da irrt ich, berauscht von Pilgerlust,", "tokens": ["Da", "irrt", "ich", ",", "be\u00b7rauscht", "von", "Pil\u00b7ger\u00b7lust", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "VVFIN", "APPR", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wie suchend durch Heide und Nebelregen.", "tokens": ["Wie", "su\u00b7chend", "durch", "Hei\u00b7de", "und", "Ne\u00b7bel\u00b7re\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "APPR", "NE", "KON", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.2": {"line.1": {"text": "Ich suchte/ ein Fl\u00fcchtling, dem Fu\u00df und Hand", "tokens": ["Ich", "such\u00b7te", "/", "ein", "Fl\u00fccht\u00b7ling", ",", "dem", "Fu\u00df", "und", "Hand"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$(", "ART", "NN", "$,", "ART", "NN", "KON", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Noch immer die Kette gefesselt h\u00e4lt;", "tokens": ["Noch", "im\u00b7mer", "die", "Ket\u00b7te", "ge\u00b7fes\u00b7selt", "h\u00e4lt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "VVPP", "VVFIN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Er schl\u00e4gt sie und schl\u00e4gt an die Felsenwand,", "tokens": ["Er", "schl\u00e4gt", "sie", "und", "schl\u00e4gt", "an", "die", "Fel\u00b7sen\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "KON", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Bis ein w\u00fctender Fluch das Eisen zerschellt.", "tokens": ["Bis", "ein", "w\u00fc\u00b7ten\u00b7der", "Fluch", "das", "Ei\u00b7sen", "zer\u00b7schellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "VVPP", "$."], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}}, "stanza.3": {"line.1": {"text": "Ich suchte/ und fand! O seliger Trost!", "tokens": ["Ich", "such\u00b7te", "/", "und", "fand", "!", "O", "se\u00b7li\u00b7ger", "Trost", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "KON", "VVFIN", "$.", "NE", "ADJA", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wo die Heide zu trotziger Klippe sich h\u00fcgelt,", "tokens": ["Wo", "die", "Hei\u00b7de", "zu", "trot\u00b7zi\u00b7ger", "Klip\u00b7pe", "sich", "h\u00fc\u00b7gelt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "ADJA", "NN", "PRF", "ADJD", "$,"], "meter": "--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.3": {"text": "Und schauerlich s\u00fc\u00df die Brandung tost,", "tokens": ["Und", "schau\u00b7er\u00b7lich", "s\u00fc\u00df", "die", "Bran\u00b7dung", "tost", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Von Wolkendunkel und M\u00f6ven umfl\u00fcgelt.", "tokens": ["Von", "Wol\u00b7ken\u00b7dun\u00b7kel", "und", "M\u00f6\u00b7ven", "um\u00b7fl\u00fc\u00b7gelt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NN", "VVFIN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Geheimnisvoll winkte das H\u00fcnengrab,", "tokens": ["Ge\u00b7heim\u00b7nis\u00b7voll", "wink\u00b7te", "das", "H\u00fc\u00b7nen\u00b7grab", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Wo oft im Sande der struppigen Heide", "tokens": ["Wo", "oft", "im", "San\u00b7de", "der", "strup\u00b7pi\u00b7gen", "Hei\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "APPRART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+++-+-", "measure": "unknown.measure.hexa"}, "line.3": {"text": "Der Sch\u00e4fer gew\u00fchlt mit dem Hirtenstab", "tokens": ["Der", "Sch\u00e4\u00b7fer", "ge\u00b7w\u00fchlt", "mit", "dem", "Hir\u00b7ten\u00b7stab"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVPP", "APPR", "ART", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Nach Wikingerk\u00f6nigs verscharrtem Geschmeide.", "tokens": ["Nach", "Wi\u00b7kin\u00b7ger\u00b7k\u00f6\u00b7nigs", "ver\u00b7scharr\u00b7tem", "Ge\u00b7schmei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}, "stanza.5": {"line.1": {"text": "Da hab ich, was nimmer der T\u00e4ppische fand,", "tokens": ["Da", "hab", "ich", ",", "was", "nim\u00b7mer", "der", "T\u00e4p\u00b7pi\u00b7sche", "fand", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "PRELS", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Das glei\u00dfende Gold in der H\u00f6hlung erschaut.", "tokens": ["Das", "glei\u00b7\u00dfen\u00b7de", "Gold", "in", "der", "H\u00f6h\u00b7lung", "er\u00b7schaut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Frei legte die Krone sich mir in die Hand,", "tokens": ["Frei", "leg\u00b7te", "die", "Kro\u00b7ne", "sich", "mir", "in", "die", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "PRF", "PPER", "APPR", "ART", "NN", "$,"], "meter": "++--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Wie eine vor Zeiten verlobte Braut.", "tokens": ["Wie", "ei\u00b7ne", "vor", "Zei\u00b7ten", "ver\u00b7lob\u00b7te", "Braut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "APPR", "NN", "ADJA", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.6": {"line.1": {"text": "Ich klomm auf den Malstein. Da hub sich das Meer", "tokens": ["Ich", "klomm", "auf", "den", "Mals\u00b7tein", ".", "Da", "hub", "sich", "das", "Meer"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$.", "ADV", "VVFIN", "PRF", "ART", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Ein Wasserwall um mein einsam Eiland.", "tokens": ["Ein", "Was\u00b7ser\u00b7wall", "um", "mein", "ein\u00b7sam", "Ei\u00b7land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "ADJD", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ja banne hinweg, du wogende Wehr,", "tokens": ["Ja", "ban\u00b7ne", "hin\u00b7weg", ",", "du", "wo\u00b7gen\u00b7de", "Wehr", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "VVFIN", "PTKVZ", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Das Land, das ich floh, und den Gram von weiland!", "tokens": ["Das", "Land", ",", "das", "ich", "floh", ",", "und", "den", "Gram", "von", "wei\u00b7land", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "KON", "ART", "NN", "APPR", "ADV", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.7": {"line.1": {"text": "Das Land, das ich floh/ ver\u00e4chtliche Sklaven,", "tokens": ["Das", "Land", ",", "das", "ich", "floh", "/", "ver\u00b7\u00e4cht\u00b7li\u00b7che", "Skla\u00b7ven", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$(", "ADJA", "NN", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Barbaren und Kr\u00e4mer/ wie lagen sie weit,", "tokens": ["Bar\u00b7ba\u00b7ren", "und", "Kr\u00e4\u00b7mer", "/", "wie", "la\u00b7gen", "sie", "weit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$(", "PWAV", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Im Flutengeh\u00fcgel begraben, entschlafen ...", "tokens": ["Im", "Flu\u00b7ten\u00b7ge\u00b7h\u00fc\u00b7gel", "be\u00b7gra\u00b7ben", ",", "ent\u00b7schla\u00b7fen", "..."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$,", "VVPP", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Ich stand in k\u00f6stlicher Einsamkeit.", "tokens": ["Ich", "stand", "in", "k\u00f6st\u00b7li\u00b7cher", "Ein\u00b7sam\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.8": {"line.1": {"text": "Ich stand erhaben auf steinernem Throne,", "tokens": ["Ich", "stand", "er\u00b7ha\u00b7ben", "auf", "stei\u00b7ner\u00b7nem", "Thro\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die Hand gebieterisch ausgestreckt.", "tokens": ["Die", "Hand", "ge\u00b7bie\u00b7te\u00b7risch", "aus\u00b7ge\u00b7streckt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Drauf hat Seek\u00f6nigs heilige Krone", "tokens": ["Drauf", "hat", "See\u00b7k\u00f6\u00b7nigs", "hei\u00b7li\u00b7ge", "Kro\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VAFIN", "NN", "ADJA", "NN"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Mein sturmgesalbtes Haupt bedeckt.", "tokens": ["Mein", "sturm\u00b7ge\u00b7salb\u00b7tes", "Haupt", "be\u00b7deckt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Und die Wogen, die \u00fcppigen Brauser und Sch\u00e4umer,", "tokens": ["Und", "die", "Wo\u00b7gen", ",", "die", "\u00fcp\u00b7pi\u00b7gen", "Brau\u00b7ser", "und", "Sch\u00e4u\u00b7mer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "ART", "ADJA", "NN", "KON", "NN", "$,"], "meter": "--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.2": {"text": "Rollten zur Huldigung jauchzend herbei,", "tokens": ["Roll\u00b7ten", "zur", "Hul\u00b7di\u00b7gung", "jauch\u00b7zend", "her\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "VVPP", "PTKVZ", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Hymnen donnernd dem gro\u00dfen Tr\u00e4umer,", "tokens": ["Hym\u00b7nen", "don\u00b7nernd", "dem", "gro\u00b7\u00dfen", "Tr\u00e4u\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Der mit Tr\u00e4umen sich kr\u00f6nte, in Tr\u00e4umen frei.", "tokens": ["Der", "mit", "Tr\u00e4u\u00b7men", "sich", "kr\u00f6n\u00b7te", ",", "in", "Tr\u00e4u\u00b7men", "frei", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "PRF", "VVFIN", "$,", "APPR", "NN", "PTKVZ", "$."], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}}, "stanza.10": {"line.1": {"text": "Des Eilands felsige Reckenbrust", "tokens": ["Des", "Ei\u00b7lands", "fel\u00b7si\u00b7ge", "Re\u00b7cken\u00b7brust"], "token_info": 