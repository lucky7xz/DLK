{"textgrid.poem.44021": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du, Himmel, kennst mein Leid, du pr\u00fcfest die Gedult.", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du, Himmel, kennst mein Leid, du pr\u00fcfest die Gedult.", "tokens": ["Du", ",", "Him\u00b7mel", ",", "kennst", "mein", "Leid", ",", "du", "pr\u00fc\u00b7fest", "die", "Ge\u00b7dult", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "$,", "VVFIN", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich seufze sp\u00e4t und fr\u00fch, und dies ist deine Schuld.", "tokens": ["Ich", "seuf\u00b7ze", "sp\u00e4t", "und", "fr\u00fch", ",", "und", "dies", "ist", "dei\u00b7ne", "Schuld", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "KON", "ADJD", "$,", "KON", "PDS", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Verzeih mir dieses Wort, das Lieb und Schmerz erzwingen!", "tokens": ["Ver\u00b7zeih", "mir", "die\u00b7ses", "Wort", ",", "das", "Lieb", "und", "Schmerz", "er\u00b7zwin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PDAT", "NN", "$,", "ART", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ich ha\u00dfe Volck und Tag, betr\u00fcbe mich vor mich,", "tokens": ["Ich", "ha\u00b7\u00dfe", "Volck", "und", "Tag", ",", "be\u00b7tr\u00fc\u00b7be", "mich", "vor", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "$,", "VVFIN", "PRF", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Sonst hab ich keine Lust; die Thr\u00e4nen schlagen sich,", "tokens": ["Sonst", "hab", "ich", "kei\u00b7ne", "Lust", ";", "die", "Thr\u00e4\u00b7nen", "schla\u00b7gen", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIAT", "NN", "$.", "ART", "NN", "VVFIN", "PRF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und davor sucht die Welt mein Lob in Ruf zu bringen.", "tokens": ["Und", "da\u00b7vor", "sucht", "die", "Welt", "mein", "Lob", "in", "Ruf", "zu", "brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "VVFIN", "ART", "NN", "PPOSAT", "NN", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Mich aber greift dabey der Kummer sch\u00e4rfer an,", "tokens": ["Mich", "a\u00b7ber", "greift", "da\u00b7bey", "der", "Kum\u00b7mer", "sch\u00e4r\u00b7fer", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PAV", "ART", "NN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Da Daphnis lange schl\u00e4ft und ich noch leben kan.", "tokens": ["Da", "Daph\u00b7nis", "lan\u00b7ge", "schl\u00e4ft", "und", "ich", "noch", "le\u00b7ben", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "ADV", "VVFIN", "KON", "PPER", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-