{"dta.poem.20280": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Vorrede.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Heb Rom Lucretien bi\u00df an das stern-ger\u00fcste!", "tokens": ["Heb", "Rom", "Lu\u00b7cre\u00b7ti\u00b7en", "bi\u00df", "an", "das", "stern\u00b7ge\u00b7r\u00fcs\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "KON", "APPR", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil sie in adern-brunn den kalten stahl gesteckt/", "tokens": ["Weil", "sie", "in", "a\u00b7dern\u00b7brunn", "den", "kal\u00b7ten", "stahl", "ge\u00b7steckt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NE", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nachdem sie vom Tarqvin durch ehbruch war befleckt/", "tokens": ["Nach\u00b7dem", "sie", "vom", "Tar\u00b7qvin", "durch", "eh\u00b7bruch", "war", "be\u00b7fleckt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "APPR", "PPER", "VAFIN", "VVPP", "$("], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Hier dringt ein reiner dolch durch unbefleckte br\u00fcste.", "tokens": ["Hier", "dringt", "ein", "rei\u00b7ner", "dolch", "durch", "un\u00b7be\u00b7fleck\u00b7te", "br\u00fcs\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "APPR", "ADJA", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Lucretia lie\u00df zu vorher die schn\u00f6den l\u00fcste.", "tokens": ["Lu\u00b7cre\u00b7tia", "lie\u00df", "zu", "vor\u00b7her", "die", "schn\u00f6\u00b7den", "l\u00fcs\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "ADV", "ART", "ADJA", "ADJA", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.6": {"text": "Olympia hat nichts von geiler brunst geschmeckt/", "tokens": ["O\u00b7lym\u00b7pia", "hat", "nichts", "von", "gei\u00b7ler", "brunst", "ge\u00b7schmeckt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PIS", "APPR", "ADJA", "NN", "VVPP", "$("], "meter": "----+-+-+-+", "measure": "unknown.measure.tetra"}, "line.7": {"text": "Die ihren helden-arm zu strenger rach ausstreckt/", "tokens": ["Die", "ih\u00b7ren", "hel\u00b7den\u00b7arm", "zu", "stren\u00b7ger", "rach", "aus\u00b7streckt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ch/ als zum ersten mahl sie Artabazes k\u00fc\u00dfte.", "tokens": ["Ch", "/", "als", "zum", "ers\u00b7ten", "mahl", "sie", "Ar\u00b7ta\u00b7ba\u00b7zes", "k\u00fc\u00df\u00b7te", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "KOKOM", "APPRART", "ADJA", "ADV", "PPER", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Lucretia verschrenckt dem sch\u00e4nder nur den thron;", "tokens": ["Lu\u00b7cre\u00b7tia", "ver\u00b7schrenckt", "dem", "sch\u00e4n\u00b7der", "nur", "den", "thron", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "ADJA", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.10": {"text": "Hier b\u00fc\u00dft der f\u00fcrsatz ein lust/ ehre/ leben/ kron.", "tokens": ["Hier", "b\u00fc\u00dft", "der", "f\u00fcr\u00b7satz", "ein", "lust", "/", "eh\u00b7re", "/", "le\u00b7ben", "/", "kron", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "$(", "VVFIN", "$(", "VVINF", "$(", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Die nachwelt wird gestehn/ die beyder bild wird sehen;", "tokens": ["Die", "nach\u00b7welt", "wird", "ge\u00b7stehn", "/", "die", "bey\u00b7der", "bild", "wird", "se\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$(", "ART", "PIAT", "NN", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Gold/ ertzt und marmol sey Olympien zu schlecht/", "tokens": ["Gold", "/", "ertzt", "und", "mar\u00b7mol", "sey", "O\u00b7lym\u00b7pien", "zu", "schlecht", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "VVPP", "KON", "NN", "VAFIN", "NE", "PTKA", "ADJD", "$("], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.13": {"text": "Lucretzen holtz zu gut; Lucretzen sey nur recht/", "tokens": ["Lu\u00b7cret\u00b7zen", "holtz", "zu", "gut", ";", "Lu\u00b7cret\u00b7zen", "sey", "nur", "recht", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "PTKA", "ADJD", "$.", "NE", "VAFIN", "ADV", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Olympien zu viel durch ihren stich geschehen.", "tokens": ["O\u00b7lym\u00b7pien", "zu", "viel", "durch", "ih\u00b7ren", "stich", "ge\u00b7sche\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PIS", "APPR", "PPOSAT", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}}}}