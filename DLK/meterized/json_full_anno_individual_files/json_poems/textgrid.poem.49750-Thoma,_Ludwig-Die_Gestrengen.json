{"textgrid.poem.49750": {"metadata": {"author": {"name": "Thoma, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Die Gestrengen", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr Herren Maler! Ihr Herren Dichter!", "tokens": ["Ihr", "Her\u00b7ren", "Ma\u00b7ler", "!", "Ihr", "Her\u00b7ren", "Dich\u00b7ter", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$.", "PPOSAT", "NN", "NN", "$."], "meter": "-+----+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Betrachten wir einmal unsere Richter!", "tokens": ["Be\u00b7trach\u00b7ten", "wir", "ein\u00b7mal", "un\u00b7se\u00b7re", "Rich\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Da wir n\u00e4chstens mit ihnen Bekanntschaft schlie\u00dfen,", "tokens": ["Da", "wir", "n\u00e4chs\u00b7tens", "mit", "ih\u00b7nen", "Be\u00b7kannt\u00b7schaft", "schlie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "PPER", "NN", "VVINF", "$,"], "meter": "--+--+--+-+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Darf euch die M\u00fche nicht verdrie\u00dfen.", "tokens": ["Darf", "euch", "die", "M\u00fc\u00b7he", "nicht", "ver\u00b7drie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Da sitzen sie oben, feierlich, stumm.", "tokens": ["Da", "sit\u00b7zen", "sie", "o\u00b7ben", ",", "fei\u00b7er\u00b7lich", ",", "stumm", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "ADJD", "$,", "ADJD", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Wir blicken hilflos im Kreise herum", "tokens": ["Wir", "bli\u00b7cken", "hil\u00b7flos", "im", "Krei\u00b7se", "he\u00b7rum"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "APPRART", "NN", "APZR"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Und pr\u00fcfen die Mienen der strengen Herren,", "tokens": ["Und", "pr\u00fc\u00b7fen", "die", "Mie\u00b7nen", "der", "stren\u00b7gen", "Her\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "Die die Macht besitzen, uns einzusperren.", "tokens": ["Die", "die", "Macht", "be\u00b7sit\u00b7zen", ",", "uns", "ein\u00b7zu\u00b7sper\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVINF", "$,", "PPER", "VVIZU", "$."], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "In der Mitte thront der Herr Pr\u00e4sident,", "tokens": ["In", "der", "Mit\u00b7te", "thront", "der", "Herr", "Pr\u00e4\u00b7si\u00b7dent", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Der keine Kunst und kein Mitleid kennt.", "tokens": ["Der", "kei\u00b7ne", "Kunst", "und", "kein", "Mit\u00b7leid", "kennt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "KON", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Seitdem ihn das Zipperlein \u00f6fter zwackte,", "tokens": ["Seit\u00b7dem", "ihn", "das", "Zip\u00b7per\u00b7lein", "\u00f6f\u00b7ter", "zwack\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.12": {"text": "Ha\u00dft er von ganzem Herzen das Nackte.", "tokens": ["Ha\u00dft", "er", "von", "gan\u00b7zem", "Her\u00b7zen", "das", "Nack\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "ART", "NN", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.13": {"text": "Der rechter Hand sitzt und so grimmig lacht,", "tokens": ["Der", "rech\u00b7ter", "Hand", "sitzt", "und", "so", "grim\u00b7mig", "lacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "KON", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Wenn der Pr\u00e4sident eine Bemerkung macht,", "tokens": ["Wenn", "der", "Pr\u00e4\u00b7si\u00b7dent", "ei\u00b7ne", "Be\u00b7mer\u00b7kung", "macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.15": {"text": "Die \u00fcber uns bildet einen Witz,", "tokens": ["Die", "\u00fc\u00b7ber", "uns", "bil\u00b7det", "ei\u00b7nen", "Witz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.16": {"text": "Ist zwar keine Leuchte der hohen Justiz,", "tokens": ["Ist", "zwar", "kei\u00b7ne", "Leuch\u00b7te", "der", "ho\u00b7hen", "Jus\u00b7tiz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PIAT", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.17": {"text": "Aber beliebt bei dem Herrn Minister,", "tokens": ["A\u00b7ber", "be\u00b7liebt", "bei", "dem", "Herrn", "Mi\u00b7nis\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ART", "NN", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.18": {"text": "Und ist nierenleidender Korpsphilister.", "tokens": ["Und", "ist", "nie\u00b7ren\u00b7lei\u00b7den\u00b7der", "Korps\u00b7phi\u00b7lis\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADJA", "NN", "$."], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.19": {"text": "Der Herr zur Linken ist auch ganz Ohr", "tokens": ["Der", "Herr", "zur", "Lin\u00b7ken", "ist", "auch", "ganz", "Ohr"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN", "VAFIN", "ADV", "ADV", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.20": {"text": "F\u00fcr den Pr\u00e4sidenten, und kehrt hervor", "tokens": ["F\u00fcr", "den", "Pr\u00e4\u00b7si\u00b7den\u00b7ten", ",", "und", "kehrt", "her\u00b7vor"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "KON", "VVFIN", "PTKVZ"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.21": {"text": "Das helle Staunen vor so viel Geist,", "tokens": ["Das", "hel\u00b7le", "Stau\u00b7nen", "vor", "so", "viel", "Geist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.22": {"text": "Wie sich aus dessen Reden erweist.", "tokens": ["Wie", "sich", "aus", "des\u00b7sen", "Re\u00b7den", "er\u00b7weist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "APPR", "PRELAT", "NN", "VVFIN", "$."], "meter": "---+-+--+", "measure": "iambic.tri.chol"}, "line.23": {"text": "Er zeigt uns mit besonderer Verve", "tokens": ["Er", "zeigt", "uns", "mit", "be\u00b7son\u00b7de\u00b7rer", "Ver\u00b7ve"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Seine Verachtung als Leutnant der Reserve.", "tokens": ["Sei\u00b7ne", "Ver\u00b7ach\u00b7tung", "als", "Leut\u00b7nant", "der", "Re\u00b7ser\u00b7ve", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KOUS", "NN", "ART", "NN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.25": {"text": "Die andern zwei sind mehr jovial.", "tokens": ["Die", "an\u00b7dern", "zwei", "sind", "mehr", "jo\u00b7vi\u00b7al", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "CARD", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.26": {"text": "Sie freuen sich schon auf ihr Mittagsmahl,", "tokens": ["Sie", "freu\u00b7en", "sich", "schon", "auf", "ihr", "Mit\u00b7tags\u00b7mahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "Das sie fr\u00f6hlich einzunehmen gedenken,", "tokens": ["Das", "sie", "fr\u00f6h\u00b7lich", "ein\u00b7zu\u00b7neh\u00b7men", "ge\u00b7den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADJD", "VVINF", "VVINF", "$,"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.28": {"text": "Wenn es ihnen gelang, uns hereinzusenken.", "tokens": ["Wenn", "es", "ih\u00b7nen", "ge\u00b7lang", ",", "uns", "her\u00b7ein\u00b7zu\u00b7sen\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "$,", "PPER", "VVINF", "$."], "meter": "+-+--+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.29": {"text": "Ihr Herren Maler, ihr Herren Dichter!", "tokens": ["Ihr", "Her\u00b7ren", "Ma\u00b7ler", ",", "ihr", "Her\u00b7ren", "Dich\u00b7ter", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$,", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.30": {"text": "Was pr\u00fcft ihr so lange diese Gesichter?", "tokens": ["Was", "pr\u00fcft", "ihr", "so", "lan\u00b7ge", "die\u00b7se", "Ge\u00b7sich\u00b7ter", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "ADV", "PDAT", "NN", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.31": {"text": "Sucht ihr den Geist von der Akademie,", "tokens": ["Sucht", "ihr", "den", "Geist", "von", "der", "A\u00b7ka\u00b7de\u00b7mie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.32": {"text": "Der hohen Schule? Den findet ihr nie!", "tokens": ["Der", "ho\u00b7hen", "Schu\u00b7le", "?", "Den", "fin\u00b7det", "ihr", "nie", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "NE", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.33": {"text": "Oder h\u00f6chstens in einigen breiten Schmissen,", "tokens": ["O\u00b7der", "h\u00f6chs\u00b7tens", "in", "ei\u00b7ni\u00b7gen", "brei\u00b7ten", "Schmis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PIAT", "ADJA", "NN", "$,"], "meter": "+-+--+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.34": {"text": "Womit sie das Antlitz entzwei gerissen,", "tokens": ["Wo\u00b7mit", "sie", "das", "Ant\u00b7litz", "ent\u00b7zwei", "ge\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "PTKVZ", "VVPP", "$,"], "meter": "----+--+-+-", "measure": "iambic.tri.relaxed"}, "line.35": {"text": "Die sie sich schlugen auf Maul und Nase", "tokens": ["Die", "sie", "sich", "schlu\u00b7gen", "auf", "Maul", "und", "Na\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "PRF", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.36": {"text": "Bei einem Korps oder einer Blase,", "tokens": ["Bei", "ei\u00b7nem", "Korps", "o\u00b7der", "ei\u00b7ner", "Bla\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.37": {"text": "Als sie noch bei der Kneiperei", "tokens": ["Als", "sie", "noch", "bei", "der", "Knei\u00b7pe\u00b7rei"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.38": {"text": "Im Chorus sangen: \u00bbDer Bursch ist frei\u00ab", "tokens": ["Im", "Cho\u00b7rus", "san\u00b7gen", ":", "\u00bb", "Der", "Bursch", "ist", "frei", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "$.", "$(", "ART", "NN", "VAFIN", "ADJD", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.39": {"text": "Und da\u00df man die Wahrheit sagen m\u00fc\u00dft.", "tokens": ["Und", "da\u00df", "man", "die", "Wahr\u00b7heit", "sa\u00b7gen", "m\u00fc\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.40": {"text": "Das haben sie l\u00e4ngst schon abgeb\u00fc\u00dft;", "tokens": ["Das", "ha\u00b7ben", "sie", "l\u00e4ngst", "schon", "ab\u00b7ge\u00b7b\u00fc\u00dft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.41": {"text": "Seitdem sie die Leber sich sauer gesoffen,", "tokens": ["Seit\u00b7dem", "sie", "die", "Le\u00b7ber", "sich", "sau\u00b7er", "ge\u00b7sof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "ART", "NN", "PRF", "ADJD", "VVPP", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.42": {"text": "Ist es vorbei mit dem Schw\u00e4rmen und Hoffen.", "tokens": ["Ist", "es", "vor\u00b7bei", "mit", "dem", "Schw\u00e4r\u00b7men", "und", "Hof\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "APPR", "ART", "NN", "KON", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.43": {"text": "Wer in Bier ertr\u00e4nkte die sch\u00f6ne Jugend,", "tokens": ["Wer", "in", "Bier", "er\u00b7tr\u00e4nk\u00b7te", "die", "sch\u00f6\u00b7ne", "Ju\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.44": {"text": "Ist desto mehr f\u00fcr die strenge Tugend;", "tokens": ["Ist", "des\u00b7to", "mehr", "f\u00fcr", "die", "stren\u00b7ge", "Tu\u00b7gend", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.45": {"text": "Die Ideale sind eitel Dunst.", "tokens": ["Die", "I\u00b7dea\u00b7le", "sind", "ei\u00b7tel", "Dunst", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.46": {"text": "O Herrgott, sch\u00fctze die arme Kunst!", "tokens": ["O", "Herr\u00b7gott", ",", "sch\u00fct\u00b7ze", "die", "ar\u00b7me", "Kunst", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Ihr Herren Maler! Ihr Herren Dichter!", "tokens": ["Ihr", "Her\u00b7ren", "Ma\u00b7ler", "!", "Ihr", "Her\u00b7ren", "Dich\u00b7ter", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$.", "PPOSAT", "NN", "NN", "$."], "meter": "-+----+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Betrachten wir einmal unsere Richter!", "tokens": ["Be\u00b7trach\u00b7ten", "wir", "ein\u00b7mal", "un\u00b7se\u00b7re", "Rich\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Da wir n\u00e4chstens mit ihnen Bekanntschaft schlie\u00dfen,", "tokens": ["Da", "wir", "n\u00e4chs\u00b7tens", "mit", "ih\u00b7nen", "Be\u00b7kannt\u00b7schaft", "schlie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "PPER", "NN", "VVINF", "$,"], "meter": "--+--+--+-+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Darf euch die M\u00fche nicht verdrie\u00dfen.", "tokens": ["Darf", "euch", "die", "M\u00fc\u00b7he", "nicht", "ver\u00b7drie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Da sitzen sie oben, feierlich, stumm.", "tokens": ["Da", "sit\u00b7zen", "sie", "o\u00b7ben", ",", "fei\u00b7er\u00b7lich", ",", "stumm", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "ADJD", "$,", "ADJD", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Wir blicken hilflos im Kreise herum", "tokens": ["Wir", "bli\u00b7cken", "hil\u00b7flos", "im", "Krei\u00b7se", "he\u00b7rum"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "APPRART", "NN", "APZR"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Und pr\u00fcfen die Mienen der strengen Herren,", "tokens": ["Und", "pr\u00fc\u00b7fen", "die", "Mie\u00b7nen", "der", "stren\u00b7gen", "Her\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "Die die Macht besitzen, uns einzusperren.", "tokens": ["Die", "die", "Macht", "be\u00b7sit\u00b7zen", ",", "uns", "ein\u00b7zu\u00b7sper\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVINF", "$,", "PPER", "VVIZU", "$."], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "In der Mitte thront der Her