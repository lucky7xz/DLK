{"textgrid.poem.66601": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "D\u00e4mon", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Fieber flammt die Stirn, und stechend f\u00e4hrt", "tokens": ["Im", "Fie\u00b7ber", "flammt", "die", "Stirn", ",", "und", "ste\u00b7chend", "f\u00e4hrt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$,", "KON", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Durch die emp\u00f6rte Seele Gottes Schwert \u2013", "tokens": ["Durch", "die", "em\u00b7p\u00f6r\u00b7te", "See\u00b7le", "Got\u00b7tes", "Schwert", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "O, nimmer, nimmer findet jener Rast,", "tokens": ["O", ",", "nim\u00b7mer", ",", "nim\u00b7mer", "fin\u00b7det", "je\u00b7ner", "Rast", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "$,", "ADV", "VVFIN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der aus dem Staube nach den Sternen fa\u00dft.", "tokens": ["Der", "aus", "dem", "Stau\u00b7be", "nach", "den", "Ster\u00b7nen", "fa\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "An seinem Herzen fri\u00dft des Geiers Gier,", "tokens": ["An", "sei\u00b7nem", "Her\u00b7zen", "fri\u00dft", "des", "Gei\u00b7ers", "Gier", ","], "token_info": ["word", "word", "word