{"dta.poem.2625": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Uber das fr\u00fchzeitige Absterben/  \n Hn. P. V. \u00e4ltesten T\u00f6chterlein/ 2. Febr. 1670.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das kalte Todes-Eis jetzt euer Hertz beschweret/", "tokens": ["Das", "kal\u00b7te", "To\u00b7des\u00b7Eis", "jetzt", "eu\u00b7er", "Hertz", "be\u00b7schwe\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Betr\u00fcbtste/ da\u00df der Frost den Bronn der Adern", "tokens": ["Be\u00b7tr\u00b7\u00fcbts\u00b7te", "/", "da\u00df", "der", "Frost", "den", "Bronn", "der", "A\u00b7dern"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$(", "KOUS", "ART", "NN", "ART", "NN", "ART", "NN"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und ein erb\u00e4rmlich Weh euch Hertz und Seel ver-", "tokens": ["Und", "ein", "er\u00b7b\u00e4rm\u00b7lich", "Weh", "euch", "Hertz", "und", "Seel", "ver"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJD", "NN", "PPER", "NN", "KON", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Weil eure Lust und Trost von dieser Erden rei\u00dft/", "tokens": ["Weil", "eu\u00b7re", "Lust", "und", "Trost", "von", "die\u00b7ser", "Er\u00b7den", "rei\u00dft", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "KON", "NN", "APPR", "PDAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ist heisser Thr\u00e4nen werth/ auch billich zu beklagen.", "tokens": ["Ist", "heis\u00b7ser", "Thr\u00e4\u00b7nen", "werth", "/", "auch", "bil\u00b7lich", "zu", "be\u00b7kla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "ADJD", "$(", "ADV", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Es solte Castalis mit vollen Str\u00f6men gehn/", "tokens": ["Es", "sol\u00b7te", "Cas\u00b7ta\u00b7lis", "mit", "vol\u00b7len", "Str\u00f6\u00b7men", "gehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "NE", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wenn nicht de\u00df Hornungs Grimm in Harnisch sie geschlagen/", "tokens": ["Wenn", "nicht", "de\u00df", "Hor\u00b7nungs", "Grimm", "in", "Har\u00b7nisch", "sie", "ge\u00b7schla\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "ART", "NE", "NE", "APPR", "NN", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und so mit Stadt und Land verfroren m\u00fcste stehn.", "tokens": ["Und", "so", "mit", "Stadt", "und", "Land", "ver\u00b7fro\u00b7ren", "m\u00fcs\u00b7te", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "NN", "KON", "NN", "VVINF", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Vergebens hab ich nur den Lorber-Wald besuchet/", "tokens": ["Ver\u00b7ge\u00b7bens", "hab", "ich", "nur", "den", "Lor\u00b7ber\u00b7Wald", "be\u00b7su\u00b7chet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Der Pindus siand verschneyt/ Parnassus gantz bedeckt;", "tokens": ["Der", "Pin\u00b7dus", "si\u00b7and", "ver\u00b7schneyt", "/", "Par\u00b7nas\u00b7sus", "gantz", "be\u00b7deckt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NE", "$(", "NE", "ADV", "ADJD", "$."], "meter": "--+-+-+-+-+-+", "measure": "anapaest.init"}, "line.11": {"text": "Und als ich bey der Th\u00fcr der Musen angepochet/", "tokens": ["Und", "als", "ich", "bey", "der", "Th\u00fcr", "der", "Mu\u00b7sen", "an\u00b7ge\u00b7po\u00b7chet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "ART", "NN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Hat mich die Grausamkeit des Wetters mehr erschreckt.", "tokens": ["Hat", "mich", "die", "Grau\u00b7sam\u00b7keit", "des", "Wet\u00b7ters", "mehr", "er\u00b7schreckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ich forschte ferner nach in der Gelehrten B\u00fcchern/", "tokens": ["Ich", "forschte", "fer\u00b7ner", "nach", "in", "der", "Ge\u00b7lehr\u00b7ten", "B\u00fc\u00b7chern", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "APPR", "ART", "NN", "NN", "$("], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "Wiewol ein grimmig Leid nimmt schlechten Trost nicht an;", "tokens": ["Wie\u00b7wol", "ein", "grim\u00b7mig", "Leid", "nimmt", "schlech\u00b7ten", "Trost", "nicht", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJD", "NN", "VVFIN", "ADJA", "NN", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Di\u00df was ", "tokens": ["Di\u00df", "was"], "token_info": ["word", "word"], "pos": ["PDS", "PRELS"], "meter": "++", "measure": "spondeus"}, "line.16": {"text": "Gilt bey uns Christen nicht und ist nur eitler Wahn.", "tokens": ["Gilt", "bey", "uns", "Chris\u00b7ten", "nicht", "und", "ist", "nur", "eit\u00b7ler", "Wahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "NN", "PTKNEG", "KON", "VAFIN", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Wie sehr wir uns bem\u00fchn die K\u00fcnste zu ergr\u00fcnden/", "tokens": ["Wie", "sehr", "wir", "uns", "be\u00b7m\u00fchn", "die", "K\u00fcns\u00b7te", "zu", "er\u00b7gr\u00fcn\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPER", "PPER", "VVFIN", "ART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Und alle Wissenschafft in den Besitz zu ziehn/", "tokens": ["Und", "al\u00b7le", "Wis\u00b7sen\u00b7schafft", "in", "den", "Be\u00b7sitz", "zu", "ziehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "APPR", "ART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "So ist die gr\u00f6ste Kunst in Gottes Schlu\u00df sich finden/", "tokens": ["So", "ist", "die", "gr\u00f6s\u00b7te", "Kunst", "in", "Got\u00b7tes", "Schlu\u00df", "sich", "fin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "APPR", "NN", "NN", "PRF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Und seinem Willen nicht vorsetzlich zu entfliehn.", "tokens": ["Und", "sei\u00b7nem", "Wil\u00b7len", "nicht", "vor\u00b7setz\u00b7lich", "zu", "ent\u00b7fliehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PTKNEG", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Es bleibet unverr\u00fcckt das G\u00f6ttliche Gesetze/", "tokens": ["Es", "blei\u00b7bet", "un\u00b7ver\u00b7r\u00fcckt", "das", "G\u00f6tt\u00b7li\u00b7che", "Ge\u00b7set\u00b7ze", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Wie wiederwertig es Vernunfft und Sinnen scheint.", "tokens": ["Wie", "wie\u00b7der\u00b7wer\u00b7tig", "es", "Ver\u00b7nunfft", "und", "Sin\u00b7nen", "scheint", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "So sind doch nirgends sonst de\u00df Himmels wahre Sch\u00e4tze/", "tokens": ["So", "sind", "doch", "nir\u00b7gends", "sonst", "de\u00df", "Him\u00b7mels", "wah\u00b7re", "Sch\u00e4t\u00b7ze", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ADV", "ADV", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Die andern aufder Welt raubt Untreu/ Zeit und Feind.", "tokens": ["Die", "an\u00b7dern", "auf\u00b7der", "Welt", "raubt", "Un\u00b7treu", "/", "Zeit", "und", "Feind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "VVFIN", "ADJD", "$(", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Ist nun ein Kind ein Schatz/ wie billich/ zu benennen/", "tokens": ["Ist", "nun", "ein", "Kind", "ein", "Schatz", "/", "wie", "bil\u00b7lich", "/", "zu", "be\u00b7nen\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "ART", "NN", "$(", "PWAV", "ADJD", "$(", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Was Wunder da\u00df auch GOtt f\u00fcr solche Sch\u00e4tze wacht!", "tokens": ["Was", "Wun\u00b7der", "da\u00df", "auch", "Gott", "f\u00fcr", "sol\u00b7che", "Sch\u00e4t\u00b7ze", "wacht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "KOUS", "ADV", "NN", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Und eh das Sodoma der Welt noch mu\u00df verbrennen/", "tokens": ["Und", "eh", "das", "So\u00b7do\u00b7ma", "der", "Welt", "noch", "mu\u00df", "ver\u00b7bren\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ART", "NN", "ADV", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Bey zeiten zu sich holt und selber nimmt in acht.", "tokens": ["Bey", "zei\u00b7ten", "zu", "sich", "holt", "und", "sel\u00b7ber", "nimmt", "in", "acht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "PRF", "VVFIN", "KON", "ADV", "VVFIN", "APPR", "CARD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Der Eltern Sorg\u2019 und Flei\u00df/ wie m\u00fchsam sie sonst wachen/", "tokens": ["Der", "El\u00b7tern", "Sor\u00b7g'", "und", "Flei\u00df", "/", "wie", "m\u00fch\u00b7sam", "sie", "sonst", "wa\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "NN", "$(", "PWAV", "ADJD", "PPER", "ADV", "VVINF", "$("], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.30": {"text": "Legt Erben von dem Gut de\u00df Gl\u00fcckes etwas bey;", "tokens": ["Legt", "Er\u00b7ben", "von", "dem", "Gut", "de\u00df", "Gl\u00fc\u00b7ckes", "et\u00b7was", "bey", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "APPR", "ART", "NN", "ART", "NN", "PIS", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Doch kan kein sterblich Mensch sie dr\u00fcber sicher machen/", "tokens": ["Doch", "kan", "kein", "sterb\u00b7lich", "Mensch", "sie", "dr\u00fc\u00b7ber", "si\u00b7cher", "ma\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PIAT", "ADJD", "NN", "PPER", "PAV", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Da\u00df di\u00df ein wahrer Grund verlangter Wolfahrt sey.", "tokens": ["Da\u00df", "di\u00df", "ein", "wah\u00b7rer", "Grund", "ver\u00b7lang\u00b7ter", "Wol\u00b7fahrt", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "ART", "ADJA", "NN", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Offt schimpfft des Enckels That der Ahnen Lorber-Kronen/", "tokens": ["Offt", "schimpfft", "des", "En\u00b7ckels", "That", "der", "Ah\u00b7nen", "Lor\u00b7ber\u00b7Kro\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Und seiner Laster Nacht verh\u00fcllt des Anherrns Licht;", "tokens": ["Und", "sei\u00b7ner", "Las\u00b7ter", "Nacht", "ver\u00b7h\u00fcllt", "des", "An\u00b7herrns", "Licht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Offt l\u00e4st der Zeiten Sturm die nicht das Land bewohnen/", "tokens": ["Offt", "l\u00e4st", "der", "Zei\u00b7ten", "Sturm", "die", "nicht", "das", "Land", "be\u00b7woh\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "ART", "PTKNEG", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Das ihre V\u00e4ter vor erbaut und zugericht.", "tokens": ["Das", "ih\u00b7re", "V\u00e4\u00b7ter", "vor", "er\u00b7baut", "und", "zu\u00b7ge\u00b7richt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "APPR", "ADJD", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Der Sohn der zu der Kron und Infel war bestimmet", "tokens": ["Der", "Sohn", "der", "zu", "der", "Kron", "und", "In\u00b7fel", "war", "be\u00b7stim\u00b7met"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "APPR", "ART", "NN", "KON", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Stirbt durch des Henckers Hand in h\u00f6chsten Schimpff und", "tokens": ["Stirbt", "durch", "des", "Hen\u00b7ckers", "Hand", "in", "h\u00f6chs\u00b7ten", "Schimpff", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "NN", "APPR", "ADJA", "NN", "KON"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.39": {"text": "Und wie das Schicksel sich auff Schlangen-weise kr\u00fcmmet/", "tokens": ["Und", "wie", "das", "Schick\u00b7sel", "sich", "auff", "Schlan\u00b7gen\u00b7wei\u00b7se", "kr\u00fcm\u00b7met", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "PRF", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Ist nicht Bolen bekand der auffdie Tochter sprach?", "tokens": ["Ist", "nicht", "Bo\u00b7len", "be\u00b7kand", "der", "auff\u00b7die", "Toch\u00b7ter", "sprach", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "NN", "VVFIN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.41": {"text": "Die Hoffnung vieler Jahr\u2019 und Sorgen-volle Stunden/", "tokens": ["Die", "Hoff\u00b7nung", "vie\u00b7ler", "Jahr'", "und", "Sor\u00b7gen\u00b7vol\u00b7le", "Stun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "KON", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Des Hauses treuer Wunsch/ des Stammes wahrer Ruhm", 