{"textgrid.poem.41291": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Geschenke", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbwer nur zu schenken hat, ist wie ein Edelstein:", "tokens": ["\u00bb", "wer", "nur", "zu", "schen\u00b7ken", "hat", ",", "ist", "wie", "ein", "E\u00b7del\u00b7stein", ":"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "ADV", "PTKZU", "VVINF", "VAFIN", "$,", "VAFIN", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wohin er sich auch kehrt, strahlt seiner Klugheit Schein.\u00ab", "tokens": ["Wo\u00b7hin", "er", "sich", "auch", "kehrt", ",", "strahlt", "sei\u00b7ner", "Klug\u00b7heit", "Schein", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "PPER", "PRF", "ADV", "VVFIN", "$,", "VVFIN", "PPOSAT", "NN", "NN", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wie leicht ist's Reichen, klug zu sein!", "tokens": ["Wie", "leicht", "ist's", "Rei\u00b7chen", ",", "klug", "zu", "sein", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "NN", "$,", "ADJD", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "\u00bbwer nur zu schenken hat, ist wie ein Edelstein:", "tokens": ["\u00bb", "wer", "nur", "zu", "schen\u00b7ken", "hat", ",", "ist", "wie", "ein", "E\u00b7del\u00b7stein", ":"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "ADV", "PTKZU", "VVI