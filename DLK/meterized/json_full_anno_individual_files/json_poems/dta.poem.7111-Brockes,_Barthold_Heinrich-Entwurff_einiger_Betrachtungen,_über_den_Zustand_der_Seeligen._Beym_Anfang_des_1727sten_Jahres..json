{"dta.poem.7111": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Entwurff einiger Betrachtungen,  \n \u00fcber den  \n  Zustand der Seeligen.  \n Beym Anfang des 1727sten Jahres.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Erhalte Stadt und Land; das Ungl\u00fcck, so uns droht,", "tokens": ["Er\u00b7hal\u00b7te", "Stadt", "und", "Land", ";", "das", "Un\u00b7gl\u00fcck", ",", "so", "uns", "droht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$.", "ART", "NN", "$,", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bey der fast allgemeinen Noth,", "tokens": ["Bey", "der", "fast", "all\u00b7ge\u00b7mei\u00b7nen", "Noth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sey gn\u00e4diglich, durch Deine Vater-Hand,", "tokens": ["Sey", "gn\u00e4\u00b7di\u00b7glich", ",", "durch", "Dei\u00b7ne", "Va\u00b7ter\u00b7Hand", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Von uns und Deutschland abgewandt!", "tokens": ["Von", "uns", "und", "Deutschland", "ab\u00b7ge\u00b7wandt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "KON", "NE", "VVPP", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "La\u00df aber uns sodann die holde Friedens-Zeit", "tokens": ["La\u00df", "a\u00b7ber", "uns", "so\u00b7dann", "die", "hol\u00b7de", "Frie\u00b7dens\u00b7Zeit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "ADV", "PPER", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Als ein Geschenck von Dir erkennen,", "tokens": ["Als", "ein", "Ge\u00b7schenck", "von", "Dir", "er\u00b7ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und la\u00df, im Feur der Danckbarkeit,", "tokens": ["Und", "la\u00df", ",", "im", "Feur", "der", "Dan\u00b7ck\u00b7bar\u00b7keit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "$,", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Stets unsre frohe Seelen brennen!", "tokens": ["Stets", "uns\u00b7re", "fro\u00b7he", "See\u00b7len", "bren\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}