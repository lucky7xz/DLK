{"textgrid.poem.39619": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Der ehemalige Spinozist", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sonst hie\u00df bei mir der liebe Gott", "tokens": ["Sonst", "hie\u00df", "bei", "mir", "der", "lie\u00b7be", "Gott"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der dunkle Urgrund, ohn' allen Spott.", "tokens": ["Der", "dunk\u00b7le", "Ur\u00b7grund", ",", "ohn'", "al\u00b7len", "Spott", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "PIAT", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Der mu\u00dfte sich dann hinaufarbeiten", "tokens": ["Der", "mu\u00df\u00b7te", "sich", "dann", "hin\u00b7auf\u00b7ar\u00b7bei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VMFIN", "PRF", "ADV", "VVINF"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und aus sich selber die Welt bereiten.", "tokens": ["Und", "aus", "sich", "sel\u00b7ber", "die", "Welt", "be\u00b7rei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PRF", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Nun aber ist es an der Zeit", "tokens": ["Nun", "a\u00b7ber", "ist", "es", "an", "der", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Vom Pantheismus mich rein zu waschen;", "tokens": ["Vom", "Pan\u00b7theis\u00b7mus", "mich", "rein", "zu", "wa\u00b7schen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Drum spiel ich eine Dreieinigkeit", "tokens": ["Drum", "spiel", "ich", "ei\u00b7ne", "Drei\u00b7ei\u00b7nig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Aus meinen metaphysischen Taschen.", "tokens": ["Aus", "mei\u00b7nen", "me\u00b7ta\u00b7phy\u00b7si\u00b7schen", "Ta\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Ich h\u00e4nge meinen Mantel geschwind", "tokens": ["Ich", "h\u00e4n\u00b7ge", "mei\u00b7nen", "Man\u00b7tel", "ge\u00b7schwind"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "VVPP"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.10": {"text": "Nach dem heidnischen oder christlichen Wind.", "tokens": ["Nach", "dem", "heid\u00b7ni\u00b7schen", "o\u00b7der", "christ\u00b7li\u00b7chen", "Wind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "KON", "ADJA", "NN", "$."], "meter": "+--+-+-+--+", "measure": "iambic.penta.invert"}}, "stanza.2": {"line.1": {"text": "Sonst hie\u00df bei mir der liebe Gott", "tokens": ["Sonst", "hie\u00df", "bei", "mir", "der", "lie\u00b7be", "Gott"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der dunkle Urgrund, ohn' allen Spot