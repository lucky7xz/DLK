{"dta.poem.5797": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Des Loix du Mouvement.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df unser Aug\u2019, indem es sich bem\u00fcht,", "tokens": ["Da\u00df", "un\u00b7ser", "Aug'", ",", "in\u00b7dem", "es", "sich", "be\u00b7m\u00fcht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "$,", "KOUS", "PPER", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Zu folgen ihren schnellen,", "tokens": ["Zu", "fol\u00b7gen", "ih\u00b7ren", "schnel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PPOSAT", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Gleichformigen behenden F\u00e4llen;", "tokens": ["Gleich\u00b7for\u00b7mi\u00b7gen", "be\u00b7hen\u00b7den", "F\u00e4l\u00b7len", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Das Ende nicht davon, auch nicht den Anfang sieht.", "tokens": ["Das", "En\u00b7de", "nicht", "da\u00b7von", ",", "auch", "nicht", "den", "An\u00b7fang", "sieht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "PAV", "$,", "ADV", "PTKNEG", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}