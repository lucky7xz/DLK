{"dta.poem.20836": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Der  \n Sterbende  \n  Socrates.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Man la\u00df ein Sch\u00f6nheit-reiches Weib/", "tokens": ["Man", "la\u00df", "ein", "Sch\u00f6n\u00b7heit\u00b7rei\u00b7ches", "Weib", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "F\u00fcr einem klugen Manne schweben/", "tokens": ["F\u00fcr", "ei\u00b7nem", "klu\u00b7gen", "Man\u00b7ne", "schwe\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die Kr\u00e4fften so in Augen leben/", "tokens": ["Die", "Kr\u00e4ff\u00b7ten", "so", "in", "Au\u00b7gen", "le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die werden r\u00fchren seinen Leib/", "tokens": ["Die", "wer\u00b7den", "r\u00fch\u00b7ren", "sei\u00b7nen", "Leib", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VAINF", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Bi\u00df da\u00df sich Hertz und Muth ergeben.", "tokens": ["Bi\u00df", "da\u00df", "sich", "Hertz", "und", "Muth", "er\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PRF