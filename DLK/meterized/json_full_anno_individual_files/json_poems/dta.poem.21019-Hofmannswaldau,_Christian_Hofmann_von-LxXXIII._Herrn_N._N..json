{"dta.poem.21019": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "LxXXIII.  \n Herrn  N. N.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Nicht setze dich alhier/ und lege dich auch nicht/", "tokens": ["Nicht", "set\u00b7ze", "dich", "al\u00b7hier", "/", "und", "le\u00b7ge", "dich", "auch", "nicht", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "PPER", "ADV", "$(", "KON", "VVFIN", "PPER", "ADV", "PTKNEG", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich haste stets die Ruh/ die dir itzund gebricht.", "tokens": ["Ich", "has\u00b7te", "stets", "die", "Ruh", "/", "die", "dir", "it\u00b7zund", "ge\u00b7bricht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$(", "PRELS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Weil mein bekanter Ruhm zu mindern sich begunte/", "tokens": ["Weil", "mein", "be\u00b7kan\u00b7ter", "Ruhm", "zu", "min\u00b7dern", "sich", "be\u00b7gun\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "APPR", "ADJA", "PRF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Als ich gleich wie zuvor nicht ferner stehen kunte.", "tokens": ["Als", "ich", "gleich", "wie", "zu\u00b7vor", "nicht", "fer\u00b7ner", "ste\u00b7hen", "kun\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word"