{"dta.poem.6506": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Licht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dies reine Licht, die Seel' und Sch\u00f6nheit dieser Welt,", "tokens": ["Dies", "rei\u00b7ne", "Licht", ",", "die", "Seel'", "und", "Sch\u00f6n\u00b7heit", "die\u00b7ser", "Welt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJA", "NN", "$,", "ART", "NN", "KON", "NN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hat in der Sonnen Rund die Lebens-reiche Quelle.", "tokens": ["Hat", "in", "der", "Son\u00b7nen", "Rund", "die", "Le\u00b7bens\u00b7rei\u00b7che", "Quel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Man sehe nur von ihrer Pracht die Stelle,", "tokens": ["Man", "se\u00b7he", "nur", "von", "ih\u00b7rer", "Pracht", "die", "Stel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da, wo der Morgen sie f\u00fcr seine Gottheit h\u00e4lt.", "tokens": ["Da", ",", "wo", "der", "Mor\u00b7gen", "sie", "f\u00fcr", "sei\u00b7ne", "Got\u00b7theit", "h\u00e4lt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "ART", "NN", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Den Augenblick, wenn sie den Lauf scheint anzufangen,", "tokens": ["Den", "Au\u00b7gen\u00b7blick", ",", "wenn", "sie", "den", "Lauf", "scheint", "an\u00b7zu\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und sie den halben Theil der Erden \u00fcberg\u00fcldet;", "tokens": ["Und", "sie", "den", "hal\u00b7ben", "Theil", "der", "Er\u00b7den", "\u00fc\u00b7berg\u00b7\u00fcl\u00b7det", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "ADJA", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "So sehen wir, so bald sie aufgegangen,", "tokens": ["So", "se\u00b7hen", "wir", ",", "so", "bald", "sie", "auf\u00b7ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADV", "ADV", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Sie als ein feurig Rund, mit Glantz gekr\u00f6nt, gebildet.", "tokens": ["Sie", "als", "ein", "feu\u00b7rig", "Rund", ",", "mit", "Glantz", "ge\u00b7kr\u00f6nt", ",", "ge\u00b7bil\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "KOUS", "ART", "ADJD", "ADJD", "$,", "APPR", "NN", "VVPP", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Es r\u00fchret alles sich durch ihren Lebens-Schein,", "tokens": ["Es", "r\u00fch\u00b7ret", "al\u00b7les", "sich", "durch", "ih\u00b7ren", "Le\u00b7bens\u00b7Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Sie f\u00fcllt, durchdringt, und nimmt die Himmels-Tieffen ein.", "tokens": ["Sie", "f\u00fcllt", ",", "durch\u00b7dringt", ",", "und", "nimmt", "die", "Him\u00b7mels\u00b7Tief\u00b7fen", "ein", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "$,", "KON", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}