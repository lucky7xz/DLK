{"dta.poem.21833": {"metadata": {"author": {"name": "Stieler, Kaspar von", "birth": "N.A.", "death": "N.A."}, "title": "Ii.  \n  Ein Degen h\u00e4lt den andern in der  \n Scheide.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1660", "urn": "urn:nbn:de:kobv:b4-20586-2", "language": ["de:0.99"], "booktitle": "Filidor der Dorfferer [i. e. Stieler, Kaspar von]: Die Geharnschte Venus. Hamburg, 1660."}, "poem": {"stanza.1": {"line.1": {"text": "Die Buhlerische Nacht trug ihr gestirntes Band/", "tokens": ["Die", "Buh\u00b7le\u00b7ri\u00b7sche", "Nacht", "trug", "ihr", "ge\u00b7stirn\u00b7tes", "Band", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dianen tanzte f\u00fcr der Venus Diamant.", "tokens": ["Di\u00b7a\u00b7nen", "tanz\u00b7te", "f\u00fcr", "der", "Ve\u00b7nus", "Di\u00b7a\u00b7mant", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "die still-verliebte Krafft straalt\u2019 auff die Er-", "tokens": ["die", "still\u00b7ver\u00b7lieb\u00b7te", "Krafft", "straalt'", "auff", "die", "Er"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "und r\u00fchret\u2019 einen Held mit tausend St\u00fcrmen", "tokens": ["und", "r\u00fch\u00b7ret'", "ei\u00b7nen", "Held", "mit", "tau\u00b7send", "St\u00fcr\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "CARD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Was solte Morfeus tuhn? er brach den braunen stab", "tokens": ["Was", "sol\u00b7te", "Mor\u00b7feus", "tuhn", "?", "er", "brach", "den", "brau\u00b7nen", "stab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "NE", "VVINF", "$.", "PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "und schmi\u00df der Treume Saat ins d\u00fcster Meer hinab.", "tokens": ["und", "schmi\u00df", "der", "Treu\u00b7me", "Saat", "ins", "d\u00fcs\u00b7ter", "Meer", "hin\u00b7ab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "NN", "APPRART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "der Held erwacht\u2019 und f\u00fchlt den Wundersahmen", "tokens": ["der", "Held", "er\u00b7wacht'", "und", "f\u00fchlt", "den", "Wun\u00b7der\u00b7sah\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "fort war noch Lager-stat noch Schlaff ihm weiter", "tokens": ["fort", "war", "noch", "La\u00b7ger\u00b7stat", "noch", "Schlaff", "ihm", "wei\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKVZ", "VAFIN", "ADV", "NN", "ADV", "NN", "PPER", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Er schliche leis\u2019 hinab nach Fillis Kammert\u00fchr", "tokens": ["Er", "schli\u00b7che", "leis'", "hin\u00b7ab", "nach", "Fil\u00b7lis", "Kam\u00b7mer\u00b7t\u00fchr"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADJA", "NN", "ADV", "APPR", "NE", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "vor Kloris Schlaaff-gemach stak stets ein Riegel f\u00fcr", "tokens": ["vor", "Klo\u00b7ris", "Schlaaff\u00b7ge\u00b7mach", "stak", "stets", "ein", "Rie\u00b7gel", "f\u00fcr"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "NE", "VVFIN", "ADV", "ART", "NN", "APPR"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "dabey der Hund/ doch h\u00e4tt\u2019 er dieses schlecht geacht/", "tokens": ["da\u00b7bey", "der", "Hund", "/", "doch", "h\u00e4tt'", "er", "die\u00b7ses", "schlecht", "ge\u00b7acht", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "$(", "ADV", "VAFIN", "PPER", "PDAT", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "wo Kloris Mutter nicht die Tochter mehr bewacht.", "tokens": ["wo", "Klo\u00b7ris", "Mut\u00b7ter", "nicht", "die", "Toch\u00b7ter", "mehr", "be\u00b7wacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "NN", "PTKNEG", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Die Magd solt\u2019 iezo dran/ die sonst alleine lag", "tokens": ["Die", "Magd", "solt'", "ie\u00b7zo", "dran", "/", "die", "sonst", "al\u00b7lei\u00b7ne", "lag"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "ADV", "PAV", "$(", "ART", "ADV", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "bi\u00df auff ein ziemlich Kind/ das sie zu warten pflag", "tokens": ["bi\u00df", "auff", "ein", "ziem\u00b7lich", "Kind", "/", "das", "sie", "zu", "war\u00b7ten", "pflag"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADV", "NN", "$(", "PRELS", "PPER", "PTKZU", "VVINF", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "der Traum-Gott hatte sie im Schlaaffe grob er-", "tokens": ["der", "Traum\u00b7Gott", "hat\u00b7te", "sie", "im", "Schlaaf\u00b7fe", "grob", "er"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PPER", "APPRART", "NN", "ADJD", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "drum lag sie Sinnlo\u00df auff den R\u00fckken au\u00dfge-", "tokens": ["drum", "lag", "sie", "Sinn\u00b7lo\u00df", "auff", "den", "R\u00fck\u00b7ken", "au\u00df\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "NN", "APPR", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Er r\u00fchrt sie sachtlich an. Schlafft oder wachet ihr?", "tokens": ["Er", "r\u00fchrt", "sie", "sacht\u00b7lich", "an", ".", "Schlafft", "o\u00b7der", "wa\u00b7chet", "ihr", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "PTKVZ", "$.", "NN", "KON", "VVFIN", "PPER", "$."], "meter": "-+-+-++--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Ich bin es liebster Schaz/ ach la\u00dft mich zu euch", "tokens": ["Ich", "bin", "es", "liebs\u00b7ter", "Schaz", "/", "ach", "la\u00dft", "mich", "zu", "euch"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ADJA", "NN", "$(", "XY", "VVIMP", "PPER", "APPR", "PPER"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "ich bin verirrt und nakt/ ich m\u00f6cht\u2019 erfroren sein.", "tokens": ["ich", "bin", "ver\u00b7irrt", "und", "nakt", "/", "ich", "m\u00f6cht'", "er\u00b7fro\u00b7ren", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "KON", "ADJD", "$(", "PPER", "VMFIN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Sie merkte bald die Kreyd\u2019 und nahm ihn ein zu sich/", "tokens": ["Sie", "merk\u00b7te", "bald", "die", "Kreyd'", "und", "nahm", "ihn", "ein", "zu", "sich", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "KON", "VVFIN", "PPER", "ART", "APPR", "PRF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "das Bett auff einen Mann gemacht/ hielt nicht den", "tokens": ["das", "Bett", "auff", "ei\u00b7nen", "Mann", "ge\u00b7macht", "/", "hielt", "nicht", "den"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVPP", "$(", "VVFIN", "PTKNEG", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "als hie der dritte kam/ die Last w\u00fcrd ihm zuschweer/", "tokens": ["als", "hie", "der", "drit\u00b7te", "kam", "/", "die", "Last", "w\u00fcrd", "ihm", "zu\u00b7schweer", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "VVFIN", "$(", "ART", "NN", "VAFIN", "PPER", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "es bog sich ziemlich ein und wakkelt\u2019 hin und her.", "tokens": ["es", "bog", "sich", "ziem\u00b7lich", "ein", "und", "wak\u00b7kelt'", "hin", "und", "her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "PTKVZ", "KON", "VVFIN", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Das Kind/ das lange schon der Wiege war entwehnt", "tokens": ["Das", "Kind", "/", "das", "lan\u00b7ge", "schon", "der", "Wie\u00b7ge", "war", "ent\u00b7wehnt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$(", "ART", "ADV", "ADV", "ART", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "als das geplagte Bett\u2019 ohn Ende wankt und tr\u00e4hnt\u2019", "tokens": ["als", "das", "ge\u00b7plag\u00b7te", "Bett'", "ohn", "En\u00b7de", "wankt", "und", "tr\u00e4hnt'"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "APPR", "NN", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "erwacht\u2019/ und lie\u00df ob man ihm freundlich zu: schon", "tokens": ["er\u00b7wacht'", "/", "und", "lie\u00df", "ob", "man", "ihm", "freund\u00b7lich", "zu", ":", "schon"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "$(", "KON", "VVFIN", "KOUS", "PIS", "PPER", "ADJD", "PTKVZ", "$.", "ADV"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "dennoch nicht sein Geschrey und furchtsam weinen", "tokens": ["den\u00b7noch", "nicht", "sein", "Ge\u00b7schrey", "und", "furcht\u00b7sam", "wei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PTKNEG", "PPOSAT", "NN", "KON", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Die Mutter: Kloris geh/ geschwind und nim ein", "tokens": ["Die", "Mut\u00b7ter", ":", "Klo\u00b7ris", "geh", "/", "ge\u00b7schwind", "und", "nim", "ein"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$.", "NE", "VVFIN", "$(", "ADJD", "KON", "VVIMP", "ART"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "ohn Ursach pflegt das Kind so hart zu weinen nicht.", "tokens": ["ohn", "Ur\u00b7sach", "pflegt", "das", "Kind", "so", "hart", "zu", "wei\u00b7nen", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "ADV", "ADJD", "PTKZU", "VVINF", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "die Tochter nierkte bald/ es h\u00e4tte nicht Gefahr", "tokens": ["die", "Toch\u00b7ter", "nierk\u00b7te", "bald", "/", "es", "h\u00e4t\u00b7te", "nicht", "Ge\u00b7fahr"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "$(", "PPER", "VAFIN", "PTKNEG", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "dieweil sie ziemlich sonst auch mit Katolisch war.", "tokens": ["die\u00b7weil", "sie", "ziem\u00b7lich", "sonst", "auch", "mit", "Ka\u00b7to\u00b7lisch", "war", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ADV", "APPR", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Doch schlug sie Feuer an. Der Argwohn r\u00fchrt sie", "tokens": ["Doch", "schlug", "sie", "Feu\u00b7er", "an", ".", "Der", "Arg\u00b7wohn", "r\u00fchrt", "sie"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "NN", "PTKVZ", "$.", "ART", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "bi\u00df Sie zur Kammer kahm/ da/ wo die Fi