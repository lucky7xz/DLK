{"dta.poem.20645": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "Den 12. Jenner 1771 .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Als die Mutter zwoer K\u00f6niginnen                   ", "tokens": ["Als", "die", "Mut\u00b7ter", "zwoer", "K\u00f6\u00b7ni\u00b7gin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADJA", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Ihren ", "tokens": ["Ih\u00b7ren"], "token_info": ["word"], "pos": ["PPOSAT"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Wu\u00dften alle Castalinnen,", "tokens": ["Wu\u00df\u00b7ten", "al\u00b7le", "Cas\u00b7ta\u00b7lin\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df ein Held geboren war.", "tokens": ["Da\u00df", "ein", "Held", "ge\u00b7bo\u00b7ren", "war", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Alle wandten gierig ihr Gesichte", "tokens": ["Al\u00b7le", "wand\u00b7ten", "gie\u00b7rig", "ihr", "Ge\u00b7sich\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "ADJD", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Nach der Clio, welche schon", "tokens": ["Nach", "der", "Clio", ",", "wel\u00b7che", "schon"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NE", "$,", "PRELS", "ADV"], "meter": "+-++-+", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Seine k\u00fcnftige Geschichte,", "tokens": ["Sei\u00b7ne", "k\u00fcnf\u00b7ti\u00b7ge", "Ge\u00b7schich\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sang in ihrer Laute Ton.", "tokens": ["Sang", "in", "ih\u00b7rer", "Lau\u00b7te", "Ton", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Schwestern, sang sie, dieser unterwindet", "tokens": ["Schwes\u00b7tern", ",", "sang", "sie", ",", "die\u00b7ser", "un\u00b7ter\u00b7win\u00b7det"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "$,", "VVFIN", "PPER", "$,", "PDS", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Sich so viel als Mars zu thun,", "tokens": ["Sich", "so", "viel", "als", "Mars", "zu", "thun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "ADV", "KOUS", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Seiner Jahre Mittag findet", "tokens": ["Sei\u00b7ner", "Jah\u00b7re", "Mit\u00b7tag", "fin\u00b7det"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihn im Lorbeerschatten ruhn.", "tokens": ["Ihn", "im", "Lor\u00b7beer\u00b7schat\u00b7ten", "ruhn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Euch gewogen, theilt Er Blumenkr\u00e4nze", "tokens": ["Euch", "ge\u00b7wo\u00b7gen", ",", "theilt", "Er", "Blu\u00b7men\u00b7kr\u00e4n\u00b7ze"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVPP", "$,", "VVFIN", "PPER", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Euren Kindern aus, und lebt", "tokens": ["Eu\u00b7ren", "Kin\u00b7dern", "aus", ",", "und", "lebt"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "PTKVZ", "$,", "KON", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dann noch andre funfzig Lenze,", "tokens": ["Dann", "noch", "and\u00b7re", "funf\u00b7zig", "Len\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJA", "CARD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bis Ihn Vater Zevs erhebt.", "tokens": ["Bis", "Ihn", "Va\u00b7ter", "Zevs", "er\u00b7hebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "NN", "NE", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Ewig, ewig singen Ihm die Musen", "tokens": ["E\u00b7wig", ",", "e\u00b7wig", "sin\u00b7gen", "Ihm", "die", "Mu\u00b7sen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJD", "$,", "ADJD", "VVFIN", "PPER", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Auf der Insul Albion;", "tokens": ["Auf", "der", "In\u00b7sul", "Al\u00b7bi\u00b7on", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "So besang der Syracusen", "tokens": ["So", "be\u00b7sang", "der", "Sy\u00b7ra\u00b7cu\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Loblied den Timoleon.", "tokens": ["Lob\u00b7lied", "den", "Ti\u00b7mo\u00b7le\u00b7on", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NE", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Ihre Dichter t\u00f6nten in die Leyer:", "tokens": ["Ih\u00b7re", "Dich\u00b7ter", "t\u00f6n\u00b7ten", "in", "die", "Le\u00b7yer", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "\u201eenkel singt mit uns ein Lied,", "tokens": ["\u201e", "en\u00b7kel", "singt", "mit", "uns", "ein", "Lied", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "VVFIN", "APPR", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "\u201edem Corinther, dem Befreyer,", "tokens": ["\u201e", "dem", "Co\u00b7rin\u00b7ther", ",", "dem", "Be\u00b7fre\u00b7yer", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "\u201eder jezt vom Olympus sieht.", "tokens": ["\u201e", "der", "jezt", "vom", "O\u00b7lym\u00b7pus", "sieht", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADV", "APPRART", "NE", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}