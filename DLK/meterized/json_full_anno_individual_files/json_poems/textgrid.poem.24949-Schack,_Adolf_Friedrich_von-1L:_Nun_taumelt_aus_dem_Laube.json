{"textgrid.poem.24949": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nun taumelt aus dem Laube", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun taumelt aus dem Laube", "tokens": ["Nun", "tau\u00b7melt", "aus", "dem", "Lau\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Traube", "tokens": ["Die", "Trau\u00b7be"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Ins durst'ge Fa\u00df wie toll;", "tokens": ["Ins", "dur\u00b7st'\u00b7ge", "Fa\u00df", "wie", "toll", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "KOKOM", "ADJD", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Wie stolpern und wie knarren", "tokens": ["Wie", "stol\u00b7pern", "und", "wie", "knar\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "KON", "PWAV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die schwer bepackten Karren,", "tokens": ["Die", "schwer", "be\u00b7pack\u00b7ten", "Kar\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Des s\u00fc\u00dfen Weines voll!", "tokens": ["Des", "s\u00fc\u00b7\u00dfen", "Wei\u00b7nes", "voll", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wie h\u00fcpft in Freudent\u00e4nzen,", "tokens": ["Wie", "h\u00fcpft", "in", "Freu\u00b7den\u00b7t\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit Kr\u00e4nzen", "tokens": ["Mit", "Kr\u00e4n\u00b7zen"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Von Weinlaub in dem Haar,", "tokens": ["Von", "Wein\u00b7laub", "in", "dem", "Haar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Zu bacchischen Ges\u00e4ngen", "tokens": ["Zu", "bac\u00b7chi\u00b7schen", "Ge\u00b7s\u00e4n\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und der Pokale Kl\u00e4ngen", "tokens": ["Und", "der", "Po\u00b7ka\u00b7le", "Kl\u00e4n\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die lust'ge Winzerschar!", "tokens": ["Die", "lust'\u00b7ge", "Win\u00b7zer\u00b7schar", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wie spr\u00fchn aus Dorf und St\u00e4dten", "tokens": ["Wie", "spr\u00fchn", "aus", "Dorf", "und", "St\u00e4d\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Raketen", "tokens": ["Ra\u00b7ke\u00b7ten"], "token_info": ["word"], "pos": ["NN"], "meter": "+--", "measure": "dactylic.init"}, "line.3": {"text": "Um Busch und Felsenkamm!", "tokens": ["Um", "Busch", "und", "Fel\u00b7sen\u00b7kamm", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "So huld'gen die Provinzen", "tokens": ["So", "huld'\u00b7gen", "die", "Pro\u00b7vin\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Dem neugebornen Prinzen", "tokens": ["Dem", "neu\u00b7ge\u00b7bor\u00b7nen", "Prin\u00b7zen"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Vom R\u00fcdesheimer Stamm.", "tokens": ["Vom", "R\u00fc\u00b7des\u00b7hei\u00b7mer", "Stamm", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "O Prinz, in dessen sch\u00f6nen", "tokens": ["O", "Prinz", ",", "in", "des\u00b7sen", "sch\u00f6\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "$,", "APPR", "PDS", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dom\u00e4nen", "tokens": ["Do\u00b7m\u00e4\u00b7nen"], "token_info": ["word"], "pos": ["NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Der Tag nicht untergeht,", "tokens": ["Der", "Tag", "nicht", "un\u00b7ter\u00b7geht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Du bist der F\u00fcrst der F\u00fcrsten;", "tokens": ["Du", "bist", "der", "F\u00fcrst", "der", "F\u00fcrs\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "So weit die Menschen d\u00fcrsten", "tokens": ["So", "weit", "die", "Men\u00b7schen", "d\u00fcrs\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "ART", "NN", "VMFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Reicht deine Majest\u00e4t!", "tokens": ["Reicht", "dei\u00b7ne", "Ma\u00b7jes\u00b7t\u00e4t", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Auf! Schie\u00dft von allen S\u00f6llern", "tokens": ["Auf", "!", "Schie\u00dft", "von", "al\u00b7len", "S\u00f6l\u00b7lern"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "$.", "VVFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit B\u00f6llern,", "tokens": ["Mit", "B\u00f6l\u00b7lern", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Und l\u00e4utet fr\u00fch und spat", "tokens": ["Und", "l\u00e4u\u00b7tet", "fr\u00fch", "und", "spat"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "KON", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Mit Gl\u00e4sern und mit Glocken,", "tokens": ["Mit", "Gl\u00e4\u00b7sern", "und", "mit", "Glo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und sind noch Kehlen trocken,", "tokens": ["Und", "sind", "noch", "Keh\u00b7len", "tro\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "NN", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Das nenn' ich Hochverrat!", "tokens": ["Das", "nenn'", "ich", "Hoch\u00b7ver\u00b7rat", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Nicht wir nur, die wir leben,", "tokens": ["Nicht", "wir", "nur", ",", "die", "wir", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PPER", "ADV", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ergeben", "tokens": ["Er\u00b7ge\u00b7ben"], "token_info": ["word"], "pos": ["NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Uns heut der Freudigkeit,", "tokens": ["Uns", "heut", "der", "Freu\u00b7dig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Es wird den alten Rittern,", "tokens": ["Es", "wird", "den", "al\u00b7ten", "Rit\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wie sie den Weinduft wittern,", "tokens": ["Wie", "sie", "den", "Wein\u00b7duft", "wit\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Im Sarg das Herz so weit.", "tokens": ["Im", "Sarg", "das", "Herz", "so", "weit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Die Deckel, sie beengend,", "tokens": ["Die", "De\u00b7ckel", ",", "sie", "be\u00b7en\u00b7gend", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zersprengend", "tokens": ["Zer\u00b7spren\u00b7gend"], "token_info": ["word"], "pos": ["VVPP"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Entsteigen sie der Gruft;", "tokens": ["Ent\u00b7stei\u00b7gen", "sie", "der", "Gruft", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Willkommen, Licht der Sonnen!", "tokens": ["Will\u00b7kom\u00b7men", ",", "Licht", "der", "Son\u00b7nen", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Willkommen, s\u00fc\u00dfer Bronnen", "tokens": ["Will\u00b7kom\u00b7men", ",", "s\u00fc\u00b7\u00dfer", "Bron\u00b7nen"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Von herzerquickendem Duft!", "tokens": ["Von", "herz\u00b7er\u00b7qui\u00b7cken\u00b7dem", "Duft", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.8": {"line.1": {"text": "Mit Giselher und Gunther", "tokens": ["Mit", "Gi\u00b7sel\u00b7her", "und", "Gun\u00b7ther"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Naht munter", "tokens": ["Naht", "mun\u00b7ter"], "token_info": ["word", "word"], "pos": ["NN", "ADJD"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Chriemhild, die sch\u00f6ne Maid,", "tokens": ["Chriem\u00b7hild", ",", "die", "sch\u00f6\u00b7ne", "Maid", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Nebst Helden rings in Heeren,", "tokens": ["Nebst", "Hel\u00b7den", "rings", "in", "Hee\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Davon in alten M\u00e4ren", "tokens": ["Da\u00b7von", "in", "al\u00b7ten", "M\u00e4\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "So wunderviel geseit.", "tokens": ["So", "wun\u00b7der\u00b7viel", "ge\u00b7seit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Es scheint den wackern Recken", "tokens": ["Es", "scheint", "den", "wa\u00b7ckern", "Re\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zu schmecken;", "tokens": ["Zu", "schme\u00b7cken", ";"], "token_info": ["word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Ihr Helm ist ihr Pokal;", "tokens": ["Ihr", "Helm", "ist", "ihr", "Po\u00b7kal", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Der eine braucht schon Hebel;", "tokens": ["Der", "ei\u00b7ne", "braucht", "schon", "He\u00b7bel", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "ADV", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der andre schwankt im Nebel", "tokens": ["Der", "and\u00b7re", "schwankt", "im", "Ne\u00b7bel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PIS", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Benebelt durch das Thal.", "tokens": ["Be\u00b7ne\u00b7belt", "durch", "das", "Thal", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}}, "stanza.10": {"line.1": {"text": "Seht, wo der Rhein erflimmert,", "tokens": ["Seht", ",", "wo", "der", "Rhein", "er\u00b7flim\u00b7mert", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "ART", "NE", "VVFIN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Da zimmert", "tokens": ["Da", "zim\u00b7mert"], "token_info": ["word", "word"], "pos": ["ADV", "VVFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Der Mond ein Flo\u00df von Gold,", "tokens": ["Der", "Mond", "ein", "Flo\u00df", "von", "Gold", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und auf dem Mondscheinflo\u00dfe", "tokens": ["Und", "auf", "dem", "Mond\u00b7schein\u00b7flo\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Liegt schnarchend Karl der Gro\u00dfe,", "tokens": ["Liegt", "schnar\u00b7chend", "Karl", "der", "Gro\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "NE", "ART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der gro\u00dfe Trunkenbold.", "tokens": ["Der", "gro\u00b7\u00dfe", "Trun\u00b7ken\u00b7bold", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Ein Gru\u00df sei auch den Toten", "tokens": ["Ein", "Gru\u00df", "sei", "auch", "den", "To\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Entboten,", "tokens": ["Ent\u00b7bo\u00b7ten", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Dies Glas der ganzen Welt!", "tokens": ["Dies", "Glas", "der", "gan\u00b7zen", "Welt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Eu'r Wohlsein, ihr Gespenster!", "tokens": ["Eu'r", "Wohl\u00b7sein", ",", "ihr", "Ge\u00b7spens\u00b7ter", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "Dein Wohlergehn, geschw\u00e4nzter", "tokens": ["Dein", "Woh\u00b7ler\u00b7gehn", ",", "ge\u00b7schw\u00e4nz\u00b7ter"], "token_info": ["word", "word", "punct", "word"], "pos": ["PPOSAT", "NN", "$,", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Komet am Himmelszelt!", "tokens": ["Ko\u00b7met", "am", "Him\u00b7mels\u00b7zelt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.12": {"line.1": {"text": "Ja! m\u00f6gen dich die Pfaffen", "tokens": ["Ja", "!", "m\u00f6\u00b7gen", "dich", "die", "Pfaf\u00b7fen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$.", "VMFIN", "PRF", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Begaffen,", "tokens": ["Be\u00b7ga\u00b7ffen", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Un