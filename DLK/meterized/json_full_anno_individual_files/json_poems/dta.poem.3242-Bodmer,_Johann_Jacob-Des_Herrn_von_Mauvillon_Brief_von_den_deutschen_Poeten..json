{"dta.poem.3242": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Des Herrn von Mauvillon Brief  \n von den deutschen Poeten.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1742", "urn": "urn:nbn:de:kobv:b4-200905198490", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Komm nur! Verziehe nicht den Adler zu besteigen,", "tokens": ["Komm", "nur", "!", "Ver\u00b7zie\u00b7he", "nicht", "den", "Ad\u00b7ler", "zu", "be\u00b7stei\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$.", "VVFIN", "PTKNEG", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und hei\u00df als deutscher Jupiter", "tokens": ["Und", "hei\u00df", "als", "deut\u00b7scher", "Ju\u00b7pi\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "KOKOM", "ADJA", "NN"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.3": {"text": "Doch einst des eiteln ", "tokens": ["Doch", "einst", "des", "ei\u00b7teln"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Vor deines Donners Knall und Drohen schweigen.", "tokens": ["Vor", "dei\u00b7nes", "Don\u00b7ners", "Knall", "und", "Dro\u00b7hen", "schwei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}