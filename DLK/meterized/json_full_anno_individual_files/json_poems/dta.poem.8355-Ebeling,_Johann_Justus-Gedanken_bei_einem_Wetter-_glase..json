{"dta.poem.8355": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Gedanken bei einem Wetter-  \n glase.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198782", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Mensch ist offtermahls dem Wetter-\nglase gleich,", "tokens": ["Der", "Mensch", "ist", "off\u00b7ter\u00b7mahls", "dem", "Wet\u00b7ter", "gla\u00b7se", "gleich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ART", "TRUNC", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Darinnen der Mercur bald aufwerts", "tokens": ["Da\u00b7rin\u00b7nen", "der", "Mer\u00b7cur", "bald", "auf\u00b7werts"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "ADV", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Nachdem die Witterung ihn in der L\u00fcf-", "tokens": ["Nach\u00b7dem", "die", "Wit\u00b7te\u00b7rung", "ihn", "in", "der", "L\u00fcf"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "PPER", "APPR", "ART", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Zum Steigen und zum Fall durch ihre Aendrung", "tokens": ["Zum", "Stei\u00b7gen", "und", "zum", "Fall", "durch", "ih\u00b7re", "A\u00b7en\u00b7drung"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "KON", "APPRART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Das menschliche Gem\u00fcth, pflegt bei dem Gl\u00fckkes-", "tokens": ["Das", "menschli\u00b7che", "Ge\u00b7m\u00fcth", ",", "pflegt", "bei", "dem", "Gl\u00fc\u00b7kkes"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "VVFIN", "APPR", "ART", "TRUNC"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Sich in gebl\u00e4hten Stolz gewaltig zu erheben.", "tokens": ["Sich", "in", "ge\u00b7bl\u00e4h\u00b7ten", "Stolz", "ge\u00b7wal\u00b7tig", "zu", "er\u00b7he\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ADJA", "NN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und als das Gl\u00fckke w\u00e4chst, wie trifts nicht oftmahls", "tokens": ["Und", "als", "das", "Gl\u00fck\u00b7ke", "w\u00e4chst", ",", "wie", "trifts", "nicht", "oft\u00b7mahls"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "VVFIN", "$,", "PWAV", "VVFIN", "PTKNEG", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Nach einer H\u00f6h vom Dunst, mit aller Macht zu", "tokens": ["Nach", "ei\u00b7ner", "H\u00f6h", "vom", "Dunst", ",", "mit", "al\u00b7ler", "Macht", "zu"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPRART", "NN", "$,", "APPR", "PIAT", "NN", "APPR"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Allein wie \u00e4ndert sich die arme Kreatur,", "tokens": ["Al\u00b7lein", "wie", "\u00e4n\u00b7dert", "sich", "die", "ar\u00b7me", "Kre\u00b7a\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "VVFIN", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Wenn eine rauhe Zeit von Tr\u00fcbsal angekommen", "tokens": ["Wenn", "ei\u00b7ne", "rau\u00b7he", "Zeit", "von", "Tr\u00fcb\u00b7sal", "an\u00b7ge\u00b7kom\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "APPR", "NN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Da f\u00e4llet gleich der Muth wird \u00e4hnlich dem Mer-", "tokens": ["Da", "f\u00e4l\u00b7let", "gleich", "der", "Muth", "wird", "\u00e4hn\u00b7lich", "dem", "Mer"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "VAFIN", "ADJD", "ART", "TRUNC"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.12": {"text": "Der in des Glases R\u00f6hr den niedren Stand ge-", "tokens": ["Der", "in", "des", "Gla\u00b7ses", "R\u00f6hr", "den", "nied\u00b7ren", "Stand", "ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "NE", "ART", "ADJA", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Ein wollgesezt Gem\u00fcth bleibt immer einerlei,", "tokens": ["Ein", "woll\u00b7ge\u00b7sezt", "Ge\u00b7m\u00fcth", "bleibt", "im\u00b7mer", "ei\u00b7ner\u00b7lei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "NN", "VVFIN", "ADV", "PIS", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Jm Gl\u00fck und Ungel\u00fck in hell und tr\u00fcben Ta-", "tokens": ["Jm", "Gl\u00fck", "und", "Un\u00b7ge\u00b7l\u00fck", "in", "hell", "und", "tr\u00fc\u00b7ben", "Ta"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "KON", "NN", "APPR", "ADJD", "KON", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Es bleibt in Sanftmuth still, in wahrer Demuth", "tokens": ["Es", "bleibt", "in", "Sanft\u00b7muth", "still", ",", "in", "wah\u00b7rer", "De\u00b7muth"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "PTKVZ", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.16": {"text": "Und schwinget sich empor bei allen Leid und Pla-", "tokens": ["Und", "schwin\u00b7get", "sich", "em\u00b7por", "bei", "al\u00b7len", "Leid", "und", "Pla"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "PTKVZ", "APPR", "PIAT", "NN", "KON", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}