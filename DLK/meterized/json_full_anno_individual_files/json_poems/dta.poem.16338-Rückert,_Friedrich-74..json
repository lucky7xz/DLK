{"dta.poem.16338": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "74.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sieh an die Pflanze, die empor aus dunklem Grunde", "tokens": ["Sieh", "an", "die", "Pflan\u00b7ze", ",", "die", "em\u00b7por", "aus", "dunk\u00b7lem", "Grun\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "ART", "NN", "$,", "PRELS", "PTKVZ", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zum Lichte treibt, von dem sie auch hat dunkle Kunde.", "tokens": ["Zum", "Lich\u00b7te", "treibt", ",", "von", "dem", "sie", "auch", "hat", "dunk\u00b7le", "Kun\u00b7de", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,", "APPR", "PRELS", "PPER", "ADV", "VAFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Mit ihrem Stengel steht sie erst in Einigkeit,", "tokens": ["Mit", "ih\u00b7rem", "Sten\u00b7gel", "steht", "sie", "erst", "in", "Ei\u00b7nig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und im Gezweige dann ist sie mit sich entzweit.", "tokens": ["Und", "im", "Ge\u00b7zwei\u00b7ge", "dann", "ist", "sie", "mit", "sich", "ent\u00b7zweit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "ADV", "VAFIN", "PPER", "APPR", "PRF", "PTKVZ", "$."], "meter": "-+-+--+-+--+", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Nicht in der Einung noch Entzweiung ist gefunden", "tokens": ["Nicht", "in", "der", "Ei\u00b7nung", "noch", "Ent\u00b7zwei\u00b7ung", "ist", "ge\u00b7fun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "APPR", "ART", "NN", "ADV", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das Licht, bis h\u00f6here Vereinung sie verbunden.", "tokens": ["Das", "Licht", ",", "bis", "h\u00f6\u00b7he\u00b7re", "Ver\u00b7ei\u00b7nung", "sie", "ver\u00b7bun\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "ADJA", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Die Knospe rundet sich, aus der die Bl\u00fct' erwacht,", "tokens": ["Die", "Knos\u00b7pe", "run\u00b7det", "sich", ",", "aus", "der", "die", "Bl\u00fct'", "er\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "$,", "APPR", "PRELS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "In deren Farbenduft das Licht ist angefacht.", "tokens": ["In", "de\u00b7ren", "Far\u00b7ben\u00b7duft", "das", "Licht", "ist", "an\u00b7ge\u00b7facht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Durch soviel Stufen hat das Licht die Pflanz' erzogen,", "tokens": ["Durch", "so\u00b7viel", "Stu\u00b7fen", "hat", "das", "Licht", "die", "Pflanz'", "er\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VAFIN", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Um auf der obersten zu ruhn als Irisbogen.", "tokens": ["Um", "auf", "der", "o\u00b7bers\u00b7ten", "zu", "ruhn", "als", "I\u00b7ris\u00b7bo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "APPR", "ART", "ADJA", "PTKZU", "VVINF", "KOKOM", "NN", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.6": {"line.1": {"text": "Das Leben der Natur ist eine solche Pflanze,", "tokens": ["Das", "Le\u00b7ben", "der", "Na\u00b7tur", "ist", "ei\u00b7ne", "sol\u00b7che", "Pflan\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "ART", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die aus sich selber ringt empor zu Gottes Glanze.", "tokens": ["Die", "aus", "sich", "sel\u00b7ber", "ringt", "em\u00b7por", "zu", "Got\u00b7tes", "Glan\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PRF", "ADV", "VVFIN", "PTKVZ", "APPR", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Die Wurzel ist Gestein, Gew\u00e4chsreich ist der Stiel,", "tokens": ["Die", "Wur\u00b7zel", "ist", "Ge\u00b7stein", ",", "Ge\u00b7w\u00e4chs\u00b7reich", "ist", "der", "Stiel", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "NN", "$,", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bl\u00e4tterverzweigungen Thierlebens reges Spiel.", "tokens": ["Bl\u00e4t\u00b7ter\u00b7ver\u00b7zwei\u00b7gun\u00b7gen", "Thier\u00b7le\u00b7bens", "re\u00b7ges", "Spiel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "$."], "meter": "+--+--+-+--+", "measure": "dactylic.di.plus"}}, "stanza.8": {"line.1": {"text": "Doch neues Leben ist von oben angez\u00fcndet,", "tokens": ["Doch", "neu\u00b7es", "Le\u00b7ben", "ist", "von", "o\u00b7ben", "an\u00b7ge\u00b7z\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VAFIN", "APPR", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo der Naturtrieb sich im Menschenantlitz r\u00fcndet;", "tokens": ["Wo", "der", "Na\u00b7tur\u00b7trieb", "sich", "im", "Men\u00b7schen\u00b7ant\u00b7litz", "r\u00fcn\u00b7det", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PRF", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Da ist des Himmels Stral im Irdischen verk\u00fcndet.", "tokens": ["Da", "ist", "des", "Him\u00b7mels", "Stral", "im", "Ir\u00b7di\u00b7schen", "ver\u00b7k\u00fcn\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Die Rose der Natur hat ihre Bl\u00fctenkrone", "tokens": ["Die", "Ro\u00b7se", "der", "Na\u00b7tur", "hat", "ih\u00b7re", "Bl\u00fc\u00b7ten\u00b7kro\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Entfaltet, da\u00df in ihr der Duft der Seele wohne.", "tokens": ["Ent\u00b7fal\u00b7tet", ",", "da\u00df", "in", "ihr", "der", "Duft", "der", "See\u00b7le", "woh\u00b7ne", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "APPR", "PPOSAT", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.11": {"line.1": {"text": "Die Rose, sterbend, haucht den Duft in Himmelsluft;", "tokens": ["Die", "Ro\u00b7se", ",", "ster\u00b7bend", ",", "haucht", "den", "Duft", "in", "Him\u00b7mel\u00b7sluft", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVPP", "$,", "VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So stirb, ein himmeleingesogner Bl\u00fctenduft!", "tokens": ["So", "stirb", ",", "ein", "him\u00b7mel\u00b7ein\u00b7ge\u00b7sog\u00b7ner", "Bl\u00fc\u00b7ten\u00b7duft", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.12": {"line.1": {"text": "Die Rose, lebend, haucht Duft \u00fcber Liebesgr\u00fcften;", "tokens": ["Die", "Ro\u00b7se", ",", "le\u00b7bend", ",", "haucht", "Duft", "\u00fc\u00b7ber", "Lie\u00b7bes\u00b7gr\u00fcf\u00b7ten", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "$,", "VVFIN", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So leb', ein himmelan entbundnes Liebesd\u00fcften!", "tokens": ["So", "leb'", ",", "ein", "him\u00b7me\u00b7lan", "ent\u00b7bund\u00b7nes", "Lie\u00b7bes\u00b7d\u00fcf\u00b7ten", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "ART", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}