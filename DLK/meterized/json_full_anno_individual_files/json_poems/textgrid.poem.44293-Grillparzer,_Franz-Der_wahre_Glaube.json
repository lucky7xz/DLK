{"textgrid.poem.44293": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Der wahre Glaube", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dahingestreckt auf gr\u00fcne Matten,", "tokens": ["Da\u00b7hin\u00b7ge\u00b7streckt", "auf", "gr\u00fc\u00b7ne", "Mat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Lag, mit Almansorn Hand in Hand,", "tokens": ["Lag", ",", "mit", "Al\u00b7man\u00b7sorn", "Hand", "in", "Hand", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "NN", "NN", "APPR", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Nadir an eines Baches Rand,", "tokens": ["Na\u00b7dir", "an", "ei\u00b7nes", "Ba\u00b7ches", "Rand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In einer Palme k\u00fchlen Schatten.", "tokens": ["In", "ei\u00b7ner", "Pal\u00b7me", "k\u00fch\u00b7len", "Schat\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Es war im Lenz. Mit s\u00fc\u00dfer Kehle", "tokens": ["Es", "war", "im", "Lenz", ".", "Mit", "s\u00fc\u00b7\u00dfer", "Keh\u00b7le"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "$.", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sang ein erfreuter V\u00f6gelchor", "tokens": ["Sang", "ein", "er\u00b7freu\u00b7ter", "V\u00f6\u00b7gelc\u00b7hor"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Aus jedem Busche froh hervor,", "tokens": ["Aus", "je\u00b7dem", "Bu\u00b7sche", "froh", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und z\u00e4rtlich klagte Philomele.", "tokens": ["Und", "z\u00e4rt\u00b7lich", "klag\u00b7te", "Phi\u00b7lo\u00b7me\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "NE", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Die Freunde sind ganz Ohr. Sie lauschen", "tokens": ["Die", "Freun\u00b7de", "sind", "ganz", "Ohr", ".", "Sie", "lau\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV", "NN", "$.", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dem tausendstimmigem Konzert", "tokens": ["Dem", "tau\u00b7sends\u00b7tim\u00b7mi\u00b7gem", "Kon\u00b7zert"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Waldbewohner, jeder h\u00f6rt", "tokens": ["Der", "Wald\u00b7be\u00b7woh\u00b7ner", ",", "je\u00b7der", "h\u00f6rt"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "PIS", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit stiller Lust des Baches Rauschen.", "tokens": ["Mit", "stil\u00b7ler", "Lust", "des", "Ba\u00b7ches", "Rau\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Bis doch Nadir das lange Schweigen,", "tokens": ["Bis", "doch", "Na\u00b7dir", "das", "lan\u00b7ge", "Schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den Freund umarmend, unterbricht,", "tokens": ["Den", "Freund", "um\u00b7ar\u00b7mend", ",", "un\u00b7ter\u00b7bricht", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Almansorn k\u00fcsset und dann spricht,", "tokens": ["Al\u00b7man\u00b7sorn", "k\u00fcs\u00b7set", "und", "dann", "spricht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "KON", "ADV", "VVFIN", "$,"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.4": {"text": "Mit Mienen, welche Ehrfurcht zeigen.", "tokens": ["Mit", "Mie\u00b7nen", ",", "wel\u00b7che", "Ehr\u00b7furcht", "zei\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PWAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Dein Geist zerrei\u00dfet jene Bande,", "tokens": ["Dein", "Geist", "zer\u00b7rei\u00b7\u00dfet", "je\u00b7ne", "Ban\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Aberglaub um Menschen schlingt,", "tokens": ["Die", "A\u00b7berg\u00b7laub", "um", "Men\u00b7schen", "schlingt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und da dein Aug den Wahn durchdringt,", "tokens": ["Und", "da", "dein", "Aug", "den", "Wahn", "durch\u00b7dringt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPOSAT", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nennt man dich weis' im ganzen Lande.", "tokens": ["Nennt", "man", "dich", "weis'", "im", "gan\u00b7zen", "Lan\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PPER", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Drum sprich: Wer lehrt die V\u00f6gel Lieder?", "tokens": ["Drum", "sprich", ":", "Wer", "lehrt", "die", "V\u00f6\u00b7gel", "Lie\u00b7der", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "$.", "PWS", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wer schuf sie selbst? auf wessen Ruf", "tokens": ["Wer", "schuf", "sie", "selbst", "?", "auf", "wes\u00b7sen", "Ruf"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "$.", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Entstanden Welten? und wer schuf", "tokens": ["Ent\u00b7stan\u00b7den", "Wel\u00b7ten", "?", "und", "wer", "schuf"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "KON", "PWS", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der W\u00e4lder Volk, der Luft Gefieder?", "tokens": ["Der", "W\u00e4l\u00b7der", "Volk", ",", "der", "Luft", "Ge\u00b7fie\u00b7der", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Kannst du mir alles dies erkl\u00e4ren?", "tokens": ["Kannst", "du", "mir", "al\u00b7les", "dies", "er\u00b7kl\u00e4\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "PIS", "PDS", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sag, Freund! mir, wie der Sch\u00f6pfer hei\u00dft!", "tokens": ["Sag", ",", "Freund", "!", "mir", ",", "wie", "der", "Sch\u00f6p\u00b7fer", "hei\u00dft", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$.", "PPER", "$,", "PWAV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dann lob ich deinen gro\u00dfen Geist", "tokens": ["Dann", "lob", "ich", "dei\u00b7nen", "gro\u00b7\u00dfen", "Geist"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und will wie einen Gott dich ehren.", "tokens": ["Und", "will", "wie", "ei\u00b7nen", "Gott", "dich", "eh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "KOKOM", "ART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Doch nicht allein will ich ihn kennen,", "tokens": ["Doch", "nicht", "al\u00b7lein", "will", "ich", "ihn", "ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ADV", "VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den unbegreifbar m\u00e4chtgen Gott;", "tokens": ["Den", "un\u00b7be\u00b7greif\u00b7bar", "m\u00e4cht\u00b7gen", "Gott", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auch wissen will ich sein Gebot,", "tokens": ["Auch", "wis\u00b7sen", "will", "ich", "sein", "Ge\u00b7bot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "VMFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auch meine Pflichten mu\u00dft du nennen!", "tokens": ["Auch", "mei\u00b7ne", "Pflich\u00b7ten", "mu\u00dft", "du", "nen\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Almansor spricht, und Tr\u00e4nen n\u00e4ssen", "tokens": ["Al\u00b7man\u00b7sor", "spricht", ",", "und", "Tr\u00e4\u00b7nen", "n\u00e4s\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VVFIN", "$,", "KON", "NN", "VVINF"], "meter": "---+-+-+-", "measure": "unknown.measure.tri"}, "line.2": {"text": "Dabei sein w\u00fcrdig Angesicht;", "tokens": ["Da\u00b7bei", "sein", "w\u00fcr\u00b7dig", "An\u00b7ge\u00b7sicht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "PPOSAT", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wer jener ist, das wei\u00df ich nicht,", "tokens": ["Wer", "je\u00b7ner", "ist", ",", "das", "wei\u00df", "ich", "nicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PDS", "VAFIN", "$,", "PDS", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wer kann den Ewigen ermessen?", "tokens": ["Wer", "kann", "den", "E\u00b7wi\u00b7gen", "er\u00b7mes\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Er hei\u00dfe Jupiter, hei\u00df Allah;", "tokens": ["Er", "hei\u00b7\u00dfe", "Ju\u00b7pi\u00b7ter", ",", "hei\u00df", "Al\u00b7lah", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,", "ADJD", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Bet Ormuzd, bet Osiris an", "tokens": ["Bet", "Or\u00b7muzd", ",", "bet", "O\u00b7si\u00b7ris", "an"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NE", "$,", "VVFIN", "NE", "PTKVZ"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Und sei ein Christ, sei ein Braman,", "tokens": ["Und", "sei", "ein", "Christ", ",", "sei", "ein", "Bra\u00b7man", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "$,", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Verehr den Wischnu, den Jehovah!", "tokens": ["Ver\u00b7ehr", "den", "Wischnu", ",", "den", "Je\u00b7ho\u00b7vah", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Doch nur erf\u00fclle deine Pflichten! \u2013", "tokens": ["Doch", "nur", "er\u00b7f\u00fcl\u00b7le", "dei\u00b7ne", "Pflich\u00b7ten", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Tu jeder Gutes, was er kann,", "tokens": ["Tu", "je\u00b7der", "Gu\u00b7tes", ",", "was", "er", "kann", ","], "token_info": ["wor