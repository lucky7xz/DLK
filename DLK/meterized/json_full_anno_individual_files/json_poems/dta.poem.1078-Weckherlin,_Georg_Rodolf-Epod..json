{"dta.poem.1078": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Epod.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1618", "urn": "urn:nbn:de:kobv:b4-20628-9", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Deinen namen nu so gro\u00df/", "tokens": ["Dei\u00b7nen", "na\u00b7men", "nu", "so", "gro\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "ADV", "ADJD", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Vnd dein lob so hoch erhaben/", "tokens": ["Vnd", "dein", "lob", "so", "hoch", "er\u00b7ha\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADV", "ADJD", "ADJD", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Will Ich mit einem gescho\u00df", "tokens": ["Will", "Ich", "mit", "ei\u00b7nem", "ge\u00b7scho\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Tief in die gedechtnus graben:", "tokens": ["Tief", "in", "die", "ge\u00b7decht\u00b7nus", "gra\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Vnd wan ja der Musen kunst", "tokens": ["Vnd", "wan", "ja", "der", "Mu\u00b7sen", "kunst"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ADV", "ART", "NN", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Nicht gar eytel vnd vmbsunst/", "tokens": ["Nicht", "gar", "ey\u00b7tel", "vnd", "vmbsunst", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "NN", "KON", "ADV", "$("], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Soll nicht weniger auf erden/", "tokens": ["Soll", "nicht", "we\u00b7ni\u00b7ger", "auf", "er\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PTKNEG", "ADV", "APPR", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Dein prei\u00df/ deine ehr vnd zier/", "tokens": ["Dein", "prei\u00df", "/", "dei\u00b7ne", "ehr", "vnd", "zier", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "PPOSAT", "NN", "KON", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Dan ich begehr das von dir", "tokens": ["Dan", "ich", "be\u00b7gehr", "das", "von", "dir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "VVFIN", "ART", "APPR", "PPER"], "meter": "---+--+", "measure": "iambic.di.chol"}, "line.10": {"text": "Meine Vers/ geliebet werden.", "tokens": ["Mei\u00b7ne", "Vers", "/", "ge\u00b7lie\u00b7bet", "wer\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "VVPP", "VAINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}