ord", "word"], "pos": ["KON", "ADV", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Bet ich in dem Breviere", "tokens": ["Bet", "ich", "in", "dem", "Bre\u00b7vie\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und hock, wie ein Magister", "tokens": ["Und", "hock", ",", "wie", "ein", "Ma\u00b7gis\u00b7ter"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PTKVZ", "$,", "PWAV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bei seinem sauren Biere!", "tokens": ["Bei", "sei\u00b7nem", "sau\u00b7ren", "Bie\u00b7re", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.18": {"line.1": {"text": "So ist zu jeden Zeiten", "tokens": ["So", "ist", "zu", "je\u00b7den", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Heuchelei vom B\u00f6sen \u2013", "tokens": ["Die", "Heu\u00b7che\u00b7lei", "vom", "B\u00f6\u00b7sen", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "M\u00f6g uns nach allen Seiten", "tokens": ["M\u00f6g", "uns", "nach", "al\u00b7len", "Sei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Herr davon erl\u00f6sen!", "tokens": ["Der", "Herr", "da\u00b7von", "er\u00b7l\u00f6\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PAV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iamb