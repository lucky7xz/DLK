{"dta.poem.21441": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Abendd\u00e4mmern.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Abendd\u00e4mmern tr\u00fcb und fahl,", "tokens": ["A\u00b7bend\u00b7d\u00e4m\u00b7mern", "tr\u00fcb", "und", "fahl", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KON", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Tiefe Stille webt im Thal.", "tokens": ["Tie\u00b7fe", "Stil\u00b7le", "webt", "im", "Thal", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schleier rings die Berge kr\u00e4nzt,", "tokens": ["Schlei\u00b7er", "rings", "die", "Ber\u00b7ge", "kr\u00e4nzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Selten nur ein Stern ergl\u00e4nzt.", "tokens": ["Sel\u00b7ten", "nur", "ein", "Stern", "er\u00b7gl\u00e4nzt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wellen zieh\u2019n und Winde rauschen,", "tokens": ["Wel\u00b7len", "zieh'n", "und", "Win\u00b7de", "rau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Tr\u00e4umend neig\u2019 ich mich, zu lauschen", "tokens": ["Tr\u00e4u\u00b7mend", "neig'", "ich", "mich", ",", "zu", "lau\u00b7schen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "PRF", "$,", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und mir d\u00fcnkt, da\u00df H\u00f6h\u2019n und Tiefen", "tokens": ["Und", "mir", "d\u00fcnkt", ",", "da\u00df", "H\u00f6h'n", "und", "Tie\u00b7fen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "$,", "KOUS", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und die W\u00e4lder all\u2019 mich riefen.", "tokens": ["Und", "die", "W\u00e4l\u00b7der", "all'", "mich", "rie\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PIS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Unbegreiflich Heimathsehnen \u2014", "tokens": ["Un\u00b7be\u00b7greif\u00b7lich", "Hei\u00b7math\u00b7seh\u00b7nen"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Str\u00f6mt hervor mir hei\u00dfe Thr\u00e4nen.", "tokens": ["Str\u00f6mt", "her\u00b7vor", "mir", "hei\u00b7\u00dfe", "Thr\u00e4\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Seele m\u00f6cht\u2019 den Leib\u2019 verlassen,", "tokens": ["See\u00b7le", "m\u00f6cht'", "den", "Leib'", "ver\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "M\u00f6chte gern ein Ewiges fassen:", "tokens": ["M\u00f6ch\u00b7te", "gern", "ein", "E\u00b7wi\u00b7ges", "fas\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "ADJA", "VVINF", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.13": {"text": "Das in s\u00fc\u00dfem Friedgew\u00e4hren", "tokens": ["Das", "in", "s\u00fc\u00b7\u00dfem", "Fried\u00b7ge\u00b7w\u00e4h\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Sie entr\u00fcckt in holde Sph\u00e4ren \u2026", "tokens": ["Sie", "ent\u00b7r\u00fcckt", "in", "hol\u00b7de", "Sph\u00e4\u00b7ren", "\u2026"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}