{"dta.poem.15766": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "26.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Irrthum ist nicht das, Einbildungen zu haben", "tokens": ["Der", "Irr\u00b7thum", "ist", "nicht", "das", ",", "Ein\u00b7bil\u00b7dun\u00b7gen", "zu", "ha\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PTKNEG", "PDS", "$,", "NN", "PTKZU", "VAINF"], "meter": "-+--+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Unwahrer Dinge, die als wirkliche sich gaben.", "tokens": ["Un\u00b7wah\u00b7rer", "Din\u00b7ge", ",", "die", "als", "wirk\u00b7li\u00b7che", "sich", "ga\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PRELS", "KOKOM", "VVFIN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Der Irrthum ist nur das, vergessen bei den Bildern,", "tokens": ["Der", "Irr\u00b7thum", "ist", "nur", "das", ",", "ver\u00b7ges\u00b7sen", "bei", "den", "Bil\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "PDS", "$,", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df wirklich da nicht ist, was sie als solches schildern.", "tokens": ["Da\u00df", "wirk\u00b7lich", "da", "nicht", "ist", ",", "was", "sie", "als", "sol\u00b7ches", "schil\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "ADV", "PTKNEG", "VAFIN", "$,", "PRELS", "PPER", "KOUS", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wer dieses Bilderspiel kann bringen frei hervor,", "tokens": ["Wer", "die\u00b7ses", "Bil\u00b7der\u00b7spiel", "kann", "brin\u00b7gen", "frei", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PDAT", "NN", "VMFIN", "VVINF", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ist ein Poet, wen unfrei es beherrscht, ein Thor.", "tokens": ["Ist", "ein", "Po\u00b7et", ",", "wen", "un\u00b7frei", "es", "be\u00b7herrscht", ",", "ein", "Thor", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$,", "PWS", "ADJD", "PPER", "VVFIN", "$,", "ART", "NN", "$."], "meter": "-+-+-+---+-+", "measure": "unknown.measure.penta"}}}}}