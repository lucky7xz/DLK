{"dta.poem.12074": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "94.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du siehst die andern rings in einer Form von Glauben,", "tokens": ["Du", "siehst", "die", "an\u00b7dern", "rings", "in", "ei\u00b7ner", "Form", "von", "Glau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "ADV", "APPR", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die kannst du ihnen nicht und sollst sie auch nicht rauben.", "tokens": ["Die", "kannst", "du", "ih\u00b7nen", "nicht", "und", "sollst", "sie", "auch", "nicht", "rau\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "PPER", "PTKNEG", "KON", "VMFIN", "PPER", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Sie glauben, da\u00df die Form die allerh\u00f6chste sei,", "tokens": ["Sie", "glau\u00b7ben", ",", "da\u00df", "die", "Form", "die", "al\u00b7ler\u00b7h\u00f6chs\u00b7te", "sei", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ART", "NN", "ART", "ADJA", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die allereinzige, von allen H\u00fcllen frei.", "tokens": ["Die", "al\u00b7ler\u00b7ein\u00b7zi\u00b7ge", ",", "von", "al\u00b7len", "H\u00fcl\u00b7len", "frei", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "APPR", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Da\u00df eine andre Form gewesen sei zuvor,", "tokens": ["Da\u00df", "ei\u00b7ne", "and\u00b7re", "Form", "ge\u00b7we\u00b7sen", "sei", "zu\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VAPP", "VAFIN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In der das reine Licht noch war verh\u00fcllt vom Flor;", "tokens": ["In", "der", "das", "rei\u00b7ne", "Licht", "noch", "war", "ver\u00b7h\u00fcllt", "vom", "Flor", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "ADJA", "NN", "ADV", "VAFIN", "VVPP", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Das glauben sie; doch da\u00df auch das enth\u00fcllte Licht", "tokens": ["Das", "glau\u00b7ben", "sie", ";", "doch", "da\u00df", "auch", "das", "ent\u00b7h\u00fcll\u00b7te", "Licht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "$.", "ADV", "KOUS", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zuwachses f\u00e4hig sei, das glauben sie dir nicht.", "tokens": ["Zu\u00b7wach\u00b7ses", "f\u00e4\u00b7hig", "sei", ",", "das", "glau\u00b7ben", "sie", "dir", "nicht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "$,", "PDS", "VVFIN", "PPER", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Du aber glaubest, da\u00df, gleichwie aus D\u00e4mmerungen", "tokens": ["Du", "a\u00b7ber", "glau\u00b7best", ",", "da\u00df", ",", "gleich\u00b7wie", "aus", "D\u00e4m\u00b7me\u00b7run\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "$,", "KOUS", "$,", "KON", "APPR", "NN"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der Bildlichkeit ein Licht unbildlicher entsprungen;", "tokens": ["Der", "Bild\u00b7lich\u00b7keit", "ein", "Licht", "un\u00b7bild\u00b7li\u00b7cher", "ent\u00b7sprun\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Auch dies unbildliche wird wieder bildlich hei\u00dfen", "tokens": ["Auch", "dies", "un\u00b7bild\u00b7li\u00b7che", "wird", "wie\u00b7der", "bild\u00b7lich", "hei\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PDS", "ADJA", "VAFIN", "ADV", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vor einem, das nach ihm die D\u00e4mmrung wird zerrei\u00dfen;", "tokens": ["Vor", "ei\u00b7nem", ",", "das", "nach", "ihm", "die", "D\u00e4mm\u00b7rung", "wird", "zer\u00b7rei\u00b7\u00dfen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "PRELS", "APPR", "PPER", "ART", "NN", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Und ewig Gottes Licht aus Klarheit w\u00e4chst in Klarheit", "tokens": ["Und", "e\u00b7wig", "Got\u00b7tes", "Licht", "aus", "Klar\u00b7heit", "w\u00e4chst", "in", "Klar\u00b7heit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "NN", "NN", "APPR", "NN", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Viel Offenbarungen hindurch zur Offenbarheit.", "tokens": ["Viel", "Of\u00b7fen\u00b7ba\u00b7run\u00b7gen", "hin\u00b7durch", "zur", "Of\u00b7fen\u00b7bar\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PAV", "APPRART", "NN", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}}}}}