{"dta.poem.3210": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "61.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie alt ist Gottes Welt? Die Rechnung magst du sparen;", "tokens": ["Wie", "alt", "ist", "Got\u00b7tes", "Welt", "?", "Die", "Rech\u00b7nung", "magst", "du", "spa\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "NN", "NN", "$.", "ART", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ihr Lebensalter z\u00e4hlt sich nicht nach tausend Jahren.", "tokens": ["Ihr", "Le\u00b7bens\u00b7al\u00b7ter", "z\u00e4hlt", "sich", "nicht", "nach", "tau\u00b7send", "Jah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "PTKNEG", "APPR", "CARD", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wenn Gott ist ewig, mu\u00df die Welt auch ewig seyn;", "tokens": ["Wenn", "Gott", "ist", "e\u00b7wig", ",", "mu\u00df", "die", "Welt", "auch", "e\u00b7wig", "seyn", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VAFIN", "ADJD", "$,", "VMFIN", "ART", "NN", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Denn Gott ist unser Licht, und Welten dessen Schein.", "tokens": ["Denn", "Gott", "ist", "un\u00b7ser", "Licht", ",", "und", "Wel\u00b7ten", "des\u00b7sen", "Schein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "PPOSAT", "NN", "$,", "KON", "NN", "PRELAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Kein Licht kann seyn, ohn' auch mit Schein sich zu umzirken,", "tokens": ["Kein", "Licht", "kann", "seyn", ",", "ohn'", "auch", "mit", "Schein", "sich", "zu", "um\u00b7zir\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "VAINF", "$,", "KOUI", "ADV", "APPR", "NN", "PRF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und kein Werkmeister, ohn' ein Meisterwerk zu wirken.", "tokens": ["Und", "kein", "Werk\u00b7meis\u00b7ter", ",", "ohn'", "ein", "Meis\u00b7ter\u00b7werk", "zu", "wir\u00b7ken", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$,", "KOUI", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Warum mu\u00df aber hier sich Gutem B\u00f6ses gatten?", "tokens": ["Wa\u00b7rum", "mu\u00df", "a\u00b7ber", "hier", "sich", "Gu\u00b7tem", "B\u00f6\u00b7ses", "gat\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "ADV", "ADV", "PRF", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil, wenn der Schein vom Licht sich trennt, er wird zum Schatten.", "tokens": ["Weil", ",", "wenn", "der", "Schein", "vom", "Licht", "sich", "trennt", ",", "er", "wird", "zum", "Schat\u00b7ten", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "KOUS", "ART", "NN", "APPRART", "NN", "PRF", "VVFIN", "$,", "PPER", "VAFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Darum, wenn Gottes Glanz, nicht Schatten seyn willst du,", "tokens": ["Da\u00b7rum", ",", "wenn", "Got\u00b7tes", "Glanz", ",", "nicht", "Schat\u00b7ten", "seyn", "willst", "du", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "KOUS", "NN", "NN", "$,", "PTKNEG", "NN", "VAINF", "VMFIN", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So wende nicht dem Licht dich ab, dir selber zu.", "tokens": ["So", "wen\u00b7de", "nicht", "dem", "Licht", "dich", "ab", ",", "dir", "sel\u00b7ber", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKNEG", "ART", "NN", "PPER", "PTKVZ", "$,", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Dein sch\u00f6nstes Streben sei, dem Lichte zuzuwenden", "tokens": ["Dein", "sch\u00f6ns\u00b7tes", "Stre\u00b7ben", "sei", ",", "dem", "Lich\u00b7te", "zu\u00b7zu\u00b7wen\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "$,", "ART", "NN", "VVIZU"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dich und die Welt, so da\u00df euch nicht die Strahlen blenden.", "tokens": ["Dich", "und", "die", "Welt", ",", "so", "da\u00df", "euch", "nicht", "die", "Strah\u00b7len", "blen\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KON", "ART", "NN", "$,", "ADV", "KOUS", "PPER", "PTKNEG", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}