{"dta.poem.13079": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Achtes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201ewas nuzet jener Ruff: die Nachwelt ehret ihn!", "tokens": ["\u201e", "was", "nu\u00b7zet", "je\u00b7ner", "Ruff", ":", "die", "Nach\u00b7welt", "eh\u00b7ret", "ihn", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VVFIN", "PDAT", "NN", "$.", "ART", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "\u201ewann ich durch meine Kraft nicht dessen B\u00fcrge bin?", "tokens": ["\u201e", "wann", "ich", "durch", "mei\u00b7ne", "Kraft", "nicht", "des\u00b7sen", "B\u00fcr\u00b7ge", "bin", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "PPER", "APPR", "PPOSAT", "NN", "PTKNEG", "PRELAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "95\u201dWahr ists, nur Stein' hab ich; und die seynd meine Zungen:", "tokens": ["\"", "Wahr", "ists", ",", "nur", "Stein'", "hab", "ich", ";", "und", "die", "seynd", "mei\u00b7ne", "Zun\u00b7gen", ":"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "NE", "VAFIN", "$,", "ADV", "NN", "VAFIN", "PPER", "$.", "KON", "PDS", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u201edurch ihre Stimme wird der Thaten Ruhm besungen.", "tokens": ["\u201e", "durch", "ih\u00b7re", "Stim\u00b7me", "wird", "der", "Tha\u00b7ten", "Ruhm", "be\u00b7sun\u00b7gen", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PPOSAT", "NN", "VAFIN", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "\u201eich bin die Welt-Posaun, die macht euch heut noch kund,", "tokens": ["\u201e", "ich", "bin", "die", "Welt\u00b7Po\u00b7saun", ",", "die", "macht", "euch", "heut", "noch", "kund", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "ART", "NN", "$,", "PRELS", "VVFIN", "PPER", "ADV", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "\u201ewas in dem Urbeginn der Vorwelt pr\u00e4chtig stund.", "tokens": ["\u201e", "was", "in", "dem", "Ur\u00b7be\u00b7ginn", "der", "Vor\u00b7welt", "pr\u00e4ch\u00b7tig", "stund", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "APPR", "ART", "NN", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.7": {"text": "\u201eman sehe die Geb\u00e4u, die B\u00f6gen, die Colossen,", "tokens": ["\u201e", "man", "se\u00b7he", "die", "Ge\u00b7b\u00e4u", ",", "die", "B\u00f6\u00b7gen", ",", "die", "Co\u00b7los\u00b7sen", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PIS", "VVFIN", "ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "100\u201dDie S\u00e4ulen, welche fast an das Gestirne stossen.", "tokens": ["\"", "Die", "S\u00e4u\u00b7len", ",", "wel\u00b7che", "fast", "an", "das", "Ge\u00b7stir\u00b7ne", "stos\u00b7sen", "."], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "$,", "PRELS", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "\u201enicht ich, nicht meine Kunst wird durch das Werck ber\u00fchmt;", "tokens": ["\u201e", "nicht", "ich", ",", "nicht", "mei\u00b7ne", "Kunst", "wird", "durch", "das", "Werck", "be\u00b7r\u00fchmt", ";"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKNEG", "PPER", "$,", "PTKNEG", "PPOSAT", "NN", "VAFIN", "APPR", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "\u201enicht jener, der den Bau zu f\u00fchren unternimmt;", "tokens": ["\u201e", "nicht", "je\u00b7ner", ",", "der", "den", "Bau", "zu", "f\u00fch\u00b7ren", "un\u00b7ter\u00b7nimmt", ";"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKNEG", "PDS", "$,", "PRELS", "ART", "NN", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "\u201edie Welt sagt: dieses ist zu dessen Ruhm gebauet,", "tokens": ["\u201e", "die", "Welt", "sagt", ":", "die\u00b7ses", "ist", "zu", "des\u00b7sen", "Ruhm", "ge\u00b7bau\u00b7et", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "$.", "PDS", "VAFIN", "APPR", "PRELAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "\u201eden man im Friese dort mit Gold benennet schauet.", "tokens": ["\u201e", "den", "man", "im", "Frie\u00b7se", "dort", "mit", "Gold", "be\u00b7nen\u00b7net", "schau\u00b7et", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "PIS", "APPRART", "NN", "ADV", "APPR", "NN", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}