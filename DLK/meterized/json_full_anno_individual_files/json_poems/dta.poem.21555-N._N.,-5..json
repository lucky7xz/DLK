{"dta.poem.21555": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "5.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Ihr schwatzt befrackt hoch vom Katheder", "tokens": ["Ihr", "schwatzt", "be\u00b7frackt", "hoch", "vom", "Ka\u00b7the\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "VVFIN", "ADJD", "APPRART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Von alter und von neuer Kunst,", "tokens": ["Von", "al\u00b7ter", "und", "von", "neu\u00b7er", "Kunst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "KON", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von Fleischgenu\u00df und Sinnenbrunst,", "tokens": ["Von", "Fleischge\u00b7nu\u00df", "und", "Sin\u00b7nen\u00b7brunst", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-++-+-+", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Und gerbt nur Leder, altes Leder.", "tokens": ["Und", "gerbt", "nur", "Le\u00b7der", ",", "al\u00b7tes", "Le\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ihr la\u00dft um jede Attit\u00fcde", "tokens": ["Ihr", "la\u00dft", "um", "je\u00b7de", "At\u00b7ti\u00b7t\u00fc\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ein wei\u00dfgewaschnes Hemdchen wehn;", "tokens": ["Ein", "wei\u00df\u00b7ge\u00b7waschnes", "Hemd\u00b7chen", "wehn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Denn um die Sch\u00f6nheit nackt zu sehn,", "tokens": ["Denn", "um", "die", "Sch\u00f6n\u00b7heit", "nackt", "zu", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Sind eure Seelen viel zu pr\u00fcde.", "tokens": ["Sind", "eu\u00b7re", "See\u00b7len", "viel", "zu", "pr\u00fc\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADV", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}