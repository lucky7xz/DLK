{"dta.poem.10430": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "LiII.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Mir tr\u00e4umte wieder der alte Traum:", "tokens": ["Mir", "tr\u00e4um\u00b7te", "wie\u00b7der", "der", "al\u00b7te", "Traum", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Es war eine Nacht im Maie,", "tokens": ["Es", "war", "ei\u00b7ne", "Nacht", "im", "Mai\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wir sa\u00dfen unter dem Lindenbaum,", "tokens": ["Wir", "sa\u00b7\u00dfen", "un\u00b7ter", "dem", "Lin\u00b7den\u00b7baum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und schwuren uns ewige Treue.", "tokens": ["Und", "schwu\u00b7ren", "uns", "e\u00b7wi\u00b7ge", "Treu\u00b7e", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "Das war ein Schw\u00f6ren und Schw\u00f6ren auf's Neu',", "tokens": ["Das", "war", "ein", "Schw\u00f6\u00b7ren", "und", "Schw\u00f6\u00b7ren", "auf's", "Neu'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "KON", "NN", "APPRART", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ein Kichern, ein Kosen, ein K\u00fcssen;", "tokens": ["Ein", "Ki\u00b7chern", ",", "ein", "Ko\u00b7sen", ",", "ein", "K\u00fcs\u00b7sen", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Da\u00df ich gedenk des Schwures sey,", "tokens": ["Da\u00df", "ich", "ge\u00b7denk", "des", "Schwu\u00b7res", "sey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hast du in die Hand mich gebissen.", "tokens": ["Hast", "du", "in", "die", "Hand", "mich", "ge\u00b7bis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ART", "NN", "PPER", "VVPP", "$."], "meter": "----+--+-", "measure": "iambic.di.relaxed"}}, "stanza.3": {"line.1": {"text": "O Liebchen mit den Aeuglein klar!", "tokens": ["O", "Lieb\u00b7chen", "mit", "den", "A\u00b7e\u00b7u\u00b7glein", "klar", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPR", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "O Liebchen sch\u00f6n und bissig!", "tokens": ["O", "Lieb\u00b7chen", "sch\u00f6n", "und", "bis\u00b7sig", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das Schw\u00f6ren in der Ordnung war.", "tokens": ["Das", "Schw\u00f6\u00b7ren", "in", "der", "Ord\u00b7nung", "war", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Bei\u00dfen war \u00fcberfl\u00fcssig.", "tokens": ["Das", "Bei\u00b7\u00dfen", "war", "\u00fc\u00b7berf\u00b7l\u00fcs\u00b7sig", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}}}}