{"textgrid.poem.65803": {"metadata": {"author": {"name": "Shakespeare, William", "birth": "N.A.", "death": "N.A."}, "title": "77", "genre": "verse", "period": "N.A.", "pub_year": 1590, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dein Spiegel zeigt dir deiner Sch\u00f6nheit Flucht;", "tokens": ["Dein", "Spie\u00b7gel", "zeigt", "dir", "dei\u00b7ner", "Sch\u00f6n\u00b7heit", "Flucht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Uhr, wie schnell die edeln Stunden schwinden:", "tokens": ["Die", "Uhr", ",", "wie", "schnell", "die", "e\u00b7deln", "Stun\u00b7den", "schwin\u00b7den", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "ADJD", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das wei\u00dfe Blatt hegt deines Geistes Frucht,", "tokens": ["Das", "wei\u00b7\u00dfe", "Blatt", "hegt", "dei\u00b7nes", "Geis\u00b7tes", "Frucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und diese Wissenschaft kann dir dies Buch verk\u00fcnden.", "tokens": ["Und", "die\u00b7se", "Wis\u00b7sen\u00b7schaft", "kann", "dir", "dies", "Buch", "ver\u00b7k\u00fcn\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "VMFIN", "PPER", "PDS", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die Runzeln, die dein Spiegel wahrhaft zeigt,", "tokens": ["Die", "Run\u00b7zeln", ",", "die", "dein", "Spie\u00b7gel", "wahr\u00b7haft", "zeigt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Sie werden dich an offne Gr\u00e4ber mahnen:", "tokens": ["Sie", "wer\u00b7den", "dich", "an", "off\u00b7ne", "Gr\u00e4\u00b7ber", "mah\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und wie des Weisers Schatten vorw\u00e4rts schleicht,", "tokens": ["Und", "wie", "des", "Wei\u00b7sers", "Schat\u00b7ten", "vor\u00b7w\u00e4rts", "schleicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "ADJA", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "L\u00e4\u00dft er der Zeiten Drang zur Ewigkeit dich ahnen.", "tokens": ["L\u00e4\u00dft", "er", "der", "Zei\u00b7ten", "Drang", "zur", "E\u00b7wig\u00b7keit", "dich", "ah\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "APPRART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Was dein Ged\u00e4chtnis nun nicht bergen kann,", "tokens": ["Was", "dein", "Ge\u00b7d\u00e4cht\u00b7nis", "nun", "nicht", "ber\u00b7gen", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "ADV", "PTKNEG", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Wirf es in diese Tafeln, und du findest", "tokens": ["Wirf", "es", "in", "die\u00b7se", "Ta\u00b7feln"