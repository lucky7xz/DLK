{"textgrid.poem.58130": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "168. Der k\u00f6stliche Zierrat", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein sanft' und stilles Kinderwesen,", "tokens": ["Ein", "sanft'", "und", "stil\u00b7les", "Kin\u00b7der\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein unverstellter Einfaltssinn,", "tokens": ["Ein", "un\u00b7ver\u00b7stell\u00b7ter", "Ein\u00b7falts\u00b7sinn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-++-", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Solch sch\u00f6nen Schmuck hat Gott erlesen,", "tokens": ["Solch", "sch\u00f6\u00b7nen", "Schmuck", "hat", "Gott", "er\u00b7le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VAFIN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der f\u00fchrt zur ersten Unschuld hin.", "tokens": ["Der", "f\u00fchrt", "zur", "ers\u00b7ten", "Un\u00b7schuld", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPRART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wer sich vom Geist dahin l\u00e4\u00dft ziehen,", "tokens": ["Wer", "sich", "vom", "Geist", "da\u00b7hin", "l\u00e4\u00dft", "zie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "APPRART", "NN", "PAV", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wird als ein lieblich Bl\u00fcmelein", "tokens": ["Wird", "als", "ein", "lieb\u00b7lich", "Bl\u00fc\u00b7me\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "KOKOM", "ART", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Schon hier im Paradiese bl\u00fchen", "tokens": ["Schon", "hier", "im", "Pa\u00b7ra\u00b7die\u00b7se", "bl\u00fc\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPRART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und ewig Gottes Lustspiel sein.", "tokens": ["Und", "e\u00b7wig", "Got\u00b7tes", "Lust\u00b7spiel", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "NN", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ein sanft' und stilles Kinderwesen,", "tokens": ["Ein", "sanft'", "und", "stil\u00b7les", "Kin\u00b7der\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein unverstellter Einfaltssinn,", "tokens": ["Ein", "un\u00b7ver\u00b7stell\u00b7ter", "Ein\u00b7falts\u00b7sinn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-++-", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Solch sch\u00f6nen Schmuck hat Gott erlesen,", "tokens": ["Solch", "sch\u00f6\u00b7nen", "Schmuck", "hat", "Gott", "er\u00b7le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VAFIN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der f\u00fchrt zur ersten Unschuld hin.", "tokens": ["Der", "f\u00fchrt", "zur", "ers\u00b7ten", "Un\u00b7schuld", "hin", "."], "token_info": ["word", "word", "wor