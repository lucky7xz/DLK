bic.tri"}}, "stanza.18": {"line.1": {"text": "Hast Du nun dieses Liebes-Band", "tokens": ["Hast", "Du", "nun", "die\u00b7ses", "Lie\u00b7bes\u00b7Band"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit eigner Hand gebunden,", "tokens": ["Mit", "eig\u00b7ner", "Hand", "ge\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Gib da\u00df es einst in Deiner Hand", "tokens": ["Gib", "da\u00df", "es", "einst", "in", "Dei\u00b7ner", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "KOUS", "PPER", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Werd eingezeichnet funden.", "tokens": ["Werd", "ein\u00b7ge\u00b7zeich\u00b7net", "fun\u00b7den", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.19": {"line.1": {"text": "Du bist ein holder Br\u00e4utigam,", "tokens": ["Du", "bist", "ein", "hol\u00b7der", "Br\u00e4u\u00b7ti\u00b7gam", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der zu der Menschen-Seele", "tokens": ["Der", "zu", "der", "Men\u00b7schen\u00b7See\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "In ihre finstre Wohnung kam,", "tokens": ["In", "ih\u00b7re", "finst\u00b7re", "Woh\u00b7nung", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "mea