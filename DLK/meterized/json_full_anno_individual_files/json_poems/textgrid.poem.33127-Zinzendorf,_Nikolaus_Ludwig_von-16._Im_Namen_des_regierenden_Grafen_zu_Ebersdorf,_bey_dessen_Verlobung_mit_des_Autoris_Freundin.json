{"textgrid.poem.33127": {"metadata": {"author": {"name": "Zinzendorf, Nikolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "16. Im Namen des regierenden Grafen zu Ebersdorf, bey dessen Verlobung mit des Autoris Freundin", "genre": "verse", "period": "N.A.", "pub_year": 1730, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie dank ichs meinem Jonathan", "tokens": ["Wie", "dank", "ichs", "mei\u00b7nem", "Jo\u00b7na\u00b7than"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "PIS", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der mich hieher gef\u00fchret,", "tokens": ["Der", "mich", "hie\u00b7her", "ge\u00b7f\u00fch\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "PAV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und der noch kaum begreiffen kan", "tokens": ["Und", "der", "noch", "kaum", "be\u00b7greif\u00b7fen", "kan"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADV", "ADV", "VVINF", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Was er dabey verlieret;", "tokens": ["Was", "er", "da\u00b7bey", "ver\u00b7lie\u00b7ret", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PAV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wie dank ichs aber noch vielmehr", "tokens": ["Wie", "dank", "ichs", "a\u00b7ber", "noch", "viel\u00b7mehr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "PIS", "ADV", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der holden Theodoren,", "tokens": ["Der", "hol\u00b7den", "Theo\u00b7do\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Die mich auf sehnliches Begehr,", "tokens": ["Die", "mich", "auf", "sehn\u00b7li\u00b7ches", "Be\u00b7gehr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Zum Br\u00e4utigam erkoren.", "tokens": ["Zum", "Br\u00e4u\u00b7ti\u00b7gam", "er\u00b7ko\u00b7ren", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Am meisten lob und preise ich", "tokens": ["Am", "meis\u00b7ten", "lob", "und", "prei\u00b7se", "ich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "KON", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Leitstern aller Dinge,", "tokens": ["Den", "Leits\u00b7tern", "al\u00b7ler", "Din\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der nicht von meinen Wegen wich,", "tokens": ["Der", "nicht", "von", "mei\u00b7nen", "We\u00b7gen", "wich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bis ich sie wohl vollbringe.", "tokens": ["Bis", "ich", "sie", "wohl", "voll\u00b7brin\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ich lobe Deinen Wunder-Rath,", "tokens": ["Ich", "lo\u00b7be", "Dei\u00b7nen", "Wun\u00b7der\u00b7Rath", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du Vater aller Gabe!", "tokens": ["Du", "Va\u00b7ter", "al\u00b7ler", "Ga\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der mir so bald gegeben hat", "tokens": ["Der", "mir", "so", "bald", "ge\u00b7ge\u00b7ben", "hat"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADV", "ADV", "VVPP", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Was ich gebeten habe.", "tokens": ["Was", "ich", "ge\u00b7be\u00b7ten", "ha\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Hast Du nun dieses Liebes-Band", "tokens": ["Hast", "Du", "nun", "die\u00b7ses", "Lie\u00b7bes\u00b7Band"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit eigner Hand gebunden,", "tokens": ["Mit", "eig\u00b7ner", "Hand", "ge\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Gib da\u00df es einst in Deiner Hand", "tokens": ["Gib", "da\u00df", "es", "einst", "in", "Dei\u00b7ner", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "KOUS", "PPER", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Werd eingezeichnet funden.", "tokens": ["Werd", "ein\u00b7ge\u00b7zeich\u00b7net", "fun\u00b7den", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Du bist ein holder Br\u00e4utigam,", "tokens": ["Du", "bist", "ein", "hol\u00b7der", "Br\u00e4u\u00b7ti\u00b7gam", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der zu der Menschen-Seele", "tokens": ["Der", "zu", "der", "Men\u00b7schen\u00b7See\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "In ihre finstre Wohnung kam,", "tokens": ["In", "ih\u00b7re", "finst\u00b7re", "Woh\u00b7nung", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df Er sich ihr verm\u00e4hle.", "tokens": ["Da\u00df", "Er", "sich", "ihr", "ver\u00b7m\u00e4h\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Und also freuest Du dich noch,", "tokens": ["Und", "al\u00b7so", "freu\u00b7est", "Du", "dich", "noch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PRF", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn Du von Treu getrieben,", "tokens": ["Wenn", "Du", "von", "Treu", "ge\u00b7trie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Zusammen spannst ins Liebes-Joch", "tokens": ["Zu\u00b7sam\u00b7men", "spannst", "ins", "Lie\u00b7bes\u00b7Joch"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zwey Herzen, die Dich lieben.", "tokens": ["Zwey", "Her\u00b7zen", ",", "die", "Dich", "lie\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "$,", "PRELS", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Du hast mir alles leicht gemacht,", "tokens": ["Du", "hast", "mir", "al\u00b7les", "leicht", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PIS", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In meinen Lebens-Tagen,", "tokens": ["In", "mei\u00b7nen", "Le\u00b7bens\u00b7Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Eh ich der Last recht nachgedacht,", "tokens": ["Eh", "ich", "der", "Last", "recht", "nach\u00b7ge\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hast Du sie abgetragen.", "tokens": ["Hast", "Du", "sie", "ab\u00b7ge\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Ich will von dieser G\u00fctigkeit", "tokens": ["Ich", "will", "von", "die\u00b7ser", "G\u00fc\u00b7tig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vor keinem Menschen schweigen,", "tokens": ["Vor", "kei\u00b7nem", "Men\u00b7schen", "schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und durch die ganze Lebens-Zeit", "tokens": ["Und", "durch", "die", "gan\u00b7ze", "Le\u00b7bens\u00b7Zeit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mein Herze zu Dir neigen.", "tokens": ["Mein", "Her\u00b7ze", "zu", "Dir", "nei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVFIN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Ach neige auch Dein Herz zu mir", "tokens": ["Ach", "nei\u00b7ge", "auch", "Dein", "Herz", "zu", "mir"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "VVFIN", "ADV", "PPOSAT", "NN", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und zieh' mich in die Liebe,", "tokens": ["Und", "zieh'", "mich", "in", "die", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Dich, mit brennender Begier,", "tokens": ["Die", "Dich", ",", "mit", "bren\u00b7nen\u00b7der", "Be\u00b7gier", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu meiner Wohlfahrt triebe.", "tokens": ["Zu", "mei\u00b7ner", "Wohl\u00b7fahrt", "trie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Gib da\u00df ich Dir mein kleines Land", "tokens": ["Gib", "da\u00df", "ich", "Dir", "mein", "klei\u00b7nes", "Land"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "KOUS", "PPER", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zum Opfer \u00fcbergebe,", "tokens": ["Zum", "Op\u00b7fer", "\u00fc\u00b7ber\u00b7ge\u00b7be", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und meinen angebornen Stand", "tokens": ["Und", "mei\u00b7nen", "an\u00b7ge\u00b7bor\u00b7nen", "Stand"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auf Deinem Altar webe.", "tokens": ["Auf", "Dei\u00b7nem", "Al\u00b7tar", "we\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Die theure Gr\u00e4fin, die Du mir", "tokens": ["Die", "theu\u00b7re", "Gr\u00e4\u00b7fin", ",", "die", "Du", "mir"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPER", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So v\u00e4terlich geschenket,", "tokens": ["So", "v\u00e4\u00b7ter\u00b7lich", "ge\u00b7schen\u00b7ket", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die sey o Seelen-Hirte! Dir", "tokens": ["Die", "sey", "o", "See\u00b7len\u00b7Hir\u00b7te", "!", "Dir"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ART", "FM", "FM", "NN", "$.", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In Deinen Schoo\u00df versenket.", "tokens": ["In", "Dei\u00b7nen", "Schoo\u00df", "ver\u00b7sen\u00b7ket", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Ist dis Geschenke Dir geweiht,", "tokens": ["Ist", "dis", "Ge\u00b7schen\u00b7ke", "Dir", "ge\u00b7weiht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das mich so sehr vergn\u00fcget;", "tokens": ["Das", "mich", "so", "sehr", "ver\u00b7gn\u00fc\u00b7get", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So werd es auf die Ewigkeit", "tokens": ["So", "werd", "es", "auf", "die", "E\u00b7wig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In meine Hand gef\u00fcget.", "tokens": ["In", "mei\u00b7ne", "Hand", "ge\u00b7f\u00fc\u00b7get", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Wie dank ichs meinem Jonathan", "tokens": ["Wie", "dank", "ichs", "mei\u00b7nem", "Jo\u00b7na\u00b7than"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "PIS", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der mich hieher gef\u00fchret,", "tokens": ["Der", "mich", "hie\u00b7her", "ge\u00b7f\u00fch\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "PAV", "VVPP", "$,"], "meter": "-+-+-+-", "me