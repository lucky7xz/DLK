": ["PPER", "VMFIN", "ADV", "APPR", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Doch besser schweigend knie ich hier;", "tokens": ["Doch", "bes\u00b7ser", "schwei\u00b7gend", "knie", "ich", "hier", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJD", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er, der f\u00fcr mich am Kreuz gelitten,", "tokens": ["Er", ",", "der", "f\u00fcr", "mich", "am", "Kreuz", "ge\u00b7lit\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "APPR", "PPER", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mein milder Anwalt, steht bei mir.", "tokens": ["Mein", "mil\u00b7der", "An\u00b7walt", ",", "steht", "bei", "mir", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich wandle stets in Finsternissen,", "tokens": ["Ich", "wand\u00b7le", "stets", "in", "Fins\u00b7ter\u00b7nis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Er war es stets der Strahlen warf.", "tokens": ["Er", "war", "es", "stets", "der", "Strah\u00b7len", "warf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der alles wei\u00df, sollt' er nicht wissen", "tokens": ["Der", "al\u00b7les", "wei\u00df", ",", "sollt'", "er", "nicht", "wis\u00b7sen"], "token_info": ["word", "word", "word