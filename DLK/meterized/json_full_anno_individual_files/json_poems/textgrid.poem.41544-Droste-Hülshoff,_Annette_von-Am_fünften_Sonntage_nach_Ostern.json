": "F\u00fcr alle, alle ist ja deine", "tokens": ["F\u00fcr", "al\u00b7le", ",", "al\u00b7le", "ist", "ja", "dei\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PIS", "$,", "PIS", "VAFIN", "ADV", "PPOSAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "So \u00fcberreiche Hand gef\u00fcllt.", "tokens": ["So", "\u00fc\u00b7berr\u00b7ei\u00b7che", "Hand", "ge\u00b7f\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Vertrauend will ich zu dir nahen,", "tokens": ["Ver\u00b7trau\u00b7end", "will", "ich", "zu", "dir", "na\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VMFIN", "PPER", "APPR", "PPER", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und spr\u00e4ch' auch T\u00f6richtes mein Mund,", "tokens": ["Und", "spr\u00e4ch'", "auch", "T\u00f6\u00b7rich\u00b7tes", "mein", "Mund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-++--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Nur Gn\u00e4diges werd' ich empfahen,", "tokens": ["Nur", "Gn\u00e4\u00b7di\u00b7ges", "werd'", "ich", "em\u00b7pfa\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "VAFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Du wirst mir geben was gesund.", "tokens": ["Du", "wirst", "mir", "ge\u00b7ben", "was", "ge\u00b7sund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVINF", "PWS", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ob schwach und irrend die Gedanken,", "tokens": ["Ob", "schwach", "und", "ir\u00b7rend", "die", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "KON", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Vertrauend bring' ich sie dir dar;", "tokens": ["Ver\u00b7trau\u00b7end", "bring'", "ich", "sie", "dir", "dar", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "PPER", "PPER", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und ziehen wirst du selbst die Schranken,", "tokens": ["Und", "zie\u00b7hen", "wirst", "du", "selbst", "die", "Schran\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VAFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und treu mein Bestes nehmen wahr.", "tokens": ["Und", "treu", "mein", "Bes\u00b7tes", "neh\u00b7men", "wahr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PPOSAT", "NN", "VVINF", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Ich bitte nicht um Gl\u00fcck der Erden,", "tokens": ["Ich", "bit\u00b7te", "nicht", "um", "Gl\u00fcck", "der", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PTKNEG", "APPR", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nur um ein Leuchten nun und dann,", "tokens": ["Nur", "um", "ein", "Leuch\u00b7ten", "nun", "und", "dann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ADV", "KON", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df sicht