{"textgrid.poem.41544": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Am f\u00fcnften Sonntage nach Ostern", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In seinem Namen darf ich beten,", "tokens": ["In", "sei\u00b7nem", "Na\u00b7men", "darf", "ich", "be\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Er hat es selber mir gesagt,", "tokens": ["Er", "hat", "es", "sel\u00b7ber", "mir", "ge\u00b7sagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit seinem Gnadenstempel treten", "tokens": ["Mit", "sei\u00b7nem", "Gna\u00b7dens\u00b7tem\u00b7pel", "tre\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Vor ihren Sch\u00f6pfer darf die Magd.", "tokens": ["Vor", "ih\u00b7ren", "Sch\u00f6p\u00b7fer", "darf", "die", "Magd", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VMFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "O s\u00fc\u00dfes Anrecht mir gegeben!", "tokens": ["O", "s\u00fc\u00b7\u00dfes", "An\u00b7recht", "mir", "ge\u00b7ge\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "O Zuversicht, die ihm entsprie\u00dft!", "tokens": ["O", "Zu\u00b7ver\u00b7sicht", ",", "die", "ihm", "ent\u00b7sprie\u00dft", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wie wei\u00df ich heut von keinem Beben,", "tokens": ["Wie", "wei\u00df", "ich", "heut", "von", "kei\u00b7nem", "Be\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Wo mich sein Sonnenschein umflie\u00dft!", "tokens": ["Wo", "mich", "sein", "Son\u00b7nen\u00b7schein", "um\u00b7flie\u00dft", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "So tret' ich denn in Jesu Namen,", "tokens": ["So", "tret'", "ich", "denn", "in", "Je\u00b7su", "Na\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "NE", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Sch\u00f6pfer, vor dein Angesicht;", "tokens": ["Mein", "Sch\u00f6p\u00b7fer", ",", "vor", "dein", "An\u00b7ge\u00b7sicht", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wo stehn die Blinden und die Lahmen,", "tokens": ["Wo", "stehn", "die", "Blin\u00b7den", "und", "die", "Lah\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dort ist mein Platz und mein Gericht.", "tokens": ["Dort", "ist", "mein", "Platz", "und", "mein", "Ge\u00b7richt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und bin ich der Geringsten eine,", "tokens": ["Und", "bin", "ich", "der", "Ge\u00b7rings\u00b7ten", "ei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ART", "NN", "ART", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die knieen unter seinem Schild:", "tokens": ["Die", "kni\u00b7e\u00b7en", "un\u00b7ter", "sei\u00b7nem", "Schild", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "F\u00fcr alle, alle ist ja deine", "tokens": ["F\u00fcr", "al\u00b7le", ",", "al\u00b7le", "ist", "ja", "dei\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PIS", "$,", "PIS", "VAFIN", "ADV", "PPOSAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "So \u00fcberreiche Hand gef\u00fcllt.", "tokens": ["So", "\u00fc\u00b7berr\u00b7ei\u00b7che", "Hand", "ge\u00b7f\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Vertrauend will ich zu dir nahen,", "tokens": ["Ver\u00b7trau\u00b7end", "will", "ich", "zu", "dir", "na\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VMFIN", "PPER", "APPR", "PPER", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und spr\u00e4ch' auch T\u00f6richtes mein Mund,", "tokens": ["Und", "spr\u00e4ch'", "auch", "T\u00f6\u00b7rich\u00b7tes", "mein", "Mund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-++--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Nur Gn\u00e4diges werd' ich empfahen,", "tokens": ["Nur", "Gn\u00e4\u00b7di\u00b7ges", "werd'", "ich", "em\u00b7pfa\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "VAFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Du wirst mir geben was gesund.", "tokens": ["Du", "wirst", "mir", "ge\u00b7ben", "was", "ge\u00b7sund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVINF", "PWS", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ob schwach und irrend die Gedanken,", "tokens": ["Ob", "schwach", "und", "ir\u00b7rend", "die", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "KON", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Vertrauend bring' ich sie dir dar;", "tokens": ["Ver\u00b7trau\u00b7end", "bring'", "ich", "sie", "dir", "dar", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "PPER", "PPER", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und ziehen wirst du selbst die Schranken,", "tokens": ["Und", "zie\u00b7hen", "wirst", "du", "selbst", "die", "Schran\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VAFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und treu mein Bestes nehmen wahr.", "tokens": ["Und", "treu", "mein", "Bes\u00b7tes", "neh\u00b7men", "wahr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PPOSAT", "NN", "VVINF", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ich bitte nicht um Gl\u00fcck der Erden,", "tokens": ["Ich", "bit\u00b7te", "nicht", "um", "Gl\u00fcck", "der", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PTKNEG", "APPR", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nur um ein Leuchten nun und dann,", "tokens": ["Nur", "um", "ein", "Leuch\u00b7ten", "nun", "und", "dann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ADV", "KON", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df sichtbar deine H\u00e4nde werden,", "tokens": ["Da\u00df", "sicht\u00b7bar", "dei\u00b7ne", "H\u00e4n\u00b7de", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "PPOSAT", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ich deine Liebe ahnden kann;", "tokens": ["Ich", "dei\u00b7ne", "Lie\u00b7be", "ahn\u00b7den", "kann", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nur in des Lebens K\u00fcmmernissen", "tokens": ["Nur", "in", "des", "Le\u00b7bens", "K\u00fcm\u00b7mer\u00b7nis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Um der Ergebung Gnadengru\u00df:", "tokens": ["Um", "der", "Er\u00b7ge\u00b7bung", "Gna\u00b7den\u00b7gru\u00df", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.7": {"text": "Dann wirst du schon am besten wissen,", "tokens": ["Dann", "wirst", "du", "schon", "am", "bes\u00b7ten", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PTKA", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Wie viel ich tragen kann und mu\u00df.", "tokens": ["Wie", "viel", "ich", "tra\u00b7gen", "kann", "und", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PPER", "VVINF", "VMFIN", "KON", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Auch nicht um Ruhm will ich dich bitten,", "tokens": ["Auch", "nicht", "um", "Ruhm", "will", "ich", "dich", "bit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "APPR", "NN", "VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dem meine Schultern viel zu schwach;", "tokens": ["Dem", "mei\u00b7ne", "Schul\u00b7tern", "viel", "zu", "schwach", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "ADV", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nur in der Menschenstimmen Mitten", "tokens": ["Nur", "in", "der", "Men\u00b7schen\u00b7stim\u00b7men", "Mit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mir bleibe das Bewu\u00dftsein wach,", "tokens": ["Mir", "blei\u00b7be", "das", "Be\u00b7wu\u00df\u00b7tsein", "wach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da\u00df, wie die Meinung kreist und rennet,", "tokens": ["Da\u00df", ",", "wie", "die", "Mei\u00b7nung", "kreist", "und", "ren\u00b7net", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "PWAV", "ART", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Doch einer ist, der nimmer irrt,", "tokens": ["Doch", "ei\u00b7ner", "ist", ",", "der", "nim\u00b7mer", "irrt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "$,", "PRELS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und jedes Wort, das ihn nicht kennet,", "tokens": ["Und", "je\u00b7des", "Wort", ",", "das", "ihn", "nicht", "ken\u00b7net", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$,", "PRELS", "PPER", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Mich tausendfach gereuen, wird.", "tokens": ["Mich", "tau\u00b7send\u00b7fach", "ge\u00b7reu\u00b7en", ",", "wird", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "VVINF", "$,", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Gesundheit! teures Erdenlehen,", "tokens": ["Ge\u00b7sund\u00b7heit", "!", "teu\u00b7res", "Er\u00b7den\u00b7le\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$.", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ach! schmerzlich hab' ich dich entbehrt!", "tokens": ["Ach", "!", "schmerz\u00b7lich", "hab'", "ich", "dich", "ent\u00b7behrt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ADJD", "VAFIN", "PPER", "PRF", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch nur um dieses mag ich flehen:", "tokens": ["Doch", "nur", "um", "die\u00b7ses", "mag", "ich", "fle\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PDS", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Seele bleibe ungest\u00f6rt;", "tokens": ["Die", "See\u00b7le", "blei\u00b7be", "un\u00b7ge\u00b7st\u00f6rt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da\u00df nicht die wirbelnden Gedanken", "tokens": ["Da\u00df", "nicht", "die", "wir\u00b7beln\u00b7den", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PTKNEG", "ART", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Der kranke Dunst bezwingen mag,", "tokens": ["Der", "kran\u00b7ke", "Dunst", "be\u00b7zwin\u00b7gen", "mag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da\u00df durch der b\u00e4ngsten Nebel Schranken", "tokens": ["Da\u00df", "durch", "der", "b\u00e4ngs\u00b7ten", "Ne\u00b7bel", "Schran\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ich immer ahnde deinen Tag.", "tokens": ["Ich", "im\u00b7mer", "ahn\u00b7de", "dei\u00b7nen", "Tag", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Viel warme Liebe h\u00e4lt umfangen", "tokens": ["Viel", "war\u00b7me", "Lie\u00b7be", "h\u00e4lt", "um\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dies \u00f6de Ich zu s\u00fc\u00dfem Schmerz", "tokens": ["Dies", "\u00f6\u00b7de", "Ich", "zu", "s\u00fc\u00b7\u00dfem", "Schmerz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und l\u00e4\u00dft die S\u00fchne nicht gelangen", "tokens": ["Und", "l\u00e4\u00dft", "die", "S\u00fch\u00b7ne", "nicht", "ge\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKNEG", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "An mein nach Strenge d\u00fcrstend Herz.", "tokens": ["An", "mein", "nach", "Stren\u00b7ge", "d\u00fcrs\u00b7tend", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "APPR", "NN", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "O sch\u00fctze mich vor jener Milde,", "tokens": ["O", "sch\u00fct\u00b7ze", "mich", "vor", "je\u00b7ner", "Mil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PRF", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die meinen M\u00e4ngeln viel zu still;", "tokens": ["Die", "mei\u00b7nen", "M\u00e4n\u00b7geln", "viel", "zu", "still", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADV", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Halt du den Spiegel mir zum Bilde,", "tokens": ["Halt", "du", "den", "Spie\u00b7gel", "mir", "zum", "Bil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Wenn Freundes Rechte z\u00f6gern will.", "tokens": ["Wenn", "Freun\u00b7des", "Rech\u00b7te", "z\u00f6\u00b7gern", "will", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Ich m\u00f6chte noch um vieles bitten,", "tokens": ["Ich", "m\u00f6ch\u00b7te", "noch", "um", "vie\u00b7les", "bit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "APPR", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Doch besser schweigend knie ich hier;", "tokens": ["Doch", "bes\u00b7ser", "schwei\u00b7gend", "knie", "ich", "hier", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJD", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er, der f\u00fcr mich am Kreuz gelitten,", "tokens": ["Er", ",", "der", "f\u00fcr", "mich", "am", "Kreuz", "ge\u00b7lit\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "APPR", "PPER", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mein milder Anwalt, steht bei mir.", "tokens": ["Mein", "mil\u00b7der", "An\u00b7walt", ",", "steht", "bei", "mir", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich wandle stets in Finsternissen,", "tokens": ["Ich", "wand\u00b7le", "stets", "in", "Fins\u00b7ter\u00b7nis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Er war es stets der Strahlen warf.", "tokens": ["Er", "war", "es", "stets", "der", "Strah\u00b7len", "warf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der alles wei\u00df, sollt' er nicht wissen", "tokens": ["Der", "al\u00b7les", "wei\u00df", ",", "sollt'", "er", "nicht", "wis\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "PIS", "VVFIN", "$,", "VMFIN", "PPER", "PTKNEG", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Was seine arme Magd bedarf?", "tokens": ["Was", "sei\u00b7ne", "ar\u00b7me", "Magd", "be\u00b7darf", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "In seinem Namen darf ich beten,", "tokens": ["In", "sei\u00b7nem", "Na\u00b7men", "darf", "ich", "be\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Er hat es selber mir gesagt,", "tokens": ["Er", "hat", "es", "sel\u00b7ber", "mir", "ge\u00b7sagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit seinem Gnadenstempel treten", "tokens": ["Mit", "sei\u00b7nem", "Gna\u00b7dens\u00b7tem\u00b7pel", "tre\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Vor ihren Sch\u00f6pfer darf die Magd.", "tokens": ["Vor", "ih\u00b7ren", "Sch\u00f6p\u00b7fer", "darf", "die", "Magd", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VMFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "O s\u00fc\u00dfes Anrecht mir gegeben!", "tokens": ["O", "s\u00fc\u00b7\u00dfes", "An\u00b7recht", "mir", "ge\u00b7ge\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "O Zuversicht, die ihm entsprie\u00dft!", "tokens": ["O", "Zu\u00b7ver\u00b7sicht", ",", "die", "ihm", "ent\u00b7sprie\u00dft", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wie wei\u00df ich heut von keinem Beben,", "tokens": ["Wie", "wei\u00df", "ich", "heut", "von", "kei\u00b7nem", "Be\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Wo mich sein Sonnenschein umflie\u00dft!", "tokens": ["Wo", "mich", "sein", "Son\u00b7nen\u00b7schein", "um\u00b7flie\u00dft", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.