{"dta.poem.6753": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Gesicht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Hiedurch nun k\u00f6nnen wir vom Aug' ein Urtheil f\u00e4llen,", "tokens": ["Hie\u00b7durch", "nun", "k\u00f6n\u00b7nen", "wir", "vom", "Aug'", "ein", "Ur\u00b7theil", "f\u00e4l\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "VMFIN", "PPER", "APPRART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es dienet, uns allein die Bilder vorzustellen:", "tokens": ["Es", "die\u00b7net", ",", "uns", "al\u00b7lein", "die", "Bil\u00b7der", "vor\u00b7zu\u00b7stel\u00b7len", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Indem es k\u00fcnstlicher geschnitten und formirt,", "tokens": ["In\u00b7dem", "es", "k\u00fcnst\u00b7li\u00b7cher", "ge\u00b7schnit\u00b7ten", "und", "for\u00b7mirt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VVPP", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und die Natur ihr Glas noch sauberer polirt.", "tokens": ["Und", "die", "Na\u00b7tur", "ihr", "Glas", "noch", "sau\u00b7be\u00b7rer", "po\u00b7lirt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PPOSAT", "NN", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}