{"textgrid.poem.67747": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Erhebung und Verlangen", "genre": "verse", "period": "N.A.", "pub_year": 1764, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbsieh auf!\u00ab so sprach's, als ich schon halb bes\u00e4t", "tokens": ["\u00bb", "sieh", "auf", "!", "\u00ab", "so", "sprach's", ",", "als", "ich", "schon", "halb", "be\u00b7s\u00e4t"], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "VVIMP", "PTKVZ", "$.", "$(", "ADV", "VVFIN", "$,", "KOUS", "PPER", "ADV", "ADJD", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mit Todtenstaub hinlag, vorm fernen Donner", "tokens": ["Mit", "Tod\u00b7ten\u00b7staub", "hin\u00b7lag", ",", "vorm", "fer\u00b7nen", "Don\u00b7ner"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "$,", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mich tiefer grub und zitternd Ketten k\u00fc\u00dfte,", "tokens": ["Mich", "tie\u00b7fer", "grub", "und", "zit\u00b7ternd", "Ket\u00b7ten", "k\u00fc\u00df\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVFIN", "KON", "ADJD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Von Thr\u00e4nen, Blut und Schwei\u00df durchnagt.", "tokens": ["Von", "Thr\u00e4\u00b7nen", ",", "Blut", "und", "Schwei\u00df", "durch\u00b7nagt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Kaum als mich der n\u00e4chste Donner rief,", "tokens": ["Kaum", "als", "mich", "der", "n\u00e4chs\u00b7te", "Don\u00b7ner", "rief", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Hob sich mein halbes Aug', doch schnell", "tokens": ["Hob", "sich", "mein", "hal\u00b7bes", "Aug'", ",", "doch", "schnell"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PRF", "PPOSAT", "ADJA", "NN", "$,", "ADV", "ADJD"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Sah's Gott. \u00bbDie Fessel weg! auf! komm", "tokens": ["Sah's", "Gott", ".", "\u00bb", "Die", "Fes\u00b7sel", "weg", "!", "auf", "!", "komm"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["NE", "NN", "$.", "$(", "ART", "NN", "PTKVZ", "$.", "PTKVZ", "$.", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Von hier, wo man nichts denkt, nichts f\u00fchlt!\u00ab", "tokens": ["Von", "hier", ",", "wo", "man", "nichts", "denkt", ",", "nichts", "f\u00fchlt", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["APPR", "ADV", "$,", "PWAV", "PIS", "PIS", "VVFIN", "$,", "PIS", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "So sprach Apoll, und wei\u00df begl\u00e4nzet sah", "tokens": ["So", "sprach", "A\u00b7poll", ",", "und", "wei\u00df", "be\u00b7gl\u00e4n\u00b7zet", "sah"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NE", "$,", "KON", "VVFIN", "VVPP", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Ich Tempe's Musent\u00e4nze, schwang den neuen,", "tokens": ["Ich", "Tem\u00b7pe's", "Mu\u00b7sen\u00b7t\u00e4n\u