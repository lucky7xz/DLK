{"dta.poem.20698": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "An  \n  Lehnchen R**  \n \u00fcber einen Zuckermann.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Lehnchen, dieser Zuckermann               ", "tokens": ["Lehn\u00b7chen", ",", "die\u00b7ser", "Zu\u00b7cker\u00b7mann"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "PDAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "War ein Mensch, wie andre Leute,", "tokens": ["War", "ein", "Mensch", ",", "wie", "and\u00b7re", "Leu\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$,", "PWAV", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Der auf Liebsgeschichten sann", "tokens": ["Der", "auf", "Liebs\u00b7ge\u00b7schich\u00b7ten", "sann"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und sich auf ein M\u00e4dchen freute,", "tokens": ["Und", "sich", "auf", "ein", "M\u00e4d\u00b7chen", "freu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "H\u00fcbsch und niedlich von Gestalt;", "tokens": ["H\u00fcbsch", "und", "nied\u00b7lich", "von", "Ge\u00b7stalt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Aber ach! Er war schon alt,", "tokens": ["A\u00b7ber", "ach", "!", "Er", "war", "schon", "alt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$.", "PPER", "VAFIN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "War schon ziemlich steif an F\u00fc\u00dfen,", "tokens": ["War", "schon", "ziem\u00b7lich", "steif", "an", "F\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ADJD", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und es war zur Winterszeit,", "tokens": ["Und", "es", "war", "zur", "Win\u00b7ter\u00b7szeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Wo das Eis von Wasserg\u00fcssen,", "tokens": ["Wo", "das", "Eis", "von", "Was\u00b7ser\u00b7g\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Ueber Nacht sehr oft verschneyt,", "tokens": ["Ue\u00b7ber", "Nacht", "sehr", "oft", "ver\u00b7schneyt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Tr\u00fcglich ist, und Ursach giebet,", "tokens": ["Tr\u00fcg\u00b7lich", "ist", ",", "und", "Ur\u00b7sach", "gie\u00b7bet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "$,", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Da\u00df der Kl\u00fcgste wankt und f\u00e4llt:", "tokens": ["Da\u00df", "der", "Kl\u00fcgs\u00b7te", "wankt", "und", "f\u00e4llt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Also ging es unsrem Held,", "tokens": ["Al\u00b7so", "ging", "es", "uns\u00b7rem", "Held", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "Der zum Sterben war verliebet.", "tokens": ["Der", "zum", "Ster\u00b7ben", "war", "ver\u00b7lie\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Voll Gedanken und voll Trieb,", "tokens": ["Voll", "Ge\u00b7dan\u00b7ken", "und", "voll", "Trieb", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "KON", "ADJD", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.16": {"text": "Seine Sch\u00f6ne bald zu gr\u00fc\u00dfen,", "tokens": ["Sei\u00b7ne", "Sch\u00f6\u00b7ne", "bald", "zu", "gr\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Und das Herzchen aufzuschlie\u00dfen,", "tokens": ["Und", "das", "Herz\u00b7chen", "auf\u00b7zu\u00b7schlie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVIZU", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Das ihm noch verschlossen blieb;", "tokens": ["Das", "ihm", "noch", "ver\u00b7schlos\u00b7sen", "blieb", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.19": {"text": "Voller Hoffnung trippelt er", "tokens": ["Vol\u00b7ler", "Hoff\u00b7nung", "trip\u00b7pelt", "er"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.20": {"text": "Auf der Stra\u00dfe, glatt wie Spiegel,", "tokens": ["Auf", "der", "Stra\u00b7\u00dfe", ",", "glatt", "wie", "Spie\u00b7gel", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ADJD", "KOKOM", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Glitschte schnell, und st\u00fcrzte schwer,", "tokens": ["Glitschte", "schnell", ",", "und", "st\u00fcrz\u00b7te", "schwer", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "$,", "KON", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.22": {"text": "Wie ein Baum ins Thal vom H\u00fcgel.", "tokens": ["Wie", "ein", "Baum", "ins", "Thal", "vom", "H\u00fc\u00b7gel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPRART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "Amor schlug mit seinem Fl\u00fcgel", "tokens": ["A\u00b7mor", "schlug", "mit", "sei\u00b7nem", "Fl\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Dreymal \u00fcber ihn zum Spott,", "tokens": ["Drey\u00b7mal", "\u00fc\u00b7ber", "ihn", "zum", "Spott", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.25": {"text": "Weil er einen Arm zerknickte;", "tokens": ["Weil", "er", "ei\u00b7nen", "Arm", "zer\u00b7knick\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "Dreimal sprach der kleine Gott,", "tokens": ["Drei\u00b7mal", "sprach", "der", "klei\u00b7ne", "Gott", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.27": {"text": "Da\u00df der alte Buntgeschm\u00fcckte", "tokens": ["Da\u00df", "der", "al\u00b7te", "Bunt\u00b7ge\u00b7schm\u00fcck\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Sich zu keinem M\u00e4dchen schickte,", "tokens": ["Sich", "zu", "kei\u00b7nem", "M\u00e4d\u00b7chen", "schick\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.29": {"text": "Und da\u00df er zu seiner Schmach", "tokens": ["Und", "da\u00df", "er", "zu", "sei\u00b7ner", "Schmach"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "APPR", "PPOSAT", "NN"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.30": {"text": "Auf dem Eise muste wallen,", "tokens": ["Auf", "dem", "Ei\u00b7se", "mus\u00b7te", "wal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.31": {"text": "Niederfiel, und sich im Fallen", "tokens": ["Nie\u00b7der\u00b7fiel", ",", "und", "sich", "im", "Fal\u00b7len"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PIS", "$,", "KON", "PRF", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.32": {"text": "Einen Arm zerbrach.", "tokens": ["Ei\u00b7nen", "Arm", "zer\u00b7brach", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}