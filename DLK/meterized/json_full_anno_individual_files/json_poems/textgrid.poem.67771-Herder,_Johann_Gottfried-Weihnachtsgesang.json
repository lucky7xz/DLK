{"textgrid.poem.67771": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Weihnachtsgesang", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die ganze Menschheit freue sich!", "tokens": ["Die", "gan\u00b7ze", "Menschheit", "freu\u00b7e", "sich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PRF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du, der Mensch bist, freue Dich!", "tokens": ["Du", ",", "der", "Mensch", "bist", ",", "freu\u00b7e", "Dich", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "ART", "NN", "VAFIN", "$,", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Geboren ist der gute Hirt,", "tokens": ["Ge\u00b7bo\u00b7ren", "ist", "der", "gu\u00b7te", "Hirt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der alle V\u00f6lker weiden wird", "tokens": ["Der", "al\u00b7le", "V\u00f6l\u00b7ker", "wei\u00b7den", "wird"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "VVINF", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "In Treu und Wahrheit.", "tokens": ["In", "Treu", "und", "Wahr\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Mit g\u00f6ttlich gro\u00dfem K\u00f6nigssinn", "tokens": ["Mit", "g\u00f6tt\u00b7lich", "gro\u00b7\u00dfem", "K\u00f6\u00b7nigs\u00b7sinn"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Giebt er sich zum Opfer hin;", "tokens": ["Giebt", "er", "sich", "zum", "Op\u00b7fer", "hin", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPRART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Er nimmt auf sich die Last der Zeit;", "tokens": ["Er", "nimmt", "auf", "sich", "die", "Last", "der", "Zeit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PRF", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Verachtung, Schmach, Undankbarkeit", "tokens": ["Ver\u00b7ach\u00b7tung", ",", "Schmach", ",", "Un\u00b7dank\u00b7bar\u00b7keit"], "token_info": ["word", "punct", "word", "punct", "word"], "pos": ["NN", "$,", "NN", "$,", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Erwarten seiner.", "tokens": ["Er\u00b7war\u00b7ten", "sei\u00b7ner", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "PPOSAT", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Doch Gottesgeist belebet ihn!", "tokens": ["Doch", "Got\u00b7tes\u00b7geist", "be\u00b7le\u00b7bet", "ihn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Jedem Frevler wird er k\u00fchn", "tokens": ["Je\u00b7dem", "Frev\u00b7ler", "wird", "er", "k\u00fchn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VAFIN", "PPER", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die Larv' entrei\u00dfen; suchen wird", "tokens": ["Die", "La\u00b7rv'", "ent\u00b7rei\u00b7\u00dfen", ";", "su\u00b7chen", "wird"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVINF", "$.", "VVINF", "VAFIN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Er das Verlorne, was verirrt", "tokens": ["Er", "das", "Ver\u00b7lor\u00b7ne", ",", "was", "ver\u00b7irrt"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "ART", "NN", "$,", "PRELS", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ist, wiederbringen.", "tokens": ["Ist", ",", "wie\u00b7der\u00b7brin\u00b7gen", "."], "token_info": ["word", "punct", "word", "punct"], "pos": ["VAFIN", "$,", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Sein Zeichen ist die D\u00fcrftigkeit,", "tokens": ["Sein", "Zei\u00b7chen", "ist", "die", "D\u00fcrf\u00b7tig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Menschenhuld sein Ehrenkleid,", "tokens": ["Men\u00b7schen\u00b7huld", "sein", "Eh\u00b7ren\u00b7kleid", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Erbarmen ziehet ihn heran;", "tokens": ["Er\u00b7bar\u00b7men", "zie\u00b7het", "ihn", "he\u00b7ran", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der V\u00f6lker Heil ist seine Bahn", "tokens": ["Der", "V\u00f6l\u00b7ker", "Heil", "ist", "sei\u00b7ne", "Bahn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Zum Himmelsfrieden.", "tokens": ["Zum", "Him\u00b7mels\u00b7frie\u00b7den", "."], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Drum singen froh willkommend ihm", "tokens": ["Drum", "sin\u00b7gen", "froh", "will\u00b7kom\u00b7mend", "ihm"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "ADJD", "VVPP", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Cherubim und Seraphim", "tokens": ["Che\u00b7ru\u00b7bim", "und", "Se\u00b7ra\u00b7phim"], "token_info": ["word", "word", "word"], "pos": ["NE", "KON", "NN"], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.3": {"text": "Ihr \u00bbEhre sei Gott in der H\u00f6h", "tokens": ["Ihr", "\u00bb", "Eh\u00b7re", "sei", "Gott", "in", "der", "H\u00f6h"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "$(", "NN", "VAFIN", "NN", "APPR", "ART", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Und Fried' auf Erden! Leid und Weh", "tokens": ["Und", "Fried'", "auf", "Er\u00b7den", "!", "Leid", "und", "Weh"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "NN", "APPR", "NN", "$.", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wird Wohlgefallen!\u00ab", "tokens": ["Wird", "Wohl\u00b7ge\u00b7fal\u00b7len", "!", "\u00ab"], "token_info": ["word", "word", "punct", "punct"], "pos": ["VAFIN", "NN", "$.", "$("], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Wir stimmen der Willkommung ein:", "tokens": ["Wir", "stim\u00b7men", "der", "Will\u00b7kom\u00b7mung", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Unser Hirte soll er sein", "tokens": ["Un\u00b7ser", "Hir\u00b7te", "soll", "er", "sein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "PPOSAT"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "In Wahrheit und Gerechtigkeit,", "tokens": ["In", "Wahr\u00b7heit", "und", "Ge\u00b7rech\u00b7tig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In Unschuld, Lieb' und Freundlichkeit", "tokens": ["In", "Un\u00b7schuld", ",", "Lieb'", "und", "Freund\u00b7lich\u00b7keit"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und Menscheng\u00fcte.", "tokens": ["Und", "Men\u00b7schen\u00b7g\u00fc\u00b7te", "."], "token_info": ["word", "word", "punct"], "pos": ["KON", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "Wer unser arm Geschlecht entehrt,", "tokens": ["Wer", "un\u00b7ser", "arm", "Ge\u00b7schlecht", "ent\u00b7ehrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "ADJD", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ist nicht dieses K\u00f6nigs werth;", "tokens": ["Ist", "nicht", "die\u00b7ses", "K\u00f6\u00b7nigs", "werth", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "PDAT", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wer Menschen hasset und betr\u00fcbt,", "tokens": ["Wer", "Men\u00b7schen", "has\u00b7set", "und", "be\u00b7tr\u00fcbt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VVFIN", "KON", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nicht statt des B\u00f6sen Gutes giebt,", "tokens": ["Nicht", "statt", "des", "B\u00f6\u00b7sen", "Gu\u00b7tes", "giebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ist sein nicht w\u00fcrdig.", "tokens": ["Ist", "sein", "nicht", "w\u00fcr\u00b7dig", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "PTKNEG", "ADJD", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.8": {"line.1": {"text": "O stimmt der Engel Gl\u00fcckwunsch bei:", "tokens": ["O", "stimmt", "der", "En\u00b7gel", "Gl\u00fcck\u00b7wunsch", "bei", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbfried' auf Erden! Friede sei", "tokens": ["\u00bb", "fried'", "auf", "Er\u00b7den", "!", "Frie\u00b7de", "sei"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word"], "pos": ["$(", "VVIMP", "APPR", "NN", "$.", "NN", "VAFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Den Menschen!\u00ab So ist Gram und Leid", "tokens": ["Den", "Men\u00b7schen", "!", "\u00ab", "So", "ist", "Gram", "und", "Leid"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$.", "$(", "ADV", "VAFIN", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Verschwunden. Unser Herz erfreut", "tokens": ["Ver\u00b7schwun\u00b7den", ".", "Un\u00b7ser", "Herz", "er\u00b7freut"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$.", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sein Wohlgefallen.", "tokens": ["Sein", "Wohl\u00b7ge\u00b7fal\u00b7len", "."], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, 