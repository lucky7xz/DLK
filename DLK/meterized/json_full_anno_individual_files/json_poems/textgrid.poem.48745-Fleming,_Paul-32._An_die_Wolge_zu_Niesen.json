{"textgrid.poem.48745": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "32. An die Wolge zu Niesen", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Seid mehr als sehr gegr\u00fc\u00dft, ihr Nymphen dieser Enden,", "tokens": ["Seid", "mehr", "als", "sehr", "ge\u00b7gr\u00fc\u00dft", ",", "ihr", "Nym\u00b7phen", "die\u00b7ser", "En\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "PIAT", "KOKOM", "ADV", "VVPP", "$,", "PPOSAT", "NN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "ihr weiches Wasservolk, und du auch, edler Flu\u00df,", "tokens": ["ihr", "wei\u00b7ches", "Was\u00b7ser\u00b7volk", ",", "und", "du", "auch", ",", "ed\u00b7ler", "Flu\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "KON", "PPER", "ADV", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "o unser sp\u00e4ter Trost, empfange diesen Gru\u00df", "tokens": ["o", "un\u00b7ser", "sp\u00e4\u00b7ter", "Trost", ",", "emp\u00b7fan\u00b7ge", "die\u00b7sen", "Gru\u00df"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["FM", "PPOSAT", "ADJA", "NN", "$,", "VVFIN", "PDAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "und nim die M\u00e4nner an, die dir die ", "tokens": ["und", "nim", "die", "M\u00e4n\u00b7ner", "an", ",", "die", "dir", "die"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVIMP", "ART", "NN", "PTKVZ", "$,", "PRELS", "PPER", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die sind es, die sich dir zu Nutzen hoch verpf\u00e4nden.", "tokens": ["Die", "sind", "es", ",", "die", "sich", "dir", "zu", "Nut\u00b7zen", "hoch", "ver\u00b7pf\u00e4n\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "$,", "PRELS", "PRF", "PPER", "APPR", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nim, Gastfreund, nim sie auf und sicher' ihren Fu\u00df,", "tokens": ["Nim", ",", "Gast\u00b7freund", ",", "nim", "sie", "auf", "und", "si\u00b7cher'", "ih\u00b7ren", "Fu\u00df", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "VVIMP", "PPER", "PTKVZ", "KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "da\u00df, was sich hat verschworn zu schaden, fallen mu\u00df,", "tokens": ["da\u00df", ",", "was", "sich", "hat", "ver\u00b7schworn", "zu", "scha\u00b7den", ",", "fal\u00b7len", "mu\u00df", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "$,", "PRELS", "PRF", "VAFIN", "VVINF", "PTKZU", "VVINF", "$,", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "sie an dein ", "tokens": ["sie", "an", "dein"], "token_info": ["word", "word", "word"], "pos": ["PPER", "APPR", "PPOSAT"], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "Di\u00df Schiff, das Venus selbst nach ihrer Muschel liebt,", "tokens": ["Di\u00df", "Schiff", ",", "das", "Ve\u00b7nus", "selbst", "nach", "ih\u00b7rer", "Mu\u00b7schel", "liebt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word",