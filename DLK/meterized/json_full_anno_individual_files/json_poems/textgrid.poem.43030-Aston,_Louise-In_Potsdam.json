{"textgrid.poem.43030": {"metadata": {"author": {"name": "Aston, Louise", "birth": "N.A.", "death": "N.A."}, "title": "In Potsdam", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.85", "sv:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vom Dome hallen Glockenkl\u00e4nge \u2013", "tokens": ["Vom", "Do\u00b7me", "hal\u00b7len", "Glo\u00b7cken\u00b7kl\u00e4n\u00b7ge", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Stille Andacht \u00fcberall,", "tokens": ["Stil\u00b7le", "An\u00b7dacht", "\u00fc\u00b7be\u00b7rall", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Gl\u00e4ubig singt des Volkes Menge", "tokens": ["Gl\u00e4u\u00b7big", "singt", "des", "Vol\u00b7kes", "Men\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zu der Orgel hellem Schall;", "tokens": ["Zu", "der", "Or\u00b7gel", "hel\u00b7lem", "Schall", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Dort in einsamer Kapelle", "tokens": ["Dort", "in", "ein\u00b7sa\u00b7mer", "Ka\u00b7pel\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.6": {"text": "An des Altars heilger Schwelle", "tokens": ["An", "des", "Al\u00b7tars", "heil\u00b7ger", "Schwel\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {