{"textgrid.poem.49870": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Weihgeschenk", "genre": "verse", "period": "N.A.", "pub_year": 1846, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von kunstfertigen H\u00e4nden gesch\u00e4lt, drei \u00c4pfelchen, zierlich,", "tokens": ["Von", "kunst\u00b7fer\u00b7ti\u00b7gen", "H\u00e4n\u00b7den", "ge\u00b7sch\u00e4lt", ",", "drei", "\u00c4p\u00b7fel\u00b7chen", ",", "zier\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,", "CARD", "NN", "$,", "ADJD", "$,"], "meter": "--+--+--+--+-+-", "measure": "anapaest.tetra.plus"}, "line.2": {"text": "H\u00e4ngend an ", "tokens": ["H\u00e4n\u00b7gend", "an"], "token_info": ["word", "word"], "pos": ["ADJD", "PTKVZ"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Wei\u00df wie das Wachs ihr Fleisch, von lieblicher R\u00f6te durchschimmert;", "tokens": ["Wei\u00df", "wie", "das", "Wachs", "ihr", "Fleisch", ",", "von", "lieb\u00b7li\u00b7cher", "R\u00f6\u00b7te", "durch\u00b7schim\u00b7mert", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "ART", "NN", "PPOSAT", "NN", "$,", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Dicht aneinandergeschmiegt, b\u00e4rgen die nackten sich gern.", "tokens": ["Dicht", "an\u00b7ein\u00b7an\u00b7der\u00b7ge\u00b7schmiegt", ",", "b\u00e4r\u00b7gen", "die", "nack\u00b7ten", "sich", "gern", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,", "VVFIN", "ART", "ADJA", "PRF", "ADV", "$."], "meter": "-+-+--++--+--+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Sch\u00e4met euch nicht, ihr Schwestern! euch hat ein M\u00e4dchen entkleidet,", "tokens": ["Sch\u00e4\u00b7met", "euch", "nicht", ",", "ihr", "Schwes\u00b7tern", "!", "euch", "hat", "ein", "M\u00e4d\u00b7chen", "ent\u00b7klei\u00b7det", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "$,", "PPOSAT", "NN", "$.", "PPER", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.6": {"text": "Und den Chariten fromm bringet ein S\u00e4nger euch dar.", "tokens": ["Und", "den", "Cha\u00b7ri\u00b7ten", "fromm", "brin\u00b7get", "ein", "S\u00e4n\u00b7ger", "euch", "dar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJD", "VVFIN", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+--+--+--+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Von kunstfertigen H\u00e4nden gesch\u00e4lt, drei \u00c4pfelchen, zierlich,", "tokens": ["Von", "kunst\u00b7fer\u00b7ti\u00b7gen", "H\u00e4n\u00b7den", "ge\u00b7sch\u00e4lt", ",", "drei", "\u00c4p\u00b7fel\u00b7chen", ",", "zier\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,", "CARD", "NN", "$,", "ADJD", "$,"], "meter": "--+--+--+--+-+-", "measure": "anapaest.tetra.plus"}, "line.2": {"text": "H\u00e4ngend an ", "tokens": ["H\u00e4n\u00b7gend", "an"], "token_info": ["word", "word"], "pos": ["ADJD", "PTKVZ"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Wei\u00df wie das Wachs ihr Fleisch, von lieblicher R\u00f6te durchschimmert;", "tokens": ["Wei\u00df", "wie", "das", "Wachs", "ihr", "Fleisch", ",", "von", "lieb\u00b7li\u00b7cher", "R\u00f6\u00b7te", "durch\u00b7schim\u00b7mert", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word