{"textgrid.poem.40973": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Metall und Glas", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es ist ein Mann von Eisen", "tokens": ["Es", "ist", "ein", "Mann", "von", "Ei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein anderer von Glas,", "tokens": ["Ein", "an\u00b7de\u00b7rer", "von", "Glas", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die wollen sich beflei\u00dfen,", "tokens": ["Die", "wol\u00b7len", "sich", "be\u00b7flei\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Einander zu unterweisen,", "tokens": ["Ein\u00b7an\u00b7der", "zu", "un\u00b7ter\u00b7wei\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PTKZU", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Probieren dies und das.", "tokens": ["Pro\u00b7bie\u00b7ren", "dies", "und", "das", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PDS", "KON", "PDS", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Aus seiner Ledertasche", "tokens": ["Aus", "sei\u00b7ner", "Le\u00b7der\u00b7ta\u00b7sche"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zieht der metallne Mann", "tokens": ["Zieht", "der", "me\u00b7tall\u00b7ne", "Mann"], "token_info": ["word", "