{"dta.poem.20757": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "Der Unterschied eines Schmauses und einer  \n kleinen vergn\u00fcgten Mahlzeit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "O Lieber! sprich, wann essen               ", "tokens": ["O", "Lie\u00b7ber", "!", "sprich", ",", "wann", "es\u00b7sen"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["NE", "ADJD", "$.", "ADJD", "$,", "PWAV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Menschen mehr als k\u00f6niglich?", "tokens": ["Die", "Men\u00b7schen", "mehr", "als", "k\u00f6\u00b7nig\u00b7lich", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "KOKOM", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u201ewenn sie die ganze Welt vergessen,", "tokens": ["\u201e", "wenn", "sie", "die", "gan\u00b7ze", "Welt", "ver\u00b7ges\u00b7sen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "\u201eund so vergn\u00fcgt als Gleim, und Du und ich", "tokens": ["\u201e", "und", "so", "ver\u00b7gn\u00fcgt", "als", "Gleim", ",", "und", "Du", "und", "ich"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "KON", "ADV", "ADJD", "KOKOM", "NE", "$,", "KON", "PPER", "KON", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "\u201emit wenig Sch\u00fcsseln sind, und niemand f\u00fcrchten", "tokens": ["\u201e", "mit", "we\u00b7nig", "Sch\u00fcs\u00b7seln", "sind", ",", "und", "nie\u00b7mand", "f\u00fcrch\u00b7ten"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "APPR", "PIAT", "NN", "VAFIN", "$,", "KON", "PIS", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "d\u00fcrfen,", "tokens": ["d\u00fcr\u00b7fen", ","], "token_info": ["word", "punct"], "pos": ["VMFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.7": {"text": "\u201eund ihren Wein mit einer Zunge schl\u00fcrfen,", "tokens": ["\u201e", "und", "ih\u00b7ren", "Wein", "mit", "ei\u00b7ner", "Zun\u00b7ge", "schl\u00fcr\u00b7fen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "PPOSAT", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "\u201edie frey und unbeurtheilt spricht.\u201c", "tokens": ["\u201e", "die", "frey", "und", "un\u00b7be\u00b7urt\u00b7heilt", "spricht", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "ADJD", "KON", "ADJD", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "O solch ein Mahl, die F\u00fcrsten habens nicht!", "tokens": ["O", "solch", "ein", "Mahl", ",", "die", "F\u00fcrs\u00b7ten", "ha\u00b7bens", "nicht", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PIAT", "ART", "NN", "$,", "ART", "NN", "VAFIN", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}