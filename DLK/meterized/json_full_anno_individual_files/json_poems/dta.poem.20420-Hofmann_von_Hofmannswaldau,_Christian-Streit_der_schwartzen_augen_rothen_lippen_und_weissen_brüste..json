{"dta.poem.20420": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Streit der schwartzen augen/ rothen  \n lippen/ und weissen br\u00fcste.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein wohlgesch\u00e4rffter spruch von unserm rothen throne", "tokens": ["Ein", "wohl\u00b7ge\u00b7sch\u00e4rff\u00b7ter", "spruch", "von", "un\u00b7serm", "ro\u00b7then", "thro\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Thut und verrichtet mehr/ als euer stoltzes licht;", "tokens": ["Thut", "und", "ver\u00b7rich\u00b7tet", "mehr", "/", "als", "eu\u00b7er", "stolt\u00b7zes", "licht", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "VVFIN", "ADV", "$(", "KOKOM", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Was seyd ihr bey der nacht? Ich red es euch zu hohne/", "tokens": ["Was", "seyd", "ihr", "bey", "der", "nacht", "?", "Ich", "red", "es", "euch", "zu", "hoh\u00b7ne", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "APPR", "ART", "NN", "$.", "PPER", "VVFIN", "PPER", "PRF", "APPR", "ADJA", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wann nicht die sonne scheint/ so sieht das auge nicht.", "tokens": ["Wann", "nicht", "die", "son\u00b7ne", "scheint", "/", "so", "sieht", "das", "au\u00b7ge", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PTKNEG", "ART", "NN", "VVFIN", "$(", "ADV", "VVFIN", "ART", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wir aber herrschen auch/ wenn Ph\u00f6bus von uns weichet/", "tokens": ["Wir", "a\u00b7ber", "herr\u00b7schen", "auch", "/", "wenn", "Ph\u00f6\u00b7bus", "von", "uns", "wei\u00b7chet", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ADV", "$(", "KOUS", "NE", "APPR", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ja/ wenn ihr sternen-heer von wolcken wird bedeckt/", "tokens": ["Ja", "/", "wenn", "ihr", "ster\u00b7nen\u00b7heer", "von", "wol\u00b7cken", "wird", "be\u00b7deckt", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$(", "KOUS", "PPER", "ADJD", "APPR", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "So hat manch kluges wort/ so durch die rosen streichet/", "tokens": ["So", "hat", "manch", "klu\u00b7ges", "wort", "/", "so", "durch", "die", "ro\u00b7sen", "strei\u00b7chet", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIAT", "ADJA", "NN", "$(", "ADV", "APPR", "ART", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die l\u00f6wen eingeschl\u00e4fft und harte stein erweckt.", "tokens": ["Die", "l\u00f6\u00b7wen", "ein\u00b7ge\u00b7schl\u00e4fft", "und", "har\u00b7te", "stein", "er\u00b7weckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "KON", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}