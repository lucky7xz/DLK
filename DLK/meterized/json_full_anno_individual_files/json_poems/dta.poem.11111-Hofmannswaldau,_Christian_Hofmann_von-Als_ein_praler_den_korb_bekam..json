{"dta.poem.11111": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Als ein praler den korb bekam.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du denckst, dein sch\u00f6nes kleid soll alle m\u00e4gdgen fangen,", "tokens": ["Du", "denckst", ",", "dein", "sch\u00f6\u00b7nes", "kleid", "soll", "al\u00b7le", "m\u00e4gd\u00b7gen", "fan\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPOSAT", "ADJA", "NN", "VMFIN", "PIS", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und dein gepudert kopff gewisse gunst erlangen.", "tokens": ["Und", "dein", "ge\u00b7pu\u00b7dert", "kopff", "ge\u00b7wis\u00b7se", "gunst", "er\u00b7lan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "VVPP", "NN", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "O nein! Calliste spricht: Es ist mit dem kein rath,", "tokens": ["O", "nein", "!", "Cal\u00b7lis\u00b7te", "spricht", ":", "Es", "ist", "mit", "dem", "kein", "rath", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKANT", "$.", "NE", "VVFIN", "$.", "PPER", "VAFIN", "APPR", "ART", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der gold auf hosen tr\u00e4gt, und keines drinnen hat.", "tokens": ["Der", "gold", "auf", "ho\u00b7sen", "tr\u00e4gt", ",", "und", "kei\u00b7nes", "drin\u00b7nen", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "VVFIN", "$,", "KON", "PIS", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}