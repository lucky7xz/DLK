{"dta.poem.15779": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "39.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sch\u00f6n ist Geringstes, das die rechte Form gefunden,", "tokens": ["Sch\u00f6n", "ist", "Ge\u00b7rings\u00b7tes", ",", "das", "die", "rech\u00b7te", "Form", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "NE", "$,", "PRELS", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Und werthlos Edelstes, von falscher Form gebunden.", "tokens": ["Und", "wert\u00b7hlos", "E\u00b7dels\u00b7tes", ",", "von", "fal\u00b7scher", "Form", "ge\u00b7bun\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "NN", "$,", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Des Edelsteines Werth erh\u00f6ht sie nicht allein,", "tokens": ["Des", "E\u00b7del\u00b7stei\u00b7nes", "Werth", "er\u00b7h\u00f6ht", "sie", "nicht", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Fassung selber macht hier erst den Edelstein.", "tokens": ["Die", "Fas\u00b7sung", "sel\u00b7ber", "macht", "hier", "erst", "den", "E\u00b7del\u00b7stein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}