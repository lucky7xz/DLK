{"dta.poem.18349": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "Fragezeichen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1886", "urn": "urn:nbn:de:kobv:b4-200905192665", "language": ["de:0.99"], "booktitle": "Holz, Arno: Das Buch der Zeit. Lieder eines Modernen. Z\u00fcrich, 1886."}, "poem": {"stanza.1": {"line.1": {"text": "\u201eder Zeitgeist ist ein alter S\u00fcnder", "tokens": ["\u201e", "der", "Zeit\u00b7geist", "ist", "ein", "al\u00b7ter", "S\u00fcn\u00b7der"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "ART", "NN", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und stopfen k\u00f6nnen ihm sein Maul", "tokens": ["Und", "stop\u00b7fen", "k\u00f6n\u00b7nen", "ihm", "sein", "Maul"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVINF", "VMFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nur Krupp'sche Vierundzwanzigpf\u00fcnder!\u201c", "tokens": ["Nur", "Krupp'\u00b7sche", "Vie\u00b7rund\u00b7zwan\u00b7zig\u00b7pf\u00fcn\u00b7der", "!", "\u201c"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Doch Paul kann Peter nicht besehn,", "tokens": ["Doch", "Paul", "kann", "Pe\u00b7ter", "nicht", "be\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VMFIN", "NE", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Weil er sein Lebtag nur gelungert", "tokens": ["Weil", "er", "sein", "Leb\u00b7tag", "nur", "ge\u00b7lun\u00b7gert"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "ADV", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und meint, als w\u00e4re nichts geschehn:", "tokens": ["Und", "meint", ",", "als", "w\u00e4\u00b7re", "nichts", "ge\u00b7schehn", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOKOM", "VAFIN", "PIS", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u201edu Peter, hast du mal gehungert?\u201c", "tokens": ["\u201e", "du", "Pe\u00b7ter", ",", "hast", "du", "mal", "ge\u00b7hun\u00b7gert", "?", "\u201c"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "NE", "$,", "VAFIN", "PPER", "ADV", "VVPP", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}