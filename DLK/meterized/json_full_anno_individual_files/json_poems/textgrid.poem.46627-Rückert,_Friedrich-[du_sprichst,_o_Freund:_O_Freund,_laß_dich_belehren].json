{"textgrid.poem.46627": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[du sprichst, o Freund: O Freund, la\u00df dich belehren]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du sprichst, o Freund: O Freund, la\u00df dich belehren,", "tokens": ["Du", "sprichst", ",", "o", "Freund", ":", "O", "Freund", ",", "la\u00df", "dich", "be\u00b7leh\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "FM", "NN", "$.", "NE", "NN", "$,", "VVIMP", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie du das Schlimme mu\u00dft zum Besten kehren,", "tokens": ["Wie", "du", "das", "Schlim\u00b7me", "mu\u00dft", "zum", "Bes\u00b7ten", "keh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VMFIN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie du den bittern Stachel kannst vers\u00fc\u00dfen", "tokens": ["Wie", "du", "den", "bit\u00b7tern", "Sta\u00b7chel", "kannst", "ver\u00b7s\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ART", "ADJA", "NN", "VMFIN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und mit Gewinne lernest einzub\u00fc\u00dfen.", "tokens": ["Und", "mit", "Ge\u00b7win\u00b7ne", "ler\u00b7nest", "ein\u00b7zu\u00b7b\u00fc\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVFIN", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Es waren Kinder dir zu viel verliehen", "tokens": ["Es", "wa\u00b7ren", "Kin\u00b7der", "dir", "zu", "viel", "ver\u00b7lie\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "NN", "PPER", "APPR", "PIS", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Schon zum Ern\u00e4hren, mehr noch zum Erziehen;", "tokens": ["Schon", "zum", "Er\u00b7n\u00e4h\u00b7ren", ",", "mehr", "noch", "zum", "Er\u00b7zie\u00b7hen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$,", "ADV", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Du h\u00e4ttest sie, mit nothgedrungner Sparung", "tokens": ["Du", "h\u00e4t\u00b7test", "sie", ",", "mit", "noth\u00b7ge\u00b7drung\u00b7ner", "Spa\u00b7rung"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Verk\u00fcrzt an Leibes- wie an Seelennahrung.", "tokens": ["Ver\u00b7k\u00fcrzt", "an", "Lei\u00b7bes", "wie", "an", "See\u00b7len\u00b7nah\u00b7rung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "TRUNC", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Nun hat dir Gott ein Drittel abgenommen,", "tokens": ["Nun", "hat", "dir", "Gott", "ein", "Drit\u00b7tel", "ab\u00b7ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Das wird zu Gute den zwei Dritteln kommen;", "tokens": ["Das", "wird", "zu", "Gu\u00b7te", "den", "zwei", "Drit\u00b7teln", "kom\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "NN", "ART", "CARD", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Da\u00df du den Ueberbliebnen gr\u00f6\u00dfre Bissen", "tokens": ["Da\u00df", "du", "den", "Ue\u00b7ber\u00b7blieb\u00b7nen", "gr\u00f6\u00df\u00b7re", "Bis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Zuschneiden kannst vom Brodlaib und vom Wissen.", "tokens": ["Zu\u00b7schnei\u00b7den", "kannst", "vom", "Brod\u00b7laib", "und", "vom", "Wis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "APPRART", "NN", "KON", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Und sehr zweckm\u00e4\u00dfig nahm er die zwei kleinsten,", "tokens": ["Und", "sehr", "zweck\u00b7m\u00e4\u00b7\u00dfig", "nahm", "er", "die", "zwei", "kleins\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VVFIN", "PPER", "ART", "CARD", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Die liebsten freilich sagst du, und die feinsten,", "tokens": ["Die", "liebs\u00b7ten", "frei\u00b7lich", "sagst", "du", ",", "und", "die", "feins\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "VVFIN", "PPER", "$,", "KON", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Doch die du selber gro\u00df zu ziehn wahrscheinlich", "tokens": ["Doch", "die", "du", "sel\u00b7ber", "gro\u00df", "zu", "ziehn", "wahr\u00b7schein\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PRELS", "PPER", "ADV", "ADJD", "PTKZU", "VVINF", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Nicht hoffen durftest, der Gedank' ist peinlich.", "tokens": ["Nicht", "hof\u00b7fen", "durf\u00b7test", ",", "der", "Ge\u00b7dank'", "ist", "pein\u00b7lich", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "VVFIN", "$,", "ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Die Sorg' ist nun mit ihrem Staub zerstoben,", "tokens": ["Die", "Sor\u00b7g'", "ist", "nun", "mit", "ih\u00b7rem", "Staub", "zer\u00b7sto\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.18": {"text": "Denn diese sind versorgt und aufgehoben;", "tokens": ["Denn", "die\u00b7se", "sind", "ver\u00b7sorgt", "und", "auf\u00b7ge\u00b7ho\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "VVPP", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Und f\u00fcr die gr\u00f6\u00dfern brauchst du nicht zu zagen;", "tokens": ["Und", "f\u00fcr", "die", "gr\u00f6\u00b7\u00dfern", "brauchst", "du", "nicht", "zu", "za\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "VVFIN", "PPER", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Die werden, wenn du stirbst, sich schon durchschlagen.", "tokens": ["Die", "wer\u00b7den", ",", "wenn", "du", "stirbst", ",", "sich", "schon", "durch\u00b7schla\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "VAINF", "$,", "KOUS", "PPER", "VVFIN", "$,", "PRF", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Ich sprach: von deiner Weisheit war's zu hoffen,", "tokens": ["Ich", "sprach", ":", "von", "dei\u00b7ner", "Weis\u00b7heit", "wa\u00b7r's", "zu", "hof\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "APPR", "PPOSAT", "NN", "VAFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.22": {"text": "Du hast den Nagel auf den Kopf getroffen.", "tokens": ["Du", "hast", "den", "Na\u00b7gel", "auf", "den", "Kopf", "ge\u00b7trof\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NE", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Und wenn vollwichtig nicht die Gr\u00fcnde schienen,", "tokens": ["Und", "wenn", "voll\u00b7wich\u00b7tig", "nicht", "die", "Gr\u00fcn\u00b7de", "schie\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADJD", "PTKNEG", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "K\u00f6nnt' ihnen dieses noch zu Nachdruck dienen:", "tokens": ["K\u00f6nnt'", "ih\u00b7nen", "die\u00b7ses", "noch", "zu", "Nach\u00b7druck", "die\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PDS", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "Ich werde nun mit meiner Schaar bequemer", "tokens": ["Ich", "wer\u00b7de", "nun", "mit", "mei\u00b7ner", "Schaar", "be\u00b7que\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "PPOSAT", "NN", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "Spazieren gehn, wenn auch nicht angenehmer,", "tokens": ["Spa\u00b7zie\u00b7ren", "gehn", ",", "wenn", "auch", "nicht", "an\u00b7ge\u00b7neh\u00b7mer", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "KOUS", "ADV", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Weil ich mich auf dem Heimweg nicht mu\u00df plagen,", "tokens": ["Weil", "ich", "mich", "auf", "dem", "Heim\u00b7weg", "nicht", "mu\u00df", "pla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPR", "ART", "NN", "PTKNEG", "VMFIN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "Die kleinen m\u00fcdgewordenen zu tragen.", "tokens": ["Die", "klei\u00b7nen", "m\u00fcd\u00b7ge\u00b7wor\u00b7de\u00b7nen", "zu", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "So werd' ich auch bei Tisch nicht Hunger leiden,", "tokens": ["So", "werd'", "ich", "auch", "bei", "Tisch", "nicht", "Hun\u00b7ger", "lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "NN", "PTKNEG", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.30": {"text": "Um ihnen vorzulegen, vorzuschneiden;", "tokens": ["Um", "ih\u00b7nen", "vor\u00b7zu\u00b7le\u00b7gen", ",", "vor\u00b7zu\u00b7schnei\u00b7den", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KOUI", "PPER", "VVIZU", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.31": {"text": "Da mir die M\u00fch' ersparen diese Rangen,", "tokens": ["Da", "mir", "die", "M\u00fch'", "er\u00b7spa\u00b7ren", "die\u00b7se", "Ran\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.32": {"text": "Die schon von selber in die Sch\u00fcssel langen.", "tokens": ["Die", "schon", "von", "sel\u00b7ber", "in", "die", "Sch\u00fcs\u00b7sel", "lan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.33": {"text": "Auch werden mich nicht mehr mit ihrem Plappern", "tokens": ["Auch", "wer\u00b7den", "mich", "nicht", "mehr", "mit", "ih\u00b7rem", "Plap\u00b7pern"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PTKNEG", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.34": {"text": "Bet\u00e4uben zwei lebend'ge Kinderklappern,", "tokens": ["Be\u00b7t\u00e4u\u00b7ben", "zwei", "le\u00b7ben\u00b7d'\u00b7ge", "Kin\u00b7der\u00b7klap\u00b7pern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "CARD", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.35": {"text": "Mich in Verlegenheit, wie in Ergetzen,", "tokens": ["Mich", "in", "Ver\u00b7le\u00b7gen\u00b7heit", ",", "wie", "in", "Er\u00b7get\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "$,", "PWAV", "APPR", "NN", "$,"], "meter": "-+-+-+-++--", "measure": "unknown.measure.penta"}, "line.36": {"text": "Mit unbeantwortbaren Fragen setzen;", "tokens": ["Mit", "un\u00b7be\u00b7ant\u00b7wort\u00b7ba\u00b7ren", "Fra\u00b7gen", "set\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.37": {"text": "Weil meine andern Wi\u00dfbegier'gen sch\u00f6pfen", "tokens": ["Weil", "mei\u00b7ne", "an\u00b7dern", "Wi\u00df\u00b7be\u00b7gier'\u00b7gen", "sch\u00f6p\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.38": {"text": "Die Weisheit selber schon aus B\u00fcchert\u00f6pfen,", "tokens": ["Die", "Weis\u00b7heit", "sel\u00b7ber", "schon", "aus", "B\u00fc\u00b7cher\u00b7t\u00f6p\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.39": {"text": "Und alles besser als ihr Vater wissen,", "tokens": ["Und", "al\u00b7les", "bes\u00b7ser", "als", "ihr", "Va\u00b7ter", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "ADJD", "KOKOM", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.40": {"text": "Der seinen alten Schulsack abgerissen.", "tokens": ["Der", "sei\u00b7nen", "al\u00b7ten", "Schul\u00b7sack", "ab\u00b7ge\u00b7ris\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.41": {"text": "Soweit ist's gut, o Freund! nur Eines wende", "tokens": ["So\u00b7weit", "ist's", "gut", ",", "o", "Freund", "!", "nur", "Ei\u00b7nes", "wen\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VAFIN", "ADJD", "$,", "FM", "NN", "$.", "ADV", "PIS", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.42": {"text": "Ich ein noch gegen deinen Trost am Ende:", "tokens": ["Ich", "ein", "noch", "ge\u00b7gen", "dei\u00b7nen", "Trost", "am", "En\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADV", "APPR", "PPOSAT", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.43": {"text": "Warum ich nicht, wollt' es der Himmel g\u00f6nnen,", "tokens": ["Wa\u00b7rum", "ich", "nicht", ",", "wollt'", "es", "der", "Him\u00b7mel", "g\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PTKNEG", "$,", "VMFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.44": {"text": "Die Kleinen auch so h\u00e4tte gro\u00dfziehn k\u00f6nnen?", "tokens": ["Die", "Klei\u00b7nen", "auch", "so", "h\u00e4t\u00b7te", "gro\u00df\u00b7ziehn", "k\u00f6n\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "VAFIN", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.45": {"text": "Wenn du das Leben nicht zu fr\u00fch mir raubest,", "tokens": ["Wenn", "du", "das", "Le\u00b7ben", "nicht", "zu", "fr\u00fch", "mir", "rau\u00b7best", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PTKNEG", "PTKA", "ADJD", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.46": {"text": "Und etwa noch zehn Jahre mir erlaubest,", "tokens": ["Und", "et\u00b7wa", "noch", "zehn", "Jah\u00b7re", "mir", "er\u00b7lau\u00b7best", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "CARD", "NN", "PPER", "VVFIN", 