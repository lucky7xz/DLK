{"textgrid.poem.43193": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ihr ahnt nicht, wie der dumpfe Drang", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr ahnt nicht, wie der dumpfe Drang", "tokens": ["Ihr", "ahnt", "nicht", ",", "wie", "der", "dump\u00b7fe", "Drang"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWAV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Seele mir zerrissen,", "tokens": ["Die", "See\u00b7le", "mir", "zer\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und wie ich litt, ach, wie ich rang", "tokens": ["Und", "wie", "ich", "litt", ",", "ach", ",", "wie", "ich", "rang"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "$,", "ITJ", "$,", "PWAV", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In Schmerz und Finsternissen \u2013", "tokens": ["In", "Schmerz", "und", "Fins\u00b7ter\u00b7nis\u00b7sen", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wie einst so bang, so qualvoll-bang", "tokens": ["Wie", "einst", "so", "bang", ",", "so", "qua\u00b7lvoll\u00b7bang"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "ADV", "ADV", "ADJD", "$,", "ADV", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Durch Hirn und Herz geklungen,", "tokens": ["Durch", "Hirn", "und", "Herz", "ge\u00b7klu