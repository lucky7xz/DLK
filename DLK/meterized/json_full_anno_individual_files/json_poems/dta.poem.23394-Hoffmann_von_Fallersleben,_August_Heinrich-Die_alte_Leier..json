{"dta.poem.23394": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die alte Leier.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Edelmann, er schenkt sich flei\u00dfig ein:", "tokens": ["Der", "E\u00b7del\u00b7mann", ",", "er", "schenkt", "sich", "flei\u00b7\u00dfig", "ein", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "VVFIN", "PRF", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ich kenne nur noch diesen G\u00e4nsewein.", "tokens": ["Ich", "ken\u00b7ne", "nur", "noch", "die\u00b7sen", "G\u00e4n\u00b7se\u00b7wein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mein Vater weiland zahlte keine Steuer;", "tokens": ["Mein", "Va\u00b7ter", "wei\u00b7land", "zahl\u00b7te", "kei\u00b7ne", "Steu\u00b7er", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das Korn ist wohlfeil jetzt, das Leben theuer.", "tokens": ["Das", "Korn", "ist", "wohl\u00b7feil", "jetzt", ",", "das", "Le\u00b7ben", "theu\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "ADV", "$,", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Doch liegt ein Trost in einer alten Sage,", "tokens": ["Doch", "liegt", "ein", "Trost", "in", "ei\u00b7ner", "al\u00b7ten", "Sa\u00b7ge", ","], "token_i