{"dta.poem.13567": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Mandelkerngedicht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Zwischen Akten, dunkeln W\u00e4nden", "tokens": ["Zwi\u00b7schen", "Ak\u00b7ten", ",", "dun\u00b7keln", "W\u00e4n\u00b7den"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bannt mich, Freiheitbegehr", "tokens": ["Bannt", "mich", ",", "Frei\u00b7heit\u00b7be\u00b7gehr"], "token_info": ["word", "word", "punct", "word"], "pos": ["VVFIN", "PPER", "$,", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Nun des Lebens strenge Pflicht,", "tokens": ["Nun", "des", "Le\u00b7bens", "stren\u00b7ge", "Pflicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und aus Schr\u00e4nken, Akten-Schichten", "tokens": ["Und", "aus", "Schr\u00e4n\u00b7ken", ",", "Ak\u00b7ten\u00b7Schich\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["KON", "APPR", "NN", "$,", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Lachen mir die beleid", "tokens": ["La\u00b7chen", "mir", "die", "be\u00b7leid"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PPER", "ART", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.6": {"text": "Musen in das Amts-Gesicht.", "tokens": ["Mu\u00b7sen", "in", "das", "Amts\u00b7Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Als an Lenz und Morgenr\u00f6the", "tokens": ["Als", "an", "Lenz", "und", "Mor\u00b7gen\u00b7r\u00f6\u00b7the"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Noch das Herz sich erlab", "tokens": ["Noch", "das", "Herz", "sich", "er\u00b7lab"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "PRF", "VVFIN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "O du stilles, heit'res Gl\u00fcck!", "tokens": ["O", "du", "stil\u00b7les", ",", "heit'\u00b7res", "Gl\u00fcck", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie ich nun auch hei\u00df mich sehne,", "tokens": ["Wie", "ich", "nun", "auch", "hei\u00df", "mich", "seh\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADV", "ADJD", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ach, aus dieser Sandeb", "tokens": ["Ach", ",", "aus", "die\u00b7ser", "San\u00b7deb"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ITJ", "$,", "APPR", "PDAT", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "F\u00fchrt kein Weg dahin zur\u00fcck.", "tokens": ["F\u00fchrt", "kein", "Weg", "da\u00b7hin", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "PAV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Als der letzte Balkentreter", "tokens": ["Als", "der", "letz\u00b7te", "Bal\u00b7ken\u00b7tre\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Steh' ich armer Enterb", "tokens": ["Steh'", "ich", "ar\u00b7mer", "En\u00b7terb"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "In des Staates Symphonie,", "tokens": ["In", "des", "Staa\u00b7tes", "Sym\u00b7pho\u00b7nie", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ach, in diesem Schwall von T\u00f6nen", "tokens": ["Ach", ",", "in", "die\u00b7sem", "Schwall", "von", "T\u00f6\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "APPR", "PDAT", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wo f\u00e4nd' ich da des eige", "tokens": ["Wo", "f\u00e4nd'", "ich", "da", "des", "ei\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "ART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Herzens s\u00fc\u00dfe Melodie?", "tokens": ["Her\u00b7zens", "s\u00fc\u00b7\u00dfe", "Me\u00b7lo\u00b7die", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Ein Gedicht soll ich Euch spenden:", "tokens": ["Ein", "Ge\u00b7dicht", "soll", "ich", "Euch", "spen\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nun, so geht mit dem Leid", "tokens": ["Nun", ",", "so", "geht", "mit", "dem", "Leid"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "ADV", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Nicht zu strenge ins Gericht!", "tokens": ["Nicht", "zu", "stren\u00b7ge", "ins", "Ge\u00b7richt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PTKZU", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Nehmt den Willen f\u00fcr Gew\u00e4hrung,", "tokens": ["Nehmt", "den", "Wil\u00b7len", "f\u00fcr", "Ge\u00b7w\u00e4h\u00b7rung", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "K\u00fchnen Reim f\u00fcr Begeist", "tokens": ["K\u00fch\u00b7nen", "Reim", "f\u00fcr", "Be\u00b7geist"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "APPR", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Diesen Unsinn als Gedicht!", "tokens": ["Die\u00b7sen", "Un\u00b7sinn", "als", "Ge\u00b7dicht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "KOUS", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}