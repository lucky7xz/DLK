{"textgrid.poem.31432": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du treuer Augensafft! wann ich schier gar verschmachte /", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du treuer Augensafft! wann ich schier gar verschmachte /", "tokens": ["Du", "treu\u00b7er", "Au\u00b7gen\u00b7safft", "!", "wann", "ich", "schier", "gar", "ver\u00b7schmach\u00b7te", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$.", "PWAV", "PPER", "ADV", "ADV", "VVFIN", "$("], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "in Ohnmacht sink dahin / so spritzstu ins Gesicht.", "tokens": ["in", "Ohn\u00b7macht", "sink", "da\u00b7hin", "/", "so", "spritz\u00b7stu", "ins", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PAV", "$(", "ADV", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Du bist bey mir / wann ich bin bey mir selber nicht.", "tokens": ["Du", "bist", "bey", "mir", "/", "wann", "ich", "bin", "bey", "mir", "sel\u00b7ber", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "$(", "PWAV", "PPER", "VAFIN", "APPR", "PPER", "ADV", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sonst alle Labnu\u00df ich / nur deine nicht / verachte.", "tokens": ["Sonst", "al\u00b7le", "Lab\u00b7nu\u00df", "ich", "/", "nur", "dei\u00b7ne", "nicht", "/", "ver\u00b7ach\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct", "word",