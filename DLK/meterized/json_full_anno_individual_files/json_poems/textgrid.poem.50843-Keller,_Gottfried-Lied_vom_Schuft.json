": ["KOUS"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Wenn ihn ein rein, einf\u00e4ltig Herz", "tokens": ["Wenn", "ihn", "ein", "rein", ",", "ein\u00b7f\u00e4l\u00b7tig", "Herz"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJD", "$,", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit gro\u00dfen, klaren Augen mi\u00dft.", "tokens": ["Mit", "gro\u00b7\u00dfen", ",", "kla\u00b7ren", "Au\u00b7gen", "mi\u00dft", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Er geht einher im Silberhaar", "tokens": ["Er", "geht", "ein\u00b7her", "im", "Sil\u00b7ber\u00b7haar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und keimt schon in des Knaben Blick,", "tokens": ["Und", "keimt", "schon", "in", "des", "Kna\u00b7ben", "Blick", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er bleibt sich auch im Kot getreu", "tokens": ["Er", "bleibt", "sich", "auch", "im", "Kot", "ge\u00b7treu"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "APPRART", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und sonnet sich im hellsten Gl\u00fcck.", "tokens": ["Und", "son\u00b7net", "sich", "im", "hells\u00b7ten", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "st