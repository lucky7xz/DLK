{"textgrid.poem.36726": {"metadata": {"author": {"name": "Waiblinger, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "5. Stanzen", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nenn' ich euch wohl den Tempel der Kunst? So erscheint die Geschichte:", "tokens": ["Nenn'", "ich", "euch", "wohl", "den", "Tem\u00b7pel", "der", "Kunst", "?", "So", "er\u00b7scheint", "die", "Ge\u00b7schich\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "ART", "NN", "ART", "NN", "$.", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Meinen Tempel hab' ich, spricht sie, hierin mir erbaut.", "tokens": ["Mei\u00b7nen", "Tem\u00b7pel", "hab'", "ich", ",", "spricht", "sie", ",", "hie\u00b7rin", "mir", "er\u00b7baut", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "$,", "VVFIN", "PPER", "$,", "ADV", "PPER", "VVPP", "$."], "meter": "+-+-+-+-++--+", "measure": "iambic.septa.chol"}, "line.3": {"text": "Aber die Philosophie er\u00f6ffnet die Schule der Weisheit,", "tokens": ["A\u00b7ber", "die", "Phi\u00b7lo\u00b7so\u00b7phie", "er\u00b7\u00f6ff\u00b7net", "die", "Schu\u00b7le", "der", "Weis\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+---+-+-+--+--+-", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Zeigt mit erhabenem Stolz ihre Gewaltigen vor.", "tokens": ["Zeigt", "mit", "er\u00b7ha\u00b7be\u00b7nem", "Stolz", "ih\u00b7re", "Ge\u00b7wal\u00b7ti\u00b7gen", "vor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Zeig' ich Apoll' euch nicht und die Musen im Chore der Dichter,", "tokens": ["Zeig'", "ich", "A\u00b7poll'", "euch", "nicht", "und", "die", "Mu\u00b7sen", "im", "Cho\u00b7re", "der", "Dich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "NE", "PPER", "PTKNEG", "KON", "ART", "NN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.6": {"text": "Spricht die Dichtkunst, ist nicht mein hier der gr\u00f6\u00dfte Triumph?", "tokens": ["Spricht", "die", "Dicht\u00b7kunst", ",", "ist", "nicht", "mein", "hier", "der", "gr\u00f6\u00df\u00b7te", "Tri\u00b7umph", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "VAFIN", "PTKNEG", "PPOSAT", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+---", "measure": "dactylic.tetra.plus"}, "line.7": {"text": "Nein, antwortet die Religion, mein tiefstes Geheimni\u00df", "tokens": ["Nein", ",", "ant\u00b7wor\u00b7tet", "die", "Re\u00b7li\u00b7gi\u00b7on", ",", "mein", "tiefs\u00b7tes", "Ge\u00b7heim\u00b7ni\u00df"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$,", "VVFIN", "ART", "NN", "$,", "PPOSAT", "ADJA", "NN"], "meter": "-+---+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Und mein Heiligthum ist hier euch vors Auge gestellt.", "tokens": ["Und", "mein", "Hei\u00b7lig\u00b7thum", "ist", "hier", "euch", "vors", "Au\u00b7ge", "ge\u00b7stellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADV", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "--+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Oeffn' ich den Himmel euch nicht, und zeig' euch den Vater im Glanze", "tokens": ["Oeffn'", "ich", "den", "Him\u00b7mel", "euch", "nicht", ",", "und", "zeig'", "euch", "den", "Va\u00b7ter", "im", "Glan\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ART", "NN", "PPER", "PTKNEG", "$,", "KON", "VVFIN", "PPER", "ART", "NN", "APPRART", "NN"], "meter": "+--+-++-+--+--+-", "measure": "iambic.septa.invert"}, "line.10": {"text": "Seines Thrones, den Sohn nicht und den heiligen Geist?", "tokens": ["Sei\u00b7nes", "Thro\u00b7nes", ",", "den", "Sohn", "nicht", "und", "den", "hei\u00b7li\u00b7gen", "Geist", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ART", "NN", "PTKNEG", "KON", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.11": {"text": "Unser ist dieser Raum, will die Kirche, was hier wir und dr\u00fcben", "tokens": ["Un\u00b7ser", "ist", "die\u00b7ser", "Raum", ",", "will", "die", "Kir\u00b7che", ",", "was", "hier", "wir", "und", "dr\u00fc\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "PDAT", "NN", "$,", "VMFIN", "ART", "NN", "$,", "PRELS", "ADV", "PPER", "KON", "ADV"], "meter": "+--+-+--+-+-+-+-", "measure": "iambic.septa.invert"}, "line.12": {"text": "L\u00f6sen und binden, du siehst's, hier ist mein m\u00e4chtigstes Reich.", "tokens": ["L\u00f6\u00b7sen", "und", "bin\u00b7den", ",", "du", "siehst's", ",", "hier", "ist", "mein", "m\u00e4ch\u00b7tigs\u00b7tes", "Reich", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "w