{"textgrid.poem.60646": {"metadata": {"author": {"name": "La Fontaine, Jean de", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sein Laster hat jeder und gibt es nicht auf,", "genre": "verse", "period": "N.A.", "pub_year": 1658, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sein Laster hat jeder und gibt es nicht auf,", "tokens": ["Sein", "Las\u00b7ter", "hat", "je\u00b7der", "und", "gibt", "es", "nicht", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PIAT", "KON", "VVFIN", "PPER", "PTKNEG", "PTKVZ", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Kein Mittel kann je ihn befreien,", "tokens": ["Kein", "Mit\u00b7tel", "kann", "je", "ihn", "be\u00b7frei\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "ADV", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Besch\u00e4mung und Furcht nimmt er gerne in Kauf,", "tokens": ["Be\u00b7sch\u00e4\u00b7mung", "und", "Furcht", "nimmt", "er", "ger\u00b7ne", "in", "Kauf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "PPER", "ADV", "APPR", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Selbst Verh\u00f6hnung und Teufeleien.", "tokens": ["Selbst", "Ver\u00b7h\u00f6h\u00b7nung", "und", "Teu\u00b7fe\u00b7lei\u00b7en", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NE", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Und weil man nicht gern auf ein Beispiel verzichtet,", "tokens": ["Und", "weil", "man", "nicht", "gern", "auf", "ein", "Bei\u00b7spiel", "ver\u00b7zich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "PTKNEG", "ADV", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.6": {"text": "So sei euch ein solches berichtet.", "tokens": ["So", "sei", "euch", "ein", "sol\u00b7ches", "be\u00b7rich\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "PIS", "VVFIN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "Ein Trunkenbold liebte das Trinken so sehr,", "tokens": ["Ein", "Trun\u00b7ken\u00b7bold", "lieb\u00b7te", "das", "Trin\u00b7ken", "so", "sehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "ADV", "ADV", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Da\u00df er Geist und Gesundheit vernichtet;", "tokens": ["Da\u00df", "er", "Geist", "und", "Ge\u00b7sund\u00b7heit", "ver\u00b7nich\u00b7tet", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "KON", "NN", "VVPP", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Nat\u00fcrlich war auch seine B\u00f6rse stets leer,", "tokens": ["Na\u00b7t\u00fcr\u00b7lich", "war", "auch", "sei\u00b7ne", "B\u00f6r\u00b7se", "stets", "leer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PPOSAT", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Denn dem Trinker bedeutet der Mammon nicht mehr,", "tokens": ["Denn", "dem", "Trin\u00b7ker", "be\u00b7deu\u00b7tet", "der", "Mam\u00b7mon", "nicht", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "NN", "PTKNEG", "ADV", "$,"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.5": {"text": "Als da\u00df er ihn Bacchus entrichtet,", "tokens": ["Als", "da\u00df", "er", "ihn", "Bac\u00b7chus", "ent\u00b7rich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "PPER", "NE", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Und Bacchus zu opfern, reut niemanden eh\u2019r,", "tokens": ["Und", "Bac\u00b7chus", "zu", "op\u00b7fern", ",", "reut", "nie\u00b7man\u00b7den", "eh'r", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NE", "PTKZU", "VVINF", "$,", "VVFIN", "PIS", "PTKVZ", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Als bis er zugrundegerichtet.", "tokens": ["Als", "bis", "er", "zu\u00b7grun\u00b7de\u00b7ge\u00b7rich\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPER", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.8": {"text": "Als einmal der S\u00e4ufer beim Safte der Reben", "tokens": ["Als", "ein\u00b7mal", "der", "S\u00e4u\u00b7fer", "beim", "Saf\u00b7te", "der", "Re\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "APPRART", "NN", "ART", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.9": {"text": "Den Rest von Verstand dahingegeben,", "tokens": ["Den", "Rest", "von", "Ver\u00b7stand", "da\u00b7hin\u00b7ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "PAV", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Da sperrte sein Weib, um ihn gr\u00fcndlich zu strafen,", "tokens": ["Da", "sperr\u00b7te", "sein", "Weib", ",", "um", "ihn", "gr\u00fcnd\u00b7lich", "zu", "stra\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$,", "KOUI", "PPER", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.11": {"text": "In grausige Gruft den Trunkenbold ein.", "tokens": ["In", "grau\u00b7si\u00b7ge", "Gruft", "den", "Trun\u00b7ken\u00b7bold", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Nachdem er dort seinen Rausch verschlafen", "tokens": ["Nach\u00b7dem", "er", "dort", "sei\u00b7nen", "Rausch", "ver\u00b7schla\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.13": {"text": "Und n\u00fcchtern erwachte, erblickt er den Schein", "tokens": ["Und", "n\u00fcch\u00b7tern", "er\u00b7wach\u00b7te", ",", "er\u00b7blickt", "er", "den", "Schein"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "$,", "VVFIN", "PPER", "ART", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.14": {"text": "Vom Totenl\u00e4mpchen und Totengebein,", "tokens": ["Vom", "To\u00b7ten\u00b7l\u00e4mp\u00b7chen", "und", "To\u00b7ten\u00b7ge\u00b7bein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.15": {"text": "Und ihn selber h\u00fcllte ein Leichentuch ein.", "tokens": ["Und", "ihn", "sel\u00b7ber", "h\u00fcll\u00b7te", "ein", "Lei\u00b7chen\u00b7tuch", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.16": {"text": "\u00bboh,\u00ab spricht er ergriffen, \u00bbwas ist denn geschehn?", "tokens": ["\u00bb", "oh", ",", "\u00ab", "spricht", "er", "er\u00b7grif\u00b7fen", ",", "\u00bb", "was", "ist", "denn", "ge\u00b7schehn", "?"], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "$,", "$(", "VVFIN", "PPER", "VVPP", "$,", "$(", "PWS", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+--+-++--+", "measure": "iambic.penta.relaxed"}, "line.17": {"text": "Soll mein Weib schon so jung als Witwe sich sehn?\u00ab", "tokens": ["Soll", "mein", "Weib", "schon", "so", "jung", "als", "Wit\u00b7we", "sich", "sehn", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "ADV", "ADV", "ADJD", "KOKOM", "NN", "PRF", "VVINF", "$.", "$("], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}, "line.18": {"text": "Da tritt als Furie verkleidet", "tokens": ["Da", "tritt", "als", "Fu\u00b7rie", "ver\u00b7klei\u00b7det"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "KOUS", "NN", "VVPP"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.19": {"text": "Seine Frau ins Gew\u00f6lbe und geht zu ihm hin", "tokens": ["Sei\u00b7ne", "Frau", "ins", "Ge\u00b7w\u00f6l\u00b7be", "und", "geht", "zu", "ihm", "hin"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "KON", "VVFIN", "APPR", "PPER", "PTKVZ"], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.20": {"text": "Und reicht ihm, indem seine Blicke sie meidet,", "tokens": ["Und", "reicht", "ihm", ",", "in\u00b7dem", "sei\u00b7ne", "Bli\u00b7cke", "sie", "mei\u00b7det", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "KOUS", "PPOSAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.21": {"text": "Eine Sch\u00fcssel mit Speise darin.", "tokens": ["Ei\u00b7ne", "Sch\u00fcs\u00b7sel", "mit", "Spei\u00b7se", "da\u00b7rin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "PAV", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.22": {"text": "Nun ist der Gatte durchaus \u00fcberzeugt,", "tokens": ["Nun", "ist", "der", "Gat\u00b7te", "durc\u00b7haus", "\u00fc\u00b7berz\u00b7eugt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Er sei in der H\u00f6lle gefangen.", "tokens": ["Er", "sei", "in", "der", "H\u00f6l\u00b7le", "ge\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.24": {"text": "\u00bbwer bist du?\u00ab so fragt er mit Bangen.", "tokens": ["\u00bb", "wer", "bist", "du", "?", "\u00ab", "so", "fragt", "er", "mit", "Ban\u00b7gen", "."], "token_info": ["punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VAFIN", "PPER", "$.", "$(", "ADV", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.25": {"text": "Und sie, indem sie sich zu ihm beugt:", "tokens": ["Und", "sie", ",", "in\u00b7dem", "sie", "sich", "zu", "ihm", "beugt", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "KOUS", "PPER", "PRF", "APPR", "PPER", "VVFIN", "$."], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.26": {"text": "\u00bbdie Speisemeisterin im Reich", "tokens": ["\u00bb", "die", "Spei\u00b7se\u00b7meis\u00b7te\u00b7rin", "im", "Reich"], "token_info": ["punct", "word", "word", "word", "word"], "pos": ["$(", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Des H\u00f6llenherrn, und ich bringe zu essen", "tokens": ["Des", "H\u00f6l\u00b7len\u00b7herrn", ",", "und", "ich", "brin\u00b7ge", "zu", "es\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "KON", "PPER", "VVFIN", "PTKZU", "VVINF"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.28": {"text": "Den Toten, die b\u00f6ser Schuld sich vermessen,", "tokens": ["Den", "To\u00b7ten", ",", "die", "b\u00f6\u00b7ser", "Schuld", "sich", "ver\u00b7mes\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "PRF", "VVPP", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.29": {"text": "Den Ausgesto\u00dfenen des Lichts \u2013.\u00ab", "tokens": ["Den", "Aus\u00b7ge\u00b7sto\u00b7\u00dfe\u00b7nen", "des", "Lichts", "\u2013", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "NN", "ART", "NN", "$(", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Da f\u00e4llt der Gatte ins Wort ihr gleich:", "tokens": ["Da", "f\u00e4llt", "der", "Gat\u00b7te", "ins", "Wort", "ihr", "gleich", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "NN", "PPER", "ADV", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.31": {"text": "\u00bbund zu trinken bringst du mir nichts?\u00ab", "tokens": ["\u00bb", "und", "zu", "trin\u00b7ken", "bringst", "du", "mir", "nichts", "?", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "KON", "PTKZU", "VVINF", "VVFIN", "PPER", "PPER", "PIS", "$.", "$("], "meter": "--+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Sein Laster hat jeder und gibt es nicht auf,", "tokens": ["Sein", "Las\u00b7ter", "hat", "je\u00b7der", "und", "gibt", "es", "nicht", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PIAT", "KON", "VVFIN", "PPER", "PTKNEG", "PTKVZ", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Kein Mittel kann je ihn befreien,", "tokens": ["Kein", "Mit\u00b7tel", "kann", "je", "ihn", "be\u00b7frei\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "ADV", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Besch\u00e4mung und Furcht nimmt er gerne in Kauf,", "tokens": ["Be\u00b7sch\u00e4\u00b7mung", "und", "Furcht", "nimmt", "er", "ger\u00b7ne", "in", "Kauf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "PPER", "ADV", "APPR", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Selbst Verh\u00f6hnung und Teufeleien.", "tokens": ["Selbst", "Ver\u00b7h\u00f6h\u00b7nung", "und", "Teu\u00b7fe\u00b7lei\u00b7en", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NE", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Und weil man nicht gern auf ein Beispiel verzichtet,", "tokens": ["Und", "weil", "man", "nicht", "gern", "auf", "ein", "Bei\u00b7spiel", "ver\u00b7zich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "w