{"dta.poem.2905": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "106.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Geh auf die Reise, Freund! Der dir das Reisen preist,", "tokens": ["Geh", "auf", "die", "Rei\u00b7se", ",", "Freund", "!", "Der", "dir", "das", "Rei\u00b7sen", "preist", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "$,", "NN", "$.", "ART", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der hat es auch erprobt, der ", "tokens": ["Der", "hat", "es", "auch", "er\u00b7probt", ",", "der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "VVPP", "$,", "PRELS"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Nicht Eine Rose gibts, nicht Einen gr\u00fcnen Baum;", "tokens": ["Nicht", "Ei\u00b7ne", "Ro\u00b7se", "gibts", ",", "nicht", "Ei\u00b7nen", "gr\u00fc\u00b7nen", "Baum", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "VVFIN", "$,", "PTKNEG", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Voll B\u00e4ume steht die Welt, voll Rosen bl\u00fcht der Raum.", "tokens": ["Voll", "B\u00e4u\u00b7me", "steht", "die", "Welt", ",", "voll", "Ro\u00b7sen", "bl\u00fcht", "der", "Raum", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "VVFIN", "ART", "NN", "$,", "ADJD", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Was willst du wie ein Huhn im Hofe K\u00f6rner klauben,", "tokens": ["Was", "willst", "du", "wie", "ein", "Huhn", "im", "Ho\u00b7fe", "K\u00f6r\u00b7ner", "klau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "KOKOM", "ART", "NN", "APPRART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn du dich schwingen kannst frei in die Luft wie Tauben?", "tokens": ["Wenn", "du", "dich", "schwin\u00b7gen", "kannst", "frei", "in", "die", "Luft", "wie", "Tau\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVINF", "VMFIN", "ADJD", "APPR", "ART", "NN", "KOKOM", "NN", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Die Schnecke reist bequem, sie reist mit ihrem Haus,", "tokens": ["Die", "Schne\u00b7cke", "reist", "be\u00b7quem", ",", "sie", "reist", "mit", "ih\u00b7rem", "Haus", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$,", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Daf\u00fcr sieht sie nicht viel, und kommt nicht weit hinaus.", "tokens": ["Da\u00b7f\u00fcr", "sieht", "sie", "nicht", "viel", ",", "und", "kommt", "nicht", "weit", "hin\u00b7aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PTKNEG", "ADV", "$,", "KON", "VVFIN", "PTKNEG", "ADJD", "PTKVZ", "$."], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}}, "stanza.5": {"line.1": {"text": "Gef\u00e4hrten such' ich mir, die etwas mit mir wagen,", "tokens": ["Ge\u00b7f\u00e4hr\u00b7ten", "such'", "ich", "mir", ",", "die", "et\u00b7was", "mit", "mir", "wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PPER", "$,", "PRELS", "PIS", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nicht einen Reisefreund, des B\u00fcndel ich soll tragen.", "tokens": ["Nicht", "ei\u00b7nen", "Rei\u00b7se\u00b7freund", ",", "des", "B\u00fcn\u00b7del", "ich", "soll", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "$,", "ART", "NN", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Der Seele Kraft besteht im Trachten und Betrachten;", "tokens": ["Der", "See\u00b7le", "Kraft", "be\u00b7steht", "im", "Trach\u00b7ten", "und", "Be\u00b7trach\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "APPRART", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Betrachten sollst du viel, doch nicht nach allem trachten.", "tokens": ["Be\u00b7trach\u00b7ten", "sollst", "du", "viel", ",", "doch", "nicht", "nach", "al\u00b7lem", "trach\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "ADV", "$,", "ADV", "PTKNEG", "APPR", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Durcheilst du alles schnell, so wirst du vieles sehn;", "tokens": ["Durch\u00b7eilst", "du", "al\u00b7les", "schnell", ",", "so", "wirst", "du", "vie\u00b7les", "sehn", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIS", "ADJD", "$,", "ADV", "VAFIN", "PPER", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das Eine siehst du recht, bleibst du beim Einen stehn.", "tokens": ["Das", "Ei\u00b7ne", "siehst", "du", "recht", ",", "bleibst", "du", "beim", "Ei\u00b7nen", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "VVFIN", "PPER", "ADJD", "$,", "VVFIN", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Ein kluger Wandersmann ruht aus am Scheidewege;", "tokens": ["Ein", "klu\u00b7ger", "Wan\u00b7ders\u00b7mann", "ruht", "aus", "am", "Schei\u00b7de\u00b7we\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "APPRART", "NN", "$."], "meter": "-+-+-+++-+-+-", "measure": "unknown.measure.septa"}, "line.2": {"text": "Da ruh' ich nicht umsonst, indes ich \u00fcberlege.", "tokens": ["Da", "ruh'", "ich", "nicht", "um\u00b7sonst", ",", "in\u00b7des", "ich", "\u00fc\u00b7berl\u00b7e\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "ADV", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Viel besser aber ists auf gut Gl\u00fcck irre gehn,", "tokens": ["Viel", "bes\u00b7ser", "a\u00b7ber", "ists", "auf", "gut", "Gl\u00fcck", "ir\u00b7re", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "VAFIN", "APPR", "ADJD", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als bis zum Untergang der Sonn' am Scheidweg stehn.", "tokens": ["Als", "bis", "zum", "Un\u00b7ter\u00b7gang", "der", "Sonn'", "am", "Scheid\u00b7weg", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "APPRART", "NN", "ART", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Ich habe viel geirrt, ich hab' auch viel getroffen", "tokens": ["Ich", "ha\u00b7be", "viel", "ge\u00b7irrt", ",", "ich", "hab'", "auch", "viel", "ge\u00b7trof\u00b7fen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "VVPP", "$,", "PPER", "VAFIN", "ADV", "ADV", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Beim Irren, was nicht war auf gradem Weg zu hoffen.", "tokens": ["Beim", "Ir\u00b7ren", ",", "was", "nicht", "war", "auf", "gra\u00b7dem", "Weg", "zu", "hof\u00b7fen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "PTKNEG", "VAFIN", "APPR", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.11": {"line.1": {"text": "Ich sehs, da\u00df ich gefehlt; was hilft, da\u00df es mich reute?", "tokens": ["Ich", "sehs", ",", "da\u00df", "ich", "ge\u00b7fehlt", ";", "was", "hilft", ",", "da\u00df", "es", "mich", "reu\u00b7te", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "VVPP", "$.", "PWS", "VVFIN", "$,", "KOUS", "PPER", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das Gestern fra\u00df der Fehl, soll fressen Reu das Heute?", "tokens": ["Das", "Ge\u00b7stern", "fra\u00df", "der", "Fehl", ",", "soll", "fres\u00b7sen", "Reu", "das", "Heu\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,", "VMFIN", "VVINF", "NE", "ART", "NN", "$."], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}}, "stanza.12": {"line.1": {"text": "Mach' es sogut du kannst; und hast du's schlecht gemacht,", "tokens": ["Mach'", "es", "so\u00b7gut", "du", "kannst", ";", "und", "hast", "du's", "schlecht", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "PPER", "VMFIN", "$.", "KON", "VAFIN", "PIS", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So preis' in Demuth Gott, der Alles recht gemacht.", "tokens": ["So", "preis'", "in", "De\u00b7muth", "Gott", ",", "der", "Al\u00b7les", "recht", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "NN", "$,", "PRELS", "PIS", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}