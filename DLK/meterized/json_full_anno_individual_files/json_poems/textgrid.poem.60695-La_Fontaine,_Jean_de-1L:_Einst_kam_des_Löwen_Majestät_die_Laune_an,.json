{"textgrid.poem.60695": {"metadata": {"author": {"name": "La Fontaine, Jean de", "birth": "N.A.", "death": "N.A."}, "title": "1L: Einst kam des L\u00f6wen Majest\u00e4t die Laune an,", "genre": "verse", "period": "N.A.", "pub_year": 1658, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Einst kam des L\u00f6wen Majest\u00e4t die Laune an,", "tokens": ["Einst", "kam", "des", "L\u00f6\u00b7wen", "Ma\u00b7jes\u00b7t\u00e4t", "die", "Lau\u00b7ne", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das Volk sich zu beschauen, das ihm untertan,", "tokens": ["Das", "Volk", "sich", "zu", "be\u00b7schau\u00b7en", ",", "das", "ihm", "un\u00b7ter\u00b7tan", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "PTKZU", "VVINF", "$,", "PRELS", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und also lie\u00df er schnell nach allen", "tokens": ["Und", "al\u00b7so", "lie\u00df", "er", "schnell", "nach", "al\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADJD", "APPR", "PIAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gebieten seines Reiches hin", "tokens": ["Ge\u00b7bie\u00b7ten", "sei\u00b7nes", "Rei\u00b7ches", "hin"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Durch seine treuesten Vasallen", "tokens": ["Durch", "sei\u00b7ne", "treu\u00b7es\u00b7ten", "Va\u00b7sal\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+---+-", "measure": "unknown.measure.tri"}, "line.6": {"text": "Ein Siegelschreiben bringen, drin", "tokens": ["Ein", "Sie\u00b7gel\u00b7schrei\u00b7ben", "brin\u00b7gen", ",", "drin"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "VVINF", "$,", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ein jeder ward zu Hof geladen,", "tokens": ["Ein", "je\u00b7der", "ward", "zu", "Hof", "ge\u00b7la\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VAFIN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Wo Majest\u00e4t von Gottes Gnaden", "tokens": ["Wo", "Ma\u00b7jes\u00b7t\u00e4t", "von", "Got\u00b7tes", "Gna\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "APPR", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Gro\u00dfe Gesellschaft geben wolle", "tokens": ["Gro\u00b7\u00dfe", "Ge\u00b7sell\u00b7schaft", "ge\u00b7ben", "wol\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVINF", "VMFIN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.10": {"text": "Mit einem Fest voll Lust und Pracht,", "tokens": ["Mit", "ei\u00b7nem", "Fest", "voll", "Lust", "und", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Das einen Monat dauern solle.", "tokens": ["Das", "ei\u00b7nen", "Mo\u00b7nat", "dau\u00b7ern", "sol\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Es wollte so des Thrones Macht", "tokens": ["Es", "woll\u00b7te", "so", "des", "Thro\u00b7nes", "Macht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Der F\u00fcrst einmal beweisen seinem Volk und Trosse.", "tokens": ["Der", "F\u00fcrst", "ein\u00b7mal", "be\u00b7wei\u00b7sen", "sei\u00b7nem", "Volk", "und", "Tros\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und alle kamen sie zum Schlosse.", "tokens": ["Und", "al\u00b7le", "ka\u00b7men", "sie", "zum", "Schlos\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Doch welch ein Schlo\u00df! \u2013 Ein Schlachthaus war's voll Fleischgestank.", "tokens": ["Doch", "welch", "ein", "Schlo\u00df", "!", "\u2013", "Ein", "Schlacht\u00b7haus", "wa\u00b7r's", "voll", "Fleischge\u00b7stank", "."], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAT", "ART", "NN", "$.", "$(", "ART", "NN", "VAFIN", "ADJD", "NN", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.16": {"text": "Es wurde manches Tier von dieser Luft fast krank.", "tokens": ["Es", "wur\u00b7de", "man\u00b7ches", "Tier", "von", "die\u00b7ser", "Luft", "fast", "krank", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "APPR", "PDAT", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Der B\u00e4r hielt sich die Nase zu. Was tat er da?!", "tokens": ["Der", "B\u00e4r", "hielt", "sich", "die", "Na\u00b7se", "zu", ".", "Was", "tat", "er", "da", "?!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ART", "NN", "PTKVZ", "$.", "PWS", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Der zornige Monarch, der die Grimasse sah,", "tokens": ["Der", "zor\u00b7ni\u00b7ge", "Mon\u00b7arch", ",", "der", "die", "Gri\u00b7mas\u00b7se", "sah", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Der sandte diesen Heiklen gleich zu Pluto hin.", "tokens": ["Der", "sand\u00b7te", "die\u00b7sen", "Heik\u00b7len", "gleich", "zu", "Plu\u00b7to", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PDAT", "NN", "ADV", "APPR", "NE", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Der Kriecher Affe pries des Herrschers strengen Sinn", "tokens": ["Der", "Krie\u00b7cher", "Af\u00b7fe", "pries", "des", "Herr\u00b7schers", "stren\u00b7gen", "Sinn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Und seine Wut und Kraft, und schmeichelnd r\u00fchmte er", "tokens": ["Und", "sei\u00b7ne", "Wut", "und", "Kraft", ",", "und", "schmei\u00b7chelnd", "r\u00fchm\u00b7te", "er"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "KON", "NN", "$,", "KON", "ADJD", "VVFIN", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Der M\u00f6rdergrube s\u00fc\u00dfe Luft,", "tokens": ["Der", "M\u00f6r\u00b7der\u00b7gru\u00b7be", "s\u00fc\u00b7\u00dfe", "Luft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Mit der verglichen Ambraduft und Blumenduft", "tokens": ["Mit", "der", "ver\u00b7gli\u00b7chen", "Am\u00b7bra\u00b7duft", "und", "Blu\u00b7men\u00b7duft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Nur wie Geruch von Knoblauch w\u00e4r.", "tokens": ["Nur", "wie", "Ge\u00b7ruch", "von", "Knob\u00b7lauch", "w\u00e4r", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "NN", "APPR", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Doch Strafe folgte schnell der bl\u00f6den Schmeichelei;", "tokens": ["Doch", "Stra\u00b7fe", "folg\u00b7te", "schnell", "der", "bl\u00f6\u00b7den", "Schmei\u00b7che\u00b7lei", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Denn unser L\u00f6we war sehr nah", "tokens": ["Denn", "un\u00b7ser", "L\u00f6\u00b7we", "war", "sehr", "nah"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NE", "VAFIN", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Verwandt mit dem Caligula.", "tokens": ["Ver\u00b7wandt", "mit", "dem", "Ca\u00b7li\u00b7gu\u00b7la", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NE", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.28": {"text": "Dann winkte der Tyrann den roten Fuchs herbei.", "tokens": ["Dann", "wink\u00b7te", "der", "Ty\u00b7rann", "den", "ro\u00b7ten", "Fuchs", "her\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "\u00bbnun,\u00ab sprach er, \u00bbwas riechst du? Sprich! Sag es ohne Scheu.\u00ab", "tokens": ["\u00bb", "nun", ",", "\u00ab", "sprach", "er", ",", "\u00bb", "was", "riechst", "du", "?", "Sprich", "!", "Sag", "es", "oh\u00b7ne", "Scheu", ".", "\u00ab"], "token_info": ["punct", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "$,", "$(", "VVFIN", "PPER", "$,", "$(", "PWS", "VVFIN", "PPER", "$.", "VVIMP", "$.", "NN", "PPER", "APPR", "NN", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "So starken Schnupfen, da\u00df er dies nur k\u00f6nne sagen:", "tokens": ["So", "star\u00b7ken", "Schnup\u00b7fen", ",", "da\u00df", "er", "dies", "nur", "k\u00f6n\u00b7ne", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,", "KOUS", "PPER", "PDS", "ADV", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Er rieche nichts! \u2013 Der F\u00fcrst gab ihn zufrieden frei.", "tokens": ["Er", "rie\u00b7che", "nichts", "!", "\u2013", "Der", "F\u00fcrst", "gab", "ihn", "zu\u00b7frie\u00b7den", "frei", "."], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "$.", "$(", "ART", "NN", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "So pr\u00e4gt euch diese Lehre ein:", "tokens": ["So", "pr\u00e4gt", "euch", "die\u00b7se", "Leh\u00b7re", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PDAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es kann euch, m\u00f6chtet ihr des Herrschers Gunst besitzen,", "tokens": ["Es", "kann", "euch", ",", "m\u00f6ch\u00b7tet", "ihr", "des", "Herr\u00b7schers", "Gunst", "be\u00b7sit\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "$,", "VMFIN", "PPER", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nicht fades Schmeicheln und nicht offnes Reden n\u00fctzen.", "tokens": ["Nicht", "fa\u00b7des", "Schmei\u00b7cheln", "und", "nicht", "off\u00b7nes", "Re\u00b7den", "n\u00fct\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJA", "NN", "KON", "PTKNEG", "ADJA", "NN", "VVINF", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.4": {"text": "Sagt nach Normannenart manchmal nicht ja, nicht nein!", "tokens": ["Sagt", "nach", "Nor\u00b7man\u00b7nen\u00b7art", "manch\u00b7mal", "nicht", "ja", ",", "nicht", "nein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NE", "ADV", "PTKNEG", "ADV", "$,", "PTKNEG", "PTKANT", "$."], "meter": "--+--+---+-+", "measure": "anapaest.di.plus"}}, "stanza.3": {"line.1": {"text": "Einst kam des L\u00f6wen Majest\u00e4t die Laune an,", "tokens": ["Einst", "kam", "des", "L\u00f6\u00b7wen", "Ma\u00b7jes\u00b7t\u00e4t", "die", "Lau\u00b7ne", "an", ","], "token_info": ["word", "word"