{"textgrid.poem.55701": {"metadata": {"author": {"name": "D\u00f6rmann, Felix", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich habe nur ihr gro\u00dfes Herz gekannt", "tokens": ["Ich", "ha\u00b7be", "nur", "ihr", "gro\u00b7\u00dfes", "Herz", "ge\u00b7kannt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "PPOSAT", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und ihres teuren Leibes Paradies. \u2013", "tokens": ["Und", "ih\u00b7res", "teu\u00b7ren", "Lei\u00b7bes", "Pa\u00b7ra\u00b7dies", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nicht wei\u00df ich, wer sie war und wie sie hie\u00df,", "tokens": ["Nicht", "wei\u00df", "ich", ",", "wer", "sie", "war", "und", "wie", "sie", "hie\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "PPER", "$,", "PWS", "PPER", "VAFIN", "KON", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Denn ihren Namen hat sie nie genannt.", "tokens": ["Denn", "ih\u00b7ren", "Na\u00b7men", "hat", "sie", "nie", "ge\u00b7nannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Doch auch den meinen wies sie stolz zur\u00fcck:", "tokens": ["Doch", "auch", "den", "mei\u00b7nen", "wies", "sie", "stolz", "zu\u00b7r\u00fcck", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "PPOSAT", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "\u00bbich brauch' ihn nicht! \u2013 In meiner Seele lebt", "tokens": ["\u00bb", "ich", "brauch'", "ihn", "nicht", "!", "\u2013", "In", "mei\u00b7ner", "See\u00b7le", "lebt"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VVFIN", "PPER", "PTKNEG", "$.", "$(", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "F\u00fcr alle Zeit das namenlose Gl\u00fcck,", "tokens": ["F\u00fcr", "al\u00b7le", "Zeit", "das", "na\u00b7men\u00b7lo\u00b7se", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Mit der Erinnerung an Dich verwebt.\u00ab \u2013", "tokens": ["Mit", "der", "E\u00b7rin\u00b7ne\u00b7rung", "an", "Dich", "ver\u00b7webt", ".", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "PPER", "VVPP", "$.", "$(", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Du bist ihr gleich, Du br\u00e4unlich blasses Kind. \u2013", "tokens": ["Du", "bist", "ihr", "gleich", ",", "Du", "br\u00e4un\u00b7lich", "blas\u00b7ses", "Kind", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "$,", "PPER", "ADJD", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Dein tiefgelegnes, dunkles Auge rief", "tokens": ["Dein", "tief\u00b7ge\u00b7leg\u00b7nes", ",", "dunk\u00b7les", "Au\u00b7ge", "rief"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "$,", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Vergangnes jach empor. \u2013 Ein Wirbelwind", "tokens": ["Ver\u00b7gang\u00b7nes", "jach", "em\u00b7por", ".", "\u2013", "Ein", "Wir\u00b7bel\u00b7wind"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word"], "pos": ["NN", "ADJD", "PTKVZ", "$.", "$(", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "W\u00fchlt Alles auf, was tr\u00e4nenm\u00fcde schlief.", "tokens": ["W\u00fchlt", "Al\u00b7les", "auf", ",", "was", "tr\u00e4\u00b7nen\u00b7m\u00fc\u00b7de", "schlief", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PTKVZ", "$,", "PRELS", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und wieder flutet um das teure Bild", "tokens": ["Und", "wie\u00b7der", "flu\u00b7tet", "um", "das", "teu\u00b7re", "Bild"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Der s\u00fc\u00dfesten Erinnerungen Meer,", "tokens": ["Der", "s\u00fc\u00b7\u00dfes\u00b7ten", "E\u00b7rin\u00b7ne\u00b7run\u00b7gen", "Meer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und aus der Seele, sto\u00dfend, dumpf und schwer,", "tokens": ["Und", "aus", "der", "See\u00b7le", ",", "sto\u00b7\u00dfend", ",", "dumpf", "und", "schwer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "ADJD", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}