{"dta.poem.8413": {"metadata": {"author": {"name": "H\u00f6lty, Ludwig Christoph Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Trinklied im Mai.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1783", "urn": "urn:nbn:de:kobv:b4-200905192568", "language": ["de:0.99"], "booktitle": "H\u00f6lty, Ludwig Christoph Heinrich: Gedichte. Hamburg, 1783."}, "poem": {"stanza.1": {"line.1": {"text": "Und zapfet mir Wein;", "tokens": ["Und", "zap\u00b7fet", "mir", "Wein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Der Mai ist begonnen,", "tokens": ["Der", "Mai", "ist", "be\u00b7gon\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Wir m\u00fcssen uns freun!", "tokens": ["Wir", "m\u00fcs\u00b7sen", "uns", "freun", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Die Winde verstummen,", "tokens": ["Die", "Win\u00b7de", "ver\u00b7stum\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Und athmen noch kaum;", "tokens": ["Und", "ath\u00b7men", "noch", "kaum", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Die Bienlein umsummen", "tokens": ["Die", "Bien\u00b7lein", "um\u00b7sum\u00b7men"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Den bl\u00fchenden Baum.", "tokens": ["Den", "bl\u00fc\u00b7hen\u00b7den", "Baum", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Die Nachtigall fl\u00f6tet", "tokens": ["Die", "Nach\u00b7ti\u00b7gall", "fl\u00f6\u00b7tet"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Im gr\u00fcnen Geb\u00fcsch;", "tokens": ["Im", "gr\u00fc\u00b7nen", "Ge\u00b7b\u00fcsch", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Das Abendlicht r\u00f6thet", "tokens": ["Das", "A\u00b7bend\u00b7licht", "r\u00f6\u00b7thet"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Uns Gl\u00e4ser und Tisch.", "tokens": ["Uns", "Gl\u00e4\u00b7ser", "und", "Tisch", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "KON", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Bekr\u00e4nzet die Tonnen,", "tokens": ["Be\u00b7kr\u00e4n\u00b7zet", "die", "Ton\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Und zapfet mir Wein;", "tokens": ["Und", "zap\u00b7fet", "mir", "Wein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Der Mai ist begonnen,", "tokens": ["Der", "Mai", "ist", "be\u00b7gon\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Wir m\u00fcssen uns freun!", "tokens": ["Wir", "m\u00fcs\u00b7sen", "uns", "freun", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Zum Mahle, zum Mahle,", "tokens": ["Zum", "Mah\u00b7le", ",", "zum", "Mah\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPRART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Die Flaschen herbei!", "tokens": ["Die", "Fla\u00b7schen", "her\u00b7bei", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Zween volle Pokale", "tokens": ["Zween", "vol\u00b7le", "Po\u00b7ka\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Geb\u00fchren dem Mai!", "tokens": ["Ge\u00b7b\u00fch\u00b7ren", "dem", "Mai", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Er tr\u00e4uft auf die Bl\u00fcten", "tokens": ["Er", "tr\u00e4uft", "auf", "die", "Bl\u00fc\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Sein Roth und sein Weiss;", "tokens": ["Sein", "Roth", "und", "sein", "Weiss", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Die V\u00f6gelein br\u00fcten", "tokens": ["Die", "V\u00f6\u00b7ge\u00b7lein", "br\u00fc\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Im Schatten des Mais.", "tokens": ["Im", "Schat\u00b7ten", "des", "Mais", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "Er schenket dem Haine", "tokens": ["Er", "schen\u00b7ket", "dem", "Hai\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Verliebten Gesang,", "tokens": ["Ver\u00b7lieb\u00b7ten", "Ge\u00b7sang", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Und Gl\u00e4sern beim Weine", "tokens": ["Und", "Gl\u00e4\u00b7sern", "beim", "Wei\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "APPRART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Melodischen Klang;", "tokens": ["Me\u00b7lo\u00b7di\u00b7schen", "Klang", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Giebt M\u00e4dchen und Knaben", "tokens": ["Giebt", "M\u00e4d\u00b7chen", "und", "Kna\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "NN", "KON", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Ein Minnegef\u00fchl,", "tokens": ["Ein", "Min\u00b7ne\u00b7ge\u00b7f\u00fchl", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Und herliche Gaben", "tokens": ["Und", "her\u00b7li\u00b7che", "Ga\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Zum Kuss und zum Spiel.", "tokens": ["Zum", "Kuss", "und", "zum", "Spiel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "APPRART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.5": {"line.1": {"text": "Ihr J\u00fcngling', ihr Sch\u00f6nen,", "tokens": ["Ihr", "J\u00fcng\u00b7ling'", ",", "ihr", "Sch\u00f6\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Gebt Dank ihm und Preis!", "tokens": ["Gebt", "Dank", "ihm", "und", "Preis", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "PPER", "KON", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Lasst Gl\u00e4ser ert\u00f6nen", "tokens": ["Lasst", "Gl\u00e4\u00b7ser", "er\u00b7t\u00f6\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "NN", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Zur Ehre des Mais!", "tokens": ["Zur", "Eh\u00b7re", "des", "Mais", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Es gr\u00fcne die Laube,", "tokens": ["Es", "gr\u00fc\u00b7ne", "die", "Lau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Die K\u00fcsse verschliesst!", "tokens": ["Die", "K\u00fcs\u00b7se", "ver\u00b7schliesst", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Es wachse die Traube,", "tokens": ["Es", "wach\u00b7se", "die", "Trau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Der Nektar entfliesst!", "tokens": ["Der", "Nek\u00b7tar", "ent\u00b7fliesst", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.6": {"line.1": {"text": "Es bl\u00fche der Rasen,", "tokens": ["Es", "bl\u00fc\u00b7he", "der", "Ra\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Wo Liebende gehn,", "tokens": ["Wo", "Lie\u00b7ben\u00b7de", "gehn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVINF", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Wo Tanten und Basen", "tokens": ["Wo", "Tan\u00b7ten", "und", "Ba\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "NN", "KON", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Die K\u00fcsse nicht sehn!", "tokens": ["Die", "K\u00fcs\u00b7se", "nicht", "sehn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Ihr lachenden L\u00fcfte,", "tokens": ["Ihr", "la\u00b7chen\u00b7den", "L\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Bleibt heiter und hell!", "tokens": ["Bleibt", "hei\u00b7ter", "und", "hell", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Ihr Bl\u00fcten voll D\u00fcfte,", "tokens": ["Ihr", "Bl\u00fc\u00b7ten", "voll", "D\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Verweht nicht so schnell!", "tokens": ["Ver\u00b7weht", "nicht", "so", "schnell", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ADV", "ADJD", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}