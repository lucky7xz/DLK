{"dta.poem.21423": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Caput XXV.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-30602-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein feierlicher Moment! Ich war", "tokens": ["Ein", "fei\u00b7er\u00b7li\u00b7cher", "Mo\u00b7ment", "!", "Ich", "war"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "PPER", "VAFIN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wie angeweht vom Hauche", "tokens": ["Wie", "an\u00b7ge\u00b7weht", "vom", "Hau\u00b7che"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Vorzeit, als ich schwur den Eid,", "tokens": ["Der", "Vor\u00b7zeit", ",", "als", "ich", "schwur", "den", "Eid", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nach uraltem Erzv\u00e4terbrauche.", "tokens": ["Nach", "ur\u00b7al\u00b7tem", "Erz\u00b7v\u00e4\u00b7ter\u00b7brau\u00b7che", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Ich hob das Gewand der G\u00f6ttin auf,", "tokens": ["Ich", "hob", "das", "Ge\u00b7wand", "der", "G\u00f6t\u00b7tin", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und legte an ihre H\u00fcften", "tokens": ["Und", "leg\u00b7te", "an", "ih\u00b7re", "H\u00fcf\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die Hand, gelobend Verschwiegenheit", "tokens": ["Die", "Hand", ",", "ge\u00b7lo\u00b7bend", "Ver\u00b7schwie\u00b7gen\u00b7heit"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "VVPP", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "In Reden und in Schriften.", "tokens": ["In", "Re\u00b7den", "und", "in", "Schrif\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}