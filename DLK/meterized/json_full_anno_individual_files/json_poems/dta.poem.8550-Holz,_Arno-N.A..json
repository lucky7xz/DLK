{"dta.poem.8550": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1899", "urn": "urn:nbn:de:kobv:b4-200905192690", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Drei Tage lang", "tokens": ["Drei", "Ta\u00b7ge", "lang"], "token_info": ["word", "word", "word"], "pos": ["CARD", "NN", "ADJD"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "fiel in den Fluss Fu ein Regen von Pfirsichbl\u00fcten.", "tokens": ["fiel", "in", "den", "Fluss", "Fu", "ein", "Re\u00b7gen", "von", "Pfir\u00b7sich\u00b7bl\u00fc\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "NE", "ART", "NN", "APPR", "NN", "$."], "meter": "+--++-+--+-+-", "measure": "trochaic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Aus ihren gelben Seidengew\u00e4ndern tauchten die M\u00e4dchen und sangen.", "tokens": ["Aus", "ih\u00b7ren", "gel\u00b7ben", "Sei\u00b7den\u00b7ge\u00b7w\u00e4n\u00b7dern", "tauch\u00b7ten", "die", "M\u00e4d\u00b7chen", "und", "san\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "ART", "NN", "KON", "VVFIN", "$."], "meter": "-+-+-+--+-+--+--+-", "measure": "iambic.septa.relaxed"}}, "stanza.3": {"line.1": {"text": "Sie wateten ins Wasser, spritzten, kreischten", "tokens": ["Sie", "wa\u00b7te\u00b7ten", "ins", "Was\u00b7ser", ",", "spritz\u00b7ten", ",", "kreischten"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,", "VVFIN", "$,", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "und kitzelten die Schw\u00e4ne.", "tokens": ["und", "kit\u00b7zel\u00b7ten", "die", "Schw\u00e4\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$."], "meter": "-+---+-", "measure": "dactylic.init"}}, "stanza.4": {"line.1": {"text": "Die Sch\u00f6nste,", "tokens": ["Die", "Sch\u00f6ns\u00b7te", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "l\u00e4chelnd,", "tokens": ["l\u00e4\u00b7chelnd", ","], "token_info": ["word", "punct"], "pos": ["ADJD", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "beide Arme unterm Kopf,", "tokens": ["bei\u00b7de", "Ar\u00b7me", "un\u00b7term", "Kopf", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "liess sich von der Str\u00f6mung treiben.", "tokens": ["liess", "sich", "von", "der", "Str\u00f6\u00b7mung", "trei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Rot,", "tokens": ["Rot", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "wie ein Flammenmantel,", "tokens": ["wie", "ein", "Flam\u00b7men\u00b7man\u00b7tel", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "floss um sie ihr Haar,", "tokens": ["floss", "um", "sie", "ihr", "Haar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "zwei kleine Tr\u00f6pfchen perlten noch auf ihren Br\u00fcsten.", "tokens": ["zwei", "klei\u00b7ne", "Tr\u00f6pf\u00b7chen", "perl\u00b7ten", "noch", "auf", "ih\u00b7ren", "Br\u00fcs\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Leda lag nicht nackter.", "tokens": ["Le\u00b7da", "lag", "nicht", "nack\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKNEG", "PTKVZ", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "Die bunten Wellen schaukelten sie an meine schwimmende Insel.", "tokens": ["Die", "bun\u00b7ten", "Wel\u00b7len", "schau\u00b7kel\u00b7ten", "sie", "an", "mei\u00b7ne", "schwim\u00b7men\u00b7de", "In\u00b7sel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+---+--+--+-", "measure": "iambic.hexa.relaxed"}}, "stanza.8": {"line.1": {"text": "Oh!", "tokens": ["Oh", "!"], "token_info": ["word", "punct"], "pos": ["ITJ", "$."], "meter": "-", "measure": "single.down"}}, "stanza.9": {"line.1": {"text": "Ein schwarzes Bocksgestell, ein Eselsbauch, ein altes, dickbehaartes Vieh mit", "tokens": ["Ein", "schwar\u00b7zes", "Bocks\u00b7ge\u00b7stell", ",", "ein", "E\u00b7sels\u00b7bauch", ",", "ein", "al\u00b7tes", ",", "dick\u00b7be\u00b7haar\u00b7tes", "Vieh", "mit"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "$,", "ART", "ADJA", "$,", "ADJA", "NN", "APPR"], "meter": "-+-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.2": {"text": "H\u00f6rnern!", "tokens": ["H\u00f6r\u00b7nern", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.10": {"line.1": {"text": "Ihre langen Wimpern schlossen sich,", "tokens": ["Ih\u00b7re", "lan\u00b7gen", "Wim\u00b7pern", "schlos\u00b7sen", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PRF", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "um ihre weissen, zitternden Kniee dr\u00e4ngten sich, wankten", "tokens": ["um", "ih\u00b7re", "weis\u00b7sen", ",", "zit\u00b7tern\u00b7den", "Kni\u00b7ee", "dr\u00e4ng\u00b7ten", "sich", ",", "wank\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["KOUI", "PPOSAT", "ADJA", "$,", "ADJA", "NN", "VVFIN", "PRF", "$,", "VVFIN"], "meter": "-+-+-+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Narzissen . . .", "tokens": ["Nar\u00b7zis\u00b7sen", ".", ".", "."], "token_info": ["word", "punct", "punct", "punct"], "pos": ["NN", "$.", "$.", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.11": {"line.1": {"text": "Sie schlug die Augen auf. Ich liess sie nicht. Sie bettelte: Nicht kitzeln,", "tokens": ["Sie", "schlug", "die", "Au\u00b7gen", "auf", ".", "Ich", "liess", "sie", "nicht", ".", "Sie", "bet\u00b7tel\u00b7te", ":", "Nicht", "kit\u00b7zeln", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "$.", "PPER", "VVFIN", "PPER", "PTKNEG", "$.", "PPER", "VVFIN", "$.", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.2": {"text": "nein?", "tokens": ["nein", "?"], "token_info": ["word", "punct"], "pos": ["PTKANT", "$."], "meter": "+", "measure": "single.up"}}, "stanza.12": {"line.1": {"text": "S\u00fcsser!", "tokens": ["S\u00fcs\u00b7ser", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.13": {"line.1": {"text": "Zehn zarte, rosenrote Finger", "tokens": ["Zehn", "zar\u00b7te", ",", "ro\u00b7sen\u00b7ro\u00b7te", "Fin\u00b7ger"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["CARD", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "krallten", "tokens": ["krall\u00b7ten"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "verliebt in meinen Zottelpelz.", "tokens": ["ver\u00b7liebt", "in", "mei\u00b7nen", "Zot\u00b7tel\u00b7pelz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "Au! Racker! Du beisst ja! Ist das der Dank?", "tokens": ["Au", "!", "Ra\u00b7cker", "!", "Du", "beisst", "ja", "!", "Ist", "das", "der", "Dank", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "NN", "$.", "PPER", "VVFIN", "ADV", "$.", "VAFIN", "PDS", "ART", "NN", "$."], "meter": "-+-+---+-+", "measure": "zehnsilber"}}, "stanza.15": {"line.1": {"text": "Sie kicherte.", "tokens": ["Sie", "ki\u00b7cher\u00b7te", "."], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVFIN", "$."], "meter": "-+--", "measure": "dactylic.init"}}, "stanza.16": {"line.1": {"text": "Mein Bett aus Moos missfiel ihr nicht,", "tokens": ["Mein", "Bett", "aus", "Moos", "miss\u00b7fiel", "ihr", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "mein langer Bocksbart imponirte ihr,", "tokens": ["mein", "lan\u00b7ger", "Bocks\u00b7bart", "im\u00b7po\u00b7nir\u00b7te", "ihr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "die Temperatur, auch nachts, ist bereits ganz vorz\u00fcglich,", "tokens": ["die", "Tem\u00b7pe\u00b7ra\u00b7tur", ",", "auch", "nachts", ",", "ist", "be\u00b7reits", "ganz", "vor\u00b7z\u00fcg\u00b7lich", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "ADV", "$,", "VAFIN", "ADV", "ADV", "ADJD", "$,"], "meter": "-+--+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "sie gedenkt also noch einige Zeit bei mir zu bleiben.", "tokens": ["sie", "ge\u00b7denkt", "al\u00b7so", "noch", "ei\u00b7ni\u00b7ge", "Zeit", "bei", "mir", "zu", "blei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "PIAT", "NN", "APPR", "PPER", "PTKZU", "VVINF", "$."], "meter": "+-+---+--+-+-+-", "measure": "trochaic.hexa.relaxed"}}}}}