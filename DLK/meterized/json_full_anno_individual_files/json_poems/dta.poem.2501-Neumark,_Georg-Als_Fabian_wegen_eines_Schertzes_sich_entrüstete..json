{"dta.poem.2501": {"metadata": {"author": {"name": "Neumark, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Als Fabian wegen eines Schertzes  \n sich entr\u00fcstete.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1652", "urn": "urn:nbn:de:kobv:b4-20428-0", "language": ["de:0.99"], "booktitle": "Neumark, Georg: Poetisch- und Musikalisches Lustw\u00e4ldchen. Hamburg, 1652."}, "poem": {"stanza.1": {"line.1": {"text": "Ergetz dich nicht so sehr du m\u00f6gtest sonst gewinnen/", "tokens": ["Er\u00b7getz", "dich", "nicht", "so", "sehr", "du", "m\u00f6g\u00b7test", "sonst", "ge\u00b7win\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKNEG", "ADV", "ADV", "PPER", "VMFIN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mein Freund Herr Fabian/ die splitterb\u00f6se Gicht.", "tokens": ["Mein", "Freund", "Herr", "Fa\u00b7bi\u00b7an", "/", "die", "split\u00b7ter\u00b7b\u00f6\u00b7se", "Gicht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "NE", "$(", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Du Thor ich nennte dich ja keinen Narren nicht/", "tokens": ["Du", "Thor", "ich", "nenn\u00b7te", "dich", "ja", "kei\u00b7nen", "Nar\u00b7ren", "nicht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "PPER", "VVFIN", "PPER", "ADV", "PIAT", "NN", "PTKNEG", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nur die\u00df hab\u2019 ich gesagt: ", "tokens": ["Nur", "die\u00df", "hab'", "ich", "ge\u00b7sagt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDS", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+---+", "measure": "dactylic.init"}, "line.5": {"text": "nen/", "tokens": ["nen", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "-", "measure": "single.down"}, "line.6": {"text": "Das Spinkelwinkel-Spiel/ drey haben", "tokens": ["Das", "Spin\u00b7kel\u00b7win\u00b7kel\u00b7Spiel", "/", "drey", "ha\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$(", "CARD", "VAFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "sich verstekkt/", "tokens": ["sich", "vers\u00b7tekkt", "/"], "token_info": ["word", "word", "punct"], "pos": ["PRF", "VVPP", "$("], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "Die andern suchen sie. Heist dieses denn gegekkt?", "tokens": ["Die", "an\u00b7dern", "su\u00b7chen", "sie", ".", "Heist", "die\u00b7ses", "denn", "ge\u00b7gekkt", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "PPER", "$.", "NN", "PDAT", "KON", "VVPP", "$."], "meter": "-+-+-+++-+-+", "measure": "unknown.measure.septa"}}}}}