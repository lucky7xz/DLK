": ["PIAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Denkt, da\u00df zu allen Tagen,", "tokens": ["Denkt", ",", "da\u00df", "zu", "al\u00b7len", "Ta\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Denkt, da\u00df bei uns von je", "tokens": ["Denkt", ",", "da\u00df", "bei", "uns", "von", "je"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "KOUS", "APPR", "PPER", "APPR", "ADV"], "meter": "---+-+", "measure": "unknown.measure.di"}, "line.3": {"text": "Man immer h\u00f6rte sagen:", "tokens": ["Man", "im\u00b7mer", "h\u00f6r\u00b7te", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "\u00bbnur Wohl und Keinem Weh!\u00ab", "tokens": ["\u00bb", "nur", "Wohl", "und", "Kei\u00b7nem", "Weh", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "ADV", "KON", "PIAT", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und la\u00dft das Scherflein springen", "tokens": ["Und", "la\u00dft", "das", "Scher\u00b7flein", "sprin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "So lustig an den Rhein,", "tokens": ["So", "lus\u00b7tig", "an", "den", "Rhein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ART", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wie ich dies Lied t\u00e4t singen", "tokens": ["Wie", "ich", "dies", "Lied", "t\u00e4t", "sin\u00b7