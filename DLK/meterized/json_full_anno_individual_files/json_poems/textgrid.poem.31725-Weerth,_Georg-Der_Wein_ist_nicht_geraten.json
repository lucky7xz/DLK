{"textgrid.poem.31725": {"metadata": {"author": {"name": "Weerth, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Der Wein ist nicht geraten", "genre": "verse", "period": "N.A.", "pub_year": 1839, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was hab ich doch vernommen", "tokens": ["Was", "hab", "ich", "doch", "ver\u00b7nom\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "PPER", "ADV", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "F\u00fcr gro\u00dfe Traurigkeit!", "tokens": ["F\u00fcr", "gro\u00b7\u00dfe", "Trau\u00b7rig\u00b7keit", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es ist ins Land gekommen", "tokens": ["Es", "ist", "ins", "Land", "ge\u00b7kom\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Gar eine schlimme Zeit!", "tokens": ["Gar", "ei\u00b7ne", "schlim\u00b7me", "Zeit", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der Wein ist nicht geraten", "tokens": ["Der", "Wein", "ist", "nicht", "ge\u00b7ra\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PTKNEG", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "An Mosel, Rhein und Lahn,", "tokens": ["An", "Mo\u00b7sel", ",", "Rhein", "und", "Lahn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "NE", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und was die Winzer taten,", "tokens": ["Und", "was", "die", "Win\u00b7zer", "ta\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Das ist umsonst getan!", "tokens": ["Das", "ist", "um\u00b7sonst", "ge\u00b7tan", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Es pflanzte seine Reben", "tokens": ["Es", "pflanz\u00b7te", "sei\u00b7ne", "Re\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein jeder nett und fein;", "tokens": ["Ein", "je\u00b7der", "nett", "und", "fein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er dachte: Gott wird geben", "tokens": ["Er", "dach\u00b7te", ":", "Gott", "wird", "ge\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "NN", "VAFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den lichten Sonnenschein,", "tokens": ["Den", "lich\u00b7ten", "Son\u00b7nen\u00b7schein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "---+-+", "measure": "unknown.measure.di"}, "line.5": {"text": "Der fern die Wolke lenket,", "tokens": ["Der", "fern", "die", "Wol\u00b7ke", "len\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Da\u00df sie sich rauschend senkt,", "tokens": ["Da\u00df", "sie", "sich", "rau\u00b7schend", "senkt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Auch unsrer H\u00fcgel denket", "tokens": ["Auch", "uns\u00b7rer", "H\u00fc\u00b7gel", "den\u00b7ket"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und frischen Tau uns schenkt.", "tokens": ["Und", "fri\u00b7schen", "Tau", "uns", "schenkt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und oft zur Morgenstunde \u2013", "tokens": ["Und", "oft", "zur", "Mor\u00b7gen\u00b7stun\u00b7de", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Kam Mai und Juni drauf \u2013", "tokens": ["Kam", "Mai", "und", "Ju\u00b7ni", "drauf", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "KON", "NN", "PTKVZ", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die irdne Pfeif im Munde,", "tokens": ["Die", "ird\u00b7ne", "Pfeif", "im", "Mun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Stieg er den Berg hinauf;", "tokens": ["Stieg", "er", "den", "Berg", "hin\u00b7auf", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Und froh war sein Gem\u00fcte,", "tokens": ["Und", "froh", "war", "sein", "Ge\u00b7m\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wenn von der Felsenwand", "tokens": ["Wenn", "von", "der", "Fel\u00b7sen\u00b7wand"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Die erste junge Bl\u00fcte", "tokens": ["Die", "ers\u00b7te", "jun\u00b7ge", "Bl\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Den s\u00fc\u00dfen Duft gesandt,", "tokens": ["Den", "s\u00fc\u00b7\u00dfen", "Duft", "ge\u00b7sandt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wenn sich zu voller Traube", "tokens": ["Wenn", "sich", "zu", "vol\u00b7ler", "Trau\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Beeren angesetzt", "tokens": ["Die", "Bee\u00b7ren", "an\u00b7ge\u00b7setzt"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und in dem gr\u00fcnen Laube", "tokens": ["Und", "in", "dem", "gr\u00fc\u00b7nen", "Lau\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein Schimmern war zuletzt:", "tokens": ["Ein", "Schim\u00b7mern", "war", "zu\u00b7letzt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Als s\u00e4h man herrlich prangen", "tokens": ["Als", "s\u00e4h", "man", "herr\u00b7lich", "pran\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "PIS", "ADJD", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Des Goldes hellen Schein,", "tokens": ["Des", "Gol\u00b7des", "hel\u00b7len", "Schein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Als w\u00e4r der Berg behangen", "tokens": ["Als", "w\u00e4r", "der", "Berg", "be\u00b7han\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "VAFIN", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Rings mit Rubinenstein!", "tokens": ["Rings", "mit", "Ru\u00b7bi\u00b7nen\u00b7stein", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.5": {"line.1": {"text": "\u00bbgott ist mir gut gewesen!\u00ab", "tokens": ["\u00bb", "gott", "ist", "mir", "gut", "ge\u00b7we\u00b7sen", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADJD", "VAFIN", "PPER", "ADJD", "VAPP", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So klang des Winzers Lied;", "tokens": ["So", "klang", "des", "Win\u00b7zers", "Lied", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbbald werd ich lustig lesen,", "tokens": ["\u00bb", "bald", "werd", "ich", "lus\u00b7tig", "le\u00b7sen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VAFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Was mir der Herr beschied!", "tokens": ["Was", "mir", "der", "Herr", "be\u00b7schied", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ein sch\u00f6ner Erntemorgen", "tokens": ["Ein", "sch\u00f6\u00b7ner", "Ern\u00b7te\u00b7mor\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Bricht in den D\u00f6rfern an,", "tokens": ["Bricht", "in", "den", "D\u00f6r\u00b7fern", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "Vorbei nun Gram und Sorgen,", "tokens": ["Vor\u00b7bei", "nun", "Gram", "und", "Sor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ich bin ein froher Mann!\u00ab", "tokens": ["Ich", "bin", "ein", "fro\u00b7her", "Mann", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Er sprach's. Da zog mit St\u00fcrmen", "tokens": ["Er", "sprach'", "s.", "Da", "zog", "mit", "St\u00fcr\u00b7men"], "token_info": ["word", "word", "abbreviation", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "VVIMP", "ADV", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der kalte Herbst daher:", "tokens": ["Der", "kal\u00b7te", "Herbst", "da\u00b7her", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er sah die Wolken t\u00fcrmen", "tokens": ["Er", "sah", "die", "Wol\u00b7ken", "t\u00fcr\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sich rings so regenschwer.", "tokens": ["Sich", "rings", "so", "re\u00b7gen\u00b7schwer", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Verschwunden ist sein Hoffen!", "tokens": ["Ver\u00b7schwun\u00b7den", "ist", "sein", "Hof\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Das kurze Gl\u00fcck ist aus!", "tokens": ["Das", "kur\u00b7ze", "Gl\u00fcck", "ist", "aus", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Von hartem Schlag getroffen", "tokens": ["Von", "har\u00b7tem", "Schlag", "ge\u00b7trof\u00b7fen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Geht weinend er nach Haus!", "tokens": ["Geht", "wei\u00b7nend", "er", "nach", "Haus", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Du wirst die H\u00e4nde legen", "tokens": ["Du", "wirst", "die", "H\u00e4n\u00b7de", "le\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nicht an die Kelter dein!", "tokens": ["Nicht", "an", "die", "Kel\u00b7ter", "dein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ART", "NN", "PPOSAT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nun tr\u00e4uft des Weines Segen", "tokens": ["Nun", "tr\u00e4uft", "des", "Wei\u00b7nes", "Se\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nicht in dein Fa\u00df hinein!", "tokens": ["Nicht", "in", "dein", "Fa\u00df", "hin\u00b7ein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Du wirst kein Lied mehr singen!", "tokens": ["Du", "wirst", "kein", "Lied", "mehr", "sin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Kein Brot und w\u00e4rmend Kleid", "tokens": ["Kein", "Brot", "und", "w\u00e4r\u00b7mend", "Kleid"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "KON", "ADJD", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wirst du den Kindern bringen,", "tokens": ["Wirst", "du", "den", "Kin\u00b7dern", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ist alles rings verschneit!", "tokens": ["Ist", "al\u00b7les", "rings", "ver\u00b7schneit", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Drum, die ihr in den St\u00e4dten", "tokens": ["Drum", ",", "die", "ihr", "in", "den", "St\u00e4d\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PAV", "$,", "PRELS", "PPER", "APPR", "ART", "NN"], "meter": "-++--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Nach vollen Sch\u00fcsseln langt,", "tokens": ["Nach", "vol\u00b7len", "Sch\u00fcs\u00b7seln", "langt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die ihr mit g\u00fcldnen Ketten,", "tokens": ["Die", "ihr", "mit", "g\u00fcld\u00b7nen", "Ket\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit Kreuz und Sternen prangt,", "tokens": ["Mit", "Kreuz", "und", "Ster\u00b7nen", "prangt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die ihr den Nierensteiner", "tokens": ["Die", "ihr", "den", "Nie\u00b7ren\u00b7stei\u00b7ner"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PPER", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Im tiefen Keller habt", "tokens": ["Im", "tie\u00b7fen", "Kel\u00b7ler", "habt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "VAFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und oft mit Ingelheimer", "tokens": ["Und", "oft", "mit", "In\u00b7gel\u00b7hei\u00b7mer"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die durst'gen Kehlen labt,", "tokens": ["Die", "dur\u00b7st'\u00b7gen", "Keh\u00b7len", "labt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.9": {"line.1": {"text": "Die ihr im schmucken Saale", "tokens": ["Die", "ihr", "im", "sc