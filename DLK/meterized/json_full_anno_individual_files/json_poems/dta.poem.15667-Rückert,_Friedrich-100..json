{"dta.poem.15667": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "100.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das Denken, das sich treibt in ungemessnem Gleise,", "tokens": ["Das", "Den\u00b7ken", ",", "das", "sich", "treibt", "in", "un\u00b7ge\u00b7mess\u00b7nem", "Glei\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PRF", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hat nirgend Ruh' als wo sichs r\u00fcndet still im Kreise.", "tokens": ["Hat", "nir\u00b7gend", "Ruh'", "als", "wo", "sichs", "r\u00fcn\u00b7det", "still", "im", "Krei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "NN", "KON", "PWAV", "PIS", "VVFIN", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ob enger solch ein Kreis, ob weiter sei, ist gleich;", "tokens": ["Ob", "en\u00b7ger", "solch", "ein", "Kreis", ",", "ob", "wei\u00b7ter", "sei", ",", "ist", "gleich", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "PIAT", "ART", "NN", "$,", "KOUS", "ADV", "VAFIN", "$,", "VAFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Geist, im engsten wohlgeschlossnen f\u00fchlt sich reich.", "tokens": ["Der", "Geist", ",", "im", "engs\u00b7ten", "wohl\u00b7ge\u00b7schloss\u00b7nen", "f\u00fchlt", "sich", "reich", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPRART", "ADJA", "ADJA", "VVFIN", "PRF", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Doch f\u00fchlt er reich sich nur auf einen Augenblick,", "tokens": ["Doch", "f\u00fchlt", "er", "reich", "sich", "nur", "auf", "ei\u00b7nen", "Au\u00b7gen\u00b7blick", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "PRF", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In neue Kreise treibt ihn ewig sein Geschick.", "tokens": ["In", "neu\u00b7e", "Krei\u00b7se", "treibt", "ihn", "e\u00b7wig", "sein", "Ge\u00b7schick", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Und volle Ruhe wird vom Denken nur gefunden,", "tokens": ["Und", "vol\u00b7le", "Ru\u00b7he", "wird", "vom", "Den\u00b7ken", "nur", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VAFIN", "APPRART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo es in Einen Kreis vermag die Welt zu runden.", "tokens": ["Wo", "es", "in", "Ei\u00b7nen", "Kreis", "ver\u00b7mag", "die", "Welt", "zu", "run\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Solange scheinen wie Planeten irr zu gehn", "tokens": ["So\u00b7lan\u00b7ge", "schei\u00b7nen", "wie", "Pla\u00b7ne\u00b7ten", "irr", "zu", "gehn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "KOKOM", "NN", "ADJD", "PTKZU", "VVINF"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Gedanken, bis bewu\u00dft sie eine Sonn' umdrehn.", "tokens": ["Ge\u00b7dan\u00b7ken", ",", "bis", "be\u00b7wu\u00dft", "sie", "ei\u00b7ne", "Sonn'", "um\u00b7drehn", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "ADJD", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Um eine Sonne drehn sich meine lange schon,", "tokens": ["Um", "ei\u00b7ne", "Son\u00b7ne", "drehn", "sich", "mei\u00b7ne", "lan\u00b7ge", "schon", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "CARD", "PRF", "PPOSAT", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die ihnen nur verh\u00fcllt ist auf dem Mittelthron.", "tokens": ["Die", "ih\u00b7nen", "nur", "ver\u00b7h\u00fcllt", "ist", "auf", "dem", "Mit\u00b7tel\u00b7thron", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVPP", "VAFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}