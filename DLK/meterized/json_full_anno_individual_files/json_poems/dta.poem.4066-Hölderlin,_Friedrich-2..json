{"dta.poem.4066": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905197104", "language": ["de:0.85", "fr:0.14"], "booktitle": "H\u00f6lderlin, Friedrich: Gedichte. Stuttgart u. a., 1826."}, "poem": {"stanza.1": {"line.1": {"text": "Aber meinest du nun, es haben die Thore ver-", "tokens": ["A\u00b7ber", "mei\u00b7nest", "du", "nun", ",", "es", "ha\u00b7ben", "die", "Tho\u00b7re", "ver"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "$,", "PPER", "VAFIN", "ART", "NN", "TRUNC"], "meter": "+----+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "gebens", "tokens": ["ge\u00b7bens"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Aufgethan und den Weg freudig die G\u00f6tter", "tokens": ["Auf\u00b7ge\u00b7than", "und", "den", "Weg", "freu\u00b7dig", "die", "G\u00f6t\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "KON", "ART", "NN", "ADJD", "ART", "NN"], "meter": "+-+--++--+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "gemacht?", "tokens": ["ge\u00b7macht", "?"], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "Und es schenken umsonst zu des Gastmahls F\u00fclle", "tokens": ["Und", "es", "schen\u00b7ken", "um\u00b7sonst", "zu", "des", "Gast\u00b7mahls", "F\u00fcl\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "NN"], "meter": "--+-++--+-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "die Guten", "tokens": ["die", "Gu\u00b7ten"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Nebst dem Weine noch auch Blumen und Honig", "tokens": ["Nebst", "dem", "Wei\u00b7ne", "noch", "auch", "Blu\u00b7men", "und", "Ho\u00b7nig"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADV", "ADV", "NN", "KON", "NN"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.8": {"text": "und Obst?", "tokens": ["und", "Obst", "?"], "token_info": ["word", "word", "punct"], "pos": ["KON", "NN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.9": {"text": "Schenken das purpurne Licht zu Festges\u00e4ngen, und", "tokens": ["Schen\u00b7ken", "das", "pur\u00b7pur\u00b7ne", "Licht", "zu", "Fest\u00b7ge\u00b7s\u00e4n\u00b7gen", ",", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["NN", "ART", "ADJA", "NN", "APPR", "NN", "$,", "KON"], "meter": "+--+--+-+-+-+", "measure": "dactylic.di.plus"}, "line.10": {"text": "k\u00fchl und", "tokens": ["k\u00fchl", "und"], "token_info": ["word", "word"], "pos": ["ADJD", "KON"], "meter": "+-", "measure": "trochaic.single"}, "line.11": {"text": "Ruhig zu tieferem Freundesgespr\u00e4che die Nacht?", "tokens": ["Ru\u00b7hig", "zu", "tie\u00b7fe\u00b7rem", "Freun\u00b7des\u00b7ge\u00b7spr\u00e4\u00b7che", "die", "Nacht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ADJA", "NN", "ART", "NN", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.12": {"text": "H\u00e4lt ein Ernsteres dich, so spar's dem Winter,", "tokens": ["H\u00e4lt", "ein", "Erns\u00b7te\u00b7res", "dich", ",", "so", "spa\u00b7r's", "dem", "Win\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "$,", "ADV", "PIS", "ART", "NN", "$,"], "meter": "--+--+-+--+-", "measure": "anapaest.di.plus"}, "line.13": {"text": "und willst du", "tokens": ["und", "willst", "du"], "token_info": ["word", "word", "word"], "pos": ["KON", "VMFIN", "PPER"], "meter": "-+-", "measure": "amphibrach.single"}, "line.14": {"text": "Freien, habe Geduld, Freier begl\u00fccket der Mai.", "tokens": ["Frei\u00b7en", ",", "ha\u00b7be", "Ge\u00b7duld", ",", "Frei\u00b7er", "be\u00b7gl\u00fc\u00b7cket", "der", "Mai", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VAFIN", "NN", "$,", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+--+---+--+", "measure": "trochaic.penta.relaxed"}, "line.15": {"text": "Jetzt ist Anderes Noth, jetzt komm und feire des", "tokens": ["Jetzt", "ist", "An\u00b7de\u00b7res", "Noth", ",", "jetzt", "komm", "und", "fei\u00b7re", "des"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADJA", "NN", "$,", "ADV", "VVFIN", "KON", "VVFIN", "ART"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.16": {"text": "Herbstes", "tokens": ["Herbs\u00b7tes"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.17": {"text": "Alte Sitte, noch jetzt bl\u00fchet die edle mit uns.", "tokens": ["Al\u00b7te", "Sit\u00b7te", ",", "noch", "jetzt", "bl\u00fc\u00b7het", "die", "ed\u00b7le", "mit", "uns", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADV", "ADV", "VVFIN", "ART", "ADJA", "APPR", "PPER", "$."], "meter": "--+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.18": {"text": "Eins nur gilt f\u00fcr den Tag, das Vaterland, und", "tokens": ["Eins", "nur", "gilt", "f\u00fcr", "den", "Tag", ",", "das", "Va\u00b7ter\u00b7land", ",", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["NN", "ADV", "VVFIN", "APPR", "ART", "NN", "$,", "ART", "NN", "$,", "KON"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "des Opfers", "tokens": ["des", "Op\u00b7fers"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.20": {"text": "Festlicher Flamme wirft jeder sein Eigenes zu.", "tokens": ["Fest\u00b7li\u00b7cher", "Flam\u00b7me", "wirft", "je\u00b7der", "sein", "Ei\u00b7ge\u00b7nes", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PIS", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.21": {"text": "Darum kr\u00e4nzt der gemeinsame Gott ums\u00e4uselnd", "tokens": ["Da\u00b7rum", "kr\u00e4nzt", "der", "ge\u00b7mein\u00b7sa\u00b7me", "Gott", "um\u00b7s\u00e4u\u00b7selnd"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "ART", "ADJA", "NN", "VVPP"], "meter": "--+--++-+-+-", "measure": "anapaest.di.plus"}, "line.22": {"text": "das Haar uns,", "tokens": ["das", "Haar", "uns", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.23": {"text": "Und den eigenen Sinn schmelzet, wie Perlen,", "tokens": ["Und", "den", "ei\u00b7ge\u00b7nen", "Sinn", "schmel\u00b7zet", ",", "wie", "Per\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "$,", "PWAV", "NN", "$,"], "meter": "--+--++--+-", "measure": "anapaest.di.plus"}, "line.24": {"text": "der Wein.", "tokens": ["der", "Wein", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.25": {"text": "Die\u00df bedeutet der Tisch, der gelehrte, wenn, wie", "tokens": ["Die\u00df", "be\u00b7deu\u00b7tet", "der", "Tisch", ",", "der", "ge\u00b7lehr\u00b7te", ",", "wenn", ",", "wie"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word"], "pos": ["PDS", "VVFIN", "ART", "NN", "$,", "ART", "ADJA", "$,", "KOUS", "$,", "PWAV"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.26": {"text": "die Bienen,", "tokens": ["die", "Bie\u00b7nen", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.27": {"text": "Rund um den Eichbaum, wir sitzen und singen", "tokens": ["Rund", "um", "den", "Eich\u00b7baum", ",", "wir", "sit\u00b7zen", "und", "sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN", "$,", "PPER", "VVFIN", "KON", "VVINF"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.28": {"text": "um ihn.", "tokens": ["um", "ihn", "."], "token_info": ["word", "word", "punct"], "pos": ["KOUI", "PPER", "$."], "meter": "-+", "measure": "iambic.single"}, "line.29": {"text": "Die\u00df der Pokale Klang und darum zwinget die wilden", "tokens": ["Die\u00df", "der", "Po\u00b7ka\u00b7le", "Klang", "und", "da\u00b7rum", "zwin\u00b7get", "die", "wil\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "ART", "NN", "NN", "KON", "PAV", "VVFIN", "ART", "ADJA"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.30": {"text": "Seelen der streitenden M\u00e4nner zusammen der", "tokens": ["See\u00b7len", "der", "strei\u00b7ten\u00b7den", "M\u00e4n\u00b7ner", "zu\u00b7sam\u00b7men", "der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN", "ADV", "ART"], "meter": "+--+--+--+--", "measure": "dactylic.tetra"}, "line.31": {"text": "Chor.", "tokens": ["Chor", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}}}}}