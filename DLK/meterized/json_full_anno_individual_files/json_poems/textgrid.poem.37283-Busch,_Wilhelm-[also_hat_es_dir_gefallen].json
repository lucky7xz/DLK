{"textgrid.poem.37283": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "[also hat es dir gefallen]", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Also hat es dir gefallen", "tokens": ["Al\u00b7so", "hat", "es", "dir", "ge\u00b7fal\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hier in dieser sch\u00f6nen Welt;", "tokens": ["Hier", "in", "die\u00b7ser", "sch\u00f6\u00b7nen", "Welt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PDAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "So da\u00df das Vondannenwallen", "tokens": ["So", "da\u00df", "das", "Von\u00b7dan\u00b7nen\u00b7wal\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "KOUS", "ART", "NN"], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.4": {"text": "Dir nicht sonderlich gef\u00e4llt.", "tokens": ["Dir", "nicht", "son\u00b7der\u00b7lich", "ge\u00b7f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "La\u00df dich das doch nicht verdrie\u00dfen.", "tokens": ["La\u00df", "dich", "das", "doch", "nicht", "ver\u00b7drie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PDS", "ADV", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn du wirklich willst und meinst,", "tokens": ["Wenn", "du", "wirk\u00b7lich", "willst", "und", "meinst", ","], "token_info": ["word", "word", "word", "word", "word", "word"