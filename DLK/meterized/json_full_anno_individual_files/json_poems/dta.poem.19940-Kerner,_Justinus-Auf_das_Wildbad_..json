{"dta.poem.19940": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Auf das Wildbad .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905193111", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Qu\u00e4lt Schmerz und Krankheit deine Glieder,", "tokens": ["Qu\u00e4lt", "Schmerz", "und", "Krank\u00b7heit", "dei\u00b7ne", "Glie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Macht welk dein Herz der Menschen Qual;", "tokens": ["Macht", "welk", "dein", "Herz", "der", "Men\u00b7schen", "Qual", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PPOSAT", "NN", "ART", "NN", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Verla\u00df' die Welt und steig hernieder", "tokens": ["Ver\u00b7la\u00df'", "die", "Welt", "und", "steig", "her\u00b7nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "ART", "NN", "KON", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In dieses unterird'sche Thal.", "tokens": ["In", "die\u00b7ses", "un\u00b7ter\u00b7ird'\u00b7sche", "Thal", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Hier legt Natur mit linden Armen", "tokens": ["Hier", "legt", "Na\u00b7tur", "mit", "lin\u00b7den", "Ar\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dich an die Brust und l\u00f6st den Schmerz.", "tokens": ["Dich", "an", "die", "Brust", "und", "l\u00f6st", "den", "Schmerz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wollt' dich kein Menschenherz erwarmen,", "tokens": ["Wollt'", "dich", "kein", "Men\u00b7schen\u00b7herz", "er\u00b7war\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Erw\u00e4rmt dich hier ihr Mutterherz.", "tokens": ["Er\u00b7w\u00e4rmt", "dich", "hier", "ihr", "Mut\u00b7ter\u00b7herz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Der Wasser gute Geister singen", "tokens": ["Der", "Was\u00b7ser", "gu\u00b7te", "Geis\u00b7ter", "sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Hier aus krystallnen Tiefen laut:", "tokens": ["Hier", "aus", "krys\u00b7tall\u00b7nen", "Tie\u00b7fen", "laut", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u201ebald werden dem wir Heilung bringen,", "tokens": ["\u201e", "bald", "wer\u00b7den", "dem", "wir", "Hei\u00b7lung", "brin\u00b7gen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VAFIN", "ART", "PPER", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "\u201eder liebend unsrer Kraft vertraut.\u201c", "tokens": ["\u201e", "der", "lie\u00b7bend", "uns\u00b7rer", "Kraft", "ver\u00b7traut", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "ADJD", "PPOSAT", "NN", "ADJD", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ja! Kranker! wie ein Kind an's Herze", "tokens": ["Ja", "!", "Kran\u00b7ker", "!", "wie", "ein", "Kind", "an's", "Her\u00b7ze"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$.", "NN", "$.", "PWAV", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Mutter sich vertrauend legt,", "tokens": ["Der", "Mut\u00b7ter", "sich", "ver\u00b7trau\u00b7end", "legt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Lieg' in den Born mit deinem Schmerze,", "tokens": ["Lieg'", "in", "den", "Born", "mit", "dei\u00b7nem", "Schmer\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "++-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Von Lieb' und Hoffnung still bewegt.", "tokens": ["Von", "Lieb'", "und", "Hoff\u00b7nung", "still", "be\u00b7wegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Wie Lenzeshauch wird's dich durchbeben;", "tokens": ["Wie", "Len\u00b7zes\u00b7hauch", "wird's", "dich", "durch\u00b7be\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Frag' nicht, wie diese Kraft man hei\u00dft;", "tokens": ["Frag'", "nicht", ",", "wie", "die\u00b7se", "Kraft", "man", "hei\u00dft", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "$,", "PWAV", "PDAT", "NN", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du kehrst, ein neuer Mensch, in's Leben", "tokens": ["Du", "kehrst", ",", "ein", "neu\u00b7er", "Mensch", ",", "in's", "Le\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "ART", "ADJA", "NN", "$,", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und sprichst: das that des Wildbads Geist!", "tokens": ["Und", "sprichst", ":", "das", "that", "des", "Wild\u00b7bads", "Geist", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "PDS", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}