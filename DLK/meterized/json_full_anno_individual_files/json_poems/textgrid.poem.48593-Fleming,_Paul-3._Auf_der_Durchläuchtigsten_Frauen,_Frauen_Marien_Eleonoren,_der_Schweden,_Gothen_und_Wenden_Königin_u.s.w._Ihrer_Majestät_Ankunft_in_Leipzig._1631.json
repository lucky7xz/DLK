", ",", "bis", "will\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJD", "$,", "KOUS", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "hochgelobte K\u00f6nigin,", "tokens": ["hoch\u00b7ge\u00b7lob\u00b7te", "K\u00f6\u00b7ni\u00b7gin", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "nun auch hieher hat genommen", "tokens": ["nun", "auch", "hie\u00b7her", "hat", "ge\u00b7nom\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PAV", "VAFIN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "seinen Pfad dein edler Sin!", "tokens": ["sei\u00b7nen", "Pfad", "dein", "ed\u00b7ler", "Sin", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Bis willkommen! rufen Alle,", "tokens": ["Bis", "will\u00b7kom\u00b7men", "!", "ru\u00b7fen", "Al\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJD", "$.", "VVFIN", "PIS", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "wer nur rufen kan mit Schalle.", "tokens": ["wer", "nur", "ru\u00b7fen", "kan", "mit", "Schal\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVINF", "VMFIN", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.21": {"line.1": {"text": "Das verlebte Jahr wird j\u00fcnger,", "tokens": ["Das", "ver\u00b7leb\u00b7te", "Jahr", "wird", "j\u00fcn\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "E