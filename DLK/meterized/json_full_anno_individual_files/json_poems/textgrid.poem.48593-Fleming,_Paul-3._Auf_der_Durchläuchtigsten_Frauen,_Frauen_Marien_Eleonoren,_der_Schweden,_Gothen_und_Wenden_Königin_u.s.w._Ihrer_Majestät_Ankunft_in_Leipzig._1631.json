{"textgrid.poem.48593": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "3. Auf der Durchl\u00e4uchtigsten Frauen, Frauen Marien Eleonoren, der Schweden, Gothen und Wenden K\u00f6nigin u.s.w. Ihrer Majest\u00e4t Ankunft in Leipzig. 1631", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nymfe, welcher ich zu Ehren", "tokens": ["Nym\u00b7fe", ",", "wel\u00b7cher", "ich", "zu", "Eh\u00b7ren"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "PPER", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "billich diese Faust setz' an,", "tokens": ["bil\u00b7lich", "die\u00b7se", "Faust", "setz'", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PDAT", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "welcher ich ein Lied la\u00df h\u00f6ren,", "tokens": ["wel\u00b7cher", "ich", "ein", "Lied", "la\u00df", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ART", "NN", "VVFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "das die Zeit bestehen kan,", "tokens": ["das", "die", "Zeit", "be\u00b7ste\u00b7hen", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "sei, Prinzessin, mir gewogen,", "tokens": ["sei", ",", "Prin\u00b7zes\u00b7sin", ",", "mir", "ge\u00b7wo\u00b7gen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "$,", "NN", "$,", "PPER", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "bis ich den Gesang vollzogen!", "tokens": ["bis", "ich", "den", "Ge\u00b7sang", "voll\u00b7zo\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Was doch aber soll ich spielen,", "tokens": ["Was", "doch", "a\u00b7ber", "soll", "ich", "spie\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADV", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "was doch soll ich heben an?", "tokens": ["was", "doch", "soll", "ich", "he\u00b7ben", "an", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VMFIN", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie vor deinem Herren fielen", "tokens": ["Wie", "vor", "dei\u00b7nem", "Her\u00b7ren", "fie\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "so viel tausent tausent Man?", "tokens": ["so", "viel", "tau\u00b7sent", "tau\u00b7sent", "Man", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "VVFIN", "PIS", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie von Kriegern, wie von Rossen", "tokens": ["Wie", "von", "Krie\u00b7gern", ",", "wie", "von", "Ros\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "APPR", "NN", "$,", "PWAV", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "blutgef\u00fcllte B\u00e4che flossen?", "tokens": ["blut\u00b7ge\u00b7f\u00fcll\u00b7te", "B\u00e4\u00b7che", "flos\u00b7sen", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wie der ", "tokens": ["Wie", "der"], "token_info": ["word", "word"], "pos": ["PWAV", "ART"], "meter": "+-", "measure": "trochaic.single"}, "line.2": {"text": "sich vor ihm gezogen ein?", "tokens": ["sich", "vor", "ihm", "ge\u00b7zo\u00b7gen", "ein", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "PPER", "VVPP", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wie sich vor ihm knechtlich b\u00fccken", "tokens": ["Wie", "sich", "vor", "ihm", "knecht\u00b7lich", "b\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PRF", "APPR", "PPER", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "die bezwungnen ", "tokens": ["die", "be\u00b7zwung\u00b7nen"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Wie sich ihre stolzen Wellen", "tokens": ["Wie", "sich", "ih\u00b7re", "stol\u00b7zen", "Wel\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PRF", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "ihm zu Dienste m\u00fcssen stellen?", "tokens": ["ihm", "zu", "Diens\u00b7te", "m\u00fcs\u00b7sen", "stel\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Nein! Die unerh\u00f6rten Taten", "tokens": ["Nein", "!", "Die", "un\u00b7er\u00b7h\u00f6r\u00b7ten", "Ta\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$.", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "r\u00fchmen an sich selbsten sich.", "tokens": ["r\u00fch\u00b7men", "an", "sich", "selbs\u00b7ten", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PRF", "VVFIN", "PRF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Was vor Siegen ihm geraten,", "tokens": ["Was", "vor", "Sie\u00b7gen", "ihm", "ge\u00b7ra\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "ist gar viel zu hoch f\u00fcr mich.", "tokens": ["ist", "gar", "viel", "zu", "hoch", "f\u00fcr", "mich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "PTKA", "ADJD", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Eine Muse mu\u00df es singen,", "tokens": ["Ei\u00b7ne", "Mu\u00b7se", "mu\u00df", "es", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "die sich gleichet solchen Dingen.", "tokens": ["die", "sich", "glei\u00b7chet", "sol\u00b7chen", "Din\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRELS", "PRF", "VVFIN", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Hier hab' ich mir angesetzet,", "tokens": ["Hier", "hab'", "ich", "mir", "an\u00b7ge\u00b7set\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Heldin, nur zu sehn auf dich,", "tokens": ["Hel\u00b7din", ",", "nur", "zu", "sehn", "auf", "dich", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "PTKZU", "VVINF", "APPR", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "nun uns dein Gesicht' ergetzet,", "tokens": ["nun", "uns", "dein", "Ge\u00b7sicht'", "er\u00b7get\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "und von uns l\u00e4\u00dft sehen sich.", "tokens": ["und", "von", "uns", "l\u00e4\u00dft", "se\u00b7hen", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VVFIN", "VVFIN", "PRF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Dieses Liedes linde Weisen", "tokens": ["Die\u00b7ses", "Lie\u00b7des", "lin\u00b7de", "Wei\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["PDAT", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "sollen deine Zukunft preisen.", "tokens": ["sol\u00b7len", "dei\u00b7ne", "Zu\u00b7kunft", "prei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Bis willkommen, bis willkommen,", "tokens": ["Bis", "will\u00b7kom\u00b7men", ",", "bis", "will\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJD", "$,", "KOUS", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "hochgelobte K\u00f6nigin,", "tokens": ["hoch\u00b7ge\u00b7lob\u00b7te", "K\u00f6\u00b7ni\u00b7gin", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "nun auch hieher hat genommen", "tokens": ["nun", "auch", "hie\u00b7her", "hat", "ge\u00b7nom\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PAV", "VAFIN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "seinen Pfad dein edler Sin!", "tokens": ["sei\u00b7nen", "Pfad", "dein", "ed\u00b7ler", "Sin", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Bis willkommen! rufen Alle,", "tokens": ["Bis", "will\u00b7kom\u00b7men", "!", "ru\u00b7fen", "Al\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJD", "$.", "VVFIN", "PIS", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "wer nur rufen kan mit Schalle.", "tokens": ["wer", "nur", "ru\u00b7fen", "kan", "mit", "Schal\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVINF", "VMFIN", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Das verlebte Jahr wird j\u00fcnger,", "tokens": ["Das", "ver\u00b7leb\u00b7te", "Jahr", "wird", "j\u00fcn\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Eurus hemmet seine Rach',", "tokens": ["Eu\u00b7rus", "hem\u00b7met", "sei\u00b7ne", "Rach'", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aquilo, der Wasserzwinger,", "tokens": ["A\u00b7qui\u00b7lo", ",", "der", "Was\u00b7ser\u00b7zwin\u00b7ger", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "bricht die St\u00fcrme, f\u00e4hrt gemach,", "tokens": ["bricht", "die", "St\u00fcr\u00b7me", ",", "f\u00e4hrt", "ge\u00b7mach", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Boreas wei\u00df sanft im Wehen", "tokens": ["Bo\u00b7re\u00b7as", "wei\u00df", "sanft", "im", "We\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ADJD", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "seiner F\u00fcrstin nachzugehen.", "tokens": ["sei\u00b7ner", "F\u00fcrs\u00b7tin", "nach\u00b7zu\u00b7ge\u00b7hen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVIZU", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "F\u00f6bus l\u00e4chelt her von fernen,", "tokens": ["F\u00f6\u00b7bus", "l\u00e4\u00b7chelt", "her", "von", "fer\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "APPR", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "streckt sein Gold erfreuter aus.", "tokens": ["streckt", "sein", "Gold", "er\u00b7freu\u00b7ter", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Luna winket aus den Sternen,", "tokens": ["Lu\u00b7na", "win\u00b7ket", "aus", "den", "Ster\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "und begl\u00e4nzt ihr blaues Haus,", "tokens": ["und", "be\u00b7gl\u00e4nzt", "ihr", "blau\u00b7es", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Tag' und N\u00e4chte sind uns helle,", "tokens": ["Tag'", "und", "N\u00e4ch\u00b7te", "sind", "uns", "hel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VAFIN", "PPER", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "weil da bleiben wirst zur Stelle.", "tokens": ["weil", "da", "blei\u00b7ben", "wirst", "zur", "Stel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "VVINF", "VAFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "die du ihm hast angetan,", "tokens": ["die", "du", "ihm", "hast", "an\u00b7ge\u00b7tan", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "da\u00df du ein hier wollen kehren", "tokens": ["da\u00df", "du", "ein", "hier", "wol\u00b7len", "keh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADV", "VMFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "und den Ort selbst sehen an,", "tokens": ["und", "den", "Ort", "selbst", "se\u00b7hen", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "VVINF", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "den Ort, da dein Held sich wagte", "tokens": ["den", "Ort", ",", "da", "dein", "Held", "sich", "wag\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "KOUS", "PPOSAT", "NN", "PRF", "VVFIN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "und den Feind zu Felde jagte.", "tokens": ["und", "den", "Feind", "zu", "Fel\u00b7de", "jag\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "O ihr wertesten zwei Flammen,", "tokens": ["O", "ihr", "wer\u00b7tes\u00b7ten", "zwei", "Flam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "CARD", "NN", "$,"], "meter": "+-+---+-", "measure": "unknown.measure.tri"}, "line.2": {"text": "er der Helden, du der Zeit,", "tokens": ["er", "der", "Hel\u00b7den", ",", "du", "der", "Zeit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "$,", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "helf' euch Gott gesund zusammen,", "tokens": ["hel\u00b7f'", "euch", "Gott", "ge\u00b7sund", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "wie ihr vor gewesen seid,", "tokens": ["wie", "ihr", "vor", "ge\u00b7we\u00b7sen", "seid", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "VAPP", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "da\u00df aus diesem Abseins-Leide", "tokens": ["da\u00df", "aus", "die\u00b7sem", "Ab\u00b7seins\u00b7Lei\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "euch erwachse lauge Freude!", "tokens": ["euch", "er\u00b7wach\u00b7se", "lau\u00b7ge", "Freu\u00b7de", "!"], "token_info": ["word", "word", "wo