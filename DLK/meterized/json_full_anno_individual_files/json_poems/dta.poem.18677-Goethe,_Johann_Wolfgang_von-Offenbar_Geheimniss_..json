{"dta.poem.18677": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "Offenbar Geheimniss .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Sie haben dich heiliger Hafis", "tokens": ["Sie", "ha\u00b7ben", "dich", "hei\u00b7li\u00b7ger", "Ha\u00b7fis"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Die mystische Zunge genannt,", "tokens": ["Die", "mys\u00b7ti\u00b7sche", "Zun\u00b7ge", "ge\u00b7nannt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Und haben, die Wortgelehrten,", "tokens": ["Und", "ha\u00b7ben", ",", "die", "Wort\u00b7ge\u00b7lehr\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VAFIN", "$,", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Den Werth des Worts nicht erkannt.", "tokens": ["Den", "Werth", "des", "Worts", "nicht", "er\u00b7kannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PTKNEG", "VVPP", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Mystisch heissest du ihnen,", "tokens": ["Mys\u00b7tisch", "heis\u00b7sest", "du", "ih\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PPER", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Weil sie n\u00e4rrisches bey dir denken,", "tokens": ["Weil", "sie", "n\u00e4r\u00b7ri\u00b7sches", "bey", "dir", "den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJA", "APPR", "PPER", "VVINF", "$,"], "meter": "---+-+-+-", "measure": "unknown.measure.tri"}, "line.3": {"text": "Und ihren unlautern Wein", "tokens": ["Und", "ih\u00b7ren", "un\u00b7lau\u00b7tern", "Wein"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "In deinen Namen verschenken.", "tokens": ["In", "dei\u00b7nen", "Na\u00b7men", "ver\u00b7schen\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Du aber bist mystisch rein", "tokens": ["Du", "a\u00b7ber", "bist", "mys\u00b7tisch", "rein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VAFIN", "ADJD", "ADJD"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Weil sie dich nicht verstehn,", "tokens": ["Weil", "sie", "dich", "nicht", "ver\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der du, ohne fromm zu seyn, selig bist!", "tokens": ["Der", "du", ",", "oh\u00b7ne", "fromm", "zu", "seyn", ",", "se\u00b7lig", "bist", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NE", "$,", "KOUI", "ADJD", "PTKZU", "VAINF", "$,", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Das wollen sie dir nicht zugestehn.", "tokens": ["Das", "wol\u00b7len", "sie", "dir", "nicht", "zu\u00b7ge\u00b7stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}}}}