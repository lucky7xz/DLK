{"dta.poem.10834": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Der  \n  Menschliche K\u00f6rper  \n eine  \n  aus vielen Kunstst\u00fckken zusammen ge-  \n setzte Maschine.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198774", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn ich des Leibes Bau anseh,", "tokens": ["Wenn", "ich", "des", "Lei\u00b7bes", "Bau", "an\u00b7seh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Woraus er ist vereint, verbunden:", "tokens": ["Wo\u00b7raus", "er", "ist", "ver\u00b7eint", ",", "ver\u00b7bun\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "PPER", "VAFIN", "VVPP", "$,", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So deucht mir das des Sch\u00f6pfers H\u00f6h,", "tokens": ["So", "deucht", "mir", "das", "des", "Sch\u00f6p\u00b7fers", "H\u00f6h", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PDS", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "An jedem St\u00fckke wird gefunden;", "tokens": ["An", "je\u00b7dem", "St\u00fck\u00b7ke", "wird", "ge\u00b7fun\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der K\u00f6rper, drin die Seele thront,", "tokens": ["Der", "K\u00f6r\u00b7per", ",", "drin", "die", "See\u00b7le", "thront", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und wie in einem Hause wohnt;", "tokens": ["Und", "wie", "in", "ei\u00b7nem", "Hau\u00b7se", "wohnt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ist wenn ich ihn nur recht anblikke,", "tokens": ["Ist", "wenn", "ich", "ihn", "nur", "recht", "an\u00b7blik\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KOUS", "PPER", "PPER", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ein durch so manche Kunst, vereintes Meister-", "tokens": ["Ein", "durch", "so", "man\u00b7che", "Kunst", ",", "ver\u00b7ein\u00b7tes", "Meis\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "APPR", "ADV", "PIAT", "NN", "$,", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Das Haupt der Sinnen Werkstatt, ist", "tokens": ["Das", "Haupt", "der", "Sin\u00b7nen", "Werk\u00b7statt", ",", "ist"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "ART", "NN", "NN", "$,", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Wunderwerk von allen K\u00fcnsten,", "tokens": ["Ein", "Wun\u00b7der\u00b7werk", "von", "al\u00b7len", "K\u00fcns\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Was in der runden H\u00f6hle flie\u00dft,", "tokens": ["Was", "in", "der", "run\u00b7den", "H\u00f6h\u00b7le", "flie\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gleicht denen feinesten Gespinsten:", "tokens": ["Gleicht", "de\u00b7nen", "fei\u00b7nes\u00b7ten", "Ge\u00b7spins\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PDS", "ADJA", "NN", "$."], "meter": "+--+---+-", "measure": "dactylic.di.plus"}, "line.5": {"text": "Das feucht und safftige ", "tokens": ["Das", "feucht", "und", "saff\u00b7ti\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ADJA"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Ist wie ein aufgewundner Zwirn,", "tokens": ["Ist", "wie", "ein", "auf\u00b7ge\u00b7wund\u00b7ner", "Zwirn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ein fl\u00fc\u00dfiges Geweb zu nennen,", "tokens": ["Ein", "fl\u00fc\u00b7\u00dfi\u00b7ges", "Ge\u00b7web", "zu", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "An dessen Feinigkeit wir ", "tokens": ["An", "des\u00b7sen", "Fei\u00b7nig\u00b7keit", "wir"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PRELAT", "NN", "PPER"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Der Augen helles Wunderglas,", "tokens": ["Der", "Au\u00b7gen", "hel\u00b7les", "Wun\u00b7der\u00b7glas", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ist wie ein Seh-Rohr das vergr\u00f6ssert,", "tokens": ["Ist", "wie", "ein", "Seh\u00b7Rohr", "das", "ver\u00b7gr\u00f6s\u00b7sert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KOKOM", "ART", "NN", "ART", "VVPP", "$,"], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ein Spiegel und cristallnes Na\u00df,", "tokens": ["Ein", "Spie\u00b7gel", "und", "cris\u00b7tall\u00b7nes", "Na\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das theils verh\u00e4rtet, theils durchw\u00e4ssert;", "tokens": ["Das", "theils", "ver\u00b7h\u00e4r\u00b7tet", ",", "theils", "durch\u00b7w\u00e4s\u00b7sert", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "ADV", "VVPP", "$,", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die Haut die drunter ist gespannt,", "tokens": ["Die", "Haut", "die", "drun\u00b7ter", "ist", "ge\u00b7spannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "PAV", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die gleichet einer dunklen Wand,", "tokens": ["Die", "glei\u00b7chet", "ei\u00b7ner", "dunk\u00b7len", "Wand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die in der Kunstmaschin ", "tokens": ["Die", "in", "der", "Kunst\u00b7ma\u00b7schin"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Woran im Schatten-Schein gedrukte Bilder", "tokens": ["Wo\u00b7ran", "im", "Schat\u00b7ten\u00b7Schein", "ge\u00b7druk\u00b7te", "Bil\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPRART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Die Ohren sehen fast so aus,", "tokens": ["Die", "Oh\u00b7ren", "se\u00b7hen", "fast", "so", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als wie ein Trichter wird formiret;", "tokens": ["Als", "wie", "ein", "Trich\u00b7ter", "wird", "for\u00b7mi\u00b7ret", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wie ein gewundnes Schnekken Haus,", "tokens": ["Wie", "ein", "ge\u00b7wund\u00b7nes", "Schnek\u00b7ken", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Wundernsw\u00fcrdig ausgezieret;", "tokens": ["Das", "Wun\u00b7derns\u00b7w\u00fcr\u00b7dig", "aus\u00b7ge\u00b7zie\u00b7ret", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Sie gleichen einem Labyrinth,", "tokens": ["Sie", "glei\u00b7chen", "ei\u00b7nem", "La\u00b7by\u00b7rinth", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Worin viel Jrreg\u00e4nge sind,", "tokens": ["Wo\u00b7rin", "viel", "Jr\u00b7re\u00b7g\u00e4n\u00b7ge", "sind", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Man findet in derselben Kammer,", "tokens": ["Man", "fin\u00b7det", "in", "der\u00b7sel\u00b7ben", "Kam\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Viel wunderbahr Ger\u00e4th, als Trommeln,", "tokens": ["Viel", "wun\u00b7der\u00b7bahr", "Ge\u00b7r\u00e4\u00b7th", ",", "als", "Trom\u00b7meln", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "NN", "$,", "KOUS", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Und wenn wir unser Augenmerk", "tokens": ["Und", "wenn", "wir", "un\u00b7ser", "Au\u00b7gen\u00b7merk"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Aufs ofne Thor ", "tokens": ["Aufs", "of\u00b7ne", "Thor"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "So kan man drin ein R\u00e4derwerk", "tokens": ["So", "kan", "man", "drin", "ein", "R\u00e4\u00b7der\u00b7werk"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PIS", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und kleine M\u00fchlensteine sehen,", "tokens": ["Und", "klei\u00b7ne", "M\u00fch\u00b7len\u00b7stei\u00b7ne", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und andre Kunstger\u00e4the mehr.", "tokens": ["Und", "and\u00b7re", "Kunst\u00b7ge\u00b7r\u00e4\u00b7the", "mehr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.7": {"text": "Als wie ein Schwerd das ausgezogen,", "tokens": ["Als", "wie", "ein", "Schwerd", "das", "aus\u00b7ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "ART", "NN", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Als wie ein spizzer Pfeil auf den gespannten", "tokens": ["Als", "wie", "ein", "spiz\u00b7zer", "Pfeil", "auf", "den", "ge\u00b7spann\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "KOKOM", "ART", "ADJA", "NN", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Der Vorderz\u00e4hne dichte Reihn,", "tokens": ["Der", "Vor\u00b7der\u00b7z\u00e4h\u00b7ne", "dich\u00b7te", "Reihn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die sich der Zung entgegen stellen,", "tokens": ["Die", "sich", "der", "Zung", "ent\u00b7ge\u00b7gen", "stel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ART", "NN", "PTKVZ", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die k\u00f6nnen Pallisaden seyn,", "tokens": ["Die", "k\u00f6n\u00b7nen", "Pal\u00b7li\u00b7sa\u00b7den", "seyn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gesezt auf denen hohen W\u00e4llen.", "tokens": ["Ge\u00b7sezt", "auf", "de\u00b7nen", "ho\u00b7hen", "W\u00e4l\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PRELS", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Sie sind wie Stacheln spiz gewezt,", "tokens": ["Sie", "sind", "wie", "Sta\u00b7cheln", "spiz", "ge\u00b7wezt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "KOKOM", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wer sich dran wagt, der wird verlezt;", "tokens": ["Wer", "sich", "dran", "wagt", ",", "der", "wird", "ver\u00b7lezt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "PAV", "VVFIN", "$,", "PRELS", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.8": {"text": "Ein ausgebreitet Thor mit zwei geschlo\u00dfnen", "tokens": ["Ein", "aus\u00b7ge\u00b7brei\u00b7tet", "Thor", "mit", "zwei", "ge\u00b7schlo\u00df\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "VVPP", "NN", "APPR", "CARD", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Sehn wir des Leibes Bauwerk an,", "tokens": ["Sehn", "wir", "des", "Lei\u00b7bes", "Bau\u00b7werk", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "O! wie viel sch\u00f6ne Kunstmaschinen!", "tokens": ["O", "!", "wie", "viel", "sch\u00f6\u00b7ne", "Kunst\u00b7ma\u00b7schi\u00b7nen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PWAV", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sind hier vereint, und jede kan,", "tokens": ["Sind", "hier", "ver\u00b7eint", ",", "und", "je\u00b7de", "kan", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "VVPP", "$,", "KON", "PIS", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Uns sicher zum Beweisthum dienen:", "tokens": ["Uns", "si\u00b7cher", "zum", "Be\u00b7wei\u00b7sthum", "die\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Da\u00df eines weisen Meisters Macht,", "tokens": ["Da\u00df", "ei\u00b7nes", "wei\u00b7sen", "Meis\u00b7ters", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dieselben all herf\u00fcrgebracht;", "tokens": ["Die\u00b7sel\u00b7ben", "all", "her\u00b7f\u00fcr\u00b7ge\u00b7bracht", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und unter sich also verbunden,", "tokens": ["Und", "un\u00b7ter", "sich", "al\u00b7so", "ver\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PRF", "ADV", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.8": {"text": "Wie er es zu dem Zwek bequem hat ausge-", "tokens": ["Wie", "er", "es", "zu", "dem", "Zwek", "be\u00b7quem", "hat", "aus\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PPER", "APPR", "ART", "NN", "ADJD", "VAFIN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Die Brust l\u00e4st ein Gew\u00f6lbe sehn,", "tokens": ["Die", "Brust", "l\u00e4st", "ein", "Ge\u00b7w\u00f6l\u00b7be", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN",