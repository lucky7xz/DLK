{"textgrid.poem.65081": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Auf den Bl\u00e4ttern, welche deine Hand", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf den Bl\u00e4ttern, welche deine Hand", "tokens": ["Auf", "den", "Bl\u00e4t\u00b7tern", ",", "wel\u00b7che", "dei\u00b7ne", "Hand"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PPOSAT", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Einst beschrieb mit Liebeshieroglyphen,", "tokens": ["Einst", "be\u00b7schrieb", "mit", "Lie\u00b7bes\u00b7hie\u00b7ro\u00b7gly\u00b7phen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Ruht mein Auge sinnend, unverwandt,", "tokens": ["Ruht", "mein", "Au\u00b7ge", "sin\u00b7nend", ",", "un\u00b7ver\u00b7wandt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "VVPP", "$,", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Und mir ist, als ob sie mahnend riefen:", "tokens": ["Und", "mir", "ist", ",", "als", "ob", "sie", "mah\u00b7nend", "rie\u00b7fen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "$,", "KOKOM", "KOUS", "PPER", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "\u00bbtr\u00fcbe Seele, welche unbewegt", "tokens": ["\u00bb", "tr\u00fc\u00b7be", "See\u00b7le", ",", "wel\u00b7che", "un\u00b7be\u00b7wegt"], "token_info": ["punct", "word", "word", "punct", "word", "word"], "pos": ["$(", "ADJA", "NN", "$,