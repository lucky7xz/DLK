{"dta.poem.5967": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Bau der Welt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn wir in diesen tieffen H\u00f6hen", "tokens": ["Wenn", "wir", "in", "die\u00b7sen", "tief\u00b7fen", "H\u00f6\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So mancherley Gestalt so vieler Sterne sehen;", "tokens": ["So", "man\u00b7cher\u00b7ley", "Ge\u00b7stalt", "so", "vie\u00b7ler", "Ster\u00b7ne", "se\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "ADV", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So fraget sichs: auf welche Weise werden", "tokens": ["So", "fra\u00b7get", "sichs", ":", "auf", "wel\u00b7che", "Wei\u00b7se", "wer\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "$.", "APPR", "PWAT", "NN", "VAINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So viele Sonnen, so viel\u2019 Erden", "tokens": ["So", "vie\u00b7le", "Son\u00b7nen", ",", "so", "viel'", "Er\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "PIAT", "NN", "$,", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "In dieser Wirbel Schoo\u00df, in welchen sie verschrenckt?", "tokens": ["In", "die\u00b7ser", "Wir\u00b7bel", "Schoo\u00df", ",", "in", "wel\u00b7chen", "sie", "ver\u00b7schrenckt", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "NN", "$,", "APPR", "PWAT", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Es fraget sich, auf welche Weise,", "tokens": ["Es", "fra\u00b7get", "sich", ",", "auf", "wel\u00b7che", "Wei\u00b7se", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "$,", "APPR", "PWAT", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.7": {"text": "Von allen Ecken her, von ihrem \u00e4ussern Creyse,", "tokens": ["Von", "al\u00b7len", "E\u00b7cken", "her", ",", "von", "ih\u00b7rem", "\u00e4us\u00b7sern", "Crey\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PTKVZ", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die Himmels-Lufft, in ihrer Fl\u00fcssigkeit,", "tokens": ["Die", "Him\u00b7mels\u00b7Lufft", ",", "in", "ih\u00b7rer", "Fl\u00fcs\u00b7sig\u00b7keit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Sich so gewaltig sencket", "tokens": ["Sich", "so", "ge\u00b7wal\u00b7tig", "sen\u00b7cket"], "token_info": ["word", "word", "word", "word"], "pos": ["PRF", "ADV", "ADJD", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Nach einem Mittel-Punct von C\u00f6rpern, die nicht gleich?", "tokens": ["Nach", "ei\u00b7nem", "Mit\u00b7tel\u00b7Punct", "von", "C\u00f6r\u00b7pern", ",", "die", "nicht", "gleich", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NE", "$,", "PRELS", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Hier wird, da alles sich mit Macht im Creyse f\u00fchrt;", "tokens": ["Hier", "wird", ",", "da", "al\u00b7les", "sich", "mit", "Macht", "im", "Crey\u00b7se", "f\u00fchrt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "$,", "KOUS", "PIS", "PRF", "APPR", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Jm Mittel-Punet ein helles Rund formirt:", "tokens": ["Jm", "Mit\u00b7tel\u00b7Pu\u00b7net", "ein", "hel\u00b7les", "Rund", "for\u00b7mirt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "Und dort auf gleiche Weise,", "tokens": ["Und", "dort", "auf", "glei\u00b7che", "Wei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Ein dunckles Rund in einem andern Creyse.", "tokens": ["Ein", "dunck\u00b7les", "Rund", "in", "ei\u00b7nem", "an\u00b7dern", "Crey\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}