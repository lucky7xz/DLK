{"textgrid.poem.54688": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "2. Uber Nicolai Copernici Bild", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du dreymal weiser Geist/ du mehr denn grosser Mann!", "tokens": ["Du", "drey\u00b7mal", "wei\u00b7ser", "Geist", "/", "du", "mehr", "denn", "gros\u00b7ser", "Mann", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJA", "NN", "$(", "PPER", "ADV", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dem nicht die Nacht der Zeit die alles pochen kan/", "tokens": ["Dem", "nicht", "die", "Nacht", "der", "Zeit", "die", "al\u00b7les", "po\u00b7chen", "kan", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PTKNEG", "ART", "NN", "ART", "NN", "ART", "PIS", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dem nicht der herbe Neyd die Sinnen hat gebunden/", "tokens": ["Dem", "nicht", "der", "her\u00b7be", "Neyd", "die", "Sin\u00b7nen", "hat", "ge\u00b7bun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PTKNEG", "ART", "ADJA", "NN", "ART", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Sinnen/ die den Lauff der Erden new gefunden.", "tokens": ["Die", "Sin\u00b7nen", "/", "die", "den", "Lauff", "der", "Er\u00b7den", "new", "ge\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "ART", "NN", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der du der alten Tr\u00e4um und Dunckel widerlegt:", "tokens": ["Der", "du", "der", "al\u00b7ten", "Tr\u00e4um", "und", "Dun\u00b7ckel", "wi\u00b7der\u00b7legt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "ADJA", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und Recht uns dargethan was lebt und was sich regt:", "tokens": ["Und", "Recht", "uns", "dar\u00b7ge\u00b7than", "was", "lebt", "und", "was", "sich", "regt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PPER", "PAV", "PWS", "VVFIN", "KON", "PWS", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Schaw itzund bl\u00fcht dein Ruhm/ den als auff einem Wagen/", "tokens": ["Schaw", "it\u00b7zund", "bl\u00fcht", "dein", "Ruhm", "/", "den", "als", "auff", "ei\u00b7nem", "Wa\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVFIN", "PPOSAT", "NN", "$(", "ART", "KOKOM", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Der Krei\u00df auff dem wir sind mu\u00df umb die Sonnen tragen.", "tokens": ["Der", "Krei\u00df", "auff", "dem", "wir", "sind", "mu\u00df", "umb", "die", "Son\u00b7nen", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PRELS", "PPER", "VAFIN", "VMFIN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wann di\u00df was irrdisch ist/ wird mit der Zeit vergehn/", "tokens"