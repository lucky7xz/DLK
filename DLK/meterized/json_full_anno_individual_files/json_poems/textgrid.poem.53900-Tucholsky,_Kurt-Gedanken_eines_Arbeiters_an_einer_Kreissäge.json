 ["NN", "$,", "ADV", "ADV", "APPR", "PDAT", "ADJA", "NN", "$."], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.13": {"text": "Junge, einmal in die frische Winterluft!", "tokens": ["Jun\u00b7ge", ",", "ein\u00b7mal", "in", "die", "fri\u00b7sche", "Win\u00b7ter\u00b7luft", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.14": {"text": "Sich ausruhn, laufen, springen, sich sielen im Schnee \u2013", "tokens": ["Sich", "aus\u00b7ruhn", ",", "lau\u00b7fen", ",", "sprin\u00b7gen", ",", "sich", "sie\u00b7len", "im", "Schnee", "\u2013"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVINF", "$,", "VVFIN", "$,", "VVFIN", "$,", "PRF", "VVFIN", "APPRART", "NN", "$("], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.15": {"text": "hinterher tun einem so sch\u00f6n die Beine weh \u2013", "tokens": ["hin\u00b7ter\u00b7her", "tun", "ei\u00b7nem", "so", "sch\u00f6n", "die", "Bei\u00b7ne", "weh", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "ART", "ADV", "ADJD", "ART", "NN", "PTKVZ", "$("], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.16": {"text": "Und zu sehen, wie der Himmel glasig wird . . .", "tokens": ["Und", "zu", "se\u00b7hen", ",", "wie", "der", "Him\u00b7mel", "gla\u00b7sig", "wird", ".", ".", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["KON", "PTKZU", "VVINF", "$,", "PWAV", "ART", "NN", "ADJD", "VAFIN", "$.", "$.", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.17": {"text": "und jetzt k\u00f6nnen wir nicht mehr weiter, wir haben uns verirrt \u2013", "tokens": ["und", "jetzt", "k\u00f6n\u00