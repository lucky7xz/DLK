{"textgrid.poem.53900": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Gedanken eines Arbeiters an einer Kreiss\u00e4ge", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kreisch \u2013", "tokens": ["Kreisch", "\u2013"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "+", "measure": "single.up"}, "line.2": {"text": "kreisch \u2013", "tokens": ["kreisch", "\u2013"], "token_info": ["word", "punct"], "pos": ["ADJD", "$("], "meter": "+", "measure": "single.up"}, "line.3": {"text": "das Holz schreit, als ginge es ihm ins Fleisch . . .", "tokens": ["das", "Holz", "schreit", ",", "als", "gin\u00b7ge", "es", "ihm", "ins", "Fleisch", ".", ".", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "KOKOM", "VVFIN", "PPER", "PPER", "APPRART", "NN", "$.", "$.", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Kalt heute.", "tokens": ["Kalt", "heu\u00b7te", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "ADV", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "W\u00e4hrend ich hier steh,", "tokens": ["W\u00e4h\u00b7rend", "ich", "hier", "steh", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "laufen andere Leute durch den dicken Winterschnee.", "tokens": ["lau\u00b7fen", "an\u00b7de\u00b7re", "Leu\u00b7te", "durch", "den", "di\u00b7cken", "Win\u00b7ter\u00b7schnee", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.7": {"text": "Oben, auf den Bergen. Ja.", "tokens": ["O\u00b7ben", ",", "auf", "den", "Ber\u00b7gen", ".", "Ja", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "$,", "APPR", "ART", "NN", "$.", "PTKANT", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Die habens feiner", "tokens": ["Die", "ha\u00b7bens", "fei\u00b7ner"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "als unsereiner.", "tokens": ["als", "un\u00b7ser\u00b7ei\u00b7ner", "."], "token_info": ["word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.10": {"text": "Dies Jahr", "tokens": ["Dies", "Jahr"], "token_info": ["word", "word"], "pos": ["PDS", "NN"], "meter": "-+", "measure": "iambic.single"}, "line.11": {"text": "ist es das erstemal, da\u00df ich auf Urlaub war \u2013", "tokens": ["ist", "es", "das", "ers\u00b7te\u00b7mal", ",", "da\u00df", "ich", "auf", "Ur\u00b7laub", "war", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PDS", "ADV", "$,", "KOUS", "PPER", "APPR", "NN", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "seit dem Krieg. Acht Tage war ich in Ostpreu\u00dfen oben \u2013", "tokens": ["seit", "dem", "Krieg", ".", "Acht", "Ta\u00b7ge", "war", "ich", "in", "Ost\u00b7preu\u00b7\u00dfen", "o\u00b7ben", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$.", "CARD", "NN", "VAFIN", "PPER", "APPR", "NN", "ADV", "$("], "meter": "+-+-+-+--+--+-", "measure": "hexameter"}, "line.13": {"text": "Verflucht! jetzt hat sich der Antrieb nach links verschoben \u2013", "tokens": ["Ver\u00b7flucht", "!", "jetzt", "hat", "sich", "der", "An\u00b7trieb", "nach", "links", "ver\u00b7scho\u00b7ben", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$.", "ADV", "VAFIN", "PRF", "ART", "NN", "APPR", "ADV", "VVPP", "$("], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "Willi! Schalt aus!", "tokens": ["Wil\u00b7li", "!", "Schalt", "aus", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$.", "NN", "PTKVZ", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.15": {"text": "Schalt ein!", "tokens": ["Schalt", "ein", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKVZ", "$."], "meter": "-+", "measure": "iambic.single"}, "line.16": {"text": "Kreisch \u2013 kreisch \u2013 \u2013 \u2013", "tokens": ["Kreisch", "\u2013", "kreisch", "\u2013", "\u2013", "\u2013"], "token_info": ["word", "punct", "word", "punct", "punct", "punct"], "pos": ["NN", "$(", "ADJD", "$(", "$(", "$("], "meter": "+-", "measure": "trochaic.single"}}, "stanza.2": {"line.1": {"text": "Die fahren Schlitten und auf so langen Dingern: die", "tokens": ["Die", "fah\u00b7ren", "Schlit\u00b7ten", "und", "auf", "so", "lan\u00b7gen", "Din\u00b7gern", ":", "die"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "ADJA", "NN", "KON", "APPR", "ADV", "ADJA", "NN", "$.", "ART"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "hei\u00dfen Ski.", "tokens": ["hei\u00b7\u00dfen", "Ski", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Ich kann das nie.", "tokens": ["Ich", "kann", "das", "nie", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PDS", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Ich steh hier, in der Fabrik, tagaus, tagein", "tokens": ["Ich", "steh", "hier", ",", "in", "der", "Fab\u00b7rik", ",", "ta\u00b7gaus", ",", "ta\u00b7gein"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "ADV", "$,", "APPR", "ART", "NN", "$,", "ADV", "$,", "NN"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "und schiebe Holz in die olle Molle rein.", "tokens": ["und", "schie\u00b7be", "Holz", "in", "die", "ol\u00b7le", "Mol\u00b7le", "rein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Kaum, da\u00df ich mich mal an die Maschine lehne . . .", "tokens": ["Kaum", ",", "da\u00df", "ich", "mich", "mal", "an", "die", "Ma\u00b7schi\u00b7ne", "leh\u00b7ne", ".", ".", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "PRF", "ADV", "APPR", "ART", "NN", "VVFIN", "$.", "$.", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Und die S\u00e4gesp\u00e4ne", "tokens": ["Und", "die", "S\u00e4\u00b7ge\u00b7sp\u00e4\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["KON", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "fliegen rum und bringen einen zum Husten!", "tokens": ["flie\u00b7gen", "rum", "und", "brin\u00b7gen", "ei\u00b7nen", "zum", "Hus\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "KON", "VVFIN", "ART", "APPRART", "NN", "$."], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.9": {"text": "Saugvorrichtung? Der Alte wird dir was pusten!", "tokens": ["Saug\u00b7vor\u00b7rich\u00b7tung", "?", "Der", "Al\u00b7te", "wird", "dir", "was", "pus\u00b7ten", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ART", "NN", "VAFIN", "PPER", "PIS", "VVINF", "$."], "meter": "+-+--+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.10": {"text": "Jetzt rutschen sie da in M\u00fcnchen immer munter", "tokens": ["Jetzt", "rut\u00b7schen", "sie", "da", "in", "M\u00fcn\u00b7chen", "im\u00b7mer", "mun\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "NE", "ADV", "ADJD"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "die beschneiten Berge runter \u2013", "tokens": ["die", "be\u00b7schnei\u00b7ten", "Ber\u00b7ge", "run\u00b7ter", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Mensch, einmal raus aus dieser verdammten Kluft!", "tokens": ["Mensch", ",", "ein\u00b7mal", "raus", "aus", "die\u00b7ser", "ver\u00b7damm\u00b7ten", "Kluft", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "ADV", "APPR", "PDAT", "ADJA", "NN", "$."], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.13": {"text": "Junge, einmal in die frische Winterluft!", "tokens": ["Jun\u00b7ge", ",", "ein\u00b7mal", "in", "die", "fri\u00b7sche", "Win\u00b7ter\u00b7luft", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.14": {"text": "Sich ausruhn, laufen, springen, sich sielen im Schnee \u2013", "tokens": ["Sich", "aus\u00b7ruhn", ",", "lau\u00b7fen", ",", "sprin\u00b7gen", ",", "sich", "sie\u00b7len", "im", "Schnee", "\u2013"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVINF", "$,", "VVFIN", "$,", "VVFIN", "$,", "PRF", "VVFIN", "APPRART", "NN", "$("], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.15": {"text": "hinterher tun einem so sch\u00f6n die Beine weh \u2013", "tokens": ["hin\u00b7ter\u00b7her", "tun", "ei\u00b7nem", "so", "sch\u00f6n", "die", "Bei\u00b7ne", "weh", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "ART", "ADV", "ADJD", "ART", "NN", "PTKVZ", "$("], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.16": {"text": "Und zu sehen, wie der Himmel glasig wird . . .", "tokens": ["Und", "zu", "se\u00b7hen", ",", "wie", "der", "Him\u00b7mel", "gla\u00b7sig", "wird", ".", ".", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["KON", "PTKZU", "VVINF", "$,", "PWAV", "ART", "NN", "ADJD", "VAFIN", "$.", "$.", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.17": {"text": "und jetzt k\u00f6nnen wir nicht mehr weiter, wir haben uns verirrt \u2013", "tokens": ["und", "jetzt", "k\u00f6n\u00b7nen", "wir", "nicht", "mehr", "wei\u00b7ter", ",", "wir", "ha\u00b7ben", "uns", "ver\u00b7irrt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PPER", "PTKNEG", "ADV", "PTKVZ", "$,", "PPER", "VAFIN", "PPER", "VVPP", "$("], "meter": "--+--+-+--+-+-+", "measure": "anapaest.di.plus"}, "line.18": {"text": "und ich trage der Marie die Dinger, die Skier \u2013 und dann kommen wir nach Haus . . .", "tokens": ["und", "ich", "tra\u00b7ge", "der", "Ma\u00b7rie", "die", "Din\u00b7ger", ",", "die", "Skier", "\u2013", "und", "dann", "kom\u00b7men", "wir", "nach", "Haus", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NE", "ART", "NN", "$,", "ART", "NN", "$(", "KON", "ADV", "VVFIN", "PPER", "APPR", "NN", "$.", "$.", "$."], "meter": "--+---+-+--+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.19": {"text": "Willi! Schalt aus!", "tokens": ["Wil\u00b7li", "!", "Schalt", "aus", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$.", "NN", "PTKVZ", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.20": {"text": "Ausschalten \u2013!", "tokens": ["Aus\u00b7schal\u00b7ten", "\u2013", "!"], "token_info": ["word", "punct", "punct"], "pos": ["NN", "$(", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.21": {"text": "Kreisch \u2013 kreisch \u2013 \u2013 \u2013", "tokens": ["Kreisch", "\u2013", "kreisch", "\u2013", "\u2013", "\u2013"], "token_info": ["word", "punct", "word", "punct", "punct", "punct"], "pos": ["NN", "$(", "ADJD", "$(", "$(", "$("], "meter": "+-", "measure": "trochaic.single"}}, "stanza.3": {"line.1": {"text": "Kreisch \u2013", "tokens": ["Kreisch", "\u2013"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "+", "measure": "single.up"}, "line.2": {"text": "kreisch \u2013", "tokens": ["kreisch", "\u2013"], "token_info": ["word", "punct"], "pos": ["ADJD", "$("], "meter": "+", "measure": "single.up"}, "line.3": {"text": "das Holz schreit, als ginge es ihm ins Fleisch . . .", "tokens": ["das", "Holz", "schreit", ",", "als", "gin\u00b7ge", "es", "ihm", "ins", "Fleisch", ".", ".", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "KOKOM", "VVFIN", "PPER", "PPER", "APPRART", "NN", "$.", "$.", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Kalt heute.", "tokens": ["Kalt", "heu\u00b7te", "."], "token_info": ["word", "word