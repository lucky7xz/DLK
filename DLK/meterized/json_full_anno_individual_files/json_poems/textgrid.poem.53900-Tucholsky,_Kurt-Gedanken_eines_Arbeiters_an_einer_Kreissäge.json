n\u00b7gern", ":", "die"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "ADJA", "NN", "KON", "APPR", "ADV", "ADJA", "NN", "$.", "ART"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "hei\u00dfen Ski.", "tokens": ["hei\u00b7\u00dfen", "Ski", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Ich kann das nie.", "tokens": ["Ich", "kann", "das", "nie", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PDS", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Ich steh hier, in der Fabrik, tagaus, tagein", "tokens": ["Ich", "steh", "hier", ",", "in", "der", "Fab\u00b7rik", ",", "ta\u00b7gaus", ",", "ta\u00b7gein"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "ADV", "$,", "APPR", "ART", "NN", "$,", "ADV", "$,", "NN"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "und schiebe Holz in die olle Molle rein.", "tokens": ["und", "schie\u00b7be", "Holz", "in", "die", "ol\u00b7le", "Mol\u00b7le", "rein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Kaum, da\u00df ich mich mal an die Maschine lehne . . .", "tokens": ["Kaum", ",", "da\u00df", "ich", "mich", "mal", "an", "die", "Ma\u00b7schi\u00b7ne", "leh\u00b7ne", ".", ".", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "PRF", "ADV", "APPR", "ART", "NN", "VVFIN", "$.", "$.", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "lin