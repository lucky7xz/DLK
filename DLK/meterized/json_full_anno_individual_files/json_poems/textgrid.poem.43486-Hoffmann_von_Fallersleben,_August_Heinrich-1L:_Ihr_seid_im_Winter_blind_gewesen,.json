{"textgrid.poem.43486": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ihr seid im Winter blind gewesen,", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr seid im Winter blind gewesen,", "tokens": ["Ihr", "seid", "im", "Win\u00b7ter", "blind", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "ADJD", "VAPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Jetzt ist es Fr\u00fchling, jetzt lernt lesen \u2013", "tokens": ["Jetzt", "ist", "es", "Fr\u00fch\u00b7ling", ",", "jetzt", "lernt", "le\u00b7sen", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "$,", "ADV", "VVFIN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Den Spiritus und Circumfler!", "tokens": ["Den", "Spi\u00b7ri\u00b7tus", "und", "Cir\u00b7cum\u00b7fler", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Herbei, ihr kritischen Soldaten,", "tokens": ["Her\u00b7bei", ",", "ihr", "kri\u00b7ti\u00b7schen", "Sol\u00b7da\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PTKVZ", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Mit Apparat und mit Citaten!", "tokens": ["Mit", "Ap\u00b7pa\u00b7rat", "und", "mit", "Ci\u00b7ta\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Hier hat der Coder einen Klecks.", "tokens": ["Hier", "hat", "der", "Co\u00b7der", "ei\u00b7nen", "Klecks", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wer gl\u00fccklich diesen Klecks durchdrungen", "tokens": ["Wer", "gl\u00fcck\u00b7lich", "die\u00b7sen", "Klecks", "durch\u00b7drun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "ADJD", "PDAT", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dem ist der gro\u00dfe Wurf gelungen,", "tokens": ["Dem", "ist", "der", "gro\u00b7\u00dfe", "Wurf", "ge\u00b7lun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Zu sein ein gro\u00dfer Humanist.", "tokens": ["Zu", "sein", "ein", "gro\u00b7\u00dfer", "Hu\u00b7ma\u00b7nist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und geht die ganze Welt verloren,", "tokens": ["Und", "geht", "die", "gan\u00b7ze", "Welt", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "So bleiben ihm doch die Autoren,", "tokens": ["So", "blei\u00b7ben", "ihm", "doch", "die", "Au\u00b7to\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und wenn er stirbt, er stirbt als \u2013 Christ.", "tokens": ["Und", "wenn", "er", "