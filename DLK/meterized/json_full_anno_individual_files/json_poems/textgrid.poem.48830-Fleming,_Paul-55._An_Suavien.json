{"textgrid.poem.48830": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "55. An Suavien", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich t\u00e4t' es, ", "tokens": ["Ich", "t\u00e4t'", "es", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "die ganze halbe Nacht, gleich als du mir versprochen.", "tokens": ["die", "gan\u00b7ze", "hal\u00b7be", "Nacht", ",", "gleich", "als", "du", "mir", "ver\u00b7spro\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,", "ADV", "KOUS", "PPER", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wie kams dann, da\u00df du mir die Treue hast gebrochen?", "tokens": ["Wie", "kams", "dann", ",", "da\u00df", "du", "mir", "die", "Treu\u00b7e", "hast", "ge\u00b7bro\u00b7chen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ADV", "$,", "KOUS", "PPER", "PPER", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Immittels starb' ich fast f\u00fcr schmerzlicher Begier.", "tokens": ["Im\u00b7mit\u00b7tels", "sta\u00b7rb'", "ich", "fast", "f\u00fcr", "schmerz\u00b7li\u00b7cher", "Be\u00b7gier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}}, "stanza.2": {"line.1": {"text": "Zuletzte lie\u00df ich dir noch einen Ku\u00df alhier,", "tokens": ["Zu\u00b7letz\u00b7te", "lie\u00df", "ich", "dir", "noch", "ei\u00b7nen", "Ku\u00df", "al\u00b7hier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PPER", "ADV", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "f\u00fcr dem auch hast da dich aus \u00dcbermut verkrochen,", "tokens": ["f\u00fcr", "dem", "auch", "hast", "da", "dich", "aus", "\u00dc\u00b7ber\u00b7mut", "ver\u00b7kro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ADV", "VAFIN", "ADV", "PRF", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "wie sehr er dich gesucht bei einer halben Wochen.", "tokens": ["wie", "sehr", "er", "dich", "ge\u00b7sucht", "bei", "ei\u00b7ner", "hal\u00b7ben", "Wo\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPER", "PRF", "VVPP", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Itzt kommt er wieder matt und ohne Trost zu mir.", "tokens": ["Itzt", "kommt", "er", "wie\u00b7der", "matt", "und", "oh\u00b7ne", "Trost", "zu", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADJD", "KON", "APPR", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Die Ursach' h\u00f6r' ich itzt, dir sei zu Ohren kommen,", "tokens": ["Die", "Ur\u00b7sach'", "h\u00f6r'", "ich", "itzt", ",", "dir", "sei", "zu", "Oh\u00b7ren", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "$,", "PPER", "VAFIN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "als h\u00e4tt' ich ", "tokens": ["als", "h\u00e4tt'", "ich"], "token_info": ["word", "word", "word"], "pos": ["KOKOM", "VAFIN", "PPER"], "meter": "+--", "measure": "dactylic.init"}, "line.3": {"text": "nein, Licht, nein, gl\u00e4ub' es nicht! Es leugt sich itzund viel.", "tokens": ["nein", ",", "Licht", ",", "nein", ",", "gl\u00e4ub'", "es", "nicht", "!", "Es", "leugt", "sich", "it\u00b7zund", "viel", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "NN", "$,", "PTKANT", "$,", "VVFIN", "PPER", "PTKNEG", "$.", "PPER", "VVFIN", "PRF", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Wie ofte wird gesagt, du meinest mehr als Einen.", "tokens": ["Wie", "of\u00b7te", "wird", "ge\u00b7sagt", ",", "du", "mei\u00b7nest", "mehr", "als", "Ei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "VAFIN", "VVPP", "$,", "PPER", "VVFIN", "PIS", "KOKOM", "ART", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich h\u00f6re, was ich mu\u00df, und gl\u00e4ube, was ich will.", "tokens": ["Ich", "h\u00f6\u00b7re", ",", "was", "ich", "mu\u00df", ",", "und", "gl\u00e4u\u00b7be", ",", "was", "ich", "will", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWS", "PPER", "VMFIN", "$,", "KON", "VVFIN", "$,", "PWS", "PPER", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Du wirst es nimmermehr ja nicht so b\u00f6se meinen.", "tokens": ["Du", "wirst", "es