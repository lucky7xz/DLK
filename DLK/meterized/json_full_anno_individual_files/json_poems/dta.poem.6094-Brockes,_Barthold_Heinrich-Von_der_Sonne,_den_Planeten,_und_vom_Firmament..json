{"dta.poem.6094": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Sonne, den Planeten,  \n und vom Firmament.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein ertzbetr\u00fcglicher Astrologus will lesen", "tokens": ["Ein", "ertz\u00b7be\u00b7tr\u00fcg\u00b7li\u00b7cher", "Ast\u00b7ro\u00b7lo\u00b7gus", "will", "le\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VMFIN", "VVINF"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Am Himmel, was von je uns unbekannt gewesen,", "tokens": ["Am", "Him\u00b7mel", ",", "was", "von", "je", "uns", "un\u00b7be\u00b7kannt", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "APPR", "ADV", "PPER", "ADJD", "VAPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das, was dem Menschen einst begegnen sollen,", "tokens": ["Das", ",", "was", "dem", "Men\u00b7schen", "einst", "be\u00b7geg\u00b7nen", "sol\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "ART", "NN", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Jhr Leben, ihren Tod. Sie glauben gar zu sehn", "tokens": ["Ihr", "Le\u00b7ben", ",", "ih\u00b7ren", "Tod", ".", "Sie", "glau\u00b7ben", "gar", "zu", "sehn"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$.", "PPER", "VVFIN", "ADV", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Jm Schoo\u00df des grossen Nichts, was nimmer soll geschehn.", "tokens": ["Jm", "Schoo\u00df", "des", "gros\u00b7sen", "Nichts", ",", "was", "nim\u00b7mer", "soll", "ge\u00b7schehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$,", "PRELS", "ADV", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}