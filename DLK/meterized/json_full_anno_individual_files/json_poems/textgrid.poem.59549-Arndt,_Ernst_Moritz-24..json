{"textgrid.poem.59549": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "24.", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Erde ist ein Garten", "tokens": ["Die", "Er\u00b7de", "ist", "ein", "Gar\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Voll s\u00fc\u00dfer Bl\u00fcmelein,", "tokens": ["Voll", "s\u00fc\u00b7\u00dfer", "Bl\u00fc\u00b7me\u00b7lein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gott selbst will ihrer warten", "tokens": ["Gott", "selbst", "will", "ih\u00b7rer", "war\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "VMFIN", "PPOSAT", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und gerne G\u00e4rtner sein,", "tokens": ["Und", "ger\u00b7ne", "G\u00e4rt\u00b7ner", "sein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "NN", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Will ihrer sp\u00e4t und fr\u00fch", "tokens": ["Will", "ih\u00b7rer", "sp\u00e4t", "und", "fr\u00fch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPOSAT", "ADJD", "KON", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "In frommer Treue pflegen,", "tokens": ["In", "from\u00b7mer", "Treu\u00b7e", "pfle\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Mit Sonnenschein und Regen", "tokens": ["Mit", "Son\u00b7nen\u00b7schein", "und", "Re\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und Tau erquicken sie.", "tokens": ["Und", "Tau", "er\u00b7qui\u00b7cken", "sie", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die erste Blum' vor allen", "tokens": ["Die", "ers\u00b7te", "Blum'", "vor", "al\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "PIAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das mu\u00df ", "tokens": ["Das", "mu\u00df"], "token_info": ["word", "word"], "pos": ["PDS", "VMFIN"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Der Menschen Wohlgefallen,", "tokens": ["Der", "Men\u00b7schen", "Wohl\u00b7ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Engel sch\u00f6nster Schein:", "tokens": ["Der", "En\u00b7gel", "sch\u00f6ns\u00b7ter", "Schein", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sie ist ", "tokens": ["Sie", "ist"], "token_info": ["word", "word"], "pos": ["PPER", "VAFIN"], "meter": "-+", "measure": "iambic.single"}, "line.6": {"text": "Und mu\u00df auf Dornen stehen,", "tokens": ["Und", "mu\u00df", "auf", "Dor\u00b7nen", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Sobald die Winde wehen,", "tokens": ["So\u00b7bald", "die", "Win\u00b7de", "we\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ist ihre Sch\u00f6ne tot.", "tokens": ["Ist", "ih\u00b7re", "Sch\u00f6\u00b7ne", "tot", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Die zweite, die Gott liebet", "tokens": ["Die", "zwei\u00b7te", ",", "die", "Gott", "lie\u00b7bet"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "$,", "ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "N\u00e4chst Liebe allerbest',", "tokens": ["N\u00e4chst", "Lie\u00b7be", "al\u00b7ler\u00b7best'", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ist, die das Gute \u00fcbet", "tokens": ["Ist", ",", "die", "das", "Gu\u00b7te", "\u00fc\u00b7bet"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "$,", "PRELS", "ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und sich nichts merken l\u00e4\u00dft;", "tokens": ["Und", "sich", "nichts", "mer\u00b7ken", "l\u00e4\u00dft", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "PIS", "VVINF", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ihr Name ", "tokens": ["Ihr", "Na\u00b7me"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "+--", "measure": "dactylic.init"}, "line.6": {"text": "Auf Erden auch ", "tokens": ["Auf", "Er\u00b7den", "auch"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NN", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Sie bl\u00fcht ein kurzes Weilchen", "tokens": ["Sie", "bl\u00fcht", "ein", "kur\u00b7zes", "Weil\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und kaum die Bl\u00fcte weist.", "tokens": ["Und", "kaum", "die", "Bl\u00fc\u00b7te", "weist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Sie duftet nur bei Nacht", "tokens": ["Sie", "duf\u00b7tet", "nur", "bei", "Nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "In aller Geister Mitte", "tokens": ["In", "al\u00b7ler", "Geis\u00b7ter", "Mit\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Bei voller Himmelspracht:", "tokens": ["Bei", "vol\u00b7ler", "Him\u00b7mel\u00b7spracht", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Da tut das Herz sich auf", "tokens": ["Da", "tut", "das", "Herz", "sich", "auf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "PRF", "APPR"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der frommen ", "tokens": ["Der", "from\u00b7men"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Wann hell von Pol zu Pole", "tokens": ["Wann", "hell", "von", "Pol", "zu", "Po\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "APPR", "NN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Sich schwingt der Sterne Lauf.", "tokens": ["Sich", "schwingt", "der", "Ster\u00b7ne", "Lauf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Auch ", "tokens": ["Auch"], "token_info": ["word"], "pos": ["ADV"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Ein liebes Gotteskind,", "tokens": ["Ein", "lie\u00b7bes", "Got\u00b7tes\u00b7kind", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wohl liebstes seiner Kinder,", "tokens": ["Wohl", "liebs\u00b7tes", "sei\u00b7ner", "Kin\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die nur hienieden sind.", "tokens": ["Die", "nur", "hien\u00b7ie\u00b7den", "sind", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Holdselig von Geb\u00e4rden,", "tokens": ["Hold\u00b7se\u00b7lig", "von", "Ge\u00b7b\u00e4r\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.6": {"text": "Du bist ihr Bild auf Erden,", "tokens": ["Du", "bist", "ihr", "Bild", "auf", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Kommst mit dem Lenz zugleich.", "tokens": ["Kommst", "mit", "dem", "Lenz", "zu\u00b7gleich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ADV", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.6": {"line.1": {"text": "Auch du, die im Gem\u00fcte", "tokens": ["Auch", "du", ",", "die", "im", "Ge\u00b7m\u00fc\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "PPER", "$,", "PRELS", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Best\u00e4ndig ist und treu,", "tokens": ["Be\u00b7st\u00e4n\u00b7dig", "ist", "und", "treu", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du, aller Zeiten Bl\u00fcte,", "tokens": ["Du", ",", "al\u00b7ler", "Zei\u00b7ten", "Bl\u00fc\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PIAT", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mir lieb gegr\u00fc\u00dfet sei!", "tokens": ["Mir", "lieb", "ge\u00b7gr\u00fc\u00b7\u00dfet", "sei", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVPP", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "O wolle Gott uns geben", "tokens": ["O", "wol\u00b7le", "Gott", "uns", "ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VMFIN", "NN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Solche Lieb' zu jeder Stund'!", "tokens": ["Sol\u00b7che", "Lieb'", "zu", "je\u00b7der", "Stund'", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Und du, die auf dem Throne", "tokens": ["Und", "du", ",", "die", "auf", "dem", "Thro\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Des Blumengartens sitzt", "tokens": ["Des", "Blu\u00b7men\u00b7gar\u00b7tens", "sitzt"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und mit der wei\u00dfen Krone", "tokens": ["Und", "mit", "der", "wei\u00b7\u00dfen", "Kro\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Gleich einem Engel blitzt,", "tokens": ["Gleich", "ei\u00b7nem", "En\u00b7gel", "blitzt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Du winkest lieb uns hinnen", "tokens": ["Du", "win\u00b7kest", "lieb", "uns", "hin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mit Herzen und mit Sinnen", "tokens": ["Mit", "Her\u00b7zen", "und", "mit", "Sin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Zur\u00fcck zum Paradies.", "tokens": ["Zu\u00b7r\u00fcck", "zum", "Pa\u00b7ra\u00b7dies", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Noch Blumen viel und Kr\u00e4uter", "tokens": ["Noch", "Blu\u00b7men", "viel", "und", "Kr\u00e4u\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "NN", "ADV", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hat Gott der G\u00e4rtner mehr,", "tokens": ["Hat", "Gott", "der", "G\u00e4rt\u00b7ner", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wer sie erz\u00e4hlte weiter,", "tokens": ["Wer", "sie", "er\u00b7z\u00e4hl\u00b7te", "wei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Z\u00e4hlt wohl den Sand am Meer:", "tokens": ["Z\u00e4hlt", "wohl", "den", "Sand", "am", "Meer", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wieviel er ausgestreut,", "tokens": ["Wie\u00b7viel", "er", "aus\u00b7ge\u00b7streut", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Wie k\u00f6nnt' ich alle z\u00e4hlen", "tokens": ["Wie", "k\u00f6nnt'", "ich", "al\u00b7le", "z\u00e4h\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "PPER", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Die zarten Blumenseelen", "tokens": ["Die", "zar\u00b7ten", "Blu\u00b7men\u00b7see\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Im bunten Sonnenkleid!", "tokens": ["Im", "bun\u00b7ten", "Son\u00b7nen\u00b7kleid", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Sollt' ich mir eine nehmen,", "tokens": ["Sollt'", "ich", "mir", "ei\u00b7ne", "neh\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "ART", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Lilie m\u00fc\u00dft' es sein,", "tokens": ["Die", "Li\u00b7lie", "m\u00fc\u00dft'", "es", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Steht wie ein Geisterschemen", "tokens": ["Steht", "wie", "ein", "Geis\u00b7ter\u00b7sche\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "KOKOM", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit hellem Himmelschein;", "tokens": ["Mit", "hel\u00b7lem", "Him\u00b7mel\u00b7schein", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wehm\u00fctig geht ihr Blick", "tokens": ["Weh\u00b7m\u00fc\u00b7tig", "geht", "ihr", "Blick"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Empor zum Licht der Sterne,", "tokens": ["Em\u00b7por", "zum", "Licht", "der", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Sie w\u00e4re gar zu gerne", "tokens": ["Sie", "w\u00e4\u00b7re", "gar", "zu", "ger\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "PTKA", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Zum Vaterland zur\u00fcck.", "tokens": ["Zum", "Va\u00b7ter\u00b7land", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "O G\u00e4rtner treu und milde,", "tokens": ["O", "G\u00e4rt\u00b7ner", "treu", "und", "mil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der alles kann und wei\u00df,", "tokens": ["Der", "al\u00b7les", "kann", "und", "wei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VMFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mach' mich zu ihrem Bilde,", "tokens": ["Mach'", "mich", "zu", "ih\u00b7rem", "Bil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mach' mich so rein und wei\u00df.", "tokens": ["Mach'", "mich", "so", "rein", "und", "wei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJD", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Dann kann ich droben froh", "tokens": ["Dann", "kann", "ich", "dro\u00b7ben", "froh"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Als Lilienm\u00e4dchen kommen", "tokens": ["Als", "Li\u00b7li\u00b7en\u00b7m\u00e4d\u00b7chen", "kom\u00b7men"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "NN", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Und unter allen Frommen", "tokens": ["Und", "un\u00b7ter", "al\u00b7len", "From\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "In Unschuld bl\u00fchen so.", "tokens": ["In", "Un\u00b7schuld", "bl\u00fc\u00b7hen", "so", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Die Erde ist ein Garten", "tokens": ["Die", "Er\u00b7de", "ist", "ein", "Gar\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Voll s\u00fc\u00dfer Bl\u00fcmelein,", "tokens": ["Voll", "s\u00fc\u00b7\u00dfer", "Bl\u00fc\u00b7me\u00b7lein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gott selbst will ihrer warten", "tokens": ["Gott", "selbst", "will", "ih\u00b7rer", "war\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "VMFIN", "PPOSAT", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und gerne G\u00e4rtner sein,", "tokens": ["Und", "ger\u00b7ne", "G\u00e4rt\u00b7ner", "sein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "NN", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Will ihrer sp\u00e4t und fr\u00fch", "tokens": ["Will", "ih\u00b7rer", "sp\u00e4t", "und", "fr\u00fch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPOSAT", "ADJD", "KON", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "In frommer Treue pflegen,", "tokens": ["In", "from\u00b7mer", "Treu\u00b7e", "pfle\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Mit Sonnenschein und Regen", "tokens": ["Mit", "Son\u00b7nen\u00b7schein", "und", "Re\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und Tau erquicken sie.", "tokens": ["Und", "Tau", "er\u00b7qui\u00b7cken", "sie", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Die erste Blum' vor allen", "tokens": ["Die", "ers\u00b7te", "Blum'", "vor", "al\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "PIAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das mu\u00df ", "tokens": ["Das", "mu\u00df"], "token_info": ["word", "word"], "pos": ["PDS", "VMFIN"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Der Menschen Wohlgefallen,", "tokens": ["Der", "Men\u00b7schen", "Wohl\u00b7ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Engel sch\u00f6nster Schein:", "tokens": ["Der", "En\u00b7gel", "sch\u00f6ns\u00b7ter", "Schein", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sie ist ", "tokens": ["Sie", "ist"], "token_info": ["word", "word"], "pos": ["PPER", "VAFIN"], "meter": "-+", "measure": "iambic.single"}, "line.6": {"text": "Und mu\u00df auf Dornen stehen,", "tokens": ["Und", "mu\u00df", "auf", "Dor\u00b7nen", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Sobald die Winde wehen,", "tokens": ["So\u00b7bald", "die", "Win\u00b7de", "we\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ist ihre Sch\u00f6ne tot.", "tokens": ["Ist", "ih\u00b7re", "Sch\u00f6\u00b7ne", "tot", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Die zweite, die Gott liebet", "tokens": ["Die", "zwei\u00b7te", ",", "die", "Gott", "lie\u00b7bet"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "$,", "ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "N\u00e4chst Liebe allerbest',", "tokens": ["N\u00e4chst", "Lie\u00b7be", "al\u00b7ler\u00b7best'", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ist, die das Gute \u00fcbet", "tokens": ["Ist", ",", "die", "das", "Gu\u00b7te", "\u00fc\u00b7bet"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "$,", "PRELS", "ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und sich nichts merken l\u00e4\u00dft;", "tokens": ["Und", "sich", "nichts", "mer\u00b7ken", "l\u00e4\u00dft", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "PIS", "VVINF", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ihr Name ", "tokens": ["Ihr", "Na\u00b7me"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "+--", "measure": "dactylic.init"}, "line.6": {"text": "Auf Erden auch ", "tokens": ["Auf", "Er\u00b7den", "auch"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NN", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Sie bl\u00fcht ein kurzes Weilchen", "tokens": ["Sie", "bl\u00fcht", "ein", "kur\u00b7zes", "Weil\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und kaum die Bl\u00fcte weist.", "tokens": ["Und", "kaum", "die", "Bl\u00fc\u00b7te", "weist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Sie duftet nur bei Nacht", "tokens": ["Sie", "duf\u00b7tet", "nur", "bei", "Nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "In aller Geister Mitte", "tokens": ["In", "al\u00b7ler", "Geis\u00b7ter", "Mit\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Bei voller Himmelspracht:", "tokens": ["Bei", "vol\u00b7ler", "Him\u00b7mel\u00b7spracht", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Da tut das Herz sich auf", "tokens": ["Da", "tut", "das", "Herz", "sich", "auf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "PRF", "APPR"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der frommen ", "tokens": ["Der", "from\u00b7men"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Wann hell von Pol zu Pole", "tokens": ["Wann", "hell", "von", "Pol", "zu", "Po\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "APPR", "NN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Sich schwingt der Sterne Lauf.", "tokens": ["Sich", "schwingt", "der", "Ster\u00b7ne", "Lauf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Auch ", "tokens": ["Auch"], "token_info": ["word"], "pos": ["ADV"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Ein liebes Gotteskind,", "tokens": ["Ein", "lie\u00b7bes", "Got\u00b7tes\u00b7kind", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wohl liebstes seiner Kinder,", "tokens": ["Wohl", "liebs\u00b7tes", "sei\u00b7ner", "Kin\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die nur hienieden sind.", "tokens": ["Die", "nur", "hien\u00b7ie\u00b7den", "sind", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Holdselig von Geb\u00e4rden,", "tokens": ["Hold\u00b7se\u00b7lig", "von", "Ge\u00b7b\u00e4r\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.6": {"text": "Du bist ihr Bild auf Erden,", "tokens": ["Du", "bist", "ihr", "Bild", "auf", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Kommst mit dem Lenz zugleich.", "tokens": ["Kommst", "mit", "dem", "Lenz", "zu\u00b7gleich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ADV", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.16": {"line.1": {"text": "Auch du, die im Gem\u00fcte", "tokens": ["Auch", "du", ",", "die", "im", "Ge\u00b7m\u00fc\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "PPER", "$,", "PRELS", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Best\u00e4ndig ist und treu,", "tokens": ["Be\u00b7st\u00e4n\u00b7dig", "ist", "und", "treu", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du, aller Zeiten Bl\u00fcte,", "tokens": ["Du", ",", "al\u00b7ler", "Zei\u00b7ten", "Bl\u00fc\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PIAT", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mir lieb gegr\u00fc\u00dfet sei!", "tokens": ["Mir", "lieb", "ge\u00b7gr\u00fc\u00b7\u00dfet", "sei", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVPP", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "O wolle Gott uns geben", "tokens": ["O", "wol\u00b7le", "Gott", "uns", "ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VMFIN", "NN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Solche Lieb' zu jeder Stund'!", "tokens": ["Sol\u00b7che", "Lieb'", "zu", "je\u00b7der", "Stund'", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.17": {"line.1": {"text": "Und du, die auf dem Throne", "tokens": ["Und", "du", ",", "die", "auf", "dem", "Thro\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Des Blumengartens sitzt", "tokens": ["Des", "Blu\u00b7men\u00b7gar\u00b7tens", "sitzt"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und mit der wei\u00dfen Krone", "tokens": ["Und", "mit", "der", "wei\u00b7\u00dfen", "Kro\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Gleich einem Engel blitzt,", "tokens": ["Gleich", "ei\u00b7nem", "En\u00b7gel", "blitzt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Du winkest lieb uns hinnen", "tokens": ["Du", "win\u00b7kest", "lieb", "uns", "hin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mit Herzen und mit Sinnen", "tokens": ["Mit", "Her\u00b7zen", "und", "mit", "Sin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Zur\u00fcck zum Paradies.", "tokens": ["Zu\u00b7r\u00fcck", "zum", "Pa\u00b7ra\u00b7dies", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.18": {"line.1": {"text": "Noch Blumen viel und Kr\u00e4uter", "tokens": ["Noch", "Blu\u00b7men", "viel", "und", "Kr\u00e4u\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "NN", "ADV", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hat Gott der G\u00e4rtner mehr,", "tokens": ["Hat", "Gott", "der", "G\u00e4rt\u00b7ner", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wer sie erz\u00e4hlte weiter,", "tokens": ["Wer", "sie", "er\u00b7z\u00e4hl\u00b7te", "wei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Z\u00e4hlt wohl den Sand am Meer:", "tokens": ["Z\u00e4hlt", "wohl", "den", "Sand", "am", "Meer", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wieviel er ausgestreut,", "tokens": ["Wie\u00b7viel", "er", "aus\u00b7ge\u00b7streut", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Wie k\u00f6nnt' ich alle z\u00e4hlen", "tokens": ["Wie", "k\u00f6nnt'", "ich", "al\u00b7le", "z\u00e4h\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "PPER", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Die zarten Blumenseelen", "tokens": ["Die", "zar\u00b7ten", "Blu\u00b7men\u00b7see\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Im bunten Sonnenkleid!", "tokens": ["Im", "bun\u00b7ten", "Son\u00b7nen\u00b7kleid", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.19": {"line.1": {"text": "Sollt' ich mir eine nehmen,", "tokens": ["Sollt'", "ich", "mir", "ei\u00b7ne", "neh\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "ART", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Lilie m\u00fc\u00dft' es sein,", "tokens": ["Die", "Li\u00b7lie", "m\u00fc\u00dft'", "es", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Steht wie ein Geisterschemen", "tokens": ["Steht", "wie", "ein", "Geis\u00b7ter\u00b7sche\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "KOKOM", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit hellem Himmelschein;", "tokens": ["Mit", "hel\u00b7lem", "Him\u00b7mel\u00b7schein", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wehm\u00fctig geht ihr Blick", "tokens": ["Weh\u00b7m\u00fc\u00b7tig", "geht", "ihr", "Blick"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Empor zum Licht der Sterne,", "tokens": ["Em\u00b7por", "zum", "Licht", "der", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Sie w\u00e4re gar zu gerne", "tokens": ["Sie", "w\u00e4\u00b7re", "gar", "zu", "ger\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "PTKA", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Zum Vaterland zur\u00fcck.", "tokens": ["Zum", "Va\u00b7ter\u00b7land", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.20": {"line.1": {"text": "O G\u00e4rtner treu und milde,", "tokens": ["O", "G\u00e4rt\u00b7ner", "treu", "und", "mil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der alles kann und wei\u00df,", "tokens": ["Der", "al\u00b7les", "kann", "und", "wei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VMFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mach' mich zu ihrem Bilde,", "tokens": ["Mach'", "mich", "zu", "ih\u00b7rem", "Bil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mach' mich so rein und wei\u00df.", "tokens": ["Mach'", "mich", "so", "rein", "und", "wei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJD", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Dann kann ich droben froh", "tokens": ["Dann", "kann", "ich", "dro\u00b7ben", "froh"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Als Lilienm\u00e4dchen kommen", "tokens": ["Als", "Li\u00b7li\u00b7en\u00b7m\u00e4d\u00b7chen", "kom\u00b7men"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "NN", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Und unter allen Frommen", "tokens": ["Und", "un\u00b7ter", "al\u00b7len", "From\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "In Unschuld bl\u00fchen so.", "tokens": ["In", "Un\u00b7schuld", "bl\u00fc\u00b7hen", "so", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}