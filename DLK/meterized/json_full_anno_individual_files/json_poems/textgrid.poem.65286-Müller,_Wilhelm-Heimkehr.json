{"textgrid.poem.65286": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Heimkehr", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vor der Th\u00fcre meiner Lieben", "tokens": ["Vor", "der", "Th\u00fc\u00b7re", "mei\u00b7ner", "Lie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PPOSAT", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "H\u00e4ng' ich auf den Wanderstab,", "tokens": ["H\u00e4ng'", "ich", "auf", "den", "Wan\u00b7der\u00b7stab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Was mich durch die Welt getrieben,", "tokens": ["Was", "mich", "durch", "die", "Welt", "ge\u00b7trie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Leg' ich ihr zu F\u00fc\u00dfen ab.", "tokens": ["Leg'", "ich", "ihr", "zu", "F\u00fc\u00b7\u00dfen", "ab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPER", "APPR", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wanderlustige Gedanken,", "tokens": ["Wan\u00b7der\u00b7lus\u00b7ti\u00b7ge", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die ihr flattert nah und fern,", "tokens": ["Die", "ihr", "flat\u00b7tert", "nah", "und", "fern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER",