{"textgrid.poem.39015": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Oft, wenn die Nacht dunkelt,", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Oft, wenn die Nacht dunkelt,", "tokens": ["Oft", ",", "wenn", "die", "Nacht", "dun\u00b7kelt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Kann ich ungeduldig kaum erwarten", "tokens": ["Kann", "ich", "un\u00b7ge\u00b7dul\u00b7dig", "kaum", "er\u00b7war\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADJD", "ADV", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Mich auf das Lager hinzuwerfen,", "tokens": ["Mich", "auf", "das", "La\u00b7ger", "hin\u00b7zu\u00b7wer\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So peinigt mich Ermattung und Schw\u00e4che.", "tokens": ["So", "pei\u00b7nigt", "mich", "Er\u00b7mat\u00b7tung", "und", "Schw\u00e4\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "KON", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Und doch am fr\u00fchen Morgen wieder", "tokens": ["Und", "doch", "am", "fr\u00fc\u00b7hen", "Mor\u00b7gen", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPRART", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Rufen mich die B\u00fccher,", "tokens": ["Ru\u00b7fen", "mich", "die", "B\u00fc\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Weckt mich das Coliseum, oder St. Peter,", "tokens": ["Weckt", "mich", "das", "Co\u00b7li\u00b7se\u00b7um", ",", "o\u00b7der", "St.", "Pe\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "abbreviation", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "KON", "NE", "NE", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Die Tr\u00fcmmer dort, das Bildni\u00df hier,", "tokens": ["Die", "Tr\u00fcm\u00b7mer", "dort", ",", "das", "Bild\u00b7ni\u00df", "hier", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Der Tempel im entlegnen Weinberg:", "tokens": ["Der", "Tem\u00b7pel", "im", "ent\u00b7leg\u00b7nen", "Wein\u00b7berg", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Zum Vatikan ist oft mein Weg gerichtet;", "tokens": ["Zum", "Va\u00b7ti\u00b7kan", "ist", "oft", "mein", "Weg", "ge\u00b7rich\u00b7tet", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Ungern vers\u00e4um' ich den alten Hymnengesang,", "tokens": ["Un\u00b7gern", "ver\u00b7s\u00e4um'", "ich", "den", "al\u00b7ten", "Hym\u00b7nen\u00b7ge\u00b7sang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+-+--+", "measure": "dactylic.di.plus"}, "line.12": {"text": "Sei's in neuen, sei's in alten Kirchen;", "tokens": ["Sei's", "in", "neu\u00b7en", ",", "sei's", "in", "al\u00b7ten", "Kir\u00b7chen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ADJA", "$,", "VAFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.13": {"text": "Dann lockt mich das Feld hinaus,", "tokens": ["Dann", "lockt", "mich", "das", "Feld", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "APZR", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.14": {"text": "Die herrlichen G\u00e4rten,", "tokens": ["Die", "herr\u00b7li\u00b7chen", "G\u00e4r\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.15": {"text": "Ein Volksfest auch, der Tiberstrom,", "tokens": ["Ein", "Volks\u00b7fest", "auch", ",", "der", "Ti\u00b7ber\u00b7strom", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Den Sonnenuntergang zu sehn", "tokens": ["Den", "Son\u00b7nen\u00b7un\u00b7ter\u00b7gang", "zu", "sehn"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Von Pietro Mantorio.", "tokens": ["Von", "Pie\u00b7tro", "Man\u00b7to\u00b7rio", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NE", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.18": {"text": "Auch die B\u00fcchersammlung", "tokens": ["Auch", "die", "B\u00fc\u00b7cher\u00b7samm\u00b7lung"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.19": {"text": "Der Kl\u00f6ster wird durchschaut,", "tokens": ["Der", "Kl\u00f6s\u00b7ter", "wird", "durch\u00b7schaut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.20": {"text": "Dann qu\u00e4lt mich am Abend das Theater,", "tokens": ["Dann", "qu\u00e4lt", "mich", "am", "A\u00b7bend", "das", "The\u00b7a\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.21": {"text": "Das ich ungern misse,", "tokens": ["Das", "ich", "un\u00b7gern", "mis\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.22": {"text": "Und immer wird mir doch der Tag zu kurz,", "tokens": ["Und", "im\u00b7mer", "wird", "mir", "doch", "der", "Tag", "zu", "kurz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ADV", "ART", "NN", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Und viel zu lang die Nacht.", "tokens": ["Und", "viel", "zu", "lang", "die", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PTKA", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Jetzt aber hat ein Unruhstiftender", "tokens": ["Jetzt", "a\u00b7ber", "hat", "ein", "Un\u00b7ruh\u00b7stif\u00b7ten\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Irrer und schlimmer Geist", "tokens": ["Ir\u00b7rer", "und", "schlim\u00b7mer", "Geist"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KON", "ADJA", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Mir noch die Arbeit geh\u00e4uft.", "tokens": ["Mir", "noch", "die", "Ar\u00b7beit", "ge\u00b7h\u00e4uft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "NN", "VVPP", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.4": {"text": "Der Verf\u00fchrer hat mich auf den Vatikan gelockt,", "tokens": ["Der", "Ver\u00b7f\u00fch\u00b7rer", "hat", "mich", "auf", "den", "Va\u00b7ti\u00b7kan", "ge\u00b7lockt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.5": {"text": "Dort die alten deutschen Schriften,", "tokens": ["Dort", "die", "al\u00b7ten", "deut\u00b7schen", "Schrif\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Vom Dietrich von Bern und Tristan,", "tokens": ["Vom", "Diet\u00b7rich", "von", "Bern", "und", "Tris\u00b7tan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NE", "APPR", "NE", "KON", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Titurell und Malagys,", "tokens": ["Ti\u00b7tu\u00b7rell", "und", "Ma\u00b7la\u00b7gys", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Vom K\u00f6nig Rother und den Heymonskindern", "tokens": ["Vom", "K\u00f6\u00b7nig", "Ro\u00b7ther", "und", "den", "Hey\u00b7mons\u00b7kin\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADJA", "KON", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Zu lesen, zu vergleichen,", "tokens": ["Zu", "le\u00b7sen", ",", "zu", "ver\u00b7glei\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Mir vieles abzuschreiben.", "tokens": ["Mir", "vie\u00b7les", "ab\u00b7zu\u00b7schrei\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "PIS", "VVIZU", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Nun schelt' ich erst den Tag", "tokens": ["Nun", "schelt'", "ich", "erst", "den", "Tag"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Und die zu fl\u00fccht'gen Stunden,", "tokens": ["Und", "die", "zu", "fl\u00fccht'\u00b7gen", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PTKZU", "VVINF", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Nun scharr' ich, dem Geizigen gleich,", "tokens": ["Nun", "scharr'", "ich", ",", "dem", "Gei\u00b7zi\u00b7gen", "gleich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ART", "NN", "ADV", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.14": {"text": "Alle Minuten zusammen,", "tokens": ["Al\u00b7le", "Mi\u00b7nu\u00b7ten", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PTKVZ", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.15": {"text": "Und karge vom Mahl und Schlaf", "tokens": ["Und", "kar\u00b7ge", "vom", "Mahl", "und", "Schlaf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPRART", "NN", "KON", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.16": {"text": "Mir die Sekunden ab,", "tokens": ["Mir", "die", "Se\u00b7kun\u00b7den", "ab", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.17": {"text": "So treibt mich ein schlimmer Flei\u00df; \u2013", "tokens": ["So", "treibt", "mich", "ein", "schlim\u00b7mer", "Flei\u00df", ";", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.18": {"text": "Und ich mu\u00dfte nach Rom gehn", "tokens": ["Und", "ich", "mu\u00df\u00b7te", "nach", "Rom", "gehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VMFIN", "APPR", "NE", "VVINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.19": {"text": "Um erst recht stockdeutsch zu werden.", "tokens": ["Um", "erst", "recht", "stock\u00b7deutsch", "zu", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADV", "ADV", "ADJD", "PTKZU", "VAINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Oft, wenn die Nacht dunkelt,", "tokens": ["Oft", ",", "wenn", "die", "Nacht", "dun\u00b7kelt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Kann ich ungeduldig kaum erwarten", "tokens": ["Kann", "ich", "un\u00b7ge\u00b7dul\u00b7dig", "kaum", "er\u00b7war\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADJD", "ADV", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"te