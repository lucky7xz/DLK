nd immer wird mir doch der Tag zu kurz,", "tokens": ["Und", "im\u00b7mer", "wird", "mir", "doch", "der", "Tag", "zu", "kurz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ADV", "ART", "NN", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Und viel zu lang die Nacht.", "tokens": ["Und", "viel", "zu", "lang", "die", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PTKA", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Jetzt aber hat ein Unruhstiftender", "tokens": ["Jetzt", "a\u00b7ber", "hat", "ein", "Un\u00b7ruh\u00b7stif\u00b7ten\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Irrer und schlimmer Geist", "tokens": ["Ir\u00b7rer", "und", "schlim\u00b7mer", "Geist"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KON", "ADJA", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Mir noch die Arbeit geh\u00e4uft.", "tokens": ["Mir", "noch", "die", "Ar\u00b7beit", "ge\u00b7h\u00e4uft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "NN", "VVPP", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.4": {"text": "Der Verf\u00fchrer hat mich auf den Vatikan gelockt,", "tokens": ["Der", "Ver\u00b7f\u00fch\u00b7rer", "hat", "mich", "auf", "den", "Va\u00b7ti\u00b7kan", "ge\u00b7lockt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.5": {"text": "Dort die alten deutschen Schriften,", "tokens": ["Dort", "die", "al\u00b7ten", "deut\u00b7schen", "Schrif\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Vom Dietrich von Bern und Tristan,", "tokens": ["Vom", "Diet\u00b7rich", "von", "Bern", "und", "Tris\u00b7tan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NE", "APPR", "NE", "KON", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Titurell und Malagys,", "tokens": ["Ti\u00b7tu\u00b7rell", "und", "Ma\u00b7la\u00b7gys", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Vom K\u00f6nig Rother und den Heymonskindern", "tokens": ["Vom", "K\u00f6\u00b7nig", "Ro\u00b7ther", "und", "den", "Hey\u00b7mons\u00b7kin\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADJA", "KON", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Zu lesen, zu vergleichen,", "tokens": ["Zu", "le\u00b7sen", ",", "zu", "ver\u00b7glei\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Mir vieles abzuschreiben.", "tokens": ["Mir", "vie\u00b7les", "ab\u00b7zu\u00b7schrei\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "PIS", "VVIZU", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Nun schelt' ich erst den Tag", "tokens": ["Nun", "schelt'", "ich", "erst", "den", "Tag"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Und die zu fl\u00fccht'gen Stunden,", "tokens": ["Und", "die", "zu", "fl\u00fccht'\u00b7gen", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PTKZU", "VVINF", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Nun scharr' ich, dem Geizigen gleich,", "tokens": ["Nun", "scharr'", "ich", ",", "dem", "Gei\u00b7zi\u00b7gen", "gleich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ART", "NN", "ADV", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.14": {"text": "Alle Minuten zusammen,", "tokens": ["Al\u00b7le", "Mi\u00b7nu\u00b7ten", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PTKVZ", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.15": {"text": "Und karge vom Mahl und Schlaf", "tokens": ["Und", "kar\u00b7ge", "vom", "Mahl", "und", "Schlaf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPRART", "NN", "KON", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.16": {"text": "Mir die Sekunden ab,", "tokens": ["Mir", "die", "Se\u00b7kun\u00b7den", "ab", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.17": {"text": "So treibt mich ein schlimmer Flei\u00df; \u2013", "tokens": ["So", "treibt", "mich", "ein", "schlim\u00b7mer", "Flei\u00df", ";", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.18": {"text": "Und ich mu\u00dfte nach Rom gehn", "tokens": ["Und", "ich", "mu\u00df\u00b7te", "nach", "Rom", "gehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "