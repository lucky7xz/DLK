{"textgrid.poem.47436": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[fort mit dem Ich und seiner Kraft]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fort mit dem Ich und seiner Kraft,", "tokens": ["Fort", "mit", "dem", "Ich", "und", "sei\u00b7ner", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PRELS", "PPER", "KON", "PPOSAT", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Gebeut die Liebe, fort damit!", "tokens": ["Ge\u00b7beut", "die", "Lie\u00b7be", ",", "fort", "da\u00b7mit", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "PTKVZ", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vor jenem Auge ziemet ihm", "tokens": ["Vor", "je\u00b7nem", "Au\u00b7ge", "zie\u00b7met", "ihm"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df es verstiebe; fort damit!", "tokens": ["Da\u00df", "es", "ver\u00b7stie\u00b7be", ";", "fort", "da\u00b7mit", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$.", "PTKVZ", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Nein, geize nach der Ehre nicht", "tokens": ["Nein", ",", "gei\u00b7ze", "nach", "der", "Eh\u00b7re", "nicht"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "VVFIN", "APPR", "ART", "NN", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dir selber ewig gleich zu sein;", "tokens": ["Dir", "sel\u00b7ber", "e\