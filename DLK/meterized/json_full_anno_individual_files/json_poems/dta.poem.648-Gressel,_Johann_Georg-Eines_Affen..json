{"dta.poem.648": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Eines Affen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Ich war ein dummer Aff/ drum weicht ihr klugen Affen", "tokens": ["Ich", "war", "ein", "dum\u00b7mer", "Aff", "/", "drum", "weicht", "ihr", "klu\u00b7gen", "Af\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$(", "PAV", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von dieser schwartzen Grufft/ sonst sage ich zu euch/", "tokens": ["Von", "die\u00b7ser", "schwart\u00b7zen", "Grufft", "/", "sonst", "sa\u00b7ge", "ich", "zu", "euch", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$(", "ADV", "VVFIN", "PPER", "APPR", "PPER", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Jhr zeiget s\u00e4mmtlich an durch eur neugierigs Gaffen", "tokens": ["Ihr", "zei\u00b7get", "s\u00e4mmt\u00b7lich", "an", "durch", "eur", "neu\u00b7gie\u00b7rigs", "Gaf\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df wir einander fast in allen Dingen gleich.", "tokens": ["Da\u00df", "wir", "ein\u00b7an\u00b7der", "fast", "in", "al\u00b7len", "Din\u00b7gen", "gleich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "APPR", "PIAT", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}