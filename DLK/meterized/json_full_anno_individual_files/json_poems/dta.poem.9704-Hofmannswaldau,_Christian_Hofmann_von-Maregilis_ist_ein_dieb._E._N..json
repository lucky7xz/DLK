{"dta.poem.9704": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Maregilis ist ein dieb.  \n E. N.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Maregilis hat mir das hertz gestohlen/", "tokens": ["Ma\u00b7re\u00b7gi\u00b7lis", "hat", "mir", "das", "hertz", "ge\u00b7stoh\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "ART", "NN", "VVPP", "$("], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Schickt in die facult\u00e4t", "tokens": ["Schickt", "in", "die", "fa\u00b7cul\u00b7t\u00e4t"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und last ein urtheil holen;", "tokens": ["Und", "last", "ein", "ur\u00b7theil", "ho\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Doch wann es hier nach gunst und gaben geht/", "tokens": ["Doch", "wann", "es", "hier", "nach", "gunst", "und", "ga\u00b7ben", "geht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADV", "APPR", "NN", "KON", "VVFIN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "So schickt in sch\u00f6ppenstuhl.", "tokens": ["So", "schickt", "in", "sch\u00f6p\u00b7pen\u00b7stuhl", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "L\u00e4ufft so ein urtheil ein/", "tokens": ["L\u00e4ufft", "so", "ein", "ur\u00b7theil", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "ART", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "Sie w\u00e4re mit den straffen zu belangen/", "tokens": ["Sie", "w\u00e4\u00b7re", "mit", "den", "straf\u00b7fen", "zu", "be\u00b7lan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Die sonst vor diebe seyn/", "tokens": ["Die", "sonst", "vor", "die\u00b7be", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "PDAT", "VAINF", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "So lasset sie an meine lippen hangen.", "tokens": ["So", "las\u00b7set", "sie", "an", "mei\u00b7ne", "lip\u00b7pen", "han\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}