{"textgrid.poem.53144": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "Abschieds-Liedchen", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hat meines Hertzens keusche Brunst", "tokens": ["Hat", "mei\u00b7nes", "Hert\u00b7zens", "keu\u00b7sche", "Brunst"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dann bey dem Himmel keine Gunst,", "tokens": ["Dann", "bey", "dem", "Him\u00b7mel", "kei\u00b7ne", "Gunst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df ich dich, Sch\u00f6nste, mu\u00df verlassen?", "tokens": ["Da\u00df", "ich", "dich", ",", "Sch\u00f6ns\u00b7te", ",", "mu\u00df", "ver\u00b7las\u00b7sen", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "$,", "NN", "$,", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hie wo du stets mit Neid vnd List", "tokens": ["Hie", "wo", "du", "stets", "mit", "Neid", "vnd", "List"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PWAV", "PPER", "ADV", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der falschen Zungen, die dich hassen,", "tokens": ["Der", "fal\u00b7schen", "Zun\u00b7gen", ",", "die", "dich", "has\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mein Sinnen-Trost, umbgeben bist?", "tokens": ["Mein", "Sin\u00b7nen\u00b7Trost", ",", "umb\u00b7ge\u00b7ben", "bist", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Entschlag dich aber aller Pein", "tokens": ["Ent\u00b7schlag", "dich", "a\u00b7ber", "al\u00b7ler", "Pein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vnd la\u00df dein Hertz versichert seyn,", "tokens": ["Vnd", "la\u00df", "dein", "Hertz", "ver\u00b7si\u00b7chert", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPOSAT", "NN", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df ich kurtzumb nicht von dir scheide,", "tokens": ["Da\u00df", "ich", "kurt\u00b7zumb", "nicht", "von", "dir", "schei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PTKNEG", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mein blosser Schatten zeucht von hier,", "tokens": ["Mein", "blos\u00b7ser", "Schat\u00b7ten", "zeucht", "von", "hier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "APPR", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich aber bleib' in Lieb' vnd Leide", "tokens": ["Ich", "a\u00b7ber", "bleib'", "in", "Lieb'", "vnd", "Lei\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Stets umb dich her vnd diene dir.", "tokens": ["Stets", "umb", "dich", "her", "vnd", "die\u00b7ne", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PRF", "APZR", "KON", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "La\u00df nur die Mi\u00dfgunst immerhin", "tokens": ["La\u00df", "nur", "die", "Mi\u00df\u00b7gunst", "im\u00b7mer\u00b7hin"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "ADV", "ART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vergifftet aus verbo\u00dftem Sinn'", "tokens": ["Ver\u00b7giff\u00b7tet", "aus", "ver\u00b7bo\u00df\u00b7tem", "Sinn'"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auff dich zu stechen sich bem\u00fchen,", "tokens": ["Auff", "dich", "zu", "ste\u00b7chen", "sich", "be\u00b7m\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PTKZU", "VVFIN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Es schmertzt sie, da\u00df dein Glantz vnd Pracht,", "tokens": ["Es", "schmertzt", "sie", ",", "da\u00df", "dein", "Glantz", "vnd", "Pracht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KOUS", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du edle Rose, so mus bl\u00fchen", "tokens": ["Du", "ed\u00b7le", "Ro\u00b7se", ",", "so", "mus", "bl\u00fc\u00b7hen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ADJA", "NN", "$,", "ADV", "VMFIN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Vnd sie, die Hecken, schamroht macht.", "tokens": ["Vnd", "sie", ",", "die", "He\u00b7cken", ",", "scham\u00b7roht", "macht", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "ART", "NN", "$,", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Es k\u00f6mpt, ob Gott wil, noch die Zeit,", "tokens": ["Es", "k\u00f6mpt", ",", "ob", "Gott", "wil", ",", "noch", "die", "Zeit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "NN", "VMFIN", "$,", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df wir der Disteln rauhes Kleidt", "tokens": ["Da\u00df", "wir", "der", "Dis\u00b7teln", "rau\u00b7hes", "Kleidt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Durch unsrer Liebe Brunst verbrennen,", "tokens": ["Durch", "uns\u00b7rer", "Lie\u00b7be", "Brunst", "ver\u00b7bren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da man hergegen nichts an dir,", "tokens": ["Da", "man", "her\u00b7ge\u00b7gen", "nichts", "an", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "PIS", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du g\u00fcldne Bluhme, wird erkennen", "tokens": ["Du", "g\u00fcld\u00b7ne", "Bluh\u00b7me", ",", "wird", "er\u00b7ken\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "ADJA", "NN", "$,", "VAFIN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Als Glantz vnd unverwelckte Zier.", "tokens": ["Als", "Glantz", "vnd", "un\u00b7ver\u00b7welck\u00b7te", "Zier", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Nun, hiemit reis' ich auff den Schlu\u00df", "tokens": ["Nun", ",", "hie\u00b7mit", "reis'", "ich", "auff", "den", "Schlu\u00df"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PAV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Himmels, dem ich folgen mu\u00df,", "tokens": ["Des", "Him\u00b7mels", ",", "dem", "ich", "fol\u00b7gen", "mu\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch wo ich mich befinden werde,", "tokens": ["Doch", "wo", "ich", "mich", "be\u00b7fin\u00b7den", "wer\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "PRF", "VVINF", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Daselbst wird auch dein Licht vnd Schein,", "tokens": ["Da\u00b7selbst", "wird", "auch", "dein", "Licht", "vnd", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "ADV", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dein Sinn vnd h\u00f6fliches Geberde", "tokens": ["Dein", "Sinn", "vnd", "h\u00f6f\u00b7li\u00b7ches", "Ge\u00b7ber\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mein Thun, Red' vnd Gedancken seyn.", "tokens": ["Mein", "Thun", ",", "Red'", "vnd", "Ge\u00b7dan\u00b7cken", "seyn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "NN", "KON", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ach, wenn es k\u00fcrtzlich wird geschehn,", "tokens": ["Ach", ",", "wenn", "es", "k\u00fcrtz\u00b7lich", "wird", "ge\u00b7schehn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "KOUS", "PPER", "ADJD", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df ich dich wieder werde sehn", "tokens": ["Da\u00df", "ich", "dich", "wie\u00b7der", "wer\u00b7de", "sehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "ADV", "VAFIN", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vnd deiner Gegenwart geniessen,", "tokens": ["Vnd", "dei\u00b7ner", "Ge\u00b7gen\u00b7wart", "ge\u00b7nies\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ich werde dieses Gut, mein Liecht,", "tokens": ["Ich", "wer\u00b7de", "die\u00b7ses", "Gut", ",", "mein", "Liecht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PDAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mit nichts hie zu vertauschen wissen,", "tokens": ["Mit", "nichts", "hie", "zu", "ver\u00b7tau\u00b7schen", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "ADV", "PTKZU", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mit keinem Kayserthum auch nicht.", "tokens"