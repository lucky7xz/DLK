{"textgrid.poem.43843": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Weihnachtsbaum", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von allen den B\u00e4umen jung und alt,", "tokens": ["Von", "al\u00b7len", "den", "B\u00e4u\u00b7men", "jung", "und", "alt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Von allen den B\u00e4umen gro\u00df und klein,", "tokens": ["Von", "al\u00b7len", "den", "B\u00e4u\u00b7men", "gro\u00df", "und", "klein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Von allen in unserm ganzen Wald,", "tokens": ["Von", "al\u00b7len", "in", "un\u00b7serm", "gan\u00b7zen", "Wald", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Was mag doch der allersch\u00f6nste sein?", "tokens": ["Was", "mag", "doch", "der", "al\u00b7ler\u00b7sch\u00f6ns\u00b7te", "sein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ADV", "ART", "ADJA", "VAINF", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Der sch\u00f6nste von allen weit und breit", "tokens": ["Der", "sch\u00f6ns\u00b7te", "von", "al\u00b7len", "weit", "und", "breit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "APPR", "PIAT", "ADJD", "KON", "ADJD"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Das ist doch allein, wer zweifelt dran?", "tokens": ["Das", "ist", "doch", "al\u00b7lein", ",", "wer", "zwei\u00b7felt", "dran", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADV", "$,", "PWS", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Den heute mir bringt der Weihnachtsmann. \u2013", "tokens": ["Den", "heu\u00b7te", "mir", "bringt", "der", "Weih\u00b7nachts\u00b7mann", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADV", "PPER", "VVFIN", "ART", "NN", "$.", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Wenn Alles schon schl\u00e4ft in stiller Nacht,", "tokens": ["Wenn", "Al\u00b7les", "schon", "schl\u00e4ft", "in", "stil\u00b7ler", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Dann holet er ihn bei Sternenschein", "tokens": ["Dann", "ho\u00b7let", "er", "ihn", "bei", "Ster\u00b7nen\u00b7schein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "APPR", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und schl\u00fcpfet, eh' einer sich's gedacht,", "tokens": ["Und", "schl\u00fcp\u00b7fet", ",", "eh'", "ei\u00b7ner", "sich's", "ge\u00b7dacht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "ART", "PIS", "VVPP", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Gar heimlich damit ins Haus hinein.", "tokens": ["Gar", "heim\u00b