{"dta.poem.10503": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Lx.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Sie haben heut Abend Gesellschaft,", "tokens": ["Sie", "ha\u00b7ben", "heut", "A\u00b7bend", "Ge\u00b7sell\u00b7schaft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "NN", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Und das Haus ist lichterf\u00fcllt.", "tokens": ["Und", "das", "Haus", "ist", "lich\u00b7ter\u00b7f\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dort oben am hellen Fenster", "tokens": ["Dort", "o\u00b7ben", "am", "hel\u00b7len", "Fens\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Bewegt sich ein Schattenbild.", "tokens": ["Be\u00b7wegt", "sich", "ein", "Schat\u00b7ten\u00b7bild", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Du schaust mich nicht, im Dunkeln", "tokens": ["Du", "schaust", "mich", "nicht", ",", "im", "Dun\u00b7keln"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$,", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Steh' ich hier unten allein;", "tokens": ["Steh'", "ich", "hier", "un\u00b7ten", "al\u00b7lein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "ADV", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Noch wen'ger kannst du schauen", "tokens": ["Noch", "wen'\u00b7ger", "kannst", "du", "schau\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PIS", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In mein dunkles Herz hinein.", "tokens": ["In", "mein", "dunk\u00b7les", "Herz", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Mein dunkles Herze liebt dich,", "tokens": ["Mein", "dunk\u00b7les", "Her\u00b7ze", "liebt", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+---", "measure": "unknown.measure.di"}, "line.2": {"text": "Es liebt dich und es bricht,", "tokens": ["Es", "liebt", "dich", "und", "es", "bricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "KON", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und bricht und zuckt und verblutet,", "tokens": ["Und", "bricht", "und", "zuckt", "und", "ver\u00b7blu\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "KON", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Aber du siehst es nicht.", "tokens": ["A\u00b7ber", "du", "siehst", "es", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}}}}