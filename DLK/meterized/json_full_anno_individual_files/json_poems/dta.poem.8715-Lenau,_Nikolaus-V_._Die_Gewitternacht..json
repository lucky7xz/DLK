{"dta.poem.8715": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "V .  \n  Die Gewitternacht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1832", "urn": "urn:nbn:de:kobv:b4-200905193572", "language": ["de:0.99"], "booktitle": "Lenau, Nikolaus: Gedichte. Stuttgart, 1832."}, "poem": {"stanza.1": {"line.1": {"text": "Mit dem Grafen Konopacki,", "tokens": ["Mit", "dem", "Gra\u00b7fen", "Ko\u00b7no\u00b7pa\u00b7cki", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NE", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Seinem Freunde, treubew\u00e4hret,", "tokens": ["Sei\u00b7nem", "Freun\u00b7de", ",", "treu\u00b7be\u00b7w\u00e4h\u00b7ret", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Spricht Johannes angelegen,", "tokens": ["Spricht", "Jo\u00b7han\u00b7nes", "an\u00b7ge\u00b7le\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Als der Abend wiederkehret.", "tokens": ["Als", "der", "A\u00b7bend", "wie\u00b7der\u00b7keh\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Eben hat der Graf des Trostes", "tokens": ["E\u00b7ben", "hat", "der", "Graf", "des", "Tros\u00b7tes"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mildberedtes Wort geendet,", "tokens": ["Mild\u00b7be\u00b7red\u00b7tes", "Wort", "ge\u00b7en\u00b7det", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und des Prinzen d\u00fcstre Seele", "tokens": ["Und", "des", "Prin\u00b7zen", "d\u00fcst\u00b7re", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Froher Hoffnung zugewendet;", "tokens": ["Fro\u00b7her", "Hoff\u00b7nung", "zu\u00b7ge\u00b7wen\u00b7det", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Leise l\u00e4chelt dem die Freude", "tokens": ["Lei\u00b7se", "l\u00e4\u00b7chelt", "dem", "die", "Freu\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf den kummerbleichen Wangen,", "tokens": ["Auf", "den", "kum\u00b7mer\u00b7blei\u00b7chen", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und er h\u00e4lt die Hand des Freundes", "tokens": ["Und", "er", "h\u00e4lt", "die", "Hand", "des", "Freun\u00b7des"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit des Dankes Druck umfangen. \u2014", "tokens": ["Mit", "des", "Dan\u00b7kes", "Druck", "um\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Draussen sind die Waffenknechte", "tokens": ["Draus\u00b7sen", "sind", "die", "Waf\u00b7fen\u00b7knech\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VAFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Rundgelagert in der Halle,", "tokens": ["Rund\u00b7ge\u00b7la\u00b7gert", "in", "der", "Hal\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und es dr\u00f6hnt der Marmorboden", "tokens": ["Und", "es", "dr\u00f6hnt", "der", "Mar\u00b7mor\u00b7bo\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Vom Pokal und W\u00fcrfelfalle.", "tokens": ["Vom", "Po\u00b7kal", "und", "W\u00fcr\u00b7fel\u00b7fal\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Weiche Provencalenlieder", "tokens": ["Wei\u00b7che", "Pro\u00b7ven\u00b7ca\u00b7len\u00b7lie\u00b7der"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "T\u00f6nen aus den rauhen Kehlen,", "tokens": ["T\u00f6\u00b7nen", "aus", "den", "rau\u00b7hen", "Keh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und sie schweben durch die Runde", "tokens": ["Und", "sie", "schwe\u00b7ben", "durch", "die", "Run\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schwankend, wie verirrte Seelen.", "tokens": ["Schwan\u00b7kend", ",", "wie", "ver\u00b7irr\u00b7te", "See\u00b7len", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWAV", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Doch den Einen von den Wachen", "tokens": ["Doch", "den", "Ei\u00b7nen", "von", "den", "Wa\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ART", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Seine Kameraden schelten,", "tokens": ["Sei\u00b7ne", "Ka\u00b7me\u00b7ra\u00b7den", "schel\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Denn er schweigt bei ihrem Jubel,", "tokens": ["Denn", "er", "schweigt", "bei", "ih\u00b7rem", "Ju\u00b7bel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hebt auch seinen Becher selten.", "tokens": ["Hebt", "auch", "sei\u00b7nen", "Be\u00b7cher", "sel\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Kl\u00e4rchens Vetter Heinrich ist es,", "tokens": ["Kl\u00e4r\u00b7chens", "Vet\u00b7ter", "Hein\u00b7rich", "ist", "es", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "VAFIN", "PPER", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Den des M\u00e4dchens Flehn bewogen,", "tokens": ["Den", "des", "M\u00e4d\u00b7chens", "Flehn", "be\u00b7wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVINF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df der Krieger auf des Kerkers", "tokens": ["Da\u00df", "der", "Krie\u00b7ger", "auf", "des", "Ker\u00b7kers"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Prevotalwacht ist gezogen. \u2014", "tokens": ["Pre\u00b7vo\u00b7tal\u00b7wacht", "ist", "ge\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["NN", "VAFIN", "VVPP", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Schweigend blicken nun die Freunde", "tokens": ["Schwei\u00b7gend", "bli\u00b7cken", "nun", "die", "Freun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Durch des Kerkers Fenstergitter,", "tokens": ["Durch", "des", "Ker\u00b7kers", "Fens\u00b7ter\u00b7git\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "N\u00e4chtlich kommt heraufgezogen", "tokens": ["N\u00e4cht\u00b7lich", "kommt", "her\u00b7auf\u00b7ge\u00b7zo\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dort vom Westen ein Gewitter.", "tokens": ["Dort", "vom", "Wes\u00b7ten", "ein", "Ge\u00b7wit\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Und die freien Wetterwolken", "tokens": ["Und", "die", "frei\u00b7en", "Wet\u00b7ter\u00b7wol\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ziehen rasch vorbei und schneiden", "tokens": ["Zie\u00b7hen", "rasch", "vor\u00b7bei", "und", "schnei\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "PTKVZ", "KON", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Finstre, h\u00f6hnische Gesichter", "tokens": ["Finst\u00b7re", ",", "h\u00f6h\u00b7ni\u00b7sche", "Ge\u00b7sich\u00b7ter"], "token_info": ["word", "punct", "word", "word"], "pos": ["NE", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In den Kerker auf die beiden.", "tokens": ["In", "den", "Ker\u00b7ker", "auf", "die", "bei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "PIAT", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Brausend fliegt des Todes Jagdhund", "tokens": ["Brau\u00b7send", "fliegt", "des", "To\u00b7des", "Jagd\u00b7hund"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVPP", "VVFIN", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sturm bergan in wilder Eile,", "tokens": ["Sturm", "ber\u00b7gan", "in", "wil\u00b7der", "Ei\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Seinen Herrn zu suchen, irrt er", "tokens": ["Sei\u00b7nen", "Herrn", "zu", "su\u00b7chen", ",", "irrt", "er"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "$,", "VVFIN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Durch die Felsen mit Geheule.", "tokens": ["Durch", "die", "Fel\u00b7sen", "mit", "Ge\u00b7heu\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Immer wird der Himmel dunkler\u201a", "tokens": ["Im\u00b7mer", "wird", "der", "Him\u00b7mel", "dunk\u00b7ler", "\u201a"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und schon ist die Nacht vollkommen\u201a", "tokens": ["Und", "schon", "ist", "die", "Nacht", "voll\u00b7kom\u00b7men", "\u201a"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie von einer finstern Ahnung", "tokens": ["Wie", "von", "ei\u00b7ner", "fins\u00b7tern", "Ah\u00b7nung"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wird der Freunde Herz beklommen.", "tokens": ["Wird", "der", "Freun\u00b7de", "Herz", "be\u00b7klom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Donnernd hallt des Todes Waidruf", "tokens": ["Don\u00b7nernd", "hallt", "des", "To\u00b7des", "Wai\u00b7druf"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVPP", "VVFIN", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ringsum in Gebirg und Thalen,", "tokens": ["Ring\u00b7sum", "in", "Ge\u00b7birg", "und", "Tha\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Pl\u00f6tzlich z\u00fcndet er die Nacht an", "tokens": ["Pl\u00f6tz\u00b7lich", "z\u00fcn\u00b7det", "er", "die", "Nacht", "an"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "ART", "NN", "PTKVZ"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit den hingeschossnen Strahlen.", "tokens": ["Mit", "den", "hin\u00b7ge\u00b7schoss\u00b7nen", "Strah\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Immer lauter schreit der Donner", "tokens": ["Im\u00b7mer", "lau\u00b7ter", "schreit", "der", "Don\u00b7ner"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PIAT", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Durch die grausen Finsternisse,", "tokens": ["Durch", "die", "grau\u00b7sen", "Fins\u00b7ter\u00b7nis\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Aus gebrochnen Wolken st\u00fcrzen", "tokens": ["Aus", "ge\u00b7broch\u00b7nen", "Wol\u00b7ken", "st\u00fcr\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Rauschend sich die Regeng\u00fcsse.", "tokens": ["Rau\u00b7schend", "sich", "die", "Re\u00b7gen\u00b7g\u00fcs\u00b7se", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PRF", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Hart am Kerker Blitze zucken", "tokens": ["Hart", "am", "Ker\u00b7ker", "Blit\u00b7ze", "zu\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "APPRART", "NN", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sehn die beiden mit Entsetzen,", "tokens": ["Sehn", "die", "bei\u00b7den", "mit", "Ent\u00b7set\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "PIAT", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "An den Felsen scheint der Tod hier", "tokens": ["An", "den", "Fel\u00b7sen", "scheint", "der", "Tod", "hier"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Seinen Flammenpfeil zu wetzen. \u2014", "tokens": ["Sei\u00b7nen", "Flam\u00b7men\u00b7pfeil", "zu", "wet\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "Doch wer sind die zwei Gestalten,", "tokens": ["Doch", "wer", "sind", "die", "zwei", "Ge\u00b7stal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VAFIN", "ART", "CARD", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die, umraset von den Wettern,", "tokens": ["Die", ",", "um\u00b7ra\u00b7set", "von", "den", "Wet\u00b7tern", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Es in solcher Stunde wagen,", "tokens": ["Es", "in", "sol\u00b7cher", "Stun\u00b7de", "wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PIAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zum Gef\u00e4ngni\u00df aufzuklettern?", "tokens": 