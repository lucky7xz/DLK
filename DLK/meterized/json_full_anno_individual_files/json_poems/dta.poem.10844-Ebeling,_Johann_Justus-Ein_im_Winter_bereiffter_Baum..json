{"dta.poem.10844": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Ein im Winter bereiffter  \n Baum.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198774", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Winter mit Schneeweissen Haa-\nren,", "tokens": ["Der", "Win\u00b7ter", "mit", "Schnee\u00b7weis\u00b7sen", "Haa", "ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "NN", "VVFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Kam eh mans dachte angefah-", "tokens": ["Kam", "eh", "mans", "dach\u00b7te", "an\u00b7ge\u00b7fah"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "KOUS", "PIS", "VVFIN", "TRUNC"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da wurden, Berge, Feld und Thal,", "tokens": ["Da", "wur\u00b7den", ",", "Ber\u00b7ge", ",", "Feld", "und", "Thal", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die B\u00e4ume, Stauden, B\u00fcsche kahl;", "tokens": ["Die", "B\u00e4u\u00b7me", ",", "Stau\u00b7den", ",", "B\u00fc\u00b7sche", "kahl", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "$,", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es hingen ihre Zweige nieder,", "tokens": ["Es", "hin\u00b7gen", "ih\u00b7re", "Zwei\u00b7ge", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Als w\u00e4ren sie erstarrte Glieder.", "tokens": ["Als", "w\u00e4\u00b7ren", "sie", "er\u00b7starr\u00b7te", "Glie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der Thau, den warme Lufft sonst sprenget,", "tokens": ["Der", "Thau", ",", "den", "war\u00b7me", "Lufft", "sonst", "spren\u00b7get", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Jm Sommer an den Bl\u00e4ttern h\u00e4nget,", "tokens": ["Jm", "Som\u00b7mer", "an", "den", "Bl\u00e4t\u00b7tern", "h\u00e4n\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die Tropfen die beim Sonnenschein,", "tokens": ["Die", "Trop\u00b7fen", "die", "beim", "Son\u00b7nen\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie kleine Sonnenspiegel seyn,", "tokens": ["Wie", "klei\u00b7ne", "Son\u00b7nen\u00b7spie\u00b7gel", "seyn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der h\u00e4ngte an den starren Kr\u00e4nzen,", "tokens": ["Der", "h\u00e4ng\u00b7te", "an", "den", "star\u00b7ren", "Kr\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der B\u00e4ume, die von Reiffen gl\u00e4nzen.", "tokens": ["Der", "B\u00e4u\u00b7me", ",", "die", "von", "Reif\u00b7fen", "gl\u00e4n\u00b7zen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Der Reif der an den Zweigen frieret,", "tokens": ["Der", "Reif", "der", "an", "den", "Zwei\u00b7gen", "frie\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den Baum verwundernsw\u00fcrdig zieret,", "tokens": ["Den", "Baum", "ver\u00b7wun\u00b7derns\u00b7w\u00fcr\u00b7dig", "zie\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ist unten breit und oben spiz", "tokens": ["Ist", "un\u00b7ten", "breit", "und", "o\u00b7ben", "spiz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ADJD", "KON", "ADV", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und wenn darauf des Lichtes Bliz", "tokens": ["Und", "wenn", "da\u00b7rauf", "des", "Lich\u00b7tes", "Bliz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PAV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mit seinen reichen Schimmer strahlet,", "tokens": ["Mit", "sei\u00b7nen", "rei\u00b7chen", "Schim\u00b7mer", "strah\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "So ist der ganze Baum bemahlet.", "tokens": ["So", "ist", "der", "gan\u00b7ze", "Baum", "be\u00b7mah\u00b7let", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "O! welch ein Heer von Demantsteinen,", "tokens": ["O", "!", "welch", "ein", "Heer", "von", "De\u00b7mant\u00b7stei\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PWAT", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sieht man alsdenn an B\u00e4umen scheinen,", "tokens": ["Sieht", "man", "als\u00b7denn", "an", "B\u00e4u\u00b7men", "schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn sie in weissen Schmuk bereifft;", "tokens": ["Wenn", "sie", "in", "weis\u00b7sen", "Schmuk", "be\u00b7re\u00b7ifft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Nicht anders als wenn drauf geh\u00e4ufft,", "tokens": ["Nicht", "an\u00b7ders", "als", "wenn", "drauf", "ge\u00b7h\u00e4ufft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "KOKOM", "KOUS", "PAV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Reichthum der in allen Gr\u00fcnden,", "tokens": ["Der", "Reicht\u00b7hum", "der", "in", "al\u00b7len", "Gr\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der unterirdschen Welt zu finden.", "tokens": ["Der", "un\u00b7ter\u00b7ird\u00b7schen", "Welt", "zu", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Allm\u00e4chtger Sch\u00f6pfer! welch Erg\u00f6zzen,", "tokens": ["All\u00b7m\u00e4cht\u00b7ger", "Sch\u00f6p\u00b7fer", "!", "welch", "Er\u00b7g\u00f6z\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "PWAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Fliest nicht aus eingebildten Sch\u00e4zzen,", "tokens": ["Fliest", "nicht", "aus", "ein\u00b7ge\u00b7bild\u00b7ten", "Sch\u00e4z\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Bei einer s\u00fcssen Augenlust,", "tokens": ["Bei", "ei\u00b7ner", "s\u00fcs\u00b7sen", "Au\u00b7gen\u00b7lust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zugleich in die begierge Brust;", "tokens": ["Zu\u00b7gleich", "in", "die", "be\u00b7gier\u00b7ge", "Brust", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich sah gereifte Diamanten,", "tokens": ["Ich", "sah", "ge\u00b7reif\u00b7te", "Di\u00b7a\u00b7man\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die von den Licht der Sonnen brandten.", "tokens": ["Die", "von", "den", "Licht", "der", "Son\u00b7nen", "brand\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ich lief darnach die regen Wunder", "tokens": ["Ich", "lief", "dar\u00b7nach", "die", "re\u00b7gen", "Wun\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PAV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ganz nah zu sehn; der Andachts-Zunder,", "tokens": ["Ganz", "nah", "zu", "sehn", ";", "der", "An\u00b7dachts\u00b7Zun\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PTKZU", "VVINF", "$.", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ward durch des Reiffes Bliz entflammt;", "tokens": ["Ward", "durch", "des", "Reif\u00b7fes", "Bliz", "ent\u00b7flammt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich dachte davon dieses stammt,", "tokens": ["Ich", "dach\u00b7te", "da\u00b7von", "die\u00b7ses", "stammt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "PDS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der kan auch seine Herrlichkeiten,", "tokens": ["Der", "kan", "auch", "sei\u00b7ne", "Herr\u00b7lich\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Jm Winter uns zur Lust ausbreiten.", "tokens": ["Jm", "Win\u00b7ter", "uns", "zur", "Lust", "aus\u00b7brei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Ein Tropfe wird durch ihn gebildet,", "tokens": ["Ein", "Trop\u00b7fe", "wird", "durch", "ihn", "ge\u00b7bil\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Durch seinen Wink gleichsam verg\u00fcldet,", "tokens": ["Durch", "sei\u00b7nen", "Wink", "gleich\u00b7sam", "ver\u00b7g\u00fcl\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Verwandelt durch der Sonnen Schein", "tokens": ["Ver\u00b7wan\u00b7delt", "durch", "der", "Son\u00b7nen", "Schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In Perlen und in Edelstein.", "tokens": ["In", "Per\u00b7len", "und", "in", "E\u00b7del\u00b7stein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sie auf den B\u00e4umen auszustreuen.", "tokens": ["Sie", "auf", "den", "B\u00e4u\u00b7men", "aus\u00b7zu\u00b7streu\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Wir meinen in den Wintertagen,", "tokens": ["Wir", "mei\u00b7nen", "in", "den", "Win\u00b7ter\u00b7ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "W\u00e4r, wie wir Sprichwortsweise sagen,", "tokens": ["W\u00e4r", ",", "wie", "wir", "Sprich\u00b7worts\u00b7wei\u00b7se", "sa\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "PWAV", "PPER", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Jm Garten keine Lust zu gehn,", "tokens": ["Jm", "Gar\u00b7ten", "kei\u00b7ne", "Lust", "zu", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PIAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nichts angenehmes zu besehn:", "tokens": ["Nichts", "an\u00b7ge\u00b7neh\u00b7mes", "zu", "be\u00b7sehn", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "ADJA", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wir irren; nichts kan sch\u00f6ner gl\u00e4nzen,", "tokens": ["Wir", "ir\u00b7ren", ";", "nichts", "kan", "sch\u00f6\u00b7ner", "gl\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PIS", "VMFIN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Als Reiffen, die den Baum bekr\u00e4nzen.", "tokens": ["Als", "Reif\u00b7fen", ",", "die", "den", "Baum", "be\u00b7kr\u00e4n\u00b7zen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "PRELS", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Wenn wir die Sinnen nur recht n\u00fczzen,", "tokens": ["Wenn", "wir", "die", "Sin\u00b7nen", "nur", "recht", "n\u00fcz\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So sehn wir allenthalben blizzen,", "tokens": ["So", "sehn", "wir", "al\u00b7len\u00b7thal\u00b7ben", "bliz\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Den Glanz wodurch die Majest\u00e4t,", "tokens": ["Den", "Glanz", "wo\u00b7durch", "die", "Ma\u00b7jes\u00b7t\u00e4t", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des Allerh\u00f6chsten wird erh\u00f6ht;", "tokens": ["Des", "Al\u00b7ler\u00b7h\u00f6chs\u00b7ten", "wird", "er\u00b7h\u00f6ht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Auch in des Winters tr\u00fcben Dunkeln,", "tokens": ["Auch", "in", "des", "Win\u00b7ters", "tr\u00fc\u00b7ben", "Dun\u00b7keln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sieht man sie in dem Reiffen funkeln.", "tokens": ["Sieht", "man", "sie", "in", "dem", "Reif\u00b7fen", "fun\u00b7keln", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}