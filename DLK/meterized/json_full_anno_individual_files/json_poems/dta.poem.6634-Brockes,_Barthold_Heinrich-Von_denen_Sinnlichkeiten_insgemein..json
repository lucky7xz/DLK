{"dta.poem.6634": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von denen Sinnlichkeiten  \n insgemein.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es werden hier und dar uns C\u00f6rper auf der Welt,", "tokens": ["Es", "wer\u00b7den", "hier", "und", "dar", "uns", "C\u00f6r\u00b7per", "auf", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "KON", "PTKVZ", "PPER", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die, wie die unsrigen bereitet, dargestellt.", "tokens": ["Die", ",", "wie", "die", "uns\u00b7ri\u00b7gen", "be\u00b7rei\u00b7tet", ",", "dar\u00b7ge\u00b7stellt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "$,", "PWAV", "ART", "PPOSS", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wir k\u00f6nnen augenscheinlich sehen,", "tokens": ["Wir", "k\u00f6n\u00b7nen", "au\u00b7gen\u00b7schein\u00b7lich", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df sie auf gleiche Weis\u2019, als wir, entstehen,", "tokens": ["Da\u00df", "sie", "auf", "glei\u00b7che", "Weis'", ",", "als", "wir", ",", "ent\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "$,", "KOUS", "PPER", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Man siehet, da\u00df sie allzusammen", "tokens": ["Man", "sie\u00b7het", ",", "da\u00df", "sie", "all\u00b7zu\u00b7sam\u00b7men"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PIS", "VVFIN", "$,", "KOUS", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Aus der Materie gef\u00fcget sind, und stammen.", "tokens": ["Aus", "der", "Ma\u00b7te\u00b7rie", "ge\u00b7f\u00fc\u00b7get", "sind", ",", "und", "stam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "VAFIN", "$,", "KON", "VVFIN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.7": {"text": "Sie sind, wer leugnet es? gebohren, so, wie wir:", "tokens": ["Sie", "sind", ",", "wer", "leug\u00b7net", "es", "?", "ge\u00b7boh\u00b7ren", ",", "so", ",", "wie", "wir", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "PWS", "VVFIN", "PPER", "$.", "VVPP", "$,", "ADV", "$,", "PWAV", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Jhr Leben f\u00e4ngt sich an; sie scheiden auch von hier.", "tokens": ["Ihr", "Le\u00b7ben", "f\u00e4ngt", "sich", "an", ";", "sie", "schei\u00b7den", "auch", "von", "hier", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "PTKVZ", "$.", "PPER", "VVFIN", "ADV", "APPR", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Allein, hat denn der Mensch, nebst allen ihren Gaben,", "tokens": ["Al\u00b7lein", ",", "hat", "denn", "der", "Mensch", ",", "nebst", "al\u00b7len", "ih\u00b7ren", "Ga\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "VAFIN", "ADV", "ART", "NN", "$,", "APPR", "PIAT", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Nicht den ", "tokens": ["Nicht", "den"], "token_info": ["word", "word"], "pos": ["PTKNEG", "ART"], "meter": "+-", "measure": "trochaic.single"}}}}}