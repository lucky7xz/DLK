{"dta.poem.11686": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Bei Gelegenheit eines Kinderspielzeugs,  \n vorstellend:  \n  Hanswurst an der Sandm\u00fchle.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.85", "cy:0.14"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "Seyd gescheidt.", "tokens": ["Seyd", "ge\u00b7scheidt", "."], "token_info": ["word", "word", "punct"], "pos": ["VAIMP", "ADJD", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Der Narr hatt' euch zum Besten, gute Leut'!", "tokens": ["Der", "Narr", "hatt'", "euch", "zum", "Bes\u00b7ten", ",", "gu\u00b7te", "Leut'", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "APPRART", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ihr kennt ihn ja, es ist der Alte.", "tokens": ["Ihr", "kennt", "ihn", "ja", ",", "es", "ist", "der", "Al\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "PPER", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}