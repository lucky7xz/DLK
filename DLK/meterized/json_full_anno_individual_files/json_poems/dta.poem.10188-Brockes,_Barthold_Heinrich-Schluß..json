{"dta.poem.10188": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Schlu\u00df.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20086-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "O GOTT! indem auch Du in unsrer Brust,", "tokens": ["O", "GoTT", "!", "in\u00b7dem", "auch", "Du", "in", "uns\u00b7rer", "Brust", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$.", "KOUS", "ADV", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wenn Du Dein Werck darin gebildet findest,", "tokens": ["Wenn", "Du", "Dein", "Werck", "da\u00b7rin", "ge\u00b7bil\u00b7det", "fin\u00b7dest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "PAV", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Sch\u00f6nheit siehest, die wir sehen;", "tokens": ["Die", "Sch\u00f6n\u00b7heit", "sie\u00b7hest", ",", "die", "wir", "se\u00b7hen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PRELS", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So hoff\u2019 ich da\u00df, aus Liebe, Du die Lust,", "tokens": ["So", "hoff'", "ich", "da\u00df", ",", "aus", "Lie\u00b7be", ",", "Du", "die", "Lust", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KOUS", "$,", "APPR", "NN", "$,", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die wir empfinden, auch empfindest.", "tokens": ["Die", "wir", "emp\u00b7fin\u00b7den", ",", "auch", "emp\u00b7fin\u00b7dest", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$,", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ach m\u00f6gte solches offt geschehen!", "tokens": ["Ach", "m\u00f6g\u00b7te", "sol\u00b7ches", "offt", "ge\u00b7sche\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PIS", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Auf! lasst uns denn, zu Seiner Ehr,", "tokens": ["Auf", "!", "lasst", "uns", "denn", ",", "zu", "Sei\u00b7ner", "Ehr", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "$.", "VVFIN", "PPER", "ADV", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Uns immer mehr und mehr bestreben,", "tokens": ["Uns", "im\u00b7mer", "mehr", "und", "mehr", "be\u00b7stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "KON", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Zu Seinem Ruhm, vergn\u00fcgt zu leben!", "tokens": ["Zu", "Sei\u00b7nem", "Ruhm", ",", "ver\u00b7gn\u00fcgt", "zu", "le\u00b7ben", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "VVPP", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}