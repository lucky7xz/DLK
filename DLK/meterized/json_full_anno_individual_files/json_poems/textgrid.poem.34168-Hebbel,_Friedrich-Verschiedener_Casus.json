{"textgrid.poem.34168": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Verschiedener Casus", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Deutsche zogen nach Rom, warum nicht Russen nach Deutschland?", "tokens": ["Deut\u00b7sche", "zo\u00b7gen", "nach", "Rom", ",", "wa\u00b7rum", "nicht", "Rus\u00b7sen", "nach", "Deutschland", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "NE", "$,", "PWAV", "PTKNEG", "NN", "APPR", "NE", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Jene waren ein Volk, tapfer und markig und frisch,", "tokens": ["Je\u00b7ne", "wa\u00b7ren", "ein", "Volk", ",", "tap\u00b7fer", "und", "mar\u00b7kig", "und", "frisch", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "ADJD", "KON", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+--+---+--+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Und als solches vom Himmel zu Erden der R\u00f6mer berufen,", "tokens": ["Und", "als", "sol\u00b7ches", "vom", "Him\u00b7mel", "zu", "Er\u00b7den", "der", "R\u00f6\u00b7mer", "be\u00b7ru\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "APPRART", "NN", "APPR", "NN", "ART", "NN", "VVPP", "$,"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.4": {"text": "Ja, sie blieben's bis heut', diese sind nur noch Geschmei\u00df,", "tokens": ["Ja", ",", "sie", "blie\u00b7ben's", "bis", "heut'", ",", "die\u00b7se", "sind", "nur", "noch", "Ge\u00b7schmei\u00df", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "APPR", "ADV", "$,", "PDS", "VAFIN", "ADV", "ADV", "NN", "$,"], "meter": "+-+------+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Und das schlechtere Volk ward nie noch der Henker des edlern,", "tokens": ["Und", "das", "schlech\u00b7te\u00b7re", "Volk", "ward", "nie", "noch", "der", "Hen\u00b7ker", "des", "ed\u00b7lern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VAFIN", "ADV", "ADV", "ART", "NN", "ART", "ADJA", "$,"], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "W\u00e4hrend der lauterste Mensch oft durch den niedrigsten f\u00e4llt.", "tokens": ["W\u00e4h\u00b7rend", "der", "lau\u00b7ters\u00b7te", "Mensch", "oft", "durch", "den", "nied\u00b7rigs\u00b7ten", "f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "ADV", "APPR", "ART", "ADJA", "VVFIN", "$."], "meter": "+--+--+-+--+-+", "measure": "dactylic.di.plus"}, "line.7": {"text": "Wenn der Russe den Tasso verbessert, der Deutsche die Knute,", "tokens": ["Wenn", "der", "Rus\u00b7se", "den", "Tas\u00b7so", "ver\u00b7bes\u00b7sert", ",", "der", "Deut\u00b7sche", "die", "Knu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "VVPP", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.8": {"text": "Will ich zittern f\u00fcr uns, aber ich warte es ab!", "tokens": ["Will", "ich", "zit\u00b7tern", "f\u00fcr", "uns", ",", "a\u00b7ber", "ich", "war\u00b7te", "es", "ab", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMF