{"dta.poem.20906": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Der  \n Sterbende  \n  Socrates.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Der Klumpen also aufgeh\u00e4ncket/", "tokens": ["Der", "Klum\u00b7pen", "al\u00b7so", "auf\u00b7ge\u00b7h\u00e4n\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Hat einen grossen Raum in sich/", "tokens": ["Hat", "ei\u00b7nen", "gros\u00b7sen", "Raum", "in", "sich", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "APPR", "PRF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie dann ein ieder ohne mich", "tokens": ["Wie", "dann", "ein", "ie\u00b7der", "oh\u00b7ne", "mich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ART", "PIAT", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es glauben wird/ der es bedencket,", "tokens": ["Es", "glau\u00b7ben", "wird", "/", "der", "es", "be\u00b7den\u00b7cket", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "VAFIN", "$(", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wir sitzen hier um Flu\u00df und Meer/", "tokens": ["Wir", "sit\u00b7zen", "hier", "um", "Flu\u00df", "und", "Meer", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Als wie der Fr\u00f6sche kaltes Heer/", "tokens": ["Als", "wie", "der", "Fr\u00f6\u00b7sche", "kal\u00b7tes", "Heer", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Es gleicht sich einem Amei\u00df-H\u00fcgel/", "tokens": ["Es", "gleicht", "sich", "ei\u00b7nem", "A\u00b7mei\u00df\u00b7H\u00fc\u00b7gel", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Was zwischen hier und da sich h\u00e4lt/", "tokens": ["Was", "zwi\u00b7schen", "hier", "und", "da", "sich", "h\u00e4lt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ADV", "KON", "KOUS", "PRF", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Wo eine Seule dieser Welt/", "tokens": ["Wo", "ei\u00b7ne", "Seu\u00b7le", "die\u00b7ser", "Welt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PDAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Ein Halbgott hat gemacht zum R\u00fcgel.", "tokens": ["Ein", "Halb\u00b7gott", "hat", "ge\u00b7macht", "zum", "R\u00fc\u00b7gel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Man schaut in vielen andern Enden/", "tokens": ["Man", "schaut", "in", "vie\u00b7len", "an\u00b7dern", "En\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "PIAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie es f\u00fcr Menschen Wohnung giebt/", "tokens": ["Wie", "es", "f\u00fcr", "Men\u00b7schen", "Woh\u00b7nung", "giebt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "NN", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und es der Erden so beliebt", "tokens": ["Und", "es", "der", "Er\u00b7den", "so", "be\u00b7liebt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "ART", "NN", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bebant zu seyn von unsern H\u00e4nden/", "tokens": ["Be\u00b7bant", "zu", "seyn", "von", "un\u00b7sern", "H\u00e4n\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VAINF", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Man schaut da\u00df nach Gestalt und Art/", "tokens": ["Man", "schaut", "da\u00df", "nach", "Ge\u00b7stalt", "und", "Art", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "KOUS", "APPR", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nachdem da\u00df Wasser nimmt die Fahrt/", "tokens": ["Nach\u00b7dem", "da\u00df", "Was\u00b7ser", "nimmt", "die", "Fahrt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "NN", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und diesen grossen Klupen netzet.", "tokens": ["Und", "die\u00b7sen", "gros\u00b7sen", "Klu\u00b7pen", "net\u00b7zet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die Seiten werden tieff gemacht/", "tokens": ["Die", "Sei\u00b7ten", "wer\u00b7den", "tieff", "ge\u00b7macht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und sch\u00f6ne Stellen f\u00fcrgebracht/", "tokens": ["Und", "sch\u00f6\u00b7ne", "Stel\u00b7len", "f\u00fcr\u00b7ge\u00b7bracht", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Darauf des Menschen Kind sich setzet.", "tokens": ["Da\u00b7rauf", "des", "Men\u00b7schen", "Kind", "sich", "set\u00b7zet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ein\u2019 Erde von viel h\u00f6hern Gaben/", "tokens": ["Ein'", "Er\u00b7de", "von", "viel", "h\u00f6\u00b7hern", "Ga\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PIAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da Freude/ Ruh und Friede lacht/", "tokens": ["Da", "Freu\u00b7de", "/", "Ruh", "und", "Frie\u00b7de", "lacht", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$(", "NN", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wo nimmermehr der Donner kracht/", "tokens": ["Wo", "nim\u00b7mer\u00b7mehr", "der", "Don\u00b7ner", "kracht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und man nicht redet vom Begraben;", "tokens": ["Und", "man", "nicht", "re\u00b7det", "vom", "Be\u00b7gra\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "PTKNEG", "VVFIN", "APPRART", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Da Glantz und Klarheit wird geschau", "tokens": ["Da", "Glantz", "und", "Klar\u00b7heit", "wird", "ge\u00b7schau"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "KON", "NN", "VAFIN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die ist im Himmel aufgebaut/", "tokens": ["Die", "ist", "im", "Him\u00b7mel", "auf\u00b7ge\u00b7baut", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPRART", "NN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da alle M\u00e4ngel m\u00fcssen weichen.", "tokens": ["Da", "al\u00b7le", "M\u00e4n\u00b7gel", "m\u00fcs\u00b7sen", "wei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Hier sind die Sternen angehenckt/", "tokens": ["Hier", "sind", "die", "Ster\u00b7nen", "an\u00b7ge\u00b7henckt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Die Frommen auch dahin gelenckt/", "tokens": ["Die", "From\u00b7men", "auch", "da\u00b7hin", "ge\u00b7lenckt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PAV", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Wo Trost und Lust niemals verstreichen.", "tokens": ["Wo", "Trost", "und", "Lust", "nie\u00b7mals", "ver\u00b7strei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "ADV", "VVINF", "$."], "meter": "-+-+---+-", "measure": "unknown.measure.tri"}}, "stanza.4": {"line.1": {"text": "Das reine Lust-Schlo\u00df/ wie man h\u00f6ret", "tokens": ["Das", "rei\u00b7ne", "Lust\u00b7Schlo\u00df", "/", "wie", "man", "h\u00f6\u00b7ret"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$(", "PWAV", "PIS", "VVFIN"], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Aus dieser grossen Leute Mund/", "tokens": ["Aus", "die\u00b7ser", "gros\u00b7sen", "Leu\u00b7te", "Mund", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der uns den rechten Wundergrund/", "tokens": ["Der", "uns", "den", "rech\u00b7ten", "Wun\u00b7der\u00b7grund", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dergleichen hoher Sachen lehret/", "tokens": ["Derg\u00b7lei\u00b7chen", "ho\u00b7her", "Sa\u00b7chen", "leh\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ist ausgezieret gantz und gar/", "tokens": ["Ist", "aus\u00b7ge\u00b7zie\u00b7ret", "gantz", "und", "gar", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "ADV", "KON", "ADV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit der Gestirne reicher Schaar/", "tokens": ["Mit", "der", "Ge\u00b7stir\u00b7ne", "rei\u00b7cher", "Schaar", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die diese Lufft wie Gla\u00df durchstreichen/", "tokens": ["Die", "die\u00b7se", "Lufft", "wie", "Gla\u00df", "durch\u00b7strei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PDAT", "NN", "KOKOM", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die gantze Welt ist ihre Bahn/", "tokens": ["Die", "gant\u00b7ze", "Welt", "ist", "ih\u00b7re", "Bahn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Sie ist bem\u00fchet da\u00df sie kan", "tokens": ["Sie", "ist", "be\u00b7m\u00fc\u00b7het", "da\u00df", "sie", "kan"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "VVFIN", "KOUS", "PPER", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Der Erden tieffe Scho\u00df erreichen.", "tokens": ["Der", "Er\u00b7den", "tief\u00b7fe", "Scho\u00df", "er\u00b7rei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Die wir hier wie in Keltern schweben/", "tokens": ["Die", "wir", "hier", "wie", "in", "Kel\u00b7tern", "schwe\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "KOKOM", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Erkennen nicht wie sichs geb\u00fchrt/", "tokens": ["Er\u00b7ken\u00b7nen", "nicht", "wie", "sichs", "ge\u00b7b\u00fchrt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "KOKOM", "PIS", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wann ihre Klarheit uns ber\u00fchrt/", 