{"dta.poem.20424": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Streit der schwartzen augen/ rothen  \n lippen/ und weissen br\u00fcste.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was euer strahl bezwingt/ was eure wort verrichten/", "tokens": ["Was", "eu\u00b7er", "strahl", "be\u00b7zwingt", "/", "was", "eu\u00b7re", "wort", "ver\u00b7rich\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "VVFIN", "$(", "PWS", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ist uns genug bekandt/ ist uns genug bewust.", "tokens": ["Ist", "uns", "ge\u00b7nug", "be\u00b7kandt", "/", "ist", "uns", "ge\u00b7nug", "be\u00b7wust", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "$(", "VAFIN", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch lassen wir uns auch nicht gantz und gar vernichten/", "tokens": ["Doch", "las\u00b7sen", "wir", "uns", "auch", "nicht", "gantz", "und", "gar", "ver\u00b7nich\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PRF", "ADV", "PTKNEG", "ADV", "KON", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wir sind/ bedenckt es wohl/ der garten aller lust.", "tokens": ["Wir", "sind", "/", "be\u00b7denckt", "es", "wohl", "/", "der", "gar\u00b7ten", "al\u00b7ler", "lust", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$(", "VVFIN", "PPER", "ADV", "$(", "ART", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die \u00e4pffel/ so allhier auff diesem stocke schweben/", "tokens": ["Die", "\u00e4pf\u00b7fel", "/", "so", "all\u00b7hier", "auff", "die\u00b7sem", "sto\u00b7cke", "schwe\u00b7ben", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ADV", "ADV", "APPR", "PDAT", "NN", "VVINF", "$("], "meter": "-+-++--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Sind s\u00fcsser noch als die/ so Abels mutter a\u00df;", "tokens": ["Sind", "s\u00fcs\u00b7ser", "noch", "als", "die", "/", "so", "A\u00b7bels", "mut\u00b7ter", "a\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ADV", "KOUS", "ART", "$(", "ADV", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ja besser/ weil sie nicht verletzen an dem leben/", "tokens": ["Ja", "bes\u00b7ser", "/", "weil", "sie", "nicht", "ver\u00b7let\u00b7zen", "an", "dem", "le\u00b7ben", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADJD", "$(", "KOUS", "PPER", "PTKNEG", "VVINF", "APPR", "ART", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und keine schlange nicht auff ihren bl\u00e4ttern sa\u00df.", "tokens": ["Und", "kei\u00b7ne", "schlan\u00b7ge", "nicht", "auff", "ih\u00b7ren", "bl\u00e4t\u00b7tern", "sa\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "VVFIN", "PTKNEG", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}